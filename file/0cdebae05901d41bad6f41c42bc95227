		//$("#sign_up").find("input:first").focus();
		//}
	});
	/*
	e.preventDefault();
	});
	$('table tr:nth-child(even)').addClass('stripe');
	*/
});

$(".bar-menu-sns").find('li').mouseover(function(){
	
	$(this).find('.mainbox').fadeIn(150);
	
	}).mouseleave(function(){
		$(this).find('.mainbox').stop().fadeOut(150);});
		$(".bar-menu-top").find('li').mouseover(function(){
	
	$(this).find('.tsbox').fadeIn(150);
	
	}).mouseleave(function(){
	$(this).find('.tsbox').stop().fadeOut(150);
		});
function goTop(){
	$('html,body').animate({'scrollTop':0},1000);
}
