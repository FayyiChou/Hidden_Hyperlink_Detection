$(function() {

<!--生成 WAP二维码-->
	var renderType = "table" ;
	//判断是否支持HTML5,如果支持HTML5则使用canvas标签生成 二维码，以提高性能
	if (window.applicationCache) {
        renderType = "canvas" ;
    }

	//替换成WAP访问地址
	//var wapURL = domainWap + window.location.pathname + window.location.hash ;
	var wapURL = domainWap + window.location.pathname ;
	jQuery('#product_wap').qrcode({
		render : renderType,
		width : 118,
		height : 118,
		text : wapURL
	});

//控制动态显示、隐藏二维码图片
$(".pro-mobile-code").hover(function() {
		//鼠标经过显示二维码
		$(this).addClass("hover") ;
	},

	function(){
		//鼠标离开隐藏二维码
		$(this).removeClass("hover") ;
	});
});
