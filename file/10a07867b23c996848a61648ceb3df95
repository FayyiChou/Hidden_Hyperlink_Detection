<script>
    $(function () {
        (function () {
            var $c_menu = $('#js-change-menu');
            $c_menu.on('click', function () {
                if (value == 1) {
                    Cookies.set('c_is_menu_new', 2, {expires: 365, path: '/'});
                    $('.old-menu').hide();
                    $('.new-menu').show();
                } else {
                    Cookies.set('c_is_menu_new', 1, {expires: 365, path: '/'});
                    $('.old-menu').show();
                    $('.new-menu').hide();
                }
                window.location.href = '/profile';
            });
            var value = Cookies.get('c_is_menu_new');
            if (value == 1) {
                $('.old-menu').hide();
                $('.new-menu').show();
                $('#js-chang-menu-name').text('旧版');
            } else {
                $('.old-menu').show();
                $('.new-menu').hide();
                $('#js-chang-menu-name').text('新版');
            }
        })();
    });
</script>

