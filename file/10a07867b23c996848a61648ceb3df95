    <form action="/csgo/search" method="get" id="search_form">
        <input type="hidden" id="search_page_no" name="search_page_no" value="1">
        <input type="hidden" id="search_relate_price" name="search_relate_price">
        <input type="hidden" id="search_product_type_id" name="search_product_type_id" value="">
        <input type="hidden" id="search_category_id" name="search_category_id" value="">
        <input type="hidden" id="search_sort_key" name="search_sort_key" value="">
        <input type="hidden" id="search_sort_rule" name="search_sort_rule" value="">
        <input type="hidden" id="search_is_sticker" name="search_is_sticker" value="">
        <input type="hidden" id="search_key" name="search_key" value="">
        <input type="hidden" id="search_price_gte" name="search_price_gte" value="">
        <input type="hidden" id="search_price_lte" name="search_price_lte" value="">
        <input type="hidden" id="search_rarity_id" name="search_rarity_id" value="">
        <input type="hidden" id="search_exterior_id" name="search_exterior_id" value="">
        <input type="hidden" id="search_is_stattrak" name="search_is_stattrak" value="">
        
