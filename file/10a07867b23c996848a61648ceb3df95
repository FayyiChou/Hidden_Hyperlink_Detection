<script type="text/javascript">
//底部显示大二维码
(function(){
    $('#js-show-big-qr').click(function(){
        layer.open({
            type:1,
            title:'扫描二维码',
            area:['250px','300px'],
            content:'<div style="padding:20px;text-align:center;"><img src="http://static.igxe.cn/csgo/images/img-wx-gzh.png" /></div>'
        });
    });
})();
