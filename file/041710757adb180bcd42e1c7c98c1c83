    function deleteConfirm(deleteurl,currenturl,refererurl){
        if (confirm('Are you sure you would like to remove this item from the shopping basket?') == true) {
            data = '&isAjax=1&qty=1';
            $j.ajax( {
                url : deleteurl,
                dataType : 'json',
                data: data,
                type: 'get',
                success : function(data) {
                    if(currenturl.indexOf('checkout/cart/add')==-1)
                    {
                        refererurl= currenturl;
                    }
                    setTimeout(function(){ location.href = refererurl; }, 0);

                    return true;
                }
            });
        }
        else{
            return false;
        }
    }
</script>

                

