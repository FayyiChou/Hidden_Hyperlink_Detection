        <script>
            try{
                jquip.fn.modal = function(a) {
                    if (a != "hide") {
                        $(this)[0].style.display = "block";
                
                        $(this)[0].style.marginTop = -$(this).children(0)[0].offsetHeight / 2 + 'px';
                        $(this).addClass("in");
                        $("body").append("<div  class='modal-backdrop fade in'></div>");
                    } else {
                        $(this)[0].style.display = "none";
                        $(this).removeClass("in");
                        $(".modal-backdrop").remove();
                    }
                };
            } catch(err) {}
            try{
                jquip.fn.show = function() {
                    $(this)[0].style.display = "block";
                };
            } catch(err) {}
        </script>
