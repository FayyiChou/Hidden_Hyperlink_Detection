    function closeModal(a) {
        $(a).hide();
        $(".modal-backdrop").remove()
        
        if ( (/^1[3|4|5|7|8]\d{9}$/.test($("#mobile").val()))) {
            $("#getcode").removeClass("getcode-gray");
        }
    }
    var value = [];
    var valueHtml = [];
    var pex = "";
    var countDown = function(l) {
        if (!l) {
            return
        }
        var m = l.obj;
        var p = l.speed || 1000;
        var o = l.wait || 40;
        var j = l.callback || "";
        var i = ")" + m[0].innerHTML || "";
        var k = function() {
            if (o > 0) {
                var a = o--;
                m.html("(" + (a + i))
            } else { 
              $('#mobile').removeAttr("readonly");
                clearInterval(set);
                j.call(this, m);
                o = l.wait
            }
        };
        var n = function() {
            set = window.setInterval(k, p)
        };
        return n()
    } 
    function Recode() {
        $("#getcode").removeClass("getcode-gray").html("再次发送验证码");
        document.getElementById("yy-form-metal").style.display="block";
        fn_yuyin_css(1);
    }
    //wxfalg 1:短信  2：语音
    function sendCode(d,wxfalg) {
    	
      if (! (/^1[3|4|5|7|8]\d{9}$/.test($("#mobile").val()))) {
                    $(".reg-error").eq(0).html("您输入的手机号有误");
                    $("#getcode").addClass("getcode-gray");
                    fn_yuyin_css(0);
                    return false;
         }
        if ($(d).hasClass("getcode-gray")) {
            return
        }
        if ($("#sound-code-div").hasClass("code-div")) {
            return
        }
        var c = $("#mobile").val();
        var b = $("#isExist").val();
        $("#getcode").addClass("getcode-gray");
        fn_yuyin_css(0);
        if(wxfalg==1){
            		$("#getcode").removeClass("getcode-gray");
            		var l=$;
        	    	var b = l('[validate="phone"]').val();
        	
            var inputCode = $("#wap-code").val();
           if(!checkImgCode()){return false;}
           
         	var a = $("#mobTemStyle").val();
            $.ajax({
                type: "POST",
                url: base + "/account/getPhoneCodeByMobile.htm" + "?d=" + new Date().getMilliseconds() + "andtoken='123'",
                data: {
                    "Num": c,
                    "temStyle": a,
                    "inputCode":inputCode
                },
                success: function(e) {
                     $(".reg-error").html("");
                  	// 0:表示成功发送验证码
                    if (e != 0) {
                        $('[validate="phone"]').removeClass("m-member-focus").addClass("m-member-orange");
                        $("#getcode").removeClass("getcode-gray");
                        fn_yuyin_css(1);
                        var f = "";
                        if (e == -2) {
                            f = "请输入正确的手机号!"
                        } else {
                            if (e == -3) {
                                f = "该手机24小时内达到最大获取验证码次数!"
                            } else {
                                if (e == -4) {
                                    f = "60秒内不能重复获取验证码,请稍候再试!"
                                } else {
                                    if (e == -5) {
                                        f = "您已绑定过手机!"
                                    } else {
                                    	if(e == 3){
                                    		f = "哎呀，图形验证码好像不正确";
                                    	}else{ 
                                            f = "系统错误,请稍后重试!" + e
                                    	}
                                    }
                                }
                            }
                        }
                        $(".reg-error").eq(0).html("<i></i>" + f + "</span>");
                        return
                    } else {
	                    $('#mobile').attr("readonly",true);
	                        $("#getcode").addClass("getcode-gray");
	                        fn_yuyin_css(0);
	                        countDown({
	                            obj:$("#getcode"),
	                            speed: 1000,
	                            wait: 60,
	                            txt: " 秒后再次发送",
	                            callback: Recode
	                    })
                    }
                },
                error: function() {
                    $("#getcode").removeClass("getcode-gray");
                    fn_yuyin_css(1);
                    $(".reg-error").html("网络出错，请重新加载");
                }
            }); 
        }else if(wxfalg==2){
            $.ajax({
                type: "POST",
                url: base + "/speechSound/getMobileSpeechSoundCode.htm" + "?d=" + new Date().getMilliseconds() + "andtoken='12345'",
                data: {
                    "Num": c,
                    "isExist": b
                },
                async: false,
                success: function(e) {
                    $(".reg-error").html("");
                    if (e != 0) {
                        $('[validate="phone"]').removeClass("m-member-focus").addClass("m-member-orange");
                        $("#getcode").removeClass("getcode-gray");
                        fn_yuyin_css(1);
                        var f = "";
                        if (e == 1) {
                            f = "手机号已经注册请登录!"
                        } else {
                            if (e == -2) {
                                f = "请输入正确的手机号!"
                            } else {
                                if (e == -3) {
                                    f = "该手机24小时内达到最大获取验证码次数!"
                                } else {
                                    if (e == -4) {
                                        f = "30秒内不能重复获取验证码,请稍候再试!"
                                    } else {
                                        if (e == -5) {
                                            f = "您已绑定过手机!"
                                        } else {
                                            f = "系统错误,请稍后重试!" + e
                                        }
                                    }
                                }
                            }
                        }
                        $(".reg-error").eq(0).html("<i></i>" + f + "</span>");
                        return
                    } else {
                    $('#mobile').attr("readonly",true);
                        $("#getcode").addClass("getcode-gray");
                        fn_yuyin_css(0);
                        countDown({
                            obj: $("#getcode"),
                            speed: 1000,
                            wait: 30,
                            txt: " 秒后再次发送",
                            callback: Recode
                        })
                    }
                },
                error: function() {
                    $("#getcode").removeClass("getcode-gray");
                    fn_yuyin_css(1);
                    $(".reg-error").html("网络出错，请重新加载");
                }
            });
        
        }
    }
    function chekPinInput() {
        $(".reg-error").html("");
        if ($("#captcha").val().trim() == "") {
            $(".reg-error").html("请输入收到的验证码");
            return false;
        }
        $("#errorMessgP").html("");
        return true;
    }
    function formSubmit() { 
        var val = $("#checkedInput").is(':checked')
        if(!val){
            $(".reg-error").eq(0).html("注册协议还没有被您认可喔！").show();
            return false;
        }
    
        var isgray=false;
        if ( $("#getcode").hasClass("getcode-gray")){
            isgray=true;
        }
        if (checkMoble2()&&chekPinInput() ) {
            $(".form-login")[0].submit();
        }
        if (isgray){
         $("#getcode").addClass("getcode-gray")
        }
    }
    function checkMoble2() {
        if (! (/^1[3|4|5|7|8]\d{9}$/.test($("#mobile").val()))) {
                    $(".reg-error").eq(0).html("您输入的手机号有误");
                    $("#getcode").addClass("getcode-gray");
                    fn_yuyin_css(0);
                    return false;
          } 
          return true;
    }
    function checkImgCode(){
    	 $(".reg-error").html(""); 
         if ($("#wap-code").val().trim() == "") {
             $(".reg-error").html("请输入图形验证码");
             return false;
         }
         $("#errorMessgP").html("");
         return true;
    }
    
    function checkMoble() {
        var mobile=document.getElementById("mobile");
        if (mobile.readOnly){
            return false;
        }
        $("#errorMessgP").html("");
        var l=$;
        var b = l('[validate="phone"]').val();
     
        if (!b.length) {
            $("#errorMessgP").html("请输入手机号");
            $("#getcode").addClass("getcode-gray");
            
            fn_yuyin_css(0);
            return false;
        }else{
        	if(b.length == 11){
        		if (!(/^1[3|4|5|7|8]\d{9}$/.test(b))) {
        			 $("#errorMessgP").html("您输入的手机号有误");
                    $("#getcode").addClass("getcode-gray");
                    
                    fn_yuyin_css(0);
                    return false;
                }
        	}else{
        		$("#errorMessgP").html("您输入的手机号有误");
                $("#getcode").addClass("getcode-gray");
                
                fn_yuyin_css(0);
                return false;
        	}
        }
        
        $("#getcode").removeClass("getcode-gray");
        fn_yuyin_css(1);
        return true;
    }
    
    
     function redayPage() {
        var mobile=document.getElementById("mobile");
        if (mobile.readOnly){
            return false;
        }
        var l=$;
        var b = l('[validate="phone"]').val();
     
        if (!b.length) {
            //$("#errorMessgP").html("请输入手机号");//避免登录页面未输入手机时，点击动态码登录链接过来后，仍然给出提示
            $("#getcode").addClass("getcode-gray");
            
            fn_yuyin_css(0);
            return false;
        }else{
        	if(b.length == 11){
        		if (!(/^1[3|4|5|7|8]\d{9}$/.test(b))) {
        			$("#errorMessgP").html("您输入的手机号有误");
                    $("#getcode").addClass("getcode-gray");
                    
                    fn_yuyin_css(0);
                    return false;
                }
        	}else{
        		$("#errorMessgP").html("您输入的手机号有误");
                $("#getcode").addClass("getcode-gray");
                fn_yuyin_css(0);
                return false;
        	}
        }
        
         $("#getcode").removeClass("getcode-gray");
         fn_yuyin_css(1);
         return true;
    } 
    
    
    function fn_yuyin_css(status){
        if(status==1){
            $("#sound-code-div").removeClass("code-div");
            $("#getcode2").removeClass("getcode2-gray");
            $("#getcode2").addClass("getcode2-blue");
        }else if(status==0){
            $("#sound-code-div").addClass("code-div");
            $("#getcode2").addClass("getcode2-gray");
            $("#getcode2").removeClass("getcode2-blue");
        }
    } 
    
    window.chageCodeImage = function (id){
		document.getElementById(id).src = base+"/account/validateCode.htm?d=" + new Date().getTime();
	};
</script>

