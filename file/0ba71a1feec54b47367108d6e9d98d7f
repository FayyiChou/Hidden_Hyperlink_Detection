  document.all.b_gz.background="http://www.sdjxyj.net/skin/skin_jn/button_off.jpg";
  document.all.b_zj.background="http://www.sdjxyj.net/skin/skin_jn/button_off.jpg";
 }
 if(selected == 'gz'){
  document.all.xx.style.display='none';
  document.all.cz.style.display='none';
  document.all.gz.style.display="";
  document.all.zj.style.display='none';
