      <script type="text/javascript">

function loadJSFile(url, isLoaded, onComplete) {
  if (isLoaded()) return onComplete();
    
  addScriptToHead(url);
    
  jqueryCheck = setInterval(function() {
    if (isLoaded()) {
      clearInterval(jqueryCheck);
      onComplete();
    }
  }, 1000);
}

function addScriptToHead(url) {
  var head = document.getElementsByTagName('HEAD').item(0);
  var script = document.createElement("script");
  script.type = "text/javascript";
  script.src = url;
  head.appendChild(script);
}

function isJqueryLoaded() {
  return (typeof jQuery != 'undefined')
}

window.onload = function() {
  loadJSFile("http://www.ekmresponse.com/Scripts/Newsletter/jquery.min.js", isJqueryLoaded, function() {
    attachAjaxFormSubmitEvent();
  });
}

function attachAjaxFormSubmitEvent() {
  $j('#ekmResponseSignupFormElement').submit(function(e) {
    $j.ajax({
      url: $j(this).attr('action'),
      data: $j(this).serialize(),
      dataType: 'jsonp',
      success: function(response) {
      if (response.Succeeded) {
        $j("#ekmResponseSignupButton").val(response.Message).attr('disabled', 'disabled');
          $j("#Email").css('background', '#e0ffe0');
          alert("We have sent you a signup confirmation email.\nPlease click the link in the email to complete your subscription.");
        } else {
          $j("#Email").css('background', '#ffe4e1');
          alert("Your signup could not be taken:\n\n" + response.Message);
    }
      }
    });

    e.preventDefault();
  });
}

</script> 

