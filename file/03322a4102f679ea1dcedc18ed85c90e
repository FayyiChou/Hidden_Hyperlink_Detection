function CheckForm(form){
	var errMsg = '';
	if (form.form_email.value == '' || form.form_email.value == form.form_email.defaultValue) { errMsg = 'Please enter your email address.'; }
	else if (form.form_password && (form.form_password.value == '' || form.form_password.value == form.form_password.defaultValue)) { errMsg = 'Please enter your password.'; }
	else if (form.form_orderid && (form.form_orderid.value == '' || form.form_orderid.value == form.form_orderid.defaultValue)) { errMsg = 'Please enter your order no.'; }
	if (errMsg != '') { alert('Correct the following issue before continuing:\n\r\n\r' + errMsg); return false; } else { return true; }
}
