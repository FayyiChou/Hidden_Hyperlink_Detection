<!--<script>
  
  $j(document).ready(function(){

    $j('.contact-form').each(function(){
        var errorMessageShowing = false;
        var form = $j(this);
        var action = form.attr('data-action');

        // Change the form action
        form.attr('action', action);

        $j(this).submit(function(event){
            var self = $j(this);
            var spamField = self.find('input.spam-field');
            var spamFieldVal = spamField.val();
            var formRow = spamField.parent('.frm-row');
            var errorMessage = $j('<div class="message-error-show">Please enter the correct number</div>');

            if(spamFieldVal != '35629'){
                event.preventDefault();
                if(!errorMessageShowing) formRow.append(errorMessage);
                errorMessageShowing = true;
            }
        });
    })

  });

</script>-->

