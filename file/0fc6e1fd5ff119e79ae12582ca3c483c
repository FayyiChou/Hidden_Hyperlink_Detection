                        <script type="text/javascript">
                            var div1 = document.getElementById("kejiTexts1");
                            var div2 = document.getElementById("kejiTexts2");
                            var title1 = document.getElementById("kejiTitle");
                            var title2 = document.getElementById("zhengceTitle");

                            function disply() {
                                div2.style.display = 'none';
                                title2.style.backgroundColor = '#F2F2F2'
                                div1.style.display = 'block';
                                title1.style.backgroundColor = '#FFFFFF'
                                title2.style.borderRight = 'solid #F2F2F2 1px'
                            }
                            function visbol() {
                                div1.style.display = 'none';
                                title1.style.backgroundColor = '#F2F2F2'
                                div2.style.display = 'block';
                                title2.style.backgroundColor = '#FFFFFF'
                                title2.style.borderRight = 'solid #CCCCCC 1px'
                            }
                    </script>
