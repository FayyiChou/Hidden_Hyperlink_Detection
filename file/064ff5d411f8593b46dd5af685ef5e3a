    //窗口改变事件
    function fixW() {
        var left = $("#main_dom").offset().left;
        left = Math.floor(left)>120 ? left-120 : 0;
        $("#fsk").stop(true,false).animate({"left":left},500);
    }
    window.onresize = fixW;

    $(function() {
        $("#rukou").find("li").hover(
            function() {
                $(this).find("span").css("color","#fff");
                $(this).css("background","#ce5a0d");
                if($(this).attr("id")=="sc")
                    $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_shou_hua.png");
                if($(this).attr("id")=="sc_after")
                    $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_shou_after.png");
                if($(this).attr("id")=="swshi")
                    $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_shang_hua.png");
                if($(this).attr("id")=="dingbu")
                    $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_hui_hua.png");
        },function() {
            $(this).find("span").css("color","#ce5a0d");
            $(this).css("background","#fff");
            if($(this).attr("id")=="sc")
                $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_shou.jpg");
            if($(this).attr("id")=="sc_after")
                $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_shou_after_hua.jpg");
            if($(this).attr("id")=="swshi")
                $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_shang.jpg");
            if($(this).attr("id")=="dingbu")
                $(this).find("img").attr("src","/public/cblcn/busiroom/images/ru_hui.jpg");
        });   
    });

    function goTop() {
        $('body,html').animate({ scrollTop:0 },50);
        return false;
    }

    $(function(){
        fixW();
        //判断是否登录
        jQuery.ajax({
            async:false,
            type:"post",
            url:"/cblcn/Home/logincheck",
            success:function(msg){
                $(".js_name").empty();
                if(msg!=null && msg.login_id!=""){
                    //判断是否已经收藏过该信息
                    jQuery.ajax({
                        async:false,
                        type:"post",
                        data:{ "info_ids":$("#infoId").val() },
                        url:"/cblcn/busiroom/hasSC",
                        dataType:"text",
                        success:function(msg){
                            if(msg=='true') {
                                $(".sc").hide();
                                $(".sc_after").show();
                            } else {
                                $(".sc_after").hide();
                                $(".sc").show();
                            }
                        }
                    });
                } else {
                    //收藏条
                    $(".sc_after").hide();
                    $(".sc").show();
                }
            }
        });
    });

    //收藏信息
    function scInfo(){
         //判断是否登录
        jQuery.ajax({
            async:false,
            type:"post",
            url:"/cblcn/Home/logincheck",
            success:function(msg) {
                if(msg==null || msg=="" || msg.login_id=="") {
                    location.href = "/cblcn/member.login/login?url=" + location.href;
                } else {
                    $(".sc").attr("disabled","true");
                    jQuery.ajax({
                        async:false,
                        type:"post",
                        data:{ "info_ids":$("#infoId").val() },
                        url:"/cblcn/busiroom/collectInfo",
                        dataType:"text",
                        success:function(msg){
                            if(msg=='true') {
                                $(".sc").hide();
                                $(".sc_after").show();
                            } else {
                                alert("你已收藏过该信息!");
                            }
                            $(".sc").removeAttr("disabled");
                        }
                     });
                }
            }
        });
    }

    //取消收藏
    function qxSCInfo() {
        $(".sc_after").attr("disabled","true");
        jQuery.ajax({
            type:"post",
            data:{ "info_ids":$("#infoId").val() },
            url:"/cblcn/busiroom/qxSCInfo",
            dataType:"text",
            success:function(msg) {
                if(msg=='true') {
                    $(".sc_after").hide();
                    $(".sc").show();
                }
                $(".sc_after").removeAttr("disabled");
            }
        });
    }

    function closeDialog(obj) {
        $(obj).parent().parent().hide();
    }
</script>
