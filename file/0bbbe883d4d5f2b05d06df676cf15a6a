<script language=javascript>
function chksmt(){
	var yhm=document.all.f.y;
	var pwm=document.all.f.p;
	var yzm=document.all.f.m;
	if ((yhm.value.length==0)||(yhm.value.length>=17)){
		alert('请输入16位以内的用户名!');
		yhm.focus();
		return false;
	}
	if ((pwm.value.length==0)||(pwm.value.length>=11)){
		alert('请输入10位以内的密码!');
		pwm.focus();
		return false;
	}
	if (yzm.value.length!=4){
		alert('请在验证码框中输入右侧的四位验证码!');
		yzm.focus();
		return false;
	}
	return true;
}
</script>


