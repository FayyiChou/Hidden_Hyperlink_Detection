$(document).ready(function(){
	getuserjoson();
	$("#KinSlideshow").KinSlideshow({
	isHasTitleBar:false,
		btn:{
	  btn_bgColor:"#35A2FF",
	  btn_bgHoverColor:"#0160B4",
      btn_fontColor:"#ffffff",
	  btn_fontHoverColor:"#ffffff",
	  btn_fontFamily:"Arial",
      btn_borderColor:"#0160B4",
	  btn_borderHoverColor:"#0160B4",
      btn_borderWidth:1,
	  btn_bgAlpha:1} 
	});

	  $("#userform").submit( function () {
	  		return checkuser();
		} );
});


function getuserjoson()
{
	
    
 
     var date = new Date();
    var ttime = date.getTime();
    
     $.get(
     "index.php?app=cifuser&mod=index&act=getuserjoson&ran="+ttime,
     { keyword: 1} ,
     function(data){
         		
             
             
               var jsonstr=eval(data);
			  
			   
			   
               if(jsonstr[0].uid>0)
			   {
			   		$("#isloginframe").show();
					$("#isnotloginframe").hide();
					$("#username_html").html(jsonstr[0].username);
			   }
			   else
			   {
			   		
					$("#isloginframe").hide();
					$("#isnotloginframe").show();
					
			   }
               
     }
     );



}

function checkuser()
{
	if($("#username").val()=="")
	{
		alert("请填写用户名");
		return false;
	}
	
	if($("#pw").val()=="")
	{
		alert("请填写密码");
		return false;
	}
	
	return true;
}

</script>
