<script>
var page = 2;

var load_flag =false;
$(window).scroll( function() {
	//cha = $(document).height()-$(window).height()- $(window).scrollTop();
	//$("#test_info").html(cha);
	totalheight = parseFloat($(window).height()) + parseFloat($(window).scrollTop());
	if ($(document).height() <= (totalheight+20) ) {  // 说明滚动条已达底部
		if(page==4){
			$(".boxlist4").append('<div style="width:100%; height:30px; line-height:30px; text-align: center; float:left;"><a href="/nba/all/5" target="_blank">查看全部图片</a></div>');
			page++;
			return false;
		}
		if(page>4){
			return false;
		}
    	if(load_flag)return false;
    	else load_flag = true;
		$.get("/nba/ajaxAll/"+page, function(data){
		  //alert("Data Loaded: " + data);
		  $(".boxlist4").append(data);
		  load_flag = false;
		  page++;
		});
	}
});

</script>


