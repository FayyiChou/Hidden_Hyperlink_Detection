        jQuery(document).ready(function() {
            jQuery("#eqfc").click(function() {
                $("#myserch").attr("action", "http://s.chinaomp.com/products/eqfc/search.php");
                $(".search-type li").removeClass("cur");
                $(this).addClass("cur");
                $("#typeinput").attr({"value":"products"});
                $("#websiteinput").attr({"value":"eqfc"});
                if($.browser.msie){
                    $("#keywordinput").attr({"value":"请输入关键字 如：产品名称、公司名称"});
                    $("#keywordinput").select();
                }else{
                    $("#keywordinput").attr({"placeholder":"请输入关键字 如：产品名称、公司名称"});
                    $('#keywordinput').focus();
                }

            });
            jQuery("#chinajtn").click(function() {
                $("#myserch").attr("action", "http://s.chinaomp.com/products/chinajtn/search.php");
                $(".search-type li").removeClass("cur");
                $(this).addClass("cur");
                $("#typeinput").attr({"value":"products"});
                $("#websiteinput").attr({"value":"chinajtn"});
                if($.browser.msie){
                    $("#keywordinput").attr({"value":"请输入关键字 如：产品名称、公司名称"});
                    $("#keywordinput").select();
                }else{
                    $("#keywordinput").attr({"placeholder":"请输入关键字 如：产品名称、公司名称"});
                    $('#keywordinput').focus();
                }

            });
            jQuery("#k6768").click(function() {
                $("#myserch").attr("action", "http://s.chinaomp.com/products/k6768/search.php");
                $(".search-type li").removeClass("cur");
                $(this).addClass("cur");
                $("#typeinput").attr({"value":"products"});
                $("#websiteinput").attr({"value":"k6768"});
                if($.browser.msie){
                    $("#keywordinput").attr({"value":"请输入关键字 如：产品名称、公司名称"});
                    $("#keywordinput").select();
                }else{
                    $("#keywordinput").attr({"placeholder":"请输入关键字 如：产品名称、公司名称"});
                    $('#keywordinput').focus();
                }
            });
            jQuery('.search-botton').click(function(){
                var URL = 'http://s.chinaomp.com/';
                var kw = $('#keywordinput').val();
                var type = $('#typeinput').val();
                var website = $('#websiteinput').val();
                var cat_id = $('#catidinput').val();
                if($.browser.msie) {
                    if(kw != '请输入关键字 如：产品名称、公司名称' ) {
                        var url = URL+type+'/'+website+'/search.php?cat_id='+cat_id+'&kw='+encodeURIComponent(kw);
                        window.open(url);
                    } else {
                        $('#keywordinput').select();
                    }
                }else{
                    if(kw != '') {
                        var url = URL+type+'/'+website+'/search.php?cat_id='+cat_id+'&kw='+encodeURIComponent(kw);
                        window.open(url);
                    } else {
                        $('#keywordinput').focus();
                    }
                }
            });
        });
