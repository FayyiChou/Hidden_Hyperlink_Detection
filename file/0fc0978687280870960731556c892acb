                function ajax_topNews(amount,level){
                    var $URL= 'http://www.chinaomp.com/Data_API/';
                    var AjaxUrl = $URL+'port.php?action=topNews&amount='+amount+'&level='+level+'&callback=?';
                    $.getJSON(AjaxUrl, function (Json) {
                        $("#toutiao_redian").html(Json.lists_html);
                    });
                }
                ajax_topNews(14);
                function ajax_hotNews(amount,level){
                    var $URL= 'http://www.chinaomp.com/Data_API/';
