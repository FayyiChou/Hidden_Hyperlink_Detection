	if(event.keyCode == 13) {
		var data = $(this).attr('innerText') + '\r\n' + '本文来自中国盲人安卓爱好者――为移动互联网信息无障碍而努力，地址： ' + location.href;		window.clipboardData.setData('text',data);
	}
}).find('a').attr("target","_blank");

