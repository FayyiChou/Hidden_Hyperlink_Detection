$(function(){
	$(regHtml).appendTo("body");
	$('.safe_level_box li').off("click").on("click",function(){ 
	    $(this).addClass('active').siblings('li').removeClass('active');
	});
	$('.attention-exam li').off("click").on("click",function(){
	    $(this).addClass('active').find('em').addClass('sel-active');
	    $(this).siblings('li').removeClass('active').find('em').removeClass('sel-active');
	});
	$('.ui-pop-title ul li').off("click").on("click",function(){ 
	     $(this).addClass('active').siblings('li').removeClass('active');
	     $('.ui-pop-content').find('form').eq($(this).index()).addClass('form_reg_show').siblings('form').removeClass('form_reg_show');
	});
	$(document).on("click", "#hidePopDiv", function(){
	    $("#popDiv").hide();
	    $("#blackMask").hide();
	});
	
	$(".pop-close").off("click").on("click",function(){ 
		$(this).parents(".pop").hide();
		$("#blackMask").hide();
		return false;
	});
	//ie下兼容placeholder
	$('input').placeholder({isUseSpan:true});
});
</script>

