<script type="text/javascript">
$.fn.myFloat = function() {
    var position = function(element) {
        var top = element.position().top;
        $(window).scroll(function() {
            var scrolls = $(this).scrollTop();
            if (scrolls > top) {
                if (window.XMLHttpRequest) {
                    element.css({
                        position: "fixed",
                        top: 0,
                        "z-index": 10 
                    });
                } else {
                    element.css({
                        position: "absolute",
                        top: scrolls,
                        "z-index": 10 
                    });
                }
            } else {
                element.css({
                    position: "static",
                    top: top
                });
            }
        });
    };
    return position($(this));
};
$("#float").myFloat();

$.fn.xcapFloat = function() {
	var changeCss = function(element) {
		var top = element.position().top;
		var id = element.attr("id");

		$(window).scroll(function() {
			var scrolls = $(this).scrollTop();
			if (top-$("#float").height()-$("#"+id).height() < scrolls  ) {
				$("a[name^=a_]").removeClass("cur");
				$("#a_"+id).addClass("cur");
			}
		});
	};
	return changeCss($(this));
};

$("#jieshao").xcapFloat();


(function(){
	$('a[name^=a_]').bind('click', function() {
		 $("a[name^=a_]").removeClass("cur");
		 $(this).addClass("cur");
		 var name = $(this).attr("name");
		 var id = name.substring(2,name.length);
		 if("static" == document.getElementById("float").style.position || "" == document.getElementById("float").style.position) {
		 	window.scrollTo(0,$("#"+id).position().top-$("#float").height());
		 } else {
			window.scrollTo(0,$("#"+id).position().top);
		 }
	 
	});
	$('#gotoJieshao').bind('click',function(){
		window.scrollTo(0,$("#float").position().top);
	});
	
}()) 
</script>
     
