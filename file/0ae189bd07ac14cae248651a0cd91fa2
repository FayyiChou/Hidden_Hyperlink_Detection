    convertMoney();
    function showmsg() {
        parent.Dialog.alert('您的单位在交易中心采购人库中已经注册，使用在交易中心注册的帐号或办理的CA证书进行登录！', function () { parent.location.href = 'http://oa.gzzb.gd.cn/SignOnServlet' });
    }
    function showTZ() {
        showDialog("通知公告", "ShowMsg.aspx", 800, 450);
        setTimeout(function () { Dialog.close(); }, 60000)
    }
    function showTZ2() {
        showDialog("通知公告", "ShowMsg2.aspx", 800, 450);
        setTimeout(function () { Dialog.close(); }, 60000)
    }
    //弹出层
    function showDialog(t, u, w, h) {

        var diag = new Dialog();
        diag.Title = t;
        diag.Width = w ? w : 600;
        diag.Height = h ? h : 500;
        diag.URL = u;
        diag.show();
    }
</script>
