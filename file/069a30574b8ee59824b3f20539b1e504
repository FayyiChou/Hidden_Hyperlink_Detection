        //滑鼠游標移出或移入時, 動態改變資料列的背景顏色和文字顏色
        function onmouseChangeRowColor(obj, IsOddLine, IsMouseOver) {
            if (obj) {
                if (IsOddLine) {
                    //單數列
                    if (IsMouseOver) {
                        //滑鼠移入
                        obj.style.backgroundColor = '#fffde2';

                    }
                    else {
                        //滑鼠移出
                        obj.style.backgroundColor = '#ffffff';

                    }
                }
                else {
                    //偶數列
                    if (IsMouseOver) {
                        //滑鼠移入
                        obj.style.backgroundColor = '#fffde2';

                    }
                    else {
                        //滑鼠移出
                        obj.style.backgroundColor = '#f4f4f4';
                    }
                }
            }
        }
        
    </script>
