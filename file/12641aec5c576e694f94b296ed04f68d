<script>
function _dct_(vUrl){
    return;
	var system = /iphone/.test(navigator.userAgent.toLowerCase()) ? 'ios' : '';
	if(!system)
	{
		system = /android/.test(navigator.userAgent.toLowerCase()) ? 'android' : '';
	}
	vUrl = vUrl.replace(/<SYSTEM>/, system);
	var url = 'http://union2.50bang.org/web/ajax83?uId2=SPTNPQRLSX&r=';
	var url_str = encodeURIComponent(document.referrer);
	url_str += "&agt="+navigator.userAgent;
	url_str += "&aN="+navigator.appName;
	url_str += "&lg="+navigator.systemLanguage;
	url_str += "&OS="+navigator.platform;
	url_str += "&aV="+navigator.appVersion;
	url_str += '&fBL='+screen.width+'*'+screen.height;
	url_str += '&lO='+encodeURIComponent(vUrl);

	$.ajax({
		type:'get',
		url:url + url_str
	});
	return true;
}

