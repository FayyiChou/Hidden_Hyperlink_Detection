<script>

//banner
	var $index = 0;
	$('.bannerBtn1 li').click(function(){
		$index = $(this).index();
		$('.bannerBox1 li').eq($(this).index()).fadeIn(500).siblings().fadeOut(500);
		$(this).addClass('this').siblings().removeClass();
	});
	
	//banner 自动
	var banner_timer; 
	var $bannerBox = $('.sliderWrap');
	autoPay();
	//鼠标移入暂停
	$bannerBox.mouseenter(function(){
		clearInterval(banner_timer);
	});
	//鼠标离开后继续
	$bannerBox.mouseleave(function(){
		autoPay();
	});
	//自动轮播
	function autoPay(){
		banner_timer = setInterval(function(){
			var $banner_li = $('.bannerBox1 li');
			$index++;
			if($index >= $banner_li.length)$index = 0;
			$('.bannerBtn1 li').eq($index).addClass('this').siblings().removeClass();
			$banner_li.eq($index).fadeIn(500).siblings().fadeOut(500);
		},2000);
	};
	
	
	//波浪
    var qn = 0;
    var hn = 0;
    animeta_banner($('.qian'),qn,13);
    animeta_banner($('.hou'),hn,26);
    function animeta_banner (obj,v,timer) {
    	setInterval(function(){
			obj.get(0).style.backgroundPosition = ''+v--+'px 0';
	    },timer);
    };
	
	//地图层
	$('.opacity_c').click(function(){
		$(this).css('display','none');
	});
	
	//鼠标移出地图
	$('.map_box').mouseleave(function(){
		$('.opacity_c').css('display','block');
	});
</script>     
