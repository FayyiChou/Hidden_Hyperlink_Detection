        $(function(){
            if($.browser.msie && $.browser.version < 10){
                $('body').addClass('ltie10');
            }
            $.fn.fullpage({
                verticalCentered: false,
                anchors: ['page1', 'page2', 'page3', 'page4', 'page5'],
				resize:true,
				afterLoad: function(anchorLink, index){
					if(index == 1){
						$('.navbar').addClass('show');
						$('.leftpage').addClass('leftpage section1').removeClass('section2 section3 section4 section5');
						
					}
					if(index == 2){
						$('.navbar').addClass('show');
						$('.leftpage').addClass('leftpage section2').removeClass('section1 section3 section4 section5');
						
					}
					if(index == 3){
						$('.navbar').addClass('show');
						$('.leftpage').addClass('leftpage section3').removeClass('section2 section1 section4 section5');
						
					}
					if(index == 4){
						$('.navbar').addClass('show');
						$('.leftpage').addClass('leftpage section4').removeClass('section2 section3 section1 section5');
						
					}
					if(index == 5){
						$('.navbar').addClass('show');
						$('.leftpage').addClass('leftpage section5').removeClass('section2 section3 section4 section1');
					}
				},
				onLeave: function(index){
					if(index == 1){
						$('.section1').removeClass('stepIndex');
					}
				},
                navigation: true,
				//menu: '#navbar'
            });
			
			$('.nav ul>li').eq(1).click(function(){
				//平台、设备和操作系统 
				var system ={
					win : false,
					mac : false, 
					xll : false 
				};    
				//检测平台    
				var p = navigator.platform;    
				system.win = p.indexOf("Win") == 0;    
				system.mac = p.indexOf("Mac") == 0; 
				system.x11 = (p == "X11") || (p.indexOf("Linux") == 0);  
				//跳转语句     
			  
				if(system.win||system.mac||system.xll){//转向后台登陆页面  
					$(this).find('a').attr('target','_blank');
					$(this).find('a').attr('href','http://cloudos.bingocloud.cn/');
			  
				}else{  
					$(this).find('a').attr('href','newweb/bingoCloudOS.html'); 
				}    
			})
			
        });

    </script>
	
