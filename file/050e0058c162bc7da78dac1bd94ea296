<script type="text/javascript">
            function BindControlEvents() {
                
                $("#mmenu").mmenu({
                    screenReader: true,
                    "navbars": [
                {
                    "position": "top",
                    "content": [
                       "searchfield"
                    ]
                }
                    ],
                    searchfield:{
                        add: true,
                        search: false
                    },
                    "extensions": [
                       "pagedim-black",
                       "theme-dark"
                    ],
                    "offCanvas": {
                        "position": "right",
                    },
                    "iconPanels": true,
            
                });

            

                $(window).scroll(function() {
                    if ($(this).scrollTop() > 80){  
                        $('#sticky-bar').addClass("sticky-show");
                    }
                    else{
                        $('#sticky-bar').removeClass("sticky-show");
                    }
                });

                $("#mmenu .mm-search input").attr('name', 'query').attr('class', 'autocomplete').wrap( "<form method=\"get\" action=\"/pages/search.aspx\"></form>" );

                $( ".autocomplete" ).autocomplete({
                    source: function(request, response) {
                        $.ajax({type: 'GET', url: "https://www.marketbeat.com/scripts/AutoComplete.ashx", dataType: 'text', data: { 'searchquery': request.term }, success: function(data) { response(JSON.parse(data)); }}); 
                    },
                    minLength: 2,
                    select: function(event, ui){
                        $('#txtAutoCompleteTwo').val(ui.item.data);
                        window.location = '/stocks/' + ui.item.data.replace(':','/') + '/';
                    }
                });
                $('table.tablesorter').DataTable({
                    responsive: true,
                    "info": false,
                    "searching": false,
                    "paging": false,
                    "order": []
                });
                $('table.ratingstable:not(.nodt)').DataTable({
                    responsive: true,
                    "info": false,
                    "searching": false,
                    "paging": false,
                    "order": []
                });
                $('table.priority7ratings').DataTable({
                    responsive: true,
                    columns: [
                        { responsivePriority: 1 },
                        { responsivePriority: 7 },
                        { responsivePriority: 2 },
                        { responsivePriority: 3 },
                        { responsivePriority: 4 },
                        { responsivePriority: 5 },
                        { responsivePriority: 6 }
                    ],
                    "info": false,
                    "searching": false,
                    "paging": false,
                    "order": []
                });
                $('.nav').css( "pointer-events", "auto" );
                $('.nav').css( "cursor", "pointer" );
             
                $.fn.dataTable.ext.errMode = 'none';
            }

            //Initial bind
            $(document).ready(function () {
                BindControlEvents();
            });

            //Re-bind for callbacks
            var prm = Sys.WebForms.PageRequestManager.getInstance(); 

            prm.add_endRequest(function() { 
                BindControlEvents();
            }); 

           
        </script>
 
