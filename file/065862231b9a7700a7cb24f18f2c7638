<script type="text/javascript" src="js/new/jquery.qrcode.min.js?t=20170327-01"></script>
<script type="text/javascript">
	window.iShare_config = {
		container : '.iShare',
		config : {
			title : '国家企业信用信息公示系统--详情',
			description : '国家企业信用信息公示系统--详情'
		// url: 'www.baidu.com', //this.location.href
		}
	};

	$(function() {
		if ($('#except_detail').offset() != null) {
			var left_dis = $('#except_detail').offset().left
					- $('#boxShadow1').offset().left;
			$('#box1').css('left', left_dis);
		}

		var qr_flag = true;
		var url = this.location.href;

		$('#qr_wechat').click(function() {
			if (qr_flag) {
				// 添加异常捕获
				try {
					// 设置参数方式
					$('#qrcode_wechat').qrcode({
						width : 200,
						height : 200,
						correctLevel : 0,
						render : "table",
						text : url
					});
				} catch (e) {
					alert("二维码生成失败，请刷新页面重试");
					return false;
				}

				qr_flag = false;
			}

			$('#qr_container').show();
			return false;
		});

		$('#qr_close_btn').click(function() {
			$('#qr_container').hide();
			return false;
		});
		
		$('#sendReportBtn').click(function() {
			popupCaptcha_JY('to_send_mail', function() {
				setTimeout(function() {
					$('.detail_bg').fadeIn(100);
					$('#dialogA1').slideDown(200);
				}, 1000);
			});
		});

		$('#to_send_mail').click(function() {
			if (!CheckMail($("#to_mail").val())) {
				$('#send_email_hint_info').text('邮箱格式不正确');
				// 提示信息两秒后消失
				setTimeout(function() {
					$('#send_email_hint_info').text('');
				}, 2000);
				return false;
			} else {
				// 弹窗隐藏				
				$('#send_mail_cancel').click();
				ajaxCommon({
					url : "notice/save_SendInfo",
					data : {
						"entUuid" : '',
						"mail" : $("#to_mail").val(),
						'geetest_challenge': $('#geetest_challenge').val(),
						'geetest_validate': $('#geetest_validate').val(),
						'geetest_seccode': $('#geetest_seccode').val()
					}
				}, function(data) {
					if (data == constants.yes) {
						window.top.dialogMessage("邮件发送成功！");
						return false;
					} else {
						window.top.dialogMessage("邮件发送失败！");
						return false;
					}
				});
			}
		});

		setIconYanse($('#iconOpState').text(), 'iconOpState');

		// 打印按钮 绑定 极验
		$('#infoPrintBtn').click(function() {
			popupCaptcha_JY('infoPrintBtn',function() {
				var srform = $('#formInfo');
				srform.attr('target', '_blank');
				srform.attr("action", "http://sh.gsxt.gov.cn/notice/notice/view_print?uuid=");
				srform.submit();
				srform.removeAttr('target');
			});
		});
	});

	function CheckMail(mail) {
		var filter = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+(.[a-zA-Z0-9_-])+/;
		if (filter.test(mail))
			return true;
		else {
			return false;
		}
	}

	/**
	 * 设置图片颜色
	 */
	function setIconYanse(opState, opStateId) {
		if (opState.indexOf('存续') >= 0) {
			$("#" + opStateId).removeClass().addClass("iconCunxu");
		} else if (opState.indexOf('吊销，未注销') >= 0) {
			$("#" + opStateId).removeClass().addClass("iconDiaoxiao");
		} else if (opState.indexOf('吊销，已注销') >= 0 || opState.indexOf('注销') >= 0) {
			$("#" + opStateId).removeClass().addClass("iconZhuxiao");
		} else if (opState.indexOf('迁出') >= 0) {
			$("#" + opStateId).removeClass().addClass("iconQianchu");
		} else {
			$("#" + opStateId).removeClass();
		}
	}

	function copyUrl() {
		$('#a_pageUrl').text('');
		var clipBoardContent = this.location.href;
		$('#a_pageUrl').text(clipBoardContent);

		if (isIE()) {
			var clipboardData = window.clipboardData; //for IE 
			clipboardData.setData("Text", clipBoardContent);
		} else {
			$('#a_hint_info').text('您使用的浏览器无法自动复制链接');
			$('#a_hint_bottom_info').text('请手动复制分享！');
		}
	}

	function isIE() { //ie?  
		if (!!window.ActiveXObject || "ActiveXObject" in window)
			return true;
		else
			return false;
	}
</script>
