<div id="popup-captcha"><div class="gt_input"><input class="geetest_challenge" type="hidden" name="geetest_challenge" value=""><input class="geetest_validate" type="hidden" name="geetest_validate" value=""><input class="geetest_seccode" type="hidden" name="geetest_seccode" value=""></div></div>
		
<script type="text/javascript">
$(function() {
	
	// 验证开始需要向网站主后台获取id，challenge，success（是否启用failback）
	$.ajax({
	    url: "pc-geetest/register?t=" + (new Date()).getTime(), // 加随机数防止缓存
	    type: "get",
	    dataType: "json",
	    success: function (data) {
	        // 使用initGeetest接口
	        // 参数1：配置参数
	        // 参数2：回调，回调的第一个参数验证码对象，之后可以使用它做appendTo之类的事件
	        initGeetest({
	            gt: data.gt,
	            challenge: data.challenge,
	            product: "popup", // 产品形式，包括：float，embed，popup。注意只对PC版验证码有效
	            offline: !data.success // 表示用户后台检测极验服务器是否宕机，一般不需要关注
	            // 更多配置参数请参见：http://www.geetest.com/install/sections/idx-client-sdk.html#config
	        }, handlerPopup);
	    }
	});

	
	
});

var captchaObjHtml;

var handlerPopup = function (captchaObj) {
	captchaObjHtml = captchaObj;
	// 成功的回调
    captchaObj.onSuccess(function () {
        var validate = captchaObj.getValidate();
        $.ajax({
            url: "pc-geetest/validate", // 进行二次验证
            type: "post",
            dataType: "json",
            data: {
                geetest_challenge: validate.geetest_challenge,
                geetest_validate: validate.geetest_validate,
                geetest_seccode: validate.geetest_seccode
            },
            success: function (data) {
                if (data && (data.status === "success")) {
                  $("#geetest_challenge").val(validate.geetest_challenge);
                  $("#geetest_validate").val(validate.geetest_validate);
                  $("#geetest_seccode").val(validate.geetest_seccode);
                  qry.searchsubmit();
                } else {
                	/* window.top.dialogMessage("验证码校验失败！"); */
                	window.top.dialogMessage("请刷新页面重试！");
                }
            }
        });
    });
	
    // 将验证码加到id为captcha的元素里
    captchaObj.appendTo("#popup-captcha");
    // 更多接口参考：http://www.geetest.com/install/sections/idx-client-sdk.html
};


</script>
			     
			</div>
		</div>
	</div>
</div>

<script type="text/javascript">
//修复 ie 9 以下 input 不支持 placeholder, 需要放在 input 标签之后才会生效
jQuery('[placeholder]').focus(function() {
    var input = jQuery(this);
    if (input.val() == input.attr('placeholder')) {
        input.val('');
        input.removeClass('placeholder');
    }
}).blur(function() {
    var input = jQuery(this);
    if (input.val() == '' || input.val() == input.attr('placeholder')) {
        input.addClass('placeholder');
        input.val(input.attr('placeholder'));
    }
}).blur().parents('form').submit(function() {
    jQuery(this).find('[placeholder]').each(function() {
        var input = jQuery(this);
        if (input.val() == input.attr('placeholder')) {
        	input.val('');
        }
    })
});

function queryTopSearchFormInfo() {
	// 搜索类型：1-企业信用信息、2-经营异常名录、3-严重违法企业名单、4-抽查检查信息、5-信息公告
	// 确定查询的类型
	var type = $.trim($('.navSearch1 .current').text());
	
	if(constants.yes == constants.capthca_gettype){
		if ("企业信用信息" == type) {
			$("#searchType").val('1');
			qry.search('#keyword', '1', constants.yes, constants.yes,captchaObjHtml,constants.no);
			//qry.search('#keyword', '1', constants.yes, constants.yes,'',constants.yes,'buttonSearch1');
		} else if ("经营异常名录" == type) {
			$("#searchType").val('2');
			qry.search('#keyword', '2', constants.yes, constants.yes,captchaObjHtml,constants.no);
			//qry.search('#keyword', '2', constants.yes,constants.yes,'',constants.yes,'buttonSearch1');
		} else if ("严重违法失信企业名单" == type) {
			$("#searchType").val('3');
			qry.search('#keyword', '3', constants.yes, constants.yes,captchaObjHtml,constants.no);
			//qry.search('#keyword', '3', constants.yes,constants.yes,'',constants.yes,'buttonSearch1');
		}
	}else if(constants.no == constants.capthca_gettype){
		if ("企业信用信息" == type) {
			$("#searchType").val('1');
			//qry.search('#keyword', '1', constants.yes, constants.yes,captchaObjHtml,constants.no);
			qry.search('#keyword', '1', constants.yes, constants.yes,'',constants.yes,'buttonSearch1');
		} else if ("经营异常名录" == type) {
			$("#searchType").val('2');
			//qry.search('#keyword', '2', constants.yes, constants.yes,captchaObjHtml,constants.no);
			qry.search('#keyword', '2', constants.yes,constants.yes,'',constants.yes,'buttonSearch1');
		} else if ("严重违法失信企业名单" == type) {
			$("#searchType").val('3');
			//qry.search('#keyword', '3', constants.yes, constants.yes,captchaObjHtml,constants.no);
			qry.search('#keyword', '3', constants.yes,constants.yes,'',constants.yes,'buttonSearch1');
		}
    } else {
		if ("企业信用信息" == type) {
			$("#searchType").val('1');
			qry.search('#keyword', '1', constants.yes, '0');
		} else if ("经营异常名录" == type) {
			$("#searchType").val('2');
			qry.search('#keyword', '2', constants.yes, '0');
		} else if ("严重违法失信企业名单" == type) {
			$("#searchType").val('3');
			qry.search('#keyword', '3', constants.yes, '0');
		}
	}
}

