<script type="text/javascript">
	function toTab(tabId) {
		/* window.open("http://www.w3school.com.cn", '_self'); */
		window.location = "http://sh.gsxt.gov.cn/notice/notice/view?uuid=&tabPanel="
				+ tabId;
	}

	$(function() {
		// 模拟分页效果
		$(".page-container").each(function() {
			var c = $(this);
			var i = c.parents("table").find(".page-item");
			var s = c.attr("rel") || 5; // 默认每页5条数据
			paging.simulate.init({
				container : c,
				items : i,
				size : s
			});
			// 添加变更信息
			c.parents("table").find(".rel-page-item").each(function() {
				var rel = $(this).attr('rel');
				$('#' + rel).after($(this)[0].outerHTML);
				//console.log($(this)[0].outerHTML);
				$(this).remove();
			});
		});

		//国家局记录访问日志  需要在企业详情页面加载完资源后被调用
		if (constants.yes == "1") {
			Collect.t2Collect_gsxt('');
		}
		
		// 初始化 table 折叠
		Table.collapse({selector: 'table tr td.collapse div'});
	});
</script>
