        <script type="text/javascript">
            var x = 50, y = 60;
            var xin = true, yin = true;
            var step = 1;
            var delay = 10;
            var obj = document.getElementById("www_CsrCode_cn");
            function floatwww_CsrCode_cn() {
                var L = T = 0;
                var R = window.screen.width - obj.offsetWidth;
                var B = window.screen.height - 300 - obj.offsetHeight;
                obj.style.left = x + document.body.scrollLeft + "px";
                obj.style.top = y + document.body.scrollTop + "px";
                x = x + step * (xin ? 1 : -1);
                if (x < L) { xin = true; x = L; }
                if (x > R) { xin = false; x = R; }
                y = y + step * (yin ? 1 : -1)
                if (y < T) { yin = true; y = T; }
                if (y > B) { yin = false; y = B; }
            }
            //var itl = setInterval("floatwww_CsrCode_cn()", delay);
            //obj.onmouseover = function () { clearInterval(itl) }
            //obj.onmouseout = function () { itl = setInterval("floatwww_CsrCode_cn()", delay) }
        </script>
