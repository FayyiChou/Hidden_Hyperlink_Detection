<script type="text/javascript">
document.oncontextmenu=function(e){
    var target = e.target;
    if(target.nodeName=='IMG'||target.nodeName=='img'){
        if(target.getAttribute('src'))
        {
            console.log(target.getAttribute('src'));
        }
        return false;
    }else{
      return true;
    }       
}
//document.oncontextmenu=new Function("event.returnValue=false"); 
//document.onselectstart=new Function("event.returnValue=false"); 
    $(function () {
    	$('#navs li').hover(function(){
			/*$nwidth = $(this).find('.menu li').width() * $(this).find('.menu li').size();
			$(this).find('.menu').width($nwidth);*/
			$(this).find('.menu').css("opacity","1");
			$(this).find('.menu').slideDown();
			$(this).addClass("on");
		},function(){
			$(this).find('.menu').css("opacity","0");
			$(this).stop().find('.menu').slideUp();
			$(this).removeClass("on");
		});	
    	$(".lazy").lazyload({
    		effect: "fadeIn"
    	});
    	$("#gotop").click(function () {
            $('html,body').animate({ scrollTop: 0 }, 600);
        })
    });   
</script>






