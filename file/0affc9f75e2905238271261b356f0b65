    <script type="text/javascript">
        if (top.location != location) top.location.href = location.href;    //noframe
        window.moveTo(0,0);
        window.resizeTo(this.screen.width,this.screen.height-20);
     function Enter2Tab(){
		var e = document.activeElement;
		if(	e.tagName == "INPUT" && 
			(
			e.type == "text"     ||
			e.type == "password" ||
			e.type == "checkbox" ||
			e.type == "radio"
			)   ||
			e.tagName == "SELECT")

		{
			if(window.event.keyCode == 13)
			{
				window.event.keyCode = 9;
			}
		}
}
 
    Ext.get("txtPwdSel").on("focus", function () {
        var account = document.getElementById("txtAccountSel").value + "_reseller";
        if (account) {
            Ext.Ajax.request({
                url: '../ValidateCode.ashx',
                method: 'post',
                params: {
                    account: account
                },
                success: function (response, option) {
                    var valueback = response.responseText;
                    if (valueback == 1) {
                        document.getElementById("validateCodeArea").style.display = "";
                    }
                    else {
                        document.getElementById("validateCodeArea").style.display = "none";
                    }
                }
            });
        }
    })

     function view(id,el){
        Ext.Win().open('../Distributor/Sys_NewsView.aspx?id=' + id, '浏览新闻公告', {target:el,width:400, height:300, resizable:'no', maximize:'no', minmize:'no',scrollbars:'yes'});
    }
     function ChangeCode()
     {
         document.getElementById('validataCode').src = document.getElementById('validataCode').src + '?';
     }
    function plsLogin()
    {
        alert("请先登录旅游分销平台！");
        $("txtAccountSel").select();
    }
    
    function register(){
        Ext.Win().open('Register.aspx', '同行注册', {width:580, height:480, resizable:'no', maximize:'no', minmize:'no'});
    }
    function CheckLogin(){
		if ($F("txtAccountSel").Trim() == ""){
			alert("请输入登录名");
			$("txtAccountSel").focus();
			return false;
		}
		if ($F("txtPwdSel").Trim() == ""){
			alert("请输入密码");
			$("txtPwdSel").focus();
			return false;
        }
        if (document.getElementById("validateCodeArea").css("display") === 'block') {
            if ($F("ValidCode").Trim() == "") {
                alert("请输入验证码");
                $("ValidCode").focus();
                return false;
            }
        }
    } 
    try {
    var pageTracker = _gat._getTracker("UA-56077-7");
    pageTracker._trackPageview();
    } catch(err) {}


          
