<script type="text/javascript">
	
	jQuery(document).ready(function(){
		var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-detail');
		var newsletterSubscriberFormDetailCms = new VarienForm('newsletter-validate-detail-cms');
		jQuery('#newsletter-validate-detail,#newsletter-validate-detail-cms').on('submit',function(event){
			event.preventDefault();
			url = jQuery(this).attr('action');
		if(event.target.id == 'newsletter-validate-detail'){
			var emailId = jQuery('#newsletter').val();
			data = {isAjax:1,email:emailId};
			if(newsletterSubscriberFormDetail.validator.validate()){
				jQuery.ajax({
					url:url,
					data:data,
					dataType:'json',
					type:'POST',
					success:function(result){
						jQuery('#confirmation_message .message > p').html(result.message);

						jQuery('#open_message_popup').fancybox({
						width:320,
						height:'auto',
						autoSize:false,
						padding:0
						}).click();
						
						if(jQuery('body').hasClass(' checkout-onepage-index')){
						     if(jQuery('#left-col-fixed').length){
								 jQuery.intelligentSidebar.init('left-col-fixed');
							 }
						}

					},
					error:function(xhr,textStatus,thrownError){
						alert(thrownError);
					}
				});
			}
		}else{
			var emailId = jQuery('#newsletter_email').val();
			data = {isAjax:1,email:emailId};
			if(newsletterSubscriberFormDetailCms.validator.validate()){
				jQuery.ajax({
					url:url,
					data:data,
					dataType:'json',
					type:'POST',
					success:function(result){
						jQuery('#confirmation_message .message > p').html(result.message);

						jQuery('#open_message_popup').fancybox({
						width:320,
						height:350,
						autoSize:false,
						padding:0
						}).click();
						
						if(jQuery('body').hasClass(' checkout-onepage-index')){
						     if(jQuery('#left-col-fixed').length){
								 jQuery.intelligentSidebar.init('left-col-fixed');
							 }
						}

					},
					error:function(xhr,textStatus,thrownError){
						alert('error');
					}
				});
			}
		}
		});

		jQuery('.continue-shopping-button > a').on('click',function(){
			jQuery.fancybox.close();
		});
	});
</script>

