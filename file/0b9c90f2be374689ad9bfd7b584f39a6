    <script>

        var copy = function (o) {
            var clip = [];
            var arr = jQuery('#' + o).attr('id').split('_');
            var i = arr[1];
            clip[o] = new ZeroClipboard.Client();
            clip[o].setHandCursor(true);
            clip[o].addEventListener('complete', function () {
                alert('复制成功!');
            });
            if (arr[0] == 'tao') {
                var codestr = '#tao_' + i;
                clip[o].setText(jQuery(codestr).val());
                clip[o].glue('ctp_' + i);
            } else {
                clip[o].setText(jQuery('#code_0').val());
                clip[o].glue('cp_0');
            }
        }

        var showBox = function (url, title) {
            art.dialog.open(url, {title: title || '', top: '45%', lock: true, resize: false});
        }

        var showMsg = function (content) {
            var myDialog = art.dialog({
                lock: true,
                icon: 'warning',
                top: '30%'
            });
            myDialog.content(content);
        }

        var errorHandler = function (error) {
            switch (error) {
                case -22:
                    showMsg('您的<strong>ip地址</strong>已经领取过此卡,<strong style="color:red;">半小时</strong>之后可继续领取');
                    break;
                case -2:
                    showMsg('已领取过');
                    break;
                case -3:
                    showMsg('请不要频繁操作,15秒后您可以再次淘号.');
                    break;
                case -4:
                    showMsg('已预订过');
                    break;
                case -5:
                    showMsg('卡片已领完');
                    break;
                case -6:
                    showMsg('暂时没有卡片可淘,请稍后再试');
                    break;
                case -7:
                    showMsg('此卡需要登录,<a style="color:red;" href="http://u.uuu9.com">点此登录</a>');
                    break;
                case -8:
                    showMsg('发卡已关闭,谢谢参与');
                    break;
                case -10000:
                    showMsg('操作未定义');
                    break;
                default :
                    showBox('http://ka.uuu9.com/Public/tips/' + error + '.html');
                    break;
            }
        }

        jQuery(function () {
            jQuery("#action a").click(function () {
                var url = 'http://ka.uuu9.com/index.php?m=Ajax&a=code';
                var arr = jQuery(this).attr('query').split('|');
                var data = {
                    act: arr[0],
                    id: arr[1]
                }
                var that = this;
                jQuery.post(url, data, function (response) {
                    try {
                        var r = jQuery.parseJSON(response);
                        if (jQuery.trim(r.error).length > 0) {
                            jQuery(that).siblings('a').removeClass('this').end().removeClass('this');
                            errorHandler(r.error);
                            return false;
                        } else {
                            jQuery(".resultbox").show();
                            jQuery(".lingkabox").hide();
                            jQuery(".taokabox").hide();
                            var clip;
                            if (r.act == 'ling') {
                                jQuery('#code_0').val(r.codes[0]);
                                jQuery(".lingkabox").show();
                                copy('code_0');
                            } else if (r.act == 'tao') {
                                var clip = [];
                                jQuery(".taokabox").show();
                                for (var i in r.codes) {
                                    jQuery("#tao_" + i).val(r.codes[i].code);
                                    copy('tao_' + i);
                                }

                            } else if (r.act == 'yu') {
                                jQuery(".resultbox").html('恭喜您，您已预订该卡');
                            }
                            jQuery(that).siblings('a').removeClass('this').end().addClass('this');
                        }
                    } catch (e) {
                        myDialog = art.dialog({
                            lock: true,
                            icon: 'warning',
                            top: '30%'
                        });
                        r = response;
                        myDialog.content(r);
                    }
                });
            });


        });
    </script>
