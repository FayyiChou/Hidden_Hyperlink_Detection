                            <script type="text/javascript">
                                javascript: News_Show(1)
                                function News_Show(id) {
                                    var title_local = document.getElementById("title_local");
                                    var title_pic = document.getElementById("title_pic");
                                    var title_province = document.getElementById("title_province");
                                    var ul_LocalNews = document.getElementById("ul_LocalNews");
                                    var ul_PicNews = document.getElementById("ul_PicNews");
                                    var ul_ProvinceNews = document.getElementById("ul_ProvinceNews");
                                   // var NewsMore = document.getElementById("NewsMore");

                                    title_local.style.borderBottom = "#ffffff solid 1px;";
                                    title_pic.style.borderBottom = " #ffffff solid 1px;";
                                    title_province.style.borderBottom = " #ffffff solid 1px;";
                                    title_local.style.color = "#B1C1C6";
                                    title_pic.style.color = "#B1C1C6";
                                    title_province.style.color = "#B1C1C6";
                                    ul_LocalNews.style.display = "none";
                                    ul_PicNews.style.display = "none";
                                    ul_ProvinceNews.style.display = "none";
                                    if (id == 1) {
                                        title_local.style.borderBottom = " #ef8742 solid 2px;";
                                        ul_LocalNews.style.display = "block";
                                        //NewsMore.href = "xwgk.asp?ClassId=26";
                                        title_local.style.color = "#25276F";
                                    }
                                    else if (id == 2) {
                                        title_pic.style.borderBottom = " #ef8742 solid 2px;";
                                        ul_PicNews.style.display = "block";
                                        //NewsMore.href = "xwgk_list.asp?ClassId=68";
                                        title_pic.style.color = "#25276F";
                                    }
                                    else if (id == 3) {
                                        title_province.style.borderBottom = " #ef8742 solid 2px;";
                                        ul_ProvinceNews.style.display = "block";
                                        //NewsMore.href = "xwgk_list.asp?ClassId=69";
                                        title_province.style.color = "#25276F";
                                    }
                                }
                            </script>




                        

