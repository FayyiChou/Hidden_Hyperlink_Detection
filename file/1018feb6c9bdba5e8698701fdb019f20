    $(function () {
        $(".menu .li_li").mouseover(
       function () {
           $(this).find("img").attr("src", "images/bottom_pic.png");
       }).mouseleave(function () {
           $(this).find("img").attr("src", "images/left_pic.png");
       });
    });
</script>







