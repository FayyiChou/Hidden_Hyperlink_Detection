var qihuan0608 = document.getElementById("qihuan0608").getElementsByTagName("img");
var _l_len = qihuan0608.length;
var _aNumber = 0;
var _aNumber_i = 0;
// http://www.eccnmall.com/Infineon/http://www.eccnmall.com/qinke/

for(var i=0;i<_l_len;i++){

	qihuan0608[i].onmousedown = function(){

		document.getElementById("qihuan0608").setAttribute("href",this.getAttribute("rel"));
	};
}
var _set = setInterval(function(){

	if(_l_len>0){

		for(var i=0;i<_l_len;i++){
			qihuan0608[i].style.display = "none";
		}

		if(qihuan0608[_aNumber]){
			qihuan0608[_aNumber].style.display = "inline";
		}

		_aNumber++;
		_aNumber_i++;
		if(_aNumber>=_l_len){
			_aNumber=0;
			_aNumber_i = 0;
		}

	}
},2000)



