<script type="text/javascript">
var test1 = document.getElementById("Wearable-index");
    var seepa = 100;
    var test1height = 320;
    var time;
    function timeHdong(){
        var test1height_all = test1.offsetHeight;

            if(test1height_all>0 && test1height_all != 0){
                test1height = Math.floor(test1height-seepa/8);
				try{
					test1.style.height = test1height+"px";
					document.getElementById("flashImages").style.height = test1height+"px";
					document.getElementById("jiqren").style.display = "none";

				}
				catch(e){
					//
				}
                time = setTimeout(function(){
                    timeHdong();
                },100)
                seepa = seepa+1;
                //document.getElementById("text").innerHTML = test1height;
            }
           if(test1height<=0){
                clearTimeout(time);
                test1.style.display = "none";
				var images_denglong = document.getElementById("images_div");

				var window_screen_width = window.screen.width;
				var numberPX = 202;
				var settime = setInterval(function(){
						//setimages();
					},100)
				var numberbigid = 2;
				var bigid = document.getElementById("bigid");
				var atl_close = document.getElementById("atl_close");
					bigid.style.display = "none";
					images_denglong.style.display = "none";
					atl_close.onmousedown = function(){
						images_denglong.style.display = "none";
						bigid.style.display = "none";
					}
				function setimages(){
					numberPX = numberPX+6;
					numberbigid = numberbigid+6;
					images_denglong.style.top = numberPX+"px";
					bigid.style.height = numberbigid+"px";
					if(numberPX>=400){
						clearInterval(settime);
					}
				}
            }


    }
    setTimeout(function(){
		timeHdong()
	},10000)



</script>
