		var q = document.form1.q;
		qvalue = q.value;
		q.onblur = function(){
			if(q.value == ""){
				q.value = qvalue;
			}
		}
		q.onfocus = function(){
			q.value = "";
		}

	ICdata_list();
function ICdata_list(){
	var ICdata_list = document.getElementById("ICdata_list");
	var ICdata_listul0 = ICdata_list.getElementsByTagName("ul")[0];
	var ICdata_listul0li = ICdata_list.getElementsByTagName("ul")[0].getElementsByTagName("li");
	var ICdata_listul1 = ICdata_list.getElementsByTagName("ul")[1];
	var ICdata_listul1li = ICdata_list.getElementsByTagName("ul")[1].getElementsByTagName("li");
		//onmousrDown1("toosname","subseach1")
		//onmousrDown1("kamkaifa","searchkamkaifa")
		function onmousrDown1(id1,id2){
		document.getElementById(id1).onmousedown = function(){
			document.getElementById(id2).style.display = "inline";
			onmousrDown2(id1,id2)

		}
	}
	function onmousrDown2(id1,id2){

			document.getElementById(id1).onmousedown = function(){
			document.getElementById(id2).style.display = "none";
			onmousrDown1(id1,id2)

		}

	}
	for(n in ICdata_listul0li){
		if(ICdata_listul0li[n]){
				ICdata_listul0li[n].onmouseover = function(){
						for(var n = 0; n<=ICdata_listul0li.length;n++){
							if(ICdata_listul0li[n]){
								ICdata_listul0li[n].style.backgroundColor = "#AAAAAA";
								document.getElementById(ICdata_listul0li[n].getAttribute("rel")).style.display = "none";

								}
						}
						this.style.backgroundColor = "#C3141B"
						document.getElementById(this.getAttribute("rel")).style.display = "inline";
				}
		}
	}

}
getElementsByClassNameAll()
function getElementsByClassNameAll(){
	var div = document.getElementsByTagName("div");//supporClass

	for(a in div){
		if(div[a]){
			//alert(div[a].className);

			if(div[a].className == "supporClass"){
				//alert(1);
			}
		}
	}
}


//supporClass
function tdTancang(div,classa){
	var divname = document.getElementsByTagName("div");
	for( a in divname){
		if(divname[a]){
			if(divname[a].className == classa){
				return divname[a];
			}
		}
	}
}

