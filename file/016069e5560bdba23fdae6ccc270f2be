<script>
	$(function(){
		$(".oss_ul").find("li").each(function(){
			$(this).hover(function(){
                var orgimg = $(this).find("img").attr("src");
				var myimg = $(this).find("img").attr("bigimg");
				if(myimg)
				{
					$(this).addClass("selected").siblings().removeClass("selected");
					$(this).find("img").attr("src", myimg).attr("bigimg", orgimg);
				}
            }, function(){
                var orgimg = $(this).find("img").attr("src");
				var myimg = $(this).find("img").attr("bigimg");
				if(myimg)
				{
					$(this).removeClass("selected");
					$(this).find("img").attr("src", myimg).attr("bigimg", orgimg);
				}
            });
		});
	});
</script>

