function yangpianshenq(dbn){
	for(n in dbn){
		if(dbn[n]){
			dbn[n].onmousedown = function(){
			var hrefi = this.getElementsByTagName("a")[0].getAttribute("hre");
			var n = this.getElementsByTagName("a")[0].innerHTML;
			document.getElementById("ads").style.display = "inline";
			document.getElementById("ads").innerHTML = "<iframe src='/regedit/user_info_index.php?id="+n+"&url="+hrefi+"'  scrolling='no' frameborder='0' width='100%' height='180'></iframe>"
			//document.getElementById("ads").innerHTML = hrefi;
			}
		}
	}
}

function closeMe_div(){
document.getElementById("ads").style.display = "none";
}
document.getElementById("abssrarch").onclick = function(){
	if(document.getElementById("abssrarch1").style.display == "inline"){
		document.getElementById("abssrarch1").style.display = "none";
	}else{
	document.getElementById("abssrarch1").style.display = "inline";
	}
}
//closeID("cloose_zhuti","Wearable-index");
closeID("theme_colse","theme");
closeID("colsee","ad_winn");

function closeID(){
	var argumentsID = arguments[0];
	var argumentsIDnone = arguments[1];
	document.getElementById(argumentsID).onclick = function(){

		document.getElementById(argumentsIDnone).style.display = "none";
	}
}

