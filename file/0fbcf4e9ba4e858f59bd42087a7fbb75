<script language="javascript" type="text/javascript">

    function loadCustomGoodsList(){
        $(".customGoodsList").each(function () {
            if( $(this).attr("data-json")){
                var itemList = eval("("+$(this).attr("data-json")+")");
                var id = itemList.siteUserCustomGoodsId;
                if(itemList && itemList.goodsIds) {
                    $.ajax({
                        type: "POST",
                        url: "/shop/ajaxSiteModuleContext.html",
                        data: {"shopId": $("#shopId").val(), "goodsIds": itemList.goodsIds},
                        dataType: "html",
                        success: function (result) {
                            $("#customGood" + id).html(result);
                        }
                    });
                }
            }
        });
    }
    loadCustomGoodsList();


    var pvareacookie = {init: function () {
        var pvareaid = this.getQueryString("pvareaid");
        var referrer = document.referrer;
        try {
            if (this.getHost(referrer).toLowerCase() == "mall.autohome.com.cn" || this.getHost(referrer).toLowerCase() == "m.mall.autohome.com.cn" || this.getHost(referrer).toLowerCase() == "mall.m.autohome.com.cn" || this.getHost(referrer).toLowerCase() == "1111.autohome.com.cn" || this.getHost(referrer).toLowerCase() == "m.1111.autohome.com.cn") {
                return
            }
        } catch (e) {
        }
        if (pvareaid != null && parseInt(pvareaid) > 0) {
            var date = new Date();
            date.setHours(date.getHours() + 12);
            document.cookie = "mpvareaid=" + parseInt(pvareaid) + "; expires=" + date.toGMTString() + "; path=/; domain="+document.domain;
        }
    }, getQueryString: function (name) {
        var ah_pvareaid = 0;
        var reg = new RegExp("(^|&|#)" +name+ "=([0-9]*)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null){
            ah_pvareaid = unescape(r[2]);
        }
        r = window.location.hash.substr(1).match(reg);
        if (r != null){
            ah_pvareaid = unescape(r[2]);
        }
        return ah_pvareaid;


    }, getHost: function (url) {
        var regex = /.*\:\/\/([^\/]*).*/;
        var match = url.match(regex);
        if (match != null)return match[1];
        return null
    }};
    pvareacookie.init();

	function getCookie(name) {
	    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");

	    if(arr=document.cookie.match(reg))
	        return unescape(arr[2]);
	    else
	        return "";
	}


    var pvTrack = function () { };
    pvTrack.site = 58;
    pvTrack.category = 582;
    pvTrack.subcategory = 3335;
    pvTrack.objectid = 83112629;
    pvTrack.seriesid = 0;
    pvTrack.specid = 0;
    pvTrack.type = 3
    pvTrack.typeid = 0;
    pvTrack.site_ref = getCookie("mpvareaid");
    pvTrack.level = 0;
    pvTrack.dealer = 83112629;
</script>

<script language="javascript" type="text/javascript">
var url_stats = "http://stats.autohome.com.cn/pv_count.php?SiteId=";
(function () {
    if (typeof (pvTrack) != "undefined" && pvTrack != null) {
        setTimeout("func_stats()", 3000);
        var doc = document,
                t = pvTrack;
        var pv_site, pv_category, pv_subcategory, pv_object, pv_series, pv_type, pv_typeid, pv_spec,pv_site_ref, pv_level, pv_dealer, pv_ref, pv_cur;
        pv_ref = escape(doc.referrer);
        pv_cur = escape(doc.URL);
        pv_site = t.site;
        pv_category = t.category;
        pv_subcategory = t.subcategory;
        pv_object = t.objectid;
        pv_series = t.seriesid;
        pv_type = t.type;
        pv_typeid = t.typeid;
        pv_spec = t.specid;
        pv_site_ref = t.site_ref;
        pv_level = t.level;
        pv_dealer = t.dealer;
        url_stats += pv_site + (pv_category != null ? "&CategoryId=" + pv_category : "") + (pv_subcategory != null ? "&SubCategoryId=" + pv_subcategory : "") + (pv_object != null ? "&objectid=" + pv_object : "") + (pv_series != null ? "&seriesid=" + pv_series : "") + (pv_type != null ? "&type=" + pv_type : "") + (pv_typeid != null ? "&typeid=" + pv_typeid : "") + (pv_spec != null ? "&specid=" + pv_spec : "") + (pv_site_ref != null ? "&site_ref=" + pv_site_ref : "") + (pv_level != null ? "&jbid=" + pv_level : "") + (pv_dealer != null ? "&dealerid=" + pv_dealer : "") + "&ref=" + pv_ref + "&cur=" + pv_cur + "&rnd=" + Math.random();
        var len_url_stats = url_stats.length;
    }
})();
document.write('<iframe id="PageView_stats" src="" style="display:none;"></iframe>');
function func_stats() {
    document.getElementById('PageView_stats').src = url_stats;
}



jQuery(function(){
    // $("#btn_buy,#btn_buy_float,#detail-ask-submit,#f1").attr("target","_self");
    jQuery.ajax({
        type: "post",
        url: "http://store.mall.autohome.com.cn/site/test.jtml",
        contentType: "application/x-www-form-urlencoded; charset=UTF-8",
        dataType:"html",
        success: function (d) {
            jQuery("#remote_header").html(d);
            replaceHeader();
            jQuery("#remote_header").remove();
            reBound();
        }
    });

 	// 店铺信息浮动
    $("#js_header-dp").mouseenter(function(){
        $("#js_dp-popup").show();
    }).mouseleave(function(){
        $("#js_dp-popup").hide();
    })

});

function replaceHeader(){
    //miniheader
    var $miniheaders = jQuery(".miniheader");
    $($miniheaders[0]).html($($miniheaders[1]).html());

    //login
    var $logindiv = jQuery("#login");
    var $loginlocal = jQuery("#login_local");
    $($loginlocal).html($($logindiv).html());
}


/**
 红包逻辑start
 */
// 领取红包后确定
$("#layer-redPacket-alert .btn-red").click(function(){
    $("#layer-redPacket-alert").layer("hide");
});

$(function(){
    getCouponInfo();
});
function getCouponInfo(){
    var coupondata ={
        providerId:'83112629'
    }
    $.ajax({
        type: "GET",
        url: "/shop/getCouponInfo.jtml",
        data: coupondata,
        dataType:"json",
        success: function(resultData){
            if(resultData&&resultData.returncode==0){
                var result = resultData.result;
                if(result&&result.returncode&&result.returncode==1000){
                    if(result.resultList && result.resultList.length > 0){
                        buildCouponHTML(result.resultList);
                    }
                }
            }

        }
    });
}

/**
 * 目前店铺只允许发一张券（取返回list中第一个）
 * */
function buildCouponHTML(resultList){
    var couponHTML = '';
    var maxCoupon = 0;
    if(resultList&&resultList.length > 0){
        var coupon = resultList[0];
        $(".redPacket-count").html(coupon.couponSum);
        $("#js_redPacket-btn").attr("data-batchid",coupon.batchId);
        $("#hongbao_div").show();
    }

    // 红包浮层内点击领取的提示
    $("#js_redPacket-btn").click(function(){
        var batchId = $(this).attr("data-batchid");
        if(mallUser&&mallUser.isLogin){  //判断是否登录
            grantCouponCode(mallUser.userId,batchId);
        }else{
            if (navigator.userAgent.indexOf("auto_ipad") > -1){
                //呼出ipad登录
                appLoginPage();
            }else{
                window.location.href = "http://account.autohome.com.cn/login/?rt=1&backurl=" + encodeURIComponent(location.href);
            }
//            gologinUrl();
        }
    })
}

function grantCouponCode(userId,batchId,mobile){
    if(typeof(userId) == 'undefined'){
        userId = '';
    }
    if(typeof(mobile) == 'undefined'){
        mobile = '';
    }
    if(typeof(batchId) == 'undefined'){
        batchId = '';
    }
    var grantCoupondata ={
        providerId:'83112629',
        batchId:batchId,
        userId:userId,
        mobile:mobile
    }
    $.ajax({
        type: "GET",
        url: "/shop/grantCouponCode.jtml",
        data: grantCoupondata,
        dataType:"json",
        success: function(resultData){
            if(resultData&&resultData.returncode==0){
                if(resultData.result.returncode == 1000){
                    $("#layer-redPacket-alert-span").html("领取成功！您可以在我的订单查看已领取的红包");
                    $("#layer-redPacket-alert").layer("show");
                }else{
                    $("#layer-redPacket-alert-span").html(resultData.result.message);
                    $("#layer-redPacket-alert").layer("show");
                }
            }else{
                $("#layer-redPacket-alert-span").html("领取失败");
                $("#layer-redPacket-alert").layer("show");
            }

        }
    });
}

