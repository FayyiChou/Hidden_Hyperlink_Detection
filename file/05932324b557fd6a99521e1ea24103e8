    <script type="text/javascript">
        function fastLogin() {
            var fUserName = $("#fUserName").val();
            var fPassword = $("#fPassword").val();
            var fCode = $("#fCode").val();

            if (fUserName != "" && fPassword != "" && fCode != "") {
                //ajax
                $.post(
                'ashxForAJAX/UserLoginH.ashx',
                { userName: fUserName, password: fPassword, code: fCode },
                    function (data) {
                        if (data == "UserNameError" || data == "PasswordError") {
                            $("#fImgCode").click();
                            alert("用户名或密码错误！");
                        } else if (data == "CodeError") {
                            $("#fImgCode").click();
                            alert("验证码错误！");
                        } else if (data == "Success") {
                            alert("登陆成功！")
                            location.reload();
                        }
                    }
                );
            } else {
                alert("请填写完整的信息!");
            }
        }
    </script>
