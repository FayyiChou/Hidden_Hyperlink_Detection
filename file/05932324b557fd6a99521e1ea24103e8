        $(function () {
            $("#Gwap  .wapClose").click(function () {
                var T_this = $(this);
                T_this.parent().css("display", "none")
            });
        });
        function ShowLoginDiv() {
            $("#Login_index").css("display", "block");
        }
        function hiddenLoginDiv() {
            $("#Login_index").css("display", "none");
        }

        function ProLogin() {
            //清空Msg信息
            $("#userNameOrPasswordMsg").html("");
            $("#CodeMsg").html("");
            //提取数据，检查输入完整性
            var userName = $("#loginUserName").val();
            var password = $("#loginPassword").val();
            var code = $("#Code").val();

            if (userName != "" && password != "" && code != "") {
                //ajax
                $.post(
                'ashxForAJAX/UserLoginH.ashx',
                { userName: userName, password: password, code: code },
                    function (data) {
                        if (data == "UserNameError" || data == "PasswordError") {
                            $("#ImgValidateCode").click();
                            $("#userNameOrPasswordMsg").html("用户名或密码错误！");
                        } else if (data == "CodeError") {
                            $("#ImgValidateCode").click();
                            $("#CodeMsg").html("验证码错误！");
                        } else if (data == "Success") {
                            location.reload();
                        }
                    }
                );
            } else {
                alert("请填写完整的信息!");
            }
        }

        //搜索
        function DoSearch() {
            //取Key
            var key = $("#searchInput1Text").val();
            if (key == "") {
                alert("请输入搜索内容！");
            } else {
                var toUrl = 'Search.aspx' + "?key=" + key;
                location.href = toUrl;
            }
        }

        function SearchThis(sender) {
            var key = $(sender).html();
            $("#searchInput1Text").val(key);
            DoSearch();
        }

        //百度搜索
        function DoBaiduSearch() {
            //取Key
            var key = $("#searchInput1Text").val();
            if (key == "") {
                alert("请输入搜索内容！");
            } else {
                var toUrl = 'SearchBaiDu.aspx' + "?key=" + key;
                location.href = toUrl;
            }
        }

