                        var zd = parseFloat(gpArr[0]) - parseFloat(gpArr[7]); //计算长跌
                        var zdShow = Math.round(zd * 100) / 100;    //控制精度

                        var zdf = parseFloat(gpArr[1]);
                        var zdfShow = Math.round(zdf * 100) / 100;

                        var str = "<tr class='" + trClass + "'><td align='left' class='" + tdClass + "'>" + gpArr[13] + "</td><td>" + gpArr[0] + "</td><td>" + zdShow + "</td><td>" + zdfShow + "%" + "</td></tr>";

