		$("#ewm").hide();
	});

})();

function setMM(n){
	$(".menu a").eq(n).addClass("menuOn");
}
</script>

<script>
    newsNavPlay();
    
    /* 新闻分类导航 */
    function newsNavPlay(){
        $(".ind_newsList").hide();
        $("#nl0").show();

        $(".ind_newsNav a").mouseover(function(){
            var n = $(this).index();
            $("#ind_newsNavLine").attr("class","ind_nnl" + n);
            $(".ind_newsList").hide();
            $("#nl" + n).show();
        });
    }

    setMM(0);
    
    $(function () {
        // Slideshow 
        $("#slider").responsiveSlides({
            auto: true,
            pager: false,
            nav: true,
            speed: 500,
            timeout:4000,
            pager: true, 
            pauseControls: true,
            namespace: "callbacks"
        });
        $("#ind_nologBox a").click();    });
</script>

