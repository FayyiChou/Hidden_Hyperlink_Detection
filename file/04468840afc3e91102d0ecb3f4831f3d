            $(".fenlei").hover(function() {
                    $(this).find(".dropdown").show().find('li').bind('mouseover', function() {
                        $(this).addClass("curr").css('color', "#fff").bind('click', function() {
                            $(this).parent().parent().hide();
                            var cls = parseInt($(this).attr("data-cls"));
                            $("#allfl").html($(this).html()).css('color', '#333');
                            $("#cid").val(cls);
                        });
                    }).bind("mouseout", function() {
                        $(this).removeClass("curr").css('color', "#666");
                    });
                }, function() {
                    $(this).find(".dropdown").hide();
                });

             function searchtourl(){
