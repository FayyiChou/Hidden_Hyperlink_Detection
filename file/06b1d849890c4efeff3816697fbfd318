function ind(ji,le)
{
var ji;var le;
	for(bi=1;bi<=11;bi++)
	{
		if (bi!=pd)
		{
			if (bi==ji & le==1)
			{
			document.getElementById("in_"+bi+"").style.backgroundImage="url(/img/dhtbbg.gif)";
			document.getElementById("ic_"+bi+"").style.color='#000000';
