<script>
var timeId = null;
jQuery(".original").hover(function(){
	$(this).siblings(".mod-tips").show();
}, function(){
	var _self = $(this);
	timeId = setTimeout(function(){
		_self.siblings(".mod-tips").hide();
	}, 200);
});

jQuery(".comment .mod-tips").hover(function(){
	clearTimeout(timeId);
}, function(){
	$(this).hide();
});
jQuery(".root img").hover(function(){
	jQuery(this).siblings(".mod-tips").show();
}, function(){
	jQuery(this).siblings(".mod-tips").hide();
});

//回到顶部
LEGO.backToTop("#backtotop",{scrollEffect:false});
</script>
