    // 定义回调方法
    var TopLoginCallback = function (cbtype, data) {
        if (cbtype == "login") {
            if (data.Success) {
                // 登录成功
                window.location.reload();
                return;
            }
        }
        if (cbtype == "qq") {
            window.location.reload();
            return;
        } else if (cbtype == "weibo") {
            window.location.reload();
            return;
        }
    }
