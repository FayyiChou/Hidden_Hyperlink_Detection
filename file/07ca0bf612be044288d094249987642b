<script type="text/javascript">
   //切换效果控制
   function showTabs(n1, n2) {
	   var h = document.getElementById("tabs" + n1).getElementsByTagName("li");
	   var d = document.getElementById("tabs" + n1).getElementsByTagName("div");
	   for (var i = 0; i < h.length; i++) {
		   if (n2 - 1 == i) {
			   h[i].className += " over";
			   d[i].className += " show";
		   }
		   else {
			   h[i].className = " ";
			   d[i].className = " ";
		   }
	   }
   }
</script>
