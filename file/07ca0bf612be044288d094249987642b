function SetEffectLink(effect) {
    if (effect == "" || effect == "无") return;
    var curEffect = effect.split(",");
    sEffectLink = "";
    for (i = 0; i < curEffect.length; i++) {

        sEffectLink += '<a href="http://so.39.net/cosmetic.aspx?words=&tab=2&gx=效果_' + curEffect[i] + '">' + curEffect[i].substring(curEffect[i].lastIndexOf('_') + 1) + '</a>' + ",";

    }
    document.getElementById("gxlink").innerHTML = sEffectLink.substring(0, sEffectLink.lastIndexOf(","));
}
