                        <li></li><li><img width="85" height="85" src="http://dooutfit.com/wp-content/uploads/2016/09/Schermata-2016-09-08-alle-11.58.39-100x100.png" class="attachment-85x85 size-85x85" alt="schermata-2016-09-08-alle-11-58-39" srcset="http://dooutfit.com/wp-content/uploads/2016/09/Schermata-2016-09-08-alle-11.58.39-100x100.png 100w, http://dooutfit.com/wp-content/uploads/2016/09/Schermata-2016-09-08-alle-11.58.39-150x150.png 150w, http://dooutfit.com/wp-content/uploads/2016/09/Schermata-2016-09-08-alle-11.58.39-50x50.png 50w" sizes="(max-width: 85px) 100vw, 85px" /></li><li><img width="85" height="85" src="http://dooutfit.com/wp-content/uploads/2016/06/5-100x100.png" class="attachment-85x85 size-85x85" alt="5" srcset="http://dooutfit.com/wp-content/uploads/2016/06/5-100x100.png 100w, http://dooutfit.com/wp-content/uploads/2016/06/5-150x150.png 150w, http://dooutfit.com/wp-content/uploads/2016/06/5-50x50.png 50w" sizes="(max-width: 85px) 100vw, 85px" /></li><li><img width="85" height="85" src="http://dooutfit.com/wp-content/uploads/2016/01/3-100x100.png" class="attachment-85x85 size-85x85" alt="3" srcset="http://dooutfit.com/wp-content/uploads/2016/01/3-100x100.png 100w, http://dooutfit.com/wp-content/uploads/2016/01/3-150x150.png 150w, http://dooutfit.com/wp-content/uploads/2016/01/3-297x300.png 297w, http://dooutfit.com/wp-content/uploads/2016/01/3-50x50.png 50w, http://dooutfit.com/wp-content/uploads/2016/01/3.png 582w" sizes="(max-width: 85px) 100vw, 85px" /></li><li><img width="85" height="85" src="http://dooutfit.com/wp-content/uploads/2016/01/8-100x100.png" class="attachment-85x85 size-85x85" alt="8" srcset="http://dooutfit.com/wp-content/uploads/2016/01/8-100x100.png 100w, http://dooutfit.com/wp-content/uploads/2016/01/8-150x150.png 150w, http://dooutfit.com/wp-content/uploads/2016/01/8-295x300.png 295w, http://dooutfit.com/wp-content/uploads/2016/01/8-50x50.png 50w, http://dooutfit.com/wp-content/uploads/2016/01/8.png 578w" sizes="(max-width: 85px) 100vw, 85px" /></li>                    </ul>
                            <script>
                    jQuery(document).ready(function(){
                        "use strict";

                        
                            var sync1 = jQuery(".noo-sync1");
                            var sync2 = jQuery(".noo-sync2");

                            sync1.owlCarousel({
                                singleItem : true,
                                slideSpeed : 400,
                                navigation: false,
                                pagination:false,
                                autoHeight: true,
                                afterAction : syncPosition,
                                responsiveRefreshRate : 200
                            });

                            sync2.owlCarousel({
                                items : 3,
                                itemsDesktop      : [1199,3],
                                itemsDesktopSmall     : [979,3],
                                itemsTablet       : [768,3],
                                itemsMobile       : [479,1],
                                pagination:false,
                                responsiveRefreshRate : 100,
                                afterInit : function(el){
                                    el.find(".owl-item").eq(0).addClass("synced");
                                }
                            });

                            function syncPosition(el){
                                var current = this.currentItem;
                                jQuery(".noo-sync2")
                                    .find(".owl-item")
                                    .removeClass("synced")
                                    .eq(current)
                                    .addClass("synced")
                                if(jQuery(".noo-sync2").data("owlCarousel") !== undefined){
                                    center(current)
                                }

                            }

                            jQuery(".noo-sync2").on("click", ".owl-item", function(e){
                                e.preventDefault();
                                var number = jQuery(this).data("owlItem");

                                sync1.trigger("owl.goTo",number);
                            });

                            function center(number){
                                var sync2visible = sync2.data("owlCarousel").owl.visibleItems;

                                var num = number;
                                var found = false;
                                for(var i in sync2visible){
                                    if(num === sync2visible[i]){
                                        var found = true;
                                    }
                                }

                                if(found===false){
                                    if(num>sync2visible[sync2visible.length-1]){
                                        sync2.trigger("owl.goTo", num - sync2visible.length+2)
                                    }else{
                                        if(num - 1 === -1){
                                            num = 0;
                                        }
                                        sync2.trigger("owl.goTo", num);
                                    }
                                } else if(num === sync2visible[sync2visible.length-1]){
                                    sync2.trigger("owl.goTo", sync2visible[1])
                                } else if(num === sync2visible[0]){
                                    sync2.trigger("owl.goTo", num-1)
                                }
                            }
                                            });
                </script>
                </div>
