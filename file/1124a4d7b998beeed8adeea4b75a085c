<script>

$(function(){
var str=location.href; //取得整个地址栏
var t=str.lastIndexOf('/');
var t1=str.lastIndexOf('.'); 
var t2=str.substring(t+1,t1);


$($(".caidan li").html())
$.each($(".caidan li"),function(i,val){
var mark=$(this).find("a").eq(0).attr("tit")

if(mark.indexOf(t2)>=0)
{

$(this).find("a").eq(0).removeClass("cur").addClass("cur")

}



})

})

</script>
