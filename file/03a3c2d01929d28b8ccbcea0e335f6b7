                            series.list.bind.LoadIngMore();
                            $(window).scroll(function () {
                                if (isexc) {
                                    if (timer) {
                                        window.clearTimeout(timer);
                                        timer = null;
                                    }
                                    timer = setTimeout(function () {
                                        if (($(window).height() + $(window).scrollTop()) >= $(document).height() - 150) {
                                            isexc = false;
                                            series.list.bind.LoadIngMore();
                                        }
                                    }, 100);
                                }
                            });
                        },
                        LoadIngMore: function () {
                            if (!hasData)
                                return;

                            $("#loading_more").text("加载中……");
                            var pageSize = 10;
                            var pageIndex = 1;
                            var p = $("#loading_more").data("pageindex");
                            if (typeof (p) != "undefined")
                                pageIndex = parseInt(p) + 1;

                            $.ajax({
                                type: "get",
                                url: "/PreVerSeries/GetSeriesSpecListAjax",
                                data: { provinceId: provinceId, cityId: cityId, seriesId: seriesId, specId: specId, pageSize: pageSize, pageIndex: pageIndex },
                                dataType: "json",
                                success: function (json) {
                                    if (json) {
                                        if (json.ContainerTemplate && json.ContainerTemplate == "SeriesSpecTemplate") {
                                            if (json.SpecList) {
                                                if (json.SpecList.length < pageSize)
                                                    hasData = false;

                                                for (var i = 0; i < json.SpecList.length; i++) {
                                                    var item = json.SpecList[i];

                                                    var obj = {};
                                                    obj.SpecID = item.SpecID;
                                                    obj.SeriesName = item.SeriesName;
                                                    obj.SpecName = item.SpecName;
                                                    obj.PriceM = item.PriceM;
                                                    obj.PriceOffM = item.PriceOffM;
                                                    obj.PriceOffPercent = item.PriceOffPercent;
                                                    obj.OriginalPriceM = item.OriginalPriceM;
                                                    obj.CityName = item.CityName;
                                                    obj.CompanySimple = item.CompanySimple;
                                                    obj.OrderRangeName = item.OrderRangeName;
                                                    obj.AppSellPhone = item.AppSellPhone;
                                                    obj.CloseOrder = item.CloseOrder;
                                                    obj.SeriesID = seriesId;
                                                    obj.DealerID = item.DealPrice.DealerID;
                                                    obj.backurl = encodeURIComponent(window.location.href);
                                                    obj.Is24Icon = item.Is24h == true ? "icon-telphone-24h" : "icon-telphone";
                                                    obj.AppSellPhoneRest = item.AppSellPhoneRest;

                                                    $("#spec_list").append(template('SeriesSpecTemplate', obj));
                                                }
                                                hasData = true;
                                                $("#loading_more").data("pageindex", pageIndex);
                                            }
                                        }
                                        else if (json[0] && json[0].ContainerTemplate) {
                                            if (json[0].ContainerTemplate == "RecommendSeriseTemplate") {

                                            }
                                            else if (json[0].ContainerTemplate == "RecommendSpecTemplate") {

                                            }
                                        }
                                    }
                                    else {
                                        hasData = false;
                                    }
                                },
                                complete: function () {
                                    isexc = true;
                                    if (!hasData) {
                                        $("#loading_more").text("已无更多经销商");
                                    }
                                    else
                                        $("#loading_more").text("上拉查看更多经销商");
                                }
                            });
                        }
                    },
                    init: function () {
                        series.list.bind.init();
                    }
                },
                toTop: function () {
                    $(window).scroll(function () {
                        var top = $(window).scrollTop();
                        if (top > 200) {
                            $('#backTop').removeClass("fn-hide");
                            $('#backTop').click(function () {
                                document.body.scrollIntoView(true);
                            });
                        } else {
                            $('#backTop').addClass("fn-hide");
                        }
                    });
