<link rel="stylesheet" type="text/css" href="css/index.css"/>
<script type="text/javascript" language="javascript" src="js/jquery-1.10.2.min.js"></script>
<script type="text/javascript" src="js/idangerous.swiper.js"></script>
<script type="text/javascript" src="js/jquery.ellipsis.js"></script>
<script type="text/javascript" src="js/tab.js"></script>
<script type="text/javascript" src="js/public.js"></script>
<script type="text/javascript">
$(".cont_jq").wordLimit(60);
$('#news_listc ul li p').wordLimit();
$(document).ready(function(){    
  slide();
  wordLimit();
  click_switch();
  search();
}); 
function myLinksChange(id){
  var obj = $("#"+id).val();
  if(obj !=""){
    var index = obj.indexOf("http://");
    if(index ==-1){
      obj = "http://"+obj;
    }
    window.open(obj);
  }
}
function search(){
   $("#search").click(function(){
    var text=$("#keyword").val();
      location.href ="search.php?keyword="+text;
  });
  }

function click_switch(){

    $("#news_lista").show();
    $("#news_listb").hide();
    $("#news_listc").hide();
    $("#news_listd").hide();  
    $("#kouan").click(function(){
      $("#news_lista").show();
        $("#news_listb").hide();
        $("#news_listc").hide();
       $("#news_listd").hide();
    });
    $("#haiguan").click(function(){ 
      $("#news_lista").hide();
      $("#news_listb").show();
       $("#news_listc").hide();
        $("#news_listd").hide();
    });
    
    $("#jianyan").click(function(){
     $("#news_lista").hide();
      $("#news_listb").hide();
       $("#news_listc").show();
        $("#news_listd").hide();
    });

    $("#wuliu").click(function(){
       $("#news_lista").hide();
      $("#news_listb").hide();
       $("#news_listc").hide();
        $("#news_listd").show();
    
    }); 
}




function slide(){
  var mySwiper = new Swiper('.swiper-container',{
    pagination: '.pagination',
    loop:true,
    grabCursor: true,
    paginationClickable: true,
    autoplay:5000
   })
   
   $(".banner_img li").hide();
   $(".banner_img li:eq(0)").show();
   mySwiper.addCallback('SlideChangeStart', function(swiper){
      //console.log(mySwiper.activeLoopIndex)循环模式索引
      var _idx = mySwiper.activeLoopIndex;
      $(".banner_img li").stop().fadeOut();
      $(".banner_img li:eq("+_idx+")").stop().fadeIn();
    })
}

//超出显示省略号
function wordLimit(){
  //首页公告
  $(".swiper-slide").each(function(index, element) {
        $(this).find(".p1 b").wordLimit(20);
    $(this).find(".p2").wordLimit(45);
    });
  $(".news_info li").wordLimit(15);
  $(".news_list li p").wordLimit(22);
  $(".dynamic_show_con b").wordLimit(11);
  $(".dynamic_show_con p").wordLimit(36);
  $(".dynamic>ul li p").wordLimit(18);
}

</script>