	var pageType = "HOME";
	var dataLayer = (typeof dataLayer !== 'undefined') ? dataLayer : [];

	</script>

<script type="text/javascript">
function trackViewProduct(obj)
{
	var product = jQuery.parseJSON($(obj).attr("data-product"));
	
	dataLayer.push({
		'event': 'ecommerce.js',
		'ecommerce': {
			'click': {
				'actionField': {'list': product.list},
				'products': [{
					'name': product.name,
					'id': product.id,
					'price': product.price,
					'brand': 'Tous',
					'variant': product.variant,
					'dimension9': product.dimension9
				}]
			}
		},
		'eventCallback': function() {document.location = "/ru"+product.url}
	});	
}

function trackUpdateCart(data)
{	
	dataLayer.push({
		'ecommerce': {
			'currencyCode': data.entryData.currency,
			'add': {
				'products': [{
					'name': data.entryData.productName,
					'id': data.productCode,
					'price': data.entryData.productPostPrice,
					'brand': 'Tous',
					'category': data.entryData.listCategories,
					'variant': data.entryData.size,
					'dimension8': data.entryData.size,
					'dimension9': data.entryData.isFixedPrice,
					'quantity': data.newCartQuantity
				}]
			}
		},
		'eventcategory': 'EEC','eventaction': 'AddToCart', 'eventlabel': data.entryData.productName, 'event': 'ecommerce.js'
	});
}

function trackRemoveFromCart(data)
{
	dataLayer.push({
		'ecommerce': {
			'remove': {
				'products': [{
					'name': data.entryData.productName,
					'id': data.productCode,
					'price': data.entryData.productPostPrice,
					'brand': 'Tous',
					'category': data.entryData.listCategories,
					'variant': data.entryData.size,
					'dimension8': data.entryData.size,
					'dimension9': data.entryData.isFixedPrice,
					'quantity': data.newCartQuantity
				}]
			}
		},
		'eventcategory': 'EEC','eventaction': 'removeFromCart', 'eventlabel': data.entryData.productName, 'event': 'ecommerce.js'
	});
}

window.mediator.subscribe('trackAddToCart', function(data) {
	
	dataLayer.push({
		'ecommerce': {
			'currencyCode': data.cartData.currency,
			'add': {
				'products': [{
					'name': data.cartData.productName,
					'id': data.cartData.productCode,
					'price': data.cartData.productPrice,
					'brand': 'Tous',
					'category': data.cartData.listCategories,
					'variant': data.cartData.size,
					'dimension8': data.cartData.size,
					'dimension9': data.cartData.isFixedPrice,
					'quantity': data.cartData.quantity
				}]
			}
		},
		'eventcategory': 'EEC','eventaction': 'AddToCart', 'eventlabel': data.cartData.productName, 'event': 'ecommerce.js'
	});
});

window.mediator.subscribe('trackUpdateCart', function(data)
{
	if (data.productCode && data.newCartQuantity)
	{
		if (parseInt(data.initialCartQuantity) > parseInt(data.newCartQuantity))
		{
			trackRemoveFromCart(data);
		}
		else
		{
			trackUpdateCart(data);
		}
	}
});

window.mediator.subscribe('trackRemoveFromCart', function(data)
{
	if (data.productCode && data.newCartQuantity)
	{
		trackRemoveFromCart(data);
	}
});

</script>




  







