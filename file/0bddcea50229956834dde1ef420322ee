  <script> 

	$(function(){
		
		$(".case_2").hover(function(){
			$(this).addClass("typeListH");
			$(this).find("h2 img").stop().animate({"width":"250px","height":"200px","top":"0px","left":"0px"},300);
			$(this).find("h3 em").stop().slideDown(300);
		},function(){
			$(this).removeClass("typeListH");
			$(this).find("h2 img").stop().animate({"width":"240px","height":"170px","top":"20px","left":"20px"},300);
			$(this).find("h3 em").stop().slideUp(300);
		});
		
		$(".proWarp .con").each(function(){
			$(this).find(".typeCon").eq(0).insertBefore($(this).find(".typeTit").eq(1));
		});
		
		$(".list").eq(1).addClass("listSec");
		$(".list:gt(3)").addClass("pa");
		$(".list:lt(4)").addClass("pr");
		$(".list").eq(4).css({"left":"0","top":"173px"});
		$(".list").eq(5).css({"left":"558px","top":"173px"});
		$(".list").eq(6).css({"left":"785px","top":"173px"});
		$(".list").hover(function(){
			$(this).find("h3").stop().animate({"left":"0px"},300);
			$(this).find("a.name").stop().animate({"left":"0px"},300);
		},function(){
			$(this).find("h3").stop().animate({"left":"322px"},"fast");
			$(this).find("a.name").stop().animate({"left":"322px"},"fast");
			$(this).find("h3").animate({"left":"-322px"},0);
			$(this).find("a.name").animate({"left":"-322px"},0);
		});
		
		$(".inewTit:first").hide();
		$(".inewCon:first").show();
		$(".inewsCon .inewsList:last").addClass("bor_b");
		$(".inewsCon .inewsList .inewTit").hover(function(){
			$(".inewTit").show();
			$(".inewCon").hide();
			$(this).hide();
			$(this).parent().find(".inewCon").stop().fadeTo(500,1);
		});
		$(".iproWarp .con .typeTit").each(function(){
			$(this).find("a").css("left",(280-$(this).find("a").innerWidth())/2);
		});
	})
