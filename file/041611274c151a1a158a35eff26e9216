		$(function(){
		$.ajax({
			type: "POST",
	         url:path+'/portal/rrfManagerAction/queryRRFSetting.action',
	         dataType : "json",
	         success: function(res) {
	        	 if(res.state=="SUCCESS"){
	        		 var params = res.content;
	        		 if(params!=null && params!=""){
	        			 var ccbRate = params.ccb_rate;
	        			 var tjScale = params.tj_scale;
	        			 var tjRate = params.tj_rate;
	        			 var yebRate = params.yeb_rate;
	        			 var rrf_rate = ccbRate*(1-tjScale*0.01)+tjRate*tjScale*0.01;
	        			 $("#rrf_rate").html(rrf_rate.toFixed(3)+"%");
	        			 //$("#yeb_rate").html((parseFloat(yebRate)).toFixed(3)+"%");
	        		 }
	        	 }
	         }
		});
		
	});
	
	//快速搜索
	function buytype(){
	if($('#Buytype').val()==1){
		$('#OfficialFinance').attr("action","/link.action?page=/front/OfficialFinance");
// 		expire
		$('#expire').html('<option value="0-30">一月以内</option><option value="30-90">1-3月</option><option value="90-180">3-6月</option><option value="180-270">6-9月</option><option value="270-360">9-12月</option><option value="360-99999">一年以上</option>');
// 		lowest
		$('#lowest').html('<option value="100-100">100</option>');
		
	}else if($('#Buytype').val()==2){
		$('#OfficialFinance').attr("action","/link.action?page=/front/rrf/rrfBuy");
// 		expire
		$('#expire').html('<option>一天起</option>');
// 		lowest
		$('#lowest').html('<option value="100-100">100</option>');

	}else if($('#Buytype').val()==3){
		$('#OfficialFinance').attr("action","/link.action?page=/front/zhuanRang/assignment");

		$('#expire').html('<option value="0">全部</option><option value="0-90">3个月以内</option><option value="90-180">3-6月</option><option value="180-270">6-9月</option><option value="270-365">9-12月</option>');

		$('#lowest').html('<option value="100-100">100</option>');
	}
		
}
	function OfficialFinanceSubmit(){
		$("#OfficialFinance").submit();
	}
	$(function(){
		buytype();
	})
