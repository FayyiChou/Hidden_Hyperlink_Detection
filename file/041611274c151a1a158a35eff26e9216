	//绑卡认证
	function  bangka()
	 {
			 var json = getuser();
		     var type=json.content.idType;
		     if(type!=null&&type!=""&&type!="0")
		     {
		    	 if(json.content.isTiecard=="1"){
		    		 window.location.href="/link.action?page=/front/my_bankCard";  
		    	 }else{
		    		 
		    		 window.location.href="/link.action?page=/front/my_bankCard_NO";  
		    	 }
		     }else{
			        layer.alert("为了您的账户安全请进行实名认证", function(){
					location.href="/requestparam.action?page=/usercenter/front/PCBrowseName"; 
				})
	 		 }	
	 }
	
	//修改交易密码验证
	function updatepassword(){
		var json = getuser();
	    var type=json.content.idType;
	  
	     if(type!=null&&type!=""&&type!="0"){
		      if(json.content.payPassword==null||json.content.payPassword==""){
		    	  window.open("/link.action?page=front/setPayPassword")
		    	  return;
		      }
		          window.open("/portal/FcustomAction/updateTradingPassword.action");
		       
	      }else{
	        	layer.alert("为了您的账户安全请进行实名认证", function(){
				location.href="/requestparam.action?page=/usercenter/front/PCBrowseName"; 
			})
	     }
	}
	//修改手机号验证
	function updatephone(){
		var json = getuser();
	    var type=json.content.idType;
	     if(type!=null&&type!=""&&type!="0"){
		      
		    	window.open("/portal/FcustomAction/changPhone.action?phId=1");
		       
	      }else{
	        	layer.alert("为了您的账户安全请进行实名认证", function(){
				location.href="/requestparam.action?page=/usercenter/front/PCBrowseName"; 
			})
	     }
	     
	 	 
	}
