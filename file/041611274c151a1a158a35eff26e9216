	function GetUrlRelativePath() {
		var url = document.location.toString();
		var arrUrl = url.split("//");
		var start = arrUrl[1].indexOf("/");
		var relUrl = arrUrl[1].substring(start);//stop省略，截取从start开始到结尾的所有字符
	 
		return relUrl;
	}
	var jsonuser = getuser();
	var customerType=jsonuser.content.flag;
	var customerIdType=jsonuser.content.idType;
	var cardNumber=jsonuser.content.cardNumber;
	if(customerType=='1' && customerIdType!='1'){
			 if(GetUrlRelativePath()!='/'&&GetUrlRelativePath()!='/link.action?page=/front/home'&&GetUrlRelativePath()!='/portal/yeepay/EnterpriseRegistered.action?&authParam=1'&&GetUrlRelativePath()!='/portal/yeepay/EnterpriseRegistered.action?&authParam=2'){
				 location.href="/";
			 }
				
			//location.href="/"; 
	 
	}
