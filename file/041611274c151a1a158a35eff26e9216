    <script type="text/javascript">
//保留两位小数
function fmoney(s, n)   
{   
   n = n > 0 && n <= 20 ? n : 2;   
   s = parseFloat((s + "").replace(/[^\d\.-]/g, "")).toFixed(n) + "";   
   var l = s.split(".")[0].split("").reverse(),   
   r = s.split(".")[1];   
   t = "";   
   for(i = 0; i < l.length; i ++ )   
   {   
      t += l[i] + ((i + 1) % 3 == 0 && (i + 1) != l.length ? "," : "");   
   }   
   return t.split("").reverse().join("") + "." + r;   
} 
</script>
<script>

//showredpacket 为打开红包弹窗的调用函数
function showredpacket(){
		$(".hshade").show();
		$(".hFrontFrame").show();
		$(".hFrontContent").show();
		redpacket();
}

function redpacket(){
$(".hcaidai").animate(
{marginTop:31},300,function(){$(".hrpstitle").fadeIn();});
}
$(function(){
$(".hFrontClose").click(function(){	
	$(".hshade").hide();
	$(".hFrontFrame").hide();
	$(".hFrontContent").hide();})
})

</script>
