	var path = "";
	
	/**
	 * 返回台返回json数据 Filter
	 **/
	function dataFilter(data,calback){
		if((typeof data=='string')&&(data.constructor==String)){
			data = eval('('+data+')');
		}
		if(data.state){
		    if(data.state=="SUCCESS"){
		    	return calback(data.content);
		    }else{
		    	return;
		    }
		}else{
			return data;
		}
	}
	
	//解析返回的数据
	function parseData(data){
		if((typeof data=='string')&&(data.constructor==String)){
			data = eval('('+data+')');
		}else{
			return data;
		}
	}
	
	//数据成功与否结果
	function dataResult(data){
		if((typeof data=='string')&&(data.constructor==String)){
			data = eval('('+data+')');
		}
		if(data.state){
		    if(data.state=="SUCCESS"){
		    	return true;
		    }else{
		    	return false;
		    }
		}else{
			return false;
		}
	}
	
	//修改验证码
	function changeValidateCode(imgEle){
		$("#"+imgEle).attr("src",path+"/image/validateCode.action?temp="+Math.floor(Math.random() * ( 10000 + 1)));
	}
	
	//当钱为空时 返回0.00
	function  Verification(s)
	{
		 if(s==null||s=="")
		 {
			 return 0.00
		 }
		 return s;
	}
	
	
	//时间搓
	function timestr(){
		var timestamp = Date.parse(new Date());
		return timestamp;
	}
	
	
	
