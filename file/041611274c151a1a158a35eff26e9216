			})
	     }
	}

	//返回用户
	function getuser()
	{
		var userjson="";
		$.ajax({
	         type: "POST",
	         url:path+'/portal/FcustomAction/queryById.action?cmId='+'&time='+timestr(),
	         async:false,
	         dataType : "json",
	         success: function(json) {
	        	 userjson=json;
	      	 }
	    });
		return userjson;
	}
	
	//返回用户金额
	function getUserMoney(){
		var userjson="";
		 $.ajax({
	         type: "POST",
	         url:path+'/portal/faccountAction/queryByUserId.action?userId='+'',
	         async:false,
	         dataType : "json",
	         success: function(json) {
	        	 userjson=json;
	      	 }
	    });
		 return userjson;
	}
	
	
