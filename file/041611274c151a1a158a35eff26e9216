			 $this.hover(function() {
			 clearInterval(scrollTimer);
			 }, function() {
			 scrollTimer = setInterval(function() {
			 scrollNews($this);
			 }, 3000);
			 }).trigger("mouseleave");

			 function scrollNews(obj) {
			 var $self = obj.find("#wzgg");
			 var lineHeight = $self.find(".wzggC:first").height();
			 $self.animate({
			 "marginTop": "0px"
			 }, 600, function() {
			 $self.css({
			 marginTop: 0
			 }).find(".wzggC:first").appendTo($self);
			 })
			 }
			 }) 
