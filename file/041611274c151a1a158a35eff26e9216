 <script type="text/javascript">
    	$(document).ready(function(){
    		$.ajax({
    			type: "POST",
		         url:path+'/portal/information/queryByCount.action?',
		         dataType : "json",
		         success: function(json) {
		        	 var oSpan=document.getElementById("oSpan");
		        		var d=json[0].count+5000;//结束的位置
		        		var s= parseInt(oSpan.innerHTML);//起始位置
		        		var time=3000;  //所用时间 1000毫秒（ 在1秒内 数值增加到d）;
		        		var outTime=0;  //所消耗的时间
		        		var interTime=30;
		        		var timer = setInterval(function(){
		        			outTime+=interTime;
		        			if(outTime<time){
		        				oSpan.innerHTML= parseInt(d/time*outTime);
		        			}else{
		        				oSpan.innerHTML=d;
		        			}
		        			},interTime);
		        	// $("#oSpan").html(json[0].count);
		        	 //$("#oSpan1").html(json[0].sum);
		        		var oSpan1=document.getElementById("oSpan1");
		        		var d1=json[0].sum.toFixed(0);//结束的位置
		        		var s1= parseInt(oSpan1.innerHTML);//起始位置
		        		var time1=3000;  //所用时间 1000毫秒（ 在1秒内 数值增加到d）;
		        		var outTime1=0;  //所消耗的时间
		        		var interTime1=30;
		        		var timer1 = setInterval(function(){
		        			outTime1+=interTime1;
		        			if(outTime1<time1){
		        				oSpan1.innerHTML= parseInt(d1/time1*outTime1);
		        			}else{
		        				oSpan1.innerHTML=d1;
		        			}
		        			},interTime1);
		         }
    		})
    	})	
    </script>
   
