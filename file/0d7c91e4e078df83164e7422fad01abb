    $ansList = $(".J-qaList").find("li");

    $(".J-filter")
        .on("click",".J-f-expert",function(){
            var expTop = $(".J-expert").offset().top - 50;
            $("html body").animate({scrollTop:expTop}, 300);
        })
        .on("click",".J-f-exp",function(){
            $ansList.filter(".hide").removeClass('hide');
            $ansList.not("[data-isexpert='1']").hide();
            $(this).addClass('cur').siblings('.cur').removeClass('cur');
        })
        .on("click",".J-f-all",function(){
            $ansList.filter(".hide").removeClass('hide');
            $ansList.not("[data-isexpert='1']").show();
            $(this).addClass('cur').siblings('.cur').removeClass('cur');
            $ansList.filter(".more").hide();
        });

    $(".J-qaList").on("click",".more",function(){
        $(this).hide();
        $ansList.filter(".hide").removeClass('hide');
    })

    $(".J-f-exp").text("经验妈妈回答("+$ansList.filter("[data-isexpert='1']").length+")")

    $(".J-f-all").text("全部回答("+$("input[name='aids[]']").length+")")

