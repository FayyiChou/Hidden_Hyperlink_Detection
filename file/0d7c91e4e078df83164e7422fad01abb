    });

    $("#askform").on("submit",function(){
        $.ajax({
            url: $(this).attr("action"),
            type: 'post',
            dataType: 'json',
            data: $(this).serializeArray(),
        })
        .done(function(res) {
            if(res.code==0){
                setTimeout(function(){location.reload()},2000);
            }
            showQuickAsking('提示','',res.msg);
        })
        .error(function() {
            showQuickAsking('提示','','请稍候再试~');
        });

        return false
    });

