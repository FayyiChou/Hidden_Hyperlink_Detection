!function(){            require.async("common:widget/ui/pager/pager.js", function(Pager) {
                var pageSize = 20, pn = "1" || parseInt("")||1;
                var total = "" || "1" || 0;

                var options = {
                    currentPage : pn, // 当前页码，从1开始
                    totalPage : Math.ceil(total/pageSize), // 总页数
                    viewSize : 10, // 每次最多显示的页码数量

                    tplUrl : '#{pageNum}', // 页面a链接href属性的模版，#{pageNum} 会被替换成页码
                    tplLabelNormal : '#{pageNum}', // 普通页码显示的文案
                    tplLabelCurrent : '#{pageNum}', // 当前页码显示的文案
                    
                    labelFirst : '首页', 
                    labelLast : '最后页'
                };

                var movieId = "";
                var pager = new Pager('j-pager-container', options);

                pager.on('pagechange', function(pageNum){
                    // 返回 false 可以阻止链接的默认行为
                    var url = "\/cinema\/0-0\/subd\/cb103-d10000-s0-o12-b1-f0";
                    if(-1 != url.indexOf("?")){
                        url += "&";
                    } else {
                        url += "?";
                    }

                    url += ("pn="+pageNum);
                    location.href = url;

                    return false;
                });
            });
        }();
