!function(){$(function() {
    F.context("userPhone", "");
        F.context("cinema_seat", "/dianying/plan");
    F.context("ajax_cinema_timetable", "/pcindex/main/filmtimetable");
    F.context("sValue", "");
    F.context("movieInfo", null);
    F.context("day", "");

    require.async(["common:widget/ui/template/template.js"],
    function(Baidu) {
        var bdTpl = Baidu.template;

        var tpl_timetable = $("#j-tpl-timetable").html();
        var cinema_timetable = {};
        var ajax_cinema_timetable = "/pcindex/main/filmtimetable";
        /* 影讯hover浮层 */
        var lastSchedulePopup = null;
        var timeCounter = new Date().getTime();

        $("#j-cinema-info-list .schedule").each(function(index, elem) {
            var timer = null;

            $(elem).on("mouseover", function(e) {
                var cinemaId = $(elem).data("cinemaid");

                if(!cinema_timetable[cinemaId]) {
                    var movieInfo = F.context("movieInfo");
                    var url = F.context("ajax_cinema_timetable");
                    url += ("?cinemaId="+cinemaId+"&movieId="+movieInfo.movieId+"&day="+F.context("day"));

                    $.getJSON(url, function(json) {
                        if(0==json.errno) {
                            var filmList = json.data.timetable;
                            var filmName = json.data.name;
                            cinema_timetable[cinemaId] = json.data;

                            renderSchedulePopup(json.data);

                            scheduleDisplay();
                        }
                    });
                } else {
                    renderSchedulePopup(cinema_timetable[cinemaId]);
                    scheduleDisplay();
                }

                function renderSchedulePopup(data) {
                    var filmList = data.timetable;
                    var filmName = data.name;

                    for(var i=0;i!=filmList.length; ++i) {
                        var film = filmList[i];
                        film.name = filmName;
                        film.now = (new Date()).getTime();

                        var outBuyTime = null
                        if(film.outBuyTime) {
                            outBuyTime = new Date(film.outBuyTime.replace(/-/g,"/")).getTime();
                        }
                        var showingTime = new Date(film.date+" "+film.time).getTime();

                        if(!outBuyTime) {
                            outBuyTime = showingTime-3600*1000;
                        }
                        else if(outBuyTime > showingTime) {
                            outBuyTime = showingTime;
                        }
                        film.showtime = outBuyTime;

                        var tpl_data = {
                            "film": film,
                            "urlBookSeat": F.context("cinema_seat"),
                            "sValue": F.context("sValue")
                        };
                        var html = bdTpl(tpl_timetable, tpl_data);

                        $("#j-cinema-info-list .schedule").each(function(index, elem) {
                            var time = $(elem).data("time");

                            if(time == film.time) {
                                $(elem).attr("data-hasdetail",true)
                                        .find(".schedule-popup").html(html);
                                hasDetail = true;
                            }
                        });
                    }

                    $("#j-cinema-info-list .schedule").each(function(index, elem) {
                        var html_no_detail = '<p>暂无影讯</p><div class="ico_markdown"></div>';
                        if(!$(elem).attr("data-hasdetail")) {
                            $(elem).find(".schedule-popup").html(html_no_detail);
                        }
                    });

                }

                function scheduleDisplay() {
                    lastSchedulePopup!=index
                            && $("#j-cinema-info-list .schedule .schedule-popup").eq(lastSchedulePopup).hide()
                    && (lastSchedulePopup=index);

                    timer && clearTimeout(timer);

                    var $target = $(e.currentTarget).find(".schedule-popup");
                    var w = $target.width();
                    var left = w/2-2;
                    ((new Date().getTime()-timeCounter<800)&&$target.css("left","-"+left+"px").show())
                    || $target.css("left","-"+left+"px").fadeIn(500);
                }
            }).on("mouseout", function(e) {
                timeCounter = new Date().getTime();
                timer = setTimeout(function() {
                    $(e.currentTarget).find(".schedule-popup").fadeOut(500);
                }, 800);
            });
        });
    });


    require.async(["common:widget/ui/passport/passport.js", "common:widget/ui/passport_bind_phone/passport_bind_phone.js"],
            function(loginApi, passBindPhone) {
                $("#j-cinema-info-list .schedule-popup").on("mouseup", function(e) {
                    var $target = $(e.target);
                    if("choose-seat"!=$target.data("type")) return false;

                    loginApi.check(function() {
                        var userPhone = F.context("userPhone");
                        if(!userPhone) {
                            passBindPhone.bind();
                        } else {
                            var url = $target.attr("href");
                            window.open(url, "_top");
                        }
                    });

                    return false;
                });
            });
    
    /* 地铁附近浮层 */
    $("#j-cinema-info-list .ico-subway").each(function(index, elem) {
        var timer = null;
        $(elem).on("mouseover", function(e) {
            timer && clearTimeout(timer);

            var $target = $(e.currentTarget).find(".subway-popup");
            var w = $target.width();
            var left = w/2-17;
            $target.css("left","-"+left+"px").fadeIn(500);
        }).on("mouseout", function(e) {
            timer = setTimeout(function() {
                $(e.currentTarget).find(".subway-popup").fadeOut(500);
            }, 800);
        });
    });

});
}();
!function(){/* 购票弹框 */
$(function() {
    require.async(["common:widget/ui/dialog/dialog.js", "common:widget/ui/template/template.js", "common:widget/map/map.js", "common:widget/ui/passport/passport.js", "common:widget/ui/passport_bind_phone/passport_bind_phone.js"],
            function(Dialog, Baidu, Map, loginApi, passBindPhone) {
                var bdTpl = Baidu.template;


                var g_ajax_cinema_groupon_url = "/pcindex/main/cinemadeal";
                $("#j-cinema-info-list").on("click", function(e) {
                    var $target = $(e.target);
                    var buyType = $target.attr("data-buy-type");
                    var cinema = $.parseJSON($target.parents(".cinema-info").attr("data-cinema"));

                    switch(buyType) {
                                                case "map": {
                            displayMapDlg(cinema);
                            return false;
                        }
                    }
                });
                function displayMapDlg(cinema) {
                    var shopData = {"shop":[cinema]};
                    initMapDlg();
                    function initMapDlg() {
                        //地图点击弹框
                        var map_dlg_func = {
                            onshow: function(shopData) {
                                $("#j-md-shop-all").hide();
                                showSearchInfoWindow(shopData);
                            }
                        };

                        var options_dlg = {
                            "width": "auto",
                            "height": "auto",
                            "title" : '查看全图'
                        };

                        var tpl = $("#tpl-map-dlg").html();

                        var data = {
                            "shopName": shopData.shop[0].name,
                            "count": shopData.count,
                            "shopList": shopData.shop,
                            "singleShop": true
                        }

                        var html = bdTpl(tpl, data);

                        options_dlg.content = html;

                        var opt = $.extend(options_dlg, map_dlg_func);

                        var dlg = new Dialog(opt);
                        dlg.center();
                        dlg.show(shopData);

                        return false;
                    }

                    function showSearchInfoWindow(shopData) {
                        var bdMap = new Map("j-md-map-view");
                        bdMap.autoView(shopData.shop)
                                .addPin(shopData.shop, markerClickHandler);

                        if(1==shopData.count || !shopData.count) {
                            bdMap.zoomTo(15);
                        }

                        var shopInfo = shopData.shop[0];

                        var siw_options = {
                            title  : shopInfo.name,      //标题
                            width  : 290,             //宽度
                            height : "auto",              //高度
                            panel  : "j-search-result",         //检索结果面板
                            enableAutoPan : false,     //自动平移
                            enableSendToPhone: false, //是否启用发送到手机
                            searchTypes   :[
                                BMAPLIB_TAB_TO_HERE, //到这里去
                                BMAPLIB_TAB_SEARCH,   //周边检索
                                BMAPLIB_TAB_FROM_HERE //从这里出发
                            ]
                        }

                        var siw_content='<p style="line-height: 150%;">'+shopInfo.address+'<br/>'+shopInfo.phone+'</p>';

                        var bdSIW = new Map.searchInfoWindow(bdMap.getMapOrigin(), siw_content, siw_options);

                        var lat = shopInfo.baidu_latitude, lng = shopInfo.baidu_longitude;
                        bdMap.zoomTo(15).centerTo({"baidu_latitude":lat, "baidu_longitude":lng});
                        //var marker = new Map.Marker({"baidu_latitude":lat, "baidu_longitude":lng});
                        bdSIW.open({"baidu_latitude":lat, "baidu_longitude":lng});

                        $("#j-md-branch-view .shop-branch").on("click", markerClickHandler);

                        function markerClickHandler(type, target) {
                            bdSIW.open({"baidu_latitude":lat, "baidu_longitude":lng});
                        }
                    }
                }
                var current_request;
                function displayBookDlg(cinema) {
                                    }

                function displayGrouponDlg(cinema) {
                    var url = g_ajax_cinema_groupon_url+"?cinemaid="+cinema.uid;
                    $.getJSON(url, function(jsonData) {
                        if(0 != jsonData.errno) {
                            return false;
                        }

                        var tpl = $("#j-dlg-cinema-groupon").html();

                        var options_dlg = {
                            "width": "630px",
                            "height": "auto",
                            "title" : cinema.name+"团购购票"
                        };

                        options_dlg.content = bdTpl(tpl, {"deal":jsonData.data.dealList});

                        var dlg = new Dialog(options_dlg);
                        dlg.center();
                        dlg.show();
                    });

                }
            });

});
