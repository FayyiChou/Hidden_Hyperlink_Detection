<script type="text/javascript">
    $(function () {
        //记录点击次数  add by jackiesun
        $("#Gbanner").click(function () {
            var ImageID = parseInt($(this).attr("ImgID"));
            if (isNaN(ImageID))
                return;
            var imgName = $(this).attr("imgName");
            $.ajax({
                url: "/cgi/AdvertiseHandler.ashx?method=dvertiseClick&r=" + Math.random() + "&ImageID=" + ImageID + "&imgName=" + imgName,
                type: "get",
                success: function (data) {

                }
            });
        })
    })
</script>

