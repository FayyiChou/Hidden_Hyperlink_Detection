    <script language="javascript" type="text/javascript">
        var cPwd = /^[0-9a-zA-Z]{8,16}$/; //密码验证
        //密码验证
        var checkPwd = function (id) {
            var txt = $(id);
            var password = txt.val();
            txt.next("img").remove();
            if (password == "") {
                txt.addClass("highlight2");
                txt.next("li").remove();
                txt.after("<li class='error'>请输入密码<li>");//添加文本节点
                return false;
            }
            if (password.length < 8 || password.length > 16) {
                txt.addClass("highlight2");
                txt.next("li").remove();
                txt.after("<li class='error'>长度必须大于等于8位小于等于16位<li>");//添加文本节点
                return false;
            }
            if (!IsNumAndCharCom(password)) {
                txt.addClass("highlight2");
                txt.next("li").remove();
                txt.after("<li class='error'>密码需要由数字字母结合2种以上组合<li>");
                return false;
            }
            if (id === "#txtPassword") {
                var password = $.trim(txt.val());
                if (IsEnglishCharacterValedate(password)) {
                    txt.addClass("highlight2");
                    txt.next("li").remove();
                    txt.after("<li class='error'>密码不能是全英文<li>");//添加文本节点
                    return false;
                } else if (IsNumCharacterValedate(password)) {
                    txt.addClass("highlight2");
                    txt.next("li").remove();
                    txt.after("<li class='error'>密码不能是全数字<li>");
                    return false;
                }
            }
            if (!cPwd.test($(id).val())) {
                return note($(id), false);
            }
            else {
                txt.next("li").remove();
                txt.removeClass("highlight2");
                return note($(id), true);
            }
        }
        var checkConfirmPwd = function (id) {
            if ($.trim($("#txtPassword").val()) != "") {
                var txt = $(id);
                if ($.trim($(id).val()) != $.trim($("#txtPassword").val())) {
                    txt.addClass("highlight2");
                    txt.next("li").remove();
                    txt.after("<li class='error'>前后密码不一致<li>");
                    return false;
                } else {
                    txt.removeClass("highlight2");
                    txt.next("li").remove();
                    return note($(id), true);
                }
            }
        }
        //验证结果样式显示
        var note = function (obj, vali) {
            if (vali) {
                yes(obj); //验证通过样式
            }
            else {
                no(obj); //验证失败样式
            }
            return vali;
        }
        //验证通过样式
        var yes = function (obj) {
            obj.next("img").remove();
            obj.after("<img src='images/sucess.png'/>");
        }
        //验证失败样式
        var no = function (obj) {
            obj.next("img").remove();
            obj.after("<img src='images/failed.png'/>");
        }
        /*验证是否全英文*/
        var IsEnglishCharacterValedate = function (strin) {
            var patrn = /^[A-Za-z]+$/;
            if (!patrn.exec(strin)) return false
            return true
        }
        /*验证是否全数字*/
        var IsNumCharacterValedate = function (strin) {
            var patrn = /^[0-9]+$/;
            if (!patrn.exec(strin)) return false
            return true
        }
        /*密码只能是数字、字母的组合*/
        var IsNumAndCharCom = function (str) {
            if (!cPwd.exec(str)) return false;
            return true;
        }

        $(document).ready(function () {
            $('.logon_box_bg').css("opacity", 0.5);
            var logonNumber = 0;
            var va_code = '/cgi/Valid_Code.ashx';
            var disValiCode = function () {
                //var logonNumber_ = $.ajax({ url: '/cgi/UserLogonHandler.ashx?method=GetLoginNumber&from=' + (new Date().getTime().toString(36)), async: false }).responseText;
                //logonNumber = parseInt(logonNumber_);                
                //if (logonNumber > 2) {
                    $('.logon_box').css({ height: '320px', marginTop: '40px' });
                    $('.logon_box_bg').css({ height: '320px' });
                    $('.logon-code').show();
                    $('#idValiCode').val('');
                    dqCode();
                //} else {
                //    $('.logon_box').css({ height: '274px', marginTop: '63px' });
                //    $('.logon_box_bg').css({ height: '274px' });
                //    $('.logon-code').hide();
                //};
            };
            var disKey = function () {
                var j = $(this), R1 = /^[0-9a-zA-Z_]{2,25}$/, R2 = /^[0-9]{4}$/, R;
                var inp_jy = j.attr('jy');
                var v = $.trim(j.val());
                R = (inp_jy == '0' ? R1 : R2);
                if (!R.test(v) && v != '') {
                    j.addClass('js-inp-err');
                } else {
                    j.removeClass('js-inp-err');
                };
            };
            var dqCode = function () {
                $('#js-png-code').attr('src', va_code + '?keys=' + (new Date().getTime().toString(36)));
            };
            var _submit = function (username, password, code) {
                $.post('/cgi/UserLogonHandler.ashx?method=UserLogin&from=' + (new Date().getTime().toString(36)),
                    { LoginName: username, Password: password, CheckCode: code },
                    function (data) {
                        var keys = $.trim(data);
                        if (keys == '1' || keys == '2') {                           
                            //alert(keys);
                            //add by 徐国海 on 20140510--start
                            //增加登录后更新成长值信息（为成长值扩展信息不存在的进行添加）
                            $.post('/cgi/UserPoints/UserPointHandler.ashx?method=GetAccountExt', null, function (data) {
                                var jsonAccount = eval('(' + data + ')');
                                if (jsonAccount.info != 'SUCCESS') {
                                    alert('用户成长值读取失败：' + jsonAccount.result);
                                }
                            });
                            //add by 徐国海 on 20140510--end                            
                            
                            //GJGD-3087
                            if (keys == "2") {
                                $.ajax({
                                    type: "post",
                                    url: "/cgi/UserLogonHandler.ashx?method=ExistMissingCustomer",                                   
                                    data: { LoginUser: username },
                                    async: false,
                                    success: function (data) {

                                    }
                                });
                            }

                            location.href = '/defaults.html?upe=' + keys + '&uid=' + (new Date().getTime().toString(36));
                            return (true);
                        };
                        $('#idPassWord').val('');
                        //logonNumber += 1;
                        //if (logonNumber > 2) {
                        //    disValiCode();
                        //    $('#idValiCode').val('');
                        //    dqCode();
                        //};
                        disValiCode();
                        if (keys == 'error') {
                            alert('抱歉系统维护中；给您带来不便深表歉意!咨询电话:021-26123111(工作时间周一到周5　9：00－18：00)');
                            return (false);
                        };
                        alert(data);
                        _from();
                    });
            };
            var _from = function () {
                var username = $('#idUserName');
                var password = $('#idPassWord');
                var valicode = $('#idValiCode');
                if ($.trim(username.val()) == "") {
                    username.get(0).focus();
                    return (false);
                };
                if ($.trim(password.val()) == "") {
                    password.get(0).focus();
                    return (false);
                };
                if (logonNumber > 2) {
                    if ($.trim(valicode.val()) == "") {
                        valicode.get(0).focus();
                        return (false);
                    };
                };
                
                //_submit(username.val(), password.val(), (logonNumber > 2 ? (valicode.val()) : ''));
                //add by zhj on 20150623 GJGD-1636 用户信息安全 --start
                $.post("/cgi/UserLogonHandler.ashx?method=ExistDomestic"
                    , { LoginName: username.val() }
                    , function (data) {
                        if (data != "SUCCESS") {
                            _submit(username.val(), password.val(), valicode.val());
                            return;
                        }
                        var elass = new Elass.Base();
                        elass.confirm({
                            tle: "国际用户修改密码",
                            width: 600,
                            height: 300,
                            css: 'padding:0',
                            isClose: true,
                            msg: "<div class=\"content9\">" +
                                    "<ul>" +
                                        "<li><span>原密码:</span><input id=\"txtOldPwd\" type=\"password\" /></li>" +
                                        "<li><span>新密码:</span><input id=\"txtPassword\" type=\"password\" onblur=\"checkPwd('#txtPassword');\" /></li>" +
                                        "<li><span>确认密码:</span><input id=\"txtConfirmPassword\" type=\"password\" onblur=\"checkConfirmPwd('#txtConfirmPassword');\"/></li>" +
                                    "</ul>" +
                                "</div>",
                            bak: false,
                            buttonList: [{
                                text: "确定", bak: function () {
                                    var oldPwd = $("#txtOldPwd").val();
                                    var pwd = $("#txtPassword").val();
                                    $.post("/cgi/UserLogonHandler.ashx?method=UpdatePwd"
                                        , { LoginName: username.val(), newPwd: pwd, oldPwd: oldPwd }
                                        , function (data) {
                                            if (data == "SUCCESS") {
                                                _submit(username.val(), pwd, valicode.val());
                                            } else {
                                                alert(data);
                                            }
                                        });
                                }
                            }, {
                                text: "返回", bak: function () {
                                    elass.hide();
                                }
                            }]
                        });
                    });
                //add by zhj on 20150623 GJGD-1636 用户信息安全 --end
            };

            $('#idUserName, #idPassWord, #idValiCode').val('').keyup(disKey);
            disValiCode();
            $('#btnLogin').click(_from);
            $('#btn-code-png').click(dqCode);
            $(document).keydown(function (event) {
                if (event.keyCode == '13') {
                    _from();
                }
            });
            //设置广告图片地址BEGIN
            //update by qinyue 2014-06-25 GJGD-178 广告管理  广告图片从数据库动态读取---------start------------------
            var imgList = $.ajax({ url: "/cgi/AdvertiseHandler.ashx?method=GetLoginAdvs&r=" + Math.random(), type: "get", async: false }).responseText;
            CkHbiii({ target: $('#Gbanner'), button: $('#Gbanner_BUTTON'), time: 5, item: eval('(' + imgList + ')') });
            //--------------------------------------------------------------------------------end--------------------
            //设置广告图片地址END          
        });

    </script>
