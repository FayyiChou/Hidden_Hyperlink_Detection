  <script>
  function bse(a){
	  
	 $("#img_"+a).addClass("imgcur"); // 追加样式 
	  
	   $("#w_"+a).addClass("wcur"); // 追加样式 
	  
  }
  
   function sse(a){
	  
	 $("#img_"+a).removeClass("imgcur"); // 删样式 
	  
	   $("#w_"+a).removeClass("wcur"); // 删样式 
	  
  }
  
  </script>  
