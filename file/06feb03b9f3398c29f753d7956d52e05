        <script type="text/javascript">
            // var clock;
            $(document).ready(function () {
                $('.clock').each(function () {
                    var clock;

                    clock = $(this).FlipClock({
                        clockFace: 'DailyCounter',
                        autoStart: false,
                        callbacks: {
                            stop: function () {
                                $('.message').html('The clock has stopped!')
                            }
                        }
                    });

                    clock.setTime($(this).data('time'));
                    clock.setCountdown(true);
                    clock.start();
                });
				
				var el = document.getElementById('action_gallary');
					el = el.getElementsByTagName('li');
					el = el[0];
					el_left = el.getElementsByClassName('action_left_block ');
					el_left = el_left[0];
					var pic = el_left.getElementsByClassName('preview_picture ');
					el_right = el.getElementsByClassName('action_right_block');
					el_right = el_right[0];
					var desc = el_right.getElementsByClassName('action_right_block_text');
					var left_count = pic.length;
					var right_count = desc.length;
				var k = 1;
				var timer1;
				var timer2;
			
				
				function action(){
					//console.log('right_count=');
							//console.log(right_count);
							//console.log('left_count=');
							//console.log(left_count);
							//console.log('k = '+k);
					if (left_count > k){	
							
							timer1 = setTimeout(function(){
								$('.action_left_block').animate({marginTop: '-=260px'}, 1000);
								if (right_count > k){	
									$('.action_right_block').animate({marginTop: '-=260px'}, 1000);	
								}
								left_count--;
							//	k++;
							//	if (left_count == k){
									//clearTimeout(timer1);
							//		k = 1;
							//	} 
								if (!!timer2){
									clearTimeout(timer2);
								}							
								action();
							}, 4000);			
					} else {		
					timer2 = setTimeout(function(){
						$('.action_left_block').animate({marginTop: '0px'}, 10);
						$('.action_right_block').animate({marginTop: '0px'}, 10);
						$('#action_gallary_next').click(); 
						}, 8000);
						//
					
					}
				};
				action();
				$('#action_gallary_next').click(function(){
					$('.action_left_block').animate({marginTop: '0px'}, 10);
					$('.action_right_block').animate({marginTop: '0px'}, 10);
					//console.log('next');
					console.log('clear' + timer1);
					console.log('clear' + timer2);
					if (!!timer1){
						clearTimeout(timer1);
					}
					if (!!timer2){
						clearTimeout(timer2);
					}
					
					el = document.getElementById('action_gallary');
						//el = el.getElementsByTagName('li');
						el = el.getElementsByClassName('news-item');
						el = el[1];
						el_left = el.getElementsByClassName('action_left_block ');
						el_left = el_left[0];
						pic = el_left.getElementsByClassName('preview_picture ');
						el_right = el.getElementsByClassName('action_right_block');
						el_right = el_right[0];
						desc = el_right.getElementsByClassName('action_right_block_text');
						left_count = pic.length;

						right_count = desc.length;
					action();
				});
				
				$('#action_gallary_prev').click(function(){
					
					$('.action_left_block').animate({marginTop: '0px'}, 10);
					$('.action_right_block').animate({marginTop: '0px'}, 10);
					if (timer1){
						clearTimeout(timer1);
					}
					if (timer2){
						clearTimeout(timer2);
					}
					action();
				});
			});
        </script>

