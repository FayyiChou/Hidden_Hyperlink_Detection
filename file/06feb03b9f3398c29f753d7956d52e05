                prev: '#section_gallary_prev',
                next: '#section_gallary_next',
                mousewheel: false,
                swipe: {
                    onMouse: true,
                    onTouch: true
                }
            });
            // $("#section_gallary").trigger("pause");

            $("#section_gallary_prev").hover(function () {
                $('#section_gallary').carouFredSel({
                    width: '100%',
                    direction: "right",
                    scroll: {items: 1, duration: 1000},
                    auto: true,
                    prev: '#section_gallary_prev',
                    next: '#section_gallary_next',
                    mousewheel: false,
                    swipe: {
                        onMouse: true,
                        onTouch: true
                    }
                });
                $("#section_gallary").trigger("play", -2000);
            }, function () {
                $("#section_gallary").trigger("pause");
            });

            $("#section_gallary_next").hover(function () {
                $('#section_gallary').carouFredSel({
                    width: '100%',
                    direction: "left",
                    scroll: {items: 1, duration: 1000},
                    auto: true,
