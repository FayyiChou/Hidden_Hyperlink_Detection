    <script type="text/javascript">
        $(".sort_display a:not(.current)").on("click", function () {
            $(this).addClass("current").siblings().removeClass("current");
        });
        $(".number_list a:not(.current)").on("click", function () {
            $(this).addClass("current").siblings().removeClass("current");
        });

        $(document).on('click', '.more_text_ajax', function () {
            if (!$(this).hasClass('loading')) {
                var url = $(this).closest('.ajax_load').find('.module-pagination .flex-direction-nav .flex-next').attr('href'),
                    th = $(this);
                th.addClass('loading');

                $.ajax({
                    url: url,
                    data: {"ajax_get": "Y"},
                    success: function (html) {
                        var new_html = $.parseHTML(html);
                        th.removeClass('loading');
                        if ($('.display_list').length) {
                            $('.display_list').append(html);
                        } else if ($('.catalog_block').length) {
                            $('.catalog_block .bx_catalog_top_home').append(html);
                        } else if ($('.module_products_list').length) {
                            $('.module_products_list tbody').append(html);
                        }
                        $('.bottom_nav').html($(html).find('.bottom_nav').html());
                    }
                })
            }
        });
				function LoadContent() {
			$('.more_text_ajax')[0].click();
			setTimeout (function () {
				if ($('.more_text_ajax').length > 0) {
					LoadContent();
				} else {
					$('.wrap_nav').remove();
				}
			}, 1000);
			
		}
		LoadContent();
		    </script>

