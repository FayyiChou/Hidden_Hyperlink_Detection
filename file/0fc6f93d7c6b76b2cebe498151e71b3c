<script charset="UTF-8" src="//webchat.7moor.com/javascripts/json2.js"></script><script type="text/javascript">
    function showInfo(){
        var isShow = document.getElementById('error-msg').style.display;
        if(isShow == 'none'){
            document.getElementById("showInfo").className = 'icon-down';
            document.getElementById('error-msg').style.display = 'inline-block';
        }
        if(isShow == 'inline-block'){
            document.getElementById("showInfo").className = 'icon-zhan';
            document.getElementById('error-msg').style.display = 'none';
        }
    }

</script>
