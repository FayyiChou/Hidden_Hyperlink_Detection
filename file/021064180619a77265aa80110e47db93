    <script>
        $(document).ready(function(){
            //首页判断icon出现
            var nowdate = Math.round(new Date().getTime()/1000);
            var img = '<img src="/r/cms/www/default/images/new_icon.gif" style="margin-left: 5px;"/>';

            var len = $(".home_box ol span.right").length;
            for(var i=0;i<len;i++){
                var $obj = $(".home_box ol span.right").eq(i);
                var rel = $obj.attr("rel");
                var relStr = DateToUnix(rel);
                if(nowdate - relStr < 604800){//7天
                    $obj.parent("a").append(img);
                }else{
                    $obj.parent("a").find("img").remove();
                }
            }
        });

        function DateToUnix(string) {
            var f = string.split(' ', 2);
            var d = (f[0] ? f[0] : '').split('-', 3);
            var t = (f[1] ? f[1] : '').split(':', 3);
            return (new Date(
                            parseInt(d[0], 10) || null,
                            (parseInt(d[1], 10) || 1) - 1,
                            parseInt(d[2], 10) || null,
                            parseInt(t[0], 10) || null,
                            parseInt(t[1], 10) || null,
                            parseInt(t[2], 10) || null
                    )).getTime() / 1000;
        };

    </script>

