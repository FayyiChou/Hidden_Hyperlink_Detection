<script language="javascript">
function info(id){
        for(var i=1;i<=4;i++){  
			document.getElementById('menu_right'+i).className="jt_off";
			document.getElementById('con_right'+i).style.display="none"; 
        }
           document.getElementById('con_right'+id).style.display=""; 
		   document.getElementById('menu_right'+id).className="jt_on";	
    }
</script> 
<script language="javascript">
function tab(id){
        for(var i=1;i<=4;i++){  
			document.getElementById('menu'+i).className="jt_off";
			document.getElementById('con'+i).style.display="none"; 
        }
           document.getElementById('con'+id).style.display=""; 
		   document.getElementById('menu'+id).className="jt_on";	
    }
</script> 
