$(function(){ 
$(window).resize(function(){ goTop();});
$(window).scroll(function(){ goTop();});
});
function goTop(){ 

$(".float").stop().animate({ "top":($(window).height()+$(window).scrollTop()-($(window).height()+$(".float").height())/2)<200 ? 200 : 
$(window).height()+$(window).scrollTop()-($(window).height()+$(".float").height())/2},800);
}
</script> 
