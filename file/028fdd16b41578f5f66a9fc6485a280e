   <script>
    function getCookie(name){
        var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
        if(arr=document.cookie.match(reg)){
            return unescape(arr[2]);
        }else{
            return '';
        }
            
    }
    function getcheck(name){
        var chk_value =[]; 
        $('input[name="'+name+'"]:checked').each(function(){ 
            chk_value.push($(this).val()); 
        }); 
        return chk_value;   
    }
    
    var vote_url="http://trade.hxyjw.com/conter/ajaxVote";
    $(document).on('click','.v-black-btn',function(){
        var obj         =$(this);
        var vote_id     =obj.attr('value');
        var user_cookie =getCookie('hxyjw_passport');
        var itemList    =getcheck('item');
        $.ajax({
            type:'POST',
            url :vote_url,
            data:{'vote_id':vote_id,user_cookie:user_cookie,item:itemList},
            dataType:'json',
            success:function(redata){
                if('1'==redata.code){
                    window.location.href='http://passport.hxyjw.com';
                }else if('2'==redata.code){
                    alert('你已经投过票!');
                    // console.log(obj.parents('.tp-list').get(0).outerHTML);
                    $('.tp-list').html(redata.html);
                }else{
                    $('.tp-list').html(redata.html);
                }
            }
        });
    });

</script>   
