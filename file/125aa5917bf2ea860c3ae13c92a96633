    function changeCode(obj){
        var rnd = "dongtai_1431944482"+Math.round(Math.random()*200000);
        var $obj = $(obj);
        var $form = $obj.parents('form');
                //var verify_img = $('img[name=verify_img]');
        $('input[name=hiddencode]', $form).val(rnd);
        $('img[name=verify_img]', $form).attr("src", 'http://kanfangtuan.focus.cn/api/captcha/Captcha?hiddencode='+rnd );
    }
    function changeEgouCode(obj){
        var rnd = "dongtai_1431944482"+Math.round(Math.random()*200000);
        $(obj).attr('src', 'http://admin.egou.focus.cn/api/captionCode/?v='+rnd);
    }
