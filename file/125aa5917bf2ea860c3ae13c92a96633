    <script type="text/javascript">
        $(function() {
            var bObj = [];
            $('.banner').each(function(i, item){
                bObj[i] = $(item).unslider({
                    fluid: true,
                    dots: true,
                    speed: 500,
                    delay: 3000
                });
            });
            $('.unslider-arrow').click(function() {
                var index = Math.floor($(this).index(".unslider-arrow")/2);
                var fn = this.className.split(' ')[1];
                bObj[index].data("unslider")[fn]();

            });
            $('.banner').find(".dot").html("");
            $('.banner').hover(function(){
                $(this).find(".unslider-arrow").show();
            },function(){
                $(this).find(".unslider-arrow").hide();
            })
