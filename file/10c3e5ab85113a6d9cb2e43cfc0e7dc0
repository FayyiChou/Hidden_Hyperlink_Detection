<script type="text/javascript">	
	CE.process(function(){
		/*
		 * 根据不同的时间段，返回不同的问候语
		 */
		function welcomeWords(){
			var d = new Date();
			var hour = d.getHours();
			if (hour >= 0 && hour < 8) {
				return i18n_Members_welcomeinfo01_goodmorning;
			} else if (hour >= 8 && hour < 12) {
				return i18n_Members_welcomeinfo01_goodforenoon;
			} else if (hour >= 12 && hour < 13) {
				return i18n_Members_welcomeinfo01_goodnoon;
			} else if (hour >= 13 && hour < 18) {
				return i18n_Members_welcomeinfo01_goodafternoon;
			} else if (hour >= 18 && hour < 24) {
				return i18n_Members_welcomeinfo01_goodnight;
			}
		}	
		$("#timewords").html(welcomeWords());
		$("#timewordsBeforeLogin").html(welcomeWords());
		//注册退出链接click事件
