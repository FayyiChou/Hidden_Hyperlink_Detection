		$("#loginAccount").blur(function(){
			var usernameprompthid = $("#usernameprompthid").val();
			var loginAccount = $("#loginAccount").val();
			if(loginAccount == "" || loginAccount == null) {
				$("#loginAccount").val(usernameprompthid);
			}
		});
		$("#loginAccount").focus(function(){
			var usernameprompthid = $("#usernameprompthid").val();
			var loginAccount = $("#loginAccount").val();
			if(loginAccount == usernameprompthid) {
				$("#loginAccount").val("");
			}
		});
		$("#password").blur(function(){
			var compId = "Members_loginCheck01-003";
			var passwordstr = trim($("#password").val());
			var passwordWrongObj = document.getElementById(compId + "_passwordWrong");
			var passwordErrObj = document.getElementById(compId + "_passwordErr");
			if(passwordstr == "" || passwordstr == null) {
				// 密码未输入提交，提示输入密码
				   passwordWrongObj.style.display='none';
				   passwordErrObj.style.display='';
			}
		});
