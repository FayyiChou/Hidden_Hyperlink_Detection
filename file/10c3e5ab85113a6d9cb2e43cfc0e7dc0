<script>
CE.process(function(){
	OrderCart_showMini01.compId="OrderCart_showMini01-1407721484565";
	OrderCart_showMini01.showCartsURL="_self";
	OrderCart_showMini01.showCartsURLString="/order_mycart.html";
	$('#OrderCart_showMini01_winclose').bind('click', function() {
	  OrderCart_showMini01.hideTipLayer();
	});
	if("true"=="true"){
		$('#OrderCart_showMini01_shopping-stat').bind({
				mouseover: function(event) {
			      OrderCart_showMini01.showPop(event,$('#OrderCart_showMini01_shopping-stat'));
			    },    
			    mouseout: function(event) {
			      OrderCart_showMini01.hidePoP(event,$('#OrderCart_showMini01_shopping-stat'));
			    }  
		});
	}
	$('#orderCheckOutintoMyCarts').bind('click', function() {
		  OrderCart_showMini01.intoMyCarts();
	});
	$('#showCartsintoMyCartsforAdd').bind('click', function() {
		  OrderCart_showMini01.intoMyCarts();
	});
	$('#showCartsintoMyCarts').bind('click', function() {
		  OrderCart_showMini01.intoMyCarts();
	});
	$('#ordercart_continueBuyURLString').bind('click', function() {
		if("/products_list.html"==""){
			//window.history.go(-1);
			OrderCart_showMini01.hideTipLayer();
		}else{
			window.open("/products_list.html","_self");
		}
	});
	jQuery.ajax({
		type : "GET",
		url : "/OrderCart.do?method=getCartsAmount",
		data : "randomid="+Math.random(),
		dataType : "html",
		cache : false,
		success : function(data) {
			var json = eval(data);
			if($("#outamountspan").length>0){
				$("#outamountspan").html(json[0].cartsAmount);
			}
			if($("#outtotalacountspan").length>0){
				$("#outtotalacountspan").html(json[0].outtotalacountspan);
			}
		}
	});
});
</script>
