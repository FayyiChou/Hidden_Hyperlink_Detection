	</div><script type="text/javascript">
	CE.process(function() {
		jQuery('#ProductKeyword_keyword01-key001 a.keywordName').bind(
				'click',
				function() {
					searchProducts('ProductKeyword_keyword01-key001', this,
							jQuery(this).metadata().kid, jQuery(this)
									.metadata().kname);
					return false;
				});
		function searchProducts(compId, comp, keywordId, keyword) {
			var ajaxHref = "/Product.do";
			jQuery.ajax({
				async : false,
				dataType : 'json',
				url : ajaxHref,
				type : 'post',
				data : {
					method : 'keyword',
					keywordId : keywordId
				},
				timeout : 5000,
				cache : false,
				error : function() {
					//alert('Error loading XML document');
				},
				success : function(data) {
					// jQuery("#" + ProductCompare_float01.compId +
					// "_productList")
					// .html(xml);
					//alert(data.views);
				}
			});
			// 提交表单
			jQuery("input[id^='" + compId + "_keyword']").val(keyword);
			jQuery("#" + compId + "_productKeyword").submit();
		}
	});
</script> 
