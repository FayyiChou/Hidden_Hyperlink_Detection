                <script type="text/javascript">	
	var News_list01 = {
		/** 
		*   根据参数获取浏览次数
		*	infoId:资讯ID
		*	degreeStr：浏览次数所在标签ID
		**/
		getAjaxValueByNewsId:function(infoId,degreeStr){
			$.post("/News.do", "method=getVisitedCountByAjax2&newsId="+infoId, function(res){
				var degreeTarget = document.getElementsByName(degreeStr);
				for(var i=0;i<degreeTarget.length;i++){
					degreeTarget[i].innerHTML = res;
				}
			});
		}
	};
	// <![CDATA[
	News_list01['News_list01-1434086905679_init'] = function(){
		var showDegree=0;
		if(showDegree == '1'){
				News_list01.getAjaxValueByNewsId("22","News_list01-1434086905679_VisitedCount_1");
				News_list01.getAjaxValueByNewsId("21","News_list01-1434086905679_VisitedCount_2");
				News_list01.getAjaxValueByNewsId("15","News_list01-1434086905679_VisitedCount_3");
				News_list01.getAjaxValueByNewsId("17","News_list01-1434086905679_VisitedCount_4");
				News_list01.getAjaxValueByNewsId("18","News_list01-1434086905679_VisitedCount_5");
				News_list01.getAjaxValueByNewsId("19","News_list01-1434086905679_VisitedCount_6");
		}		
	}
	function setVisited(id,linkUrl,detailOpenStyle){	
		//读取资讯的浏览次数！
		$.get("/News.do", "method=getVisitedCountByAjax&newsId="+id, function(data){
			var linkUrl1 = linkUrl;
			var OpenStyle = detailOpenStyle
			var url = '/News.do?method=setVisitedCountByAjax&newsId='+id+'&times='+data;
			//保存资讯的浏览次数！
			$.post(url, function(data){
				window.open(linkUrl1,OpenStyle);
				});
		},'html');
	}
	$(News_list01['News_list01-1434086905679_init']);
	// ]]>
</script>
