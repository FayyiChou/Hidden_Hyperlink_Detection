<script type="text/javascript">
	//闭店状态
	var url = window.location.href;
	var randomNumberString = "";
	var randomNumberSecretString = "";
	if(url.indexOf("randomNumberString=") != -1 && url.indexOf("randomNumberSecretString=") != -1){
		var es1 = /randomNumberString=/;
		es1.exec(url);
		var right1 = RegExp.rightContext;
		randomNumberString = right1.substring(0,3);
		
		var es2 = /randomNumberSecretString=/;
		es2.exec(url);
		var right2 = RegExp.rightContext;
		randomNumberSecretString = right2.substring(0,6);
	}
	function createXHR(){
		if(typeof XMLHttpRequest != "undefined"){
			return new XMLHttpRequest();
		}else if(typeof ActiveXObject != "undefined"){
			if(typeof arguments.callee.activeXString != "string"){
				var versions = ["MSXML2.XMLHttp.6.0","MSXML2.XMLHttp.3.0","MSXML2.XMLHttp"];
				for(var i = 0 , len = versions.length ; i < len; i++){
					try{
						var xhr = new ActiveXObject(versions[i]);
						arguments.callee.activeXString = versions[i];
						return xhr;
					}catch(ex){
					}
				}
			}
			return new ActiveXObject(arguments.callee.activeXString);
		}else{
			throw new Error("No XHR object available.");
		}
		
	}
	var xhr = createXHR();
	xhr.onreadystatechange = function(){
		if(xhr.readyState == 4){
			if((xhr.status >= 200 && xhr.status < 300 ) || xhr.status == 304 ){
				//后台点进来的  不需要处理  可以正常访问
				if(xhr.responseText == 'nocheck'){
				//网站闭店跳转到闭店公告页面
				}else if(xhr.responseText == '0'){
					window.location.href = '/tip_site_close.html';
				}
			}
		}
	};
	xhr.open("post","/Public.do?method=checkStatus&randomNumberString="+randomNumberString+"&randomNumberSecretString="+randomNumberSecretString,"true");
	xhr.send(null);
</script>  
