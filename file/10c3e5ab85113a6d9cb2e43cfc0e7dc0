	//<![CDATA[
	var Help_listByMultiCategory01 = {
		getUrlParam : function(name) { // 获取URL参数
			var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
			var r = window.location.toString().match(reg);
			if (r != null)
			 if(r[2]!=null){
				var r2=r[2].replace(".html","");
				return unescape(r2);
			 }
			return null;
		},
		setCurrent: function(compId){
			var extCurrentHelp = this.getUrlParam(compId+"FHId");
			if(extCurrentHelp == null){
				extCurrentHelp =  $("#"+compId+" ul > li:first").attr("id");
			}else{
				extCurrentHelp = "help"+extCurrentHelp;
			}
			jQuery("#"+ compId + " ul > li > a ").removeClass();
			jQuery("#"+ compId + " #" + extCurrentHelp + " a").addClass("current"); 
		}
   }
	Help_listByMultiCategory01['Help_listByMultiCategory01-001_init'] = function (){
		Help_listByMultiCategory01.setCurrent('Help_listByMultiCategory01-001');
	}
	$(Help_listByMultiCategory01['Help_listByMultiCategory01-001_init']);
