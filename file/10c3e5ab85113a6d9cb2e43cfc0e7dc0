            <div class="columnSpace" id="elem-Column_navigation01-1404118441593" name="栏目导航"> 
              




<script type="text/javascript">
		//<![CDATA[
		var Column_navigation01_02 = {

		getUrlParam : function(name) { // 获取URL参数
			var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
			var r = window.location.toString().match(reg);
		
			if (r != null)
			 
			 if(r[2]!=null){
				var r2=r[2].replace(".html","");
				return unescape(r2);
			 }
				
			return null;
		},
		//导航样式2效果
		extEvent2:function(compId, showSecond,menuLocationType){
			
			/*加一级图片默认图 开始*/
			var zis=$("#" + compId + " ul.nav-first li[id^='column']").size();
				for (var j=1; j <=zis ; j++) {
					var clumnDefaultPic=$("#" + compId + " ul.nav-first li.nav-first" + j);
					var clumnDefaultPicA=$("#" + compId + " ul.nav-first li.nav-first" + j).find("a");
					if($("#" + compId + " ul.nav-first li.nav-first" + j).attr("clumnDefaultPictuire") != ""){
						clumnDefaultPicA.css("background-image","url(" + clumnDefaultPic.attr("clumnDefaultPictuire") + ")");
					}
				}
			/*加一级图片默认图 结束*/
				
			/*取一级当前选中栏目 开始*/	
			var extCurrentColumn = this.getUrlParam(compId+"FCId");
			if(extCurrentColumn != null){
				extCurrentColumn = "column"+extCurrentColumn;
			}
			/*取一级当前选中栏目 结束*/	
			
			/*加入一级当前效果 开始*/
			$("#"+compId+" #"+extCurrentColumn).find("a").removeAttr("style");
			$("#"+compId+" #"+extCurrentColumn).addClass("current");

			var passPictuire=$("#"+compId+" #"+extCurrentColumn);
			var passPictuireA=$("#"+compId+" #"+extCurrentColumn).find("a");
			
			if(passPictuire.attr("clumnPassPictuire") != ""){
				passPictuireA.css("background-image","url(" + passPictuire.attr("clumnPassPictuire") + ")");
			}
			/*加入一级当前效果 结束*/
			
			/*取二级当前选中栏目 开始*/
			var secondColumnId = this.getUrlParam(compId+"SCId");
			//二级显示控制
			if(showSecond){
				if(secondColumnId != null){
					secondColumnId = "cols"+secondColumnId+"Sub";
				}
			/*取二级当前选中栏目 结束*/	
			
				/*加二级图片默认图 开始*/
				var zis2=$("#" + compId + " div.nav-second li[id^='cols']").size();
					for (var j=1; j <=zis2 ; j++) {
						var clumnDefaultPic2=$("#" + compId + " div.nav-second li.nav-second" + j);
						var clumnDefaultPicA2=$("#" + compId + " div.nav-second li.nav-second" + j).find("a");
						if($("#" + compId + " ul.nav-second li.nav-second" + j).attr("clumnDefaultPictuire") != ""){
							clumnDefaultPicA2.css("background-image","url(" + clumnDefaultPic2.attr("clumnDefaultPictuire") + ")");
						}
					}
				/*加二级图片默认图 结束*/
			/*二级显示效果 开始*/
			$("#"+compId+" #"+extCurrentColumn+"Sub").show();
			$("#"+compId+" #"+secondColumnId).addClass("current");
				
			//跟着一级菜单
			if(menuLocationType=="follow"){
				var compIdtype = $("#"+compId);
				var compId_cloum = $("#"+compId+" #"+extCurrentColumn);
				
				if(compId_cloum.offset()){
					//组件位置				
					var compIdOffse=compIdtype.offset().left;
					//一级left位置
					var compId_cloumOffset=compId_cloum.offset().left;
					var compwidth = $("#"+compId).innerWidth();
					
					var ulSubwidth = $("#"+compId+" #"+extCurrentColumn+"Sub"+" ul.align-first").innerWidth();
		
					if(compwidth>=ulSubwidth){
						if(((compIdOffse+compwidth)-compId_cloumOffset)>ulSubwidth){
							$("#"+compId+" #"+extCurrentColumn+"Sub ul.align-first").css("left",compId_cloumOffset-compIdOffse);
						}else{
							$("#"+compId+" #"+extCurrentColumn+"Sub ul.align-first").css("left",(compwidth-ulSubwidth));
						}
					}else{
							 $("#"+compId+" #"+extCurrentColumn+"Sub ul.align-first").css("left","0px");
										
						}
					}
			}
			/*触发一级显示效果 开始*/	
			$("#"+compId+" ul.nav-first li[id^='column']").hover(
				function () {
					 if($(this).attr("id") != extCurrentColumn){
								$("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");	
								
								if($(this).attr("clumnPassPictuire")!= ""){
									$(this).find("a").css("background-image","url("+$(this).attr("clumnPassPictuire")+")");
								}
								
								$(this).addClass("current");
					}	
				 },
				function () {
						 if($(this).attr("id") != extCurrentColumn){
							$(this).removeClass("current");
							$("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");
							
							//加默认图片
							if($(this).attr("clumnDefaultPictuire")!= ""){
								$(this).find("a").css("background-image","url("+$(this).attr("clumnDefaultPictuire")+")");
							}
						}
					 }
			);
			 //加入二级导航当前样式
			 $("#"+compId+" li[id^='cols']").hover(
					function () {
								  if($(this).attr("id") != secondColumnId){	
								  
								      $("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");	
								
										if($(this).attr("clumnPassPictuire")!= ""){
											$(this).find("a").css("background-image","url("+$(this).attr("clumnPassPictuire")+")");
										}
										$("#"+compId+" #"+$(this).attr("id")).addClass("current");
								  }
								
					},
					function () {
								 if($(this).attr("id") != secondColumnId){	
										$(this).removeClass("current");
										
										$("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");
										
										//加默认图片
										if($(this).attr("clumnDefaultPictuire")!= ""){
											$(this).find("a").css("background-image","url("+$(this).attr("clumnDefaultPictuire")+")");
										}
								  }
					}
			);			
		}else{
				//不显示二级
				 $("#"+compId+" ul.nav-first li[id^='column']").hover(
					  function () {
						  if($(this).attr("id") != extCurrentColumn){
							$("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");	
								$(this).addClass("current");
								//加鼠标经过图片
								if($(this).attr("clumnPassPictuire")!= ""){
									$(this).find("a").css("background-image","url("+$(this).attr("clumnPassPictuire")+")");
								}
							}
									
					  },
					  function () {
						if($(this).attr("id") != extCurrentColumn){
							$(this).removeClass("current");
							$("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");
							
							//加默认图片
							if($(this).attr("clumnDefaultPictuire")!= ""){
								$(this).find("a").css("background-image","url("+$(this).attr("clumnDefaultPictuire")+")");
							}
						}
					  }
					);
			}
		}
	}

	Column_navigation01_02['Column_navigation01-1404118441593_init'] = function (){
		Column_navigation01_02.extEvent2("Column_navigation01-1404118441593", "","left");
	}
	$(Column_navigation01_02['Column_navigation01-1404118441593_init']);
	// ]]>
</script>
