$("#cooperationQQ_handle_1").click(function(){
	var thisHost = document.location.hostname;
	//聚合页面的请求URL
	var location = document.location.href; 
	var method = location.substring(("http://"+thisHost).length,location.length);
	method = window.encodeURIComponent(method);
	window.location.href="/Members.do?method=forwardQQLoginPage&url="+method; 
});
