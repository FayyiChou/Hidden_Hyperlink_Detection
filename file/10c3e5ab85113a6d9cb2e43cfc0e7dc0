//<![CDATA[
	CE.process(function(){
		//$("#ProductCategory_show01-1406531571995 .first-li > .show-div").hide();
		$("#ProductCategory_show01-1406531571995 li.first-li").hover(function(){
			$("#ProductCategory_show01-1406531571995 .first-li > a").removeClass("act");
			$("#ProductCategory_show01-1406531571995 .first-li > a").removeClass("noact");
			$("#ProductCategory_show01-1406531571995 .first-li > .show-div").hide();
			if($(this).children("div").hasClass('show-div')){
				$(this).addClass("has-second");
				$(this).addClass("hover");
			}
			//-------------计算悬浮层的悬浮位置 begin---------------
			//组件高度(包括上下边框)
			var compHeight = $("#ProductCategory_show01-1406531571995 div.content").outerHeight();
			//悬浮层高度
			var layerHeight = $(this).find(".show-div").height();
			//alert("compHeight="+compHeight+"layerHeight="+layerHeight);
			if (layerHeight >= compHeight) {
				//与组件顶部对齐
				$(this).children("div.show-div").css("top", "0px");
			}else {
				//上边距高度
				var paddingHeight = $("#ProductCategory_show01-1406531571995 ul.menu-ul").innerHeight() - $("#ProductCategory_show01-1406531571995 ul.menu-ul").height();
				var hoverHeight = paddingHeight / 2;
				for (var i = 0; i < $(this).attr("hoverId"); i++) {
					hoverHeight += $("#ProductCategory_show01-1406531571995 .hoverId" + i).outerHeight();
				}
				//alert("paddingHeight="+paddingHeight+"hoverHeight="+hoverHeight);
				//悬停分类顶部到组件的底部高度
				var firstHeight = compHeight - hoverHeight;
				//alert("layerHeight="+layerHeight+"firstHeight="+firstHeight);
				if (layerHeight > firstHeight) {
					var compBorder = parseInt($("#ProductCategory_show01-1406531571995 div.content").css('borderTopWidth'))||0;
					//alert(compBorder);
					//与组件底部对齐(compBorder - 组件的边框)
					$(this).children("div.show-div").css("top", (compHeight - compBorder - layerHeight) + "px");
				} else {
					//与悬浮分类顶部对齐
					$(this).children("div.show-div").css("top", hoverHeight + "px");
				}
			}
			//-------------计算悬浮层的悬浮位置 end---------------
			$(this).children("div.show-div").show();
			CE.lazyload.imgLoad($(this).find("img"));
		},function(){
			$("#ProductCategory_show01-1406531571995 .first-li > .show-div").hide();
			$(this).removeClass("has-second");
			$(this).removeClass("hover");
		});
		$("#ProductCategory_show01-1406531571995 dl").hover(function(){
		    $(this).addClass("hover");
		},function(){
		    $(this).removeClass("hover");
		});
					$("#ProductCategory_show01-1406531571995 div.main").hover(function(){
						$("#ProductCategory_show01-1406531571995 div.content").show();
						CE.lazyload.imgLoad($("#ProductCategory_show01-1406531571995 li.first-li h4 img"));
					},function(){
						$("#ProductCategory_show01-1406531571995 div.content").hide();
					});
	});
//]]>
</script>
