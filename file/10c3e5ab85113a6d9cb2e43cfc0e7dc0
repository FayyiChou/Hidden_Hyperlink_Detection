</div><script type="text/javascript">
	CE.process(function() {
		jQuery("#Product_search01-001_searchkey").bind({
			click : function() {
				submitForm();
				return false;
			}
		});
		jQuery("#Product_search01-001_keyword").bind({
			mouseout : function() {
				$(this).removeClass('input-up');
			},
			focus : function() {
				Product_search01.getFocus(this);
				$("#Product_search01-001_searchTip").hide();
			},
			blur : function() {
				Product_search01.getBlur(this);
			}
		});
		jQuery("#Product_search01-001_keyword").keydown(function(event) {
			if (event.keyCode == 13 ) {
				submitForm();
			}
		});
		$("#Product_search01-001_tipClose").click(function(){
			$("#Product_search01-001_searchTip").hide();
			Product_search01.getBlur("#Product_search01-001_keyword");
		});
		function submitForm() {
			var isCheck = Product_search01.validateForm('Product_search01-001');
			if (isCheck == true) {
				var keyword = $("input[id^='Product_search01-001_keyword']").val();
				if(keyword == '' && 'showError' == 'showError'){
					$("#Product_search01-001_searchTip").show();
					return;
				}else{
					var ajaxHref = "/Product.do";
					jQuery.ajax({
						async : false,
						dataType : 'json',
						url : ajaxHref,
						type : 'post',
						data : {
							method : 'keywordStat',
							keyword : keyword
						},
						timeout : 5000,
						cache : false,
						error : function() {
							//alert('Error loading XML document');
						},
						success : function(data) {
							// jQuery("#" + ProductCompare_float01.compId +
							// "_productList")
							// .html(xml);
							//alert(data.views);
						}
					});
					jQuery("#Product_search01-001_form").submit();
				}
			}
		}
		;
		Product_search01.onload("Product_search01-001_keyword");
	});
	function checkNull(){
		var keyword = $("input[id^='Product_search01-001_keyword']").val();
		if(keyword == '' && 'showError' == 'showError'){
			$("#Product_search01-001_searchTip").show();
			$("input[id^='Product_search01-001_keyword']").blur(); 
			return false;
		}
	}
</script> 
