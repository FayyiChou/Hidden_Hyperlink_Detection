<script type="text/javascript">
	if($.browser.msie && $.browser.version === '6.0'){
		$(document).ready(function(){
			$("#Product_showcase01-1425547622282 img.auto-resize").load(function(){
				var image = new Image();
				var iwidth = $(this).parent().width();
				var iheight = $(this).parent().height();
				image.src = $(this).attr("src");
				if (image.width > 0 && image.height > 0) {
					if (image.width / image.height >= iwidth / iheight) {
						if (image.width > iwidth) {
							$(this).width(iwidth);
							$(this).height((image.height * iwidth) / image.width);
						} else {
							$(this).width(image.width);
							$(this).height(image.height);
						}
					} else {
						if (image.height > iheight) {
							$(this).height(iheight);
							$(this).width((image.width * iheight) / image.height);
						} else {
							$(this).width(image.width);
							$(this).height(image.height);
						}
					}
				}
			});
		});
	}
CE.process(function() {
    var autoimg=false;
    var scrollTypess=false;//如果有滚动效果则不进行图片切换
    var imageToggle=false;
	if(!autoimg&&!scrollTypess&&imageToggle){
	    	 $("#Product_showcase01-1425547622282 img.mouseOverShowInfo").hover(function(){
		    		 var mediumImagesSrc=$(this).metadata().mediumImagesSrc;
		 			 if(mediumImagesSrc!=""){
		 			  $(this).attr("src",$(this).metadata().mediumImagesSrc);
		    		 }
	    		},function(){
	    			$(this).attr("src", $(this).metadata().fristImg);
	    	  });
	}
	/*-动态获得相关促销信息 -*/
	var isDiscounts = false;
	var isPromotions = false;
	if( '' == 'd_salesmark' || isDiscounts || isPromotions){
		$.ajax({
			url : '/Product.do?method=getProductPromInfo',
			type : 'post',
			data : {productId: 85, d_salesmark: '', discounts: isDiscounts, promotions: isPromotions },
			dataType : 'json',
			timeout : 5000,
			cache : false,
			success : function(json) {
				//显示促销图标
				if('' == 'd_salesmark' && (json.coupon || json.discount || json.gift || json.score)){
					var shtml = '';
					if(json.discount){
						shtml += '<img src="../../images/product/color-fold.gif" alt="" />';
					}
					if(json.gift){
						shtml += '<img src="../../images/product/color-present.gif" alt="" />';
					}
					if(json.coupon){
						shtml += '<img src="../../images/product/color-securities.gif" alt="" />';
					}
					if(json.score){
						shtml += '<img src="../../images/product/color-indigestion.gif" alt="" />';
					}
					$("#Product_showcase01-1425547622282 li.salesmark"+85).html(shtml);
				}
				//显示促销信息
				//显示打折优惠
			},
			error : function() {
			}
		});
		$.ajax({
			url : '/Product.do?method=getProductPromInfo',
			type : 'post',
			data : {productId: 152, d_salesmark: '', discounts: isDiscounts, promotions: isPromotions },
			dataType : 'json',
			timeout : 5000,
			cache : false,
			success : function(json) {
