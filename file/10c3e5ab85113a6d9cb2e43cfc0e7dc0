          <DIV class="columnSpace" id="elem-Column_navigation01-footnav001" name="栏目导航"> 
            



<script type="text/javascript">
	//<![CDATA[
    var Column_navigation01_03 = {

	getUrlParam : function(name) { // 获取URL参数
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
		var r = window.location.toString().match(reg);
	
		if (r != null)
		 
		 if(r[2]!=null){
		 	var r2=r[2].replace(".html","");
		 	return unescape(r2);
		 }
			
		return null;
	},
	
	extEvent3: function(compId) {
		/*加一级图片默认图 开始*/
		var zis=$("#" + compId + " div.nav-first li[id^='column']").size();
			for (var j=1; j <=zis ; j++) {
				var clumnDefaultPic=$("#" + compId + " div.nav-first li.nav-first" + j);
				var clumnDefaultPicA=$("#" + compId + " div.nav-first li.nav-first" + j).find("a");
				if($("#" + compId + " div.nav-first li.nav-first" + j).attr("clumnDefaultPictuire") != ""){
					clumnDefaultPicA.css("background-image","url(" + clumnDefaultPic.attr("clumnDefaultPictuire") + ")");
				}
			}
		/*加一级图片默认图 结束*/
			
		/*取一级当前选中栏目 开始*/	
		var extCurrentColumn = this.getUrlParam(compId+"FCId");
		if(extCurrentColumn != null){
			extCurrentColumn = "column"+extCurrentColumn;
		}
		/*取一级当前选中栏目 结束*/	
		
		/*加入一级当前效果 开始*/
		$("#"+compId+" #"+extCurrentColumn).find("a").removeAttr("style");
		$("#"+compId+" #"+extCurrentColumn).addClass("current");

		var passPictuire=$("#"+compId+" #"+extCurrentColumn);
		var passPictuireA=$("#"+compId+" #"+extCurrentColumn).find("a");
		
		if(passPictuire.attr("clumnPassPictuire") != ""){
			passPictuireA.css("background-image","url(" + passPictuire.attr("clumnPassPictuire") + ")");
		}			
					
		 //不显示二级
		 $("#" + compId + " div.nav-first li[id^='column']").hover(
				  function () {
				  	  if($(this).attr("id") != extCurrentColumn){
				  	  	$("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");	
							$(this).addClass("current");
							//加鼠标经过图片
							if($(this).attr("clumnPassPictuire")!= ""){
								$(this).find("a").css("background-image","url("+$(this).attr("clumnPassPictuire")+")");
							}
						}
								
				  },
				  function () {
				  	if($(this).attr("id") != extCurrentColumn){
						$(this).removeClass("current");
						$("#"+compId+" #"+$(this).attr("id")).find("a").removeAttr("style");
						
						//加默认图片
						if($(this).attr("clumnDefaultPictuire")!= ""){
							$(this).find("a").css("background-image","url("+$(this).attr("clumnDefaultPictuire")+")");
						}
				  	}
				  }
				);
		
	}
}

	Column_navigation01_03['Column_navigation01-footnav001_init'] = function (){
		Column_navigation01_03.extEvent3("Column_navigation01-footnav001");
	}
	$(Column_navigation01_03['Column_navigation01-footnav001_init']);
	// ]]>
</script>
