<script>
function setCookie(name, value) {
			var days = 30;
			var exp = new Date();
			exp.setTime(exp.getTime() + days * 24 * 60 * 60 * 1000);
			document.cookie = name + "=" + value + ";expire=" + exp.toGMTString()+";path=/";
	}
function getCookie(name){
	var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
     if(arr != null) return unescape(arr[2]); return null;
}
//渠道头尾显示
$(function(){
      //关于手机网厅头尾隐藏控制-----start
	  var mallurl = window.location.href;
	  if(mallurl.indexOf("?")>-1){
		  var res = mallurl.split("?");
		  var urichannel = res[1];
		  if(urichannel.indexOf("hf=nhf")>-1){
		     $("#headershow").hide();
		     $("#footershow").hide();
		     $("#zgdxlogo").hide();
		     //setCookie("hf","nhf");
		     sessionStorage.setItem("hf","nhf");
		  }else if(urichannel.indexOf("hf=nh")>-1){
		     $("#headershow").hide();
		     //setCookie("hf","nh");
		     sessionStorage.setItem("hf","nh");
		  }else if(urichannel.indexOf("hf=nf")>-1){
		     $("#footershow").hide();
		      //setCookie("hf","nf");
		      sessionStorage.setItem("hf","nf");
		  }
		  if(urichannel.indexOf("dh=hide")>-1){
		     $("#showdnfk").hide();
		  } 
	  }
	  //cookie里面再判断头尾隐藏
	  var hf = sessionStorage.getItem("hf");
	  if(hf=="nhf"){
		$("#headershow").hide();
		$("#footershow").hide();
		$("#zgdxlogo").hide();
	  }else if(hf=="nh"){
		$("#headershow").hide();
	  }else if(hf=="nf"){
	  	$("#footershow").hide();
	  }
      //关于手机网厅头尾隐藏控制-----end
      var get_minihome = sessionStorage.getItem("minihome");
      if(get_minihome=="minihome"){
           $("#footershow").hide();
           $("#minihome").show();
      }
});


//链接参数拼装组合
function hreffun(hrefstr){
    var mallurl = window.location.href;
    var hiddenchannel = $("#hiddenchannel").val();

	if(hiddenchannel!=''){
	   if(hrefstr.indexOf("channel")>-1){//已有渠道信息
	      window.location=hrefstr; 
	   }else{
	     if(hrefstr.indexOf("?")>-1){
		       window.location=hrefstr+"&channel="+hiddenchannel;
		  }else{
		       window.location=hrefstr+"?channel="+hiddenchannel;
		  }
	   }
	}else{
	   window.location=hrefstr;      
	}
}



