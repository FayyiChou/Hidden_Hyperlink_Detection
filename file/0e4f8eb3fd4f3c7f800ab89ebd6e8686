 <script>

            $(function(){

            

            	var product_type = "";//产品分类

            	

            	var activities_city = "";//地区分类

            

                var $choice_result = $("#choice_result"),$thisChoice = $("#thisChoice");



                $(".choice_showMore").on("click",function(){

                    var $this = $(this);

                    if($this.attr("data-type")=="on") {

                        $choice_result.css("height","auto");

                        $this.attr("data-type","off").find("i.ico-subtract").addClass("ico-subtract2");

                        $this.find("span").text("收缩");

                    } else {

                        setHResult();

                        $this.attr("data-type","on").find("i.ico-subtract").removeClass("ico-subtract2");

                        $this.find("span").text("展开");

                    }

                })

                $(document).click(function (event) {

                    $('.this_pa').slideUp();

                    // $thisChoice.find("a.curr").removeClass("curr");

                });

                // 选择展示不同分类

                $thisChoice.find("a").on("click",function(event){

                    event.stopPropagation();

                    var $this = $(this),thisID = $this.data("id");

                    if($this.hasClass("curr")) {

                        $thisChoice.find("a.curr").removeClass("curr");

                        $("#"+thisID).slideUp();

                    } else {

                        $thisChoice.find("a.curr").removeClass("curr");

                        $this.addClass("curr");

                        $(".this_pa").hide();

                        $("#"+thisID).slideDown();

                    }

                })

                // 产品分类

                $("#product_type li a").on("click",function(event){

                    event.stopPropagation();

                    var $this = $(this),thisProduct = $this.data("product");

                    if(thisProduct=="all") {

                    	product_type = "";

                    	getlist();

                        $thisChoice.find("a").eq(1).html("产品分类<i></i>");

                    } else {

                    	product_type = $this.html();

                    	getlist();

                        $thisChoice.find("a").eq(1).html($this.html()+"<i></i>");

                    }

                    $this.addClass("curr").closest("li").siblings().find("a").removeClass("curr");

                })

                // 地区分类

                $("#area_type li a").on("click",function(event){

                    event.stopPropagation();

                    var $this = $(this),thisArea = $this.data("area");

                    if(thisArea=="all") {

                    	activities_city = "";

                    	getlist();

                        $thisChoice.find("a").eq(0).html("地区分类<i></i>");

                    } else {

                    	activities_city = $this.html();

                    	getlist();

                        $thisChoice.find("a").eq(0).html($this.html()+"<i></i>");

                    }

                    $this.addClass("curr").closest("li").siblings().find("a").removeClass("curr");

                })

			getlist();	

			function getlist(){

                $thisChoice.find("a.curr").removeClass("curr");

                $(".this_pa").hide();

                $("#jxList").html("<div style='padding:30px 0; text-align:center; font-size:14px; color:#999;'>数据读取中...</div>");

				$.ajax({

	            url : "/market/kdzq/getKDProductList.whtml",

	            type : "POST",

		        data:{'product_type':product_type,'activities_city':activities_city},

		        dataType:'json',

		        async:false,

		        cache:false,

	            success : function(data){

		    	var goods_list = data.goodsList;

		    	if(goods_list==undefined)

		    	{

		    		//$j("#goods_list").html("");

		    		//$j("#paging_list").html("");

		    		return;

		    	}	           

		    	$("#jxList").html("");

    	        for(i=0;i<goods_list.length ;i++)

    	        {

		    		var goodsMap = goods_list[i];

		    		var remark1 = goodsMap.REMARK1;  //产品卖点

		    		var field_name_show = goodsMap.FIELD_NAME_SHOW;//图片url

		    		var field_name = goodsMap.FIELD_NAME;//产品名称

		    		var field_value = goodsMap.FIELD_VALUE+"?channel=";//产品链接地址

		    		var field_desc = goodsMap.FIELD_DESC;//产品描述

		    		var price = goodsMap.PRICE;//产品价格					

		    		var str = '<li>'+

								'<div class="chosenImg"><img src="'+field_name_show+'"></div>'+

									'<div class="chosenTxt">'+

										'<div class="chosenLf">'+

										'<p class="fOrange">￥'+price+' </p>'+

										'<p>'+remark1+'</p>'+

									'</div>'+

									'<div class="chosenRi">'+

										'<a class="btnBuy" href="'+field_value+'">立即抢购</a>'+

									'</div>'+

								'</div>'+

							'</li>';

					$("#jxList").append(str);

    		      }	

					setTimeout(function(){

                        setHResult();

                    },300);



	            }

	        });

			}

		 function setHResult(){

                    return $choice_result.height($choice_result.find("li").eq(0).outerHeight(true))

                }

            })

