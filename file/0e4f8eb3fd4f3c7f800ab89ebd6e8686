function fMain(p_adr,p_fiber_optic,p_spend,q_type,closewid,p_zgbl){

  	if(closewid=="close"){

		$('#cxcx').show();

		$('#cyc').hide();

  		$('.wrap').show();

     	$('.map').hide();

     }

  	if(q_type==2){

  		//document.getElementById("docDetail").height="150";

  		//document.getElementById("accountBox").style.height="0px";

  	}else if(q_type==1){

  		//document.getElementById("docDetail").height="550";

  		//document.getElementById("accountBox").style.height="550px";

  	}	

  	if(p_adr!=""){

  		$("#p_adr").text(p_adr);

  		$("#p_gx").text(p_zgbl);

  		$("#p_sl").text(p_spend+"M");

  	}

  }

