			type : "POST",

			success : function(data) {

				if(data == "null"){

					document.getElementById('accountBox').innerHTML='<iframe id="docDetail" align="center" height="550" id="win" name="win"  frameborder="0" scrolling="no" src="http://134.96.146.158:8080/resourceServers/map"></iframe>';

				}else{

				 	var retMsg = eval('('+data+')');

	    			var data = encodeURI(retMsg.c3);

	    			var ip = encodeURI(retMsg.ip);

	    			var code = encodeURI(retMsg.codeMd5);

	    			var dynamicCode = encodeURI(retMsg.code);

	    			var dynamicKey = encodeURI(retMsg.dynamicKey);

// 	    			var value=  '<iframe id="docDetail" width="350" align="center" height="550" id="win" name="win"  frameborder="0" scrolling="no" src="http://134.96.146.158:8080/resourceServers/map?cityName='+data+'&ip='+ip+'&sign='+code+'&dynamicCode='+dynamicCode+'&dynamicKey='+dynamicKey+'"></iframe>'

					var value=  '<iframe id="docDetail" align="center" height="500" id="win" name="win"  frameborder="0" scrolling="no" src="http://115.239.138.164:8001/resourceServers/wap/wt/map?cityName='+data+'&ip='+ip+'&sign='+code+'&dynamicCode='+dynamicCode+'&dynamicKey='+dynamicKey+'"></iframe>'

					document.getElementById('accountBox').innerHTML=value;

				}

			}

		});	

	 $('.wrap').hide();

     $('.map').show();

  }

