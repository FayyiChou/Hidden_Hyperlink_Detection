//终端设备以及浏览器版本判断
var browser={
    versions:function(){
        var u = navigator.userAgent, app = navigator.appVersion;
        return {//移动终端浏览器版本信息
            trident: u.indexOf('Trident') > -1, //IE内核
            presto: u.indexOf('Presto') > -1, //opera内核
            webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
            gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1, //火狐内核
            mobile: !!u.match(/Mobile/), //是否为移动终端
            ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
            android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, //android终端或者uc浏览器
            iPhone: u.indexOf('iPhone') > -1 || u.indexOf('Mac') > -1, //是否为iPhone或者QQHD浏览器
            iPad: u.indexOf('iPad') > -1, //是否iPad
            webApp: u.indexOf('Safari') == -1, //是否web应该程序，没有头部与底部
            weixin: u.indexOf('MicroMessenger') > -1 , //微信浏览器
            ad_pa:u.indexOf('534') > -1 && u.indexOf('Linux') > -1, //安卓自带浏览器
            uc:u.indexOf('UCBrowser') > -1  //uc浏览器
        };
    }(),
    language:(navigator.browserLanguage || navigator.language).toLowerCase()
}

//footer导航以及反馈弹窗
function setFooter(){
	//footer导航
	$('.footer .btn-nav').click(function(e){
		e.preventDefault();
		var _this = $(this);
		var sTop = $(window).scrollTop()+100+'px';
		_this.find('i').toggleClass('hide');
		_this.parent('.list-service').next('.short-nav').toggle();
		if (!_this.find('i').hasClass('hide')) {
			$('html,body').animate({'scrollTop':sTop});
		};
	});
	//反馈弹窗
	$('#J_feedBack').click(function(e){
		e.preventDefault();
		var _this = $(this);
		var wth_win = $(window).width();
		var ht_win = $(window).height();
		var ht_doc = $(document).height();
		$('body').append('<div class="feedback-mask"></div>');
		$('.feedback-mask').height(ht_doc).show();
		$('#wrap-feedback').width(wth_win).height(ht_win/2).show();

	});
	$('.feedback-close').click(function(){
		$('#wrap-feedback').hide();
		$('.feedback-mask').remove();
	});

	$('#J_clet').click(function(e){
		e.preventDefault();
		var wth_win = $(window).width();
		var ht_win = $(window).height();
		var ht_doc = $(document).height();
		var top = 0;
		$('body').append('<div class="feedback-mask"></div>');
		$('.feedback-mask').height(ht_doc).show();
		if (browser.versions.mobile) {
			if (browser.versions.ios) {
				if (browser.versions.iPad) {
					$('.icon-refer').addClass('refer-pad').show();
					$('.show-pad').show().siblings('.clet-show').hide();
				}else{
					$('.icon-refer').removeClass('refer-pad').show();
					$('.show-ph').show().siblings('.clet-show').hide();
				}
			}else{
				$('.show-ad1').show().siblings('.clet-show').hide();
			}
			top = (ht_win-$('.wrap-clet').height())/2+'px';
			$('.wrap-clet').css('top',top).show();
		}else{
			return false;
		}
	});

	$('.clet-close').click(function(e){
		e.preventDefault();
		$('.wrap-clet').hide();
		$('.feedback-mask').remove();
	});
}

</script>


