 $("#J-flow-drop").hide();
});
$("#ECS_CARTINFO").mouseenter(function() {
    $("#J-flow-drop").show();
});
$("#ECS_CARTINFO").mouseleave(function() {
 $("#J-flow-drop").hide();
});
</script>
