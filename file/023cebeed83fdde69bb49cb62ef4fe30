		if(advtable.style.display == 'block'){
		  advtable.style.display = 'none';
		}else if(advtable.style.display == 'none'){
		  advtable.style.display = 'block';
		}
	  }
</script>
<script>
  var searchSel = document.getElementById('searchSel'),
      searchSelLi = document.getElementById('searchSelLi'),
	  searchType = document.getElementById('searchtype');
  var t = 'shorttitle';
	  searchSel.onmouseover = searchSelLi.onmouseover = function(){
	    searchSelLi.style.display = 'block';
	  }
	  searchSel.onmouseout = searchSelLi.onmouseout = function(){
	    searchSelLi.style.display = 'none';
	  }
  var Type = searchSelLi.getElementsByTagName('a');
     for(var i=0;i<Type.length;i++){
	   Type[i].index = i;
	   Type[i].onclick = function(){
	     searchSel.innerHTML =this.innerHTML;
		 if(this.index == 0){
		   t = 'shorttitle';
		 }
		 if(this.index == 1){
		   t = 'writer';
		 }
		 if(this.index == 2){
		   t = 'keywords';
		 }
		 if(this.index == 3){
		   t = 'source';
		 }
         searchType.value = t;
	   }
	 }
</script>


