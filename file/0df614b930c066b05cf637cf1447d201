	<script type="text/javascript">
	function is_weixn(){
	    var ua = navigator.userAgent.toLowerCase();
	    if(ua.match(/MicroMessenger/i)=="micromessenger") {
	        return true;
	    } else {
	        return false;
	    }
	}

	if (is_weixn()) {
		var wx = document.getElementById('shareDetailWx');
		var wechat = document.querySelector('.wechat-tips');
		wx.style.display = '';
		document.getElementById('shareDetail').style.display = 'none';
		wx.addEventListener('click', function () {
			wechat.style.display = 'block';
		})
		wechat.addEventListener('click', function () {
			this.style.display = 'none';
		})
	}
