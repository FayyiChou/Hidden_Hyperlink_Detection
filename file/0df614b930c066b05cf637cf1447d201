	MyTouch.prototype.fnNum = function() {
		var listCon = this.list;
		var listLi = listCon.querySelectorAll("li");
		for (var i = 0; i < listLi.length; i++) {
			listLi[i].innerHTML = (i + 1);
		}
		var all = document.createElement('span');
		all.className = 'all-num';
		all.innerHTML = '\/' + listLi.length;
		listCon.appendChild(all);
	};

	var touch = MyTouch("#touch", {
		"list": ".num",
		"automatic": true,
		"speed": 3000,
		"loop": true
	});

	touch.fnNum();

	var touchBig = MyTouch("#touchBig", {
		"list": ".num",
		"automatic": false,
		"loop": true
	});

	touchBig.fnNum();
	
	var tBig = $('#touchBig');
	$('#touch').on('click', function () {
		var w = touch.oDiv.style.transform;
		touchBig.index = touch.index;
		touchBig.loopRight = touch.loopRight;
		touchBig.loopLeft = touch.loopLeft;
		touchBig.juli = touch.juli;
		touchBig.endjuli = touch.endjuli;
		touchBig.oDiv.style.transitionDuration = "0ms";
		touchBig.oDiv.style.transform = w;
		touchBig.oDiv.style.webkitTransform = w;
		tBig.addClass('active');
	});

	tBig.on('click', function () {
		tBig.removeClass('active');
	});

	if(document.getElementById('star')) {
		var star = document.getElementById('star');
		var stars = star.querySelectorAll('.c-star-num');
		var num = star.getAttribute('data-score');
		var numInt = parseInt(num);
		var numFixed = num - numInt;

		star.className += (' star' + numInt);

		stars[numInt].innerHTML = '<div class="c-star-num" style="width:' + (numFixed / 1) * 100 + '%"></div>';
	}

</script>
