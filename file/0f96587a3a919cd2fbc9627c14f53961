<script type="text/javascript">
    function setTab(evt, name, cursel, n) {
        var eventSource = evt.srcElement ? evt.srcElement : evt.target;
        if (eventSource.id == "one1") {
            document.getElementById("hrefmore").href = '/threestyle/estonetech/productcatalog.html';
        }
        if (eventSource.id == "one2") {
            document.getElementById("hrefmore").href = '/threestyle/estonetech/tag.html';
        }
        for (i = 1; i <= n; i++) {
            var menu = document.getElementById(name + i);
            var con = document.getElementById("con_" + name + "_" + i);
            menu.className = i == cursel ? "hover" : "";
            con.style.display = i == cursel ? "block" : "none";
        }
    }
    document.getElementById("minu1").className = "minuinfoshow";
</script>

