<script type="text/javascript">
    
	$(".search_box .choose ul li").each(function(){
		if($(this).attr("title")==$("#goIndex").val()){
			$(".search_box .choose ul .active_1").removeAttr("class");
			$(this).attr("class","active_1");
		}
		if( $(".search_box .choose ul .active_1").text()=="企业"){
  			$("#goIndex").val("company");
     		$("#indexSearch").attr("action","/company/list.html");
     	}
     	
     	if( $(".search_box .choose ul .active_1").text()=="供求"){
  			$("#goIndex").val("supply");
     		$("#indexSearch").attr("action","/supply/no.-.html");
     	}
     	
     	if( $(".search_box .choose ul .active_1").text()=="会展"){
  			$("#goIndex").val("show");
  			
     		$("#indexSearch").attr("action","/show/no..html");
     	}
	});
	
	$(".search_box .choose ul li").click(function(){
		$(".search_box .choose ul .active_1").removeAttr("class");
		$(this).attr("class","active_1");
		if( $(".search_box .choose ul .active_1").text()=="企业"){
  			$("#goIndex").val("company");
     		$("#indexSearch").attr("action","/company/list.html");
     	}
     	
     	if( $(".search_box .choose ul .active_1").text()=="供求"){
  			$("#goIndex").val("supply");
     		$("#indexSearch").attr("action","/supply/no.-.html");
     	}
     	
     	if( $(".search_box .choose ul .active_1").text()=="会展"){
  			$("#goIndex").val("show");
  			
     		$("#indexSearch").attr("action","/show/no..html");
     	}
	}); 
     function goSearch(){
          var flag = true;
          jQuery.ajax({
                async: false, 
				url :'/home/mallSearch.html',
				type : 'POST',
				data : {},
				dataType:"text",
				success : function(data) {
				    if(data == '0'){
				         layer.tips('操作过于频繁，请稍后再试', '#searchId', {
                         tips: [2, '#ff9900'],
                         time: 3000 });
                         flag = false;
				    }
				},
				error: function(){
					layer.msg("出现异常", {icon: 5}); 
					flag = false;  
	  			}
			});
			return flag;
}

     
    $(function(){
      jQuery.ajax({
				url :'/home/hotNames.html',
				type : 'POST',
				data : {},
				dataType:"json",
				success : function(data) {
				    if(data){
					var num = data.length;
						for(var i=0;i<num;i++){
						     $option=$("<li><a type='"+data[i].searchType+"' style='cursor:pointer'>"+data[i].searchName+"</a>");
							 $("#hotNamesUl").append($option);
						}
					}
				    
				},
				error: function(){
					layer.msg("出现异常", {icon: 5});   
	  			}
			});			
       
        $(".hotword ul").on("click","li a",function(){
           $("#hotSearchKeyword").val($(this).html());
            if($(this).attr("type")=='1'){
               $("#hotIndex").val("supply");
     		   $("#hotSearch").attr("action","/supply/no.-.html").submit();
            } 
            
            if($(this).attr("type")=='2'){
               $("#hotIndex").val("company");
     		   $("#hotSearch").attr("action","/company/list.html").submit();
            } 
            
            if($(this).attr("type")=='3'){
               $("#hotIndex").val("show");
     		   $("#hotSearch").attr("action","/show/no..html").submit();
            } 
       });
    });
     
</script>
