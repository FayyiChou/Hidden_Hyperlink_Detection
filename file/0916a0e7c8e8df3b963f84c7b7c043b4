<script type="text/javascript">
window.onload = window.onresize = window.onscroll = function ()
{
	var oBox = document.getElementById("divQQbox");	
	var iScrollTop = document.documentElement.scrollTop || document.body.scrollTop;
	setTimeout(function ()
	{
		clearInterval(oBox.timer);
		var iTop = 200 + iScrollTop;
		oBox.timer = setInterval(function ()
		{
			var iSpeed = (iTop - oBox.offsetTop) / 8;
			iSpeed = iSpeed > 0 ? Math.ceil(iSpeed) : Math.floor(iSpeed);
			oBox.offsetTop == iTop ? clearInterval(oBox.timer) : (oBox.style.top = 

oBox.offsetTop + iSpeed + "px");
		}, 30)
	}, 100)

         var oBoxzc = document.getElementById("divzuoce");	
	var iScrollTopzc = document.documentElement.scrollTop || document.body.scrollTop;
	setTimeout(function ()
	{
		clearInterval(oBoxzc.timer);
		var iTopzc = 550 + iScrollTopzc;
		oBoxzc.timer = setInterval(function ()
		{
			var iSpeedzc = (iTopzc - oBoxzc.offsetTop) / 24;
			iSpeedzc = iSpeedzc > 0 ? Math.ceil(iSpeedzc) : Math.floor(iSpeedzc);
			oBoxzc.offsetTop == iTopzc ? clearInterval(oBoxzc.timer) : (oBoxzc.style.top = 

oBoxzc.offsetTop + iSpeedzc + "px");
		}, 30)
	}, 20)

	var oBoxyc = document.getElementById("divyouce");	
	var iScrollTopyc = document.documentElement.scrollTop || document.body.scrollTop;
	setTimeout(function ()
	{
		clearInterval(oBoxyc.timer);
		var iTopyc = 40 + iScrollTopyc;
		oBoxyc.timer = setInterval(function ()
		{
			var iSpeedyc = (iTopyc - oBoxyc.offsetTop) / 8;
			iSpeedyc = iSpeedyc > 0 ? Math.ceil(iSpeedyc) : Math.floor(iSpeedyc);
			oBoxyc.offsetTop == iTopyc ? clearInterval(oBoxyc.timer) : (oBoxyc.style.top = 

oBoxyc.offsetTop + iSpeedyc + "px");
		}, 30)
	}, 100)

	
};
</script>




