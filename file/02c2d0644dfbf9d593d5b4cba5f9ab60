$(function(){
    function tabs(tabTit,on,tabCon){
	$(tabCon).each(function(){
	  $(this).children().eq(0).show();
	  });
	$(tabTit).each(function(){
	  $(this).children().eq(0).addClass(on);
	  });
     $(tabTit).children().hover(function(){
        $(this).addClass(on).siblings().removeClass(on);
         var index = $(tabTit).children().index(this);
         $(tabCon).children().eq(index).show().siblings().hide();
    });
     }
  tabs(".news_h_t","news_h_t2",".news_h_c");	 
  tabs(".abox1_b_t","abox1_li2",".abox1_b_c");
  tabs(".abox2_t","abox2_t_li2",".abox2_c");
  tabs(".jyjs_c_t","jyjs_c_t_li",".jyjs_c_c");
  tabs(".jslj_t","jslj_t_li",".jslj_c");
  tabs(".main_five_t","main_five_t_li",".main_five_c");
   });
   <!---->
</script>
