<script>
<!---->
function SubForm(form)
{
	if(form.keyword.value.replace(/\s+/g,"") == '')
	{
		return false;
	}
	else
	{
		keys = encodeURIComponent(encodeURIComponent(form.keyword.value));
		form.action = 'search.php?keys='+keys;
	}
}
<!---->
</script>
