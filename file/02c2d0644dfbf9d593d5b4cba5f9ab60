<!---->
  function Marquee(sdemo)
  {
  	var sdemo2 = eval(sdemo+'2');
	var sdemo = eval(sdemo);
  	if(sdemo.scrollLeft<=0)
	{
  		sdemo.scrollLeft+=sdemo2.offsetWidth
	}
  	else
	{
  		sdemo.scrollLeft--
 	 }
  }
  var ss='demo';
  var speed=30;
  clearInterval(MyMar);
  function ScrollGo(tdemo)
  {
 	 
	 
 	if(tdemo == 'demo')
	{
		//clearInterval(setInterval("Marquee('wstar')",50));
		//clearInterval(setInterval("Marquee('wpic')",50));
		//alert(speed);
		ss = 'demo';
	}
	else if(tdemo == 'wstar')
	{
		//clearInterval(setInterval("Marquee('demo')",50));
		//clearInterval(setInterval("Marquee('wpic')",50));
		//alert(speed);
		ss = 'wstar';
	}
	else
	{
		//clearInterval(setInterval("Marquee('demo')",50));
		//clearInterval(setInterval("Marquee('wstar')",50));
		ss = 'wpic';
	}
	var tdemo2 = eval(tdemo+'2');
	var tdemo1 = eval(tdemo+'1');
	
	
	tdemo2.innerHTML=tdemo1.innerHTML
	tdemo.scrollLeft=tdemo.scrollWidth
	demo.onmouseover=function() {clearInterval(MyMar)}
	demo.onmouseout=function() {MyMar=setInterval("Marquee(ss)",speed)}
	wstar.onmouseover=function() {clearInterval(MyMar)}
	wstar.onmouseout=function() {MyMar=setInterval("Marquee(ss)",speed)}
	wpic.onmouseover=function() {clearInterval(MyMar)}
	wpic.onmouseout=function() {MyMar=setInterval("Marquee(ss)",speed)}
  }
  var MyMar = setInterval("Marquee(ss)",speed);
  function s_show(tt)
  {
  	switch(tt)
	{
		case "t_1":
			ScrollGo('demo');
			break;
		case "t_2":
			ScrollGo('wstar');
			break;
		case "t_3":
			ScrollGo('wpic');
			break;
	}
  }
  window.onload = function()
  {
  	ScrollGo('demo');
  }
 
 <!---->
</script>
