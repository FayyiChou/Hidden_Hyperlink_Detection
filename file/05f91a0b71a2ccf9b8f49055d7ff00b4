<script>
  toShow();
  function toShow(){
	var did='A91105';
	if(did.indexOf('A001')!=-1) show(document.getElementById("ba_soccer"));
	else if(did.indexOf('A9')!=-1) show(document.getElementById("ba_lottery"));
	else show(document.getElementById("ba_soccer"));
  }
  function show(e){
      e.className="xzabc";
      var index;
      var pnode=e.parentNode;
      var gnode=pnode.parentNode.parentNode;
      for(var i=0;i<pnode.getElementsByTagName("li").length;i++){
          if(pnode.getElementsByTagName("li")[i].innerHTML!=e.innerHTML) pnode.getElementsByTagName("li")[i].className="";
          else index=i;
      }
      
      var ins=-1;
      for(var j=0;j<gnode.getElementsByTagName("div").length;j++){
           if(gnode.getElementsByTagName("div")[j].className && gnode.getElementsByTagName("div")[j].className=="xznew000_news_item"){
                   ++ins;
                   if(ins==index) gnode.getElementsByTagName("div")[j].style.display="";
                   else  gnode.getElementsByTagName("div")[j].style.display="none";        
           }
      }
      
  }
  
   function setHeaderGoogleAd() {
	if(document.getElementById("div_ad_head")) {
	document.getElementById("div_ad_head").innerHTML = document.getElementById("ad_google_header_div").innerHTML;
	document.getElementById("div_ad_head").className='adGoogleHead';
	document.getElementById("div_ad_head").style.display = '';
	(adsbygoogle = window.adsbygoogle || []).push({});
	}
}
setHeaderGoogleAd();
