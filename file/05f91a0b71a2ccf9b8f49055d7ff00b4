	if(document.getElementById("div_ad_head")) {
	document.getElementById("div_ad_head").innerHTML = document.getElementById("ad_google_header_div").innerHTML;
	document.getElementById("div_ad_head").className='adGoogleHead';
	document.getElementById("div_ad_head").style.display = '';
	(adsbygoogle = window.adsbygoogle || []).push({});
	}
}
