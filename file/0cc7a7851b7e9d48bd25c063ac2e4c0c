    function commentDel(uid, cid, aid) {
        var userState = $("#selblack").val();
        utils.showloading();
        $.ajax({
            url:"/pcajax/DelComment",
            type:"POST",
            dataType:"json",
            data:{ fId: vzanConfig.fid, LoginUId: vzanConfig.uid, uid: uid, cid: cid, state: $("#selstate").val(), userState: userState, aid: aid },
            headers: {
                'RequestVerificationToken': '2RTXzf2B0H4m-UfNzPnWAYUmOU9YIpwN9XjYjAD2lp4PJlCmn33bMGlwqJuayoSqJmcyjCMsXDuxWxQqFoCsL2BFWyIZCYDXSGs9xp7_k_g1:GvzL20F28CSMByZZFYeSLTm97Cra91g7NxwGJVj5ty__cwyuEOrtbPmCbLdjZIjJoIXxRf3qNjaGSvBL98IJY4upYasKL-xc5uPwhzXMVdI1'
            },
            success:function (data) {
                if (data == "1") {
                    utils.vzanPostIMDels(vzanConfig.uid, aid, cid, 'Topic', uid);
                    layer.closeAll();
                    layer.msg("修改成功", { icon: 1 });
                    window.location.reload();
                } else {
                    layer.msg("系统繁忙，请稍后再试", { icon: 2 });
                }
            }
        });

    }
