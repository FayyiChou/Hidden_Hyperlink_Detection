    <script>
    //判断登录
        $(function () {
            $.ajax({
                type: "get",
                url: "/pcajax/lginfo",
                data:{fid:'910425'},
                success: function (data) {
                    receive(data);
                },
                error: function () {
                    alert('system error!');
                }
            });
    });
    function receive(data) {
        if (data.Id > 0) {
            $('#loginerimg').attr("src", data.HeadImgUrl);
            var dom = $('#islogin').html().replace("{FId}", data.FId)
             .replace("{NickName}", data.NickName);
            $('#islogin').remove();
            $('#linfo').html(dom);
            $('#hiduserId').val(data.Id);
            $('#hidvustate').val(data.State);


        }
        else {
            $('#notlogin').children().appendTo($('#linfo'));
            $('#QrCode').val(data.QrCode);

        }
        vzanConfig.uid = $('#hiduserId').val();
        vzanConfig.fid = $('#minisnsId').val();
        vzanConfig.qrcode = $('#QrCode').val();
        vzanConfig.ustate = $("#hidvustate").val();

    }
    function logout() {
        utils.wxlogout($('#QrCode').val())
    }
    </script>
        

