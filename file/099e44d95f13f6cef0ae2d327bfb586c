  <script type="text/javascript"> 


    $(function(){



        

        //分享
        $('.D_share').on('click',function(){
            if(! $(this).hasClass('vns')){
                $('.D_fxbox').show();
            }
            $('.Dqxbo').on('click',function(){
                $('.D_fxbox').hide();   
            })
            $('section').scrollTop(sctop) 
        })

        


        //回复
        $('.D_publish').on('click','.Dnririg',function(){

            if($(this).hasClass('vnq')){
                $(this).removeClass('vnq').parents('.D_publish').find('.D_newplb').hide()
            }else{
                $(this).addClass('vnq').parents('.D_publish').find('.D_newplb').show();
            }
            $('section').scrollTop(sctop) 
        })

        //同步到
        $('.Dtlef').on('click','span',function(){
            if($(this).hasClass('vpn')){
                $(this).removeClass('vpn');
                $("#sub_weibo").val('0');
            }else{
                $(this).addClass('vpn');
                $("#sub_weibo").val('1');
            }
            $('section').scrollTop(sctop) 
        })

        

    })


    </script> 
