                   <script type="text/javascript">
/**
 * 在这里定义bds_config
 */
var title = 'http://biz.click.ganji.com/#赶集网http://biz.click.ganji.com/#' + document.title ; //'http://biz.click.ganji.com/#赶集网http://biz.click.ganji.com/# + 取得的当前页面的title'
var bds_config = {
       'bdText':title
}


document.getElementById("bdshell_js").src = "http://bdimg.share.baidu.com/static/js/shell_v2.js?cdnversion=" + new Date().getHours();
// 百度分享延迟200ms显示
// IE下无法实现延迟
var elem = document.getElementById('bdshare');

GJ.waiter(function() {
    return (elem.onmouseover && elem.onmouseout) || (elem.onmouseenter && elem.onmouseleave);
},function() {
    var fnOver = elem.onmouseover;
    var fnOut = elem.onmouseout;
    var startTime, endTime, timer;
    // IE不进行处理
    if ( !fnOver || !fnOut) {
        return;
    }
    elem.onmouseover = function(e) {
        startTime = Date.now();
        timer = setTimeout(function(){
            fnOver.call(elem, e);
            clearTimeout(timer);
        }, 200);
    };
    elem.onmouseout = function(e) {
        endTime = Date.now();
        if (endTime - startTime < 200) {
            clearTimeout(timer);
        } else {
            fnOut.call(elem, e);
        }
    };
});
</script> 
