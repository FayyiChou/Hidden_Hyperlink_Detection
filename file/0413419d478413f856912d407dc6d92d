    <script>
			$("#area").change(function(){
				$.get("/office-gettown.html",{aid:$('#area').val()},
					function(data){
				$('#town option').remove();
				 $('#town').append(data);
					 }
					);
			})
			$("#tj").click(function(){
				var name=$("#name").val();
				var tel=$("#tel").val();
				var area=$("#area").find("option:selected").text();
				var town=$("#town").find("option:selected").text();
				if (name=='' || name=='请输入您的称呼')
				{
					$("#name").focus();
					alert("请输入您的称呼");
					return false;

				}
				if (tel=='' || tel=='请输入您的手机号码')
				{
					$("#tel").focus();
					alert("请输入您的手机号码");
					return false;

				}
				if(!$("#tel").val().match(/^1[3|4|5|8][0-9]\d{4,8}$/)){
				alert('号码格式不正确');
				$('#tel').focus();
				return false;
				 }
				if (area=='请选择区')
				{
					$("#area").focus();
					alert("请选择区镇");
					return false;

				}
				if (town=='请选择')
				{
				town=''
				}
				var demand='需要'+area+'-'+town+'的商铺';
				$.get("/store-add.html",{name:name,tel:tel,demand:demand},
					function(data){
					alert('信息登记成功');
				 $("input").val("");

					 }
					);


			})
			
