$("#cx").click(function(){
	var cspy=$("#cspy").val();
	if(cspy ==''){
		alert("请输入城市简称");
		
	}
	else{
		$.get("/office-getcid.html",{cpy:cspy},
					function(data){
			if(data !='0')
			{
				var url="/sp_list-0-"+data+".html";
				location.href=url;

			}
			else{
				alert("请输入正确的城市简称");
			}
		
		})
	}

})
