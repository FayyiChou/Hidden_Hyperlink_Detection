	})
	
	
	function recommendArt(typeId){
    	$.ajax({
            type: "get",
            url: "/h5/recommendArt/"+typeId+"/2460706",
            dataType : 'json',
            success: function(data){
            	for (var i = 0; i < data.length; i++) {
            		var str = "";
               		if(data[i].litpictype){
               			switch (data[i].litpictype) {
						case 1:
							str += "<div class=\"list\"><a href=\"/h5/detail/"+data[i].id+"/"+typeId+"\" class=\"link\">";
							str += "<div class=\"news_pic\"><img src=\""+data[i].litpic+"\" width=\"100%\"></div>"
							str += "<div class=\"news_text\">"
							str += "<p class=\"news_title\">"+data[i].title+"</p>"
							str += "<p class=\"hui\"><span>"+data[i].source+"</span></p>"
							str += "</div><div class=\"clear\"></div></a></div>"
							break;
						case 2:
							str += "<div class=\"list2\"><a href=\"/h5/detail/"+data[i].id+"/"+typeId+"\" class=\"link\"><div class=\"news_text\">"
							str += "<p class=\"news_title\">"+data[i].title+"</p>";
							str += "<p><img src=\""+data[i].litpic+"\" width=\"100%\"></p>";
							str += "<p class=\"hui\" style=\"margin-top:11px;\"><span>"+data[i].source+"</span></p>"
							str += "</div></a></div>";
							break;
						case 3:
							str += "<div class=\"list2\"><a href=\"/h5/detail/"+data[i].id+"/"+typeId+"\" class=\"link\"><div class=\"news_text\">"
							str += "<p class=\"news_title\">"+data[i].title+"</p><ul>";
							var imgs = data[i].litpic.split(',');
							for(var j = 0;j<imgs.length;j++){
								if(j != 2){
									str += "<li><img src=\""+imgs[j]+"\" width=\"100%\"></li>"
								}else{
									str += "<li style=\"margin:0;\"><img src=\""+imgs[j]+"\" width=\"100%\"></li>"
									str += "<div class=\"clear\"></div></ul>";
								}
							}
							str += "<p class=\"hui\"><span>"+data[i].source+"</span></p>";
							str += "</div></a><div class=\"clear\"></div></div>"
							break;
						case 4:
							str += "<div class=\"list\"><a href=\"/h5/detail/"+data[i].id+"/"+typeId+"\" class=\"link\"><div class=\"news_text\">";
	            			str += "<p class=\"news_title\">"+data[i].title+"</p>";
	            			str += "<p class=\"hui\"><span>"+data[i].source+"</span></p>"
	            			str += "</div><div class=\"news_pic\"></div></a><div class=\"clear\"></div></div>"
            				break;
						}
               		}
            		$('.news_list').append(str);
				}
            	//append ad
        		callBackAd(typeId);
            }
    	});
    }
	
	function recommendArc(){
    	$.ajax({
            type: "get",
            url: "/h5/recommendArc",
            dataType : 'json',
            success: function(data){
            	var str = "";
            	var flagId;
            	for (var i = 0; i < data.length; i++) {
            		str += "<a href='"+data[i].link+"'><li>"+data[i].showname+"</li></a>";
				}
            	$('.pindao_cont ul').prepend(str);
            }
    	});
    }
	
	function URLencode(sStr){
        return sStr.replace(/\+/g, '%2B');  
    }
	
	
	function clickImg(id,url,group){
    	if(url.indexOf('http:') != 0){
    		url = 'http://' + url;
    	}
    	callBackStats('click',group,id,function () {
            window.location.href = URLencode(url);
        });
    }
	
	var callBackStats = function (event,group, id,callback) {
        //event : show or click
        $.get("/stats/ad/"+event+"/"+group+"/"+id+"?"+new Date().getTime(),null, function (data) {
            if(callback != null) callback();
        })
    }
	
	
	
	var imageRequest = 'http://rs3.uu08.net/iad/waph5/';
    var callBackAd = function(typeId){
        var param = new Object();
        $.ajax({
             type: "get",
             url: "/h5/getAd/"+typeId,
             dataType : 'json',
             data: param,
             success: function(data){
            	 var items = $('.news_list > div');
            	 $.each(data,function(i,v){
            		 var uuid = '';
            		 var s = [];
                   	 var hexDigits = "0123456789abcdef";
                   	 for (var j = 0; j < 32; j++) {
                       s[j] = hexDigits.substr(Math.floor(Math.random() * 0x10), 1);
                     }
              		 s[14] = "4";
                     s[19] = hexDigits.substr((s[19] & 0x3) | 0x8, 1);
              		 uuid = s.join("");
         			 uuid+=v.id;
            		 var str = "";
            		 var adtype = v.adtype;
            		 var showing = false;
            		 var w = $(window).width();
                     if(v.attr != 5) w = w*0.95;
       		  		 if(adtype == 0){
       		  			if(v.attr == 5){
       		  				str += "<div style='display: none; name='advert' ";
       		  			}else{
       		  				str += "<div style='display: none;padding:0px 0px 0px 2px' name='advert' ";
       		  			}
                        str +="id='"+v.id+"'>";
	                    str += "<a href='javascript:clickImg(" +v.id+",\"" + v.adurl +"\","+v.attr + ");' ";
	                    str +="><img src='";
	                    str += imageRequest + v.imgpath;
	                    str += "' style='width:";
	                    str += w;
	                    str += "px;' /></a></div>";
            		 }else if(adtype == 2){
            			 str = showGetBaiDuAD(v.id);//百度广告定位
                         str.attr("data-position",v.attr);
                         str.attr("name","advert");
                         str.attr("id",uuid);
                         str.attr("data-id",v.id);
//                          str.css('overflow','hidden');
                         if(v.attr != 5) str.get(0).style.webkitTransform="scale(0.94,0.94)";
                     }else{
                         var w = $(window).width()/v.width;
                         w = (w > 0)?w:1;
                         str +='<div id="'+v.id+'" ';
                         str +='style="display: none;';
                         if(v.attr == 5){
                        	 str += 'height:'+(v.height * w)+'px;width:'+$(window).width()+'px;padding: 0px 0px;overflow: hidden;">'
                         }
                         if(v.attr == 6){
                             str += 'height:'+(v.height * w)+'px;width:'+$(window).width()*(1-0.06)+'px;padding: 5px 3px;overflow: hidden;">'
                         }
                         if(v.attr != 6 && v.attr != 5 ){
                             str += 'height:'+(v.height * w)+'px;width:'+$(window).width()*(1-0.06)+'px;padding: 5px 3px;overflow: hidden;">'
                         }
                         str +='<iframe src="/h5/ad/json/' + v.id+'"';
                         str +='id="adIframe'+v.id+'"';
                         str +='frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes"';
                         str += 'style="';
                         str += 'width:' + v.width + 'px;';
                         str += 'height:'+v.height+'px;';
                         if(v.attr == 5){
                             str += '-webkit-transform: scale('+w+','+w+');-webkit-transform-origin: 0% 0%;"'
                         }
                         if(v.attr == 6){
                             str += '-webkit-transform: scale('+(w*0.95)+','+(w)+');-webkit-transform-origin: 0% 0%;"'
                         }
                         if(v.attr != 6 && v.attr != 5){
                             str += '-webkit-transform: scale('+(w*0.95)+','+(w)+');-webkit-transform-origin: 0% 0%;"'
                         }
                         str +='></iframe></div>';
            		 }
       		  		 switch (v.attr) {
						case '5':
							$('#ad1').append(str);
							showing = true;
							break;
						case '6':
							$('#ad2').append(str);
							$('#ad2').show();
							showing = true;
							break;
						case '7':
							$('#ad3').append(str);
							$('#ad3').show();
							showing = true;
							break;
						case '8':
							if(items.length >= 4) {
								items.eq(-9).after(str);
	                       	 	showing = true;//展示成功
							}
							break;
						case '9':
							if(items.length >= 8) {
								items.eq(-4).after(str);
	                       	 	showing = true;//展示成功
							}
							break;
						case 'A':
							$('#ad4').append(str);
							$('#ad4').show();
							showing = true;
							break;
						case 'B':
							$('#ad5').append(str);
							$('#ad5').show();
							showing = true;
							break;	
						default:
							showing = false;
							break;
					 }
       		  		 if(v.attr != 5 && adtype == 2) str.parent().css('padding','6px 0px');
            		 if(!showing) return; //没有展示不统计
                     if(adtype == 0){
                         setTimeout(function(){$('#'+v.id).slideDown('fast');},500);
                         callBackStats('show',v.attr,v.id);//100%正常显示发送统计
                         return;
                     }
                     //延迟显示广告-保证广告100%渲染成功后,再显示...
                     $("#adIframe"+v.id).on("load",function(){
                         $('#'+v.id).slideDown('fast');
                         callBackStats('show',v.attr,v.id);//100%正常显示发送统计
                     });
            	 });
             }
     	});
    }
	</script>
