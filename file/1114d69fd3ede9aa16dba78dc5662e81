<script type="text/javascript">
GetMainData('stockdat.php?action=mystock&guestmode=1');
setInterval("reloader()", 15010);
function reloader(){
	var url = "stockdat.php?action=mystock&guestmode=1&ref=1&div=",
		div = new Array(), i = 0,
		myDate = new Date(),
		myHour = myDate.getHours(),
		myMinute = myDate.getMinutes(),
		thistime = myHour * 100 + myMinute;
	if (thistime >= 900 && thistime < 2230) {
				if (chkdivshow("stockindex") || chkdivshow("stockmonitor")) { div[i] = "stockmonitor"; i++; }
		if (chkdivshow("rank_one")) { div[i] = "rank_one"; i++; }
		if (chkdivshow("rank_two")) { div[i] = "rank_two"; i++; }
	}
	if (chkdivshow("newpostbox")) { div[i] = "newposts"; i++; }
	if(div.length > 0){ GetMainData(url + div.join(",")); }
}
</script>

