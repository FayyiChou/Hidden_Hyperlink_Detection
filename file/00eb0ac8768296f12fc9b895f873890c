scrolldemo482324.scrollLeft=scrolldemo482324.scrollWidth
function Marquee482324(){
if(scrolldemo482324_2.offsetWidth-scrolldemo482324.scrollLeft<=0)
scrolldemo482324.scrollLeft-=scrolldemo482324_2.offsetWidth
else{scrolldemo482324.scrollLeft++}
}
var MyMar482324=setInterval(Marquee482324,speed482324)
