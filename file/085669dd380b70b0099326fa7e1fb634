function change () {
	
	$('.img li').css('display','none');
	$('.img li').eq(ii).css('display','block');
	var bg = $('.img a').eq(ii).attr('data-color');
	
	if(bg != ''){
	
	$('#main_visual_bg').css('backgroundColor','#'+bg);
	}
	else{
	$('#main_visual_bg').css('backgroundColor','#fff');
	}
	$('.main-promotion a').removeClass('on');
	$('.main-promotion a').eq(ii).addClass('on');
	ii++;		
	var nums = $('.img li').length;
	if(ii>=nums){
		ii=0;
	}
	$('.btn a').attr('data-i',ii);
	int=setTimeout(change,3000);	
}
