function clickmove (classname) {
var i = $('.' +classname).attr('data-count');
if(classname=='next'){
$('.ks-switchable-nav li').removeClass('ks-active');
var lengths = $('.ks-switchable-nav li').length;
if(i<parseInt(lengths)-1){
i++;
$('.ks-switchable-nav li').eq(i).addClass('ks-active');
}else{
$('.ks-switchable-nav li').eq(0).addClass('ks-active');
i=0;
}
}else {
$('.ks-switchable-nav li').removeClass('ks-active');
var lengths = $('.ks-switchable-nav li').length;
if(i>0){
i--;
$('.ks-switchable-nav li').eq(i).addClass('ks-active');
}else{
i=parseInt(lengths)-1;
$('.ks-switchable-nav li').eq(i).addClass('ks-active');
}
}
$('.next').attr('data-count',i);
$('.prev').attr('data-count',i);
var newleft = parseFloat(i)*916;
$('.ks-switchable-content').css('left','-' + newleft +'px');
$('.' +classname).hover(function(){
clearInterval(idInt);
},function(){
	idInt = setInterval("clickmove('next')",4000);
});
}
</script>
