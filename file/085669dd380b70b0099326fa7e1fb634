	var status = $('.btn a').attr('class');
	
	if(status=='pause'){
			
		clearTimeout(int);
	$('.btn a').attr('class','play');
	$('.btn a').html('play');
	}else{
		
		//var y = $('.btn a').attr('data-i');
		$('.btn a').attr('class','pause');
		$('.btn a').html('pause');
		int=setTimeout(change,3000);
	}
})
	
