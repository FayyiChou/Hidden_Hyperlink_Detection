<script language="JavaScript">
function num(_nPageCount, _nCurrIndex, _sPageName, _sPageExt){
		var nCurrIndex = _nCurrIndex || 0;
		if(nCurrIndex == 0)
			document.write("<span style='color:#0000FF;font-weight:blod;' >1</span>&nbsp;");
		else
			document.write("<a target='_self' style='font-size:12px;color:#007355' href=\""+_sPageName+"."+_sPageExt+"\">1</a>&nbsp;");
		
		for(var i=1; i<_nPageCount; i++){
		if(nCurrIndex == i)
			document.write("<span style='color:#0000FF;font-weight:blod;'>"+(i+1)+"</span>&nbsp;");
		else
			document.write("<a target='_self' style='font-size:12px;color:#007355;' href=\""+_sPageName+"_" + i + "."+_sPageExt+"\">"+(i+1)+"</a>&nbsp;");
			}
		return "";
		}
		
function createPageHTML(_nPageCount, _nCurrIndex, _sPageName, _sPageExt){
	if(_nPageCount == null || _nPageCount<=1){
		return;
	}
	
	var nCurrIndex = _nCurrIndex || 0;
	
	if (_nPageCount==2){
		if(nCurrIndex == 0){
		document.write("<span style='color:#007355'>1</span>&nbsp;<a target='_self' style='font-size:12px;color:#007355' href=\""+_sPageName+"_" + (nCurrIndex+1) + "."+_sPageExt+"\">2</a>&nbsp;<a target='_self' style='font-size:10.5pt;color:#007355;text-decoration:none;' href=\""+_sPageName+"_" + (nCurrIndex+1) + "."+_sPageExt+"\">【下一页】</a>&nbsp;");
		}
		else if (nCurrIndex == 1){
		document.write("<a target='_self' style='font-size:10.5pt;color:#007355;text-decoration:none;' href=\""+_sPageName+ "."+_sPageExt+"\">【上一页】</a>&nbsp;<a target='_self' style='font-size:12px;color:#007355' href=\""+_sPageName+ "."+_sPageExt+"\">1</a>&nbsp;<span style='color:#007355'>2</span>&nbsp;");
		}
	}
	else{

	if(nCurrIndex == 0){
		document.write(num(_nPageCount, _nCurrIndex, _sPageName, _sPageExt));
		document.write("<a target='_self' style='font-size:10.5pt;color:#007355;text-decoration:none;' href=\""+_sPageName+"_" + (nCurrIndex+1) + "."+_sPageExt+"\">【下一页】</a>&nbsp;");
		}
	else if (nCurrIndex == 1){
		document.write("<a target='_self' style='font-size:10.5pt;color:#007355;text-decoration:none;' href=\""+_sPageName+"."+_sPageExt+"\">【上一页】</a>&nbsp;");
		document.write(num(_nPageCount, _nCurrIndex, _sPageName, _sPageExt));
		document.write("<a target='_self' style='font-size:10.5pt;color:#007355;text-decoration:none;' href=\""+_sPageName+"_" + (nCurrIndex+1) + "."+_sPageExt+"\">【下一页】</a>&nbsp;");
		}
	else if(nCurrIndex == (_nPageCount-1)){
		document.write("<a target='_self' style='font-size:10.5pt;color:#007355;text-decoration:none;' href=\""+_sPageName+"_" + (nCurrIndex-1) + "."+_sPageExt+"\">【上一页】</a>&nbsp;");
		document.write(num(_nPageCount, _nCurrIndex, _sPageName, _sPageExt));
		}
	else{
		document.write("<a target='_self' style='font-size:10.5pt;color:#007355;text-decoration:none;' href=\""+_sPageName+"_" + (nCurrIndex-1) + "."+_sPageExt+"\">【上一页】</a>&nbsp;");
		document.write(num(_nPageCount, _nCurrIndex, _sPageName, _sPageExt));
