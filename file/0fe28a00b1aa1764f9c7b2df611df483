$(function(){
    $(".layerC31FDAA2AA9E9D30A612629554FB8C99tab_box div.skin3").each(function(){
        //var showskin= $(this).not('.hide').attr('class'); 
        //var showskin= $(this).hasClass('hide'),showskinc;
