			$('#nav_layer3B9BB82B5D31C5A1F9DE937F9DD534D1 .wp_subtop').removeClass("lihover").children('a').removeClass("ahover");
		}
	},function(){
		if($(this).parent().hasClass('navigation'))
		{
			wp_showdefaultHoverCss('layer3B9BB82B5D31C5A1F9DE937F9DD534D1');
		}
