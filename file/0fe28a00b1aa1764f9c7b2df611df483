function layer3B9BB82B5D31C5A1F9DE937F9DD534D1_liHoverCallBack()
{
	$(".menu_hs9 #nav_layer3B9BB82B5D31C5A1F9DE937F9DD534D1 li").hover(function(){
				$(this).children("ul").css("left", "0px").show();
	},function(){
		$(this).children("ul").hide();

		
	});

}
	
function layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss(css_pro,type)
{
	var typeval=type;
	if(typeval==1){
		var regex = "#nav_layer[0-9|a-z|A-Z]+\\s+ul+\\s+li+\\s+a:\\s*hover\\s*{\\s*"+css_pro+"\\s*:[^;]+";
	}else if(typeval == 2){
		var regex = "#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s*{\\s*"+css_pro+"\\s*:[^;]+";
	}else if(typeval == 3){
		var regex = "#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s*:\\s*hover\\s*{\\s*"+css_pro+"\\s*:[^;]+";
	}
	else{
		var regex = "#nav_layer[0-9|a-z|A-Z]+\\s+li\.wp_subtop>a:\\s*hover\\s*{\\s*"+css_pro+"\\s*:[^;]+";
	}
		var navStyle = $.trim($("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").data("datastys_"));
		if(navStyle.length > 0)
	{
		var patt1 =new RegExp(regex,'i');
		var tmp = patt1.exec($.trim(navStyle));
		if(tmp)
		{
			return $.trim((tmp[0].match(/{[^:]+:[^;]+/)[0]).match(/:[^;]+/)[0].replace(':',''));
		}
	}
		navStyle = $.trim($("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").data("datasty_"));
	 
	if(navStyle.length > 0)
	{
		if(typeval==1){
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul+\\s+li+\\s+a:\\s*hover\\s*{[^}]+}",'i');
		}else if(typeval == 2){
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s*{[^}]+}",'i');
		}else if(typeval == 3){
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s*:\\s*hover\\s*{[^}]+}",'i');
		}else{
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+li\.wp_subtop>a:\\s*hover\\s*{[^}]+}",'i');
		}
		var tmp = patt1.exec(navStyle);
		
		if(tmp)
		{
			var tmp1 = tmp[0].match(/{[^}]+}/)[0];
			var patt2 = new RegExp(css_pro+"\\s*:\\s*[^;]+;",'i');
			tmp = patt2.exec(tmp1);
			if(tmp) return $.trim(tmp[0].replace(/[^:]+:/,'').replace(';',''));
		}
	}

	return $.trim($("#nav_layer3B9BB82B5D31C5A1F9DE937F9DD534D1 ul li a").css(css_pro));
}

layer3B9BB82B5D31C5A1F9DE937F9DD534D1_liHoverCallBack;
$('#layer3B9BB82B5D31C5A1F9DE937F9DD534D1').layer_ready(function(){
	layer3B9BB82B5D31C5A1F9DE937F9DD534D1_liHoverCallBack();

	//第三级即下级菜单随高度增加位置动态修改
	$('#layer3B9BB82B5D31C5A1F9DE937F9DD534D1 .menu_hs9 li').hover(function(){
		$(this).children('ul').css('top',$(this).outerHeight());
	});

	function getSubMenuHoverCsslayer3B9BB82B5D31C5A1F9DE937F9DD534D1(css_pro,regex,isLi)
	{
				var navStyle = $.trim($("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").data("datastys_"));
				if(navStyle.length > 0)
		{
			var patt1 =new RegExp(regex,'i');
			var tmp = patt1.exec($.trim(navStyle));
			if(tmp)
			{
				return $.trim((tmp[0].match(/{[^:]+:[^;]+/)[0]).match(/:[^;]+/)[0].replace(':',''));
			}
		}
						navStyle = $.trim($("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").data("datasty_"));
			 
		
		if(navStyle.length > 0)
		{
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s+a\\s*\\s*{[^}]+}",'i');
			if(isLi)
				patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s*\\s*{[^}]+}",'i');
			var tmp = patt1.exec(navStyle);
			
			if(tmp)
			{
				var tmp1 = tmp[0].match(/{[^}]+}/)[0];
				var patt2 = new RegExp(css_pro+"\\s*:\\s*[^;]+;",'i');
				tmp = patt2.exec(tmp1);
				if(tmp) return $.trim(tmp[0].replace(/[^:]+:/,'').replace(';',''));
			}
		}
		return false;
	}
	
	$("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1 li").hover(function(){ 			
			var thisleft=$(this).offset().left;
			var thiswidth=$(this).outerWidth();	
			//父中心点坐标
			var thiswidthcenter=0;
			//子中心点坐标
			var thischildwidthcenter=0;			
			var orgleft = $("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").offset().left;
			var orgright = $("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").outerWidth();
			orgright=orgright+orgleft;
			thiswidthcenter=thisleft+thiswidth/2;

			//计算子菜单总宽度
			var childlidom=$(this).children('ul').children('li');
			var childliwidth=0;
			childlidom.each(function(){
				childliwidth=childliwidth+$(this).outerWidth();			
			});
			
			thischildwidthcenter=childliwidth/2;			
			var chavalue=thiswidthcenter-thischildwidthcenter;
			var charightvalue=thiswidthcenter+thischildwidthcenter;			
			
			var ulwidth=$("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").width();
			if(chavalue<orgleft && charightvalue>orgright){ 
				//超出边界不做处理			
			}else{
				if(chavalue>orgleft && charightvalue<orgright){
					$(this).children("ul:eq(0)").css("padding-left", (chavalue-orgleft)+"px");				
					if(ulwidth>0){
						$(this).children("ul:eq(0)").css("width", (ulwidth-chavalue+orgleft)+"px");
					}	
				}else{
					if(chavalue>orgleft && charightvalue>orgright){
						$(this).children("ul:eq(0)").css("padding-left", (ulwidth-childliwidth)+"px");
						if(ulwidth>0){
							if(childliwidth>ulwidth) childliwidth=ulwidth;
							$(this).children("ul:eq(0)").css("width", (childliwidth)+"px");
						}
					}
				}		
			}
			
			if(!$(this).hasClass('wp_subtop'))
			{
				$(this).css('background-image',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-image',3));
				$(this).css('background-repeat',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-repeat',3));
				$(this).css('background-color',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-color',3));
				$(this).css('background-position',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-position',3));
			}
			if($(this).children('ul').length > 0)
			{
				$(this).children('ul').css('background-image',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-image',2));
				$(this).children('ul').css('background-repeat',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-repeat',2));
				$(this).children('ul').css('background-color',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-color',2));
				$(this).children('ul').css('background-position',layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss('background-position',2));

				$(this).children('ul').children('li').css({'background-image':'none','background-color':'transparent'});
			}
			var type=$("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").find('.wp-new_navigation_content').attr('type');
			if(type==2){
				var self = $(this);
				var pos = 0 ;
				var loops = 0;
				$('#nav_layer3B9BB82B5D31C5A1F9DE937F9DD534D1').find('li').each(function(){
					if(loops == 1) return true;
					if(self.html() == $(this).html()){
						loops = 1;
						return true;
					}else{
						pos = pos + $(this).outerWidth();
					}	
					 
				})
				 
				$("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").find('.ddli').hide();
				var this_width = $('#nav_layer3B9BB82B5D31C5A1F9DE937F9DD534D1').outerWidth();
				var thisul_left = $('#nav_layer3B9BB82B5D31C5A1F9DE937F9DD534D1').css("padding-left");
				thisul_left = parseInt(thisul_left);
				$(this).children('.ddli').outerWidth(this_width).css("margin-left","-"+(thisul_left+pos)+"px");
				$(this).children('.ddli').eq(0).slideDown();
			}			
 
	},function(){ 
			if(!$(this).hasClass('wp_subtop'))
			$(this).css({'background-color':'transparent','background-image':'none'});
			$(this).children("ul:eq(0)").css("left", "-99999px").hide(); 
							var type=$("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").find('.wp-new_navigation_content').attr('type');
			if(type==2){
				$("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").find('.ddli').slideUp();
			}
	});

	$('.menu_hs9 #nav_layer3B9BB82B5D31C5A1F9DE937F9DD534D1').find('li').hover(function(){
		var direction=$("#layer3B9BB82B5D31C5A1F9DE937F9DD534D1").find('.nav1').attr('direction');
		var height = parseInt($(this).outerHeight());
		if(direction==1){				
			$(this).children('ul').css('top','auto').css('bottom',height + 'px');
		}else{				
			$(this).children('ul').css('top',height+'px').css('bottom','auto');	
		}
		if($(this).parent().hasClass('navigation'))
		{			
			$(this).children('a').css({'font-family':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-family",0),'font-size':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-size",0),'color':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("color",0),'font-weight':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-weight",0),'font-style':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-style",0)});
		}else{			
			$(this).children('a').css({'font-family':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-family",1),'font-size':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-size",1),'color':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("color",1),'font-weight':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-weight",1),'font-style':layer3B9BB82B5D31C5A1F9DE937F9DD534D1_getSubMenuHoverCss("font-style",1)});
