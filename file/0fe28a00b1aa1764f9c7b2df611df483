if (showskinc === "skin3") {
	  	var maxliheight = 0,tmplayerid = "#layerC31FDAA2AA9E9D30A612629554FB8C99";
		if (tmplayerid.length == 1) return;var $tmpnode = $(tmplayerid+' li > .wp-new-tabs-style-c');
		maxliheight = Math.max.apply(null,$tmpnode.map(function(){return $(this).outerHeight();}).toArray());
		if (maxliheight) $tmpnode.height(maxliheight); //设置高是多文字描述末显示完隐藏掉 2014.7.4先处理为隐蔵
		// 右间距 2014/03/17
		$(tmplayerid).bind("fixedmarginright", function(e, margin){
                        $(this).find('.artprotext').each(function(){
                             $(this).attr('abc','test');
                            var target = "#layerC31FDAA2AA9E9D30A612629554FB8C99",$li =  $(this).find('li');
                            if(margin != undefined) $li.css("margin-right", margin+'px');
                            var $first = $li.filter(':first'),liwidth = $first.width(),
                            mgnright = $._parseFloat($first.css("marginRight")),
                                                        maxwidth = $(target).children('.wp-tabs_tag_content').width();
                    		                            var maxcols = Math.floor((maxwidth) / (liwidth + mgnright));
                            if(maxwidth >= maxcols * (liwidth + mgnright) + liwidth) maxcols += 1;
                            for(var i = 1,licnt = $li.length; i <= licnt; i++){
                                 $li.attr("attr",maxwidth);
                                    if (i % maxcols != 0) continue;
                                    if ((maxcols == 1) && (2*liwidth <= maxwidth)) continue;
                                    $li.filter(':eq('+(i - 1)+')').css("margin-right", '0');
                            }
                            $li = i = null;
                        });
		}).triggerHandler("fixedmarginright");
		// <<End
		tmplayerid = $tmpnode = null;
}
});
});

