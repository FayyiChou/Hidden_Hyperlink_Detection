<script>
	page.showPage(1,16,"turnPage");
function turnPage(pageNum){
	var keyword = $("#keyword").val();
	window.location.href="/portal/courseNetwork/index?page="+pageNum+"&keyword="+keyword;
}

function searchCourse() {
	var keyword = $("#keyword").val();
	window.location.href="/portal/courseNetwork/index?keyword="+keyword;
}

