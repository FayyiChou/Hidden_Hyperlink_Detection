    <script type="text/javascript">
        function setAdShow(id) {
            var obj = document.getElementById(id);
            if (obj.getElementsByTagName('ins')[0].innerHTML.replace(/^(\s|\u00A0)+|(\s|\u00A0)+$/g, "") != '') {
                obj.style.display = 'block';
            }
        }

        setAdShow("ad_zT");
        setAdShow("Ad_leftbtn");
        setAdShow("Ad_rightbtn");
        setAdShow("Ad_dealer");
        //--------------解决锚点定位问题-----------------
        var anchorBase = {
            $: function(id) { return document.getElementById(id) },
            setTop: function(y) { //兼容DTD头
                document.documentElement.scrollTop = y;
                document.body.scrollTop = y;
            },
            offsetLT: function(o) { //元素相对于整个窗口的位置 o对象 返回 x y
                var x = 0, y = 0;
                do {
                    x += o.offsetLeft, y += o.offsetTop;
                } while (o = o.offsetParent);
                return { 'x': x, 'y': y };
            }
        };

        function initAnchor() {
            var base = anchorBase;
            var id = location.href.match(/#([^#]+)$/);
            if (!id || !id[1]) return;
            var obj = base.$(id[1] + "Tag");
            if (!obj) return;
            var y = base.offsetLT(obj).y - 72;
            base.setTop(y);
        }

        initAnchor();
        //-------------------------------------
    </script>
