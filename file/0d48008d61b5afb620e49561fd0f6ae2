        //二手车
        (function() {
            function getUCarForBottom(data, csId, csSpell, csShowName) {
                try {
                    var dataCarListInfo = data.CarListInfo;
                    var dataRelateCityList = data.RelateCityList;

                    if (dataCarListInfo.length <= 0) {
                        $("#ucarlist").hide();
                        return;
                    }
                    var strHtml = [];

                    strHtml.push("<div class=\"section-header header2 mb0\"><div class=\"box\">");
                    strHtml.push("<h2><a target=\"_blank\" href=\"http://www.taoche.com/" + csSpell + "/?ref=pc_yc_bj_tj_esc\">相关二手车推荐</a></h2></div>");
                    //strHtml.push("<div class=\"more\"><a target=\"_blank\" href=\"http://www.taoche.com/" + csSpell + "/\">");
                    strHtml.push("<div class=\"more\">");
                    if (dataRelateCityList.length > 0) {
                        $.each(dataRelateCityList, function(index, res) {
                            strHtml.push("<a href=\"" + res.CityUrl + "\" target=\"_blank\">" + res.CityName + "</a>");
                        });
                    }

                    strHtml.push("<a target=\"_blank\" href=\"http://www.taoche.com/" + csSpell + "/?ref=pc_yc_bj_tj_gd\">更多</a></div>");
                    strHtml.push("</div>");


                    strHtml.push("<div class=\"col4-180-box clearfix\">");
                    $.each(dataCarListInfo, function(i, n) {
                        if (i > 7) return;
                        var leads_source = n.CarlistUrl.indexOf("?") > -1 ? "&leads_source=p004012" : "?leads_source=p004012";
                        //var className = "";
                        //if (i == 0 || i == 4) className = "first";
                        //if (i == 3 || i == 7) className = "last";
                        strHtml.push(" <div class=\"img-info-layout-vertical  img-info-layout-vertical-180120-2\"><div class=\"img\"><a href=\"" + n.CarlistUrl + leads_source + "\" target=\"_blank\"><img src=\"" + n.PictureUrl + "\"></a></div>");
                        strHtml.push(" <ul class=\"p-list\"><li class=\"name\"><a href=\"" + n.CarlistUrl + leads_source + "\" target=\"_blank\">" + n.BrandName + "</a></li>");
                        strHtml.push("<li class=\"price\">￥" + n.DisplayPrice + "<span class=\"tag\">省" + n.SaveMoney + "万</span></li>");
                        strHtml.push("<li class=\"info\">" + n.BuyCarDate.replace("上牌","") + "上牌 " + n.DrivingMileage + "公里</li></ul></div>");
                    });
                    strHtml.push("</div>");

                    $("#ucarlist").html(strHtml.join(''));
                } catch (e) {
                }
            }

            var guidPrice = $("#emDirPrice").text();
            var lp;
            var hp;
            if (guidPrice != "暂无") {
                var guidPriceArray = guidPrice.split("-");
                if (guidPriceArray.length == 1) {
                    lp = guidPrice.substring(0, guidPrice.length - 1);
                    hp = lp;
                } else {
                    lp = guidPriceArray[0];
                    hp = guidPriceArray[1].substring(0, guidPriceArray[1].length - 1);
                }
            }
            var lpStr = typeof (lp) == "undefined" ? "" : "&lp=" + lp;
            var hpStr = typeof (hp) == "undefined" ? "" : "&hp=" + hp;
            $.ajax({
                url: "http://yicheapi.taoche.cn/CarSourceInterface/ForJson/CarListForYiChe.ashx?sid=3528&ctid=1229&count=8&ref=bjtj" + lpStr + hpStr,
                cache: true,
                dataType: "jsonp",
                jsonpCallback: "callback",
                success: function(data) {
                    getUCarForBottom(data, 3528, 'beijingbenchiglkji', '奔驰GLK级');
                }
            });
        })();

    </script>
