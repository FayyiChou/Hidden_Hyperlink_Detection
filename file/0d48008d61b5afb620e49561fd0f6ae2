    <script type="text/javascript">
        function setYiTuanGouEntrance() {
            $.ajax({
                //  url: 'http://api.market.bitauto.com/MessageInterface/YiTuanGou/GetYiTuanGouUrl.ashx?mediaId=1&locationId=4&cmdId=3528&cityId=1229',
                url: 'http://api.market.bitauto.com/MessageInterface/Product/GetProductUrl.ashx?MediaId=1&LocationId=1313,3&cmdId=3528&cityId=1229',
                cache: true,
                dataType: "jsonp",
                jsonpCallback: "jsonpcallbacktgorych",
                success: function(data) {
                    if (data && data.result == "yes" && data.carIds.length > 0) {
                        var sg = data.sign;
                        var carids = data.carIds;
                        for (var i = 0, len = carids.length; i < len; i++) {
                            if (sg == "1") {
                                $("#a_" + carids[i]).before("<a href='" + data.url + "' target='_blank' class='color-block'>团购</a>");
                            }
                            if (sg == "2") {
                                $("#a_" + carids[i]).before("<a href='" + data.url + "' target='_blank' class='color-block'>优惠</a>");
                            }
                        }
                    }
                }
            });
        }
        setYiTuanGouEntrance();

    </script>
