var onecount;
onecount=0;
subcat = new Array();
        

function changelocation(locationid)
    {
    document.myform.kechengid.length = 0; 

    var locationid=locationid;
    var i;
    for (i=0;i < onecount; i++)
        {
            if (subcat[i][1] == locationid)
            { 
                document.myform.kechengid.options[document.myform.kechengid.length] = new Option(subcat[i][0], subcat[i][2]);
            }        
        }
        
    }    
</SCRIPT> 
