<script LANGUAGE="JavaScript">
function checken()
{
 if (document.guestbook_en.name.value=="")
  {
   alert("请输入中文姓名！")
   document.guestbook_en.name.focus()
   document.guestbook_en.name.select()
   return
  }
 var mobile=document.guestbook_en.mobile.value;
 var reg0=/^1\d{6,10}$/;   //130--139。至少7位
 var my=false;
 if (reg0.test(mobile))my=true;
 if (!my){
	document.guestbook_en.mobile.value='';
	alert('请输入正确的手机号码');
	document.guestbook_en.mobile.focus()
    document.guestbook_en.mobile.select()
   return
} 
 if (document.guestbook_en.mobile.value=="")
  {
   alert("请输入手机号码！")
   document.guestbook_en.mobile.focus()
   document.guestbook_en.mobile.select()
   return
  }
   document.guestbook_en.submit()
}
</script>
