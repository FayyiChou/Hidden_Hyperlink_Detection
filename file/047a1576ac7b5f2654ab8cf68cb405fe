var curpage = 1;//当前页面
var allcount = 1553;//总记录数
var pageCount = 104;//总页数
function showacmore(){
	if(curpage<=pageCount){
		curpage++;
		$.ajax({
			type: "GET",
			url: "/index/index-"+curpage+".html",
			global:"false",
			data:'',
			error:function(){
				curpage--;
			},
			success: function(msg){
				var s = '<!--' + 'bodystart-->';
				var e = '<!--' + 'bodyend-->';
				if(msg.indexOf(s)>-1){
					msg = msg.split(s)[1];
					msg = msg.split(e)[0];
					$('#listbox').append(msg);
				}
				if(curpage>=pageCount){
					$('#showmore').hide();
					$('#showmorenone').show();
				}
			}
		});
	}else{
		$('#showmore').hide();
	}
}

//swiper
Common.onSwiper('#homeSwiper');
$('.slide-content h2').each(function(index,element){
	
	var html = '';
	var arr = $(element).text().split('%%');	
        
    for (var i = 0; i < arr.length; i++) {
        html += "<p><span class='bg-black'>" + arr[i] + "</span></p>" 
    }
    $(element).next().append(html);
})
</script>
