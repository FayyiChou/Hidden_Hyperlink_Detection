<script>
setInterval("getIndex()",10);
function getIndex() {
	var iconIndex = $(".count").find(".current").index();
	$(".list").find("li:eq(" + iconIndex + ")").css("z-index","10000");
	$(".list").find("li:eq(" + iconIndex + ")").siblings("li").css("z-index","10");
}
$("#box").find(".count").css("z-index","10000");
</script>
