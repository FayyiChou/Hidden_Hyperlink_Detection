			marqueeContent6 = new Array(imageUrlStr6Arr.length);
			for(var i = 0; i < imageUrlStr6Arr.length; i++){
				marqueeContent6[i] = '<ul><li><a href="#"><img src="' + 'http://www.scfp.cn:80/' + imageUrlStr6Arr[i] + '" width="115" height="42"></a></li></ul>';				
			}
			var p6=new createObject('6');
			p6.initContent(marqueeContent6);
		</script>
	</dd>
	<dd>
		<input type="hidden" id="imageUrlStr7" value="file_upload/doc/focusImageUrl_2012-07-03-124648.gif,file_upload/doc/focusImageUrl_2012-07-03-124718.gif,file_upload/doc/focusImageUrl_2012-07-19-104334.gif,file_upload/doc/focusImageUrl_2012-07-19-104346.gif,file_upload/doc/focusImageUrl_2012-07-19-104401.gif" />
		<script type="text/javascript">
			var imageUrlStr7 = document.getElementById("imageUrlStr7").value;
			var imageUrlStr7Arr = imageUrlStr7.split(",");
			marqueeContent7 = new Array(imageUrlStr7Arr.length);
			for(var i = 0; i < imageUrlStr7Arr.length; i++){
				marqueeContent7[i] = '<ul><li><a href="#"><img src="' + 'http://www.scfp.cn:80/' + imageUrlStr7Arr[i] + '" width="115" height="42"></a></li></ul>';				
			}
			var p7=new createObject('7');
			p7.initContent(marqueeContent7);
		</script>
	</dd>
</dl> 
