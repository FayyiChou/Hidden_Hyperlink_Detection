  function(){
    $('.open').addClass('close');
    $('.alignCenter').hide();
    $(".kf_main").animate({width:'hide',opacity:'hide'},'normal',function(){
      $(".kf_main").hide();
      var href="data/images/other/20151202101158_308.jpg";
