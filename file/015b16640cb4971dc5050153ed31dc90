<script>

    COLLIERS.cookieNotificationDisplay = {
        CookieName: 'Cookie_Consent_d8a3a92e-a238-49f8-a4c8-d1425e23f3de',
        ShowMessage: 'True'

    }

    function hideCookieNotificationPanel(cookieName) {
        var cookiesPanel = document.getElementById("cookieUsageNotification");
        var url = COLLIERS.analytics.trackUrl; // endpoint url here;
        var currentVal = COLLIERS.utilities.cookie.read(cookieName)
        if (currentVal == null)
            COLLIERS.utilities.cookie.create(cookieName, true, 99999);

        $.ajax({
            type: 'POST',
            url: url + '?HideMessage=true',
            data: 'HideMessage=true',
            success: function (data) {
                if (cookiesPanel != null) {
                    cookiesPanel.style.display = "none";
                    cookiesPanel = null;
                }
            },
            error: function (e) {
            }
        });
    };
</script>


