	function jubao()
	{
		var link = $("input[name='jblj']").attr('value');
		var uid	 = '210365';
		var seller_name	 = '枣庄美玲物流有限公司';
		var jbyy = $("input[name='jbyy']:checked").attr('value');
		var wtsm = $("textarea[name='wtsm']").attr('value');
		var lxdh = $("input[name='lxdh']").attr('value');
		if(wtsm=='不低于5个字，最多200字' || wtsm==''){alert('请填写问题说明');return false;}else if(lxdh=='方便我们给您反馈处理结果' || lxdh==''){alert('请填写联系电话');return false;}
		
		$.getJSON('/index.php?app=accusation&act=index&ajax=1',{jb_link:link,jb_tel:lxdh,by_jb_uid:uid,seller_name:seller_name,jb_content:jbyy,jb_question:wtsm},function(data){
			alert(data.msg);
			$('.sc1').hide();
		});
	}
</script> 
