<script>
	//点击左侧QQ关闭QQ
	$(function(){
		$('.right_qq .cha').click(function(){
			$('.right_qq').hide();	
		});
		$('.right_qq .tanchu').click(function(){
			$('.qqkuang').fadeIn(600);
			$('.tanchukuang').show();	
		});	
		$('.qq_main .xiaocha').click(function(){
			$('.qqkuang').fadeOut(300);
			$('.tanchukuang').hide();	
		});
	});
</script>
