				qqSell[2] = '<li class="fl"><img border="0" title="点击这里给我发消息" alt="点击这里给我发消息" src="http://wpa.qq.com/pa?p=2:721775240:4"><a href="http://wpa.qq.com/msgrd?v=3amp;&amp;uin=19767526&amp;site=qq&amp;menu=yes" target="_blank">晚班包子</a></li>';
