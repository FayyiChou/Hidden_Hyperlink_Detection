function rotate1(){
var m=0;
for(var j=1; j<=3; j++){
if(document.getElementById("mcc1"+j).style.display=="block"||document.getElementById("mcc1"+j).style.display==""){
if(j==3){m=1;
}else{m=j+1;}}}
for(var j=1; j<=3; j++){
if(m==j){
document.getElementById("mcc1"+m).style.display="block";
document.getElementById("mck1"+m).src="/images/hui_over.jpg";
}else{
document.getElementById("mcc1"+j).style.display="none";
document.getElementById("mck1"+j).src="/images/hui.jpg";}}
setTime_g1=setTimeout("rotate1()",interval1);}
var interval1="5000";
var setTime_g1;
clearTimeout(setTime_g1);
setTime_g1=setTimeout("rotate1()",interval1);

function rotate2(){
