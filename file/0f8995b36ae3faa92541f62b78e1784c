for(var j=1; j<=3; j++){
if(document.getElementById("mcc1"+j).style.display=="block"){dang=j;
}}if(dang==3){dang=1;
}else{dang=dang+1;}
for(var j=1; j<=3; j++){
document.getElementById("mcc1"+j).style.display="none";
document.getElementById("mck1"+j).src="/images/hui.jpg";}
document.getElementById("mcc1"+dang).style.display="block";
document.getElementById("mck1"+dang).src="/images/hui_over.jpg";}

