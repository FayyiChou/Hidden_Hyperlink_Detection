var timeout         = 500;
var closetimer		= 0;
var ddmenuitem      = 0;

function m2open(id)
{	
	// cancel close timer
	//mcancelclosetime();

	
	// get new layer and show it
	
	if(id=="pro1")
	{
	ddmenuitem = document.getElementById("pro2");
