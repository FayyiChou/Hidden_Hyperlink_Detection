function ddd2(ii,obj, sType) { 
var oDiv = document.getElementById(obj); 
if (sType == 'show') { oDiv.style.visibility = 'visible';} 
if (sType == 'hide') { oDiv.style.visibility = 'hidden';} 
if (sType == 'show') 
{
    if(ii=="1")
    {
        document.getElementById("li_1").className="nav2_1o";
        document.getElementById("li_2").className="";
    }
    else
    {
        document.getElementById("li_2").className="nav2_1o";
        document.getElementById("li_1").className="";
    }
}
else
{
    document.getElementById("li_2").className="";
    document.getElementById("li_1").className="";
}

} 

function ddd(obj, sType) { 
var oDiv = document.getElementById(obj); 
if (sType == 'show') { oDiv.style.visibility = 'visible';} 
if (sType == 'hide') { oDiv.style.visibility = 'hidden';} 


} 
