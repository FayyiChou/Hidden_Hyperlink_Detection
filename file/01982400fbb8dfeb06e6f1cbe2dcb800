<script language="JavaScript" type="text/javascript" src="http://samsungbbs.cnmo.com/js/public_base.js"></script>
<script>
//公用日历脚本 签到
var calender_loadedCalender = [],calender_loadedNumber = 0,calender_isLoading = 0,calender_isFirstLoad = 0;
function signIn(obj) {
	var isclass = $(obj).hasClass('cur');
	if (isclass) {
		if ( $('.Google_div').is(':hidden') ) {
			$('.Google_div').show();
		} else {
			$('.Google_div').hide();
		}
		if ( calender_isFirstLoad == 0 ) {
			$('#reward').show();
			loadCalendar();
			loadNumber();
			calender_isFirstLoad = 1;
		}
	} else {
		$.get("http://samsungbbs.cnmo.com/index.php?c=SignUp&m=SignIn", function (data) {
			var content = eval('(' + data + ')');
			if (content['status'] == 0) {
				LoginWindow.showLoginBox();
			} else if (content['status'] == -1) {
				ConfirmNotice.showBox({setContent: content['msg']});
			} else if (content['status'] == -2) {
				ConfirmNotice.showBox({setContent: content['msg']});
			} else if (content['status'] == 1) {
				if ( $('.Google_div').length > 0 && $('.Google_div').is(':hidden') ) {
					$('.Google_div').show();
				}
				if ( $('#reward').length > 0 && $('#reward').is(':hidden') ) {
					$('#reward').show();
				}
				$(obj).addClass('cur');
				loadCalendar();
				loadNumber();
				ConfirmNotice.showBox({setContent: '签到成功！'});
			}
		});
	}
}
// 显示日历
function loadCalendar(y, m) {
	if ( calender_isLoading == 1 ) {
		return false;
	}
	if ( calender_loadedCalender[y+'_'+m] ) {
		$('#calendar').empty().append(calender_loadedCalender[y+'_'+m]).show();
	} else {
		$.get('http://samsungbbs.cnmo.com/index.php?c=SignUp&m=SignLog&year=' + y + '&month=' + m, function (data) {
			if (data) {
				$('#calendar').empty().append(data).show();
				calender_loadedCalender[y+'_'+m] = data;
			}
		});
	}
}
function loadNumber() {
	if ( calender_loadedNumber == 1 ) {
		return false;
	}
	$.get('http://samsungbbs.cnmo.com/index.php?c=SignUp&m=Number', function (data) {
		var content = eval('(' + data + ')');
		if ( typeof content == 'object' ) {
			var str = "<p>已经累计签到：<em id='days'>" + content.signhistory + "</em>天</p>";
			str += "<p>本次签到奖励：<em id='gold'>" + content.today.integral + "</em>积分" + "<em id='gold'>" + content.today.gold + "</em>金币" + "，明日签到可领<em id='tgold'>" + content.tomorrow.integral + "</em>积分" + "<em id='tgold'>" + content.tomorrow.gold + "</em>金币" + "</p>";
			$('#reward').empty().append($(str)).show();
		}
		calender_loadedNumber = 1;
	});
}
</script><script type="text/javascript">
//分类标签
$("#tag").click(function(event) {
	if ($('#tag_menu').is(":visible")) {
	    $("#tag").removeClass('cur');
	    $('#tag_menu').hide();
	} else {
	    $("#tag").addClass('cur');
	    $('#tag_menu').show();
	}
	//点击任意地方隐藏
	$(document).one("click", function(){
		$("#tag").removeClass('cur');
	    $('#tag_menu').hide();
	});
	
	event.stopPropagation();
});

//排序标签
$("#order").click(function(event) {
	if ($('#order_menu').is(":visible")) {
	    $("#order").removeClass('cur');
	    $('#order_menu').hide();
	} else {
	    $("#order").addClass('cur');
	    $('#order_menu').show();
	}
	//点击任意地方隐藏
	$(document).one("click", function(){
		$("#order").removeClass('cur');
	    $('#order_menu').hide();
	});
	
	event.stopPropagation();
});
</script>
