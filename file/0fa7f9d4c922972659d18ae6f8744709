<div class="seaCon4"><!--下载简历-->
<div id=demo style="overflow:hidden;height:25px;"><div id=demo1>
 <div class="seaCon41" id="downResume"></div>
 </div><div id=demo2></div></div>
 <script>
  var speed		= 15;//滚动速度
  var rows		= 25;//每行高度
  var stim		= 160; //停留时间倍数 * speed
  var stop		= 0; //初始化值，不管 
  var idemo    =  document.getElementById("demo");
  var idemo1    =  document.getElementById("demo1");
  var idemo2    =  document.getElementById("demo2");
  idemo2.innerHTML	= idemo1.innerHTML
  function Marquee(){
    if(idemo.scrollTop%rows==0 && stop<=stim){ stop++;return; }
    stop = 0;
    if(idemo2.offsetTop-idemo.scrollTop<=0)
      idemo.scrollTop-=idemo1.offsetHeight
    else{ idemo.scrollTop++ }
  }
  var MyMar	   = setInterval(Marquee,speed)
  idemo.onmouseover = function() {clearInterval(MyMar)}
  idemo.onmouseout  = function() {MyMar=setInterval(Marquee,speed)}
</script>
</div>
