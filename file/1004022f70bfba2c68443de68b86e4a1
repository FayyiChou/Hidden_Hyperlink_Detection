<script language="javascript">
function checkSO()
{
    var keyword=document.getElementById("txtkeyword");
    if(keyword.value=="" || keyword.value=="请输入要搜索的关键字")
    {
        alert("请输入要搜索的关键字");
		keyword.value='';
        keyword.focus();
        return false;
    }
    else
    {
        window.location.href="Search.aspx?KeyWord="+escape(keyword.value);
    }
}

</script>
