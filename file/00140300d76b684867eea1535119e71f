    <script type="text/javascript">
    var DealerPages = {
        PageSize: 10,
        TotalCount: $("#dealersCache>ul>li").length,
        Pages: 0,
        CurrentNum: 1,
        WillNum: 1,
        PrePage: $("#btnPrePage"),
        NextPage: $("#btnNextPage"),
        PageInfo: $("#divPageInfo"),
        Init: function () {
            this.Pages = parseInt((this.TotalCount + this.PageSize - 1) / this.PageSize);
            $("#pageSelect").change(function () {
                DealerPages.WillNum = $(this).val();
                DealerPages.changePage();
            });
            this.PrePage.click(function () {
                DealerPages.WillNum--;
                DealerPages.changePage();
            });
            this.NextPage.click(function () {
                DealerPages.WillNum++;
                DealerPages.changePage();
            });
        },
        btnDisable: function () {
            if (this.CurrentNum == 1)
                this.PrePage.addClass("m-pages-none");
            else
                this.PrePage.removeClass("m-pages-none");
            if (this.CurrentNum == this.Pages)
                this.NextPage.addClass("m-pages-none");
            else
                this.NextPage.removeClass("m-pages-none");
        },
        canUse: function (obj) {
            if (obj.hasClass("m-pages-none"))
                return false;
            return true;
        },
        changePage: function () {
            if (this.CurrentNum == this.WillNum || this.WillNum > this.Pages || this.WillNum < 1) {
                this.WillNum = this.CurrentNum;
                return;
            }
            var start = (this.WillNum - 1) * this.PageSize;
            var end = this.WillNum * this.PageSize;
            if (end > this.TotalCount)
                end = this.TotalCount;
            $("#dealersDiv ul").html("");
            var str = "";
            for (var i = start; i < end; i++) {
                str += "<li>" + $("#dealersCache>ul>li:eq(" + i + ")").html() + "</li>";
            }
            $("#dealersDiv ul").append(str);
            this.CurrentNum = this.WillNum;
            this.PageInfo.html(this.CurrentNum + "/" + this.Pages);
            $("#pageSelect option[value=" + this.CurrentNum + "]").attr("selected", "selected");
            this.btnDisable();
            $("html,body").animate({ scrollTop: $("#anchor").offset().top }, 300)
        }
    }
    DealerPages.Init();
</script>

