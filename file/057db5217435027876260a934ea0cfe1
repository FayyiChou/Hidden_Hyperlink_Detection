function getPic2(picPath,sthumb){
	//alert(picPath);
	document.newContentForm.picUrl.value = picPath;
	var obj = document.getElementById("prePic");
	var div = document.getElementById("prePicDiv");
	obj.src=sthumb;
	div.style.display = "";
	
}

function deletePic2(){
	document.newContentForm.picUrl.value = "";
	var obj = document.getElementById("prePic");
	var div = document.getElementById("prePicDiv");
	obj.src="";
	div.style.display = "none";
}
