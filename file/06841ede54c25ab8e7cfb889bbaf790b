<script>
$(document).ready(function(){

//头区荔枝新闻打开判断（Islichinewsapp=1代表是app客户端打开）
function GetQueryString(name) {     
	var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");     
	var r = window.location.search.substr(1).match(reg);     
	if (r != null) return  unescape(r[2]);
	return null;
}
if(GetQueryString("Islichinewsapp")==1){
	$(".header1").hide();
}
if(GetQueryString("channel")=="city"){
	$(".header1").hide();
        $(".secTitle").css("border-left","8px solid #4C73D3");
}
//头区荔枝新闻打开判断结束
	var docWidth = document.documentElement.clientWidth;
	$(".video-wrapper").height(docWidth*0.5714);
	var video = videoPlayer("video",{
		autoPlay:false,
		/*width:320,
		height:240,*/
		virtualFullScreen:false,
		setSource:function(canplayType){
            if(canplayType == 'mp4' &&　ad.before_ad != null && ad.before_ad != ""){
                return ad.before_ad;
            }else if(canplayType == 'mp4' &&　(ad.before_ad == null || ad.before_ad == "")){
				return ad.video;
			}
        },
		success:function(videoElement,node,videoObj){
			$("#video video").height(docWidth*0.5714);
			$(videoElement).attr('poster',poster);
			$(videoElement).attr("webkit-playsinline","true");
			if(ad.before_ad != null && ad.before_ad != ""){
				$("#video").find("video").on("ended",function(){
					$("#video").find("video").attr({"src":ad.video,"autoplay":"autoplay"}).get(0).play();
					/*$("#video").find(".yvp_controls,.yvp_progress").css({"opacity":"1","z-index":"100","display":"block"});*/
					$(videoElement).attr("controls","controls");
					$(".yvp_membrane").css("display","block");
					$("#video").find("video").unbind("ended");

				});
			}else{
				$(videoElement).attr("controls","controls");
			}
			window.onorientationchange = orientationChange;
        },
        error:function(){
	    }
	});
	function orientationChange(){
		if(window.orientation==180||window.orientation==0){
			var videoWrapWidth = document.documentElement.clientWidth;
			$(".video-wrapper").height(videoWrapWidth*0.5714);
			$("#video video").height(videoWrapWidth*0.5714);      
	    }
		if(window.orientation==90||window.orientation==-90){
			var videoWrapWidth = document.documentElement.clientWidth;
			$(".video-wrapper").height(videoWrapWidth*0.5714);
			$("#video video").height(videoWrapWidth*0.5714);      
	    }
	}
});

</script>

