    	<div id="mainDiv" class="mainDiv" avalonctrl="login.index" style="margin-top: -290px; ">
		<script>
			
			/**
			**弱密码对应的提示
			**/
			function passwordTip(vm){
				var desc = '检测到您的密码设置采用弱口令：<br>' + vm.desc;

				var index = layer.alert(desc, {icon:0}, function (){
					layer.close(index);
					window.location.href = "/views/nsryhgl/force_pwd.jsp?nsrsbh=" + vm.login.nsrsbh;
					return false;
				});

				//未找到layer右上角关闭按钮事件绑定函数，采用补丁式增加绑定事件
				/**
				setTimeout(function(){
					$('.layui-layer-close').bind('click', function(){
						window.location.href = "/bsfw/login/lhrzForward.do";
					});
				}, 500);**/
				
			}
		
			if(window.top != window.self){
				window.top.location.href = window.self.location.href;
			}
			var login_vm = null;
			!function() {
				var vm = avalon.define({
					$id : "login.index",
					loginSuccess : true,
					passwordCheck : 'N',
					desc : '',
					login : {
						nsrsbh : "识别号",
						yhmm : null,
						checkCode : "输入计算结果",
						loginType : 'bsfw'
					},
					$actions : {
						checkAndLogin : {
							url : "/bsfw/login/checkAndLogin.do",
							args : "login",
							before : "checkVal",
							success : "showSuccess"
						}
					},
					checkVal: function(id) {
						//return false,不执行action:
						if(!vm.login.nsrsbh){
							layer.alert("纳税人识别号不能为空！",{icon:0});
							return false;
						}
						if(!vm.login.yhmm){
							layer.alert("密码不能为空！",{icon:0});
							return false;
						}
						if(!vm.login.checkCode){
							layer.alert("验证码不能为空",{icon:0});
							return false;
						}
						vm.loginSuccess = true;
						vm.passwordCheck = 'N';
						return true;
					},
					showSuccess: function() {
						changeCheckcode();
						if(vm.passwordCheck && vm.passwordCheck == 'Y'){
							passwordTip(vm);
							return false;
						}
						
						if(!vm.loginSuccess){
							layer.alert(vm.desc,{icon:2});
							return false;
						}
						window.location.href = "/bsfw/login/lhrzForward.do";
					}
				});

				avalon.ready(function(){
		
				});

				login_vm = vm;
			}();


			function changeCheckcode(){
				document.getElementById("imgcheckcode").src = "/bsfw/login/checkcode.do?r="+Math.random()+"&ct=bsfw";
			}

			


			
			//回车登录功能
			$(function(){
				$('body').keyup(function(e){
					e = e || window.event;
					var code = e.which || e.keyCode;
					if(code == 13){
						otp4.doAction("checkAndLogin", login_vm);
					}
				});

			});	

			
		</script>
		<div id="bsfwLoginHead">
   			<img src="/res/images/bsfwhall/bsfw_title.png">
 		</div>
