<script>
window.onload = function(){
	$('#mainDiv').css('marginTop',($('body').height()/2-290)+'px');
	
	$("#bsfwdtLoginId").bind({
		"focus":function(){
			if(this.value == '识别号') {
				this.value=''; 
				this.style.color='#000000';
			}
		},
		"blur":function(){
			if(this.value == ''){
				this.value='识别号'; 
				this.style.color='#cccccc';
			}
		}
	});

	$("#bsfwdtPasswordText").bind({
		"focus":function(){
				$(this).hide();
				$('#bsfwdtPassword').show();
				$('#bsfwdtPassword').focus();	
		}
	});

	$("#bsfwdtPassword").bind({
		"blur":function(){
			if(this.value == ''){
				$('#bsfwdtPasswordText').show();
				$(this).hide();
			}
		}
	});

	
	$("#bsfwdtCheckCode").on({
		"focus":function(){
			if(this.value == '输入计算结果'){
				this.value=''; 
				this.style.color='#000000';
			}
		},
		"blur":function(){
			if(this.value == ''){
				this.value='输入计算结果'; 
				this.style.color='#cccccc';
			}
		}
	});


	var bodyWidth = $('body').width();
	$('.bsfwdt_login_buttom').css('left', (bodyWidth/2-300)+'px');
	$('#loginMode').css('left', (bodyWidth/2+200)+'px');
}
</script>



  
  


</div>
</div>
