<script>
    $(function () {
        // 各期导航选项卡
        function initIssueNav() {
            var $btn = $('#periodicalYear>li');
            var $con = $('#periodicalIssue>div');
            var $txt = $('#periodicalYearTit>span');
            var $tit = $('#periodicalYearTit');
            var $ico = $('#periodicalYearTit>i');
            var $list = $('#periodicalYearCon');

            $btn.click(function () {
                var $this = $(this);
                var idx = $this.index();
                $this.find('a').addClass('current');
                $this.siblings('li').find('a').removeClass('current');
                $txt.text($this.text());
                $con.eq(idx).show().siblings('div').hide();
                $list.removeClass('open').addClass('close');
                return false;
            });
            $tit.click(function () {
                var $this = $(this);
                var $box = $('#periodicalYearCon');
                if ($box.hasClass('open')) {
                    $list.removeClass('open').addClass('close');
                    $ico.removeClass('icon-arr-up').addClass('icon-arr-down');
                    $this.removeClass('open').addClass('close');
                } else {
                    $list.removeClass('close').addClass('open');
                    $ico.removeClass('icon-arr-down').addClass('icon-arr-up');
                    $this.removeClass('close').addClass('open');
                };
            });
        };
        initIssueNav();
    });
</script>

    
