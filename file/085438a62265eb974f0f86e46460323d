<script type="text/javascript">
$(function () {
    /* 回到顶部 */
    $('#btnGoTop').click(function (e) {
        e.preventDefault();
        $('body, html').animate({
            scrollTop: 0
        }, 'fast');
    });
    $(window).scroll(function () {
        var $this = $(this);
        var winTop = $this.scrollTop();
        var winHeight = $this.height();
        if (winTop > $(window).height()) {

            $('.med-toolbar').fadeIn();
        } else {
            $('.med-toolbar').fadeOut();
        };
    });
});
