<script>
/* 页头 登录 */
function GetAuthContext() {
    var context = null;
    var cookieArray = document.cookie.split("; ");
    $(cookieArray).each(function () {
        var arr = this.split("=");
        if (arr[0] == "WFMed.Auth") {
            context = eval("(" + decodeURIComponent(arr[1]) + ")");
        };
    });
    return context;
}

function GetDisplayName(context, accountIdStr) {
    for (var i = 0; i < context.Data.length; i++) {
        if (context.Data[i].Key === (accountIdStr + ".DisplayName") && context.Data[i].Value.length > 0) {
            return decodeURIComponent(context.Data[i].Value);
        }
    }
    return GetAccountIdObj(accountIdStr).Key;
}

function GetAccountIdObj(accountIdStr) {
    var accountId = {
        AccountType: '',
        Key: ''
    },
        index = accountIdStr.indexOf(".");
    if (index === -1) {
        accountId.AccountType = accountIdStr;
    } else {
        accountId.AccountType = accountIdStr.substring(0, index);
        accountId.Key = accountIdStr.substring(index + 1);
    }
    return accountId;
}
function createAutocompleteList(arr, type) {
    var $autocompleteList = $('#autocompleteList');
    var $headSearchInput = $('#headSearchInput');
    $autocompleteList.hide();
    $autocompleteList.empty(); // 重置列表内容
    $.each(arr, function (i) {
        var item = arr[i];
        var keywordStr = '';
        var keywordinfoStr = '';
        var typeStr = '';
        var typeStr_k = '';
        var kidStr = '';
        var itemStr = '';
        if (type == "" || type == null) {
            keywordStr = '<span>' + item.Keyword + '</span>';
            if (item.KeywordInfo) {
                keywordinfoStr = '（' + item.KeywordInfo + '）';
            };
            var trimKeyword = item.Keyword.replace(/<\/?em>/g, '').replace(/(^[\"\'])|([\"\']$)/g, '');
            typeStr_k = trimKeyword;
            switch (item.Type) {
                case 1:
                    typeStr = '<strong>主题词=</strong>';
                    typeStr_k = '主题词="' + trimKeyword + '"';
                    break;
                case 2:
                    typeStr = '<strong>作者=</strong>';
                    typeStr_k = '作者="' + trimKeyword + '"';
                    if (item.KId) {
                        kidStr = '<a href="http://med.wanfangdata.com.cn/Author/General/' + item.KId + '" class="link">进入作者空间</a>';
                    };
                    break;
                case 3:
                    typeStr = '<strong>期刊=</strong>';
                    typeStr_k = '期刊="' + trimKeyword + '"';
                    if (item.KId) {
                        kidStr = '<a href="http://med.wanfangdata.com.cn/Periodical/' + item.KId + '" class="link">进入期刊首页</a>';
                    };
                    break;
                case 4:
                    typeStr = '<strong>机构=</strong>';
                    typeStr_k = '机构="' + trimKeyword + '"';
                    if (item.KId) {
                        kidStr = '<a href="http://med.wanfangdata.com.cn/Organization/General/' + item.KId + '" class="link">进入机构空间</a>';
                    };
                    break;
            };
            typeStr_k = typeStr_k.replace(/\"/g, '&quot;').replace(/\'/g, "&apos;");
            // 拼接
            itemStr = '<li class="med-autocomplete-item"><div class="tags log" data-logname="autoComplete" data-logvalue="' + item.Type
                + '" data-keyword="' + typeStr_k + '">' + typeStr + keywordStr + keywordinfoStr + '</div>' + kidStr + '</li>';
        }
        else {

            keywordStr = '<span>' + item.Keyword + '</span>';
            if (item.KeywordInfo) {
                keywordinfoStr = '（' + item.KeywordInfo + '）';
            };
            var trimKeyword = item.Keyword.replace(/<\/?em>/g, '').replace(/(^[\"\'])|([\"\']$)/g, '');
            typeStr_k = trimKeyword;
            switch (item.Type) {
                case 1:
                    typeStr_k = '主题词="' + trimKeyword + '"';
                    break;
                case 2:
                    typeStr_k = trimKeyword ;
                    if (item.KId) {
                        kidStr = '<a href="http://med.wanfangdata.com.cn/Author/General/' + item.KId + '" class="link">进入作者空间</a>';
                    };
                    break;
                case 3:
                    typeStr_k = trimKeyword ;
                    if (item.KId) {
                        kidStr = '<a href="http://med.wanfangdata.com.cn/Periodical/' + item.KId + '" class="link">进入期刊首页</a>';
                    };
                    break;
                case 4:
                    typeStr_k = trimKeyword ;
                    if (item.KId) {
                        kidStr = '<a href="http://med.wanfangdata.com.cn/Organization/General/' + item.KId + '" class="link">进入机构空间</a>';
                    };
                    break;
            };
            typeStr_k = typeStr_k.replace(/\"/g, '&quot;').replace(/\'/g, "&apos;");
            // 拼接
            itemStr = '<li class="med-autocomplete-item"><div class="tags log" data-logname="autoComplete" data-logvalue="' + item.Type
                + '" data-keyword="' + typeStr_k + '">' + keywordStr + keywordinfoStr + '</div>' + kidStr + '</li>';
        }

        $(itemStr).appendTo($autocompleteList);
    });
    if (arr.length > 0) {
        // 设置列表位置
        if ($('.container').width() > 640) {
            $autocompleteList.css({
                'top': $headSearchInput.position().top + 31,
                'left': $headSearchInput.position().left + 1,
                'width': $headSearchInput.innerWidth() - 2
            }).show();
        } else {
            $autocompleteList.css({
                'top': $headSearchInput.position().top + 34,
                'left': $headSearchInput.position().left - 2,
                'width': $headSearchInput.parent('.bar').innerWidth() - 2
            }).show();
        };
    }
};
$(function () {
    /* 主导航下拉菜单 LG */
    iDropdown('mainNavLG');
    iDropdown('mainNavMD');
    iDropdownMini('mainNavSM');
    /* 导航按钮 SM */
    var $btnList = $('#btnList');
    $btnList.click(function () {
        $('#searchBox').hide(0, function () {
            var docHeight = $(document).height();
            $overlay.css('height', docHeight - 50);
            $overlay.toggle();
        });
        $btnMagnifier.removeClass('active');
        $(this).toggleClass('active');
        showAndHide('mainNavSM');
        showAndHide('language');
    });
    /* 添加遮罩层 点击遮罩层收缩导航 并防止误点击 */
    var $overlay = $('#overlay');
    $overlay.click(function () {
        hideMainNav();
    });
    /* 检索按钮 SM */
    var $btnMagnifier = $('#btnMagnifier');
    $btnMagnifier.click(function () {
        hideMainNav();
        $(this).toggleClass('active');
        showAndHide('searchBox');
    });
    InitTopSearch();
    InitHighlight();
    InitSimpleTradeConvert();
    InitLoginStatus();
	$(document).on('click', '.log', function (event) {
            var me = $(this);
            var name = me.data("logname");
            var value = me.data("logvalue");
            if (me.attr("id") === "searchButton") {
                name = "secondSearchBtn";
            }
            var anctionName = name;
            if (!name) {
                return;
            }
            if (!value) {
                value = name;
            } else {
                anctionName = anctionName + "_" + value;
            }
            _hmt.push(['_setCustomVar', 1, name, value, 1]);
           $.post('/api/log', { "": anctionName });
        });
    var $autocompleteList = $('#autocompleteList');
    var $headSearchInput = $('#headSearchInput');

    $headSearchInput.keyup(function () {
        var word = $.trim($(this).val());
        $('.med-search .menu').children('li').slice(0, 5).find('a').each(function () {
            var type = $(this).attr("data-type");
            if ($(this).hasClass('current')) {
                if (word) {
                    $.ajax({
                        url: 'http://med.wanfangdata.com.cn/AutoComplete',
                        data: {
                            q: word,
                            type: type
                        },
                        dataType: "jsonp",
                        success: function (data) {
                            createAutocompleteList(data, type);
                        },
                        error: function () {
                            //console.log('error');
                        }
                    });
                }
                else {
                    $autocompleteList.hide();
                }
            }
        })
    });

    $('body').on('click', '.med-autocomplete-item', function () {
        $headSearchInput.val($(this).find('.tags').data('keyword'));
    });
    $('body').click(function () {
        $autocompleteList.hide();
    });
});
/* 检索功能 */
function InitTopSearch() {
    var $searchInput = $("#headSearchInput");
    $searchInput.keypress(function (e) {
        if (e.keyCode === 13) {
            TopSearch();
        }
    });
    $("#btntopSearch").click(TopSearch);
    $('#searchBox ul li a').click(function () {
        var me = $(this);
        me.parent().siblings().find('a').removeClass('current');
        me.addClass('current');
        $searchInput.watermark(me.attr('data-title'));
        $('#autocompleteList').hide();
        return false;
    });
}

function TopSearch() {
    var query = $.trim($("#headSearchInput").val());
    if (query.length === 0) {
        alert("请输入搜索关键词");
        $("#headSearchInput").focus();
        return false;
    }
    window.location.href = $("#searchBox ul li a.current").attr('data-url') + encodeURIComponent(query);
    return false;
}
/* 菜单及检索高亮 */
function InitHighlight() {
    var highlight = $('#topSearchHighLight').val();
    var highlightSearch = highlight.split("_", 1);
    var $currentTab = $('#searchBox ul li a').removeClass('current').filter('[data-id=search_' + highlightSearch + ']').addClass('current');
    $("#headSearchInput").watermark($currentTab.attr('data-title'));
    $('#mainNavLG li a, #mainNavMD li a').removeClass('current').filter('[data-id=nav_' + highlight + ']').addClass('current');
    $('#mainNavLG li a, #mainNavMD li a').filter('[data-id=nav_' + highlight + ']').parents('.dropdown').addClass('current');
}
/* 繁简体转换 */
function InitSimpleTradeConvert() {
    $("a[name='multiLanguageSwitch']").click(function () {
        var date = new Date();
        date.setTime(date.getTime() + 24 * 60 * 60 * 1000);
        var expires = date.toUTCString();
        var cookieValue = "PreferredCulture=" + escape(this.id);
        cookieValue += ";expires=" + expires;
        cookieValue += ";path=/;domain=.wanfangdata.com.cn";
        cookieValue += ";true";
        document.cookie = cookieValue;
        //保持原站点简繁体cookie
        var lng = this.id == "zh-CN" ? "zh_cn" : (this.id === "zh-TW" ? "zh_TW" : this.id);
        cookieValue = "lng=" + escape(lng);
        cookieValue += ";expires=" + expires;
        cookieValue += ";path=/;domain=med.wanfangdata.com.cn";
        cookieValue += ";true";
        document.cookie = cookieValue;
        window.location.href = document.location.protocol + "//" + document.location.host + document.location.pathname + document.location.search;
        return false;
    });
}
/* 登录状态 */
function InitLoginStatus() {
    var id = "";
    var authContext = GetAuthContext();
    if (authContext == null || authContext.Context.AccountIds.length == 0) {
        $("#userLogout").hide();
        $("#userLogin").show();
        var returnUrl = document.location.href.toLocaleLowerCase().indexOf("returnurl") != -1 ? "" : '?ReturnUrl=' + encodeURIComponent(document.location.href);
        $("#loginUrl").attr("href", $("#loginUrl").attr("href") + returnUrl);
        $("#registerUrl").attr("href", $("#registerUrl").attr("href") + returnUrl);
        $("#btnUser a").attr("href", $("#loginUrl").attr("href"));
        return;
    }
    $("#userLogin").hide();
    $(authContext.Context.AccountIds).each(function () {
        if (this.indexOf("MedGroupAdmin") >= 0) {
            id = this;
            return false;
        }
        if (this.indexOf("MedPerson") >= 0) {
            id = this;
            return false;
        }
        if (this.indexOf("MedGroup") >= 0) {
            id = this;
        }
        return true;
    });
    var displayName = GetDisplayName(authContext.Context, id);
    $("#loginUserName").text(displayName);
    $("#btnUser a").attr("href", $("#loginUserName").attr("href"));
    $("#userLogout").show();
}
/* 下拉菜单 LG */
function iDropdown(id) {
    var $dropdown = $('#' + id).find('.dropdown');
    $dropdown.each(function () {
        var $this = $(this);
        var $item = $this.children('div');
        $this.hover(function () {
            $item.stop().fadeIn(100);
        }, function () {
            $item.stop().fadeOut(100);
        });
    });
};
/* 下拉菜单 SM */
function iDropdownMini(id) {
    var $dropdown = $('#' + id).find('.dropdown>span>a');
    $dropdown.each(function () {
        var $this = $(this);
        var $item = $this.parent().next('div');
        $this.click(function (e) {
            e.preventDefault();
            $item.toggle();
        });
    });
};
/* 显示隐藏 */
function showAndHide(id) {
    return $('#' + id).toggle();
};
/* 收缩导航 */
function hideMainNav() {
    $('#mainNavSM, #language').hide();
    $('#btnList').removeClass('active');
    $('#overlay').hide();
};
</script>
