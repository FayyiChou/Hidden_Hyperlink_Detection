	<script>
	    $(function () {
			
	        $.fn.InitFavoriteOrSub({
			    category: 'Periodical',
				hasLogined: 'False' === 'True',
				loginUrl: 'http://login.med.wanfangdata.com.cn/Account/LogOn?ReturnUrl=' + encodeURIComponent(window.location.href)
			});
			
			InitInnerSearch();
		});
		
		function InitInnerSearch() {
			var $searchInput = $('#innerQuery');
			$('#innerSearchForm').submit(function() {
				var query = $.trim($searchInput.val());
				if (!query) {
					$searchInput.focus();
					alert("请输入搜索关键词");
					return false;
				};
				return true;
			});
		};
	</script>
	
    <script>
        $(function () {
            $('div#relatePublication').load('/Periodical/RelatePublication', {
                "meshs": $('#mesh').val(),
                "periodicalId": $('#periodicalId').val()
            });
            //获取期刊列表中有在线阅读的期
            var isChineseMagazine = $('#isChineseMagazine').val();
            if (isChineseMagazine) {
                $.post('/Periodical/GetOnlineIssue', {
                    periodicalId: $('#periodicalId').val()
                }).done(function (jsonResult) {
                    $.each(jsonResult.onLineIssueList, function (name, issue) {
                        $("#" + issue.Id).after('<a title="整刊在线阅读" href="http://f.med.wanfangdata.com.cn/Periodical/Online?id=PeriodicalIssue_' + issue.Id + '" target="_blank"><i class="icon icon-earth"></i></a>');
                    });
                }).error(function (ex) {
                    console.log('获取在线阅读刊期失败', ex);
                });
                }
        });
    </script>


