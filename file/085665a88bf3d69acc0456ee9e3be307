    canvas.width = ClientWidth;*/
    //canvas.height = ClientHeight;
    init_var();
    
    var hammer = new Hammer(canvas);//绑定手势
    hammer.ontap = touchevent;
    var ctx=canvas.getContext("2d");
    var start = 0;
    var end=100;
    //init_var();

function init_var()
{
    if(show_type)
    {
        canvas=document.getElementById("mainCanvas");
        can = document.getElementById("cav");
        ClientWidth = can.offsetWidth;//document.body.offsetWidth;
        ClientHeight =  can.clientHeight;//document.body.offsetHeight;//document.body.clientHeight;
        canvas.width = ClientWidth;
        
        contextinterface = new TimeLineInterface(ctx);
        contextinterface.setDecimalPlace(4);
        contextinterface.setHasVolume(false);
        /*contextinterface.setClientRect(canvas.width,canvas.height);
        contextinterface.setClickProcessFunc(clickProcess);*/
    }
    else
    {
        canvas=document.getElementById("mainCanvas");
        can = document.getElementById("cav");
        ClientWidth = can.offsetWidth;//document.body.offsetWidth;
        ClientHeight =  can.clientHeight;//document.body.offsetHeight;//document.body.clientHeight;
        canvas.width = ClientWidth;
        
        contextinterface = new TrendLineInterface(ctx);
        /*contextinterface.setClickProcessFunc(clickProcess);
        contextinterface.setClientRect(canvas.width,canvas.height);*/
    }
    contextinterface.setClickProcessFunc(clickProcess);
    contextinterface.setClientRect(canvas.width,canvas.height);
    contextinterface.setFonts(10);
}
    
function touchevent(event)
{

    if (event.type=='tap')
    {
        var position = event.position;
        var wrap = document.getElementById("wrap");
        //alert(position[0].x+"\n"+canvas.offsetLeft+"\n"+position[0].y+"\n"+wrap.offsetTop);
        //坐标是针对浏览器的坐标，需要减去canvas相对于浏览器位置的偏移量，否则会出现偏差
        if(show_type)
        {
            contextinterface.setSelectXY(position[0].x - canvas.offsetLeft, position[0].y - wrap.offsetTop);
        }
        else
        {
            
            contextinterface.setSelectXY(position[0].x - canvas.offsetLeft, position[0].y - (wrap.offsetTop+canvas.offsetTop));
        }
        
        
        contextinterface.draw();
    
        
    }
}

function testdata()
{
    getDataFromServer();
}
var index =0;
function clickbt()
{
    init_var();
    index++;
    contextinterface.draw();
    getDataFromServer();
    //	testdata(0.1*(index ));

}
            
    
//前收，需要先获取前收数据设置上

var preclose = 1;
function getDataFromServer()
{
    var xmlhttp;
    if (window.XMLHttpRequest)
    {// code for IE7+, Firefox, Chrome, Opera, Safari
        xmlhttp=new XMLHttpRequest();
    }
    else
    {// code for IE6, IE5
        xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
    }
    xmlhttp.onreadystatechange=function()
    {
        var responsetext = "";
        if (xmlhttp.readyState==4 && xmlhttp.status==200)
        {
            responsetext = xmlhttp.responseText;

            var datalist =  setData(responsetext.trim());

            contextinterface.setData(datalist, 0);
            contextinterface.Status = 1;
            //contextinterface.setFonts(10);
            contextinterface.draw();
        }
    }
    var data = $('#fund_news').attr('data-sort').split(',');
    var code = data[1];
    //month=20 3month=60 year=240
    var obj1 = $('#fund_trend .fund_checked');
    var lineurl = '';
    var type = 'HBX';
    //if(obj1.html() == "净值走势")show_type
    if(show_type == 0)
    {
        lineurl = "http://q.m.hexun.com/ajax/ajax_fund_detail_fundtrend.php?code=" + code+"&c=";
        var obj2 = $('#net2 .checked');
        switch(obj2.attr("data-time"))
        {
            case 'month': lineurl = lineurl+'20'; ;break;
            case '3month': lineurl = lineurl+'60'; break;
            case 'year': lineurl = lineurl+'240'; break;
            default: lineurl = lineurl+"20"; 
        }
    }
    //if(obj1.html() == "估值走势")
    if(show_type == 1)
    {
        lineurl = "http://q.m.hexun.com/ajax/ajax_fund_detail_iopv.php?code=" + code;
    }
    var httpurl = lineurl;
    //alert(httpurl);
    xmlhttp.open("GET",httpurl,true);
    xmlhttp.send();
}
loadXML = function(xmlString){
    xmlString = xmlString.trim();
    var xmlDoc=null;
    //判断浏览器的类型
    //支持IE浏览器 
    if(!window.DOMParser && window.ActiveXObject){   //window.DOMParser 判断是否是非ie浏览器
        var xmlDomVersions = ['MSXML.2.DOMDocument.6.0','MSXML.2.DOMDocument.3.0','Microsoft.XMLDOM'];
        for(var i=0;i<xmlDomVersions.length;i++){
            try{
                 xmlDoc = new ActiveXObject(xmlDomVersions[i]);
                xmlDoc.async = false;
                xmlDoc.loadXML(xmlString); //loadXML方法载入xml字符串
                break;
            }catch(e){
            }
        }
    }
    //支持Mozilla浏览器
    else if(window.DOMParser && document.implementation && document.implementation.createDocument){
        try{
            /* DOMParser 对象解析 XML 文本并返回一个 XML Document 对象。
             * 要使用 DOMParser，使用不带参数的构造函数来实例化它，然后调用其 parseFromString() 方法
             * parseFromString(text, contentType) 参数text:要解析的 XML 标记 参数contentType文本的内容类型
             * 可能是 "text/xml" 、"application/xml" 或 "application/xhtml+xml" 中的一个。注意，不支持 "text/html"。
             */
             
            domParser = new  DOMParser();
            xmlDoc = domParser.parseFromString(xmlString, 'text/xml');
        }catch(e){
            console.log(e);
        }
    }
    else{
        return null;
    }

    return xmlDoc;
}

function setData(content)
{

    var datalist = new Array(0);
    var xmldoc=loadXML(content)

    //var elements = xmldoc.getElementsByTagName("data");
    elements = xmldoc.getElementsByTagName("data");
    if (elements == null || elements.length == 0)
    {
        return null;
    }
    if(show_type == 0)
    {
        arr_data = new Array();
        
        for (var i = 0; i < elements.length; i++) {
        var candle = new TrendLineInfo();
        var elechild = elements[i].getElementsByTagName("enddate")[0].firstChild;
        var datetime = elechild == null? "":elechild.nodeValue;
        elechild = elements[i].getElementsByTagName("unitnetvalue")[0].firstChild;
        var price = elechild == null? "":elechild.nodeValue;
        
        if(i == 0)
        {
            $('#unitnet').html(price);
        }
        elechild = elements[i].getElementsByTagName("netvalue")[0].firstChild;
        var averprice = elechild == null? "":elechild.nodeValue;
        elechild = elements[i].getElementsByTagName("netastrate")[0].firstChild;
        var riserate = elechild == null? "":elechild.nodeValue;
        arr_data[i] = {'enddate':datetime,'unitnetvalue':price,'netvalue':averprice,'netastrate':riserate};
        candle.DateTime = datetime;
        candle.Price = price;
        candle.AvePrice = averprice;
        candle.UpDownRate = riserate;
        //console.log(i + "--" + datetime);
        datalist.unshift(candle);
		}
        //alert("in setData净值走势");
        set_data(elements.length-1);
    }
    //else if($('#fund_trend .fund_checked').html() == "估值走势")
    else if(show_type == 1)
    {
        arr_data = {'Date':'','mindata':''};
        arr_data.mindata = new Array();
        var today = elements[0].getElementsByTagName("Date")[0].firstChild.nodeValue;
        arr_data.Date = today;
        var list = elements[0].getElementsByTagName("mindata");
        var mindata = 0;
        var ioptime = '';
        for (var i = 0; i < list.length; i++) {
            var candle = new TimeLineInfo();
            var elechild = list[i].firstChild.nodeValue;
            arr_data.mindata[i] = elechild;
            var datemin = 570 + i;
            if (i >=120)
            {
                datemin = 570 + i + 90;
            }
            var datetime = parseInt(datemin /60) + ":" + ((datemin%60 < 10) ?  ('0'+datemin%60) : datemin%60);

            var price = elechild == null? "0":elechild;
            var averprice = 0;
            candle.DateTime = datetime;
            candle.Price = price;
            candle.UpDown = price - preclose;
            candle.UpDownRate = candle.UpDown *100 /preclose ;
            console.log(i + "--" + datetime);
            datalist.push(candle);
            mindata = price;
            ioptime = datetime;
        }
        //alert("in setData估值走势");
        set_data(list.length-1);
    }

    return datalist;
}

function getTime(tstr)
{
    var result = "";
    result = tstr.substring(0,2) +":" + tstr.substring(2,4);
    return result;
}

function get_date(tstr)
{
    if(tstr.length > 6)
    {
        var md = tstr.substring(4,tstr.length);
        if(md.length == 4)
        {
            var mmdd = md.substring(0,2) + '-' + md.substring(2,4);
        }
        else if(md.length == 3)
        {
            if(md.substring(0,2) > 12)
            {
                var mmdd = md.substring(0,2) + '-' + md.substring(2);
            }
            else
            {
                var mmdd = md.substring(0) + '-' + md.substring(1,md.length);
            }
        }
        else
        {
            var d = new Date()
            var mmdd = (d.getMonth()+1) + '-' + d.getDate();
        }
    }
    else
    {
        var d = new Date()
        var mmdd = (d.getMonth()+1) + '-' + d.getDate();
    }
    return mmdd;
}

