var elements;
function reload() {
    $obj = $('#fund_trend .fund_checked');
    $obj.siblings().removeClass("fund_checked");
    $obj.addClass("fund_checked");
    show_type = 0;

    if($obj.html() == "净值走势") {
        $('#estimate').css('display','none');
        $('#net1').css('display','block');
        $('#net2').css('display','block');
    }
    if($obj.html() == "估值走势") {
        show_type = 1;
        $('#net1').css('display','none');
        $('#net2').css('display','none');
        $('#estimate').css('display','block');
    }
    
    $('#net2 li .checked').removeClass("checked");
    $('#net2 li .checked').addClass("checked");
    
    $('.click_more').attr('style','display:block');
    var data = $('#fund_news').attr('data-sort').split(',');
    var code= data[1];
    var type = data[0];
    var pn = $('#pn').html();
    var pc = $('#pc').html();
    $('#fund_news').siblings().removeClass("checked");
    $('#fund_news').addClass("checked");
    
    //set_data(elements.length-1);
    
    ajax_fund_detail_news(code,type,pn,pc);
    
    //画图重载
    clickbt();

}
//以下为画图部分
var debugstr="";
function showmsg(msg)
{
    var dis=document.getElementById("dis");
    dis.innerHTML=msg; 
}

function set_data(id)
{
    if(show_type == 0)
    {
        /*var id1 = (elements.length-1) - id;
        var elechild = elements[id1].getElementsByTagName("enddate")[0].firstChild;
        var enddate = elechild == null? "":elechild.nodeValue;
        elechild = elements[id1].getElementsByTagName("unitnetvalue")[0].firstChild;
        var unitnetvalue = elechild == null? "":elechild.nodeValue;
        elechild = elements[id1].getElementsByTagName("netvalue")[0].firstChild;
        var netvalue = elechild == null? "":elechild.nodeValue;
        netvalue = parseFloat(netvalue);
        elechild = elements[id1].getElementsByTagName("netastrate")[0].firstChild;
        var netastrate = elechild == null? "":elechild.nodeValue;*/
        
        var id1 = (arr_data.length-1) - id;
        var elechild = arr_data[id1].enddate;
        enddate = elechild == null? "":elechild;
        var elechild = arr_data[id1].unitnetvalue;
        var unitnetvalue = elechild == null? "":elechild;
        var elechild = arr_data[id1].netvalue;
        var netvalue = elechild == null? "":elechild;
        netvalue = parseFloat(netvalue);
        var elechild = arr_data[id1].netastrate;
        netastrate = elechild == null? "":elechild;
        
        
        
        //alert(parseFloat(netastrate));
        if('HBX' == 'HBX' || 'HBX' == 'CXLCX' || 'HBX' == 'CXX')
        {
            if(netvalue > 0)
            {
                //$('#netvalue').removeClass();
                $('#netvalue').addClass("ft_red");
                //netvalue = Math.ceil(netvalue * 100)/100;
                netvalue = Math.round(netvalue * 100,2)/100;
                $('#netvalue').html('+'+netvalue.toFixed(2) + '%');
            } else if(netvalue < 0)
            {
                //$('#netvalue').removeClass();
                $('#netvalue').addClass("ft_green");
                //netvalue = Math.floor(netvalue * 100)/100;
                netvalue = Math.round(netvalue * 100,2)/100;
                $('#netvalue').html(netvalue.toFixed(2) + '%');
            } else
            {}
        } else
        {
            if(netvalue != 0)
            {
                $('#netvalue').html(netvalue.toFixed(4));
            }
            
            if(parseFloat(netastrate) > 0)
            {
                $('#netastrate').removeClass();
                $('#netastrate').addClass("ft_red");
                $('#netastrate').html('+'+netastrate);
            } else if(parseFloat(netastrate) < 0)
            {
                $('#netastrate').removeClass();
                $('#netastrate').addClass("ft_green");
                $('#netastrate').html(netastrate);
            } else
            {
                $('#netastrate').removeClass();
                netastrate = '0.00';
                $('#netastrate').html(netastrate);
            }
        }

        $('#unitnetvalue').html(unitnetvalue);
        $('#time').html(enddate);
            
    }
    if(show_type == 1)
    {
        /*var today = elements[0].getElementsByTagName("Date")[0].firstChild.nodeValue;
        var list = elements[0].getElementsByTagName("mindata");*/
        
        var today = arr_data.Date;
        var list = arr_data.mindata;
        
        var mindata = 0;
        var ioptime = '';
        /*var elechild = list[id].firstChild.nodeValue;
        alert("id == "+id+"\nlist["+id+"] == " + elechild);*/
        if((list.length-1) < id)
        {
            id = (list.length - 1);
        }
        var elechild = list[id];
        //alert("id == "+id+"\nlist["+id+"] == " + list[id]);
        var datemin = 570 + id;
        if (id >=120)
        {
            datemin = 570 + id + 90;
        }
        var datetime = parseInt(datemin /60) + ":" + ((datemin%60 < 10) ?  ('0'+datemin%60) : datemin%60);
        var price = elechild == null? "0":elechild;
        mindata = price;
        ioptime = datetime;
        $('#iopvalue').html(mindata);
        var unitnetvalue = $('#unitnet').html();
        //alert(unitnetvalue);
        var ioprise = mindata - unitnetvalue;
        if(ioprise > 0) {
            $('#ioprise').removeClass();
            $('#ioprise').addClass("ft_red");
            $('#ioprise').html(ioprise.toFixed(4));
        } else if(ioprise < 0) {
            $('#ioprise').removeClass();
            $('#ioprise').addClass("ft_green");
            $('#ioprise').html(ioprise.toFixed(4));
        } else {
            $('#ioprise').removeClass();
            $('#ioprise').html(ioprise);
        }

        ioprate = (ioprise / unitnetvalue)*10000;
        if(ioprate > 0) {
            ioprate = Math.floor(ioprate)/100;
            $('#ioprate').removeClass();
            if(ioprate == 0) {
                ioprate = '0.00';
            } else {
                $('#ioprate').addClass("ft_red");
                ioprate = '+'+ioprate.toFixed(2) + '%';
            }
        } else if(ioprate < 0) {
            ioprate = Math.ceil(ioprate)/100;
            $('#ioprate').removeClass();
            if(ioprate == 0) {
                ioprate = '0.00';
            } else {
                $('#ioprate').addClass("ft_green");
                ioprate = ioprate.toFixed(2) + '%';
            }
        } else {
            ioprate = '0.00';
            $('#ioprate').removeClass();
        }
        $('#ioprate').html(ioprate);
        var ioptime = get_date(today) + ' ' + datetime;
        $('#ioptime').html(ioptime);
    }
}
 
function dbclck()
{
    
    ClientWidth = dbclckstat++ == 0? ClientWidth /2 :ClientWidth *2;
    dbclckstat = dbclckstat %2;
    canvas.width = ClientWidth;
    //canvas.height = ClientHeight;
    contextinterface.setClientRect(canvas.width,canvas.height);
    contextinterface.draw();
    showmsg("double");
}

function tramsform(event)
{
    //showmsg(event.originalEvent+"|" + event.position+"|" +event.scale+"|" + event.rotation);
    var type;
    for(type in event )
    {
        showmsg(type);
    }
}
