<script type="text/javascript">
   
 
    $(function(){
		$(".seven a").click(function(){
			$(".seven").toggleClass("small");
		});
		setTimeout(function(){
			var banner=$(".seven");
			if(!banner.hasClass("small")){
				banner.addClass("small");
			} 
		},4500);
        $('.nivoSlider').nivoSlider({
            effect:'fade'
        });
    });
 
</script>
	
