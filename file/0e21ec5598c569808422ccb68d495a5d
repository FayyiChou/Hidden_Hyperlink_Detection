<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {
jQuery('#tupian').after("<a id=\"refresh\" href=\"\/member\/account\/captcha?refresh=1\">\u770b\u4e0d\u6e05\uff1f\u6362\u4e00\u5f20<\/a>");
jQuery(document).on('click', '#refresh', function(){
	jQuery.ajax({
		url: "\/member\/account\/captcha?refresh=1",
		dataType: 'json',
		cache: false,
		success: function(data) {
			jQuery('#tupian').attr('src', data['url']);
			jQuery('body').data('captcha.hash', [data['hash1'], data['hash2']]);
		}
	});
	return false;
});

});
/*]]>*/
</script>


<script>
  $(function(){

    var isFocus=$("#login-account").is(":focus");
    if(true==isFocus){
      $('#login-account').addClass('active');
      $('.ren').addClass('active');
    }

    $('#login-account').focus(function(){
      $(this).addClass('active');
      $('.ren').addClass('active');
      $('.main_content_4').removeClass('active');
      $('.mima').removeClass('active');
    })
    $('#login-password').focus(function(){
      $(this).addClass('active');
      $('.mima').addClass('active');
      $('.main_content_3').removeClass('active');
      $('.ren').removeClass('active');
    })
    $('.main_content_3').mouseover(function(){
        $(this).addClass('active');
      $('.ren').addClass('active');
    })

    $('.main_content_3').mouseout(function(){
        var isFocus=$('#login-account').is(":focus");
        if(false==isFocus){
        $(this).removeClass('active');
        $('.ren').removeClass('active');
        }
      })
    $('.main_content_4').mouseover(function(){
      $(this).addClass('active');
      $('.mima').addClass('active');
    })

    $('.main_content_4').mouseout(function(){
      var isFocus=$('#login-password').is(":focus");
      if(false==isFocus){
        $(this).removeClass('active');
        $('.mima').removeClass('active');
      }
    })
  })
</script>



