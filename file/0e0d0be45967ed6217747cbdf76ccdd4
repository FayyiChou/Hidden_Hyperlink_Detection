<script>var online= new Array();</script>
<script src="http://webpresence.qq.com/getonline?Type=1&2937738336:1543918991:2937738336:"></script>
<script>
for (var i=0; i<online.length; i++) {
    if (online[i]) jQuery("#box_kefu .qq a").eq(i).addClass("online");
}

(function(){
    var _open = $("#box_kefu .open"),
        _close = $("#box_kefu .close");

    if ( $.pe.cookie("kefu_status")=="1" ) {
        _open.css("right", 5);
        _close.css("right", -45);
    }
    
    _open.find(".hd a").click(function(){
        _close.removeClass("closed");
        _open.animate({right:10}, 180, function(){
            _open.animate({right:-137}, 180, function(){
                _close.animate({right:-10}, 180);
                $.pe.cookie("kefu_status", 0, {path:"/"});
            });
        });
    });
    _close.click(function(){
        $(this).addClass("closed").animate({right:0}, 180, function(){
            $(this).animate({right:-45}, 180, function(){
                _open.animate({right:5}, 180);
                $.pe.cookie("kefu_status", 1, {path:"/"});
            });
        });
    }).hover(function(){
        $(this).stop(1,1).animate({right:-5}, 180);
    },function(){
        if (!$(this).hasClass("closed")) {
            $(this).stop(1,1).animate({right:-10}, 180);
        }
    });

    var t;
    $(window).scroll(function(){
        clearTimeout(t);
        t = setTimeout(function(){
            $("#box_kefu").stop(1,1).animate({marginTop:$(window).scrollTop()}, 200, "swing");
        }, 50);
    }).scroll()
})()
</script>



