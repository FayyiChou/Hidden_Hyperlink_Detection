var base_url='http://huodong.4399.com/';

function fabu()
{
  //window.scroll(0,50000);
  $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
  $body.animate({scrollTop: $('.pl_t').offset().top}, 10);
  return false;
}
$(document).ready(function(){
	var g_more_game = new Array("http://www.4399.com/special/195.htm","http://www.4399.cn/");
	$("#g_game_select").find("li").each(function(index){
		$(this).click(function(){
			$(this).find("a").addClass("on").parent().siblings().find("a").removeClass("on");
			$("div[id^=g_game_select_]").hide();
			$("#g_game_select_"+index).show();
			$("#g_more_game a").attr("href", g_more_game[index]);
			return false;
		});
	});
	$("#g_game_select_0").show();
	var show_status  = false;
	var show_status2 = false;
	$("#show_all").mouseover(function(){
		$("#mt_select").show();
		show_status = true;
	});
	$("#show_all").mouseout(function(){
		if (!show_status2){
			$("#mt_select").hide();
		}
	});
	$("#mt_select").mouseover(function(){
		$("#mt_select").show();
		show_status  = true;
		show_status2 = true;
	});
	$("#mt_select").mouseout(function(){
		show_status  = false;
		show_status2 = false;
		$("#mt_select").hide();
	});

	$(".dhinfo").find("a").each(function(index){
		$(this).mouseover(function(){
			$(this).find("span").css("display", "block");
		});
		$(this).mouseout(function(){
			$(this).find("span").css("display", "none");
		});
	});
	$(".xz_list").find("a").each(function(index){
		var nav_index = index + 1;
		$(this).click(function(){
			$(".xz_list a").slice(0).attr("class", "");
			$(this).attr("class", "on");
			$(".cbg_5").each(function(index2){
				var game_index = index2 + 1;
				if($(this).attr("id") == "game_"+nav_index)
				{
					$("#game_"+nav_index).css("display", "block");
				}
				else
					$("#game_"+game_index).css("display", "none");
			});
			return false;
		});
	});
	
});
