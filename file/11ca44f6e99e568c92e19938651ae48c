            AddRowEx('txt','txt_i','txt_c');
            function checkJourType(obj)
            {
            var joursourceObj = document.getElementById('@joursource');
            if(joursourceObj)
            joursourceObj.value = obj.value;
            }
            function changeIssue(el)
            {
            var copyel = document.getElementById(el.id.substr(0, el.id.length-1)+"2");
            var issueel = document.getElementById(el.id.substr(0, el.id.length-2));
            if(copyel)
            {
            if(el.tagName == "SELECT")
            {
            copyel.disabled = (el.value.length>0)?true:false;
            copyel.style.backgroundColor = (el.value.length>0)?"#eeeeee":"";
            copyel.value = (el.value.length>0)?"":el.value;
            issueel.value = (el.value.length>0)?el.value:"";
            }
            else
            {
            issueel.value = el.value;
            }
            }
            }
          </script>
