<input type="hidden" id="hid_KLogin_HFUrl" name="hid_KLogin_HFUrl" value="http%3A//kns.cnki.net/KLogin/Request/GetKHF.ashx%3Fcallback%3D%3F">
<script type="text/javascript">
    (function ($) {
        var klogin = function () { };
        //加载头部
        klogin.getHeader = function (uid) {
            var url = unescape($("#hid_KLogin_HFUrl").val());

            var flag = "1";//带首页链接
            if (!uid)
                uid = "";
            var urlTotal = url + "&uid=" + uid + "&flag=" + flag;
            var $boxH = $("#header");
            var $boxF = $("#KLogin_footer");
            if ($boxH.length == 1 && $boxF.length == 1 && $.trim(url) != "") {
                $.getJSON(urlTotal, null, function (json) {
             
                        $boxH.html(unescape(json.headhtml));
                    $boxF.html(unescape(json.foothtml));
                });
            };
        };
        //登出
        klogin.loginOut = function () {
            var url = "/kns/Logout.aspx?q=-1";
            $.post(url, null, null);
            KLogin.getHeader();

        };
        //登陆
        klogin.loginIn = function (uid) {
            var url = "/kns/Loginid.aspx";
            $.post(url, { uid: uid }, function () {

            });
        };
        //登陆成功
        klogin.loginSuccess = function (uid) {
            KLogin.getHeader(uid);
            KLogin.loginIn(uid);
            KLogin.uid = uid;
        };
        window.KLogin = klogin;
    })(jQuery);

    KLogin.getHeader();
</script>



