		<script type="text/javascript">

			/**
			*取得元素的真实css属性 dds[i].style.display=tmpDisplay;
			*written in 06.7 sheneyan
			*/
			function bt(num){
			   for(i=1;i<=2;i++)
			   {
			      document.getElementById("li"+i).className="fonthei";
				  document.getElementById("tra"+i).style.display="none";
			   }
			   document.getElementById("li"+num).className="fonthong";
			   document.getElementById("tra"+num).style.display="block";
			}
			</script>
		
