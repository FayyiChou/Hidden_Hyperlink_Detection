function touclickTransfer(TcLocation, CodeLocation){

if(typeof touclickFire != 'undefined' && touclickFire instanceof Function){      	
touclickFire(TcLocation, CodeLocation);
}else{
touclickToLocal(TcLocation, CodeLocation);
}
}

