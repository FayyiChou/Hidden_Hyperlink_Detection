    <script type="text/javascript">
    if (location.hostname == "esf1.soufun.com") {
        var obj = document.getElementsByTagName("a");
        var myArray = ["esf.fang.com", "zu.fang.com"];
        var reArray = ["esf1.fang.com", "zu1.fang.com"];
        for (var j = 0; j < obj.length; j++) {
            for (var i = 0; i < myArray.length; i++) {
                obj[j].href = obj[j].href.replace(myArray[i], reArray[i]);
            }
        }
    }
</script>
