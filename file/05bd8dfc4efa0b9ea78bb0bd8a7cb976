            <script type="text/javascript">
                function changeShowSQ() {
                    var sqTitle = document.getElementById("sqTitle");
                    var sqInfo = document.getElementById("SQinfo");
                    if (sqTitle.className == 'style10 style11 sqZhiDa') {
                        sqTitle.className = 'style10 sqZhiDa';
                        sqInfo.style.display = 'none';
                    }
                    else {
                        sqTitle.className = 'style10 style11 sqZhiDa';
                        sqInfo.style.display = 'block';
                    }
                }
            </script>
