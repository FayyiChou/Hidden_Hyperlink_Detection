<script type="text/javascript">
    var timeout;

    function hiddenPt(dl) {
        var citychange = document.getElementById(dl.id);
        citychange.style.display = "none";
        timeout = setTimeout("hiddenPt", 100)
    }
    function showPt(dl) {
        var citychange = document.getElementById(dl.id);
        window.clearTimeout(timeout);
        citychange.style.display = "";
    }
    function limouseover(obj) {
        if (obj.className != "open_selected") {
            obj.className = 'open_hover';
        }
    }
    function limouseout(obj) {
        if (obj.attributes["selected"] == "true") {
            obj.className = 'open_selected';
        }
        else {
            if (obj.className != "open_selected") {
                obj.className = 'open';
            }
        }
    }
    function dvmouseover(obj, ulid) {
        document.getElementById(ulid).style.display = '';
        obj.className = "tag_select_hover";
    }
    function dvmouseout(obj, ulid) {
        document.getElementById(ulid).style.display = 'none';
        obj.className = "tag_select";
    }

    var shortdb = 'FS';
    var searchprice = '/house-a0620/c2100-d2100-h34-l3000/';
    var searcharea = '/house-a0620/c250-d280-j2100-k2100-h34-l3000/';
    var u = 'http://img.soufun.com/secondhouse/image/newsearch/';
    function tijiao(url) {

        if (navigator.userAgent.indexOf("MSIE") > 0) {
            var referLink = document.createElement('a');
            referLink.href = url;
            document.body.appendChild(referLink);
            referLink.click();
        } else {
            window.location.replace(url);
        }
    }
</script>
<script type="text/javascript">
  
    function htagclick(element) {
        var cb = document.getElementsByName("cb_htag");
        var cbvalue = element.getAttribute("value") + ',';
        if (element.className.indexOf("on") > -1) {
            element.className = element.className.replace('on', '');
            cbvalue = '';
        }
        for (var i = 0; i < cb.length; i++) {
            if (cb[i].className.indexOf('on') > -1) {
                cbvalue += cb[i].getAttribute("value") + ',';
            }
        }
        if (cbvalue) {
            cbvalue = cbvalue.substring(0, cbvalue.length - 1);
        }
        var htag = '';
        var href = window.location.href.replace("#","");

        if (href.indexOf('t0') > 0) {
            if (cbvalue != '') {
                href = href.replace('t0' + htag, 't0' + cbvalue);
            }
            else {
                href = href.replace('/t0' + htag, '').replace('-t0' + htag, '').replace('-t0' + htag, '');
            }
            LocationHrefReplace(href);
        } else {
            if (cbvalue != '') {
                var arrUrl = href.replace('http://', '').split('/');
                if (arrUrl) {
                    if (arrUrl.length == 4) {
                        if (href.indexOf('-kw') > -1) {
                            href = href.replace('-kw', '-t0' + cbvalue + '-kw');
                        }
                        else if (href.indexOf('/kw') > -1) {
                            href = href.replace('/kw', '/t0' + cbvalue + '-kw');
                        }
                        else {
                            href = href.substring(0, href.length - 1) + '-t0' + cbvalue + '/';
                        }
                    }
                    else if (arrUrl.length == 3) {
                        href = href + 't0' + cbvalue + '/';
                    }
                    else {
                        href = href.replace('.com/', '.com') + '/house/t0' + cbvalue + '/';
                    }
                }
                LocationHrefReplace(href);
            }
        }
    }

    jQuery(".floatingclose").click(function () {
        jQuery(".searchbg").hide();
    })
    jQuery("p[name='cb_htag']").click(function () {
        htagclick(jQuery(this)[0]);
    });

</script>

