<script type="text/javascript" src="http://esf.js.soufunimg.com/esf/esf/online/secondhouse/old/secondhouse/image/esfnew/scripts/jquery/jquery1.4.2.js?v=3.201703302"></script>
<script type="text/javascript">
    function InitSuggestion(inputStr) {
        //To Spilt For Shop And Office
        var newtempPurpose = (tempPurpose == '写字楼' || tempPurpose == '商铺') ? tempPurpose : '住宅,别墅,用户词';
        jQuery.getJSON("/NewSecond/SuggestionForFrontPage.aspx?jsoncallback=?&city=" + escape(city) + '&q=' + escape(inputStr) + '&purpose=' + escape(newtempPurpose),
            function (response) {
                if (response.history || response.hit) {
                    RemoveRows();
                    ShowHintNewHistory(response.history);
                    if (inputStr != '') {
                        ShowHintNew(response.hit);
                        document.getElementById('trhistory').style.display = 'none';
                    }
                    else {
                        document.getElementById('trhistory').style.display = '';
                    }
                    if (suggestion.rows.length > 1) {
                        divsuggestion.style.display = '';
                        index = 1;
                        tag = 1;
                    }
                }
                else {
                    RemoveRows();
                }
            });
    }

    //清空下拉框
    function RemoveRows() {
        for (var i = suggestion.rows.length - 1; i > 0; i--) {
            suggestion.tBodies[0].removeChild(suggestion.rows[i]);
        }

        if (suggestion.rows.length == 1) {
            divsuggestion.style.display = 'none';
        }
    }

    //填充下拉项
    function AddRow(text, dis, com, type) {
        //济南特殊处理区县房源数标记为-1
        if (type > 0 || type == -1) {
            var tr = document.createElement('TR');
            tr.setAttribute('index', suggestion.rows.length);
            tr.onmouseover = function () {

                var i = parseInt(this.getAttribute('index'));

                if (i != index || index == 1) {
                    if (index != -1) suggestion.rows[index].className = 'item';
                    this.className = 'select';
                    index = i;
                }
            }
            var th = document.createElement('th');
            th.setAttribute('key', text);
            th.innerHTML = text + "<span class='gray9'> " + dis + " " + com + "</span>";
            tr.appendChild(th);
            if (type > -1) {
                td = document.createElement('TD');
                td.innerHTML = "约" + type + "条房源";
                tr.appendChild(td);
            }
            suggestion.tBodies[0].appendChild(tr);
        }
    }

    //填充历史搜索下拉项
    function AddHistoryRow(text,count) {
        var tr = document.createElement('TR');
        tr.setAttribute('index', suggestion.rows.length);
        tr.onmouseover = function () {

            var i = parseInt(this.getAttribute('index'));

            if (i != index || index == 1) {
                if (index != -1) suggestion.rows[index].className = 'item';
                this.className = 'select';
                index = i;
            }
        }
        var th = document.createElement('th');
        th.setAttribute('key', text);
        th.innerHTML = "<span class='gray9'> " + text + "</span>";
        tr.appendChild(th);
        if (count != "" && count != "0") {
            td = document.createElement('TD');
            td.innerHTML = "新增"+count + "条自营房源";
        } else {
            td = document.createElement('TD');
            td.innerHTML = "";
        }
        tr.appendChild(td);
        suggestion.tBodies[0].appendChild(tr);
    }

    //显示下拉框
    function ShowHintNew(json) {

        for (var i = 0; i < json.length; i++) {
            AddRow(json[i].projname, json[i].district, json[i].comerce, json[i].esfcount);
        }
    }

    //显示历史搜索下拉框
    function ShowHintNewHistory(json) {
      
        for (var i = 0; i < json.length; i++) {
            if (document.location.href.indexOf("/chengjiao") > -1) {
                json[i].housecount = 0;
            }
            AddHistoryRow(json[i].key,json[i].housecount);
        }

    }

    var city = "佛山";
    var tempPurpose = "住宅";
    var isAgentHomePage = false;
    var defaultInputText = "请输入小区名称、学校名称…";
    if (city == "北京")
    {
        defaultInputText = "请输入小区名称…";
    }

    /*搜索成功后赋值*/
    jQuery(document).ready(function () {

        if (location.href.indexOf("/agenthome") > -1 || location.href.indexOf("/AgentCenterNew.aspx") > -1) {
            isAgentHomePage = true;
            defaultInputText = "请输入经纪人姓名、楼盘、商圈名称…";
        }
        if (location.href.indexOf("/chengjiao") > -1 || location.href.indexOf("/SearchDealDataList.aspx") > -1) {
            defaultInputText = "请输入小区名称";
        }
        try {
            if (k && k != '') {
                jQuery("#input_keyw1").val(k);
            }
            else {
                jQuery("#input_keyw1").val(defaultInputText);
            }
        }
        catch (ex2)
        { }

        if (location.href.indexOf("/villa") > -1 || location.href.indexOf("/chushou/10_") > -1) {
            tempPurpose = "别墅";
        }
        else if (location.href.indexOf("/shop") > -1 || location.href.indexOf("/chushou/6_") > -1) {
            tempPurpose = "商铺";
        }
        else if (location.href.indexOf("/office") > -1 || location.href.indexOf("/chushou/11_") > -1) {
            tempPurpose = "写字楼";
        }
        else if (location.href.indexOf(".com/school") > -1) {
            tempPurpose = "学校";
        }
        else {
            tempPurpose = "住宅";
        }
        jQuery("#strPurpose1").val(tempPurpose);

        //地图切换地址
        if (location.href.indexOf("/house1") > -1) {
            if (jQuery("#newlisttomapurl")) {
                jQuery("#listmapurl").attr("href", jQuery("#newlisttomapurl").val());
            }
            else {
                jQuery("#listmapurl").attr("href", location.href.replace(".com/house1", ".com/map"));
            }
        }
        else if (location.href.indexOf("/houseprom") > -1)
        {
            jQuery("#listmapurl").attr("href", location.href.replace(".com/houseprom", ".com/map"));
            jQuery("#fromhouseprom").val("1");
        }
        else if (location.href.indexOf("/house") > -1) {
            if (jQuery("#newlisttomapurl")) {
                jQuery("#listmapurl").attr("href", jQuery("#newlisttomapurl").val());
            }
            else {
                jQuery("#listmapurl").attr("href", location.href.replace(".com/house", ".com/map"));
            }
        }
        else {
            jQuery("#listmapurl").attr("href", "http://" + location.host + "/map/");
        }
        var floattype = getfirstCookie("searchbox_floattype");
        if (floattype==undefined||floattype=="") {
            floattype = "float";
        }
         
        jQuery(window).scroll(function () {//只有列表
            if (typeof listordetail != "undefined") {
            if (listordetail == "list" && floattype == "float") {
                var i = navigator.userAgent.indexOf("MSIE 6.0");
                if (jQuery(window).scrollTop() < 450 && i <= 0) {
                    jQuery(".searchbg").show();
                }
                if (jQuery(window).scrollTop() > 450 && i <= 0) {
                    jQuery(".fylist").hide();
                    jQuery(".floatingclose").show();
                    jQuery(".floatinglogo").show();
                    jQuery(".fsearch_con").css({ "margin-left": "250px" })
                    jQuery("#searchbox")[0].className = "searchbox-floating";
                    jQuery("#searchbg")[0].className = "searchbg-floating";
                    jQuery(".hot").hide();
                    jQuery(".btn_sf329").css("display", "inline-block");
                } else {
                    jQuery(".fylist").show();
                    jQuery(".floatingclose").hide();
                    jQuery(".floatinglogo").hide();
                    jQuery(".fsearch_con").css({ "margin-left": "0px" })
                    jQuery("#searchbox")[0].className = "searchbox";
                    jQuery("#searchbg")[0].className = "searchbg";
                    jQuery(".hot").show();
                    jQuery(".btn_sf329").hide();
                }
                jQuery(".floatingclose").click(function () {
                    jQuery("#searchbox")[0].className = "searchbox";
                    jQuery("#searchbg")[0].className = "searchbg";
                    jQuery(".floatingclose").hide();
                    jQuery(".btn_sf329").hide();
                    floattype = "fixed";
                    document.cookies.set('searchbox_floattype', "fixed", 1000 * 60 * 60 * 24, '/', window.location.host);                  
                })
            }}
        });
    });

    //下拉框脚本
    var keyword = document.getElementById('input_keyw1');
    var suggestion = document.getElementById('Suggestion');
    var divsuggestion = document.getElementById('div_Suggestion');
    var text = ''; //临时保存文本值, 值变化了才去请求接口
    var index = 0; //提示层被选中的项

    //获取焦点
    keyword.onfocus = function () {
        if (this.value == defaultInputText) {
            this.value = "";
        }

        if (text != this.value || this.value == "") {
            InitSuggestion(this.value);
        }
    }

    //失去焦点
    keyword.onblur = function () {
        if (this.value == "") {
            this.value = defaultInputText;
        }
    }

    document.documentElement.onclick = function (ev) {
        ev = ev || window.event;
        var target = ev.target || ev.srcElement;

        if (divsuggestion.style.display == '' && target != keyword) {
            divsuggestion.style.display = 'none';
        }
    }

    keyword.onkeyup = function (ev) {
        ev = ev || window.event;
        if (__IsInput(ev)) {
            if (suggestion.style.display == '') {
                if (ev.keyCode == 13) {
                    if (index == 1) {
                        if (tag == 0) {
                            if (suggestion.rows.length <= 1) {
                                index = 0;
                            }
                            else {
                                var _key = suggestion.rows[index].cells[0].getAttribute('key');
                                this.value = _key;
                                form_action1();
                                document.getElementById('form_esf').submit();
                            }
                        }
                        else {
                            this.value = keyword.value;
                        }
                    }
                    else if (index > 1) {
                        var _key = suggestion.rows[index].cells[0].getAttribute('key');
                        this.value = _key;
                        form_action1();
                        document.getElementById('form_esf').submit();
                    }
                    suggestion.style.display = 'none';
                    form_action1();
                    document.getElementById('form_esf').submit();
                    return false;
                }
                else if (ev.keyCode == 40) {
                    if (suggestion.rows.length > 1) {
                        if (tag == 1) {
                            tag = 0;
                            suggestion.rows[1].className = 'select';
                        }
                        else {
                            suggestion.rows[index].className = 'item';
                            index++;
                            if (index == suggestion.rows.length) index = 1;
                            suggestion.rows[index].className = 'select';
                        }
                        var _key = suggestion.rows[index].cells[0].getAttribute('key');
                        keyword.value = _key;
                    }
                }
                else if (ev.keyCode == 38) {
                    //up
                    if (suggestion.rows.length > 1) {
                        suggestion.rows[index].className = 'item';
                        index--;
                        if (index < 1) index = suggestion.rows.length - 1;
                        suggestion.rows[index].className = 'select';
                        var _key = suggestion.rows[index].cells[0].getAttribute('key');
                        keyword.value = _key;
                    }
                }
                else if (ev.keyCode == 27) {
                    //esc
                    suggestion.style.display = 'none';
                }
            }
            else {
                if (ev.keyCode == 40) {
                    //downa
                    suggestion.style.display = '';
                }
            }
        }
        else {
            text = this.value;
            InitSuggestion(this.value);
        }
    }

    suggestion.onclick = function (ev) {
        ev = ev || window.event;
        var target = ev.target || ev.srcElement;
        if (target.tagName == "SPAN") {
            target = target.parentNode;
        }
        index = parseInt(target.parentNode.getAttribute('index'));
        if (index > 0) {
            var _key = target.parentNode.cells[0].getAttribute('key');
            keyword.value = _key;

            form_action1();
            document.getElementById('form_esf').submit();
        }
        divsuggestion.style.display = 'none';
    }

    __IsInput = function (ev) {
        return __IsSelect(ev) || __IsMove(ev) || ev.keyCode == 9 || ev.keyCode == 13;
    }
    __IsSelect = function (ev) {
        if (ev.shiftKey || (ev.ctrlKey && ev.shiftKey)) {
            return (ev.keyCode >= 35 && ev.keyCode <= 40);
        }
        else if (ev.ctrlKey && !ev.shiftKey) {
            return ev.keyCode == 65;
        }
        else {
            return false;
        }
    }
    __IsMove = function (ev) {
        return (ev.keyCode >= 35 && ev.keyCode <= 40);
    }

    //更新提交表单
    function form_action1() {
        if (jQuery("#input_keyw1").val() == defaultInputText || jQuery("#input_keyw1").val().replace(/^\s*/, '') == defaultInputText) {
            jQuery("#input_keyw1").val('');
        }
    }

    function form_before_action1() {
        var searchKeyword = document.getElementById("input_keyw1").value;
        if (searchKeyword == defaultInputText || searchKeyword.replace(/^\s*/, '') == defaultInputText) {
            document.getElementById("input_keyw1").value = '';
        }
        if (searchKeyword == '' || searchKeyword == 'undefined' || searchKeyword == defaultInputText || searchKeyword.replace(/^\s*/, '') == defaultInputText) {
            document.getElementById("strKeyword1").value = '';
            document.getElementById("strNameKeyword11").value = '';
        } else {
            document.getElementById("strKeyword1").value = searchKeyword;
            document.getElementById("strNameKeyword11").value = searchKeyword;
        }
    }
    if (typeof (k) != 'undefined') {
        if (k != '' && k != null) {
            document.getElementById("input_keyw1").value = k;
        }
    }
  
  
</script>

    <!--搜索头文件结束-->
    
