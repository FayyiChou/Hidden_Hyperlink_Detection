    <!--近期违规房源公告 start-->
    <script type="text/javascript">
    var isloadblackList = true;
    function GetblackList() {
        try {
            var http = new DefineRequest();
            http.open("Get", "/Agent/Ajax/AgentHome/GetBlackList.aspx?shortindb=FS", true);
            http.onreadystatechange = function () {
                if (http.readyState == 4) {
                    if (http.status == 200) {
                        if (http.responseText != "") {
                            picLength = http.responseText.substring(http.responseText.lastIndexOf('>') + 1, http.responseText.length);
                            document.getElementById("blackList").innerHTML = http.responseText.substring(0, http.responseText.lastIndexOf('>') + 1);
                            isloadblackList = false;
                        }
                        else {
                            document.getElementById("blackList").style.display = "none";
                        }
                    }
                    else {
                        //alert("服务器异常或通信延迟，请稍后重新提交");
                        document.getElementById("blackList").style.display = "none";
                    }
                }
                else {
                    //提示页面正在提交中
                    document.getElementById("blackList").innerHTML = "Loading...";
                }
            };
            http.send(null);
          
        } catch (e) {
        }
    }
    jQuery(window).scroll(function () {
        var fold = jQuery(window).height() + jQuery(window).scrollTop();
        if (fold > jQuery("#blackList").offset().top && isloadblackList) {
            GetblackList();
        }
    });
</script>
    <!--近期违规房源公告 end-->   
    


