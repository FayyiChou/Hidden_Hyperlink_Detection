    function AddAgentEvaluationUB() {
        try {
            var agentEvaluationUBb = 43;
            var agentEvaluationUBp = { };
            agentEvaluationUBp["vwe.agentid"] = jQuery("#AgentID").val();
            switch(jQuery("#hELevel").val()) {
                case "1":
                    agentEvaluationUBp["vwe.agentevaluate"] = EncodeUtf8("差评");
                    break;
                case "2":
                    agentEvaluationUBp["vwe.agentevaluate"] = EncodeUtf8("中评");
                    break;
                case "3":
                    agentEvaluationUBp["vwe.agentevaluate"] = EncodeUtf8("好评");
                    break;
                default:
                    agentEvaluationUBp["vwe.agentevaluate"] = EncodeUtf8("好评");
                    break;
            }
            agentEvaluationUBp["vwe.housevalidity"] =jQuery("#hHouseInfoAccuracy").val();
            agentEvaluationUBp["vwe.servicedegree"] = jQuery("#hServiceAttitude").val();
            agentEvaluationUBp["vwe.businessdegree"] = jQuery("#hProfessional").val();

            _ub.collect(agentEvaluationUBb, agentEvaluationUBp);
        }
        catch(err) {
        }
    }
