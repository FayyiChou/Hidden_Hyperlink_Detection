        function sydcaddCookie(name, value, expires, path, domain) {
            var str = name + "=" + escape(value);
            if (expires != "") {
                var date = new Date();
                date.setTime(date.getTime() + expires * 24 * 3600 * 1000);
                str += ";expires=" + date.toGMTString();
            }
            str+=";path=/";
            if (domain != "") {
                str += ";domain=" + domain;
            }
            document.cookie = str;
        }
