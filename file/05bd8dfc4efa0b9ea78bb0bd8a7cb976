        function GetAgentHouseAD() {
            var xml_httph = new DefineRequest();
            var comarea;
            comarea = '';
            var proj_code;
            proj_code = '';
    if (proj_code != '' || comarea != '') {
        xml_httph.open("Get", "/newsecond/include/Houselist/AgentHouseAD2014.aspx?purpose=%d7%a1%d5%ac&kw=" + proj_code + "&page=1" + "&comarea=" + comarea, true);
        xml_httph.onreadystatechange = function () {
            if (xml_httph.readyState == 4) {
                if (xml_httph.status == 200) {
                    if (jQuery.trim(xml_httph.responseText) != '') {
                        document.getElementById('divAgentHouseAD').innerHTML = xml_httph.responseText;
                        document.getElementById('divAgentHouseAD').style.display = "";
                    }
                }
            }
        };
        xml_httph.send(null);
    }
}
 
        function AgentADClick(houseID, adid, validkey, validkeyid, purpose) {
            var xml_httph = new DefineRequest();
            var urlparams = "&adid=" + adid + "&validkey=" + validkey + "&validkeyid=" + validkeyid + "&houseID=" + houseID + "&purpose=" + purpose;
            xml_httph.open("Get", "/newsecond/include/Hits/Search.aspx?key=&source=1" + urlparams, true);
    xml_httph.send(null);
}
