<script type="text/javascript">
    _ub.city = '佛山';
    var strdistrict = '', strprice = '', stroperastion = '';
    _ub.onload = function () {
        var isload = true;
        var houseload = true;
        jQuery(window).scroll(function () {
            var fold = jQuery(window).height() + jQuery(window).scrollTop();
            
            if (fold > jQuery("#list_B02_03").offset().top && isload) {
                for (var i = 1; i <= 5; i++) {
                    if (i > 1) {
                        _ub.load(0);
                    }
                    getDingTouCondtion(_ub, dingtoupurpose, i);
                }
                jQuery.ajax({
                    url: "/EsfHouse/Ajax/UBNewHouseDingTou2014.aspx?strdistrict=" + strdistrict + "&strprice=" + strprice + "&stroperastion=" + stroperastion + "&num=" + num + "&page=esflb",
                    type: "GET",
                    beforeSend: function (XMLHttpRequest) {
                        isload = false;
                    },
                    success: function (data) {
                        jQuery("#list_B02_03").html(data);
                    },
                    error: function () {
                        isload = false;
                    }
                });
            }
            
           
        });

    }
    //_ub用户行为对象
    //当前页物业类型
    //次数
    function getDingTouCondtion(_ub, purpose, num) {
        switch (purpose) {
            case "住宅":
                if (typeof (_ub["vwe.position"]) != "undefined" && _ub["vwe.position"] != "") {
                    strdistrict += "|" + _ub["vwe.position"];
                }
                if (typeof (_ub["vwe.totalprice"]) != "undefined" && _ub["vwe.totalprice"] != "" && num == 1) {
                    strprice = _ub["vwe.totalprice"];
                }
                if (num == 1) {
                    stroperastion = "r";
                }
                break;
            case "别墅":
                if (typeof (_ub["vwe.position"]) != "undefined" && _ub["vwe.position"] != "") {
                    strdistrict += "|" + _ub["vwe.position"];
                }
                if (typeof (_ub["vwe.totalprice"]) != "undefined" && _ub["vwe.totalprice"] != "" && num == 1) {
                    strprice = _ub["vwe.totalprice"];
                }
                if (num == 1) {
                    stroperastion = "v";
                }
                break;
        }
    }
    _ub.request("v");
</script>
