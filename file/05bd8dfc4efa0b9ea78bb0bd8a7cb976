        function GetIsNewHouse() {

            jQuery.ajax({
                url: "/newsecond/include/Houselist/IsNewHouse2014.aspx?purpose=%d7%a1%d5%ac&district=%ec%f8%b3%c7&comarea=",
            type: "GET",
            beforeSend: function (XMLHttpRequest) {
                isload = false;
            },
            success: function (data) {
                jQuery("#recommend_xfmy").html(data);
                if (document.getElementById('xfmy_count').value >= 5) {
                    document.getElementById('recommend_xfmy').style.display = "";

                }
            },
            error: function () {
                isload = false;
            }
        });

    }
    
