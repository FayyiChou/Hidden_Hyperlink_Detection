<script type="text/javascript">
    function showDbox(index) {
        var obj = document.getElementById("dbox_" + index);
        obj.style.display = '';
    }
    function hiddenDbox(index) {
        var obj = document.getElementById("dbox_" + index);
        obj.style.display = 'none';
    }
    jQuery(".righticon1").hover(
function () { jQuery(".buyfanghover").show(); }, function () { jQuery(".buyfanghover").hide(); }
)
    jQuery(".righticon2").hover(
    function () { jQuery(".kefuhover").show(); }, function () { jQuery(".kefuhover").hide(); }
    )
    jQuery(".righticon3").hover(
    function () { jQuery(".mahover").show(); }, function () { jQuery(".mahover").hide(); }
    )
    jQuery(window).scroll(function () {
         var fold = jQuery(window).height() + jQuery(window).scrollTop();
         var w = document.documentElement.clientHeight; //获取页面可见高度
         if(fold>w)
         {
            jQuery("#floatingup").css("display","");
         }
         else
         {
           jQuery("#floatingup").css("display","none");
         }
        
    })
</script>
