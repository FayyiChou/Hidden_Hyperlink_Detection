                <script type="text/javascript">
                    //hdm
                    var newsID = ' ';
                    function news(id) {
                        var newsObj = document.getElementById('sqInfo' + newsID);
                        if (newsObj) {
                            newsObj.style.display = 'none';
                            document.getElementById('sqType' + newsID).className = ' ';
                        }
                        var obj = document.getElementById('sqInfo' + id);
                        document.getElementById('sqType' + id).className = 'li-on';
                        obj.style.display = '';
                        newsID = id;
                    }
                    news(1);
                    function SQZhiDaClick(obj) {
                        var SQName = jQuery(obj).text();
                        window.location.href = jQuery("#TabView1").find("a:contains('" + SQName + "')")[0].href;
                    }
                </script>
                
