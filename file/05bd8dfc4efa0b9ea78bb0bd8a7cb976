    function LocationHrefBanlk(url) {
        if (navigator.userAgent.indexOf("MSIE") > 0) {
            var referLink = document.createElement('a');
            referLink.href = url;
            referLink.target = '_blank';
            document.body.appendChild(referLink);
            referLink.click();
        } else {
            window.open(url);
        }
    }
</script>
