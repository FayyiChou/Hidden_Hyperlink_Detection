    String.prototype.TrimStr = function () {
        return this.replace(/^\s+/g, "").replace(/\s+$/g, "");
    }
    function getfirstCookie(key) {
        var cookie = document.cookie;
        var cookieArray = cookie.split(';');
        var getvalue = "";
        for (var i = 0; i < cookieArray.length; i++) {

            if (cookieArray[i].TrimStr().substr(0, key.length) == key) {
                getvalue = cookieArray[i].TrimStr().substr(key.length + 1);
                break;
            }
        }
        return getvalue;
    }
    function getsecondnameCookie(sMainName, sSubName) {
        var child = getfirstCookie(sMainName);
        var childs = child.split('&');
        var getvalue = "";
        for (var i = 0; i < childs.length; i++) {
            if (childs[i].TrimStr().substr(0, sSubName.length) == sSubName) {
                getvalue = childs[i].TrimStr().substr(sSubName.length + 1);
                break;
            }
        }
        return getvalue;
    }
    checkLogin();
    function checkLogin() {
        function ClentUser() { };
        ClentUser.prototype = { name: "", valid: "" };
        var objnew = new ClentUser();
        objnew.name = getsecondnameCookie("passport", "username");
        objnew.valid = getsecondnameCookie("passport", "validation");
        var passportcookie = "";
        passportcookie = getfirstCookie("sfut");
        var sellHouse = document.getElementById("sellHouse");
        var isopends = 'True';
        if (passportcookie != "" || objnew.name != "" && objnew.valid != "") {
            if (window.location.href.indexOf('/shop')>-1 && isopends.toUpperCase()=='TRUE')
            {
                sellHouse.href = "/newsecond/DelegateSale/NewInput.aspx?purpose=shop&insert=1";
            }
            else if (window.location.href.indexOf('/office') > -1 && isopends.toUpperCase() == 'TRUE')
            {
                sellHouse.href = "/newsecond/DelegateSale/NewInput.aspx?purpose=office&insert=1";
            }
            else {
                sellHouse.href = "/newsecond/DelegateSale/HouseInput.aspx";
            }
        }
        else {
            if (window.location.href.indexOf('/shop') > -1 && isopends.toUpperCase() == 'TRUE') {
                sellHouse.href = "/newsecond/NonameSale/Newcheckinhouse.aspx?purpose=shop&insert=1";
