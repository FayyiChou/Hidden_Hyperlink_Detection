<script type="text/javascript" src="http://gzjjwx.gzjd.gov.cn/WechatAPIServer/assets/javascript/jquery-2.1.1.min.js"></script>
<script type="text/javascript" src="http://gzjjwx.gzjd.gov.cn/WechatAPIServer/assets/js/commonModal.js"></script>
<script type="text/javascript">
	var base_url = "http://gzjjwx.gzjd.gov.cn/WechatAPIServer/";
	var userid = "fbf79b2fb5764382afa913bc2a3d4a9c";
	var openid = "obp-tjmywZdvG-5Lxb2bHgVGLqCA";
	var sfzmhm = "440102195901264058";
	$(function(){
		$("#unbind").click(function(){
			var obj = {"type":1,msg:"是否解绑该驾驶证","confirmText":"确定","cancelText":"取消","confirmFun":unbind};
    		showAlert(obj);
		});

		$("#kfxq").click(function(){
			window.location.href = base_url+"member/licenseKfxq/"+sfzmhm;
		});
	});
	function unbind(){
		var url = base_url+"member/unBindLicense";
        var obj = {"userid":userid,"sfzmhm":sfzmhm};
        $.post(url,obj,function(data){
            data = JSON.parse(data);
            if(data.status == 'OK'){
                var obj = {"type":0,msg:"解绑成功","confirmFun":returnFun};
    			showAlert(obj);
            }else{
                var obj = {"type":0,msg:data.msg};
    			showAlert(obj);
            }
        });
	}
	function returnFun(){
		window.location.href = base_url+"member/myLicense2/"+openid;
	}
</script>

