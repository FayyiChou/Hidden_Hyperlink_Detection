$(function(){
$("#changeSize #big").click(function(e) {
    $(".content").css('font-size','22px');
    $(".content").css('line-height','32px');
	$("#changeSize a").removeClass('over');
	$(this).addClass('over');
}); 
$("#changeSize #mid").click(function(e) {
    $(".content").css('font-size','16px');
    $(".content").css('line-height','28px');
	$("#changeSize a").removeClass('over');
	$(this).addClass('over');
}); 
