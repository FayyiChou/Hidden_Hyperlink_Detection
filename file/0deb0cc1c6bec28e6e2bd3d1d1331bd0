<script>
//鼠标移入，在线时间显示，否则消失
document.getElementById('doyoo_panel').onmouseover=(function()
{
	document.getElementById('online_tel').style.top=404+"px";
	document.getElementById('online_xuzhan').style.top=467+"px";
	// document.getElementById('bot_baidu').style.top=479+"px";
	document.getElementById('worktip').style.display="block";
})
document.getElementById('doyoo_panel').onmouseout=(function()
{
	document.getElementById('online_tel').style.top=323+"px";
	document.getElementById('online_xuzhan').style.top=386+"px";
	// document.getElementById('bot_baidu').style.top=398+"px";
	document.getElementById('worktip').style.display="none";
})
</script>
