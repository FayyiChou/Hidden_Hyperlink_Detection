<script>
function nocustomer()
{
	var doyoo_panel=document.getElementById('doyoo_panel');
	doyoo_panel.parentNode.removeChild(doyoo_panel);//删除图片
	var customer=document.getElementById('customer');
	customer.style.display='none';//隐藏百度分享等
}

var _hmt = _hmt || [];

(function() {

  var hm = document.createElement("script");

  hm.src = "//hm.baidu.com/hm.js?96b5ff2a07925cd545262ad9de67bb0a";

  var s = document.getElementsByTagName("script")[0]; 

  s.parentNode.insertBefore(hm, s);

})();



</script> 
<div class="top" style="position:fixed;bottom: 50px;right:0px;width:60px;height:60px;cursor:pointer;display:none; background-image:url(images/back_top.png)" />
<script type="text/javascript">
    //返回顶部开始
    $(window).scroll(function()
    {
        
        if($(window).scrollTop()>100)
        {
            $('.top').css({"display":"block"});
        }
        else
        {
            $('.top').css({"display":"none"});   
        }
    });
    $('.top').click(function()
    {
        timer=setInterval(function()
        {
			
           	document.documentElement.scrollTop=document.documentElement.scrollTop-10;
            if(document.documentElement.scrollTop==0)
            {
                clearInterval(timer);
            }
        },5);
                            
    });
    //返回顶部结束
</script>
</body>
