<script type="text/javascript">
	$("#but_up").click(function () {
		li_shang_xia("but_up");
	})

	jQuery(".fullSlide").slide({
		titCell: ".hd ul",
		mainCell: ".bd ul",
		effect: "fold",
		delayTime:1000,
		interTime:4000,
		mouseOverStop:true,
		autoPlay: true,
		autoPage: true,
		trigger: "click",
		startFun: function(i) {
			var curLi = jQuery(".fullSlide .bd li").eq(i);
			if ( !! curLi.attr("_src")) {
				curLi.css("background-image", curLi.attr("_src")).removeAttr("_src");
			}
		}
	});
	//上一个下一个按钮，不要的话注释掉中间的即可 begin
	$(".fullSlide").hover(function(){
		$(".prev,.next").fadeTo("show",0.7);
	},function(){
		$(".prev,.next").hide();
	})
	//鼠标移过某个按钮 高亮显示
	$(".prev,.next").hover(function(){
		$(this).fadeTo("show",1);$(this).siblings("span").hide();
	},function(){
		$(this).fadeTo("show",0.5);$(this).siblings("span").fadeTo("show",0.4);
	})
	//上一个下一个按钮，不要的话注释掉中间的即可 end
	$(function(){
		doResize();
	});
	$(window).resize(function(){
		doResize();
	});
	doResize();
	function doResize(){
//        var w=$(window).width();//因为ipad不能取完整的，11-11
		var w=$(document.body).width();
		$(".bd").css("width",w+"px");
		if (w < 1920){
			$(".bd ul").css({"left":(w-1920)/2+"px"});
		}else{
			$(".bd ul").css({"left":0+"px"});
		}
	}
</script>



