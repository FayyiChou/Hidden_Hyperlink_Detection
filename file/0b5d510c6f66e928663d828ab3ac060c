<script type="text/javascript">
	function AutoScroll(obj){
		$(obj).find("ul:first").animate({
			marginTop:"-20px"
		},1000,function(){
			$(this).css({marginTop:"0px"}).find("li:first").appendTo(this);
		});
	}
	$(document).ready(function(){
		var timer;
		function run(){timer=setInterval('AutoScroll("#scrollDiv")',2000);}
		run();
		$("#scrollDiv").hover(function(){clearInterval(timer)});
		$("#scrollDiv").mouseleave(function(){run();});
	});

</script>

