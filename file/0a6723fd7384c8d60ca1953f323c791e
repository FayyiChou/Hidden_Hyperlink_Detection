                    type: 'GET',
                    dataType: 'jsonp',
                    success: function (data) {
                        if(data.checked == 0 || data.checked == 1)
                        {
                            jQuery(".mast_box").show();
                            jQuery(".pop_box").eq(0).show();
                        }
                        else if(data.checked == 2 || data.checked == 3)
                        {
                            jQuery(".mast_box").show();
                            jQuery(".pop_box").eq(1).show();
                        }
                    }
                })
            }
            else
            {
                location.href = "https://account.youku.com/login.htm?callback=//c.youku.com/nbfilm";
            }
        });
        jQuery(".close").on('click',function(){
            jQuery(".mast_box").hide();
            jQuery(".pop_box").hide();
        });
        jQuery(".qrz_btn").on('click',function(){
            var _this = jQuery(this);
            jQuery.ajax({
                url: 'http://rz.youku.com/ajax/setWhiteList?from=nbfilm',
                type: 'GET',
                dataType: 'jsonp',
                success: function (data) {
                    if(data.succ == 1)
                    {
                      _this.attr('href', 'http://rz.youku.com/yc/apply');
                    }
                }
            })
        });
        jQuery(".xyb_btn").on('click',function(){
            jQuery(".mast_box").hide();
            jQuery(".pop_box").hide();
        })
    })
</script>
</div>



</div>


