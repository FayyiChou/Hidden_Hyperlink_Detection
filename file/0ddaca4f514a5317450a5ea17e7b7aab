        <script type="text/javascript">
            (function(doc, win) {
                var docEl = doc.documentElement,
                        resizeEvt = 'orientationchange' in window ? 'orientationchange' : 'resize';
                var recalc = function() {
                    var width = docEl.clientWidth;
                    if (width < 360) {
                        width = 360;
                    }
                    if(width>414){
                        width=414;
                    }
                    docEl.dataset.width = width;
                    docEl.dataset.percent = 100 * (width / 360);
                    docEl.style.fontSize = 100 * (width / 360) + 'px';
                };
                recalc();
                if (!doc.addEventListener) return;
                win.addEventListener(resizeEvt, recalc, false);
            })(document, window);
            function imageerror(){
                document.querySelector('.focus').innerHTML = '<div class="focus-error"></div>';
            }
        </script>
