<script type="text/javascript" language="javascript">
    function InitUserInfo(ot,s,t,n,uid,param) {
       try{
           var hkUrl = '//www.ctrip.com.fr/main/LoginAgent/RedirectToHKStie?param='+param+'&callback=?';
           j.jsonp({
               dataType: "jsonp",
               url: hkUrl,
               jsonp: "callback",
               success: function (data) {
                   if(data.success == true){__SSO_loginShow(ot,s,t,n,uid);}
               },
               error: function () {
                   __SSO_loginShow(ot,s,t,n,uid);
               }
           });
       }catch(err){ 
           __SSO_loginShow(ot,s,t,n,uid);
       }
    }
</script>  
