        //Thirdparty登录弹窗
        var nwin;
        function openwindow(url,sTitle,sSize)
        {
            if (url==''){
                return false;
            }

            if (nwin && !nwin.closed){
                nwin.close();
            }


            if (sSize == undefined) {
                alert("请指定窗口型号!");
                return false;
            }
            else if (sSize == 100) {
                sWidth  = screen.availWidth;
                sHeight = screen.availHeight;
            }

            else if (sSize == 1) {
                sWidth  = 280;
                sHeight = 120;
            }
            else if (sSize == 2) {
                sWidth  = 600;
                sHeight = 500;
            }

            var l = ( screen.availWidth - sWidth ) / 2;
            var  t = ( screen.availHeight - sHeight ) / 2;  

            

            nwin = window.open(url,sTitle,'left=' + l +',top=' + t +',width='+sWidth+',height='+sHeight+',scrollbars=yes,resizable=yes');
            nwin.focus();

        }

        var overLayerShow=function(){
            j('.overlay-bg-light').css('display','block');
        };

        var overLayerClose=function(){
            j('.overlay-bg-light').css('display','none');
        };

        var signinFailureShow=function(){
            j('.pack-pop-ico').css('display','block');
