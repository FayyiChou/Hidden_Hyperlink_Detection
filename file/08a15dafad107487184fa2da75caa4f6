                    __SITE_CONFIG__.initFunc = {
                            LANGUAGE_PARAM_NAME: 'language',
                            LANGUAGE_COOKIE_NAME: 'ibulanguage',
                            DEFAULT_SITE_CODE: 'EN',
                            CURRENCY_PARAM_NAME: 'curr',
                            CURRENCY_COOKIE_NAME: 'cookiePricesDisplayed',
                            getCookie: function (name) {
                                var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)")),
                                    value = null;
                                if (arr != null)
                                    value = arr[2] && decodeURIComponent(arr[2]);
                                return value;
                            },
                            setCookie: function (name, value, expires, path, domain) {
                                expires = new Date(new Date().getTime() + (((typeof expires == "undefined") ? 12 * 7200 : expires)) * 1000);
                                var tempcookie = name + "=" + encodeURIComponent(value) +
                                    ((expires) ? "; expires=" + expires.toGMTString() : "") +
                                    ((path) ? "; path=" + path : "; path=/") +
                                    ((domain) ? "; domain=" + domain : "");
                                (tempcookie.length < 4096) ? document.cookie = tempcookie : function () {
                                    throw new Error("The cookie is bigger than cookie lagrest");
                                }();
                                return !!this.getCookie(name);
                            },
                            getParam: function (name) {
                                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i"),
                                    arr = location.search.substr(1).match(reg),
                                    r = arr && arr[2],
                                    value = r ? decodeURIComponent(r) : null;
                                return value;
                            },
                            getLangByParam: function () {
                                var lang = this.getParam(this.LANGUAGE_PARAM_NAME) && this.getParam(this.LANGUAGE_PARAM_NAME).toUpperCase();
                                if (lang && __SITE_CONFIG__.cookiedomain[lang]) {
                                    return lang;
                                }
                            },
                            getCurrByParam: function () {
                                var curr = this.getParam(this.CURRENCY_PARAM_NAME) && this.getParam(this.CURRENCY_PARAM_NAME).toUpperCase();
                                if (curr && this.checkHasCurr(curr)) {
                                    return curr;
                                }
                            },
                            checkHasCurr: function (value) {
                                //todo 默认都能通过白名单
                                if (!__SITE_CONFIG__.allcurrency) {
                                    return true;
                                }
                                for (var i = 0, len = __SITE_CONFIG__.allcurrency.length; i < len; i++) {
                                    if (__SITE_CONFIG__.allcurrency[i] === value) {
                                        return true;
                                    }
                                }
                            },
                            getLangByCookie: function () {
                                 var lang = this.getCookie(this.LANGUAGE_COOKIE_NAME) && this.getCookie(this.LANGUAGE_COOKIE_NAME).toUpperCase();
                                if (lang && __SITE_CONFIG__.cookiedomain[lang]) {
                                    return lang;
                                }
                            },
                            setLanguageCookie: function (languageCode) {
                                this.setCookie(this.LANGUAGE_COOKIE_NAME, languageCode, 2592000, '/', this.getCookieDomainBySiteCode(this.getPageSiteCode()));
                            },
                            setCurrencyCookie: function (currencyCode) {
                                this.setCookie(this.CURRENCY_COOKIE_NAME, currencyCode, 2592000, '/', this.getCookieDomainBySiteCode(this.getPageSiteCode()));
                            },
                            getPageSiteCode: function () {
                                return (__SITE_CONFIG__.page && __SITE_CONFIG__.page.site) || this.DEFAULT_SITE_CODE;
                            },
                            getCookieDomainBySiteCode: function (siteCode) {
                                return __SITE_CONFIG__.cookiedomain[siteCode];
                            },
                            updateCookies: function () {
                                var lang = this.getLangByParam(),
                                    cLang = this.getLangByCookie(),
                                    curr = this.getCurrByParam();
                                if (lang && (lang !== cLang)) {
                                    this.setLanguageCookie(lang);
                                } else {
                                    this.setLanguageCookie(__SITE_CONFIG__.page.language);
                                }
                                if (curr) {
                                    this.setCurrencyCookie(curr);
                                }
                            }};

                __SITE_CONFIG__.initFunc.updateCookies();
                __SITE_CONFIG__.calendar={"Months":"janvier_février_mars_avril_mai_juin_juillet_août_septembre_octobre_novembre_décembre","ShortMonths":"janv._févr._mars_avr._mai_juin_juil._août_sept._oct._nov._déc.","Weeks":"dimanche_lundi_mardi_mercredi_jeudi_vendredi_samedi","ShortWeeks":"dim._lun._mar._mer._jeu._ven._sam.","Holidays":[],"Language":4,"LangText":"FR","Site":4,"SiteText":"FR"}
} catch (e) {throw new Error('site config error');}
 </script>
