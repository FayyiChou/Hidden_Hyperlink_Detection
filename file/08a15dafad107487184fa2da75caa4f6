        //登录回调函数
        var signinCallback=function(json){

            nwin.close();
            
            if(json.result=='3'){//登录成功

                try {
                    var behavior = json.isnew == 1 ? 'signup' : 'signin';

                    if (popSigninPromptShow == 0&&json.isnew == 1) {
                        IBU.page.ubt.log('100550', 'type='+ encodeURIComponent(json.type) + '&behavior=callback_signup&module=common&result=success');
                    }
                    else if (popSigninPromptShow == 0) {
                        IBU.page.ubt.log('100550', 'type='+ encodeURIComponent(json.type) + '&behavior=callback_signin&module=common&result=success');
                    }


                    if (popSigninPromptShow == 1 && signinAndBindType == 0) {
                        behavior = 'signup_via_bind';
                    } 

                    IBU.page.ubt.log('100550', 'type='+ encodeURIComponent(json.type) + '&behavior=' + encodeURIComponent(behavior) + '&module=common&result=success');
                }
                catch(ex){

                }

                signClose(json.data);

            }else if(json.result=='2'){//弹出登录并绑定或直接登录窗

                IBU.page.ubt.log('100550', 'type='+ encodeURIComponent(json.type) + '&behavior=callback_signup&module=common&result=success');

                popSigninPromptShow = 1;
                 
                IBU.page.ubt.log('100550', 'type='+ encodeURIComponent(json.type) + '&behavior=bind&module=common&result=success');

                j('#data').val(json.data);
                j('#loginName').val(json.email);
                j('#loginName').attr('disabled','disabled');
                overLayerShow();
                signinPromptShow();

            }else{
                if (popSigninPromptShow == 0) { 
                    IBU.page.ubt.log('100550', 'type='+ encodeURIComponent(json.type) + '&behavior=callback_signin&module=common&result=success');
                }

                //登录失败
               
                IBU.page.ubt.log('100550', 'type='+ encodeURIComponent(json.type) + '&behavior=signin&module=common&result=failure');

                overLayerShow();
                signinFailureShow();

            }
        };
        //点击绑定登录或直接登录
        var signinAndBind=function(loginType){
            var data=j('#data').val();
            var password=j('#password').val();
            if(checkSigninAndBind(password,loginType)){
                signinAndBindType = loginType;

                var opts="&loginType="+loginType+"&data="+data+"&password="+password;//+"&f=f"
                window.location.href = __SITE_CONFIG__.accountdomain + "ThirdpartySignin/FacebookSigninBind?ts=" + new Date().getTime()+opts;
            }
        };
        //登录跳转
        var signClose=function(data){
            var opts="&data="+data;
            var url = __SITE_CONFIG__.accountdomain + "ThirdpartySignin/FacebookSigninClose?ts=" + new Date().getTime()+opts;
            window.location.href=url;
        };
        //检查Email绑定登录或直接登录
        var checkSigninAndBind=function(password,loginType){
            if(loginType==0 &&(password==''||password==null||password=='undefined')){
                j('#bindErrorMsg').html('Veuillez renseigner votre mot de passe.');
                return false;
            }
            j('#bindErrorMsg').html();
            return true;
        };

    //错误返回

        function changePartner() {
            changeUrl();
        }

        function blurPartnerAccount()
        {
            var card = j("#selectpartner");
            var account = j("#txtAccount");

            var accountVal = account.val();
            var cardType = card.find("option:selected")[0].attributes["value"].value;
            if(cardType == "103" && accountVal.length == 8)//深圳航空8位卡补0
            {
                accountVal = "0" + accountVal;
            }

            var username = card.find("option:selected")[0].attributes["prefix"].value + accountVal;
            j("#txtUserName").val(username);

            ShowCaptcha(checkurl,username);
        }

        //验证合作卡号后登录
        function partnerLogin()
        {
            var account = j("#txtAccount");

            if (account.val() == "") {
                alert("Veuillez fournir le bon compte.");
                return false;
            }
            blurPartnerAccount();

            return SigninCheck('/global/fr/Check', 4);
        }

        function ChangeSignType(loginType)
        {
            var requestType = "POST";
            var url = "/global/fr/Signin/2";

            if(window.location.href.split('?').length == 2){
                url += "?" + window.location.href.split('?')[1];
            }

            if(requestType=="GET")
            {
                window.location.href = url;
            }
            else
            {
                if (document.forms.length == 0) {
                    var objForm = document.createElement("form");
                    if (objForm != null) {
                        document.body.appendChild(objForm);
                    }
                }
                var backURL = document.createElement("input");
                if (backURL != null) {
                    backURL.type = "hidden";
                    backURL.name = "BackURL";
                    backURL.value = "http://fr.ctrip.com/hotels/vieworder?orderid&amp;writeback=true";
                    document.forms[0].appendChild(backURL);
                }

                var sessionidHidden = document.createElement("input");
                if (sessionidHidden != null) {
                    sessionidHidden.type = "hidden";
                    sessionidHidden.name = "sessionid";
                    sessionidHidden.value = "gldxfsn2axvbpje0wlfmqbir330141";
                    document.forms[0].appendChild(sessionidHidden);
                }

                var requestHidden = document.createElement("input");
                if (requestHidden != null) {
                    requestHidden.type = "hidden";
                    requestHidden.name = "responsemethod";
                    requestHidden.value = requestType;
                    document.forms[0].appendChild(requestHidden);
                }
                //submit to login page
                document.forms[0].target = "_top";
                document.forms[0].action = url;
                document.forms[0].method = "POST";

                document.forms[0].onsubmit = null;
                document.forms[0].submit();
            }
        }

        var type = "1";
        function forgetPwd()
        {
            var uid = "";
            if(type == "2")
            {
                var card = j("#selectpartner");
                var account = j("#txtAccount");

                if (account.val() != "") {
                    uid = card.find("option:selected")[0].attributes["prefix"].value + account.val();
                }
            }
            else
            {
                uid = j("#txtUserName").val();
            }
            j("#uid").val(uid);

//            var url = "/global/fr/ResetPassword"; // old
            var url = "/global/fr/PasswordReset";

            document.forms[0].method = "POST";
            document.forms[0].target = "_blank";
            document.forms[0].action = url;
            document.forms[0].submit();
        }

        function RegeistPage()
        {
            var type = j("#selectpartner").val();
            var url = "/global/fr/Register/1";
            if(type!=""&& type!= null)
            {
                url +="?partnerType="+type;
            }
            window.location=url;
        }
        keyCollector && keyCollector.define('input.UserName', 'input.Password');
    </script>

    
</form>
    

