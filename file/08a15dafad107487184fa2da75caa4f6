    var globalSiteCommon = function () {
        var domreadyEvents = [];
        return {
            onReady: function (e) {
                domreadyEvents.push(e);
            },
            ready: function () {
                for (var i = 0; i < domreadyEvents.length; i++) {
                    domreadyEvents[i]();
                }
            }
        };
