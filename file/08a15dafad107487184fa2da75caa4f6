        <script type="text/javascript" language="javascript">
            ;(function ($) {
                function updateLayer($dom, marginLeft, top) {
                    $dom.css({
                        'marginLeft': -marginLeft,
                        'top': top,
                        'left': '50%'
                    });
                }
                $.fn.scrollCenter = function (bindscroll, options) {

                    if (!this.data('__SCROLL_INIT')) {
                        var $win = $(window);
                        this.data('__SCROLL_INIT', true);
                        // 扩展参数
                        var op = $.extend({
                            z: 1000000
                        }, options);

                        this.appendTo(document.body).css({
                            'position': 'absolute',
                            'z-index': op.z
                        });
                        updateLayer(this, this.width() / 2,  ($win.height() - this.height()) / 2 + $win.scrollTop());
                        var cDom = this;
                        // 滚动事件
                        $win.bind('scroll', function (e) {
                
                            updateLayer(cDom, cDom.width() / 2, ($win.height() - cDom.height()) / 2 + $win.scrollTop());

                        }).bind("resize", function () {
                            updateLayer(cDom, cDom.width() / 2, ($win.height() - cDom.height()) / 2 + $win.scrollTop());
                        
                        });

                    }

                    return this;

                };

            })(jQuery);

            var language = 'fr';

            var j = jQuery.noConflict();

            var $$ = j;

            $$.module = {};

            j.module = {};

            j.module.Language = 'fr';

            j.module.CurrentSiteName = 'home';

            j.module.SiteDomain = 'https://accounts.ctrip.com/global/fr/';

            var CTRIP_RES_HOST = 'www.webresource.english.c-ctrip.com/ResGlobalOnline/R8/EnglishSite/';

            var CTRIP_MOD_PATH = 'js/v2/';

            var englishHostName = 'accounts.ctrip.com';

            if (j.module.Language == "hk") {
                englishHostName = 'accounts.ctrip.com';
            }

            var initModule = {
                Language: 'fr',
                MultiLangHostName: 'accounts.ctrip.com',
                EnglishHostName: englishHostName,
                Index: 0
            };

            var ResourceUrl = {
            };

