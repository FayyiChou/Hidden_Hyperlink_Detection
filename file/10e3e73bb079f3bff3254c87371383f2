<script type="text/javascript">
    var SearchID = 5;
    var SearchURL = new Array();
    SearchURL[4] = '/company/';
    SearchURL[5] = '/sell/';
    SearchURL[6] = '/buy/';
    SearchURL[7] = '/quote/';
    SearchURL[21] = '/news/';
    function setDsearch(ID) {
        if (ID == SearchID) return;
        $('ms' + ID).className = 'sch_on';
        $('ms' + SearchID).className = 'sch_li';
        $('destoon_search').action = SearchURL[ID] + 'search.php';
        SearchID = ID;
    }
    function Dsearch() {
        if ($('destoon_kw').value == '' || $('destoon_kw').value == '请输入关键字') {
            alert('请输入关键字');
            $('destoon_kw').focus();
            return false;
        }
    }
 
</script></div>
