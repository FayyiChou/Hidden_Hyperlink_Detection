    var i = Math.floor(Math.random() * baseDomains.length);
    //根据随机数，取数组中的元素
    var randomDomain = baseDomains[i];
    //parseInt中的，是取从Fri Jan 01 2016 00:00:00 GMT+0800 (CST)到目前的小时数
    var xzq_URL = 'http://' + parseInt((Date.parse(new Date()) / 1000 - 1451577600) / 3600) + '.' + randomDomain + '/down/' + xzq_softname + '@' + xzq_channelID + '_' + xzq_softID + '.exe';
