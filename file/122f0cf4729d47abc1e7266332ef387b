 <script type="text/javascript" tt-ignored-node="1">
 function close_list(){
  document.getElementById("zxslistu").style.display="none";
 }

 function zxSelectSou(event){
 var ul=document.getElementById("zxslistu");
 var name;

		if (ul.style.display=="none")
		{
			ul.style.display="";

		}
		else{

		name=event.srcElement.lastChild.data;
  var a=document.getElementById("zxslista");
   a.innerText=name;
	ul.style.display="none"
	}
	}
  </script>

