		<form id="contactForm">
          <div class="form-group">
            <input type="text" class="form-control input-name" name="InputName" id="InputName" placeholder="Ваше имя">
          </div>
          <div class="form-group">
            <input type="tel" class="form-control input-tel" name="InputTel" id="InputTel" placeholder="Ваш телефон">
          </div>
          <div id="success"> </div> <!-- For success/fail messages -->
          <!--<input type="submit" class="btn btn-shopping-cart" id="submit_modal" placeholder="Заказать звонок">-->       
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-theme-default" data-dismiss="modal">Закрыть окно</button>
        <button type="submit_modal" class="btn btn-shopping-cart" id="submit_modal">Заказать звонок</button>
      </div>         
    </div>
  </div>
</div>
<!-- callback end-->

<script type="text/javascript">
$(document).ready(function(){
	$("#submit_modal").click(function() {
    //$("#contactForm").submit(function() { //устанавливаем событие отправки для формы с id=contactForm
            //var form_data = $(this).serialize(); //собераем все данные из формы
	        var name = $(".input-name").val();  
	        var tel = $(".input-tel").val();

            $.ajax({
	            type: "POST", //Метод отправки
	            url: "/catalog/view/theme/pav_styleshop/template/common/send.php", //путь до php файла отправителя
	            //data: form_data,
            	data: {name: name, tel: tel},
	            success: function() {
                   	//код в этом блоке выполняется при успешной отправке сообщения
                   	$('#success').html("<div class='alert alert-success' style='margin-bottom: 15px;'>");
            	   	$('#success > .alert-success').html("<button type='button' class='close' data-dismiss='alert' aria-hidden='true' style='font-size: 15px;font-family: Geneva, Arial, Helvetica, sans-serif;'>&times;")
 	           		.append( "</button>");
             	    $('#success > .alert-success').append("<strong>Ваш запрос отправлен. Мы перезвоним вам в ближайшее время! </strong>");
 		            $('#success > .alert-success').append('</div>');
 						    
           		    //clear all fields
           		    $('#contactForm').trigger("reset");
	            },
           	  	error: function() {		
	           		// Fail message
	             	$('#success').html("<div class='alert alert-danger'>");
	                $('#success > .alert-danger').html("<button type='button' class='close' data-dismiss='alert' aria-hidden='true' style='font-size: 15px;font-family: Geneva, Arial, Helvetica, sans-serif;'>&times;").append( "</button>");
	                $('#success > .alert-danger').append("<strong>Кажется у нас что-то поломалось. Пожалуйста, напишите нам по почте nashi@umnenkie-deti.ru и мы вам сразу же ответим. Спасибо!</strong>");
	             	$('#success > .alert-danger').append('</div>');
             		//clear all fields
             		$('#contactForm').trigger("reset");
             	},   
            });
    });
});    
</script>

