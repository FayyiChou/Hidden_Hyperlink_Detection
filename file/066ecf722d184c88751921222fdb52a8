<script>
  $(document).ready(function(){
	$("#subscribe_button").click(function() {
	        var email = $(".input-email").val();  

            $.ajax({
	            type: "POST", //Метод отправки
	            url: "/catalog/view/theme/pav_styleshop/template/common/subscribe.php", //путь до php файла отправителя
	            //data: form_data,
            	data: {email: email},
	            success: function() {
                   	//код в этом блоке выполняется при успешной отправке сообщения
                   	$('.successfully').html("<div class='successfully-subscribed' style='margin-bottom: 15px;'>");
            	   	$('.successfully > .successfully-subscribed').html("<button type='button' class='close' data-dismiss='alert' aria-hidden='true' style='font-family: Geneva, Arial, Helvetica, sans-serif;'>&times;")
 	           		.append( "</button>");
             	    $('.successfully > .successfully-subscribed').append("<h1 class='text-center'>Спасибо! Теперь вы подписаны на наши новости!</h1>");
 		            $('.successfully > .successfully-subscribed').append('</div>');
 						    	            },
           	  	error: function() {		
	           		// Fail message
	             	$('.successfully').html("<div class='not-subscribed'>");
	                $('.successfully > .not-subscribed').html("<button type='button' class='close' data-dismiss='alert' aria-hidden='true' style='font-family: Geneva, Arial, Helvetica, sans-serif;'>&times;").append( "</button>");
	                $('.successfully > .not-subscribed').append("<h1>Кажется у нас что-то поломалось. Пожалуйста, напишите нам по почте nashi@umnenkie-deti.ru и мы вам сразу же ответим. Спасибо!</h1>");
	             	$('.successfully > .not-subscribed').append('</div>');
             	}   
            });
    });
});    

  
</script>-->

    <!--scroll-->
    <script type="text/javascript">
      $(document).ready(function() {
        $("a.scroll").click(function () { 
          var elementClick = $(this).attr("href");
          var destination = $(elementClick).offset().top;
          $('html,body').animate( { scrollTop: destination }, 1100 );
          return false;
        });
      });
    </script>
    <!--scroll--> 


</body></html>