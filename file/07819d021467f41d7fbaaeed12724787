    $(document).ready(function () {
        $(".nav_r_2016 li").hover(function () {
            $(this).addClass("on").siblings().removeClass("on");
            $(this).find("div").stop(true, false).show().removeAttr("style").parent().siblings().find("div").hide();
            if ($(this).attr("class") == "nav_r_four on") {
                $(".nav_r_2016 li").find("div").hide();
            }
        });
        $(".nav_r_three textarea").focus(function () {
            if ($(this).val() == this.defaultValue) {
                $(this).val('');
            }
        });
        $(".nav_r_three textarea").blur(function () {
            if ($(this).val() == "") {
                $(this).val(this.defaultValue);
            }
        });
        $(".wx_b_f i").click(function () {
            $(this).parent().hide();
        });
        $('#btnAskDoc').click(function () {
            var val = $('#txtQuesiotn').val();
            if (val != '无需注册，5分钟回复，10万医生，免费在线咨询！') {
                window.open('/addtopic.aspx?from=dh&words=' + escape(val) + '');
            } else {
                window.open('/addtopic.aspx?from=dh');
            }
        });
        var d = {};
        d.key = getkey();
        d.action = "GetLeftbarConfig";
        $.ajax({
            url: '/ask39/ajaxData/QrCodeLogin.aspx',
            data: d,
            dataType: 'json',
            success: function (resp) {
                $(".nav_r_2016 li").removeClass('on');
                $(".nav_r_2016 li").find('div').hide();
                if (resp.ActiveApp) {
                    $('.nav_r_one').addClass('on');
                    $('.nav_r_one').find('div').show();
                } else if (resp.ActiveWx) {
                    $('.nav_r_two').addClass('on');
                    $('.nav_r_two').find('div').show();
                } else {
                    $('.nav_r_three').addClass('on');
                    $('.nav_r_three').find('div').show();
                }
            }
        });

        $(window).scroll(function () {
            $(".nav_r_2016 li div").animate({
                left: '250px',
                opacity: '0.5',
                height: '0',
                width: '0'
            });
            $(".nav_r_2016 li").removeClass("on");
        });
    });
    function getkey() {
        var url = window.location.href;
        var keys = ['user_topics', 'browse', 'question', 'index', 'addtopic', 'doctors'];
        for (var i = 0, len = keys.length; i < len; i++) {
            if (url.indexOf(keys[i]) != -1) {
                return keys[i];
            }
        }
        $('.wx_b_f').show();
        return "index";
    }
