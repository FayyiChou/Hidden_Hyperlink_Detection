var temp1=document.searchform.positiontwo
function redirect(x){  
for (m=temp1.options.length-1;m>0;m--)  
temp1.options[m]=null  
for (i=0;i<group1[x].length;i++){  
temp1.options[i+1]=new Option(group1[x][i].text,group1[x][i].value)  
}  
temp1.options[0].selected=true  
}  

</script>
