function setTab(name,cursel,n){
 for(i=1;i<=n;i++){
  var menu=document.getElementById(name+i);
  var main=document.getElementById("con_"+name+"_"+i);
  menu.className=i==cursel?"hover":"";
  main.style.display=i==cursel?"block":"none";
 }
}
</script>
