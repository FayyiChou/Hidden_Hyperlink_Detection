  <script>
function checkoptions(obj,optionnum,$v)
{
    var opstions=$('.options'+optionnum);
    for(var i=0;i<opstions.length;i++)
    {
        opstions[i].className="options"+optionnum;
    }
    obj.className="options"+optionnum+" optionshover";
    document.getElementById("options"+optionnum).value=$v;
}
</script> 
