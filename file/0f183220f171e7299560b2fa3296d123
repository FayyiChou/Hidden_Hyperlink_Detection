     <script>
                    //设置促销活动信息
                    var goodsId = 2223;
                    var isOnSale = 0;
                    var prorespnub = 0;
                    var _nowprice_dsp = 0;
                    var _markprice_dsp = 0;
                    var leaveTimeLen = -1;
                    var zxtype = 0;
                    var score = 5;
                    var userArraystr = getCookie("user_province");
                    var userArray = userArraystr.split("_");
                    var addGoods = new Array(14);
                    jQuery("#_nub").val(1);
                    function getCurDate() {
                        return new Date().getTime().toString();
                    }
                    function initRespal(wareId, proId, proNum0) {
                        jQuery.ajax({
                            type: 'post',
                            url: '/pro/selectRespSals.htm?t=' + getCurDate(),
                            data: { 'proId': proId, 'resId': wareId, 'proNum0': proNum0 },
                            dataType: 'json',
                            cache: false,
                            async: true,
                            success: function(data, textStatus) {
                                if (data && data.code == 0) {
                                    inStock();
                                } else if (data && data.code == -10) {
                                    notsupportoutOfStock();
                                } else {
                                    outOfStock();
                                }
                            },
                            error: function(XMLHttpRequest, textStatus, errorThrown) {
                            }
                        });
                    }
                    function inittz(proids, goodsId) {

                        jQuery("#_tzvm").load('/pro/selecttz.htm?goodsId=' + goodsId + '&proids=' + proids + '&t=' + getCurDate(), {}, function() {
                            var tzProSum = ($(".detailSetEmble ul li").width()) * ($(".detailSetInfo").eq(0).find(".detailSetEmble ul li").length);
                            $(".detailSetEmble ul").css('width', tzProSum);

                            $(".detailSetEmble ul").each(function() {
                                $(this).find("li").last().addClass("lastItem");
                            });
                            var tzEveSum = $(".detailSetInfo").eq(0).find(".detailSetEmble ul").width();
                            var tzEveWidth = $(".detailSetInfo").find(".detailSetEmble").width();
                            if (tzEveSum > tzEveWidth) {

                                $(".detailSetInfo").eq(0).find(".detailSetEmble").css('overflow-x', 'scroll');
                            } else {
                                $(".detailSetInfo").eq(0).find(".detailSetEmble").css('overflow-x', '');
                            }
                            $(".detailSetTab").find("li").bind('mouseover', function() {

                                var tzs = $(this).attr('tzs');
                                var tzarr = tzs.split("_");
                                var t0 = parseFloat(tzarr[0]);
                                var t1 = parseFloat(tzarr[1]);
                                var t2 = parseFloat(tzarr[2]);
                                var t3 = parseFloat(tzarr[3]);
                                jQuery('#_ppricestr').html('￥' + t1.toFixed(2));
                                jQuery('#_oppricestr').html('￥' + t2.toFixed(2));
                                jQuery('#_leftstr').html('￥' + t3.toFixed(2));
                                jQuery('#_tzids').html('￥' + tzarr[4]);

                                jQuery('#_tzactid').val(t0);


                                var ln = $(".detailSetTab li").index($(this));
                                var tzProSum = ($(".detailSetEmble ul li").width()) * ($(".detailSetInfo").eq(ln).find(".detailSetEmble ul li").length);
                                var tzEveSum = $(".detailSetInfo").eq(ln).find(".detailSetEmble ul li").length;
                                var tzEveWidth = $(".detailSetInfo").find(".detailSetEmble").width();
                                if (tzEveWidth <= 630) {
                                    if (tzEveSum > 3) {
                                        $(".detailSetInfo").eq(ln).find(".detailSetEmble").css('overflow-x', 'scroll');
                                    } else {
                                        $(".detailSetInfo").eq(ln).find(".detailSetEmble").css('overflow-x', '');
                                    }
                                } else {
                                    if (tzEveSum > 4) {
                                        $(".detailSetInfo").eq(ln).find(".detailSetEmble").css('overflow-x', 'scroll');
                                    } else {
                                        $(".detailSetInfo").eq(ln).find(".detailSetEmble").css('overflow-x', '');
                                    }
                                }


                                $(".detailSetEmble ul").css('width', tzProSum);

                                if (ln == 0) {
                                    $(".detailSetTab").find(".tipLine").stop().animate({ left: +((82 * ln + 10)) }, 250)
                                } else {
                                    $(".detailSetTab").find(".tipLine").stop().animate({ left: +(82 * ln + 10) }, 250)
                                }
                                $(".detailSetInfoWrap").find(".detailSetInfo").hide();
                                $(".detailSetInfoWrap").find(".detailSetInfo").eq(ln).show();
                            });
                            $(".detBox_title").css({ 'marginTop': '10px' });



                        }).show();





                    }

                    function initProCoupon() {
                        $("#_canusecoupons").load("/pro/selectCouponByPro.htm", {
                            "proId": 2223,
                            "bId": 113,
                            "cId": 4
                        },
                        function() {
                            if ($("#_canusecoupons").children().length > 0) {
                                $("#_canusecoupons").show();
                            }
                        });

                    }


                    function initReturncoupon(cidandnubs) {
                        $("#_returncoupons").load("/pro/selectRrtCouponByCid.htm", {
                            "cidandnub": cidandnubs
                        },
                        function() {
                            var ilength = $(".pri-con-pre").length + 3;
                            $(".pri-con-pre").each(function(i) {
                                $(this).css('z-index', (ilength - i));
                            })
                        });

                    }

                    function initMzpro(proidss) {
                        $("#_mzpros").load("/pro/selectMzProNamesByCid.htm", {
                            "proids": proidss
                        },
                        function() {
                            var ilength = $(".pri-con-pre").length + 3;
                            $(".pri-con-pre").each(function(i) {
                                $(this).css('z-index', (ilength - i));
                            })
                        });

                    }

                    function initjjgproprice(proandprices) {
                        $("#_proandprice").load("/pro/selectjjgProAndPriceByCid.htm", {
                            "proandprice": proandprices
                        },
                        function() {
                            var ilength = $(".pri-con-pre").length + 3;
                            $(".pri-con-pre").each(function(i) {
                                $(this).css('z-index', (ilength - i));
                            })
                        });

                    }


                    function initLargesspro(resId, largesspro) {
                        jQuery.ajax({
                            type: 'post',
                            url: '/pro/selectLargesspro.htm?t=' + getCurDate(),
                            data: { 'resId': resId, 'largesspro': largesspro },
                            dataType: 'json',
                            cache: false,
                            async: true,
                            success: function(data, textStatus) {
                                var lartgessprostr = '<div class="premiums-tit">赠&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;品</div><div class="premiums-list-box">';
                                var largessprolist = data.largessprolist;
                                for (var i = 0; i < largessprolist.length; i++) {
                                    lartgessprostr += '<div class="premiums-list"><div class="premiums-list-pic"><p><img src="http://img10.jiuxian.com' + largessprolist[i]["ListImagePath"] + '" width="32" height="32" /></p><em><a href="http://www.jiuxian.com/goods-' + largessprolist[i]["ID"] + '.html" target="_blank">' + largessprolist[i]["Name"] + '</a></em><span>' + largessprolist[i]["zsnubs"] + '</span>瓶</div></div>';
                                }
                                lartgessprostr += '</div><div class="clear"></div>';
                                jQuery('#_lartgessprostr').html(lartgessprostr).show();
                            },
                            error: function() {
                            }
                        });
                    }
                    //有货
                    function inStock() {
                        if (!isOnSale) $("#respNubs").text('该商品已下架');
                        else $("#respNubs").text(' 有货');
                        if (isOnSale) {
                            $("#_jrgwc").show();
                            $("#_jrgwc2").show();
                            $("#_nosale").hide();
                            $("#_buyover").hide();
                            $("#_nosupportsh").hide();
                            $("#_buyover_show").hide();


                            if ($("#_canusecoupons").children().length > 0) {
                                $("#_canusecoupons").show();
                            }
                            //setContHeigth();
                        }
                    }
                    //不支持配送
                    function notsupportoutOfStock() {
                        if (!isOnSale) $("#respNubs").text('该商品已下架');
                        else $("#respNubs").text('无法配送！');
                        if (isOnSale) {
                            $("#_jrgwc").hide();
                            $("#_jrgwc2").hide();
                            $("#_nosale").hide();
                            $("#_buyover").hide();
                            $("#_nosupportsh").show();
                            $("#_buyover_show").show();
                            $("#_canusecoupons").hide();
                            //setContHeigth();
                        }
                    }
                    //无货
                    function outOfStock() {
                        if (!isOnSale) $("#respNubs").text('该商品已下架');
                        else $("#respNubs").text('无货');
                        if (isOnSale) {
                            $("#_jrgwc").hide();
                            $("#_jrgwc2").hide();
                            $("#_nosale").hide();
                            $("#_buyover").show();
                            $("#_nosupportsh").hide();
                            $("#_buyover_show").show();
                            $("#_canusecoupons").hide();
                            //setContHeigth();
                        }
                    }
                    function setProAct() {
                        var wareId = 0;
                        if (userArray[0] != undefined && userArray[0] != "" && userArray[0] != "undefined") {
                            wareId = userArray[0];
                        }
                        jQuery.ajax({
                            type: 'post',
                            url: '/pro/selectProActByProId.htm?t=' + getCurDate(),
                            data: { 'proId': goodsId, 'resId': wareId },
                            dataType: 'json',
                            cache: false,
                            async: true,
                            success: function(data) {
                                if (data && data.code == 0 && data.act) {
                                    var _actPriceStr = data.act.actPrice;
                                    var _nowPriceStr = data.act.nowPrice;
                                    var _shopPriceStr = data.act.shopPrice;
                                    var _markPriceStr = data.act.markPrice;
                                    var _goldCoinNubStr = data.act.goldCoinNub;
                                    var _morepricestr = data.act.morepricestr;
                                    var _proownactmap = data.act.proownactmap;
                                    var _purchaseCycle = data.act.purchaseCycle;
                                    var _purchaseNubs = data.act.purchaseNubs;
                                    var _endTimeLen = data.act.endTimeLen;
                                    var _nowTime = data.act.nowTime;
                                    var _isDisableBonus = data.act.isDisableBonus;
                                    var _proadvstr = data.act.advDes;
                                    var _niuCoinNubs = data.act.niuCoinNubs;
                                    var _isShopping = data.act.isShopping;
                                    var _isCashOnDelivery = data.act.isCashOnDelivery;
                                    var _isZsYcstd = data.act.isZsYcstd;
                                    var _isOnSale = data.act.isOnSale;
                                    var _orderbuymap = data.act.orderbuymap;
                                    var isorderbuy = 0;
                                    for (var key in _orderbuymap) {
                                        if (key == '11') {
                                            var yystate = _orderbuymap[key]["yystate"];

                                            if (yystate && yystate == 8) {
                                                jQuery('#_isorderbuypro').val(1);
                                                //$("#_orderbuy").show();
                                            } else if (yystate && yystate == 1) {
                                                isorderbuy = 1;
                                                var _actid = _orderbuymap[key]["actid"];
                                                var _bstime = _orderbuymap[key]["bstime"];
                                                $("#_gmsl").hide();
                                                $("#_shdz").hide();
                                                $("#_orderbuy").attr("href", "http://www.jiuxian.com/pro/orderbuy.htm?proId=2223&qgtime=" + _bstime + "&actid=" + _actid);
                                                $("#_orderbuy").show();
                                            } else if (yystate && yystate == 2) {
                                                isorderbuy = 1;
                                                $("#_gmsl").hide();
                                                $("#_shdz").hide();
                                                $("#_orderbuyover").show();
                                            }


                                        }
                                    }
                                    if (_isOnSale == 1) {
                                        isOnSale = true;
                                    } else {
                                        isOnSale = false;
                                    }
                                    var giftstrs = data.act.giftstrs;
                                    if (_proadvstr) {
                                        jQuery('#_proadvstr').html(_proadvstr).show();
                                    }

                                    if (_actPriceStr == -1) {
                                        jQuery('#_jxjgmc').html('酒&nbsp;仙&nbsp;价');
                                    }
                                    if (_nowPriceStr >= 0) {
                                        addGoods[5] = _nowPriceStr.toFixed(2);
                                        jQuery('#_nowPriceStr').html(_nowPriceStr.toFixed(2));
                                        jQuery('#_baknowPriceStr').val(_nowPriceStr.toFixed(2));
                                    }

                                    if (_markPriceStr) {
                                        addGoods[10] = _markPriceStr.toFixed(2);
                                        jQuery('#_markPriceStr').html(_markPriceStr.toFixed(2));
                                    }

                                    if (_endTimeLen && _endTimeLen > 0 && _nowTime && _nowTime > 0) {
                                        leaveTimeLen = _endTimeLen - _nowTime;
                                        if (leaveTimeLen > 0 && leaveTimeLen <= 60 * 60 * 24 * 3) {     //显示0-72小时
                                            setTimeout(timeAct, 1000);
                                            $("#timeAct").show();
                                        }
                                    }
                                    if (_isCashOnDelivery && _isCashOnDelivery == 1) {
                                        jQuery('#_isCashOnDelivery').html('<i></i>货到付款（部分地区）').show();
                                    }
                                    if (_purchaseNubs > 0) {
                                        if (_purchaseCycle == 0) {
                                            var _purstr = '<div style="float:left;width:60px;">限&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;购</div><div style="width:200px;color:#D60000" >每人限购' + _purchaseNubs + '瓶/套</div>';
                                            jQuery("#_pur").html(_purstr).show();
                                        } else if (_purchaseCycle > 0) {
                                            var _purstr = '<div style="float:left;width:60px;">限&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;购</div><div style="width:200px;color:#D60000" >每人' + _purchaseCycle + '天限购' + _purchaseNubs + '瓶/套</div>';
                                            jQuery("#_pur").html(_purstr).show();
                                        }

                                    }
                                    if (_isDisableBonus && _isDisableBonus == 1) {
                                        jQuery('#_isDisableBonus').html('温馨提示<span >特价商品不能使用优惠券抵扣。</span>').show();
                                    } else if (isOnSale && _isDisableBonus == 0) {
                                        initProCoupon();
                                    }
                                    if (isorderbuy == 0) {
                                        if (!isOnSale) {
                                            $("#_jrgwc").hide();
                                            $("#_jrgwc2").hide();
                                            $("#_nosale").show();
                                            $("#_buyover_show").show();

                                        } else {
                                            if (userArray[0] != undefined && userArray[0] != "" && userArray[0] != "undefined") {
                                                var wareId = userArray[0];
                                                var proId = $("#comment_goodsid").val();
                                                initRespal(wareId, proId, -1);
                                            }
                                        }

                                    }
                                    if (_goldCoinNubStr) {
                                        jQuery('#_goldCoinNubStr').html(_goldCoinNubStr);
                                    }

                                    if (_proownactmap) {
                                        var actisexit = 0;
                                        for (var key in _proownactmap) {
                                            if (actisexit == 0) {
                                                actisexit = 1;
                                                break;
                                            }
                                        }
                                        if (actisexit == 1) {
                                            var nowactstr = '<div class="pricing-tit">优&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;惠</div><div class="pricing-con">';
                                            var tmpnowactstr = '';
                                            var bcoupons = 0;
                                            var mzpros = 0;
                                            var jjgproandprice = 0;
                                            for (var key in _proownactmap) {
                                                if (key == '1') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '</div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                } else if (key == '2') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '</div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                } else if (key == '3') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + ' <div id="_proandprice" class="u-coupons-prompt"></div></div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                    jjgproandprice = _proownactmap[key]["markupprice"];
                                                } else if (key == '4') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '</div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                } else if (key == '5') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '</div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                } else if (key == '6') {
                                                    var proidslist = _proownactmap[key];
                                                    var tzmapstr = "";
                                                    for (var j = 0; j < proidslist.length; j++) {
                                                        tzmapstr += proidslist[j]['actid'] + "@" + proidslist[j]['packageprice'] + "@" + proidslist[j]['proids'] + "@" + proidslist[j]['oldpackageprice'] + "_";
                                                    }
                                                    inittz(tzmapstr, goodsId);

                                                } else if (key == '7') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '</div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                } else if (key == '8') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '<div id="_mzpros" class="u-coupons-prompt"></div></div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                    mzpros = _proownactmap[key]["proudctids"];
                                                } else if (key == '9') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '<div id="_returncoupons" class="u-coupons-prompt" ></div></div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';
                                                    bcoupons = _proownactmap[key]["proudctids"];
                                                } else if (key == '10') {
                                                    if (userArray[0] != undefined && userArray[0] != "" && userArray[0] != "undefined") {
                                                        var wareId = userArray[0];
                                                        if (_proownactmap[key]["largesspro"] && _proownactmap[key]["largesspro"] != "") {
                                                            initLargesspro(wareId, _proownactmap[key]["largesspro"]);
                                                        }
                                                    }
                                                } else if (key == '11') {
                                                    tmpnowactstr += '<div class="pri-con-act"><div class="pri-con-bg"><i></i><em>' + _proownactmap[key]["actname"] + '</em></div><div class="pri-con-pre">' + _proownactmap[key]["actadv"] + '<div id="_returncoupons" class="u-coupons-prompt" ></div></div><div class="pri-con-view"><a href="' + _proownactmap[key]["zdyurl"] + '" target="_blank">' + _proownactmap[key]['zdyname'] + '</a></div></div>';

                                                }
                                            }
                                            if ('' != tmpnowactstr) {
                                                nowactstr += tmpnowactstr;
                                                nowactstr += '</div><div class="clear"></div>';
                                                jQuery("#_nowactstr").html(nowactstr).show();
                                                if (bcoupons != 0) {
                                                    initReturncoupon(bcoupons);
                                                }
                                                if (mzpros != 0) {
                                                    initMzpro(mzpros);
                                                }

                                                if (jjgproandprice != 0) {
                                                    initjjgproprice(jjgproandprice);
                                                }

                                            }

                                        }
                                    }
                                    if ("" != _morepricestr) {
                                        var _dmdyhtzstr = '<div class="set-tit">选&nbsp;套&nbsp;装</div><div class="set-con">';
                                        var _dmdyhtzstryxz = '';
                                        var morepricearr = _morepricestr.split(",");
                                        morepricearr = morepricearr.sort();
                                        for (var i = 0; i < morepricearr.length; i++) {
                                            var tmppricearr = morepricearr[i].split("-");
                                            if (i == 0) {
                                                _dmdyhtzstr += "<span  ><em>￥" + tmppricearr[3] + "/瓶</em><b>买" + tmppricearr[0] + "瓶</b><i></i></span>";
                                                //_dmdyhtzstryxz ="已选择<span>"+tmppricearr[0]+"瓶&nbsp;&nbsp;￥"+tmppricearr[3]+"/瓶</span>";
                                            } else {
                                                _dmdyhtzstr += "<span><em>￥" + tmppricearr[3] + "/瓶</em><b>买" + tmppricearr[0] + "瓶</b><i></i></span>";
                                            }
                                        }
                                        _dmdyhtzstr += '</div><div class="clear"></div>';
                                        jQuery("#_dmdyhtz").html(_dmdyhtzstr).show();
                                        if (_dmdyhtzstryxz != "") {
                                            jQuery("#_dmdyhtzyxz").html(_dmdyhtzstryxz).show();
                                        }

                                    }
                                }
                            },
                            error: function() {
                            }
                        });

                        //setContHeigth();
                    }
                    setProAct();
</script> 
