var speed3=25;//速度数值越大速度越慢
var com11 = document.getElementById("com11"); //非w3c标准要找到表单，必须是要用getElementById("XX");
var com21 = document.getElementById("com21");
var comn = document.getElementById("comn");
com21.innerHTML=com11.innerHTML;

function Marquee3(){
if(com21.offsetWidth-comn.scrollLeft<=0)
comn.scrollLeft-=com11.offsetWidth;
else{
comn.scrollLeft++;
}
}
var MyMar3=setInterval(Marquee3,speed3)
com.onmouseover=function() {clearInterval(MyMar3)}
com.onmouseout=function() {MyMar3=setInterval(Marquee3,speed3)}
