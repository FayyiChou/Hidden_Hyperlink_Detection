function searchGo(){
	var kw = document.getElementById('keyword').value;
	document.location.href = '/source/search-'+encodeURI(kw)+'-1.html';
}

	function Check(event){
		if(window.event){
			event = window.event;
		}
		if(event.keyCode==13) {
		document.getElementById("lkSearch").focus();
		return;
		}
	}
