<script type="text/javascript">
$(function(){
	$(".syt_btn").click(function () {
		$(".syt_pop").css("display","block");
	});
	$(".syt_close").click(function () {
		$(".syt_pop").css("display","none");
	});
	$(".sy_nav").hover(
		function () {
		$(this).find(".sub_nav").css("display","block");
		},function () {
		$(this).find(".sub_nav").css("display","none");
	});
	var key = decodeURI(getQueryStr('keyword'));
	if (key.lenght==0){
		$('#tit_keyword').html('全部');
	} else {
		$("#tit_keyword").html(key);
		$('#q').val(key);
		showsy(1);
	}	
})

function showsy(page){
	var keywords = $('#q').val();
	$.ajax({  
		url:"http://www.eeyy.com/system.php?m=shouyounumber&c=index&a=search_ajax",  
		type:"get",  
		data:"page="+page+'&keyword='+encodeURIComponent(keywords),  
		dataType:"jsonp",
		success:function(jsonp){
			if(jsonp.lists){
				$('#lists').html(jsonp.lists);
			}else{
				$('#lists').html('<div class="fhover_tips"><p>暂无有效的的礼包！</p></div>');
			}
			
			if(jsonp.pages){
				$('#pages').html(jsonp.pages);
			}else{
				$('#pages').css('display','none');
			}
		}
	});
}
function getQueryStr(str){   
    var LocString=String(window.document.location.href);
    var rs = new RegExp("(^|)"+str+"=([^/&]*)(/&|$)","gi").exec(LocString), tmp;
    if(tmp=rs){   
        return tmp[2];   
    }   
    // parameter cannot be found   
    return "";   
}   

</script>

