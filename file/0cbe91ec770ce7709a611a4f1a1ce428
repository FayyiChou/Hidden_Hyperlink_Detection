		function check(){
		var query = document.getElementById('query');
		if(query.value == ""){			
			query.value ='输入关键词';
			query.style.color='#ff0000';
			return false;
		}
		if(query.value == "输入关键词"){			
			query.value ='输入关键词';
			query.style.color='#ff0000';
			return false;
		}
		return true;
	}
