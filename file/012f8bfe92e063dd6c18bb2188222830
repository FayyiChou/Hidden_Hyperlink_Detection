    <script type="text/javascript">
        $(function () {
            //1.\定义变量
            var pcl = $(".qiao_bt ul li");
            var pcn = $(".qiaoc_div");

            //2.\控制内容显示和隐藏
            pcn.eq(0).show().siblings(".qiaoc_div").hide();

            $("#qiaoc_divmore").attr("href", pcl.attr("data-url"));

            pcl.mouseover(function () {
                //3.1\获取当前Sid
                var _sid = $(this).attr("data-id");
                if (_sid == "project4") {
                    return false;
                }
                var _url = $(this).attr("data-url");
                $("#qiaoc_divmore").attr("href", _url);
                //3.2\控制选中和未选中
                $(this).addClass("cur").siblings().removeClass("cur");
                pcn.each(function () {
                    if ($(this).attr("data-id") == _sid) {
                        $(this).show();
                    }
                    else {
                        $(this).hide();
                    }
                });
            }
            );
        });
    </script>
