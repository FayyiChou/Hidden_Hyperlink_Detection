<script type="text/javascript">
	function hideHtyList(){
		$("#htylist").hide();
	}
	
	var floatLinkLeftPos = 0,floatLinkRightPos = 0; 
	var flagX = true, flagY = true;
	var stepfloatdiv = 1;
	var delayfloatdiv = 20; 
	var objWidth = 173;//图片宽度
	var objHeight = 106;//图片高度
	var objfloatdiv=document.getElementById("floatdiv");
	
	function floatLink() {
		var L = T = 0;
		var R = $(window).width()-objWidth;
		var B = $(window).height()-objHeight;
		$("#floatdiv").css({
				"left" : floatLinkLeftPos + document.body.scrollLeft,
				"top" : floatLinkRightPos + document.body.scrollTop
			});		
		floatLinkLeftPos = floatLinkLeftPos + stepfloatdiv*(flagX?1:-1); 
		if (floatLinkLeftPos < L) { flagX = true; floatLinkLeftPos = L;} 
		if (floatLinkLeftPos > R){ flagX = false; floatLinkLeftPos = R;} 
		floatLinkRightPos = floatLinkRightPos + stepfloatdiv*(flagY?1:-1);
		if (floatLinkRightPos < T) { flagY = true; floatLinkRightPos = T; } 
		if (floatLinkRightPos > B) { flagY = false; floatLinkRightPos = B; } 
	}

	
	$( document ).ready(function() {
		/*$("#floatdiv").show();
		var itl= setInterval("floatLink()", delayfloatdiv);
		$("#floatdiv").mouseover(function(){clearInterval(itl)});
		$("#floatdiv").mouseout(function(){itl=setInterval("floatLink()", delayfloatdiv)});
		*/
		//search form
		$('#searchform input:text').focus(function() {
			if($(this).val() == '请输入关键字'){
				$(this).val('');
			}
		}).blur(function() {
			if($(this).val() == ''){
				$(this).val('请输入关键字');
			}
		}).bind('keydown', function (e) {
            var key = e.which;
            if (key == 13) {
                e.preventDefault();
				$("#searchform").attr("action", "/e21web" + "/" + $("#searchform select").val()+".php");
                $('#searchform').submit();
            }
        });
		
		$('#searchform img:eq(3)').css("cursor","pointer").click(function(){
			$("#searchform").attr("action", "/e21web" + "/" + $("#searchform select").val()+".php");
			$('#searchform').submit();
		});
		
		$('#searchform input:text').on('keydown', function(e) {			
			e.keyCode == 13 && $('#searchform').submit();;
		});
		
		//submenu
		$("#hty").mouseover(function(){
			var currentPosLeft = $(this).offset().left;
			var currentPosTop = $(this).offset().top;			
			$("#htylist").css({
							  "top": currentPosTop+20,
							  "left": currentPosLeft
						});
			$("#htylist").show();
		});
		$("#hty").mouseout(function(){
			timeouthandle = setTimeout(hideHtyList,500);
		});
		
		$("#htylist").mouseover(function(){
			clearTimeout(timeouthandle);
			$("#htylist").show();
		});
		$("#htylist").mouseout(function(){
			hideHtyList();
		});
		
		//如果是网站首页，则显示填报志愿图片和微信图片
						if(typeof showTianBaoZhiYuan !== 'undefined' && $.isFunction(showTianBaoZhiYuan)){
					showTianBaoZhiYuan();
				}
					
				var liScrollNum = $('#nav3 li').length;
				$('#nav3').css('width', liScrollNum*1140);
				$('#nav3 ul:eq(0)').css('width', liScrollNum*1140);
			});
</script>
