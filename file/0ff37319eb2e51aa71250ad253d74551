<script type="text/javascript">  
        function changeDivUp(){
            var d = document.getElementById("daiyanbao_com_content");
            var h = d.offsetHeight;
            var maxh=403;
            function dmove(){
                h+=2;
                if(h>=403){
                    d.style.display='block';
                    clearInterval(iIntervalId);
                }else{
                    d.style.height=h+'px';
                }
            }
            iIntervalId=setInterval(dmove,2);
        }
        function hidden(){
            document.getElementById("daiyanbao_com_content").style.display="none";
        }
        setTimeout("changeDivUp()", 5500 )
        setTimeout("hidden()", 24200 )        
</script>
