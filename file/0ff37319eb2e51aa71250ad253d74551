<script language="javascript" type="text/javascript">(function () {
    var currentindex = 0;
    var timerID = null;
    function changeflash(i) {
        currentindex = i;
        $("#ad_slider .ad_img li:eq(" + i + ")").fadeIn("fast").siblings().hide();
        $("#ad_slider .ad_num li:eq(" + i + ")").addClass("current").siblings().removeClass("current");
        //$("#ad_sliderWrap").removeClass().addClass("bg"+i+"");
    }
    function startAm() {
        if (timerID) {
            return;
        }
        timerID = setInterval(timer_tick, 3000);
    }
    function stopAm() {
        if (timerID) {
            clearInterval(timerID);
            timerID = null;
        }
    }
    function timer_tick() {
        currentindex = currentindex < 3 ? currentindex + 1 : 0;
        changeflash(currentindex);
    }
    startAm();
    $("#ad_slider .ad_img>li").hover(function () { stopAm(); }, function () { startAm(); });
    $("#ad_slider .ad_num li").hover(function () { var num = $(this).index(); changeflash(num); stopAm(); }, function () { startAm(); })
})()
    $(".cases .cases_num li").hover(function () { var i = $(this).index(); $(".cases .cases_img li:eq(" + i + ")").fadeIn("fast").siblings().hide(); $(".cases .cases_num li:eq(" + i + ")").addClass("current").siblings().removeClass("current"); })
</script>
