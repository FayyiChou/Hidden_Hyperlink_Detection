</script><script type="text/javascript">
function layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss(css_pro,type)
{
	var typeval=type;
	if(typeval==1){
		var regex = "#nav_layer[0-9|a-z|A-Z]+\\s+ul+\\s+li+\\s+a:\\s*hover\\s*{\\s*"+css_pro+"\\s*:[^;]+";
	}else{
		var regex = "#nav_layer[0-9|a-z|A-Z]+\\s+li\.wp_subtop>a:\\s*hover\\s*{\\s*"+css_pro+"\\s*:[^;]+";
	}
		var navStyle = $.trim($("#layerC9ED7165457126D9B987944D1029738B").data("datastys_"));
		if(navStyle.length > 0)
	{
		var patt1 =new RegExp(regex,'i');
		var tmp = patt1.exec($.trim(navStyle));
		if(tmp)
		{
			return $.trim((tmp[0].match(/{[^:]+:[^;]+/)[0]).match(/:[^;]+/)[0].replace(':',''));
		}
	}

		navStyle = $.trim($("#layerC9ED7165457126D9B987944D1029738B").data("datasty_"));
		if(navStyle.length > 0)
	{
		if(typeval==1){
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul+\\s+li+\\s+a:\\s*hover\\s*{[^}]+}",'i');
		}else{
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+li\.wp_subtop>a:\\s*hover\\s*{[^}]+}",'i');
		}
		var tmp = patt1.exec(navStyle);
		
		if(tmp)
		{
			var tmp1 = tmp[0].match(/{[^}]+}/)[0];
			var patt2 = new RegExp(css_pro+"\\s*:\\s*[^;]+;",'i');
			tmp = patt2.exec(tmp1);
			if(tmp) return $.trim(tmp[0].replace(/[^:]+:/,'').replace(';',''));
		}
	}

	return $.trim($("#nav_layerC9ED7165457126D9B987944D1029738B ul li a").css(css_pro));
}


	$('#layerC9ED7165457126D9B987944D1029738B').layer_ready(function(){
		setTimeout(function(){
			wp_nav_addMoreButton('layerC9ED7165457126D9B987944D1029738B');
		},0);
		$('#nav_layerC9ED7165457126D9B987944D1029738B').find('li').hover(function(){
						$(this).children('ul').show();
			var type=$("#layerC9ED7165457126D9B987944D1029738B").find('.wp-new_navigation_content').attr('type');
			if(type==2){
				var self = $(this);
				var pos = 0 ;
				var loops = 0;
				$('#nav_layerC9ED7165457126D9B987944D1029738B').find('li').each(function(){
					if(loops == 1) return true;
					if(self.html() == $(this).html()){
						loops = 1;
						return true;
					}else{
						pos = pos + $(this).outerWidth();
					}	
					 
				})
				 
				$("#layerC9ED7165457126D9B987944D1029738B").find('.ddli').hide();
				var this_width = $('#nav_layerC9ED7165457126D9B987944D1029738B').outerWidth();
				var thisul_left = $('#nav_layerC9ED7165457126D9B987944D1029738B').css("padding-left");
				thisul_left = parseInt(thisul_left);
				$(this).children('.ddli').outerWidth(this_width).css("margin-left","-"+(thisul_left+pos+5)+"px");
				$(this).children('.ddli').eq(0).slideDown();
			}
		},function(){
			$(this).children('ul').hide();
						var type=$("#layerC9ED7165457126D9B987944D1029738B").find('.wp-new_navigation_content').attr('type');
			if(type==2){
				$("#layerC9ED7165457126D9B987944D1029738B").find('.ddli').slideUp();
			}
		});


		//子菜单位置设置
		$(".menu_hs6 #nav_layerC9ED7165457126D9B987944D1029738B").find('li').mouseenter(function(){
			var firstLi = $(this);
			var firestLiouterWidth = firstLi.outerWidth();
			var tmp_max_width = 0;
			firstLi.children('ul').children('li').width('auto').each(function(){
				if($(this).outerWidth() < firestLiouterWidth)
					$(this).width(firestLiouterWidth - parseInt($(this).css('padding-left')) - parseInt($(this).css('padding-right')));
				else if($(this).outerWidth() > tmp_max_width) tmp_max_width = $(this).outerWidth();
			});
				
			if(tmp_max_width > 0) firstLi.children('ul').children('li').each(function(){
				$(this).width(tmp_max_width - parseInt($(this).css('padding-left')) - parseInt($(this).css('padding-right')));
			});
				
			if(firstLi.parent('ul').attr('id') != 'nav_layerC9ED7165457126D9B987944D1029738B')
				firstLi.children('ul').css('margin-left',firstLi.outerWidth());
			tmp_max_width = 0;
		});
		
		//第三级即下级菜单随高度增加位置动态修改
		$(".menu_hs6 #nav_layerC9ED7165457126D9B987944D1029738B ul li").hover(function(){
			if($(this).children('ul').length > 0)
			{
				var marginTop = parseInt($(this).children('ul').css('margin-top'));
				if($(this).children('ul').offset().top > $(this).offset().top)
					$(this).children('ul').css('margin-top',marginTop - ($(this).children('ul').offset().top - $(this).offset().top) + 'px');
			}
		});

		$('.menu_hs6 #nav_layerC9ED7165457126D9B987944D1029738B').find('li').hover(function(){
			var direction=$("#layerC9ED7165457126D9B987944D1029738B").find('.nav1').attr('direction');
			var height = parseInt($(this).outerHeight());
