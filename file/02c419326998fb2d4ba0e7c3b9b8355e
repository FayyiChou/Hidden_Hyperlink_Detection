			{
				$('#nav_layerC9ED7165457126D9B987944D1029738B .wp_subtop').removeClass("lihover").children('a').removeClass("ahover");
				if(direction==1){				
					$(this).children('ul').css('top','auto').css('bottom',height + 'px');
				}else{				
					$(this).children('ul').css('top',height+'px').css('bottom','auto');	
				}
				$(this).children('a').css({'font-family':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-family",0),'font-size':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-size",0),'color':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("color",0),'font-weight':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-weight",0),'font-style':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-style",0)});
			}else{
				if(direction==1){
					$(this).children('ul').css('top','auto').css('bottom', '-0px');
				}else{
					$(this).children('ul').css('top',height+'px').css('bottom','auto');					
				}
				$(this).children('a').css({'font-family':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-family",1),'font-size':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-size",1),'color':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("color",1),'font-weight':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-weight",1),'font-style':layerC9ED7165457126D9B987944D1029738B_getSubMenuHoverCss("font-style",1)});
