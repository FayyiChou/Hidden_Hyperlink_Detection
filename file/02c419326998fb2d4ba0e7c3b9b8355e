	$LAB
	 .script('http://static.websiteonline.cn/website/plugin/lantern/js/lrscroll.js')
	.wait(function(){
		var marquee=$("#layer4BC7ECA74D60D29F0BA65144B150BAFD_scroll");
		marquee.css('height',marquee.closest('.wp-lantern_content').css('height'));
		
