//联播的服务器一定不能设置防盗链

function GetMusicType(Site ,MusicFile){
var ReturnStr;	
    ReturnStr=MusicFile.toLowerCase();
	ReturnStr=ReturnStr.replace('wma','mp3');
  
  return ReturnStr;	 
}

