  function showthis(page){
	  var linkstr;
	if(page>0){
	   linkstr="zccd=PZ2032505&artcode=art5102050026&Type=paipin&Page="+page;
	}else{
	   linkstr="zccd=PZ2032505&Type=paipin&artcode=art5102050026";
	}
	$.ajax({
      type: "POST",
      url: "/photo_get_new.php",
      data: linkstr,
      success: function(msg){
		  if(trim(msg)){
           $('#contents').html(trim(msg));
		   	$(".imgSum > li").hover(function(){
			if( $(this).prev()){
				if($(this).hasClass("current")){
					return;
				}
				$(this).addClass("hover").prev().addClass("prevH");
			}
	    },function(){
			if( $(this).prev()){
				if($(this).hasClass("current")){
					return;
				}
		    	$(this).removeClass("hover").prev().removeClass("prevH");
			}
	});
		 }
       }
    });
  }
//报告列表
function reportlist() {
    		var serUl = $(".serviceUl"),
			  serLiHei = serUl.children("li").height();
		for(var i = 0; i < serUl.length; i++){
			var serLi = serUl.eq(i).children("li"),
			serLiLen = serLi.length;
			if(serLiLen > 3){
				serUl.eq(i).siblings(".unfold").show();
			};
		};
		$(".unfold").click(function(){
			if($(this).hasClass("fold")){
				$(this).removeClass("fold");
				$(this).siblings(serUl).stop().animate({
						"height": 72+"px"
				}, 200);
			}else{
				$(this).addClass("fold");
				$(this).siblings(serUl).stop().animate({
						"height": serLiHei*$(this).siblings(serUl).children("li").length+"px"
				}, 200);
			};
		});
}

