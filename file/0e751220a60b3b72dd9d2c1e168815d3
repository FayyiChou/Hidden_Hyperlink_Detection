function getcookie(name) {
	var cookie_start = document.cookie.indexOf(name);
	var cookie_end = document.cookie.indexOf(";", cookie_start);
	return cookie_start == -1 ? '' : unescape(document.cookie.substring(cookie_start + name.length + 1, (cookie_end > cookie_start ? cookie_end : document.cookie.length)));
}
function gotologin() {
    var url = "http://passport.artron.net/login?appId=2&redirect="+ encodeURIComponent(window.location.href);
    if (navigator.userAgent.indexOf("MSIE") > 0) {
        document.write("<a id='ssoclick' href='" + url + "' style='display:none;' target='_parent'>a</a>");
        document.getElementById("ssoclick").click();
    } else {
        window.location.href = url;
    }
    return false;
}

function gotobigpic(v, ele) {
    if(!v){
		$("#dialogBg").css({
			"left" : "0",
			"height" : $(document).height() + "px",
			"display" : "block"
		}).animate({
			"opacity" : "0.7"
		},199);

		$("#alertDiv").show();

		var wHeight = $(window).height(),
			wWidth = $(window).width(),
			height = $("#alertDiv").outerHeight(),
			width = $("#alertDiv").outerWidth(),
			scrollTop = document.documentElement.scrollTop || document.body.scrollTop;

		$("#alertDiv").css({
			"top": (wHeight - height)/2 + scrollTop + "px",
			"left": (wWidth - width)/2 + "px"
		});

		$("#alertDiv .close, #alertDiv .cancel").one("click",function(event){
			event.preventDefault();
			$("#alertDiv").hide();

			$("#dialogBg").animate({
				"opacity" : "0"
			},199, function(){
				$(this).css("height", "0");
			});
		});
		return false;
    }else{
      //window.location.href="/showbigpic-art5102050026/";
	  $(ele).attr("target", "_blank");
    }
}
function DrawImage(ImgD,width,height){
 var image=new Image();
 image.src=ImgD.src;
 if(image.width>0 && image.height>0){
  flag=true;
  if(image.width/image.height>= width/height){
   if(image.width>width){
    ImgD.width=width;
    ImgD.height=(image.height*width)/image.width;
   }else{
    ImgD.width=image.width;
    ImgD.height=image.height;
   }
   ImgD.alt=image.width+"x"+image.height;
  }
  else{
   if(image.height>height){
    ImgD.height=height;
    ImgD.width=(image.width*height)/image.height;
   }else{
    ImgD.width=image.width;
    ImgD.height=image.height;
   }
   ImgD.alt=image.width+"x"+image.height;
  }
 }
}
  // 去左空格
function  ltrim(s){
  return  s.replace(  /^\s*/ ,"");
}
// 去右空格;
function  rtrim(s){
  return  s.replace(  /\s*$/,"" );
}
// 去左右空格;
function  trim(s){
  return  rtrim(ltrim(s));
}
var alertcontent;
function search_lot(lot){
 if(lot){
   $.ajax({
   type: "POST",
   url: "/auction_lotsearch.php",
   data: "type=show&phcd=PMH208292&lot="+lot,
   success: function(msg){
	 alertcontent=$('#alertinfo').html();
     msg=trim(msg);
	 if(msg=='1'){
		$('#alertinfo').show().closest(".lotSearch").addClass("prela");
		$('#alertinfo').html('参数错误!'); //参数错误
	 }else if(msg=='2'){
		$('#alertinfo').show().closest(".lotSearch").addClass("prela");
		$('#alertinfo').html('未找该图录号拍品!'); //未找该图录号拍品！
	 }else if(msg!=''){
		window.location.href=msg;
	 }
   }
    });
  }
}
function addsort(){
   var sortname=encodeURIComponent(trim($('#sortname').val()));
     if(sortname==''||sortname=='请填写新分组'){
       alert('请添加新分类名称！');
	   $('#sortname').focus();
       return;
      }
   $.ajax({
   type: "GET",
   url: "/AddSort.php",
   data: "Type=add_sort"+"&sortname="+sortname,
   success: function(msg){
        var data=msg.split('_');
        if(data[0]=='ok'){
		 var str=$('#sorts').html()+'<span><input type="radio" name="sorts"  value="'+data[1]+'" style="cursor:pointer"/>  '+decodeURI(sortname)+'</span>';
		   $('#sorts').html(str);
		   $('#sortname').val('');
		}
    }
  });
  }
  function addatention(picid,Title) {
   var Sid=$("input[name='sorts']:checked").val();
   $.ajax({
   type: "GET",
   url: "/AddConnect.php",
   data: "Type=add_collect&picid="+picid+"&Title="+Title+"&Sid="+Sid,
   success: function(msg){
       if(msg=='ok'){
		 $('#tishi_content').html('收藏成功！');
		 document.getElementById('collection_tishi').style.display="block";
		  document.getElementById('hrefShowCang').style.display="block";
		  $('#groupArea_A').hide();
	   }else if(msg=='haveconnect'){
         $('#tishi_content').html('您已经收藏过！');
		 	  document.getElementById('collection_tishi').style.display="block";
			  document.getElementById('hrefShowCang').style.display="block";
		 }
   }
  });
  }
