      $(document).ready(function(){
        $('.model003box').click(function(){
          var ww = $(window).width() * 700/1200;
          $('.lightbox').css({'width':ww});
          if($(this).attr('href')!=''){
            var link = $(this).attr('href');
            if($(this).attr('iframe')=='Y'){
            $.fancybox.open({
              href : link,
              type : 'iframe',
              padding : 5
            });
            }else{
            $.fancybox.open({
              href : link,
              padding : 5
            });
            }
          }       
          return false;
        });
      });
