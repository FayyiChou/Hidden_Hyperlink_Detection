//给手机号输入框添加事件
$("input#yy_phone").keyup(function() {
    //是否绑定手机号
    if($("input#yy_phone").val()==bind_phone&&bind_phone!=yy_phone_default&&bind_phone!='') {
        //隐藏获取手机号验证码的对话框
        $("dl.yy_captcha_dl").css("display","none");
    }else {
        //显示获取手机号验证码的对话框
       $("dl.yy_captcha_dl").css("display","block");
    }
})
//===添加表框的动作事件
$("input#yy_phone").focus(function() {
    if($("input#yy_phone").val()==yy_phone_default) {
        $("input#yy_phone").val('');
    }
})

$("input#yy_name").focus(function() {
    if($("input#yy_name").val()==yy_name_default) {
        $("input#yy_name").val('');
    }
})
$("input#yy_captcha").focus(function() {
    if($("input#yy_captcha").val()==yy_captcha_default) {
        $("input#yy_captcha").val('');
    }
})
$("input#yy_projname").focus(function() {
    if($("input#yy_projname").val()==yy_captcha_default) {
        $("input#yy_projname").val('');
    }
})

