function kftValCodeHint_2() {
    $("#tg_getValCode_2").val(kftHintNum_2+"秒后重新获取");
    kftClockCodeHint_2 = setInterval(kftClockCodeHint_2Fun, 1000);
}

function kftClockCodeHint_2Fun() {
    if(kftHintNum_2 > 0) {
        kftHintNum_2--;
        $("#tg_getValCode_2").val(kftHintNum_2+"秒后重新获取");
    } else {
        clearInterval(kftClockCodeHint_2);
        $("#tg_getValCode_2").val("重新获取验证码");
        $("#tg_getValCode_2").attr("disabled", false);
        kftHintNum_2 = 60;
    }
}

var tg_signup_name_default = "请输入用户名";
var tg_signup_phone_default = "请输入手机号";
var tg_signup_email_default = "请输入邮箱";
var tg_signup_captcha_default = "验证码";

function signUpRefresh(){
	//$("input#tg_signup_phone").trigger("blur");
	var x = '';
	if(bind_phone!='') {
		x = bind_phone;
	}else {
		x = tg_signup_phone_default;
	}
	$("input#tg_signup_phone").val(x);
    $("input#tg_signup_name").val(tg_signup_name_default);
    $("input#tg_signup_captcha").val(tg_signup_captcha_default);
    $("input#tg_signup_email").val(tg_signup_email_default);
}

