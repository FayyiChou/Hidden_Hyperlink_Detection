    if($("input#yy_phone").val()=='') {
        $("input#yy_phone").val(yy_phone_default);
    }
})
$("input#yy_name").blur(function() {
    if($("input#yy_name").val()=='') {
        $("input#yy_name").val(yy_name_default);
    }
})
$("input#yy_captcha").blur(function() {
    if($("input#yy_captcha").val()=='') {
        $("input#yy_captcha").val(yy_captcha_default);
