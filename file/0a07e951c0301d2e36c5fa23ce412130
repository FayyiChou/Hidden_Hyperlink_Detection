    })
})

//报名提交
$("button[id^='signup_']").each(function(index, domEle1){
    // click动作
    $(this).click(function(){
        //埋码取newcode
        maima_newcode = '';
        var maima_a_site = $(this).parents('.kft_hot_list').find('.kflx td a')
        var maima_a_size = maima_a_site.length;
        for(var i=0;i< maima_a_size;i++){
            var maima_temp_value = maima_a_site[i].id;
            var maima_temp_arr = maima_temp_value.split('_');
            if( i<maima_a_size-1){
                maima_newcode += maima_temp_arr[2]+',';
            }else{
                maima_newcode += maima_temp_arr[2];
            }
        }
        //每次点击报名那么都要将js中的默认值写入到input框中，
        signUpFormInit();
        var tmp = domEle1.id.split("_");
        var signup_id = tmp[1];  var line_id = tmp[2];
        $(".maptck_btn1").attr("id","submit_"+line_id);
        $("#signup_id").val(signup_id);
        var yxloupan = '';
        var yxloupanInfo = housedata[line_id];
        for(var lpid  in yxloupanInfo) {
            yxloupan += '<div class="check-line" style="width: 130px;float: left;"><input type="checkbox" class="lpyx-chk" value="'+lpid+'"/><label for="" class="lpyx-lab">'+yxloupanInfo[lpid].housetitle+'</label></div>';
        }
        $("#signup_yxloupan").html(yxloupan);

        // 设置位置
        var thisTop = $(this).offset().top;  var thisLeft = $(this).offset().left;
        var tipTop = thisTop+200+"px";  var tipLeft = thisLeft -450 +"px";
        $("#signup_box").css({position:"absolute",top:tipTop,left:tipLeft});

        $("#signup_box").show();
        $("#line_id").val(line_id);
        $("#linename").val($(this).data('linename'));
    })
});
//看房团报名
function submitSignUp(domId)
{
    var linename = $("#linename").val();//线路名称
    var ajaxUrl = "/house/web/kft_signup_ajax.php";
    var sign_id = $("#signup_id").val();//看房团ID
    var line_id = $("#line_id").val();//线路ID
    var phone = $("#signup_phone").val();//手机号
    var name = $("#signup_name").val();//用户名
    var count = $("#signup_count").val();//报名人数
    var phonecode = $("#signup_captcha").val();//手机号验证码
    phonecode = (phonecode==''||phonecode==signup_captcha_default)?'':$("#signup_captcha").val();
    if(phonecode==''||phonecode==signup_captcha_default){
        alert("请输入手机验证码");
        return false;
    }
    var newcodedom = $('#signup_yxloupan :checked');
    var newcodelist = new Array();
    var BehaviorID = 154;
    for(var i=0; i < newcodedom.length; i++) {
        newcodelist[i] = $(newcodedom[i]).val();
    }
    newcodelist = newcodelist.join(',');
    var strP=/^\d+$/;
    if(count>10 || !strP.test(count)){
        alert("你好，报名人数最多10人!");
        return false;
    }else if(phone==''||phone==signup_phone_default) {
        alert("请填写手机号！");
        return false;
    }else if(name==""||name==signup_name_default) {
        alert("请填写用户名！");
        return false;
    }else if(count<=0) {
        alert("报名人数不能为空！");
        return false;
    }
    //执行回调
    _ub.collect(33,{'vwg.page':'nhkftlist','vwn.seehouseline':encodeURIComponent(linename),'vwn.phone':encodeURIComponent(phone),'vwn.name':encodeURIComponent(name),'vwn.number':encodeURIComponent(count),'vwn.projectid':encodeURIComponent(newcodelist)});
    maima_newcode = '';

    $.ajax({
        type:'get',
        dataType:'jsonp',
        data:{mobilephone:phone,service:"newhouse-open-web",mobilecode:phonecode},
        url:"https://passport.fang.com/loginverifysms.api",
        error:function(){
            alert("验证码错误，请重试");},
        success:function(msg){
            if(msg.Message == "Success" || bind_phone){
                $.ajax({
                    type:'get',
                    dataType:'json',
                    url:ajaxUrl,
                    data:'BehaviorID='+BehaviorID+'&signid='+sign_id+'&lineid='+line_id+'&city='+urlencode(city)+'&phone='+phone+'&name='+urlencode(name)+'&count='+count+'&phonecode='+phonecode+'&yxloupan='+newcodelist,
                    //complete:function(){},
                    success:function(msg){
                        if(msg.result==1){
                            bind_phone = msg.bind_mobile;
                            $("#signup_box").hide();
                            $("#baoming_suc").show();

                        }else{
                            alert(msg.msg);
                        }

                        if(msg.captcha=='pass') {
                            bind_phone = msg.bind_mobile;
                            Xml.Request("/house/esf/getusernewhouse.php", null, doHeadloginuser);
                        }
                        signUpFormInit();
                    }
                });
            }else{
                alert(msg.Tip);
                return false;
            }
        }
    });

}
//线路下一张，上一张图片
function nextImg(lineID,count){
	reloadImg(lineID);
	for(var i=1;i<count;i++){
		if($('#kftImg_'+lineID+'_'+i).is(":visible")){	
		    $('#kftImg_'+lineID+'_'+i).hide();
			$('#kftImg_label_'+lineID+'_'+i).hide();
		    j = i+1;
			if(j<=count){
			    $('#kftImg_'+lineID+'_'+j).show();
				$('#kftImg_label_'+lineID+'_'+j).show();
			}else{
			    $('#kftImg_'+lineID+'_'+count).show();
				$('#kftImg_label_'+lineID+'_'+count).show();
			}
			break;
		}	
	}
}
function preImg(lineID,count){
	reloadImg(lineID);
	for(var i=count;i>0;i--){
		if($('#kftImg_'+lineID+'_'+i).is(":visible")){	
		    $('#kftImg_'+lineID+'_'+i).hide();
			$('#kftImg_label_'+lineID+'_'+i).hide();
		    j = i-1;
			if(j>=1){
			    $('#kftImg_'+lineID+'_'+j).show();
				$('#kftImg_label_'+lineID+'_'+j).show();
			}else{
			    $('#kftImg_'+lineID+'_'+1).show();
				$('#kftImg_label_'+lineID+'_'+1).show();
			}
			break;
		}	
	}
}
//图片动作加载
function reloadImg(lineID) {
	$("img[id^='kftImg_"+lineID+"']").each(function() {
		if(!$(this).attr("src")) {
			$(this).attr("src",$(this).attr("data-src"));
		}
	})
}
var getIdFromString = function(str){
	var tmp = str.split("_");
	for(var i=0; i<tmp.length; i++){
		if(parseInt(tmp[i]) == tmp[i]) {
			return tmp[i];
		}
	}
};

$("#xl_detail").hover(function(){
    $(this).show();
	var show_id = $(".syld_fr").attr("id");
	$(".kflx_list > a").removeClass("current");
	$("#xl_"+show_id).addClass("current");
},
    function(){
	$(".kflx_list > a").removeClass("current");
	$(this).hide();
});

//==================================================edit by huajie.sun

var signup_phone_default = '请输入手机号';
var signup_all_count = '请输入报名人数';
var signup_name_default = '请输入用户名';
var signup_captcha_default = '请输入验证码';
var signup_authimg_default = '请输入图文验证码';
var signup_email_default = '请输入邮箱';
var bind_phone = '';

$(document).ready(function() {
	
	var ajaxUrl = "/house/ajaxrequest/isCorrectMobileUser.php?rand="+Math.random();
	$.ajax({
		type:'get',
		dataType:'json',
		url:ajaxUrl,
		data:'',
		complete:function(){},
		error:function(){},
		success:function(msg){
			//如果用户已经登录
			if(msg.uid) {
				$("input#hide_cell").val(msg.bindMobile);
				//signup_phone_default = msg.bindMobile;
				bind_phone = msg.bindMobile;
				//alert(bind_phone);
				//初始化表框
				signUpFormInit();
				signUpRefresh();
				yy_signUpRefresh();
				if (msg.bindMobile){
					//隐藏获取手机号验证码的对话框
					$("input#signup_captcha").parent("li").css("display","none");
					$("input#tg_getValCode").css("display","none");
					$("input#tg_signup_captcha").parent("li").css("display","none");
					$("input#tg_getValCode_2").css("display","none");
					$("dl.yy_captcha_dl").css("display","none");
				}else {
					//显示获取手机号验证码的对话框
					$("input#signup_captcha").parent("li").css("display","block");
					$("input#tg_getValCode").css("display","block");
					$("input#tg_signup_captcha").parent("li").css("display","block");
					$("input#tg_getValCode_2").css("display","block");
					$("dl.yy_captcha_dl").css("display","block");
				}
				
			}else {
				//初始化表框
				signUpFormInit();
				signUpRefresh();
				yy_signUpRefresh();
			}
		}
	});
})
//给手机号输入框添加事件
$("input#signup_phone").keyup(function() {
	//是否绑定手机号
	if($("input#signup_phone").val()==bind_phone&&bind_phone!=signup_phone_default&&bind_phone!='') {
		//隐藏获取手机号验证码的对话框
		$("input#signup_captcha").parent("li").css("display","none");
		$("input#tg_getValCode").css("display","none");
	}else {
		//显示获取手机号验证码的对话框
		$("input#signup_captcha").parent("li").css("display","block");
		$("input#tg_getValCode").css("display","block");
	}
})
//===添加表框的动作事件
$("input#signup_phone").focus(function() {
	//检查是否和默认值相同，如果相同，那么直接清空
	if($("input#signup_phone").val()==signup_phone_default) {
		$("input#signup_phone").val('');
	}
})

$("input#signup_name").focus(function() {
	//检查是否和默认值相同，如果相同，那么直接清空
	if($("input#signup_name").val()==signup_name_default) {
		$("input#signup_name").val('');
	}
})

//失去焦点动作
$("input#signup_phone").blur(function() {
	//检查是否为空，如果为空那么直接等于默认值
	if($("input#signup_phone").val()=='') {
		$("input#signup_phone").val(signup_phone_default);
	}
})
$("input#signup_name").blur(function() {
	//检查是否为空，如果为空那么直接等于默认值
	if($("input#signup_name").val()=='') {
		$("input#signup_name").val(signup_name_default);
	}
})

//===初始化表框
function signUpFormInit() {
	$("input#signup_phone").val((bind_phone!='')?bind_phone:signup_phone_default);
	$("input#signup_name").val(signup_name_default);
	$("input#signup_count").val(signup_all_count);
}
//===验证码发送函数
var kftHintNum = 60;

var kftClockCodeHint;
    
function kftValCodeHint() {
    $("#tg_getValCode").val(kftHintNum+"秒后重新获取");
    kftClockCodeHint = setInterval(kftClockCodeHintFun, 1000);
}

function kftClockCodeHintFun() {
    if(kftHintNum > 0) {
        kftHintNum--;
        $("#tg_getValCode").val(kftHintNum+"秒后重新获取");
    } else {
        clearInterval(kftClockCodeHint);
        $("#tg_getValCode").val("重新获取验证码");
        $("#tg_getValCode").attr("disabled", false);
        kftHintNum = 60;
    }
}
//===============================================================================
//=====================第二个团购框
//给手机号输入框添加事件
$("input#tg_signup_phone").keyup(function() {
    //是否绑定手机号
    if($("input#tg_signup_phone").val()==bind_phone&&bind_phone!=tg_signup_phone_default&&bind_phone!='') {
        //隐藏获取手机号验证码的对话框
        $("input#tg_signup_captcha").parent("li").css("display","none");
        $("input#tg_getValCode_2").css("display","none");
    }else {
        //显示获取手机号验证码的对话框
        $("input#tg_signup_captcha").parent("li").css("display","block");
        $("input#tg_getValCode_2").css("display","block");
    }
})
//===添加表框的动作事件
$("input#tg_signup_phone").focus(function() {
    //检查是否和默认值相同，如果相同，那么直接清空
    if($("input#tg_signup_phone").val()==tg_signup_phone_default) {
        $("input#tg_signup_phone").val('');
    }
})
$("input#kft_mf_authimg").focus(function() {
    //检查是否和默认值相同，如果相同，那么直接清空
    if($("input#kft_mf_authimg").val()==signup_authimg_default) {
        $("input#kft_mf_authimg").val('');
    }
})
