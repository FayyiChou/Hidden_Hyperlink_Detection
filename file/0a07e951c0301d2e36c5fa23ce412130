<script type="text/javascript">
//路线搜索
$(function(){
_ub.city = '漳州';
		_ub.biz = 'n';
		_ub.location = 0/1;
		_ub.collect(0,{'vwg.page':'nhkftlist'}); 
})
</script>


<!--看房团热门团购搜索埋码-->

<script type="text/javascript">
//区域
	function hotPurchase()
	{		
		var district = $("#bs_strDistrict option:selected").text();
		var Price = $("#bs_strPrice option:selected").text();
		var Purpose = $("#bs_strPurpose option:selected").text();
		if(Price.substr(-2) == '以下')
		{
			var smallPrice = parseInt(Price);
			Price = '0-'+smallPrice;
		}

		if(Price.substr(-2) == '以上')
		{
			var bigPrice = parseInt(Price);
			Price = bigPrice+'-99999';
		}
		if(Price == '价格')
		{
			_ub.collect(1,{'vwn.position':encodeURIComponent(district),'vwn.genre':encodeURIComponent(Purpose)});
		}else {
			_ub.collect(1,{'vwn.position':encodeURIComponent(district),'vwn.unitprice':encodeURIComponent(Price),'vwn.genre':encodeURIComponent(Purpose)});
		}
	}
</script>



</body></html>