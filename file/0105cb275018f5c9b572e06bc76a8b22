<script>
	$(function(){

		$("#nav > a").each(function(index,obj){
			var offset = $(obj).offset();
			if(index==0)
				return true;
			$("#cmenu"+(index)).css({
				left:offset.left
			})
		});
		
		$("#nav > a").hover(function(e){
			var index = $(this).index();
			if(index==0)
			return;
			$(".cmenu").hide();
			$("#cmenu"+index).show();
			//$(".cmenu").eq(index).show();
			
			
		});
		$(".header-link ").hover(null,function(e){
		
			$(".cmenu").hide();
			
			
		});
		$("#nav > a").mouseleave(function(){
			var index = $(this).index();
			$(".cmenu").hide();
			$("#cmenu"+index).show();
			//$(".cmenu").eq(index).show();
		});
		$(".cmenu").hover(function () {
			$(this).show();
		}).mouseleave(function(){
			
			$(this).hide();
		});
	});
</script>
