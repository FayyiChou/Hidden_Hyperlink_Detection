     <SCRIPT type=text/javascript>
		 var _c = _h = 0; 
		$(function(){
			$('#play > li').click(function(){
				var i = $(this).attr('alt') - 1; 
				clearInterval(_h);
				_c = i;
				play();
				change(i);        
			})
			$("#pic div").hover(function(){clearInterval(_h)}, function(){play()});
			play();
		})
		function play()
		{
			_h = setInterval("auto()", 6000);
		}
		
		function change(i)
		{
			$("#play li:eq(" + i + ")").removeClass("normal").addClass("active").siblings().addClass("normal");
			$("#pic div").hide().eq(i).show();
		}
		function auto()
		{    
			_c = _c > 4 ? 0 : _c + 1;
			change(_c);
		}
		
    </script>  
