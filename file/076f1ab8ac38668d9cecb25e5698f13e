<script type="text/javascript">
    $('.search-suggest').focus(function(){
        if($(this).val()=='Ââåäèòå ïîèñêîâûé çàïðîñ')$(this).val('');
    });
    $('.search-suggest').blur(function(){
        if($(this).val()=='')$(this).val('Ââåäèòå ïîèñêîâûé çàïðîñ');
    });

    $('#srcform').submit(function(){
        if($('.search-suggest').val()=='Ââåäèòå ïîèñêîâûé çàïðîñ' || $('.search-suggest').val()=='')
        {
            alert('Ââåäèòå ïîèñêîâûé çàïðîñ!');
            return false;
