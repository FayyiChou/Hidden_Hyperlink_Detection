<script type="text/javascript">
    $('.right-menu').on('hide', 'ul', function(e){
        $(this).parent("li").find("i:first").removeClass("icon-chevron-up").addClass("icon-chevron-down");
        $(this).parent("li").find("i:first").removeClass("icon-chevron-left").addClass("icon-chevron-down");
        e.stopPropagation();
    });

    $('.right-menu').on('show', 'ul', function(e){
        $(this).parent("li").find("i:first").removeClass("icon-chevron-down").addClass("icon-chevron-up");
        $(this).parent("li").find("i:first").removeClass("icon-chevron-left").addClass("icon-chevron-up");
        e.stopPropagation();
    });

    /*$('.accordion-group.lvl1 a.green-gradient').mouseover(function(){
        $(this).parent().addClass('podsvet');
    });
    $('.accordion-group.lvl1 a.green-gradient').mouseout(function(){
        $(this).parent().removeClass('podsvet');
    });*/
</script>

  
