		<script type="text/javascript">
			$(document).ready(function() {
				// Add Class to External Links
				$('a[href^="http"]').not('[href$=".pdf"]').addClass('external');
				// Remove Class for Local Links
				$('a[href^="http://naturalsciences"]').not('[href$=".pdf"]').removeClass('external');
				$('a[href^="http://www.naturalsciences"]').not('[href$=".pdf"]').removeClass('external');
				$('#social-media-links a[href^="http"]').removeClass('external');

				// Add Email Class
				$('a[href^="mailto:"]').addClass('email');
				// Add Target="_blank" to all external links automatically in main-content
				$('a[href^="http://"]').not('a[href*=naturalsciences]').attr('target','_blank');
				// Page-specific javascript/jQuery
				

				$('#homeCarousel').carousel({
			       interval: 9000
			       //interval: false,
			       //pause: 'hover'
			    })

				$('#mobileDateInput').datepicker({
					format: "yyyy-mm-dd",
					clearBtn: false,
					orientation: "bottom left",
					autoclose: true
				});

 			    // Home Calendar
		    	$('#selectedDate').glDatePicker({
		        	cssName: 'ncmns',
		        	showAlways: true,
		        	allowYearSelect: false,
		        	allowMonthSelect: false,

		        	onClick: function(target, cell, date, data) {
		        		target.val(date.getFullYear() + ' - ' +
		                    	   date.getMonth() + ' - ' +
		                    	   date.getDate());
		                var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
				        var namedMonth = months[date.getMonth()];
						$('#displayDate').val(namedMonth + ' ' + date.getDate() + ', ' + date.getFullYear());
		        		if(data != null) {
				            alert(data.message + ' ' + date);
		        		}
						var targetDate;
						targetDate = date.getFullYear() + '-' + ('0' + (date.getMonth()+1)).slice(-2) + '-' + ('0' + date.getDate()).slice(-2);
						$('#selectedDateData').val(targetDate);
		        		calendarAjaxRequest();
		    		}
		        });

		        // Set Current Date on Page Load
		        var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
		        var d = new Date();
		        var namedMonth = months[d.getMonth()];
				$('#displayDate').val(namedMonth + ' ' + d.getDate() + ', ' + d.getFullYear());

			    // Populate Date Field for Ajax
				var strDate;
				strDate = d.getFullYear() + '-' + ('0' + (d.getMonth()+1)).slice(-2) + '-' + ('0' + d.getDate()).slice(-2);
				$('#selectedDateData').val(strDate);

			    // Set initial loction type to 0
				$('#selectedLocationData').attr('value','0');

				calendarAjaxRequest();

			    // When Location is Selected Get Data
				$('#locationSelect').change(function() {
					var optionChange = $('#locationSelect option:selected').val();
  					$('#selectedLocationData').val(optionChange);
						calendarAjaxRequest();
				});

				// When Mobile Location is Selected Get Data
				$('#locationSelectMobile').change(function() {
					var optionChange = $('#locationSelectMobile option:selected').val();
  					$('#selectedLocationData').val(optionChange);
						calendarAjaxRequest();
				});

				// When Mobile Date is Selected
				$('#mobileDateInput').on('changeDate', function(e) {
					var theDate = (new Date(e.date).getFullYear()) + '-' + ('0' + (new Date(e.date).getMonth() + 1)).slice(-2) + '-' + ('0' + (new Date(e.date).getDate())).slice(-2);
				    // Populate Large Date Label
		            var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
				    var namedMonth = months[new Date(e.date).getMonth()];
					$('#displayDate').val(namedMonth + ' ' + (new Date(e.date).getDate()) + ', ' + (new Date(e.date).getFullYear()));
				    // Populate Data Fields
					$('#selectedDateData').val(theDate);
				    // Run Query
					calendarAjaxRequest();
				});

			    // Get Calendar Table Data Non-mobile
				function calendarAjaxRequest() {
				    $.ajax({
				        url: 'inc/getCalendarData.php',
				        type: 'GET',
				        dataType: 'html',
				        data: { selectedDate: $('input#selectedDateData').val(),
				        		selectedLocation: $('#selectedLocationData').val()},
				        success: function(response) {
				            $('table#eventsTable tbody').html(response);
				        },
				    });
				}

 				// Get Calendar Table Data Mobile
				function calendarAjaxRequestMobile() {
				    $.ajax({
				        url: 'inc/getCalendarData.php',
				        type: 'GET',
				        dataType: 'html',
				        data: { selectedDate: $('input#selectedDateData').val(),
				        		selectedLocation: $('#locationSelectMobile').val()},
				        success: function(response) {
				            $('table#eventsTable tbody').html(response);
				        },
				    });
				}

				// Get Alert Message
				alertAjaxRequest();

				// Alert Message
				function alertAjaxRequest() {
				    $.ajax({
				        url: 'inc/getAlertBanner.php',
				        type: 'GET',
				        dataType: 'html',
				        success: function(response) {
				            $('#first-band').append(response);
				        },
				    });
				}
// iOS App Message
//var os;
//os = getMobileOperatingSystem();
//if ( os == 'iOS' ) {
//	var appmsg = '<div id="app-banner"><h3>Going to BugFest? <a href="https://itunes.apple.com/us/app/bugfest-2016/id1027428997?mt=8">Get the App Here</a>.</h3></div>';
//	$('#first-band').append(appmsg);
//}
//else {
  // DO Nothing
//}

			});
		</script>

		
				
