     <script language="javascript">
function  isNumber(String)
{  
var      Letters = "1234567890-";      //可以自己增加可输入值
var      i;
var      c;
if(String.charAt(0)=='-')
return false;
if(String.charAt(String.length-1)=='-')
return false;
for(i=0;i<String.length;i++)
{  
 c = String.charAt(i);
if (Letters.indexOf(c)<0)
return false;
}
return true;
}
if(!document.getElementById("comdtype").checked)
showChild(2);
function isEmail(str){
var reg = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
return reg.test(str);
}
function showChild(str){
   if(str==1){
     document.getElementById("ch").style.display="none";
 document.getElementById("na").style.display="none";
   }
   if(str==2){
     document.getElementById("ch").style.display="";
 document.getElementById("na").style.display="";
   }
}
function Checkbaoming(){
if(document.baoming.name.value==""){
alert("请填写您的姓名!");
document.baoming.name.focus();
return false;
}

if  (document.baoming.name.value.length>4){  
alert("请正确填写您的姓名");
document.baoming.name.focus();
return  false;
}

if(document.baoming.address.value==""){
alert("请填写您的联系地址!");
document.baoming.address.focus();
return false;
}

if(document.baoming.tel.value==""){
alert("请填写您的联系电话!");
document.baoming.tel.focus();
return false;
}

if(document.baoming.content.value==""){
alert("请填写您需要说明的地方!");
document.baoming.content.focus();
return false;
}

if(document.baoming.Verifycode.value==""){
alert("请填写验证码!");
document.baoming.Verifycode.focus();
return false;
}
if(!isNumber(document.baoming.tel.value)){  
alert("您的电话号码格式不正确！");
document.baoming.tel.focus();
return false;
}
if  (document.baoming.tel.value.length > 13){  
    alert("电话号码长度不正确！");
    document.baoming.tel.focus();
    return false;
}

if  (document.baoming.email.value.length > 0){  
if (!isEmail(document.baoming.email.value)){
    alert("电子邮箱格式不正确！");
    document.baoming.email.focus();
    return false;
 }
}
if  (document.baoming.qq.value.length > 0){  
if(!isNumber(document.baoming.qq.value)){ 
alert("qq号码格式不正确！"); 
document.baoming.qq.focus();
return false; 
} 
}
if  (document.baoming.content.valuelength > 240){
alert("备注说明字数不能超过240个字");
document.baoming.content.focus();
return  false;
}
return true;
}
</script> 
