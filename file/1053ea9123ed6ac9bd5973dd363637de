$(document).ready(function(){
	var li=$('#my1').children('li');
	var count=li.length;
	var one=Math.round(950/count);
	var son=Math.round((one-110)/2);
	$.each(li,function(i,val){
		li.eq(i).css({'width':one});
		li.find('ul').css({'left':son}); //作用：修正二级菜单的绝对定位问题
	});
})
</script>
