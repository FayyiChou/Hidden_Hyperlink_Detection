<script>
$(function(){
	$("#J_MainNav li h1").css('background-color','#0099ee');
	$("#J_MainNav li:eq(3)").css('background-color','#0082cb');
	$("#J_MainNav li h1").hover(function(){
		$(this).css('background-color','#0082cb'); 
	},function(){
		$(this).css('background-color','#0099ee'); 
	});
});

//字母筛选品牌
$("#letterfilter a").click(function(){
	var filterletter = $(this).attr('title');
	if(filterletter == 0){
		var brandobj = $("#sub_m_id a");
		$.each(brandobj, function(i, n){
			$(this).removeClass('display').css('display','block');
		});
		$("#manu_sub_id").css('display','none');
		$("#sub_m_id").css('display','block');
	}else{
		var brandobj = $("#sub_m_id a");
		$.each( brandobj, function(i, n){
		  if($(this).attr('title') != filterletter){
			 $(this).removeClass('display').css('display','none');
		  }else{
			 $(this).removeClass('display').css('display','block');
		  }
		});
		$("#manu_sub_id").css('display','none');
		$("#sub_m_id").css('display','block');
	}
});

function re_url_act(page,num,url){
  var b= document.getElementById('return_page').value;
  var a=page;
  if(a == '')
	a = 1;
  if (b<num){
	  document.location.href="http://driver.zol.com.cn/"+url+a+".html";
  } else {
	  alert("输入页数大于现有页面总数~！");
  }
  return false;
 }
$('.navBoxList').hover(function(){
	$(this).addClass('hover');
},function(){
	$(this).removeClass('hover');
});
$('.Graphics a').attr('target','_self')
</script>

