var w_h = jq(document).height(),
c_h = window.innerHeight,
h = w_h - c_h;
jq(".easy-guide, #um, #qmenu_menu").hide();
(g_p > 138 && h > 233) && a.css({position:"fixed",top:"0",zIndex:"199"});
}
setTimeout(function () {
g_t = g_p;
},0);
});
