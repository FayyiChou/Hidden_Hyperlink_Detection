                                    G_time = result.times;
                                    jQuery('.sign-body li:lt(' + result.times + ')').addClass('qd-icon');
                                    day.html(result.deldays);
                                    G_deldays = result.deldays;
                                } else {
                                    day.html(30);
                                }
                            } else {
                                tice.html('今日已签').css('backgroundPosition', '-5px -1114px');
                                jQuery('.sign-body li:lt(' + result.times + ')').addClass('qd-icon');
                                b.html(_html);
                                result.type == 'A' ? c.html("您已获得" + result.snum + "个金币") : c.html("您已获得" + result.snum + "天VIP");
                            }
                        }
                    }
                });
            }
