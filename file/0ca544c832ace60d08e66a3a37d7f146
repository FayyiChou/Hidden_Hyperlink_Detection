//查询Hr新简历数
function getHrNewJianliNum(){
		jQuery.ajax({ 
		     type: 'post',   
				contentType : 'application/json; charset=utf-8',   
				dataType: 'json',   
				url: '/ajax/newnum.do', 
				data: JSON.stringify(new Object()),   
				success: function(data){
				   if(data.num > 0){
				      $('.num').show();
				      $('.num').text(data.num);
				   }else{
				      $('.num').hide();
				   }
				}
		});
}

//查询C端未处理的消息数量
function getJobJianLiMessagenum(){
    var param = new Object();
    var total = 0;
    jQuery.ajax({   
        type: 'post',   
        contentType : 'application/json; charset=utf-8',   
        dataType: 'json',   
        url: '/ajax/getJobJianLiMessagenum.do', 
        data: JSON.stringify(param),   
        success: function(data){
           var data_np=data.np;
           if(data_np!=null && data_np!=""){
                total = data_np.num1*1.0+data_np.num2*1.0+data_np.num3*1.0+data_np.num4*1.0;
                if(total!=0){
                    $(".ctotalnum").show();
                    $(".ctotalnum").text(total);
                }
                $(".tdcnum").text(data_np.num1);
                $(".dgtnum").text(data_np.num2);
                $(".mstnum").text(data_np.num3);
                $(".bhsnum").text(data_np.num4);
           }
        }
    });
};
//获取用户的头像
function getUserPhoto(){
    jQuery.ajax({
        type: 'post',   
        contentType : 'application/json; charset=utf-8',   
        dataType: 'json',   
        url: '/ajax/getuserphoto.do', 
        data: JSON.stringify(new Object()),   
        success: function(data){
            var data_np=data.np;
            if(data_np!=null && data_np!=""){
                $(".yonghuming").find("img").attr("src",user_photo_url+'/'+data_np.photo);
            }
        }
    });
}

//登录结束 
