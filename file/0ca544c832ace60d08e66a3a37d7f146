function logout(){
    jQuery.ajax({   
        type: 'post',   
        contentType : 'application/json; charset=utf-8',   
        dataType: 'json',   
        url: '/ajax/logout.do', 
        data: JSON.stringify(new Object()),   
        success: function(data){
            window.location.href = "/";
        }
    })
}

