<script type="text/javascript">
	$(function(){
	    if(document.getCookie("changesiteflg")!='1'){
            $(".changesite").show();
            $(".pop_bg65").show();
        }
		$(".changesite").find("span").click(function(){
			$(this).parents(".changesite").hide();
			$(".pop_bg65").hide();
			document.setCookie("changesiteflg","1");
		})
	})
    window.Document.prototype.getCookie=function(name){
        var cvs=this.cookie;
        var values=cvs.split(";");
        var value="";
        for(var i=0;i<values.length;i++)
        {
             var cv=values[i];
             cv=cv.trim(cv);
             if(cv.indexOf(name)>=0) {
                var v = cv.substring(name.length + 1, cv.length);
                value=v;
                break;
            }
        }
        return value;
    }
</script></body>
</html>
