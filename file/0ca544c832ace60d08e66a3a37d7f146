$(document).ready(function(){
    $(".classpart-left a:nth-child(2n)").css({"width":"68px","margin-left":"20px"});

    $("._pel").load("/job/personalinformation");
    var xian="";
    if(xian==1){
    	$('#search_list').slideDown();
    }   
    
    $(".btn_ss").live("click",function(){
        var keywd = $("#keywd").val();
        if(keywd != "" && keywd != undefined)
        {
            window.location.href = BaseJSURL + "/job/?kwd="+keywd;
        }
    });

     $('.hr_sao span').click(function(){
		$(this).parent('.hr_sao').hide();
		$('.hsbj').hide();
	});
	//	友情链接tab
	$(".friendlinkcon").eq(0).show();
	$(".friendlinktab").find("span").click(function(){
	    headerloadflg="0";
	    var param = new Object();
        param.param1 = $(this).text();
        jQuery.ajax({
            type : 'post',
            contentType : 'application/json; charset=utf-8',
            dataType : 'json',
            url : BaseJSURL + '/seo/linkdata',
            data : JSON.stringify(param),
            success : function(data) {
                $('.friendlinkcon').html('');
                $('.friendlinkcon').html(template('youlian', data));
            }
        });
		if(borderflag='0'){
			$(this).addClass("borderactive");
		}
		if(borderflag='1'){
			$(this).siblings("span").removeClass("borderactive");
		}
		$(this).addClass("active");
		$(this).siblings().removeClass("active");
	})
  })


function jiazerwm()
{
    jQuery.ajax({   
            type: 'get',   
            contentType : 'application/json; charset=utf-8',   
            dataType: 'json',   
            url: '/qcode/bind', 
            data: '',   
            success: function(data){
                $('.hsbj').show();
                $("#erweima").attr("src","https://mp.weixin.qq.com/cgi-bin/showqrcode?ticket="+data.ticket);
                if(ukbn == "0")
                {
                    //c
                    $(".hr_sao").addClass("b_sao");
                }
                $('.hr_sao').show();
            }
    });
}
