<script>
$(function(){
    //首页和热招企业轮播
    $('.flexslider').flexslider({
        directionNav: true,//是否显示左右控制按钮
        pauseOnAction: true,  //暂停幻灯片的控制要素互动的时候
        pauseOnHover: true, //鼠标滑过是否停止轮播
        slideshow: true, //载入页面时，是否自动播放
        animationLoop: true, //循环播放
        slideshowSpeed: 3000, //展示时间间隔ms
    	animationSpeed: 500 //滚动时间ms
    
    });
    var jiaodiansize = "5";
    $(".flexslider").hover(function(){    	
        if(jiaodiansize >1){ 
            $(".flex-prev").animate({"left":"0"},200);
            $(".flex-next").animate({"right":"0"},200);
        }
    },function(){
        if(jiaodiansize >1){
            $(".flex-prev").animate({"left":"-40px"},200);
            $(".flex-next").animate({"right":"-40px"},200);
        }
    });
})
</script>
