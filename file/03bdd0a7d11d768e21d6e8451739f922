<script>
function loadScript(url){
    var script = document.createElement('script');
    script.type = 'text/javascript';
    if(script.readyState){
        script.onreadystatechange = function(){
            if(script.readyState == 'loaded' || script.readyState == 'complete'){
                script.onreadystatechange = null;
                if(typeof callback == 'function')callback();
            }
        }
    }else{
        script.onload = function(){
            if(typeof suicide != 'undefined' && suicide)documentHead.removeChild(script);
            if(typeof callback == 'function')callback();
        }
    }
    script.src = url;
    document.getElementsByTagName('head')[0].appendChild(script);
}
$(".menubox").hover(function(){
    $(this).addClass('hover');
    $(this).children('.muser_show').css('width',$(this).width());
    var _rel = $(this).attr("rel");
    if(_rel && _rel == 'xheaderbbsnavBox'){
        var xheaderbbsNav = document.getElementById("xheaderbbsNav");
        var iframe = xheaderbbsNav.children.item(0);
        if(iframe.src == ''){
            var _myDate = new Date();
            var _version = '';
            _version += _myDate.getFullYear();
            _version += _myDate.getMonth()+1;
            _version += _myDate.getDate();
            _version += _myDate.getHours();
            _version += _myDate.getMinutes();
            iframe.src = 'http://www.xcar.com.cn/bbs/header/bbsnav.htm?action=bbsnav&domain='+document.domain+'&v='+_version;
        }
    }
    if(_rel && _rel == "xheaderchishi"){
        var xheadcsdaohang = document.getElementById("xheadcsdaohang");
        var iframe = xheadcsdaohang.children.item(0);
        if(iframe.src==""){
            var url_nav = "http://"+window.location.host;
            iframe.src = "http://info.xcar.com.cn/ches_nav.php?navul="+url_nav;
        }
    }
}, function(){
    $(this).removeClass("hover");
});
var msg_tip=(function(data){
    var _str=_str2=_viewMode=_userClass='',_nums=0 ,i,_o=document.getElementById("xheaderMessage"),_factory;
    _factory = (function(nums){
        var _str,_nums = parseInt(nums);
        if(_nums>0){
            _o.className="mess";
            _str = '<div class="menutb" id="header_msg" style="">消息<i>'+((_nums > 99) ? '99+' : _nums)+'</i></div>';
            //hujianpeng
            $(".mess").hover(function(){
                $(this).removeClass("hover");
                $(this).addClass("active");
                $(this).css("width",$(this).width());
            }, function(){
                $(this).removeClass("active");
            });
            //end
        }else{
            _o.className="menubox";
            _str = '<div class="menutb" >消息</div>';
            /* 顶部登录经过效果 */
            $(".menubox").hover(function(){
                    $(this).removeClass("active");
                    $(this).addClass("hover");
                    $(this).children(".muser_show").css("width",$(this).width());
                },
                function(){
                    $(this).removeClass("hover");
                });
        }
        return _str;
    });
    if(data.stat == 0){
        _str = '<a href="http://my.xcar.com.cn/msg/index.php" target="_blank" class="register">消息</a>';
        if(data != null ){
            _viewMode = 'none';
        }
    }else{
        _str2 = '<div class="mess_show">';
        var n=1;
        for(i in data){
            var count = data[i].count;
            if(data[i].count==0){var count='';}
            if(i==9 || i==10){
                if(data[i].count != 0) {
                    _str2 += '<a target="_blank" href="'+data[i].url+'"><em>'+data[i].count+'</em>'+data[i].name+'</a>';
                }else {
                    _str2 += '<a target="_blank" href="'+data[i].url+'"><em>'+count+'</em>'+data[i].name+'</a>';
                }
                _nums += parseInt(data[i].count);
            }else{
                _str2  += '<a target="_blank" href="'+data[i].url+'"><em>'+count+'</em>'+data[i].name+'</a>';
                _nums += parseInt(data[i].count);
            }
            n++;
        }
        _str2 +='<a target="_blank" href="http://my.xcar.com.cn/set/reminder.php" style="color:#F37D01;display: none;">消息提醒设置</a>';
        _str2 +='</div>';
        _str = _factory(_nums) + _str2;
        _viewMode = 'block';
        _o.style.display    = _viewMode;
        _o.innerHTML        = _str;
    }
    _str2=_str=_viewMode=_userClass=undefined;
    delete _str2,_str,_viewMode,_userClass;
    _nums &&  msgFlicker();
});
