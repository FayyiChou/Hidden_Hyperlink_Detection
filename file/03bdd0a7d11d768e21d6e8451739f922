/*searchForm*/
$('#search').bind('click',function(event){
    var word = $.trim($('#k').val());
    if(word == '' || word == '请输入关键字'){
        $('#k').focus();
        return false;
    }
    $('#k').next().hide();
    $('.checktype').parent('div').hide();
    $('#searchForm').submit();
    $('#k').val('请输入关键字');
    $('#k').blur();
});
$('.serach_word').live('click',function(){
    $('.checktype').parent('div').hide();
    $('#searchForm').submit();
    $('#k').val('请输入关键字');
});

$('.input_w178').bind('keydown',function(e){
    var e = e || window.event;
    var kc = e.which || e.keyCode;
    if(kc == 13){
        $('#k').blur();
        $('#search').trigger('click');
        return false;
    }
});


