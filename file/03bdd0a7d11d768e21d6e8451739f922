/*品牌车系*/
var xsubsel = new xcar.DivSelectbox('header_select_ps', function(v){});
var xgobbs_select = new xcar.DivSelectbox('header_select_pb', function(v){
    if(ps_arr_bbs[v]){
        var bdtypes =ps_arr_bbs[v].split(',');
        xsubsel.reset('选择车系');
        xsubsel.init(bdtypes, function(data) {
            var len = data.length, str='', fid=0;
            for (var i=0; i<len; i+=2) {
                fid = parseInt(data[i]);
                if(fid>=10000) {
                    str += '<li class="tit" value="0" onclick="xcar.stopEvent(event)" style="cursor:default"><a style="text-decoration:none;font-weight:bold;color:gray;cursor:default;">'+ data[i+1].replace(/[=\s]+/g,'') +'</a></li>';
                }else if(fid !== 0){
                    str += '<li value="' +fid+ '"><a>'+ data[i+1] +'</a></li>';
                }
            }
            return str;
        });
    }
});

xgobbs_select.initlazy("http://html.xcar.com.cn/newcar/pub_js/car_arr_newcar_2012_bbs.js", function() {
    var brands = pb_arr_bbs.split(','), brands_kv=[],
        len = brands.length, len2=0, str='', cx=null, hf=false,i,j;
    for(i=0;i<len;i+=2) {
        brands_kv.push({'k':brands[i],'v':brands[i+1]});
    }
    brands_kv.sort(function(a,b){return a.v.localeCompare(b.v);});
    len=brands_kv.length;
    for (i=0,j=0; i<len; ++i) {
        cx=ps_arr_bbs[brands_kv[i].k].split(',');
        hf=false; len2=cx.length;
        for (j=0; j<len2; j+=2) {
            if(cx[j] !=='0') {
                hf=true;
                break;
            }
        }
        if(hf) {
            str += '<li value="' +brands_kv[i].k+ '"><cite>' + brands_kv[i].v.charAt(0) +'</cite><a>'+ brands_kv[i].v.substring(2) +'</a></li>';
        }
    }
    return str;
});

$('#header_gobbs').bind('click',function(){
    var subsel = $('.selectem', document.getElementById('header_select_ps'))[0],
        fid = subsel.getAttribute('value');
    if(!xcar.bool(fid)) {
        var selctl = $('.selectem', document.getElementById('car_to_bbs_pb'))[0],
            bid = selctl.getAttribute('value');
        fid = xcar.brand2forum[bid];
    }
    if(fid > 0){
        window.open('http://www.xcar.com.cn/bbs/forumdisplay.php?fid='+fid+'&zoneclick=101863');
    }else{
        if( ! parseInt($('#header_select_pb').find('.selectem').attr('value'))){
            alert('请选择品牌');
            return false;
        }else if( ! parseInt($('#header_select_ps').find('.selectem').attr('value'))){
            alert('请选择车系');
            return false;
        }
    }
});

$('#k').bind('keyup',function(e){
    var kc = e.which || e.keyCode;
    var word = $.trim($('.input_w178').val());
    var $ul = $('.checktype');
    var len = $ul.find('li').length;
    var s_index = 0;
    if(kc == 38 && len){//up

        s_index = $ul.find("li[class='active']").index();
        if(s_index == -1){
            $ul.find("li:last").attr('class','active').siblings('li').removeAttr('class');
        }else{
            window.console && console.log(s_index);
            --s_index;
            window.console && console.log(s_index);
            $ul.find("li").eq(s_index).attr('class','active').siblings('li').removeAttr('class');
        }
        $('#k').val($ul.find("li[class='active']").find('a').html());
        return false;
    }else if(kc == 40 && len){//down
        s_index = $ul.find("li[class='active']").index();
        window.console && console.log(s_index);
        if(s_index == len - 1){
            $ul.find("li:first").attr('class','active').siblings('li').removeAttr('class');
        }else{
            window.console && console.log(s_index);
            ++s_index;
            window.console && console.log(s_index);
            $ul.find("li").eq(s_index).attr('class','active').siblings('li').removeAttr('class');
        }
        $('#k').val($ul.find("li[class='active']").find('a').html());
        return false;
    }else if(kc == 13){

        return false;

    }
    if(word == ''){
        $('.checktype').html('').parent('div').hide();
    }else{
        $.getJSON("http://tools.xcar.com.cn/suggest/suggestutf8.php?k="+word+"&callback=?",
            function(data){
                if(data){
                    var str = '';
                    for(var s in data){
                        str += '<li><a class="serach_word" href="javascript:;">'+data[s]+'</a></li>';
                    }
                    if(str){
                        $('.checktype').html(str).parent('div').show();
                        $('.checktype > li').bind('click',function(){
                            $('.input_w178').val($(this).find('a').html());
                            $(this).parents("div[class='option']").hide();
                        });
                    }
                }
            }
        );
    }
});







$(document).click(function(event){   //by figo 2014 11 24
    var eo=$(event.target);
    if($(".selectbox").is(":visible") && eo.attr("class")!="option" && !eo.parent(".option").length&&eo.attr('id')!='k'){
         //隐藏月日option


        //$(this).next().hide();
       // var kc = e.which || e.keyCode;
        var word = $.trim($('.input_w178').val());
        var $ul = $('.checktype');
        var len = $ul.find('li').length;
        var s_index = 0;
        if(word == ''){
            $('.checktype').html('').parent('div').hide();
        }else{
            $.getJSON("http://tools.xcar.com.cn/suggest/suggestutf8.php?k="+word+"&callback=?",
                function(data){
                    if(data){
                        var str = '';
                        for(var s in data){
                            str += '<li><a class="serach_word" href="javascript:;">'+data[s]+'</a></li>';
                        }
                        if(str){
                            $('.checktype').html(str);
                            $('#k').next().hide();
                        }
                    }
                }
            );
        }

    }

});



$('#k').focus(function(){//增加获取焦点显示opntion by Figo
    if($('#k').val()!=''&&$('#k').val()!='请输入关键字'){
        $(this).next().show();
    }
})


/*意见反馈弹框*/
$(function(){
    $('#CollectOpinions').click(function(){
        var iframe_src = $("#CollectIfrm").attr("src");
        if(iframe_src=="#"){
            $("#CollectIfrm").attr("src","http://my.xcar.com.cn/interface/cms_feedback.php?style=cms&amp;host=club.xcar.com.cn&v=5");
        }
    });
})




/*品牌车系*/
var timer;
function msgFlicker(){
    obj = document.getElementById('header_msg');
    if(obj.className == 'menutb glint'){
        obj.className = 'menutb'
    }else{
        obj.className = 'menutb glint'
    }
    timer = setTimeout("msgFlicker()",500)
}
