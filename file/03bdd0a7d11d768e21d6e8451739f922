<script language="javascript">
$(document).ready(function(){
    $("img").lazyload({
        placeholder : "/images/default_218x162.gif",
        effect      : "fadeIn"//show(直接显示),fadeIn(淡入),slideDown(下拉)
    });
    /* 下拉插件JS */
    $(".selectbox").hover(
        function () {
            $(this).addClass("selectbox_hover");
        },
        function () {
            $(this).removeClass("selectbox_hover");
        }
    );
    $('.option li').live('mouseenter',function(){
        $(this).addClass("active");
    }).live('mouseleave',function(){
            $(this).removeClass("active");
        });
    $(".selecttxt_yue").click(function(event){
//停止冒泡
        event.stopPropagation();
        /* 定义下拉内容最大高度 */
        if($(this).siblings(".option").height()>=300){
            $(this).siblings(".option").height(300);
        }else{
            $(this).siblings(".option").height('auto');
        }
//判断当前状态如果打开则关闭
        if($(this).parent().is('.selectbox_on')){
            $(this).parent().find(".option").hide();
            $(this).parent().removeClass("selectbox_on");
            return;
        }
//普通点击事件
        $(".selectbox").removeClass("selectbox_on");
        $(this).parent().addClass("selectbox_on");

        $(".selectbox").find('.option').hide();
        $(this).parent().find(".option").show();
    });
    $(".option li").live('click',function(){
        var text_val=$(this).text();
        if(text_val.indexOf('年')>=0){
            return false;
        }else{
            if($(this).children("a").text()!=''){
                var value=$(this).children("a").text();
                $(this).parents(".option").siblings(".selecttxt").children(".selectem").text(value);
                $("#select_value").val(value);
                $('.option').hide();
                $(".selectbox").removeClass("selectbox_on");
                $(this).parents(".option").siblings(".selecttxt").css("color","#666");
            }
        }
    })
    /* 下拉插件JS end */
});
$(function(){

    var is_all = '1';
    var year=new Date();
    year=year.getFullYear();
    var val_start=$('.change_width').find('a').eq(0).attr('href');
    var val_end=$('.change_width').find('a').eq(6).attr('href');
    var val_start_arr=val_start.split("/");
    var val_end_arr=val_end.split("/");
    var val_start_2=val_start_arr[2];
    var val_end_2=val_end_arr[2];
    var compare1='20170402';
    var compare2='20130903';
    //日期翻页
    var browser=navigator.appName;
    var panduan_hide='style="display:none;"';
    if(browser=="Microsoft Internet Explorer"){
        var b_version=navigator.appVersion;
        var version=b_version.split(";");
        var trim_Version=version[1].replace(/[ ]/g,"");
        if(browser=="Microsoft Internet Explorer" && trim_Version=="MSIE8.0")
        {
            panduan_hide='';
        }
    }
    $('.next_page,.last_page').live('click',function(){
        var var1=$(this).attr("class");
        var var2=var1.split(' ');
        var cla=var2[1];
        if(cla=='last_page'&&compare1==val_start_2){
            return false;
        }else if(cla=='next_page'&&compare2==val_end_2){
            return false;
        }
        if($('.now_year').val()!=0){
            window.console&&console.log($('.now_year').val());
            year=$('.now_year').val();
        }
        var new_5str='';
        var d='1490558320000';
        var fenye_riqi = $(".all_width").find("a").eq(0).text();
        fenye_riqi=fenye_riqi.replace("日","");
        fenye_riqi=fenye_riqi.replace("月","|");
        var fenye_arr=fenye_riqi.split('|');
        var date = new Date(year,fenye_arr[0]-1,fenye_arr[1]);
        var miao=date.getTime();
        var riqi_arr=[];
        var new_5=[];
        var new_5_arr=[];
        var new_url=[];
        if(is_all==1){
            var active='class="sort_active"';
        }else if(is_all==2){
            var active='';
        }

        if(miao<1379174400001&&cla=='next_page'){//1378310400000
            new_5str+='<a href="/selected/20130909/">09月09日</a><a href="/selected/20130908/">09月08日</a><a href="/selected/20130907/">09月07日</a><a href="/selected/20130906/">09月06日</a><a href="/selected/20130905/">09月05日</a><a href="/selected/20130904/">09月04日</a><a href="/selected/20130903/">09月03日</a>';
        }else if(miao>d&&cla=='last_page'){
            var today = new Date();
            today.setHours(0);
            today.setMinutes(0);
            today.setSeconds(0);
            today.setMilliseconds(0);
            for(i=0;i<7;i++){
                var f_date =i*1000 * 60 * 60 * 24;
                var zday= new Date(today - f_date);
                var z_year=zday.getFullYear();
                var z_yue=zday.getMonth()+1;
                var z_ri=zday.getDate();

                z_yue=z_yue.toString();
                if(z_yue.length==1){
                    z_yue='0'+z_yue;
                }
                z_ri=z_ri.toString();
                if(z_ri.length==1){
                    z_ri='0'+z_ri;
                }
                z=z_year+z_yue+z_ri;
                z2=z_yue+'月'+z_ri+'日';
                new_5str+='<a href="/selected/'+z+'/" >'+z2+'</a>';
            }
        }else{
            var z=7;
            for(var i= 0;i<7;i++){
                var j=i+7;
                var _year='';
                var _yue='';
                var _ri='';
                if(cla=='next_page'){
                    _year=new Date(miao-j*24*3600*1000).getFullYear();
                    _yue=new Date(miao-j*24*3600*1000).getMonth()+1;
                    _ri=new Date(miao-j*24*3600*1000).getDate();
                }else{
                    _year=new Date(miao+z*24*3600*1000).getFullYear();
                    _yue=new Date(miao+z*24*3600*1000).getMonth()+1;
                    _ri=new Date(miao+z*24*3600*1000).getDate();
                }
                z--;
                var _yue2='';
                var _ri2='';
                _yue=_yue.toString();
                if(_yue.length==1){
                    _yue2='0'+_yue;
                }else{
                    _yue2=_yue;
                }
                _ri=_ri.toString();
                if(_ri.length==1){
                    _ri2='0'+_ri;
                }else{
                    _ri2=_ri;
                }
                new_5[i]=_yue2+'月'+_ri2+'日';
                new_5_arr[i]=_year+_yue2+_ri2;
                if(i==0){
                    form_time_str=_year+''+_yue2+''+_ri2;
                    new_url[i]='/selected/'+form_time_str+'/';
                }else{
                    var linshi=_year+''+_yue2+''+_ri2;
                    new_url[i]='/selected/'+linshi+'/';
                }
                if(i==0){
                    $('.now_year').val(_year);
                }
                new_5str+='<a href="'+new_url[i]+'" '+panduan_hide+'>'+new_5[i]+'</a>';
            }

        }
        $('.change_width').children('a').remove();
        $('.change_width').prepend(new_5str);
        $('.change_width').children('a').animate({
            opacity: "show"
        }, "slow");

        val_start=$('.change_width').find('a').eq(0).attr('href');
        val_end=$('.change_width').find('a').eq(6).attr('href');
        val_start_arr=val_start.split("/");
        val_end_arr=val_end.split("/");
        val_start_2=val_start_arr[2];
        val_end_2=val_end_arr[2];
        if(val_start_2==compare1){
            $(".tab_right").removeClass('tab_right_hover');
            $(".tab_right").unbind('mouseenter mouseleave');
            $(".tab_right").css("cursor","default");
        }else{
            $(".tab_right").bind('mouseenter mouseleave');
            $('.tab_right').hover(function(){
                $(this).addClass('tab_right_hover');},function(){
                $(this).removeClass('tab_right_hover');
            })
            $(".tab_right").css("cursor","pointer");
        }
        if(val_end_2==compare2){
            $(".tab_left").removeClass('tab_left_hover');
            $(".tab_left").unbind('mouseenter mouseleave');
            $(".tab_left").css("cursor","default");
        }else{
            $(".tab_left").bind('mouseenter mouseleave');
            $('.tab_left').hover(function(){
                $(this).addClass('tab_left_hover');},function(){
                $(this).removeClass('tab_left_hover');
            })
            $(".tab_left").css("cursor","pointer");
        }

    })
    //$(".tab_right").unbind('mouseenter mouseleave');
    //左右箭头颜色

    if(val_start_2!=compare1){
        $('.tab_right').hover(function(){
            $(this).addClass('tab_right_hover');},function(){
            $(this).removeClass('tab_right_hover');
        })
    }else{
        $(".tab_right").css("cursor","default");
    }
    if(val_end_2!=compare2){
        $('.tab_left').hover(function(){
            $(this).addClass('tab_left_hover');},function(){
            $(this).removeClass('tab_left_hover');
        })
    }else{
        $(".tab_left").css("cursor","default");
    }
    $('.select_box').click(function(){
        if($('.select_pop').css('display')=='none'){
            $('.select_pop').show();
            $(this).addClass("select_box_on");
        }else{
            if($('#SMonth').css('display')=='block'){
                $('.yue').trigger('click');
            }
            if($('#SDay').css('display')=='block'){
                $('.ri').trigger('click');
            }
            $('.select_pop').hide();
            $(this).removeClass("select_box_on");
            $(this).addClass("selectbox_on");
        }
    })
    //点击空白
    $(function($) {//by Figo
        $(document).click(function(event){
            var eo=$(event.target);
            if(eo.text()!='切换日期'&&eo.parent().parent().attr('class')!='SMonth'&&eo.parent().parent().attr('class')!='SDay'&&eo.parent().attr('class')!='SDay'&&eo.parent().attr('class')!='SMonth'){
                if($('.select_pop').css('display')=='block'){
                    $('.select_reight').find('.select_box').removeClass('select_box_on');
                    $('.select_pop').hide();
                }
                if($('#SMonth').css('display')=='block'){
                    $('.yue').trigger('click');
                }
                if($('#SDay').css('display')=='block'){
                    $('.ri').trigger('click');
                }
            }
        });
    });

    $('.sift_img').parent().hover(
        function(){
            $(this).find('.data_pop').show();
            $(this).addClass('bghover');
        },function(){
            $(this).find('.data_pop').hide();
            $(this).removeClass('bghover');
        }
    )

    //确定按钮变色
    $('.SDay li').live('click',function(){
        $('.input_b68').removeClass('default_but');
    })


    $('.option').find('li').click(function(){
        var text_val2=$(this).text();
        if(text_val2.indexOf('年')>=0){
            return false;
        }
        var id =$(this).attr('id');
        var cl =$(this).attr('class');
        cl=cl.split(" ");
        cl=cl[0];
        $(this).parent().parent().siblings().find('.yincang').val(id)
        $(this).parent().parent().siblings().find('.yincang2').val(cl);

        var yearid=$('.yincang2');
        var monthid = $('.yincang');
        var dayid = $('.SDay');    //天所在的控件

        var yearValue = yearid.val()
        var monthValue = parseInt(monthid.val())
        var dayvalue ;
        dayid.html('');


        if(monthValue==1||monthValue==3||monthValue==5||monthValue==7||monthValue==8||monthValue==10||monthValue==12)   {
            dayvalue = 31
        }else if(monthValue==4||monthValue==6||monthValue==11||monthValue==9)    {
            dayvalue = 30
        }else if(monthValue==2){

            if(yearValue%4==0 && (yearValue%4 !=0 || yearValue%400==0)){ //闰年
                dayvalue = 29
            }else{
                dayvalue = 28
            }
        }
        if(yearValue==2013&&monthValue==9){
            for(var i=3;i<=30;i++){
                dayid.append('<li class=""><a href="javascript:void(0)">'+dayvalue+'日</a></li>');
                dayvalue--;
            }
        }else{
            var myDate = new Date();
            window.console&&console.log(year);
            window.console&&console.log(myDate.getMonth());
            window.console&&console.log(myDate.getDate());

            if(yearValue==year&&monthValue==myDate.getMonth()+1){
                for(var i=1;i<=myDate.getDate();i++){
                    f=myDate.getDate()-(i-1);
                    dayid.append('<li class=""><a href="javascript:void(0)">'+f+'日</a></li>');
                }
            }else{
                for(var i=1;i<=dayvalue;i++){
                    f=dayvalue-(i-1);
                    dayid.append('<li class=""><a href="javascript:void(0)">'+f+'日</a></li>');
                }
            }

        }
    })

    $('.input_b68').click(function(){
        if($(this).hasClass('default_but')){}else{
            if($('.yincang').val()==''||$('.ri').text()=='日期'){
                alert('请完整填写日期');
            }else{
                var yue=$('.yincang').val();
                var ri=$('.ri').text();
                ri=ri.replace('日','');
                var year=$('.yincang2').val();

                yue=yue.toString();
                if(yue.length==1){
                    yue2='0'+yue;
                }else{
                    yue2=yue;
                }
                ri=ri.toString();
                if(ri.length==1){
                    ri2='0'+ri;
                }else{
                    ri2=ri;
                }
                var riqi=year+''+yue2+''+ri2;

                location.href='/selected/'+riqi+'/';
            }
        }
        $(".tab_right").bind('mouseenter mouseleave');
        $(".tab_left").bind('mouseenter mouseleave');
        $(".tab_right").css("cursor","pointer");
        $(".tab_left").css("cursor","pointer");
    })





});
</script>
<!--顶部浮动JS-->
<script type="text/javascript">

    var boxHeight;

    var startPos;

    function startbox(div,box,classN){

        var elm = $(div);

        //classN_width = $(".classN").width();

        $(window).scroll(function(){
            $('.select_pop').hide();
            $('.select_box').removeClass("select_box_on");
            //if(!startPos){

            startPos=$(box).offset().top;

            //}

            var p = $(window).scrollTop();

            if (startPos < p)

            {

                $(elm).addClass(classN);

            }

            else

            {

                $(elm).removeClass(classN);

            }

        });

    }

    startbox('#sml_title','#sml_box','twfir');

</script>
<!-- /顶部浮动JS end-->
