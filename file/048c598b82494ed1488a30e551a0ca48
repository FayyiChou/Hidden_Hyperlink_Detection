<script type="text/javascript">
function showBox_timeup(id)
{
	id = '#'+id;
	var scrollTop = $(window).scrollTop();
	var scrollLeft = $(window).scrollLeft();
	var browerWidth = $(window).width();
	var browerHeight = $(window).height();
	var currentWidth = $(id).outerWidth(true);
	var currentHeigth = $(id).outerHeight(true);
	var left = scrollLeft + (browerWidth - currentWidth)/2;
	var top = scrollTop + (browerHeight - currentHeigth)/2;
	$(id).css({
		top:top,
		left:left,
		display:'block'
	});
}

$(window).resize(
	function()
	{
		if ($('#timeup_box').css('display') == 'block')
		{
			showBox_timeup('timeup_box');
		}
	}
);

$(document).ready(function() {
	if ('schedule' == location.href.split('#')[1]) {
		showBox_timeup('timeup_box');
		location.href =  location.href.replace('#schedule', '#');
	}

});
</script>
