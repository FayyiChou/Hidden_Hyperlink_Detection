switchPostBox();

function switchPostBox() 
{/*{{{*/
   var patientsActiveLists = $(".patientsActiveList > li");
   var patientsActiveContents = $(".patientsActiveContent");
   patientsActiveLists.bind("click",function(){
       $('#inputform_3_skill').attr('value', '-2');
       $('#inputform_3_attitude').attr('value', '-2');
        var index= $(this).index();
        $(this).addClass("active").siblings().removeClass("active");
        if(index==1){
            patientsActiveContents.eq(1).show().siblings().hide();
        }else{
            patientsActiveContents.eq(0).show().siblings().hide();
        }
        if ($(this).hasClass("thankLetters"))
        {
            document.getElementById('isThankLetter').value = 'true';
            document.getElementById('comment_type').value = '1';
            document.getElementById('content_fix_0').value="";
            document.getElementById('comment_desc').innerHTML = "感谢信...";
            document.getElementById('comment_content_type').innerHTML = "感谢信";
            $('#content_fix_0').removeClass("content_type_process");
            $('#content_fix_0').addClass("content_type_thank");
            $('.displeasure').attr('disabled', 'true');
            $('#comment_remedy').css('display', 'none');
            var defaultText = "请注意：\n1、给帮助过你的医生送去真诚的感谢；\n2、相同内容的感谢信无需重复发表；\n3、需要咨询的请到大夫个人网站，此处不发表咨询内容。";
            $("#content_fix_0").attr("defaulttext", defaultText);
            $("#content_fix_0").blur();
        }
        else if($(this).hasClass("doctorExper"))
        {
            document.getElementById('isThankLetter').value = 'false';
            document.getElementById('comment_type').value = '3';
            document.getElementById('content_fix_0').value="";
            document.getElementById('comment_desc').innerHTML = "看病经验...";
            document.getElementById('comment_content_type').innerHTML = "看病过程";
            $('#content_fix_0').removeClass("content_type_thank");
            $('#content_fix_0').addClass("content_type_process");
            $('.displeasure').removeAttr("disabled"); 
            $('#comment_remedy').css('display', 'block');
            var defaultText = "请注意：\n1、只有您在黎达均大夫处就医后，才能发表看病经验，相同内容无需重复发表；\n2、分享看病经验要求公正、客观、详实。涉及“医疗纠纷”的，需提供法院已生效的法律文书，经审核，对事实清楚、证据充分，合法有效的，可予以发表。\n3、无事实根据，发布损毁医生名誉的不实言论，发布者需承担相应法律责任；\n4、需要咨询的请到大夫个人网站，此处不发表咨询内容。";
            $("#content_fix_0").attr("defaulttext", defaultText);
            $("#content_fix_0").blur();
        }
    })
    
    check_skill_attitude();
}/*}}}*/

//看病过程,如果有选不满意则弹出联系方式让填写
function check_skill_attitude()
{/*{{{*/
    $('#userrealname').css('display', 'none');
    $('#usertelnumber').css('display', 'none');
    $('#thanks_notice').css('display', 'none');
    if(needMoreInfo())
    {
        $('#userrealname').css('display', 'block');
        $('#usertelnumber').css('display', 'block');
        $('#thanks_notice').css('display', 'block');
    }
}/*}}}*/

function needMoreInfo()
{/*{{{*/
    var trustIndex = 100;
    var minTrustIndex = 98;
    var selSkillValue = $('#inputform_3_skill').val();
    var selAttitudeValue = $('#inputform_3_attitude').val();

    return (null == hdf.get_cookie('userinfo[id]') && (trustIndex < minTrustIndex || selSkillValue == 0 || selAttitudeValue == 0));
}/*}}}*/

function showDetail(showid, hiddenid)
{
    $('#'+showid).show();
    $('#'+hiddenid).hide();
}
</script>
<script type="text/javascript">
$(document).ready(function(){
    $.ajax({
        type: "post",
            url:"/api/doctor/ajaxshowdoctorcollect",
            data: {
                'id' : 98792            },
    success: function(data){
        if (data)
                {
                                            $('#ajaxcollecdoctor').html(data)
                                    }
            }
    });
});

    $(function () 
    {
        $("#ajaxcollecdoctor").click(function () 
        {
            $.ajax({
                type: "post",
                data: {
                    'userid' : 0,
                      'type' : 5, 
                    'relatedid' : 98792                    },
                    url:"/api/index/ajaxcollect",
                    success: function(data){
                        var data = eval('('+data+')');
                        if (data.success == "1")
                        {
                            $('#ajaxcollecdoctor').html("<a class='favs ml30' style='top: -3px;' ><span class='ok'></span>已收藏</a>")
                        }
                    }
                });
        })
    })
</script>

