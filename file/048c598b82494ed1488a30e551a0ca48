<script type="text/javascript">
//******************* sickness list which consists of buttonlike link  *************************
function toggleSicknessList(){
    $(".kbjy_tabbox .rightarrow").click(function(){
        if(!$(this).hasClass("rightarrow2")){
            $(this).html("收起");
        }else{
            $(this).html("更多");
        }
        $(this).toggleClass("rightarrow2").parents(".tabmain").toggleClass("tabshowall");
    })
}

$(document).ready(function(){
    var tabHeight = $("#tabmainin").height();
    if(tabHeight < 40){
        $(".rightarrow").hide();
    }
    toggleSicknessList(); 
});
