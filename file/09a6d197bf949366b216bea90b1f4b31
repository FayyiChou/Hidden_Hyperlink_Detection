        <script type="text/javascript">
            var url = window.location.href;
            if(url.indexOf("?") > -1 && url.indexOf("=") > -1){
                var flag1 = url.split("?")[1].split("=")[0];
                var flag2 = url.split("?")[1].split("=")[1];
