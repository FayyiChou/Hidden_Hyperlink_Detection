            success: function (res) {
            }
        });
    }
    var mobileUrl =  "/Circle/IsMobile/1-"+ CircleId;
    //打开APP
    function OpenApp() {
        //location.href="http://a.app.qq.com/o/simple.jsp?pkgname=com.szhome.dongdong";
        var ua = navigator.userAgent;
        if (ua.toLowerCase().indexOf("like mac os x") > 0) {
            var regStr_saf = /os [\d._]*/gi ;
            var verinfo = ua.match(regStr_saf) ;
            var version = (verinfo+"").replace(/[^0-9|10|_.]/ig,"").replace(/_/ig,".");
            var version_str = version+"";
            if(version_str == "undefined" && version_str.length ==0){
                location.href =mobileUrl;
            }  
            version=version.substring(0,1);
            if(version <9 && version != 1){
                location.href =mobileUrl;
            }else{
                location.href = "https://deeplink.app.szhome.com/DongDong/DongCircleDetail?CircleId=" + CircleId +"&return=" + encodeURIComponent("http://dongdong.szhome.com" +mobileUrl);
            }
        }
        else if(ua.match(/(Android);?[\s\/]+([\d.]+)?/)){
            location.href = "/Circle/IsMobile/0-"+ CircleId;
        }else{
            location.href = mobileUrl;
        }
    }
