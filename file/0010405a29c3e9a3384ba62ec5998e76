function changeImg(url,picname)
{var bigimg = document.getElementById("myimg");
var pichref = document.getElementById('pichref');
var  obj= document.getElementById('ad_text');
obj.innerHTML=picname;
pichref.href = url;
bigimg.src=url;
}
