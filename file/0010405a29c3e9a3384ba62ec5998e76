       <script type="text/javascript">  
    //变量
    var reqName;
    var reqContact;
    var reqContent;
 
    $("body").bind('click',function(){
        closeQuestionArea();
    });
    //打开需求项
    function openQuestionArea(e) {
        $("#side_rent").css({ 'height': "235px" });
        $("#side_rent").addClass("shadow");
        $(".form_nav").removeClass("div_none");
        stopBubble(e);
    }
    //关闭需求项
    function closeQuestionArea() {
        $("#side_rent").css({ 'height': "167px" });
        $("#side_rent").removeClass("shadow");
        $(".form_nav").addClass("div_none");
    }
    //停止事件冒泡
    function stopBubble(e) {
        if (e && e.stopPropagation)
            e.stopPropagation();
        else
            window.event.cancelBubble = true;
    }
    //输入框提示
    $(function () { 	
        InitTipsInput();
    });  
</script> 
       <script language="JavaScript">
function check()
{
 if(document.form25.content.value=="请在这里写下您的寻租需求，我们将在1个工作日内与您联系。（例如，黄浦区，近地铁，6块之内）") {
	document.form25.content.focus();
    alert("请在这里写下您的寻租需求，我们将在1个工作日内与您联系。（例如，黄浦区，近地铁，6块之内）！");
	return false;
  }
 if(document.form25.companyname.value=="请输入您的公司名称") {
	document.form25.companyname.focus();
    alert("请输入您的公司名称！");
	return false;
  }
  if(document.form25.phone.value=="请输入您的联系方式") {
	document.form25.phone.focus();
    alert("请输入您的联系方式！");
	return false;
  }
}

 
</script> 
