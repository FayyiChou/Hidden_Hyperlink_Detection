<script>
var login_flag = false;
$(function(){
    $.get("/foreuser_web/checklogin/", function(data){
        obj = eval('('+data+')');
        if(obj.status == 1){
            $("#login_block").css('display', '');
            $("#nickname_flag").html(obj.nickname);
            $("#loginout_block").css('display', 'none');
            login_flag = true;
            if(obj.headpic){
                $("#eUserpic img").attr('src',obj.headpic);
            }
        }
    });
	GetAjaxShareInfo();
});
function GetAjaxShareInfo(){
	$.ajax({
		type : 'POST',
		cache : false,
		dataType : 'JSON',
		url  : SLPGER.root + 'home/GetAjaxShareInfo/',
		data : {
			free : 1
		}, //form表单序列化
		success:function(result){ //用ajax方法发送信息到当前Action中的main方法
			if(result.status == 1){
				$(".b_cen").html(result.data);
				//登陆后			
				$(".djzk").toggle(function(){
					$(this).html("点击收起");
					$('.sykc').show();
					$(".mylessons").show();
				},function(){
					$(this).html("点击展开");
					$('.sykc').hide();
					$(".mylessons").hide();
				});
			}
		}
	});	
}
</script>

