<script type="text/javascript">
function check_vote_1() {
	var inputs = $('vote_1').getElementsByTagName('input');
	var inputs_length = inputs.length;
	var selected = 0;
	for(var i = 0; i < inputs_length; i++) {
		if(inputs[i].type == 'checkbox' && inputs[i].checked == true) selected++;
	}
		if(selected < 1) {
		alert('最少需要选择1个投票选项');
		return false;
	}
	if(selected > 3) {
		alert('最多可选择3个投票选项');
		return false;
	}
		return true;
}
