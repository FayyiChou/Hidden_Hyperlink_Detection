  <script>
function checkForm_sou_eol()
{
if(document.getElementById('q_eol').value == '请输入关键词')
{ 
document.getElementById('q_eol').value = '中国教育在线';
}
document.charset='utf-8';
document.forms["w_eol"].action="http://sou.eol.cn/search.s?all="+encodeURI(document.getElementById("q_eol").value)+"&type=facet&selectStyle=cat&facet.field=keywords";

}

function ches_sou_eol(){
if(event.keyCode==13){
 document.charset='utf-8';
 document.forms["w_eol"].action="http://sou.eol.cn/search.s?all="+encodeURI(document.getElementById("q_eol").value)+"&type=facet&selectStyle=cat&facet.field=keywords";
return false;
}
}
