	function generateNewArray(array, newExtension){
		var newArray = [];
		for (var item in array) {
			var indice = array[item].lastIndexOf(".");
			newArray.push(array[item].substr(0,indice)+"."+newExtension);
		}
		return newArray;
	}
	photos = generateNewArray(vids, "jpg");
	BV.init();
	BV.showPlaylist(vids,{ambient:true});
