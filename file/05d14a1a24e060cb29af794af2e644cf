	var default_speed = 1500;
	var default_transition = 1; // 0-none, 1-fade, 2-slide down, 3-slide left, 4-slide top, 5-slide right, 6-blind horizontal, 7-blind vertical, 90-slide right/left, 91-slide top/down (only 1-fade allows tinyscrollbar)

	// PRELOAD FIRST IMAGE BEFORE EXECUTE SUPERBGIMAGE
	if($('#sb-superbgimage a').length > 1) {
		var superbgimage_first_one = new Image();
		superbgimage_first_one.onload = function() {
			$('#sb-superbgimage').superbgimage();
			mueveHome();
		}
		superbgimage_first_one.src = $('#sb-superbgimage a:first').attr('href');
	}

	// SLIDESHOW
	function ba_superbgimage_hide(img) {
		superbgimageActive = jQuery("#sb-superbgimage a.activeslide").attr("name");
		if ($(".sb-slideshow .sb-phrase[id="+superbgimageActive+"]").length > 0){
			jQuery(".sb-slideshow .sb-phrase[id="+superbgimageActive+"]").fadeOut();
		}
		jQuery(".sb-slideshow").fadeOut();
	}
	function ba_superbgimage_show(img) {
		superbgimageActive = jQuery("#sb-superbgimage a.activeslide").attr("name");
		if ($(".sb-slideshow .sb-phrase[id="+superbgimageActive+"]").length > 0){
			jQuery(".sb-slideshow .sb-phrase[id="+superbgimageActive+"]").fadeIn('slow').css('display','block');
		}
		jQuery(".sb-slideshow").fadeIn('slow');
		$.fn.superbgimage.options = {
			speed: default_speed,
			transition: default_transition
		};
	}

	if($('#sb-superbgimage a').length > 1) {
		// Options for SuperBGImage // If there are more than 1 photo > slideshow
		$.fn.superbgimage.options = {
			inlineMode: 1, // 0-resize to browser size, 1-do not resize to browser-size
			preload: 0, // 0-none, 1-preload images
			slideshow: 1, // 0-none, 1-autostart slideshow
			slide_interval: 5000, // interval for the slideshow
			speed: default_speed, // function-callback hide image
			transition: default_transition,
			onShow: ba_superbgimage_show, // function-callback show image
			onHide: ba_superbgimage_hide // function-callback hide image
		};

		function superbgSwipeStatus(event, phase, direction, distance)
		{
			//If we are moving before swipe, and we are going Lor R in X mode, or U or D in Y mode then drag.
			if( phase=="move" && (direction=="left" || direction=="right") )
			{
				var duration=0;
				
				if (direction == "left"){
						$.fn.superbgimage.options = {
							speed: 500,
							transition: 3
						};
						$('#thumbs').nextSlide();
				
				 }else if (direction == "right") {
						$.fn.superbgimage.options = {
							speed: 500,
							transition: 5
						};
						$('#thumbs').prevSlide();
				}
			}
		}
		var superbgSwipeOptions =
		{
			allowPageScroll:"vertical",
			swipeStatus : superbgSwipeStatus
		}
		$(function()
		{				
			slideshow = $('.sb-slideshow')
			slideshow.swipe( superbgSwipeOptions );
		});
	} else {
		// Options for SuperBGImage // If there is only 1 photo > no slideshow
		$.fn.superbgimage.options = {
			inlineMode: 1, // 0-resize to browser size, 1-do not resize to browser-size
			slideshow: 0, // 0-none, 1-autostart slideshow
			onShow: ba_superbgimage_show, // function-callback show image
		};
		$('#sb-superbgimage').superbgimage();
		mueveHome();
	}


	/***********************************************************/
	/****	CAROUSSEL										****/
	/***********************************************************/

	$('.carousel').carousel();
	function swipeStatus(event, phase, direction, distance)
	{
		//If we are moving before swipe, and we are going Lor R in X mode, or U or D in Y mode then drag.
		if( phase=="move" && (direction=="left" || direction=="right") )
		{
			var duration=0;
			
			if (direction == "left")
				$(this).carousel('next');
			
			else if (direction == "right")
				$(this).carousel('prev');
			
		}
	}
	var carouselSwipeOptions=
	{
		allowPageScroll:"vertical",
		swipeStatus : swipeStatus
	}
	$(function()
	{				
		carousel = $(".carousel");
		carousel.swipe( carouselSwipeOptions );
	});






	/************************************************************************/
	/*	*** EXTERNAL LINKS ***						*/
	/************************************************************************/
	function BA_targetBlank(element) {
		title = "(opens in new window)";
		if ($(element).attr('title') != undefined && $(element).attr('title') != '(opens in new window)') {
			title = $(element).attr('title') + ' ' + title;
		}
		if ($(element).attr('target') == '' || $(element).attr('target') == undefined) {$(element).attr('target','_blank');}
	}
	/* DOCS */
	$('a[href$=".doc"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-file-text sb-file-doc"></i>');BA_targetBlank($(this));});
	$('a[href$=".docx"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-file-text sb-file-doc"></i>');BA_targetBlank($(this));});
	$('a[href$=".rtf"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-file-text sb-file-doc"></i>');BA_targetBlank($(this));});
	/* PDF */
	$('a[href$=".pdf"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-file sb-file-pdf"></i>');BA_targetBlank($(this));});
	$('a[href$=".PDF"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-file sb-file-pdf"></i>');BA_targetBlank($(this));});
	/* POWERPOINT */
	$('a[href$=".pps"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-toggle-right sb-file-pps"></i>');BA_targetBlank($(this));});
	$('a[href$=".ppsx"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-toggle-right sb-file-pps"></i>');BA_targetBlank($(this));});
	$('a[href$=".ppt"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-toggle-right sb-file-pps"></i>');BA_targetBlank($(this));});
	$('a[href$=".pptx"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-toggle-right sb-file-pps"></i>');BA_targetBlank($(this));});
	/* EXCEL */
	$('a[href$=".xls"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-table sb-file-xls"></i>');BA_targetBlank($(this));});
	$('a[href$=".xlsx"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-table sb-file-xls"></i>');BA_targetBlank($(this));});
	/* ZIP FILES */
	$('a[href$=".zip"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-folder sb-file-zip"></i>');BA_targetBlank($(this));});
	$('a[href$=".rar"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-folder sb-file-zip"></i>');BA_targetBlank($(this));});
	$('a[href$=".ace"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-folder sb-file-zip"></i>');BA_targetBlank($(this));});
	$('a[href$=".bzip"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-folder sb-file-zip"></i>');BA_targetBlank($(this));});
	$('a[href$=".gzip"]').each(function(){$(this).prepend('<i class="fa fa-fw fa-folder sb-file-zip"></i>');BA_targetBlank($(this));});
	/* EXTERNAL URL */
	var myURL = location.protocol + '//' + location.hostname;
	var urlMobile = 'http://m.bookassist.com';
	// $('a[href^="http://"]').not('[href^="javascript"]').not('[href^="'+urlMobile+'"]').not('[href^="'+myURL+'"]').not('[rel^="shadowbox"]').each(function(){$(this).append('<i class="fa fa-external-link fa-fw"></i>');BA_targetBlank($(this));});
	// $('a[href^="https://"]').not('[href^="javascript"]').not('[href^="'+urlMobile+'"]').not('[href^="'+myURL+'"]').not('[rel^="shadowbox"]').each(function(){$(this).append('<i class="fa fa-external-link fa-fw"></i>');BA_targetBlank($(this));});
	$('a[href^="http://"]').not('[href^="javascript"]').not('[href^="'+urlMobile+'"]').not('[href^="'+myURL+'"]').not('[rel^="shadowbox"]').each(function(){BA_targetBlank($(this));});
	$('a[href^="https://"]').not('[href^="javascript"]').not('[href^="'+urlMobile+'"]').not('[href^="'+myURL+'"]').not('[rel^="shadowbox"]').each(function(){BA_targetBlank($(this));});

});

$(window).resize(function() {
	if(this.resizeTO) clearTimeout(this.resizeTO);
	this.resizeTO = setTimeout(function() {$(this).trigger('resizeEnd');}, 200);

	SB.mediaCalculate();
});

$(window).bind('resizeEnd', function() {
	// THIS WILL BE EXECUTED AFTER 500ms OF FINISHED RESIZING
	moveHomeContainer();

	//RESIZE .sb-img-autoresize INSIDE THE .sb-packages.sb-featured.sb-banners
	resizeAutoresizeImagesContainer()
	// AUTORESIZE/CENTER IMAGES INTO RESPONSIVE MODE
	sbAutoresizeProcessAllImages();


	});

</script>

