<script>
function ba_popup_show(speed) {
	if (!speed) { speed = 400; }
	$('#sb-popups-overlay, #sb-popups-close, #sb-popups').fadeIn(speed);
}
function ba_popup_hide() {
	$('#sb-popups-overlay, #sb-popups-close, #sb-popups').fadeOut(400);
	// last number is the days when popup closed will expire
	}
function ba_popup_set_cookie(name, value, expiration_days) {
	var exdate=new Date();
	exdate.setDate(exdate.getDate() + expiration_days);
	var c_value=escape(value) + ((expiration_days==null) ? "" : "; expires="+exdate.toUTCString());
	document.cookie = name + "=" + c_value;
}
$(function() {
	if ($('#sb-popups .-popup').length > 0) {
		ba_popup_show(2000);
		$('#sb-popups-close').prependTo('#sb-popups').click(function(){ba_popup_hide();});
			}
});
</script>


<!-- S_popups END -->
<!-- <S_booking_script -->





















<script>
$(document).ready(function() {

	var sb_dateinput_options = { trigger: true, format: 'dd mmm yy', min: -1, offset: [0, 0] };
	var ba_dateinput_options_yyyymmdd = { trigger: true, format: 'yyyy-mm-dd', min: -1, offset: [0, 0] };

	var next_arrow = '<i class="fa fa-angle-right fa-2x"></i>';
	var prev_arrow = '<i class="fa fa-angle-left fa-2x"></i>';

	// Apply calendar to all calendar html5 date fields
	$('.sb-booking form :date').dateinput(sb_dateinput_options);
	$('.sb-form form :date').dateinput(ba_dateinput_options_yyyymmdd);	

	// process ALL found calendars in page
	$('.sb-form').each(function() {

		var $calendar = $(this);
		var $caltrigger = $calendar.find('.caltrigger').addClass('fa fa-calendar fa-lg');

		$caltrigger.click(function(){
			var offset = $(this).offset();
			$('#calroot').css('right', $(window).width()-(offset.left+$(this).outerWidth())).css('left','auto');
			$('#calroot').css('top', offset.top+$(this).outerHeight());
		});

		// use the same callback for two different events. possible with bind
		$calendar.find(':date').bind('onShow onHide', function() {
			$(this).parent().toggleClass('active');
			$('a#calnext').html(next_arrow);
			$('a.calnext').html(next_arrow);
			$('a#calprev').html(prev_arrow);
			$('a.calprev').html(prev_arrow);
		});
		/*$calendar.find(':date').bind('onShow', function() {
			var disabled = ["20","25","29"];
			$('.calweek > a').not('.caldisabled').each(function(){ 
				if ($.inArray($(this).html(), disabled )!=-1){
					console.log("disable day: "+$(this).html());
					$(this).addClass("caloff caldisabled");
				}
			});
		});*/
	});

	// process ALL found calendars in page
	$('.sb-booking').each(function() {

		var $calendar = $(this);
		var $calendar_arrival_and_departure = $calendar.find(':date.sb-arrival, :date.sb-departure');
		var $calendar_arrival = $calendar.find(':date.sb-arrival');
		var $calendar_departure = $calendar.find(':date.sb-departure');
		var $calendar_room = $calendar.find('select.sb-room');
		var $calendar_special = $calendar.find('input.sb-pricegroup');
		var $calendar_promo = $calendar.find('input.sb-promo');
		var $caltrigger_left = $calendar.find('.sb-caltrigger-left .caltrigger').addClass('fa fa-calendar fa-lg');
		var $caltrigger_right = $calendar.find('.sb-caltrigger-right .caltrigger').addClass('fa fa-calendar fa-lg');

		$caltrigger_right.click(function(){
			var offset = $(this).offset();
			$('#calroot').css('right', $(window).width()-(offset.left+$(this).outerWidth())).css('left','auto');
			$('#calroot').css('top', offset.top+$(this).outerHeight());
		});
		$caltrigger_left.click(function(){
			var offset = $(this).offset();
			$('#calroot').css('left', offset.left).css('right','auto');
			$('#calroot').css('top', offset.top+$(this).outerHeight());
		});

		// use the same callback for two different events. possible with bind
		$calendar.find(':date').bind('onShow onHide', function() {
			$(this).parent().toggleClass('active');
			$('a#calnext').html(next_arrow);
			$('a.calnext').html(next_arrow);
			$('a#calprev').html(prev_arrow);
			$('a.calprev').html(prev_arrow);
			/*console.log('onShow onHide');*/
		});

		// when first date input is changed
if (typeof $calendar_arrival.data('dateinput') !== "undefined") {
		$calendar_arrival.data('dateinput').change(function() {
			// we use its value for the seconds input min option
			var d = new Date(this.getValue());
			d.setDate(d.getDate()+1);
			$calendar_departure.data('dateinput').setMin(d, true);
			// $calendar_arrival_and_departure.blur();
		});

		$calendar_departure.data('dateinput').change(function() {
			// $calendar_arrival_and_departure.blur();
		});
}
		$calendar_arrival_and_departure.focus(function() {
			// $calendar_arrival_and_departure.blur();
		});

		$calendar.find('.sb-book-now').click(function(e) {
			var bookingPage = '/hotel-reservation-prague/';
			var queryStr = 'date_in=' + $calendar_arrival.data('dateinput').getValue('yyyy-mm-dd');
			queryStr += '&date_out=' + $calendar_departure.data('dateinput').getValue('yyyy-mm-dd');
			if($calendar_room.length > 0 && $calendar_room.val()!='') {
				queryStr += '&price_group=' + $calendar_room.val();
			}
			if($calendar_special.length > 0 && $calendar_special.val()!='') {
				queryStr += '&price_group=' + $calendar_special.val();
			}
			if($calendar_promo.length > 0 && $calendar_promo.val()!='') {
				queryStr += '&promo_code=' + $calendar_promo.val();
			}
			document.location.href = bookingPage + '?' + queryStr;
		});
	})
});
</script>
<!-- S_booking_scrips> -->
