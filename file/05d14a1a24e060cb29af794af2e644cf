<script type="text/javascript">

// <AUTORESIZE/CENTER IMAGES INTO RESPONSIVE MODE
// PROCESS ALL IMAGES INSIDE DIV WITH CLASS ".sb-img-autoresize"
function sbAutoresizeProcessAllImages() {
	$('.sb-img-autoresize img').each(function() {sbAutoresizeCenterInContainer($(this));});  
}
// EXECUTION PER IMAGE
function sbAutoresizeCenterInContainer($image) {
		$container = $image.parent();
		
		var container_width = $container.width();
		var container_height = $container.height();

		var new_img = new Image();
		new_img.onload = function() {
			var image_width = new_img.width;
			var image_height = new_img.height;
			
			var w = container_width / image_width;
			var h = container_height / image_height;
			if (w > h) {
				var max_ratio = w;
			} else {
				var max_ratio = h;
			}

			var new_width = Math.ceil(max_ratio * image_width);
			var new_height = Math.ceil(max_ratio * image_height);
			var new_offset_x = (container_width - new_width) / 2;
			var new_offset_y = (container_height - new_height) / 2;

			$container.css('overflow', 'hidden');
			$image.attr('height', new_height);
			$image.attr('width', new_width);
			$image.css('height', new_height+'px');
			$image.css('width', new_width+'px');
			$image.css('margin', new_offset_y+'px 0 0 '+new_offset_x+'px');
			$image.css('max-height', 'none');
			$image.css('max-width', 'none');
			$image.css('min-height', 'none');
			$image.css('min-width', 'none');
		}
		new_img.src = $image.attr('src');
}
// AUTORESIZE/CENTER IMAGES INTO RESPONSIVE MODE>


function squarePackages(){
	$('.sb-packages .sb-package .sb-img').not('.sb-packages .sb-package.sb-package-no-offer .sb-img').each(function(){
		var packageWidth = $(this).width();
		$(this).height(packageWidth);
	})
};


var useNewPinterest = true;

function MM_openBrWindow(a,h,n){window.open(a,h,n)}

function moveHomeContainer(){
	setTimeout(function() {
		$(".media-xs .sb-footer").animate({"bottom": - (15 + $(".media-xs .sb-footer").height())}, "slow");
		$(".media-sm .sb-footer").animate({"bottom": - (15 + $(".media-sm .sb-footer").height())}, "slow");
	}, 600);
	if(!useNewPinterest){
		$('.media-xs .sb-packages.sb-boxes').pinterest_grid({
			no_columns: 1,
			padding_x: 15,
			padding_y: 15,
			margin_bottom: 50,
			single_column_breakpoint: 768
		});
		$('.media-sm .sb-packages.sb-boxes').pinterest_grid({
			no_columns: 2,
			padding_x: 15,
			padding_y: 15,
			margin_bottom: 50,
			single_column_breakpoint: 768
		});
		$('.media-md .sb-packages.sb-boxes').pinterest_grid({
			no_columns: 2,
			padding_x: 15,
			padding_y: 15,
			margin_bottom: 50,
			single_column_breakpoint: 768
		});
		$('.media-lg .sb-packages.sb-boxes').pinterest_grid({
			no_columns: 3,
			padding_x: 15,
			padding_y: 15,
			margin_bottom: 50,
			single_column_breakpoint: 768
		});
	}
}
$('html').on("mediaXs", function(){moveHomeContainer();});
$('html').on("mediaSm", function(){moveHomeContainer();});

/***********************************************************/
/****	ANIM HOME + MENU RESP				****/
/***********************************************************/
function mueveHome(){

	$(".sb-menu-for-responsive > .container > ul, .sb-menu-for-responsive > .container-fluid > ul").prepend('<li class="sb-languages visible-xs visible-sm"><a href="/"><i class="sb-icon sb-icon-2x sb-icon-flag en"></i>English</a></li>');
	$(".sb-footer .sb-languages-container ul").clone().appendTo(".sb-menu-for-responsive li.sb-languages");
	$(".sb-menu li.sb-languages ul").removeClass('dropdown-menu');

	if ($('.media-xs').length > 0 || $('.media-sm').length > 0) {
		if ($('.sb-menu-for-responsive').length > 0) {
			$('.sb-header .sb-menu').css('visibility', 'visible');
			$('.sb-header').append('<div class="sb-menu-responsive visible-xs visible-sm"></div>');
			$('.sb-menu-responsive').append('<div class="sb-logo"><a href="/" title="Hotel Paris Prague"><img src="/files/img/xs-paris_prague_hotel_logo.png" alt="Hotel Paris Prague" /></a></div>');
			$('.sb-menu-for-responsive').meanmenu({meanMenuClose: '×', meanScreenWidth: '991', meanMenuContainer: '.sb-menu-responsive'});
			$('.sb-menu-responsive.mean-container > .mean-bar > nav > ul > li:nth-child(2) > a').text('Home');
		}
	} else {
		if ($('.sb-tpl-index-home').length > 0) {
						
						$('.sb-tpl-index-home .sb-tabs li:first-child a').click();
						if ($('#sb-popups .-popup').length > 0) {
				ba_popup_show(2000);
				$('#sb-popups-close').prependTo('#sb-popups').click(function(){ba_popup_hide();});
							}
			if ($('.sb-menu-for-responsive').length > 0) {
					$('.sb-header').append('<div class="sb-menu-responsive visible-xs visible-sm"></div>');
				$('.sb-menu-responsive').append('<div class="sb-logo"><a href="/" title="Hotel Paris Prague"><img src="/files/img/xs-paris_prague_hotel_logo.png" alt="Hotel Paris Prague" /></a></div>');
				$('.sb-menu-for-responsive').meanmenu({meanMenuClose: '×', meanScreenWidth: '991', meanMenuContainer: '.sb-menu-responsive'});
				$('.sb-menu-responsive.mean-container > .mean-bar > nav > ul > li:nth-child(2) > a').text('Home');
			}
		} else {
			if ($('.sb-menu-for-responsive').length > 0) {
				$('.sb-header .sb-menu').css('visibility', 'visible');
				$('.sb-header').append('<div class="sb-menu-responsive visible-xs visible-sm"></div>');
				$('.sb-menu-responsive').append('<div class="sb-logo"><a href="/" title="Hotel Paris Prague"><img src="/files/img/xs-paris_prague_hotel_logo.png" alt="Hotel Paris Prague" /></a></div>');
				$('.sb-menu-for-responsive').meanmenu({meanMenuClose: '×', meanScreenWidth: '991', meanMenuContainer: '.sb-menu-responsive'});
				$('.sb-menu-responsive.mean-container > .mean-bar > nav > ul > li:nth-child(2) > a').text('Home');
			}
		}
	}
}

//RESIZE .sb-img-autoresize INSIDE THE .sb-packages.sb-featured.sb-banners
function resizeAutoresizeImagesContainer(){
	$(".sb-packages.sb-featured.sb-banners .sb-package .sb-img-autoresize, .sb-tpl-reservations .sb-packages .sb-package .sb-img").each(function(){
		$(this).hide();
		var packageHeight = $(this).parent(".sb-package").height();
		$(this).css({"height":packageHeight});
		$(this).show();
	});
};

//EQUAL HEIGHTS OF OFFERS ON THE OFFERS-BOX PAGE
$.fn.extend({
	equalHeights: function(options){
		var ah=(Math.max.apply(null, $(this).map(function(){ return $(this).outerHeight(); }).get()));
		if (typeof ah=='number') $(this).height(ah);
	}
});

//EQUAL HEIGHTS OF PACKAGES BY ROW
function equalPackagesHeaders(selector){
	var selectorChildren = $(selector).length;
	if (SB.media == 'sm'){
		var numberOfRows = Math.ceil(selectorChildren / 2);
	} else if ( SB.media == 'md' || SB.media == 'lg'){
		var numberOfRows = Math.ceil(selectorChildren / 3);
	}

	$(selector).css({'height':'auto'});
	for (i = 0; i <= numberOfRows; i ++) {
		if (SB.media == 'md' || SB.media == 'lg'){
			firstItem = i * 3;
			lastItem = i * 3 + 3;
		} else if (SB.media == 'sm'){
			firstItem = i * 2;
			lastItem = i * 2 + 2;
		}
		$(selector).slice(firstItem,lastItem+1).equalHeights();
	};
}

