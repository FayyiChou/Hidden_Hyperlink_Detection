	      var win = $(this); //this = window
		var conHeight = ($(window).height() - 265);
		$(".sb-tabs-container .tab-content").css("max-height", conHeight + "px");

	});

	//FUNCTION TO HIDE AND SHOW IMAGES OF THE OFFERS ON THE OFFERS CAROUSEL (TO CENTER IMAGE BEFORE THE FADEIN)
	if($('#offers-carousel').length){
		$('#offers-carousel').on('slide.bs.carousel', function () {
			$('#offers-carousel .sb-package .sb-img').animate({'opacity':'0'},"fast");
		});
		$('#offers-carousel').on('slid.bs.carousel', function () {
			sbAutoresizeProcessAllImages();
			$('#offers-carousel .sb-package .sb-img').css({'opacity':'0'}).animate({'opacity':'1'},"fast");
		})
	};

	
		$('.sb-menu .container > ul > li > a').each(function(){
		$(this).addClass('inactive');
		var t = $(this).attr('id');
		var tab = t.match(/tab(.*)/g);
				if (t == tab){
			$(this).removeAttr('href');
			$(this).on("touchstart", function (e) {
				$('.sb-menu .container > ul > li > a.hover').removeClass('hover').blur();
			});
		}
			});
	
	$('.sb-menu .container > ul > li > a').click(function(){
		var t = $(this).attr('id');
		if($(this).hasClass('inactive')){ //this is the start of our condition 
			$('.sb-menu .container > ul > li > a').addClass('inactive');
			$(this).removeClass('inactive');
			$('.tab-container').hide();
			$('#'+ t + 'C').fadeIn('slow');
		} else {
			$('.tab-container').fadeOut('slow');
			$('.sb-menu .container > ul > li > a').addClass('inactive');
		}
	});

	$('.sb-menu .container > ul > li > a').hover(function(){
