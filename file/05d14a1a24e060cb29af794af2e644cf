		$(".sb-menu .container > ul > li > ul").hover(function () {

		var elementHeights = $(this).find('ul').map(function() {
			return $(this).height();
		}).get();

		var maxHeight = Math.max.apply(null, elementHeights);
		if(maxHeight=="-Infinity"){maxHeight = "0";} else{
			maxHeight = maxHeight + 70;
		}
		
		$('<style>.sb-menu .container > ul >li > ul:hover:after{height:'+maxHeight+'px}</style>').appendTo('head');
	});  

	$(".sb-menu .container > ul > li").hover(function() {$(this).siblings('.active').addClass('temp-inactive');}, function() {$(this).siblings('.active').removeClass('temp-inactive');});

	//RESIZE .sb-img-autoresize INSIDE THE .sb-packages.sb-featured.sb-banners
