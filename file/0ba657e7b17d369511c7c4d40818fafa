<script type="text/javascript">

function gotopage() {
    $('#gotopage_form')[0].submit();
}

$(function(){
	$(window).scroll(function(){  //只要窗口滚动,就触发下面代码 
		var scrollt = document.documentElement.scrollTop + document.body.scrollTop; //获取滚动后的高度 
		if( scrollt >200 ){  //判断滚动后高度超过200px,就显示  
			$("#goTopBtn").fadeIn(400); //淡出
		}else{
			$("#goTopBtn").stop().fadeOut(400); //如果返回或者没有超过,就淡入.必须加上stop()停止之前动画,否则会出现闪动
		}
	});
	
	$("#goTopBtn").click(function(){ //当点击标签的时候,使用animate在200毫秒的时间内,滚到顶部
		$("html,body").animate({scrollTop:"0px"},200);
	});

    //点击效果
    $('.attention').click(function(event) {

        var $this = $(this);

        var _trade_id = $(this).attr('trade_id');

        $.ajax({    
            type: "POST",
            url: "/attention/add",
            data: {"trade_id":_trade_id},
            dataType: "json",
            success: function(re) {
                if (re == 0) {
                    $this.removeClass('attention').addClass('attentioned').text('已关注');
                } else if (re == 1) {
                    location.href = '/user/login';
                } else if (re == 2) {
                    alert('关注异常!')
                }
            }
        });

        
    });
	
	//分类点击效果
	// $(".two_type li").click(function(){
	// 	$(this).addClass("cur").siblings("li").removeClass("cur");
	// 	//展示对应分类详情
	// 	$(".two_type_left .two_type_con").eq($(this).index()).show().siblings(".two_type_con").hide();
	// })
})
</script>

</body></html>