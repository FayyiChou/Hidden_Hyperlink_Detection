<script>
    $(function(){
        var i=0;
        $('.scroll ul li').click(function(){
            var index=$(this).index();
            $(this).addClass('active').siblings().removeClass('active');
            $('.focus .imgshow ul li').eq(index).fadeIn().siblings().hide();
            i=index+1;
        });
        var llistN=$('.scroll ul li').length;
        $('.focus .btnR').click(function(){
            if(i<llistN){i++}else{i=1}
            $('.scroll ul li').eq(i-1).addClass('active').siblings().removeClass('active');
            $('.focus .imgshow ul li').eq(i-1).fadeIn().siblings().hide();
        });
        $('.focus .btnL').on('click',function(){
            if(i>0){
                i--
            }else if(i=llistN){
                i=llistN-1;
            }
            $('.scroll ul li').eq(i-1).addClass('active').siblings().removeClass('active');
            $('.focus .imgshow ul li').eq(i-1).fadeIn().siblings().hide();
        });
        var timer=setInterval(function(){$('.focus .btnR').click()},3000);
        $('.focus').hover(function(){clearInterval(timer)},function(){timer=setInterval(function(){$('.focus .btnR').click()},3000)})
    });
</script>

