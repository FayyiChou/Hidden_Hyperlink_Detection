    var _hmt = _hmt || [];
    (function() {
        var hm = document.createElement("script");
        hm.src = "//hm.baidu.com/hm.js?6a1d4f18f62d602b7b3d95df38e345e7";
        var s = document.getElementsByTagName("script")[0];
        s.parentNode.insertBefore(hm, s);
    })();

    $(document).ready(function(){
        var isRun = 0,
            imgBoxWidth = 510,
            showNum = 2,
            ts = '',
            imgNum = $('.lb-div').children().length;
        $('.ib-sp').on('click', function () {
            if (isRun == 1) {
                return false;
            }
            isRun = 1;
            run(-510, 0, 0);
        })
        $('.ib-sp-r').on('click', function () {
            if (isRun == 1) {
                return false;
            }
            isEmptyRight();
            isRun = 1;
            run(510, 1, 0);
        })

        initRun();

        function initRun () {
            ts = setInterval(function () {
                isRun = 1;
                run(-510, 0);
            }, 3000);
        }
        $('#container').on('mouseover', function () {
            window.clearInterval(ts);
        })

        $('#container').on('mouseleave', function () {
            ts = setInterval(function () {
                isRun = 1;
                run(-510, 0);
            }, 3000);
        })

        function run (step, direction) {
            var firstImgBox = $('.lb-div').children().eq(0);
            firstImgBox.animate({"margin-left": parseInt(firstImgBox.css('margin-left'))+step}, 500, function () {
                isRun = 0;
                if (direction == 0) {
                    isEmptyLeft();
                }else {
                    isEmptyRight();
                }
            });

//            var t = setInterval(function () {
//                stepCount += step;
//                firstImgBox.css('margin-left', parseInt(firstImgBox.css('margin-left'))+step);
//                if (Math.abs(stepCount) == imgBoxWidth) {
//                    isRun = 0;
//                    window.clearInterval(t);
//                    if (direction == 0) {
//                        isEmptyLeft();
//                    }else {
//                        isEmptyRight();
//                    }
//                }
//            }, 1000/60);
        }

        function isEmptyLeft () {
            var firstImgBox = $('.lb-div').children().eq(0);
            var firstImgBoxMarginLeftTemp = firstImgBox.css('margin-left');
            var firstImgBoxMarginLeft = firstImgBoxMarginLeftTemp.substr(0, firstImgBoxMarginLeftTemp.indexOf('px'));
            if (Math.abs(firstImgBoxMarginLeft)+12 >= imgBoxWidth * (imgNum - showNum)) {
                firstImgBox.next().css('margin-left', (parseInt(firstImgBoxMarginLeft) + imgBoxWidth) + 'px');
                firstImgBox.css('margin-left', '12px');
                var firstImgBoxClone = firstImgBox.get(0).outerHTML;
                $('.lb-div').append(firstImgBoxClone);
                firstImgBox.remove();
            }
        }

        function isEmptyRight () {
            var firstImgBox = $('.lb-div').children().eq(0);
            var firstImgBoxMarginLeftTemp = firstImgBox.css('margin-left');
            var firstImgBoxMarginLeft = firstImgBoxMarginLeftTemp.substr(0, firstImgBoxMarginLeftTemp.indexOf('px'));
            if (Math.abs(firstImgBoxMarginLeft) <= 12) {
                var lastImgBoxClone = $('.lb-div').children().last().get(0).outerHTML;
                $('.lb-div').prepend(lastImgBoxClone);
                $('.lb-div').children().last().remove();
                firstImgBox.prev().css('margin-left', (parseInt(firstImgBoxMarginLeft) - imgBoxWidth) + 'px');
                firstImgBox.css('margin-left', '12px');
            }
        }
    });

</script>

