<script>
        $(function(){
        if(challeng.checkIsMobile()){
            var videoUrl = $('#videoWin').data('videourl');
            var videohtml = '<video src="' + videoUrl + '" height="100%" width="100%"  autoplay="autoplay" id="smallVideo" controls="controls" webkit-playsinline></video>';
            $("#smallAltContent").append(videohtml);

            $("#smallAltContent").get(0).addEventListener('click', function () {
                challeng.smalllaunchFullscreen(document.getElementById('smallVideo'));

                window.setTimeout(function exit() {
                    //检查浏览器是否处于全屏
