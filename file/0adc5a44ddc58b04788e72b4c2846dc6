<script src="http://image.meilele.com/js/ana.min.js?1121"></script>
<script type="text/javascript">
/*
 * @ 各种鸡公第三方代码，搞毛线
 * @ 全都搞到页面load之后去
 */
window._gaq = window._gaq||[];
window._ana = window._ana||[];

 
//网页关键词标签
;(function(){
	if( window._IS_GET_TAG )return;
	window._IS_GET_TAG = true;
	var cookieName = 'utag';
	var cookieLength = 18;
	if( /^\/article/i.test( location.pathname ) ){
		cookieName = 'atag';
	}

		$.ajax({
			url:'/java_api/jmll/tag/getTag.do?u='+encodeURIComponent(location.pathname),
			cache:true,
			dataType:'json',
			async:false,
			success:function(json){
				var old = $.cookie(cookieName) || '';
				var thisPageTag = json || {};
				var finalTag = {},oldArray=[],thisArray = [],finalArray=[];
				var to36N = function(n){
					n = n - 0;
					n = "00" + n.toString(36);
					return n.substr( n.length - 2 , 2 );
				}
				old = old.split("*");
				for(var k = 0 , kk = old.length; k < kk; k++){
					if(old[k] && old[k].length == 5){
						var key = old[k].substr(0,3);
						var number = parseInt(old[k].substr(3,2),36);
						finalTag[ key ] = {key:key,number:number};
						oldArray.push( finalTag[ key ] )
					}
				}
				for(var k in thisPageTag){
					if( finalTag[k] ){
						finalTag[k].number += thisPageTag[k];
					}else{
						finalTag[k] = {key:k,number:thisPageTag[k]};
					}
					finalTag[k].thisPage = true;
					thisArray.push( finalTag[ k ] );
				}
				thisArray.sort( function(a,b){
					return b.number - a.number;
				});
				for( k = 0 , kk = thisArray.length ; k < kk ; k++ ){
					finalArray.push( thisArray[k].key + to36N( thisArray[k].number ) )
				}
				for( k = 0 , kk = oldArray.length ; k < kk ; k++ ){
					if(!oldArray[k].thisPage)finalArray.push( oldArray[k].key + to36N( oldArray[k].number ) )
				}
				if(finalArray.length > cookieLength){
					finalArray.length = cookieLength;
				}
				$.cookie(cookieName,finalArray.join("*"),{expires:90,domain:'.meilele.com',path:'/'});
				window._SITETAGS = {
					tags:finalTag,
					thisPageTag:thisPageTag
				}
				if( window._SITETAGSCALLBACK )_SITETAGSCALLBACK( _SITETAGS );
			}
		});

})();

// 美乐乐自有统计代码
function loadAna(){
	_ana.push( ['trackView'] );
}

// ga统计代码
function loadGoogleAnalytics() {
	window.MLLgaq = window.MLLgaq || [];
	for ( var k = 0; k < MLLgaq.length; k++ ) {
		_gaq.push( MLLgaq[k] );
	}
	_gaq.push(['_setSiteSpeedSampleRate', 10]);
	_gaq.push(["_setAccount", "UA-10173353-6"]);
	_gaq.push(["_trackPageview"]);
	var ga = document.createElement('script'), date = new Date(); 
	ga.type = 'text/javascript'; 
	ga.async = true;
	//ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
	ga.src = 'http://image.meilele.com/zximages/js/dc.js?'+ date.getFullYear() + (date.getMonth() + 1) + (date.getDate() < 10 ? '0' + date.getDate() : date.getDate());
	var s = document.getElementsByTagName('script')[0];
	s.parentNode.insertBefore(ga, s);
}

// 百度
function loadBaiDuRtJs() {
	window.bd_cpro_rtid = "rHnLnHT";
	var s = document.createElement("script");s.type = "text/javascript";s.async = true;s.src = location.protocol + "//cpro.baidu.com/cpro/ui/rt.js";
	var s0 = document.getElementsByTagName("script")[0];
	s0.parentNode.insertBefore(s, s0);
}

// 广点通
function ls(src) {
	var ga = document.createElement('script'); 
	ga.type = 'text/javascript'; 
	ga.async = true;
	ga.src = src;
	var s = document.getElementsByTagName('script')[0]; 
	s.parentNode.insertBefore(ga, s);
}
function loadTajs() {
	ls("http://tajs.qq.com/gdt.php?sId=23384945");
	ls("http://tajs.qq.com/gdt.php?sId=21253825");
	ls("http://tajs.qq.com/gdt.php?sId=26418742");
	ls("http://tajs.qq.com/gdt.php?sId=43311811");
}

// 聚效访客代码
function loadMvl() {
	var _mvq = window._mvq || []; 
	window._mvq = _mvq;
	_mvq.push(['$setAccount', 'm-194968-0']);
	_mvq.push(['$logConversion']);

	var mvl = document.createElement('script');
	mvl.type = 'text/javascript'; mvl.async = true;
	mvl.src = ('https:' == document.location.protocol ? 'https://static-ssl.mediav.com/mvl.js' : 'http://static.mediav.com/mvl.js');
	var s = document.getElementsByTagName('script')[0];
	s.parentNode.insertBefore(mvl, s); 
}

// Criteo
