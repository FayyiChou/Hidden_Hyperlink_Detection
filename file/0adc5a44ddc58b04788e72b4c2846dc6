}(Zepto), function($){
    // 评论
    window._gaq = window._gaq || [];

    var getCookie = function (name) {
        var cookieValue;
        if (document.cookie && document.cookie !== '') {
            var cookies = document.cookie.split(';');
            var cookieLength = cookies.length;
            for (var i = 0; i < cookieLength; i++) {
                var cookie = cookies[i].replace(/(^\s*)|(\s*$)/g, "");
                if (cookie.substring(0, name.length + 1) == (name + '=')) {
                    cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                    break;
                }
            }
        }
        return cookieValue;
    };
    var userName = document.getElementById('JS_in_username');
    var uname = (getCookie("meilele_login") == "1" && getCookie("ECS[username]"))?getCookie("ECS[username]"):"美乐乐匿名用户";
    if( uname ) userName.value = uname;

    var discuss = {
        article_page:0,
        comment_page:0,
        more_replay_flag:1,
        more_comment_flag:1,
        sayGood_flag:true,
        sendMsg_falg:true,
        submit_flag:true,
        article_style:window.__isNewArt ? 'new' : 'old',
        versions:window.__isNewArt ? 1 : 0,
        articleId: $('#jsArticleCommentList dl').eq(0).attr('data-id'),

        init: function() {
            
            $('#jsArticleCommentAdd').click(function(){
                window.scrollTo(0, getDestY('JS_dis_input'));
