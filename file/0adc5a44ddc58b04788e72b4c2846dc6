}(Zepto),function(){
    // 秒杀倒计时
    /* -----
     * 倒计时
     * --------------------------------------------- */
    Zepto.ajax({
        url: '/mll_api/api/date',
        type: 'GET',
        dataType: 'json',
        success: function(json) {
            if ( json.time ) {
                var currentTime = json.time;

                var
                    target = $('#JS_ms_count_down'),
                    _tag = [],
                    __ = 0;

                if ( target.length <= 0 ) return; 
                // Timer类
                function Timer(id) {
                    this.id = id;
                    this.obj = $('#'+id);
                    this.html = this.obj.attr("data-format");
                    this.leftTime = this.obj.attr("data-lefttime") - currentTime;
                }
                Timer.prototype = {
                    fill: function() {
                        var that = this;
                        var sec = (that.leftTime - __) || 0; //防止NaN的出现
                        if ( sec <= 0 ) {
                            sec = 0;
                            that.obj.html('');
                            return;
                        }
                        var s = sec % 60;
                        if ( s < 10 ) s = "0" + s;
                        var m = parseInt(sec % 3600 / 60);
                        if ( m < 10 ) m = "0" + m;
                        var h = parseInt(sec % (3600 * 24) / 3600);
                        if ( h < 10 ) h = "0" + h;
                        var d = parseInt(sec / (3600 * 24));
                        if ( that.html.indexOf("$D") < 0 ) h = (d * 24) - 0 + (h - 0);
                        var out = '';
                        //if ( d > 0 ) out = '<span class="num num-mr">'+(d < 10 ? "0"+d : d)+'</span><span class="dot">:</span>';
                        out = out + '<span class="num num-mr">'+h+'</span><span class="dot">:</span><span class="num num-mr">'+m+'</span><span class="dot">:</span><span class="num num-mr">'+s+'</span>';  
                        that.obj.html( out );
                    }
                }

                // 为dom添加属性
                var id = 'count_down_OoO_00000';
                target.attr('id', id);
                _tag[0] = new Timer(id);


                function countDown() {
                    _tag[0].fill();
                    __++;
                    setTimeout(function(){
                        countDown();
                    }, 1000);
                }

                countDown();
            }
        }
    });
}(Zepto), function($){
    var idOld = 'is_oldArticle';
    var pageSn = idOld ? 'm_article_old' : 'm_article';
