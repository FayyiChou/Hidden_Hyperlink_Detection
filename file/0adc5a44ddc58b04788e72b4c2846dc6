        rendring: function(data) {
            var html = '';
            var len = data.length
            for(var i=0;i<len;i++){
                var avatar = 'http://image.meilele.com/' + (data[i].avatar ? 'zximages/' + data[i].avatar : 'wap/images/article/head.png'),
                name = data[i].name ? data[i].name : '美乐乐匿名网友';
                html += '<dl><dt class="clearfix"><div class="Left clearfix"><img class="Left" src="'+avatar+'" alt=""><span class="Left">'+ name +'</span></div><div class="Right"><span>'+data[i].add_time+'</span></div></dt><dd>'+data[i].content+'</dd></dl>';
            }

            $('#jsArticleCommentList').append(html);
        },

        submit: function(aid) {
            var username = $('#JS_in_username').val(),
                content = $('#JS_in_comment').val(),
                urlStr = window.__isNewArt ? '/www_api/add_article_comments.html' : '/www_api/add_article_comments_old.html';
            var commRegex = new RegExp('(?=https?://|(?:[a-z-]+\.)+(?:aero|asia|arpa|biz|cc|cn|co|com|coop|edu|gov|info|int|jobs|me|mobi|mil|museum|name|nato|net|org|pro|tv|tel|travel))(?:https?:)?[a-z&/?.\-=]*');

            if ( content.length < 1 ) {
                this.showMsg('还没有输入内容哟!');
                return false;
            } else if ( content.replace(/^\s+$/, '') == '' ) {
                this.showMsg('还没有输入内容哟!');
                return false;
            }
            $.ajax({
                type : "post",
                url : urlStr,
                data:'article_id='+aid+'&name='+username+'&content='+content,
                dataType : "json",
                success : function (json) {
                    
                    if (json.error == 1) {
                        discuss.showMsg(json.msg);
                        return;
                    }
                    if (json.error == 0) {
                        discuss.showMsg3("发表成功<br/>待管理员审核");
                        $("#JS_in_comment").val("");
                    }
                },
                error : function () {
                    discuss.showMsg("请求失败，请稍后重试！");
                }
            });
        },

        showMsg: function(msg) {
            var html = '<div style="padding:20px;border-radius:5px;background:#fff;text-align:center;font-size:14px;">'+msg+'</div>';
            $$.layer(html, {
                contentBoxWidth: '80%',
                isShowMask: true
            });
            discuss.sendMsg_falg = false;
            discuss.submit_flag = false;
            setTimeout(function(){
                $$.closeLayer();
                discuss.sendMsg_falg = true;
                discuss.submit_flag = true;
            }, 1000);
        },
        // 顶部浮层显示文字（新增样式）
        showMsg2: function(msg) {
            var html = '<div style="border-radius:5px;font-size:15px;color:#fff;line-height:34px;z-index:2;position:fixed;top:63px;text-align:center;width:100%;"><p>'+msg+'</p></div><div style="position:fixed;top:63px;background:#000;opacity:0.6;height:34px;width:100%;z-index:-1;"></div>';
            $('#showTopMsg').append(html);
            this.sayGood_flag = false;
            setTimeout(function(){
                $('#showTopMsg').empty();
                discuss.sayGood_flag = true;
            }, 3000);
        },
        // 成功时浮层显示（新增样式）
        showMsg3: function(msg) {
            var html = '<div style="padding:20px;border-radius:5px;text-align:center;font-size:14px;z-index:2;color:#fff;line-height:18px;"><p><span id="success-flag-tip" style="width:28px;height:28px;"></span></p>'+msg+'</div><div style="position:absolute;left:28%;right:28%;top:0;bottom:0;opacity:0.6;background:#000;border-radius:6px;z-index:-1;"></div>';
            $$.layer(html, {
                contentBoxWidth: '60%',
                isShowMask: false
            });
            discuss.sendMsg_falg = false;
            discuss.submit_flag = false;
            setTimeout(function(){
                $$.closeLayer();
                discuss.sendMsg_falg = true;
                discuss.submit_flag = true;
            }, 800);
        },
        // 点赞功能
        sayGood: function(_this) {

            if (this.sayGood_flag) {
                var _commentsId = $.cookie('COMMENTS_ID') ? $.cookie('COMMENTS_ID').split(',') : [];
                var _commentId = $(_this).parents('dl').attr('comment-id');
                var _commentNum = _commentsId.indexOf(_commentId);
                var _operateObj = $(_this).parent('dl');
                if (_commentNum === -1 && !$(_this).hasClass('active')) {
                    $.ajax({
                        url: '/dubbo_api/mll/comments/like',
                        type: 'GET',
                        dataType: 'json',
                        data: {commentsId: _commentId},
                        success:function(data){
                            if (data.error === 0) {
                                _commentsId.push(_commentId);
                                _commentsId = _commentsId.join(',');
                                $.cookie('COMMENTS_ID',_commentsId);
                                _operateObj.addClass('active');
                                var _numObj = $('.num-good', _this);
                                var _num = _numObj.text();
                                _num = parseInt(_num) + 1;
                                _numObj.text(_num);
                                $(_this).css({"background-position":"0 -21px"});
                            }else{
                                discuss.showMsg(data.msg);
                            }
                        }
                    })
                }else{

                    discuss.showMsg2('你已经点过赞了哦！');
                }
            } else {
                return false;
            }
        },

        // 回复功能
        replay: function(_this) {
            var textareaBox = '<dd class="discuss-input discuss-form"><textarea maxlength="200" class="JS_in_comment"></textarea><input name="userName" class="JS_in_username" type="hidden" /><p class="btn-send-box"><a href="javascript:;" class="btn-send">发送</a><p></dd>';
            var nowDom = $(_this).parents('dl');
            var manName = $('.man-name', nowDom).text();
            var setName,nowThis;

            setName = "@"+manName+"：";

            if ($('textarea', nowDom).length <= 0) {
                $('dd', nowDom).eq(0).after(textareaBox);
                $('textarea', nowDom).attr('placeholder', setName);
            } else {
                $('.discuss-input', nowDom).show();
                $('textarea', nowDom).attr('placeholder', setName);
            }

            $('textarea', nowDom).focus(function() {
                $(this).attr('placeholder', '');
            });
            $('textarea', nowDom).blur(function() {
                $(this).attr('placeholder', setName);
            });
            $('.btn-send', nowDom).click(function() {
                if (!discuss.sendMsg_falg) {return}

                var nowThis = $(this).parents('.discuss-input'),
                _contentVals = $('.JS_in_comment', nowThis).val(),//获取回复的内容                
                _articleId = $(this).parents('dl').attr('data-id'),//获取文章id
                _username = (getCookie("meilele_login") == "1" && getCookie("ECS[username]"))?getCookie("ECS[username]"):"美乐乐匿名用户",
                _commentId = $(this).parents('dl').attr('comment-id') ? $(this).parents('dl').attr('comment-id') : 0,//判断是子评论回复还是父评论回复
                regEmail = new RegExp('(?=https?://|(?:[a-z-]+\.)+(?:aero|asia|arpa|biz|cc|cn|co|com|coop|edu|gov|info|int|jobs|me|mobi|mil|museum|name|nato|net|org|pro|tv|tel|travel))(?:https?:)?[a-z&/?.\-=]*');
                //内容是否合法的匹配
                _contentVal = _contentVals.replace(/^\s+$/, '');
                _useremail = $.cookie("ECS[email]") || '';
                if($.trim(_contentVal).length == 0 || $.trim(_contentVal) == setName){
                    discuss.showMsg("还没有输入内容哟！");
                    return;
                }
                //发表内容的请求
                $.ajax({
                    url:'/dubbo_api/mll/comments/add',
                    type:'POST',
                    dataType:'json',
                    data:{'articleId':_articleId,'name':_username,'email':_useremail,'content':_contentVal,'parentCommentId':_commentId,'version':discuss.versions},
                    success:function(json){
                        if (json.error == 1) {
                            discuss.showMsg(json.msg);
                            return;
                        }
                        if (json.error == 0) {
                            discuss.showMsg3("发表成功<br/>待管理员审核");
                            nowThis.remove();
                        }
                    },
                    error:function(){
                        discuss.showMsg("提交失败，请稍后重试！");
                    }
                });
            });

        },

        //加载更多回复
        addReplay: function(_this){
            var addReplayStr = '';
            var nowDoms, comment_replay_page;
            var hasAddOks;

            nowDoms = $(_this).parents('dl');
            commentIdNow = nowDoms.attr('comment-id');

            if ($(_this).hasClass('addOks')) {
                discuss.openClose(_this,nowDoms);
            } else {
                comment_replay_page = parseInt($(_this).attr('data-num'));
                comment_replay_page = comment_replay_page + 1;
                $(_this).attr('data-num', comment_replay_page);
            }
            hasAddOks = $(_this).hasClass('addOks') ? 0 : 1;
            if (discuss.more_replay_flag == 1 && hasAddOks == 1) {
                $.ajax({
                    url: '/article_replay/'+commentIdNow+'-'+comment_replay_page+'.html',
                    type:'GET',
                    dataType:'json',
                    success:function(data) {
                        discuss.more_replay_flag = data.more_replay;
                        data.replay_list.map(function(val, index) {
                            addReplayStr += '<dl class="discuss-replay add-more-replays" comment-id="'+val.id+'" data-id="'+val.article_id+'">'+
                                '<dt class="clearfix">'+
                                    '<div class="Left clearfix">'+
                                        '<img class="Left" src="http://img003.mllres.com/'+val.avatar+'" alt=""><span class="Left">'+val.name.substring(0,8)+'</span>'+
                                    '</div>'+
                                    '<div class="Right">'+
                                        '<span>'+val.add_time+'</span>'+
                                    '</div>'+
                                '</dt>'+
                                '<dd>'+val.content+'</dd>'+
                            '</dl>'
                        });
                        $(_this).parents('.btn-more-box').before(addReplayStr);
                        if (discuss.more_replay_flag == 0) {
                            $(_this).unbind('click');
                            $(_this).text('收起 ');
                            $(_this).removeClass('btn-more').addClass('addOks ups').on('click', function(){
                                var _thiss = this,
                                nowDomss = $(_this).parents('dl');

                                discuss.openClose(_thiss,nowDomss);
                            });
                            discuss.more_replay_flag = 1;
                        }         
                    }
                });
            } 
        },
        openClose: function(_this,nowDoms) {
            if ($('.add-more-replays', nowDoms).is(':hidden')) {
                $('.add-more-replays', nowDoms).show();
                $(_this).addClass('ups').text('收起 ');
            } else {
                $('.add-more-replays', nowDoms).hide();
                $(_this).removeClass('ups').text('展开 ');
            }
        },
        assignedIdList: function() {
            var _idList = [];
            var _commentList = $('.btn-good');
            var _commentId;
            for (var i = 0; i < _commentList.length; i++) {
                _commentId = _commentList.eq(i).parents('dl').attr('comment-id');
                _idList.push(_commentId);
            }
            _idList = _idList.join(',');
            discuss.defaultLike(_idList);
        },
        defaultLike: function(_idList) {
            $.ajax({
                url: '/dubbo_api/mll/comments/getLikes',
                type: 'GET',
                dataType: 'json',
                data: {commentIds: _idList},
                success:function(json){
                    if (json.error === 0) {
                        json.likeInfo.map(function(v, i) {
                            $('#comment-list-'+v.id).find('.num-good').text(v.likeNum);
                        })
                    }
                }
            })
        }
    };

    function getDestY(destNodeID) {
        var destNode = document.getElementById(destNodeID);
        if ( !destNode ) return;
        var destY = destNode.offsetTop;
        var thisNode = destNode;
        while ( thisNode.offsetParent && (thisNode.offsetParent != document.body) ) {
            thisNode = thisNode.offsetParent;
            destY += thisNode.offsetTop;
        }
        return destY;
    }
    
    discuss.init();
