<script type="text/javascript">
window._onReadyList.push(function(){
	var py = $.cookie('region_pinyin');
	if ( py ) $('#JS_header_logo').attr('href', '/'+py+'/');
},function(){
	var back = $('#JS_header_left_icon');
	if ( back[0] ) {
		back.click(function(){
			history.go(-1);
		});
	}
}, function(){
	var headerMenuIcon = $('#JS_header_right_menu'), headerMenuBox = $('#JS_mll_header_menu');
	if ( !headerMenuIcon[0] ) return;

	headerMenuIcon.click(function(){
		headerMenuBox.html($('#JS_mll_header_menu_textarea').val());
		var h = $(window).height() - 45;
		headerMenuBox.find('.scroll-box').css({'height': h + 'px'});
        $('body').css('position', 'fixed');
		headerMenuBox.removeClass('menuoff').addClass('menuon');
		var imgs = headerMenuBox.find('img');
		imgs.each(function(i){
			if ( imgs.eq(i).attr('lazy-src') ) {
				imgs.eq(i).attr('src', imgs.eq(i).attr('lazy-src'));
			}
		});

		var _headerMenuExpr = $('#JS_header_menu_expr'), _headerMenuIndex = $('#JS_header_menu_index');
		var py = $.cookie('region_pinyin');
		if ( py ) {
			_headerMenuExpr[0] && _headerMenuExpr.attr('href', '/'+py+'/expr.html?fromarticle');
			_headerMenuIndex[0] && _headerMenuIndex.attr('href', '/'+py+'/?fromarticle');
		}
        if($.cookie('region_pinyin')){
            $('#JS_header_article_menu_list a:eq(0)').attr('href','/'+$.cookie('region_pinyin')+'/?fromarticle')
        }
	})
	headerMenuBox.click(function(){
        $('body').css('position','static');
		headerMenuBox.removeClass('menuon').addClass('menuoff');
		setTimeout(function(){
			headerMenuBox.removeClass('menuoff');
		}, 400);
	});
});
</script>

