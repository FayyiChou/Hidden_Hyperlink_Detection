		if( ios_app && ios_app.src){
			box.style.display = "block";
			src = "http://image.meilele.com/" + ios_app.src;
		}
		is_ios = true;
	}else if(  navigator.userAgent.match(/android/i) ){
		if( android_app && android_app.src){
			box.style.display = "block";
			src = "http://image.meilele.com/" + android_app.src;
		}
	}else{
		return;
	}
	clos.onclick = function(){
		box.parentNode.removeChild(box);
	}
	getAppDownFn(["JS_downapp_link"],{ad_code:"M_article_APP_dlink",c:"article_悬浮广告",p:{from:"article"}});

	var fix_img_obj = new Image;
	ajustImgW();
	fix_img_obj.src = src;
	fix_img.setAttribute('src',src);
	function ajustImgW(){
		var clientW = document.documentElement.clientWidth;
			clientW = document.documentElement.clientWidth;
		fix_img.width = clientW;
		clos.style.width = 0.075 * clientW + 'px';
		clos.style.height = 0.075 * clientW + 'px';
	}
	$(window).resize(
		debounce(ajustImgW,200,false,2)
	);
	function debounce(func, threshold, execAsap,execAsapT) {  
		var timeout;  
		return function debounced () {  
		var obj = this, args = arguments; 
		var execTime = 0;
		function delayed(){
			if(execAsapT && execTime < execAsapT){
				(function(){func.apply(obj.args);execTime++;})();
			}else if(!execAsap) func.apply(obj, args);  
		timeout = null;  
		};  
		if (timeout)  clearTimeout(timeout);  
		else if (execAsap) {
			func.apply(obj, args)
			timeout2 = true;
		};  
		timeout = setTimeout(delayed, threshold || 100);  
		};  
	}  
})();
