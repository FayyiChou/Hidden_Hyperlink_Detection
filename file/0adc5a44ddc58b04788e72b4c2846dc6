}(Zepto),function($){//顶部广告
    var src1 = 'http://img001.mllres.com/images/201703/1489706509518081151.jpg',// 大图
        src2 = 'http://image.meilele.com/',// 小图
        img1 = new Image(),
        img2 = new Image(),
        imgWid = 16, //默认宽度
        img1Hei = 10,
        img2Hei = 4,
        tadBtn = $('#JS_toggle'),
        tabBox = $('#JS_ad_div'),
        isBigImg = false,
        offsetTop = $(window).scrollTop(),
        preImg = null;
    img1.src = src1;
    img2.src = src2;
    if (tadBtn.length > 0) { // 大小图
        img1.onload = function () {
            img1Hei = imgWid * img1.height/img1.width || img1Hei;
            img2.onload = function () {
                img2Hei = imgWid * img2.height/img2.width || img2Hei;
                twoHandle();
            };
        };
        img1.onerror = function () {
            twoHandle();
        };
        // 显示顶部广告
        tadBtn.on('click',function(){
            toggle(300, preImg);
        });
        $(window).on('scroll', function(){
            if (isBigImg) {
                var offsetTop2 = $(window).scrollTop();
                if (offsetTop2 > offsetTop) {
                  toggle(300, preImg);
                }
            }
        });
    } else if (tabBox.length > 0 && tadBtn.length === 0) { // 大或小图
        if ($('#JS_img_ad').length > 0) {
            img1.onload = function () {
                img1Hei = imgWid * this.height/this.width || img1Hei;
                tabBox.animate({height: img1Hei + 'rem'}, 0);
                $('#JS_img_ad').removeClass('none');
                setAttr(0, 1, '', img1Hei);
            };
            img1.onerror = function () {
                tabBox.animate({height: img1Hei + 'rem'}, 0);
            };
        }
        if ($('#JS_img_ad2').length > 0) {
            img2.onload = function () {
                img2Hei = imgWid * this.height/this.width || img2Hei;
                tabBox.animate({height: img2Hei + 'rem'}, 0);
                $('#JS_img_ad2').removeClass('none');
                setAttr(0, 0, '', img2Hei);
            };
            img2.onerror = function () {
                tabBox.animate({height: img2Hei + 'rem'}, 0);
            };
        }
        // 显示顶部广告
        $('.M_AD_top').removeClass('none');
    }
    function toggle(time, dom) {
        if(tabBox.attr('data-state') == 0){//大图
            dom.attr('src',src1);
            setAttr(time, 1, '点击收起',  img1Hei);
            isBigImg = true;
            offsetTop = $(window).scrollTop();
        }else{//小图
            dom.attr('src',src2);
            setAttr(time, 0, '点击展开',  img2Hei);
            isBigImg = false;
            offsetTop = $(window).scrollTop();
        }
    }
    function twoHandle() {
        // 首次进去默认大图
        if ($.cookie('adtop')) {//小图
            setAttr(0, 0, '点击展开', img2Hei);
            $('#JS_img_ad2').removeClass('none');
            $('#JS_img_ad').remove();
            preImg = $('#JS_img_ad2');
            isBigImg = false;
        }else{  // 大图
            setAttr(0, 1, '点击收起', img1Hei);
            $('#JS_img_ad').removeClass('none');
            $('#JS_img_ad2').remove();
            preImg = $('#JS_img_ad');
            isBigImg = true;
            $.cookie('adtop', '1', {expires: 1 ,path:'/'});
        }
        $('.M_AD_top').removeClass('none');
    }
    function setAttr(time, state, msg, hei) {
        if (msg) {
            tadBtn.text(msg);
        }
        tabBox.attr('data-state',state);
        if (time !== null) {
            tabBox.animate({height: hei + 'rem'}, time);
        }
    }
}(Zepto), function($){
    // 缩放图片
    var 
        contentBox = $('#jsArticleContent'),
        sw = contentBox.width(),
        imgs = contentBox.find('img');

    imgs.each(function(i){
        var 
            item = imgs.eq(i),
            width;

        if ( item.attr('width') ) {
            width = item.attr('width');
        } else {
            width = item.width();
        }

        if ( width > sw ) {
            item.attr('width', '100%').removeAttr('height');
        }
    });
