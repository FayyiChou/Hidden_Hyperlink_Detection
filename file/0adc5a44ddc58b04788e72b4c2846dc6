function loadCriteo() {	
	if ( criteo_q.length == 0 ) window.criteo_q.push( 
		{event: "viewHome"}
	);
	var mail = $.cookie && $.cookie("ECS[email]");
	window.criteo_q.push(
		{event: "setAccount", account: 28178},
        {event: "setCustomerId", id: mail || ""},
        {event: "setSiteType", type: "m"}
	);
	var cri = document.createElement('script');
	cri.type = 'text/javascript'; cri.async = true;
	cri.src = 'http://static.criteo.net/js/ld/ld.js';
	var c = document.getElementsByTagName('script')[0];
	c.parentNode.insertBefore(cri, c); 
}


//是否发送事件
window.isSendLocation=true;

!function(){
	var _t=$.cookie('_t');
	if (_t=='1') {
		isSendLocation=false
	}
}()

if (isSendLocation) {
	var GPS={};
	GPS.gps=$.cookie('gps');
	if(GPS.gps){
		GPS.gps=GPS.gps.split('|');
		GPS.x=(GPS.gps[1].split(':')[1]);
		GPS.y=(GPS.gps[0].split(':')[1]);
	}
}

//使用自有浏览轨迹系统，发送事件: location
function sendLocation(){
	if (GPS.gps) {
	
			$.ajax({
				url:'http://loc.map.baidu.com/wloc?x='+GPS.x+'&y='+GPS.y+'&r=60&prod=mapapi&addr=province|city|district|street|street_number&fn=_cbk650',
				dataType: 'jsonp', 
				type: 'GET',
				success:function(data){},
				 error:function(){
				 	console.log('error')
				 }
			});
		
	}
} 
//jsonp函数
