<script type="text/javascript">
function getAppDownFn(domId,setting){
	if(!domId)return;
	setting = setting || {};
	var defaultSetting = {
		ad_code : "Mfoot_dlink",
		c       : "unset", //click 来源描述
		r       :  location.href, //referrer 来源网址
		d       :  domId.join("*"), //dom id
		i       : false //初始一次iframe(开启app协议)
	}
	for(var k in defaultSetting){
		if( setting[k] === undefined ){
			setting[k] = defaultSetting[k];
		}
	}
	var param = [];
	for(var k in setting){
		if( k == "p" ){
			for(var kk in setting.p){
				param.push( "p%5B" + kk + "%5D=" + encodeURIComponent(decodeURIComponent(setting.p[kk] )));		
			}
			continue;
		}
		param.push( k + "=" + encodeURIComponent(decodeURIComponent(setting[k] )));
	}
	var ga = document.createElement('script'); 
	ga.type = 'text/javascript'; 
	ga.async = true;
	ga.defer = true;
	ga.src = "http://"+(/(dev|test\.meilele)/i.test( location.host ) ? "test" : "www")+".meilele.com/mll_api/api/appdown?"+param.join("&")+"&_="+(new Date - 0);
	var s = document.getElementsByTagName('script')[0]; 
	s.parentNode.insertBefore(ga, s);
}
