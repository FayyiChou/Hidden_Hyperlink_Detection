<script type="text/javascript">
window._onReadyList = window._onReadyList || [];
_onReadyList.push(function(){
	var _loadSwiper = null;
	$(window).on('load', function(){
		var m = $('#jsArticleMenuSwiper img');
		m.each(function(){
			if ( $(this).attr('lazy-src') ) {
				$(this).attr('src', $(this).attr('lazy-src')).removeAttr('lazy-src');
			}
		});
		_loadSwiper = setInterval(function(){
			if ( Swiper ) {
				clearInterval(_loadSwiper);
				window._menuSwiper = new Swiper('#jsArticleMenuSwiper', {
					lazyLoading: true,
			        slidesPerView: 'auto'
				});
				$(window).resize(function(){
			        setTimeout(function(){
			            _menuSwiper.onResize();
			        }, 400);
			    })
			}
		}, 50);
	});

	// 漂浮菜单
    $(window).on('scroll', function(){
    	var 
    		offset = _headerConfig.menuToggleFixedOffsetTop || $('#JS_mll_header').offset().top,
    		isFixed = $('body').css('position'),
    		show = function() {
    			$('#jsArticleMenuFixedPlaceholder').show();
	            $('#jsArticleMenuBox').addClass('_article_menu_fixed_');
	            $('#jsArticleMenuBox li:last').show();
	            window._menuSwiper && window._menuSwiper.onResize();
    		},
    		hide = function() {
    			$('#jsArticleMenuFixedPlaceholder').hide();
	            $('#jsArticleMenuBox').removeClass('_article_menu_fixed_');
	            $('#jsArticleMenuBox li:last').hide();
	            window._menuSwiper && window._menuSwiper.onResize();
    		};
    	if ( isFixed == 'fixed' ) {
    		hide();
    		return;
    	}
        if ( $(window).scrollTop() >= offset ) {
            show();
        } else {
            hide();
        }
    });

    $('#jsArticleFixedMenu').click(function(){
    	$('#JS_header_right_menu').trigger('click');
    }); 
});
</script>

