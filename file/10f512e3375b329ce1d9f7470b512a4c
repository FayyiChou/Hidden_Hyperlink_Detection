            <SCRIPT>
           var speed=50
           demog.innerHTML=demof.innerHTML
             function Marquee1(){
              if(demog.offsetWidth-demoe.scrollLeft<=0)
              demoe.scrollLeft-=demof.offsetWidth
               else{
           demoe.scrollLeft++
            }
             }
           var MyMar1=setInterval(Marquee1,speed)
           demoe.onmouseover=function() {clearInterval(MyMar1)}
           demoe.onmouseout=function() {MyMar1=setInterval(Marquee1,speed)}
