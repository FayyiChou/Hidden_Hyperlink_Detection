    <script>
      // 统计组件
      $.ajax({ url: '//data.bilibili.com/rec.js',cache: true,dataType:'script'});
      //设置默认点击上传关键字
      rec_rp("setField", "search_click", {
        keyword: encodeURI(url('?keyword')),
        abtest: '',
        abtest_search:''
      });
      // 全局事件委托上传关键字
      $('body').on('click', '[se-linkid]', function(event) {
        var linkid = $(this).attr('se-linkid');
        var linktitle=$(this).attr('title')==undefined?"":$(this).attr('title');
        var linkurl=$(this).attr('href')==undefined?"":$(this).attr('href');
        var linktype=$(this).attr('lnk-type')==undefined?"":$(this).attr('lnk-type');
        var trackid = $("body").attr('data-trackid');
        var sTrackid = $("body").attr('data-trackid_special');
        var isSpecial = false;
        if($(this).parents('.all-other') && $(this).parents('.all-other').length > 0) isSpecial = true;
        rec_rp( "search_click", {
          linkid:linkid,
          linktitle:linktitle,
          linkurl:linkurl,
          linktype:linktype,
          trackid:isSpecial ? sTrackid : trackid
        });
      });
      
    </script>
  

