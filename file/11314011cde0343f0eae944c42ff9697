    var imgLdTime = '', imgStartTime = new Date().getTime();
    var loadImgSucc = function(){
      return function(){        
        if(imgLdTime){
          imgLdTime +=',' + (new Date().getTime() - imgStartTime);
        }else{
          imgLdTime += (new Date().getTime() - imgStartTime);
        }
      }()
