    <script>
        //AllCommonObject.GetVendorDetails(100018363);
        AllCommonObject.InitParam();
        function showCity(pid,pname) {
            
            $.ajax({
                type: "GET",
                url: "/Ajax/getlocationinfo.ashx?pid=" + pid+"&cityId="+AllCommonObject.cityId+"&serialId="+AllCommonObject.serialId+"&dealertype="+AllCommonObject.dealertype,
                success: function (data) {
                    //请求成功
                    if (data && data != "") {
                        $("#childCityUl").html("");
                            
                        $("#childCityUl").append("<li class='root sub-return'><a href='###'>" + pname + "</a></li>");
                        $("#childCityUl").append(data);
                    }
                }
            });
        }

        
    </script>
