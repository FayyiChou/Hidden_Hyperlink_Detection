	 $('.jt>li>a').map(function(){
     $(this).bind({'click':function(){

          i=$(this).attr('href');
          switch(i)
          { 
            case '#items01':
            $('#items01>ul>li').slice(5).hide();
            $('.check_more').attr('href','javascript:wzjt(\'cz\');').html('查看更多交通信息<i class="fa fa-chevron-right"></i>');
            break;
            case '#items02':
            $('#items02>ul>li').slice(5).hide();
            $('.check_more').attr('href','javascript:wzjt(\'dt\');').html('查看更多交通信息<i class="fa fa-chevron-right"></i>');
            break;
            case '#items03':
            $('#items03>ul>li').slice(5).hide();
            $('.check_more').attr('href','javascript:wzjt(\'jd\');').html('查看更多交通信息<i class="fa fa-chevron-right"></i>');
            break;
          }

     }})
   })

