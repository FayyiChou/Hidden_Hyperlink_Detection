 function jiaotong(id,type)
  {
    document.getElementById('divcolse').style.display="";
    var lng = document.getElementById('lng-'+id).value;
    var lat = document.getElementById('lat-'+id).value;
    $('#map_jd').css({'width':'100%','height':'100%','background':'rgba(0,0,0,.4)','position':'fixed','top':'0','left':'0','z-index':'10000'});
     if(type == 1){
      var map = new BMap.Map("con");
    map.centerAndZoom(new BMap.Point("116.231486", "40.277849"), 12);
    var driving = new BMap.DrivingRoute(map, {renderOptions: {map: map, panel: "plul", autoViewport: true}});
    driving.search(new BMap.Point(lng,lat), new BMap.Point("116.231486", "40.277849"));

     }else if(type == 2){
      var map = new BMap.Map("con");
    map.centerAndZoom(new BMap.Point("116.231486", "40.277849"), 12);
    var transit = new BMap.TransitRoute(map, {
      renderOptions: {map: map, panel: "plul"}
    });
        transit.search(new BMap.Point(lng,lat), new BMap.Point("116.231486", "40.277849"));
     } 
  }

