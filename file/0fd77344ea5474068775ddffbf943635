  <section id="section02"> 
   <div class="container"> 
    <div class="row no_margin"> 
     <!-- .left --> 
     <div class="col-md-9"> 
      <!-- 锚点导航 --> 
      <div class="row scrollspy-nav" data-am-sticky="" data-am-scrollspy-nav="{offsetTop: 50px}"> 
       <div class="col-md-12"> 
        <nav class="nav_bar"> 
