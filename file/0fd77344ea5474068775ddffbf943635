  function colse_pl()
  {
  	$('#input_comments').hide();
  }

  function fb_comment(id)
  {
  		

  		if(!id)
  		{
  			alert('发表评论失败');
  			return;
  		}

  		var info=$('#comment_info').val();
  		
  		if(!info)
  		{
  			alert('评论内容不能为空');
  			return;
  		}

  		var conent_en=encodeURI(info);
  		//$('#input_comments').css('display','none');
  		//$('.modal-backdrop.in').remove();
  		$.ajax({
            
                      url:"http://www.changdiwang.com/member/ajax_comment_pl.php",
                      data:"pid="+id+"&content="+conent_en+"&dotype=insert&random="+Math.random(),
                      async:true,
                      type: 'POST',
                      success:function(data)
                      {  
                      	colse_pl();
                        $("#pl_list").html(data);

                 }});



  }

