     function inserttj(curpid){ 

      var ddid ="";
      if(ddid=='' || isNaN(ddid))
      {
        alert('订单ID号错误');
        return;
      }
      $.ajax({
            
            url:"http://www.changdiwang.com/member/inserttj.php",
            data:"ddid="+ddid+"&pid="+curpid+"&random="+Math.random(),
            async:true,
            type: 'POST',
            success:function(data)
            {  

              
              var d=data.split("|");
             

              if(d[0] == '1' || d[0] == '0') 
              {
                submitxj(curpid);
                
              }
              else
              {

                var dd=d[1].replace(/<\/?.+?>/g,""); 
                var dds=dd.replace(/&nbsp;/g,"");
                alert(dds);
              }
              

              
            }});

            
    }  

    function submitxj(curpid){
    
    //显示标签
    var ddid_val = "";
    if(ddid_val =='' || isNaN(ddid_val))
    {
        alert("订单号错误"); 
        return ;
    }
    
        $("#statusdiv_"+curpid).html('询价发送中..');
        
    $.ajax({
            
            url:"http://www.changdiwang.com/member/submitxj_s1_pp.php",
            data:"ddid="+ddid_val+"&random="+Math.random(),
            async:true,
            type: 'POST',
            success:function(data)
            {  
              var d=data.split("||");
          
                $("#statusdiv_"+curpid).html('已询价');
                $('#statusdiv_'+curpid).css('background','#3385ff');
                $('#statusdiv_'+curpid).attr('href','javascript:;');
              

        }});

    



   }

  function fun_merchant(id,aid,fxid)
  {
    
    $('#startdate_r').val('');
    $('#edate_r').val('');
    $('#price').val('');
    $('#citySelect').val('');
    $('#company').val('');
    $('#username').val('');
    $('#mobile').val('');
    $('#city_s').val('');

      $('#sid').val(aid);
      $('#fxid').val(fxid);
    
    
    $('#citySelect').val('');
            $('#city_s').val('');
            $.ajax({
                
            url:"http://www.changdiwang.com/hdpt/controller/controller_getsubcity.php",
            data:"id="+id+"&fxid="+fxid+"&dotype=cityroom&random="+Math.random(),
            async:true,
            type: 'POST',
            success:function(data)
            {
             
              var h=data.split('|');
              $('#citySelect').val(h[0]);
              $('#city_s').val(h[0]);
              $('#price').val(h[1]);
            }  

          })

  }

  function chkdate_r(bdate,edate){
          
          var endLogTimeDate = new Date(Date.parse(edate.replace(/-/g, "/")));  
          var startLogTimeDate = new Date(Date.parse(bdate.replace(/-/g, "/"))); 

          if(startLogTimeDate >endLogTimeDate )
          {
            
            return false;
          }
          
          

    }

  function chklogin_p()
  {
    var data="2016-09-06";

    var city=$('#citySelect').val();
    var city_en=encodeURI(city);

    var start_date=$('#startdate_r').val();
    if(!start_date)
    {
      alert('入住日期不能为空');
      return;
    }
    var start_date_en=encodeURI(start_date);

     if(start_date<data)
    {
        alert('入住日期不能小于今日的日期');
        return ;
    }


    var end_date=$('#edate_r').val();
    if(!end_date)
    {
      alert('离店日期不能为空');
      return;
    }
    var end_date_en=encodeURI(end_date);

    if(chkdate_r(start_date,end_date)==false)
    {
      alert('入住日期不能大于离店日期');
      return;
    }

    var price=$('#price').val();
    if(!/[0-9]{1,}$/.test(price))
    {
      alert('住房价格信息不完善，请稍后预订');
      return;
    }

    var zfjs=$('#fjs').val();
    if(!/[0-9]{1,}$/.test(zfjs))
    {
      alert('住房间数不能为空或不是数字');
      return;
    }

    var company=$('#company').val();
    if(!company)
    {
      alert('公司名不能为空');
      return;
    }
    var company_en=encodeURI(company);

    var name=$('#username').val();
    if(!name)
    {
      alert('姓名不能为空');
      return;
    }
    var name_en=encodeURI(name);

    var mobile=$('#mobile').val();
    if(!/^(0|86|17951)?(13[0-9]|15[0-9]|17[0-9]|18[0-9]|14[57])[0-9]{8}$/.test(mobile))
    {
      alert('手机格式错误');
      return;
    }

    var shid=$('#sid').val();
    var fxid=$('#fxid').val();
    $.ajax({
          url:"controller/controller_create_room.php",
          data:"jbd="+city_en+"&startdate="+start_date_en+"&edate="+end_date_en+"&zfjs="+zfjs+"&zfysmax="+price+"&mobile="+mobile+"&kfgs="+company_en+"&lxs="+name_en+"&shid="+shid+"&fxid="+fxid+"&newver=1&dotype=room&soure=pcnew&random="+Math.random(),
          async:true,
          type: 'POST',
          success:function(data)
          {  
            
             var h=data.split('*');
             
            if(h[0]>0){
              
              window.location.href="http://www.changdiwang.com/palipay/index.php?dotype=room&ddid="+h[1];

            }else{
              alert('您还未登录，请登录后进行操作');
              window.location.href="http://www.changdiwang.com/member/login_wx.php";
            }     

          }
      });


  }

  function fun_Meeting(hid,flag,hytype)
  {
  	$('#search_yanhui').show();
    //初始化
    $('#hdrs').val('');
    $('#startdate_ry').val('');
    $('#ys').val('');
    $('#username_y').val('');
    $('#kfgs_p').val('');
    $('#mobile_y').val('');
    $('#yh_sid').val('');
    $('#hytype').val('');
    //$('#search_yanhui').addClass('in');

    if(flag==1)
    {
      $('#hdtype_hd>option').val('宴会');
      $('#hdtype_hd>option').html('宴会');
      $('#yh_num').html('人数：');
      $('#ptitle_3').html('宴会询价');
    }
    else if(flag==2)
    {
      $('#hdtype_hd>option').val('婚宴');
      $('#hdtype_hd>option').html('婚宴');
      $('#yh_num').html('人数：');
      $('#ptitle_3').html('婚宴询价');
    }
    else if(flag==3)
    {
      $('#hdtype_hd>option').val('年会');
      $('#hdtype_hd>option').html('年会');
      $('#yh_num').html('人数：');
      $('#ptitle_3').html('年会询价');
    }

    $('#yh_sid').val(hid);
    $('#hytype').val(hytype);
    var id="1006";

     $.ajax({
          
      url:"http://www.changdiwang.com/hdpt/controller/controller_getsubcity.php",
      data:"id="+id+"&dotype=cityqy&random="+Math.random(),
      async:true,
      type: 'POST',
      success:function(data)
      {

          $('#citySelect_y').val(data);
      }

    });


  }

  function yh_colse(){
  	$('#search_yanhui').hide();
  }

function chklogin_yh()
{
  var data="2016-09-06";
  var city=$('#citySelect_y').val();
  var citty_en=encodeURI(city);

  var type=$('#hdtype_hd').val();
  var dotype='';
  if(type=='宴会')
  {
    dotype='yanhui';
  }
  else if(type=='婚宴')
  {
    dotype='hunyan';
  }
  else if(type=='年会')
  {
    dotype='nianhui';
  }
  var hdtype_hd_en=encodeURI(type);

  var hdrs=$('#hdrs').val();
  hdrs=$.trim(hdrs);
  if(!/[0-9]{1,}$/.test(hdrs))
  {
     alert(type+'人数必须是数字');
     return;
  }

  if(hdrs=='0')
  {
    alert(type+'不能为0');
    return;
  }


  var startdate_ry=$('#startdate_ry').val();
  if(!startdate_ry)
  {
    alert('使用时间不能为空');
    return;
  }

  if(data>startdate_ry)
  {
    alert('使用时间不能小于今日日期');
    return;
  }


  var ys=$('#ys').val();
  ys=$.trim(ys);
  if(!/[0-9]{1,}$/.test(ys))
  {
    alert('预算必须是数字');
    return;
  }

  if(ys=='0')
  {
    alert('预算不能为0');
    return;
  }

  var kfgs_p=$('#kfgs_p').val();
  if(!kfgs_p)
  {
    alert('公司名不能为空');
    return;
  }

  var kfgs_p_en=encodeURI(kfgs_p);

  var username_y=$('#username_y').val();
  if(!username_y)
  {
    alert('姓名不能为空');
    return;
  }

  var username_en=encodeURI(username_y);

  var mobile_y=$('#mobile_y').val();
  if(!/^(0|86|17951)?(13[0-9]|15[0-9]|17[0-9]|18[0-9]|14[57])[0-9]{8}$/.test(mobile_y))
  {
      alert('手机格式错误');
      return;
  }
  var hytype=$('#hytype').val();

  var shid=$('#yh_sid').val();
  $.ajax({
      url:"http://www.changdiwang.com/hdpt/controller/controller_create_yanhui.php",
      data:"jbd="+citty_en+"&hylx="+hdtype_hd_en+"&yhrs="+hdrs+"&ycysmax="+ys+"&syrq="+startdate_ry+"&kfgs="+kfgs_p_en+"&name="+username_en+"&mobile="+mobile_y+"&shid="+shid+"&dcf=0&step=1&newver=1&dotype="+dotype+"&random="+Math.random(),
      async:true,
      type: 'POST',
      success:function(data)
      {  

        var h=data.split('*');
        if(parseInt(h[0])){
         
        yh_colse();
        $('#cdhy-'+hytype).html('已询价');
        $('#cdhy-'+hytype).css('background','#18A3E4');
        $('#cdhy-'+hytype).removeAttr('onclick');

         
        }else{
          alert('您未登录场地网，请登录');
                  window.location.href="../member/login_wx.php";
        }       
      }

    });


}



  function fun_merchant_x(id,aid,flag)
  {

    $('#startdate_rx').val('');
  
    $('#zzc_price').val('');
    $('#citySelect_x').val('');
    $('#company_x').val('');
    $('#username_x').val('');
    $('#mobile_x').val('');
    $('#city_s').val('');
    $('#lx_price').val('');

      $('#sid').val(aid);
      $.ajax({
          
      url:"http://www.changdiwang.com/hdpt/controller/controller_getsubcity.php",
      data:"id="+id+"&sid="+aid+"&dotype=cityzzcy&random="+Math.random(),
      async:true,
      type: 'POST',
      success:function(data)
      {
        
        var h=data.split('|');
        $('#citySelect_x').val(h[0]);
        if(flag=='1')
        {
          
          $('#zzc_price').val(h[1]);
          
          $('#lx_price').val(flag);
        } 
        else
        {
         
          $('#zzc_price').val(h[2]);
          
          $('#lx_price').val(flag);
        }
        
      }  

    })

  }

  function chklogin_zzc()
  {
  	var data="2016-09-06";
    var city=$('#citySelect_x').val();
    var city_en=encodeURI(city);
    var xzflag=$('#lx_price').val();
    var start_date=$('#startdate_rx').val();
    if(!start_date)
    {
      alert('使用日期不能为空');
      return;
    }

    if(data>start_date)
  	{
    alert('使用时间不能小于今日日期');
    return;
  	}

    var start_date_en=encodeURI(start_date);

    var zzzc='';
    var zzwc='';
    if(xzflag=='1')
    {
       zzzc=$('#zzc_price').val();
       if(zzzc<1)
       {
        alert('自助午餐价格信息不完善，稍后操作');
        return;
       }
    }
    else if(xzflag=='2')
    {
       zzwc=$('#zzc_price').val();
       if(zzwc<1)
       {
        alert('自助晚餐价格信息不完善，稍后操作');
        return;
       }
    }

    var ycrs=$('#ycrs').val();
    if(!/[0-9]{1,}$/.test(ycrs))
    {
      alert('自助餐人数必须是数字');
      return;
    }
    
    var company=$('#company_x').val();
    if(!company)
    {
      alert('公司名不能为空');
      return;
    }
    var company_en=encodeURI(company);

    var name=$('#username_x').val();
    if(!name)
    {
      alert('姓名不能为空');
      return;
    }
    var name_en=encodeURI(name);

    var mobile=$('#mobile_x').val();
    if(!/^(0|86|17951)?(13[0-9]|15[0-9]|17[0-9]|18[0-9]|14[57])[0-9]{8}$/.test(mobile))
    {
      alert('手机格式错误');
      return;
    }

    var shid=$('#sid').val();


   $.ajax({
          url:"http://www.changdiwang.com/hdpt/controller/controller_create_zizhucan.php",
          data:"jbd="+city_en+"&startdate="+start_date_en+"&ycrs="+ycrs+"&zzzc="+zzzc+"&zzwc="+zzwc+"&mobile="+mobile+"&kfgs="+company_en+"&lxs="+name_en+"&shid="+shid+"&xzflag="+xzflag+"&newver=1&dotype=zizhucan&soure=pcnew&random="+Math.random(),
          async:true,
          type: 'POST',
          success:function(data)
          {  
            //alert(data);
             var h=data.split('*');
             
            if(h[0]>0){
              
                            window.location.href="http://www.changdiwang.com/palipay/index.php?dotype=room&ddid="+h[1];
            }else{

              alert('您还未登录，请登录后进行操作');
                            window.location.href="http://www.changdiwang.com/member/login_wx.php";
            }     

          }
      });

  }

  function chklogin_p_r() {
			

			  if(!chk_mobile_win_r())
			     return;

			  $("#btn_1").css("display", "none");
			  //$("#btn_1_doing").css("display", "block");
			  $.ajax({
						url:"/member/ajax_loginchk.php",
						data:"random="+Math.random(),
						async:true,
						type: 'POST',
						success:function(data)
						{  
							
							if(data=='nologin')
							{
								  

								  $("#ptitle_4").html("请提交如下信息，确保推荐场地信息能送达到您手机上！");
								  $("#ptitle_2").html("");

								  $("#space_stpe_1").css("display", "none");
								  $("#space_stpe_chkmobile").css("display", "block");
								  $("#space_stpe_2").css("display", "none");

							}
							else  //已经登录进入发布
							{
								 
								 $("#mid").val(data);
								 submitfrm_place_step1();
								
							}
		

						}});

	}

	function chk_mobile_win_r(){
			
			//检查数据
			var jbd = $("#citySelect_xj").val();
			if(jbd == '0' || jbd == 0 || jbd == '')
			{
					alert("城市未选择"); 
					return false;
			}
			   
			var jbd_en = encodeURI(jbd);
			var hylx = $("#hylx").val();
			var hylx_en = encodeURI(hylx);

			var hdrs_sel = $("#hdrs_sel").val();
			var hdrs;
			if(hdrs_sel == '0')
			{
				hdrs = $("#hdrs").val();
				if( hdrs == "" )
				{
						alert("活动人数必须为整数或不能为空"); 
						return false;
				}
			}
			else
			{
			  hdrs = hdrs_sel;
			}

			var startdate = $("#startdate").val();
			if( startdate == "")
			{
					alert("开始日期不能为空"); 
					return false;
			}

			var startdate_en = encodeURI(startdate);


			var edate;
			var edate = $("#edate").val();
			var jbts = $("#jbts").val();
			if(jbts == '0')
			{
				
				if( edate == "")
				{
						alert("结束日期不能为空"); 
						return false;
				}

			}
			else if(jbts == '11' || jbts == '12' || jbts == '13')
			{

			   $("#cdysdw").val('元/半天');
			   $("#cdysdw_title").html('元/半天');

			}

			var edate_en = encodeURI(edate);




			return true;
	
	}

	function chgview_date(){

			var jbts = $("#jbts").val();
			if(jbts == '0')
			{
				$("#edate").css("display", "inline");
				$("#jbts").css("display", "none");

			}

	}

	function submit_reg_p(){
		
		var login = $("#umobile2_p").val();
			var kfgs = $("#kfgs_pc").val();
			var lxs = $("#lxs_p").val();
			if(lxs == '')
			{
				alert('称呼不能为空');
				return;
			}

			if(!login)
			{
				alert('手机号码不能为空');
				return;
			}

			if(!/^(0|86|17951)?(13[0-9]|15[0-9]|17[0-9]|18[0-9]|14[57])[0-9]{8}$/.test(login))
			{
			    alert('手机格式错误');
			    return;
			}

			if(kfgs == '')
			{
				alert('公司不能为空');
				return;
			}

			

			$("#btn_user_chk_doing").css("display", "block");
			$("#btn_user_chk").css("display", "none");
			$.ajax({
				 dataType:"json",
				 type: "POST",
				 url: "http://www.changdiwang.com/member/ajax_login.php",
				 //data:   "userid="+login+'&pwd='+login+'&vdcode='+captcha+'&company='+company_en+'&logintype=mobile&random='+Math.random(),
				 data:   "userid="+login+'&pwd='+login+'&vdcode=no&logintype=mobile&subtype=sjdxrz_new&random='+Math.random(),
				 success: function(msg){
					//alert(msg);
					if(msg=='1' || msg=='2'){
					   //登录成功
						submitfrm_place_step1();
					}
					else if(msg=='3'){
						timeout = 0;
						alert('校验码错');
						$("#btn_user_chk_doing").css("display", "none");
						$("#btn_user_chk").css("display", "block");
					}
					else if(msg=='4'){
						alert('管理员账号不能从前台登录');
						$("#btn_user_chk_doing").css("display", "none");
						$("#btn_user_chk").css("display", "block");
					}
					else if(msg=='5'){
						alert('账号输入格式错误');
						$("#btn_user_chk_doing").css("display", "none");
						$("#btn_user_chk").css("display", "block");
					}
					else if(msg=='6'){
						alert('输入的账号不存在');
						$("#btn_user_chk_doing").css("display", "none");
						$("#btn_user_chk").css("display", "block");
					}
					else if(msg=='7'){
						alert('手机号不正确，请重输');
						$("#btn_user_chk_doing").css("display", "none");
						$("#btn_user_chk").css("display", "block");
					}
				 } 
			}); 

	}

	 function submitfrm_place_step1() { 

			var jbd = $("#citySelect_xj").val();
			if(jbd == '0' || jbd == 0 || jbd == '')
			{
					alert("城市未选择"); 
					return ;
			}
			   
			var jbd_en = encodeURI(jbd);

			var hdrs_sel = $("#hdrs_sel").val();
			var hdrs;
			if(hdrs_sel == '0')
			{
				hdrs = $("#hdrs").val();
				if( hdrs == "" )
				{
						alert("活动人数必须为整数或不能为空"); 
						return ;
				}
			}
			else
			{
			  hdrs = hdrs_sel;
			}

			var hylx = $("#hylx").val();
			var hylx_en = encodeURI(hylx);

			var startdate = $("#startdate").val();
			if( startdate == "")
			{
					alert("举办日期不能为空"); 
					return ;
			}

			var startdate_en = encodeURI(startdate);


			var edate;
			var edate = $("#edate").val();
			var jbts = $("#jbts").val();
			if(jbts == '0')
			{
				
				if( edate == "")
				{
						alert("结束日期不能为空"); 
						return ;
				}

			}
			else if(jbts == '11' || jbts == '12' || jbts == '13')
			{

			   $("#cdysdw").val('元/半天');
			   $("#cdysdw_title").html('元/半天');

			}

			var edate_en = encodeURI(edate);




			/*
			var khqwqy = setsubcitydiv();
			var khqwqy_en = encodeURI(khqwqy);
			*/


			var kfgs_en = encodeURI($("#kfgs_p").val());
			var lxs_en = encodeURI($("#lxs_p").val());


			var mid = $("#mid").val();
			
	

			//$("#btn_td").html("正在提交需求，请稍后..");
			$.ajax({
						url:"http://www.changdiwang.com/member/add_newht_pc.php",
						data:"jbd="+jbd_en+"&hylx="+hylx_en+"&startdate="+startdate_en+"&edate="+edate_en+"&hdrs="+hdrs+"&jbts="+jbts+"&kfgs="+kfgs_en+"&lxs="+lxs_en+"&mid="+mid+"&dcf=0&step=1&newver=1&dotype=place&soure=index&random="+Math.random(),
						async:true,
						type: 'POST',
						success:function(data)
						{  
							 window.location.href="http://www.changdiwang.com/hdpt/search_list.php?soure=index&hdid="+data;
							 //alert(data);
							 //$("#hdid").val(data);
							  //document.getElementById("gosearch").submit();
							  //open_cd_step2();
							
						}});
		
		

            
    } 
</script>
