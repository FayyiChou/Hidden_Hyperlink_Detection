				            function placeshow(i){
				              if(i == 1){

				                $("#placelist").hide();
				                $("#allplacelist").show();
				                $("#placeshow").html('<a href="javascript:placeshow(0);" >收起</a><i class="fa fa-chevron-up"></i>');
				              }else{
				                $("#placelist").show();
				                $("#allplacelist").hide();
				                $("#placeshow").html('<a href="javascript:placeshow(1);" >查看更多会议室</a><i class="fa fa-chevron-down"></i>');
				              }
