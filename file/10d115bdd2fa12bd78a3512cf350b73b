	<script type="text/javascript">
	
	function showFloorFch (project) {

		$("#projectCity").val(project.cityId);
		$("#projectId").val(project.projectId);
		$("#projectName").html(project.floor);
		$("#projectDate").html(project.beginTimeStr + "~" + project.endTimeStr);
		$("#projectFee").html('<input type="text" readonly name="subsidy" class="input" value="' + project.customerSourceFee + '"/>' + project.customerSourceFeeUnit);
		$(".mobile_detail").show();
	}

	var time_statu = false;
	$(function(){
		$("#closeTk").click(function(){
			$(".mobile_detail").hide();
			time_statu = false;
		});
		
		$("#mailTip").hide();
	});
	
	function submitInfo() {
			
		  $("#projectBtn").attr("disabled", "disabled");
	      if($("#projectCustomerName").val()==""){
	         alert("请输入真实姓名！");
	         $("#projectCustomerName").focus();
	         $("#projectBtn").removeAttr("disabled");
	         return false;
	      }
	      if($("#projectMobile").val()==""){
	         alert("请输入手机号码！");
	         $("#projectMobile").focus();
	         $("#projectBtn").removeAttr("disabled");
	         return false;
	      }
	      if(mobileEmptyVal1()==0){
	         alert("请输入正确的手机号码！");
	         $("#projectMobile").focus();
	         $("#projectBtn").removeAttr("disabled");
	         return false;
	      }
	      if(!$("input[name='check']").is(':checked')){
			 alert("请阅读《中国房产超市网红包使用协议》！");
			 $("#projectBtn").removeAttr("disabled");
			 return false;
		  }
	 	
		$.ajax({
		
			url:"http://newhouse.lc.fccs.com/newhouse/floor/fchBaoMingSave.do?nd=" + new Date().getTime(),
			type: "post",
		    data: {
		    	
		    	projectId: $("#projectId").val(),
		    	city: $("#projectCity").val(),
		    	customerName: escape(escape($("#projectCustomerName").val())),
		    	mobile: $("#projectMobile").val()
		    },
			dataType:'jsonp',  
		    jsonp:'callback',
		    success:function(res) {
		    	
		    	$("#projectBtn").removeAttr("disabled");
		    	alert(res);
				$(".mobile_detail").hide();
				$("#projectCustomerName").val("");
				$("#projectMobile").val("");
		    },
		    error:function(result){
		    	
		    	$("#projectBtn").removeAttr("disabled");
		    }
		});
	}
	
	
	var mobileRex=/^1[0-9][0-9]\d{8}$/;
	var mobileMarker=false;
	var mobileflag=0;
	
	function mobileEmptyVal1(){
	   var flag=1;
	 	var mobile=$("#projectMobile").val();
		if(!mobileRex.test(mobile)){
			
			flag=0;
		}
      return flag;
	}
		
	function mobileEmptyVal(flag){
	 	var mobile=$("#projectMobile").val();
		if(!mobileRex.test(mobile)){
			if(flag==1){
				 $('#mailTip').text("手机号码格式不正确");
				 $("#mailTip").show();
				 $('#mailTip').addClass('colorRed');
				 $('#mobile').focus();
			}
			mobileMarker=false;
			return;
		}
		mobileMarker=true;
	}
	</script>
