        function SetImgOriginalUrl() {
            try {
                $("img[data-original]").each(function (i) {
                    var url = $(this).attr("data-original");
                    if (url != "") {
                        $(this).attr("src", url);
                    }
                });
            } catch (e) {
            }
        }
