<script src="jquery-1.11.3/jquery.js"></script>
<script>
//ppt幻灯效果

$(document).ready(
function(){
var i=1;
var Interval=5000;
var inter=setInterval(autoScroll,Interval);
$("#pptBtns span").click(function(){

 i=$(this).index(); //返回这个元素在同辈中的索引位置
autoScroll();
  clearInterval(inter);
  $(this).css("background","#3366ff");//FFFF00  e7e7e7
$(this).siblings().css("background","#ffffff");   //00FF00 aba399

//setInterval("autoScroll(i)",Interval);
inter=setInterval(autoScroll,Interval);
});

function autoScroll(){
for(var j=0;j<$("#ppt div").length;j++)
{
 if($("#ppt div").eq(j).attr("id") == null)
 $("#ppt div").eq(j).hide(0);
}
$("#ppt div").eq(i).find("img").css({"height":"476px","width":"100%"});
$("#ppt div").eq(i).fadeIn();
$("#pptBtns span").eq(i).css("background","#3366ff");
$("#pptBtns span").eq(i).siblings().css("background","#ffffff");  //00FF00

i++;
if(i==5)i=0;
}

// 图片点击移动效果
  var k=0;
$("#arr_right").click(function(){
   // alert($("#alzs_tps").css("margin-left"));
 
if($("#alzs_tps").css("margin-left")=="-1000px"||$("#alzs_tps").is(":animated"))
 return false;
 else
 { k-=1000;
$("#alzs_tps").animate({marginLeft:k+"px"},1000,function(){return false;});

}

})

$("#arr_left").click(function(){
  
 // alert($("#alzs_tps").css("margin-left"));
if($("#alzs_tps").css("margin-left")=="0px"||$("#alzs_tps").is(":animated"))
 return false;
 else
 {  
 k+=1000;
$("#alzs_tps").animate({marginLeft:k+"px"},1000,function(){return false;});

}

})

  

	$(".suspend").mouseover(function() {
        $(this).stop();
        $(this).animate({width: 160}, 400);
    });
    $(".suspend").mouseout(function() {
        $(this).stop();
        $(this).animate({width: 40}, 400);
    });


}
);


</script>

</head>

