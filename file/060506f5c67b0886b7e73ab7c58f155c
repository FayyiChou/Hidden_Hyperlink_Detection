            <script>
            var ycssnr=document.getElementById("ycssnr");
            var ssnr=document.getElementById("ssnr");
            function qhss(n,obj){
              
                var ycssnrn    = document.getElementById("ycssnr"+n);
                var currentVal = ycssnrn.innerHTML;
                var firstVal   = jQuery(ycssnrn).parent().parent().prev().find('a').html();
                var secondVal  = jQuery(ycssnr).find('p:eq(0) a').html();
                var thirdVal   = jQuery(ycssnr).find('p:eq(1) a').html(); 
                var fourthVal  = jQuery(ycssnr).find('p:eq(2) a').html()
                
                if(n == 1){
                  
                   jQuery(ycssnrn).parent().parent().prev().find('a').html(currentVal);
                   jQuery(ycssnr).find('p:eq(0) a').html(firstVal);
                   
                }else if(n == 2){
                  
                   jQuery(ycssnrn).parent().parent().prev().find('a').html(currentVal);
                   jQuery(ycssnr).find('p:eq(0) a').html(firstVal);
                
                   jQuery(ycssnr).find('p:eq(1) a').html(secondVal);
                   jQuery(ycssnr).find('p:eq(2) a').html(fourthVal);
                }else if(n == 3){
                  
                   jQuery(ycssnrn).parent().parent().prev().find('a').html(currentVal);
                   jQuery(ycssnr).find('p:eq(0) a').html(firstVal);
                
                   jQuery(ycssnr).find('p:eq(1) a').html(secondVal);
                   jQuery(ycssnr).find('p:eq(2) a').html(thirdVal);
                  
                }
                ycssnr.style.display="none";
                
                //获得当前显示的文本
                var ssnr_txt = ssnr.innerHTML;
                var showtext = document.getElementById("_keyword");
                var searchtype = document.getElementById("c");
                var nselect = document.getElementById('nselect');
                if(ssnr_txt == '求购'){
                    searchtype.value = "求购信息";
                    showtext.value= "请输入感兴趣的求购信息";	
                    nselect.value = 1;
                } else if(ssnr_txt == '公司'){
                    searchtype.value = "企业库";
                    showtext.value="请输入感兴趣的公司名称";
                    nselect.value = 2;
                } else if(ssnr_txt == '供应'){
                    searchtype.value = "供应信息";
                    showtext.value="请输入感兴趣的商品";
                    nselect.value = 5;						
                }  else {
                    searchtype.value = "行业资讯";
                    showtext.value="请输入感兴趣的资讯信息";
                    nselect.value = 6;
                }
            }
            function hideYcssnr(){
                ycssnr.style.display = "none";
            }
            
            function showYcssnr(){
                ycssnr.style.display = "";
            }
            document.body.onclick = function(event){
              
                var event = event || window.event;
                var target = event.target || event.srcElement;
               
                if(!(target.id && target.id == '_keyword')){
                   jQuery('#Banner1_Menu').hide(); 
                   //document.getElementById('Banner1_Menu').style.display = 'block';
                } 				
            
            }
            

            var tempVal;  
            function setfocus(obj){
                tempVal = obj.value;
                obj.value = '';
            }

            function setblur(obj){
               obj.value = tempVal;
            }				
              
            </script>
