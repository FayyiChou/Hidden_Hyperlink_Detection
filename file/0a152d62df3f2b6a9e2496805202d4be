     <script>
(function (){
	var searchbox = $$('.searchBar_922 .keywords')[0],
searchbut = $$('.searchBar_922 .btn_search')[0];
if (searchbox.get('value') == '') {
    searchbox.set('value', '');
    searchbox.addEvents({
        'focus': function() {
            this.set('value', '')
        },
        'blur': function() {
            this.set('value', '')
        },
        'change': function() {
            this.removeEvents();
            searchbut.removeEvents()
        }
    })
}
if (searchbox.get('value') == '') {
    searchbut.addEvent('click',
    function() {
        searchbox.set('value', '')
    })
}
})();</script> 
