<script type="text/javascript">

    $(document).on('blur', '#lk-set-price', function() {

        var $lkSetPrice = $('#lk-set-price');

        var price = parseFloat($lkSetPrice.val());
        if (price <= 1) {
            $lkSetPrice.val(price.toFixed(2));
        } else if (price > 1 && price <= 100) {
            $lkSetPrice.val(price.toFixed(1));
        } else if (price > 100) {
            $lkSetPrice.val(price.toFixed(0));
        }
    })

</script>
<script>
    $("#quickBar").on('change', function () {
        var minData = $(this).data('slider-min');
        var maxData = $(this).data('slider-max');
        var percent = ($(this).val() - minData) / (maxData - minData);
        var percent = percent.toFixed(2);
        if (percent <= 0.5) {
            r = 30 + 450 * percent;
            g = 255;
        } else {
            r = 255;
            g = 255 - 450 * (percent - 0.5);
        }
        $(".slider-selection").css('background-color', 'rgb(' + r + ',' + g + ',30)');
        $('.sell-input').each(function () {
            var min = $(this).data('min');
            var max = $(this).data('max');
            var diff = max - min;
            var price = min + (diff * percent);
            $(this).val(price.toFixed(2));
        });
        $('.sell-input').trigger('keyup');
        var loa = $('#quickBar').val();
        $('#discount-val').val(loa);
    });
</script>


