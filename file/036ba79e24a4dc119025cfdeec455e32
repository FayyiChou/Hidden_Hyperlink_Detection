<script type="text/javascript">
/*<![CDATA[*/
jQuery(function($) {

jQuery(document).on('click', '#yw1', function(){
	jQuery.ajax({
		url: "\/api\/purchase\/captcha.html?refresh=1",
		dataType: 'json',
		cache: false,
		success: function(data) {
			jQuery('#yw1').attr('src', data['url']);
			jQuery('body').data('/api/purchase/captcha.hash', [data['hash1'], data['hash2']]);
		}
	});
	return false;
});

});
