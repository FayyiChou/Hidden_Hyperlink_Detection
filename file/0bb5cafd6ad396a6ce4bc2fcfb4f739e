var current_picture = 1, max_pictures = 5;

function changePictures()
{
if( max_pictures == 1 )
return;
$( '#rolling_image_' + current_picture ).fadeOut( 'slow' );
current_picture = current_picture == max_pictures ? 1 : current_picture + 1;
$( '#rolling_image_' + current_picture ).fadeIn( 'slow' );
setTimeout( changePictures, 6 * 1000 );
}

$(window).load(function()
{
setTimeout( function() { $( '.actions' ).height( $( '#rolling_image_' + current_picture ).height() ); }, 100 );
setTimeout( changePictures, 6 * 1000 );
});

$(window).resize(function()
{
$( '.actions' ).height( $( '#rolling_image_' + current_picture ).height() );
});
</script>
