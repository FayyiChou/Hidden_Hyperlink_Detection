<script>
function addToBasket( id, count )
{
processSimpleAsyncReqForModule( 'basket', 1, '&id=' + id + '&count=' + count, 'processAfterAddingToBasket( data, ' + id + ' );' );
}

function changeBusketValue( id, count )
{
processSimpleAsyncReqForModule( 'basket', 3, '&id=' + id + '&count=' + count, 'processAfterAddingToBasket( data, ' + id + ' );' );
}

function deleteFromBasket( id )
{
processSimpleAsyncReqForModule( 'basket', 2, '&id=' + id, 'redirect();' );
}

function processAfterAddingToBasket( data, id )
{
$( 'html' ).css( 'cursor', 'default' );
if( data.length > 0 && data != '0' ) { 
var ar = data.toString().split( '^^^' );
$( '#basket_count' ).html( ar[0] ); 
$( '#basket_summa' ).html( ar[1] ); 
} else { 
$( '#good_' + id ).attr( 'value', '1' );
}
}

function redirect()
{
document.location = '/basket';
}
</script>

<div class='search'>
<input type=text id='search_str' name='search_str' autocomplete='off' value='поиск по каталогу'
onfocus="if( this.value == 'поиск по каталогу' ) this.value = '';"
onblur="if( this.value == '' ) this.value = 'поиск по каталогу';"
onkeypress="
var code = processKeyPress( event );
if( code == 13 && this.value != 'поиск по каталогу' ) { 
var goes = false;
if( $( '.helper_top' ).is( ':visible' ) ) {
$( '.helper_top .prep' ).each( function() {
if( $( this ).hasClass( 'prep_selected' ) ) {
processOnClick( $( this ).html() );
goes = true;
}
});
}
if( !goes )
urlmove( '/search/search_str!' + encodeURIComponent( $( '#search_str' ).val() ) ); 
} else if( code == 46 ) {
hideHelperTop( 1 );
}
"
onkeydown="
var code = processKeyPress( event );
if( code == 40 ) {
event.preventDefault();
var nextset = false, setted = false;
if( $( '.helper_top' ).is( ':visible' ) ) {
$( '.helper_top .prep' ).each( function() {
if( nextset ) {
$( this ).addClass( 'prep_selected' );
nextset = false;
setted = true;
} else if( $( this ).hasClass( 'prep_selected' ) ) {
$( this ).removeClass( 'prep_selected' );
nextset = true;
}
});
if( !setted ) {
$( '.helper_top .prep_first' ).addClass( 'prep_selected' );
}
}
return false;
} else if( code == 38 ) {
event.preventDefault();
var nextset = false, setted = false;
if( $( '.helper_top' ).is( ':visible' ) ) {
jQuery.fn.reverse = function() {
    return this.pushStack(this.get().reverse(), arguments);
};
$( '.helper_top .prep' ).reverse().each( function() {
if( nextset ) {
$( this ).addClass( 'prep_selected' );
nextset = false;
setted = true;
} else if( $( this ).hasClass( 'prep_selected' ) ) {
$( this ).removeClass( 'prep_selected' );
nextset = true;
}
});
if( !setted ) {
$( '.helper_top .prep' ).last().addClass( 'prep_selected' );
}
}
return false;
}
"
onkeyup="
var code = processKeyPress( event );
if( code == 27 ) {
hideHelperTop( 1 );
return;
}
if( this.value.toString().length <= 1 ) {
hideHelperTop( 1 );
return;
}
if( code == 40 || code == 38 )
return false;
getHelperElementsTop( this.value );
"
