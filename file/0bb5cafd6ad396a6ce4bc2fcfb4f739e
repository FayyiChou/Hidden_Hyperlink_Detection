<script>
$(window).load(function()
{
$( '.fancybox' ).fancybox();
showCallbackAskForm();
});

$(window).resize(function()
{
$( '#vk_groups' ).width( $( '#vk_groups' ).parent().width() );
$( '#vk_groups iframe' ).width( $( '#vk_groups' ).width() );
});

function getHelperElementsTop( str )
{
processSimpleAsyncReqForModule( 'catalog', 2, '&str=' + str, 'updateAndOpenHelperTop( data );' );
}

function getHelperElementsCatalog( str, cat )
{
processSimpleAsyncReqForModule( 'catalog', 2, '&str=' + str + ( cat != undefined ? '&cat=' + cat : '' ), 'updateAndOpenHelperCatalog( data );' );
}

function updateAndOpenHelperTop( data )
{
if( !data || data == '' || data == '0' ) {
hideHelperTop( 1 );
return;
}
hideHelperCatalog( 1 );
$( '.helper_top' ).width( $( '#search_str' ).width() + 12 );
$( '.helper_top' ).html( data ).show();
}

function updateAndOpenHelperCatalog( data )
{
if( !data || data == '' || data == '0' ) {
hideHelperCatalog( 1 );
return;
}
hideHelperTop( 1 );
$( '.helper_catalog' ).width( $( '#mh_search' ).width() + 26 );
$( '.helper_catalog' ).html( data ).show();
}

function hideHelperTop( clear )
{
$( '.helper_top' ).hide();
if( clear != undefined )
$( '.helper_top' ).html( '' );
}

function hideHelperCatalog( clear )
{
$( '.helper_catalog' ).hide();
if( clear != undefined )
$( '.helper_catalog' ).html( '' );
}

function processOnClick( data )
{
if( $( '.helper_top' ).is( ':visible' ) ) {
$( '#search_str' ).val( data ).focus();
getHelperElementsTop( data );
} else if( $( '.helper_catalog' ).is( ':visible' ) ) {
$( '#mh_search' ).val( data ).focus();
getHelperElementsCatalog( data );
}
}

function showMobileMenu()
{
$( '.mobile_menu' ).slideDown( 200 );
$( '.modal' ).fadeIn( 200 );
$( '.mobile_nav' ).hide();
$( '.mobile_nav_close' ).show();
}

function hideMobileMenu()
{
$( '.mobile_menu' ).slideUp( 200 );
$( '.modal' ).fadeOut( 200 );
$( '.mobile_nav_close' ).hide();
$( '.mobile_nav' ).show();
}

var clickedOverflow = false;
var clickedAskOverflow = false;

function showCallbackForm()
{
if( $( '.overflow_form' ).is( ':visible' ) )
return;

$( '.overflow_form' ).fadeIn( 300 );
$( '.overflow_form .callback_window' ).animate( { top: '100px', opacity: 1 }, 300 );
setTimeout( function() { $( '#fio' ).focus(); }, 300 );
}

function closeCallbackform( alerting )
{
clickedOverflow = false;
$( '.overflow_form .callback_window' ).animate( { top: '0px', opacity: 0 }, 300 );
$( '.overflow_form' ).fadeOut( 300, function() { $( '#fio' ).val( '' ); $( '#phone' ).val( '' ); $( '.overflow_form .confirm_button' ).attr( 'disabled', false ); if( alerting != undefined && alerting == 1 ) { alert( 'Спасибо! Вам перезвонят!' ); } } );
}

function showCallbackAskForm()
{
if( $( '.overflowadsk_form' ).is( ':visible' ) )
return;

$( '.overflowadsk_form' ).fadeIn( 300 );
$( '.overflowadsk_form .select_window' ).animate( { top: '100px', opacity: 1 }, 300 );
}

function closeCallbackAskform( aptekaselected )
{
if( aptekaselected != undefined && aptekaselected == 1 ) {
set_cookie ( 'aptekaselected', 3, 0, 0, 0, '/' );
}

clickedAskOverflow = false;
$( '.overflowadsk_form .select_window' ).animate( { top: '0px', opacity: 0 }, 300 );
$( '.overflowadsk_form' ).fadeOut( 300 );
}
</script>

