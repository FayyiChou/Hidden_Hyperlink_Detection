//阅读记录
showReaded('single');
var imgID='';
if(imgID){
	var Readed=getcookie('Readed');
	if(Readed){
		Readed=','+Readed+',';
		var reg=new RegExp(','+imgID+',',"g");
		Readed=Readed.replace(reg,',');
		Readed=Readed.substring(0,Readed.length-1);
		Readed=imgID+Readed;
		setcookie('Readed',Readed,30);
	}else{
		setcookie('Readed',imgID,30);
	}
}
