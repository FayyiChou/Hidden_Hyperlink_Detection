<script language="javascript" type="text/javascript">
function changeLang(lang)
{
    var tmpD = new Date();
    var timeStamp = tmpD.getTime() + 365 * 24 * 3600 * 1000;
    tmpD.setTime(timeStamp);
    $.setCookie('lang', lang, tmpD.toGMTString());
	var $current_url=document.location.href;
	var arrStr=$current_url.split(window.location.host);
	if(arrStr[arrStr.length-1]=="/"){
		window.location.href=$current_url+"index_en.html";
	}else{
		var $html_page=arrStr[arrStr.length-1];
		$current_url=$current_url.replace(".html","_en.html");
		window.location.href=$current_url;
	}
}
</script>
