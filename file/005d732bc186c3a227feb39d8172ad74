<!-- Piwik --> 
<script type="text/javascript">
function parseURL(url) {
	var a =  document.createElement('a');
	a.href = url;
	return {
		source: url,
		protocol: a.protocol.replace(':',''),
		host: a.hostname,
		port: a.port,
		query: a.search,
		params: (function(){
			var ret = {},
				seg = a.search.replace(/^\?/,'').split('&'),
				len = seg.length, i = 0, s;
			for (;i<len;i++) {
				if (!seg[i]) { continue; }
				s = seg[i].split('=');
				ret[s[0]] = s[1];
			}
			return ret;
		})(),
		file: (a.pathname.match(/\/([^\/?#]+)$/i) || [,''])[1],
		hash: a.hash.replace('#',''),
		path: a.pathname.replace(/^([^\/])/,'/$1'),
		relative: (a.href.match(/tps?:\/\/[^\/]+(.+)/) || [,''])[1],
		segments: a.pathname.replace(/^\//,'').split('/')
	};
}
var url = parseURL(document.location);
if (url.host == 'emall.chinapay.com')
{
	var piwik_track_url_http  = 'http://analy.f5.chinapay.com:81/';
	var piwik_track_url_https = 'https://analy.f5.chinapay.com:81/';
} else {
	var piwik_track_url_http  = 'http://pwiki.mall.chinova.com:81/';
	var piwik_track_url_https = 'https://pwiki.mall.chinova.com:81/';
}
</script>
<script type="text/javascript">
	 var pkBaseURL = (("https:" == document.location.protocol) ? piwik_track_url_https : piwik_track_url_http);
	 document.write(unescape("%3Cscript src='" + pkBaseURL + "piwik.js' type='text/javascript'%3E%3C/script%3E"));
</script><script src="http://analy.f5.chinapay.com:81/piwik.js" type="text/javascript"></script>
<script type="text/javascript">
	var getArgs = function() {
	    var args = new Object(); // 声明一个空对象
	    var query = window.location.search.substring(1); // 取查询字符串，如从
	                                                     // http://www.snowpeak.org/testjs.htm?a1=v1&a2=&a3=v3#anchor
	                                                     // 中截出 a1=v1&a2=&a3=v3。
	    var pairs = query.split("&"); // 以 & 符分开成数组
	    for ( var i = 0; i < pairs.length; i++) {
	        var pos = pairs[i].indexOf('='); // 查找 "name=value" 对
	        if (pos == -1) {
	            continue; // 若不成对，则跳出循环继续下一对
	        }
	        var argname = pairs[i].substring(0, pos); // 取参数名
	        var value = pairs[i].substring(pos + 1); // 取参数值
	        value = decodeURIComponent(value); // 若需要，则解码
	        args[argname] = value; // 存成对象的一个属性
	    }
	    return args; // 返回此对象
	}


	 try {
		 var piwikTracker = Piwik.getTracker(pkBaseURL + "piwik.php", 1);
		 var referrer = document.referrer ? document.referrer : '';
		 piwikTracker.setReferrerUrl(referrer);
		 
		 var customVariable = piwikTracker.getCustomVariable(1);
		 if (!customVariable){
			 var url_args = getArgs();
             if (url_args['channel']) {
            	 piwikTracker.setCustomVariable(1,"ref", url_args['channel']);
            	 piwikTracker.setCustomVariable(3,"ref_full_url", document.location.href);
             }
		 }
         piwikTracker.setCustomVariable(1, "store", 0, 'page');
         piwikTracker.setCustomVariable(2, "goods", 0, 'page');
         piwikTracker.setCustomVariable(3, "type", 'other', 'page');
         piwikTracker.setCustomVariable(4, "session", jQuery.cookie('PHPSESSID'), 'page');
		 piwikTracker.trackPageView();
		 piwikTracker.enableLinkTracking();
	 } catch( err ) {}
</script>
