(function($){
	if($(window).width()<1240){
		$('.duilian').hide();
	}
	$(window).resize(function(){
		if($(window).width()<1240){
			$('.duilian').hide();
		}else{
			$('.duilian').show();
		}
	});
	$('.closedl').click(function(){
		$(this).parent().parent().remove();
		$('.duilian').remove();
	});
})(jQuery);
</script>
