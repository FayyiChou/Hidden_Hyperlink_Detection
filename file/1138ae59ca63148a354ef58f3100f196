/**
 * for menu-drop type
 */
var slide0 = function () {
	var curpid="0"; 
	var type = "slide";
	if (type == 'slide') {
		$("#pro_type_slide_utU8DG > ul").droppy({curid:"cap_"+curpid});
	}
	
	if(curpid!=""){
		$(function(){
			if (type == 'click') {
				$(".cap_"+curpid).parents("ul").show();
				//setTimeout(function(){$("#cap_"+curpid).parents("ul").show();},1000);
			} else if(type == 'default') {
				$("#pro_type_default_utU8DG ul ul").css('display', 'block');
			} else {
				$('#pro_type_slide_utU8DG').bind("mouseleave",function(){
					setTimeout(function(){$(".cap_"+curpid).parents("ul").slideDown();},1000);
				});
				setTimeout(function(){$(".cap_"+curpid).parents("ul").slideDown();},500);
				//$(".cap_"+curpid).parents("ul").slideDown();
			}
		});	
	}
}
slide0();
