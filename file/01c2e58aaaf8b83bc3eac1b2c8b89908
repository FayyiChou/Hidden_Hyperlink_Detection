<script type="text/javascript">
    $(document).ready(function () {
        $('input').each(function () {
            var ss = $(this);
            ss.bind('propertychange', function () {
                //进行相关操作 
                var a = $(this)[0];
                var re = /and|or|exec|execute|insert|select|delete|update|alter|create|drop|count|\*|chr|char|asc|mid|substring|master|truncate|declare|xp_cmdshell|restore|backup|net +user|net +localgroup +administrators|’|"|=|;|>|<|%|[`~!@#\$%\^\&\*\(\)\+<>\?:"\{\},\.\\\/;'\[\]]/i;
                if (re.test(a.value)) {
                    alert("请您不要在参数中输入特殊字符和SQL关键字！"); //注意中文乱码
                    a.value = '';
                    a.focus();
                }
            });
        });
    });

</script>

