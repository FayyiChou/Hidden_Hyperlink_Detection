<script>
/*
 * 老代码，有空调下
 */
var isIE=!!window.ActiveXObject;
var isIE8=isIE&&!!document.documentMode;
function preventDefault(event) {
        if (event.preventDefault) {
            event.preventDefault();
        } else {
            event.returnValue = false;
        }
}
function next(elem) {
    do {
        elem = elem.nextSibling;
    } while (elem && elem.nodeType != 1);
    return elem;  
} 
 function addEvent(obj,eventType,func){   
     if(obj.attachEvent){   
         obj.attachEvent("on" + eventType,func);   
     }else{   
         obj.addEventListener(eventType,func,false);  
     }   
 }  
 function zoomImg(obj,v, e){  
     if(isIE){  
         var o = obj.parentNode.parentNode;
         var zoom = parseInt(obj.style.zoom, 10) || 100;  
         zoom += v * 10;  
         if(zoom > 0 && isIE8){ 
             obj.style.zoom = zoom + "%";
             var nextPic = next(o)
             var marginTop = obj.offsetHeight * (zoom/100-1);
             nextPic.style.marginTop = marginTop; 
         } else if (zoom > 0) {
                obj.style.zoom = zoom + "%";
                document.body.attachEvent("onmousewheel", preventDefault)
         }  
     }else{  
         var minWidth = 5;  
         var minHeight = 5;  
         var zoomWidth = 0;  
         var zoomHeight = 0;  
         var step = 20;  
         zoomWidth = obj.width + v * step;  
         zoomHeight = obj.height * zoomWidth / obj.width;
         document.body.addEventListener("DOMMouseScroll", preventDefault,false)
         if(zoomWidth > minWidth && zoomHeight > minHeight){  
             obj.width = zoomWidth;  
             obj.height = zoomHeight;  
             obj.id = "max"; 
         }  
     }  
 }  
 function wheel(event){  
     var v = 0;  
     event = event || window.event;  
     var target = event.target || event.srcElement;  
     if (event.wheelDelta) {  
         v = event.wheelDelta/120;  
         if(window.opera){v = -v};  
      } else if (event.detail) {  
         v = -event.detail/3; 
     }  
     if (v) {  
         return zoomImg(target,v);  
     }
    
 }
 var Pic = document.getElementsByTagName("img")
 for(var i=0,len=Pic.length;i<len;i++){
     if (Pic[i].className == "img_list") {
         addEvent(Pic[i], "mousewheel", wheel);   
         addEvent(Pic[i], "DOMMouseScroll", wheel);
     }
 }
function targetElement(e) {
   e = e || window.event;
   return e.target || e.srcElement;
}

function regainScroll(obj) {
    if (obj.addEventListener) {
        obj.addEventListener("DOMMouseScroll", function(e){
            if (targetElement(e).tagName == "DIV" || targetElement(e).tagName == "A") {
               document.body.removeEventListener("DOMMouseScroll",preventDefault,false) 
           }
        },false)
    } else if (obj.attachEvent) {
        obj.attachEvent("onmousewheel", function(e){
            if (targetElement(e).tagName == "DIV" || targetElement(e).tagName == "A") {
               document.body.detachEvent("onmousewheel",preventDefault) 
           }
        }) 
    }
}
regainScroll(document.body)

</script>

