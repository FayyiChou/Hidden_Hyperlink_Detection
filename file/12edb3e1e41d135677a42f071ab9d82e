			function stab(name,cursel,n) {
				for(i=1;i<=n;i++) {
					if (i==cursel) {
						$("#"+name+i).addClass("now");				
						$("#con_"+name+"_"+i).show();
					}
					else {
						$("#"+name+i).removeClass("now");
						$("#con_"+name+"_"+i).hide();
					}
				}
			}
            </script>
