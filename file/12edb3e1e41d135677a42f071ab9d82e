<script type="text/javascript">
$(document).ready(function(){
   $(".fenlei ul li").mouseover(function(){
  $(this).children("ul").show();
  $(this).css("background","#fff");
  $(this).children(".biaoti").css("color","#d21801");
  $(this).children(".biaoti").css("text-decoration","underline");
  $(this).children(".biaoti").css("background","none");
  $(this).children(".bt").children("a").css("color","#999");
  });
   $(".fenlei ul li").mouseout(function(){
  $(this).children("ul").hide();
  $(this).css("background","#88766e");
  $(this).children(".biaoti").css("color","#fff");
  $(this).children(".biaoti").css("text-decoration","none");
  $(this).children(".biaoti").css("background","url(images/sanjiao2.jpg) no-repeat right center");
  $(this).children(".bt").children("a").css("color","#fff");
  });
});
</script>
