		function winresize(){
			$(".header").width($(document.body).width());
			$(".footer").width($(document.body).width());
			$(".header").width($(document).width());
			$(".footer").width($(document).width());
		}
        $(".tab li").mouseenter(function () {
            $(this).parent().find("li").removeClass("checked");
            $(this).addClass("checked");
            var b = $(this).parent().parent().find(".b .i");
            b.removeClass("checked");
            $(b.get($(this).parent().find("li").index($(this)))).addClass("checked");
        });
		$(window).resize(function(){winresize();});
		$(function(){winresize();});
		$(window).scroll(function(){
			if($(window).scrollTop()>127)
			{
				$("#toTop").show();
				if($.browser.msie && ($.browser.version == "6.0"))
				{
					$("#header_menu").attr("style","position:absolute;top:"+$(window).scrollTop()+"px;z-index:9999;left:0;");
					$("#anchor").attr("style","position:absolute;top:"+($(window).scrollTop()+$(window).height()-150)+"px;");
				}
				else
				{
					$("#header_menu").attr("style","position:fixed;top:0;z-index:9999;left:0;");
				}
			}
			else
			{
				$("#header_menu").removeAttr("style");
				$("#anchor").removeAttr("style");
				$("#toTop").hide();
			}
		});
		$("#toTop").click(function(){$("html, body").animate({ scrollTop: 0 }, 300);});
		$("#anchor .qrCode").mouseenter(function(){
			$("#anchor .qr").show();
		}).mouseleave(function(){ $("#anchor .qr").hide(); });
    </script>
