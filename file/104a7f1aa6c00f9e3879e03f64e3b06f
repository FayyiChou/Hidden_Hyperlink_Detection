<script language="javascript">

	function gemsLogin(){
		if(document.getElementById('gems_login').value!=''){
			window.location='http://www.thegemsolution.com/Gems/jsp/login.jsp';
			//window.location='http://localhost:8080/Gems/jsp/login.jsp';
		}else{
			document.getElementById('errSelect').innerHTML="Choose any one from the list";
			document.getElementById('gems_login').focus();
		}
	}
</script>
