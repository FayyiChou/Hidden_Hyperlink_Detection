﻿<script type="text/javascript"> 
 
function strlen(str){  
    var len = 0;  
    for (var i=0; i<str.length; i++) {   
     var c = str.charCodeAt(i);   
    //单字节加1   
     if ((c >= 0x0001 && c <= 0x007e) || (0xff60<=c && c<=0xff9f)) {   
       len++;   
     }   
     else {   
      len+=2;   
     }   
    }   
    return len;  
}  


function submitTest(){
	    var ly= $("#ly").val();
		var lxr=$("#lxr").val();
		var value=$("#wish_value").val();
        if(ly==''){alert('请填写留言内容');return false;}
		if(strlen(ly)>120){alert('留言不得超过120个字符');return false;}
		if(lxr==''){alert('请填写联系人');return false;}
		if(strlen(lxr)>8){alert('联系姓名不得超过8个字符');return false;}
	
}

</script>
