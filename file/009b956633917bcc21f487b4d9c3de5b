(function(){
	var cdl=$('#classdl');
	var dds=cdl.find('dd');
	dds.each(function(index, element) {
		var elem=$(element);
		var box=elem.find('.mtseg');
		var mtbox=elem.find('.mtbox');
		elem.mouseover(function(){
			box.show();
			mtbox.addClass('hover');
			}).mouseout(function(){
			box.hide();
			mtbox.removeClass('hover');
			});
	});
})();
