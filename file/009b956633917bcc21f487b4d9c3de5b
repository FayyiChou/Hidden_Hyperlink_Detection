    function tags(l, u) {
        var a = $(l), b = $(u);
        a.each(function(e, t) {
            $(t).data("d", e).mouseenter(function() {
                var e = $(this), t = e.data("d");
                a.removeClass("idx"), e.addClass("idx");
                b.hide();
                $(b[t]).show()
            })
        });
    }
    $(function() {
        tags('.maintag .ticbg > div', '.maintag ul');
        $('.listbox').each(function(index, element) {
            var elem = $(element);
            var btns = elem.find('.ticbg > .ntic');
            var uls = elem.find('ul');
            tags(btns, uls);
        });
        picmoveTop('#task_recm_mover', '#task_recm');
