	$(function(){
		$(".active-place").sortable({
			handle: '.s-handle',
			cursor: 'move',
			opacity: 0.8,
			items: "> div.section",
			tolerance: 'pointer',
			update:function(){
				$.post('/wid'+'get/',{'blocks':$('.active-place').sortable('toArray')});
			}
		});
		$.getJSON('/curre'+'ncy/', function(json){
			$('#euro').html(json.euro);
			$('#usd').html(json.usd);
		});
		$.getJSON('/weat'+'her/', function(json){
			var prec = json.prec;
			//$('#prec').html(', '+prec.toLowerCase().substring(0, prec.length-1));
		});
		$.getJSON('http://query.yahooapis.com/v1/public/yql?q=select%20item.condition%20from%20weather.forecast%20where%20woeid%3D24553449&format=json&jsoncallback=json', function(data){
			console.log("###");
			var condition = data.query.results.channel.item.condition;

			$('#temp').html(fahrenheitToCelsius(condition.temp)+' c&deg;');
		});

		var setWeather = function(data) {
		  var condition = data.query.results.channel.item.condition;
		  $(function(){
		  var informer = $(".informer", "header"),
		      temp = '<i>В Санкт-Петербурге</i> ' + fahrenheitToCelsius(condition.temp),
		      code = condition.code;
		  informer.children('.meteo').addClass('w_code_' + condition.code).html(temp);
		  informer.show();
		  });
		};

		function fahrenheitToCelsius(f) {
		  var c = (f - 32) * (5/9);
		  if (c > 0) {
		    c = '+' + Math.round(Math.abs(c));
		  } else if (c < 0) {
		    c = '-' + Math.round(Math.abs(c));
		  }
		  return c.toString();
		};
	});
</script>
