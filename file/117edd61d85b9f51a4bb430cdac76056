		$('#last_poll').submit(function(){
			var action = $(this).attr('action');
		    var that = $(this);
		    $.ajax({
		        url : action,
		        type : 'POST',
				dataType : 'json',
		        data : that.serialize()
		        ,success: function(data){
		            switch(data.error){
						case 'no':
							$('#poll_js').html(data.html);
							break;
						case 'not_auth':
							message_login();
							break;
						case 'no_choice':
							alert('Вариант ответа не выбран или не верен');
							break;
						case 'already':
							alert('Вы уже голосовали');
							break;
					}
		        }
		    });
		    return false;
		});
	</script>
