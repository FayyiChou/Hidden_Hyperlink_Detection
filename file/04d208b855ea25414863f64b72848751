 var m_screen_name  = new Array("訂閱\電子報 ");
 var m_data_type = new Array("e");
 var m_null      = new Array(0);
var f_length = m_screen_field.length; 
function datacheck() {
 return datavalid('_EcForm_1821');
}
 
 </script>
