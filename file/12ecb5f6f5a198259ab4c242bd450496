<script type="text/javascript">
var ptDelayTime ;
var ptObj ;
$(".float_box").find("div[data-role='pt']").hover(
function(){
if(ptDelayTime)
clearInterval(ptDelayTime);
$(this).find(".online_service_box").show();
$(this).siblings("div[data-role='pt']").find(".online_service_box").hide();
},
function(){
ptObj = this ;
ptDelayTime = setInterval(function(){
$(ptObj).find(".online_service_box").hide();
},200);
}	
);
</script>
