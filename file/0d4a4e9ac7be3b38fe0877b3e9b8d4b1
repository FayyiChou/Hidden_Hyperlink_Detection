				var speed=35
				decc2.innerHTML=decc1.innerHTML
				function Marquee(){
				if(decc2.offsetWidth-decc.scrollLeft<=0)
				decc.scrollLeft-=decc1.offsetWidth
				else{
				decc.scrollLeft++
				}
				}
				var MyMar=setInterval(Marquee,speed)
				decc.onmouseover=function() {clearInterval(MyMar);
				}
				decc.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
