<script type="text/javascript">
	//跳转到指定的分页
	var max_page = 3;
	function goToPage(id)
	{
		var input_num = document.getElementById(id).value;
		if(isNaN(input_num) || input_num <= 0) {
			alert('请输入大于0的数字!'); 
			return;
		}
		if(input_num > max_page) {
			input_num = max_page;
		}
		var url = "/category-dianshigui/list-s303-p2/?from=page#p";
		if( input_num == 1 ){
			url = url.replace("-p2","");
		}else{
			url = url.replace("-p2","-p"+input_num);
		}
		location.href = url;
	}
</script>


<script type="text/javascript">


window._DATA = {
		'goods_id': '',
		'number':1,
		'show_price':''
	};
	function goSubmit(id,price){
		_DATA.goods_id=id;
		_DATA.show_price=price;
		oneBuy();
	
	}

	function oneBuy(){ //一键购买
		var that = this;
		var goodsData = {};
		goodsData[_DATA.goods_id] = {
			'number': _DATA.number,
			'show_price': _DATA.show_price
		};
		var addedGoods = [];var tmpH = [];
		if( goodsData ){
			for(var k in goodsData){
				var data = goodsData[k];
				if( $.isNumeric(data) ){//如果是数字，代表商品ID
					tmpH.push( k+':'+data );
					addedGoods.push( k );

				} else {
					var tmp = [];
					tmp.push(k);
					addedGoods.push( k );
					tmp.push(data.number || 1);
					tmp.push(data.show_price? data.show_price : '');
					tmp.push(data.act_id ? data.act_id : '');
					tmp.push(data.is_install ? 1 : 0);

					tmpH.push( tmp.join(':') );
				}
			};
			
			tmpH = tmpH.join('|');
		}else{
			$.alert('无效商品!');
			return false;
		}
		var extraData = {'is_logistics':_DATA.isLogistics || ''}
		// 去购物车接口
		$.ajax({
			url:'/ajax_flow/?step=add_to_cart_new&nolist=1&goods_id='+ _DATA.goods_id +'&goods='+tmpH,
			data:extraData,
			cache:false,
			dataType:'json',
			success:function(json){
				if(json.error == 0){
					goEnd( json.recGoods[_DATA.goods_id] );
				}else if(json.error == 1 && json.needLogin == 1){//超过限定个数，需要登录。
					$.cookie("meilele_login",0,{expires : -1});
					$.cookie("ECS[username]",'',{expires : -1});
					$.showLoginBox(function(){//弹出登录框
						that.oneBuy();
					});
				}else{
					$.alert('发生错误。请稍候再试。');
				}	
			},error:function(){
				$.alert('数据连接失败，请重试！');
			}
		})
		// 去结算接口
		function goEnd( recId ){
			var recIdArr = [];
			recIdArr.push( recId  );
			$.ajax({
				url:'/core_api/PushMem/apicheckCartGoods/',
				type:'post',
				data:{rec_id:recIdArr},
				dataType:'json',
				success:function(json){
					if(!$.cookie('ECS[username]') || $.cookie('meilele_login') != 1){
						$.showLoginBox(function(){
							that.oneBuy();
						});
						return false;
					}
					var h = '<form action="/flow/?step=pre_checkout" method="post" id="JS_pre_check_form_one" target="_self"><input type="hidden" value="'+ recId+'" name="rec_id[]"/></form>';
						$('body').append(h);
					setTimeout(function(){
						$('body').find('#JS_pre_check_form_one').submit();
					}, 10);
				},error:function(){
					alert('数据连接失败，请重试！');
				}
			})
		}
				
	}

</script>



