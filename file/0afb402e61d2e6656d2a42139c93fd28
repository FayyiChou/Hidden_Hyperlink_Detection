;(function(_curRegionInfo,_allCityInfo){
	var proId = 0;
	_curRegionInfo.pro = 0;
	_curRegionInfo.city = 0;
	_curRegionInfo.region = 0;
	_curRegionInfo.proName = '0' || '';
	_curRegionInfo.cityName = '0' || '';
	_curRegionInfo.regionName = '0' || '';
	_curRegionInfo.fromCookie = 0;//地区信息不从cookie中来
	proId = 0;

	var regionId = M.getCookie('region_id') || 0;
	if ( !regionId ) return false;
	var _info = _allCityInfo[regionId];
	_curRegionInfo.pro = _info.province_id || 0;
	_curRegionInfo.city = _info.city_id || 0;
	_curRegionInfo.region = 0;
	_curRegionInfo.proName = _info.province_name || '';
	_curRegionInfo.cityName = _info.city_name || '';
	_curRegionInfo.regionName = '';
	_curRegionInfo.fromCookie = 1;//地区信息从cookie中来
	proId = _info.province_id;

	if ( !proId ) return false;
	var cityData = [];
	for ( var c in _allCityInfo ) {
		if ( _allCityInfo[c].province_id == proId ) {
			cityData.push({region_id : _allCityInfo[c].city_id,region_name : _allCityInfo[c].city_name});
		}
	}
	_curRegionInfo.cityData = cityData;
})(_curRegionInfo,_allCityInfo);
