<script type="text/javascript">
var guessYouLike = {
	init: function() {
		guessYouLike.root = $('#JS_like_root');
		guessYouLike.ajaxGoodsPanel = guessYouLike.root.find('ul');
		guessYouLike.ajaxCatPanel = guessYouLike.root.find('.JS_cat_20150527');
		guessYouLike.ajaxStylePanel = guessYouLike.root.find('.JS_style_20150527');

		guessYouLike.getDataByAjax();
	},
	getDataByAjax: function() {
		var 
			that = this,
			uTag = $.cookie('utag') || '',
			url = location.pathname;

		$.ajax({
			url: '/solr_api/recommend/listpage/?userTags='+uTag+'&pageUrl='+url+'&pageSize=12',
			type: 'GET',
			dataType: 'json',
			success: function(json) {
				if ( json ) {
					that.render(json);
				}
			}
		});
	},
	render: function(json) {
		var cat = [], style = [], goods = [], data = json.goods;

		if ( data.length <= 0 ) return;

		if ( json.recm_cats && json.recm_cats.length > 0) {
			var cd = json.recm_cats;
			cat.push('<span>您关注的分类：</span>');
			for ( var c = 0, cc = json.recm_cats.length; c < cc; c++ ) {
				cat.push('<a href="'+cd[c].url+'" target="_blank" title="'+cd[c].keyword+'">'+cd[c].keyword+'</a>');
			}
			this.ajaxCatPanel.html(cat.join(''));
		}

		if ( json.recm_styles && json.recm_styles.length > 0 ) {
			var sd = json.recm_styles;
			style.push('<span>您喜欢的风格：</span>');
			for ( var s = 0, ss = json.recm_styles.length; s < ss; s++ ) {
				style.push('<a href="'+sd[s].url+'" target="_blank" title="'+sd[s].keyword+'">'+sd[s].keyword+'</a>');
			}
			this.ajaxStylePanel.html(style.join(''));
		}

		for ( var i = 0, ii = data.length; i < ii; i++ ) {
			var last = '';
			if ( window.LOAD ) {
				if ( i % 4 == 3 ) last = ' last4';
			} else {
				if ( i % 4 == 3 ) last = '';
				if ( i % 3 == 2 ) last = ' last3';
			}
			goods.push('<li class="g-item'+last+'">');
				if ( data[i].topr_img ) {
					goods.push('<div class="g-float-custom" style="background:url('+_IMG+'/'+data[i].topr_img+') center center no-repeat;_background: none;_filter: progid:DXImageTransform.Microsoft.AlphaImageLoader('+_IMG+'/'+data[i].topr_img+');"></div>');
				}
				goods.push('<div class="g-dtl">');
					goods.push('<a target="_blank" href="'+data[i].goods_uri+'?from=list_tj1"><img class="d-img" src="'+_IMG+'/'+(data[i].goods_thumb_2 || data[i].goods_thumb_1)+'" alt="'+data[i].new_goods_name+' 图片" target="_blank"></a>');		
					goods.push('<div class="d-price">');
						goods.push('<strong class="p-money"><sub class="m-mark">&yen;</sub><span data-goods_id="'+data[i].id+'" class="m-count JS_async_price">'+data[i].show_price+'</span></strong>');
						if ( data[i].price_name != '本站价' && data[i].shop_price ) {
							goods.push('<del class="p-del">&yen;'+data[i].shop_price+'</del>');
						}
					goods.push('</div>');
					var linkTitle, catName, redTitle = '';
					if ( data[i].desc ) {
						linkTitle = redTitle = data[i].desc;
					} else if ( data[i].red_title ) {
						linkTitle = redTitle = data[i].red_title;
					} else {
						linkTitle = data[i].new_goods_name;
					}
					catName = (!data[i].style_hidden) ? data[i].style_name : '';
					goods.push('<a href="'+data[i].goods_uri+'?from=list_tj1" target="_blank" title="'+linkTitle+'" class="d-name"><span>['+data[i].brand_name+'] '+catName+' '+data[i].new_goods_name+'</span><span class="n-extra">'+redTitle+'</span></a>');
					goods.push('<div class="d-tags">');

					if (data[i].total_sold_yes_count>0) {
						goods.push('<span class="t-item t-sale"><a target="_blank" href="'+data[i].goods_uri+'?from=list_tj1">已售 <b class="t-num">'+(data[i].total_sold_yes_count || 0)+'</b></a></span>');
					}
					if (data[i].comment_good_count>0) {
						goods.push('<span class="t-item t-score"><a target="_blank" href="'+data[i].goods_uri+'?from=list_tj1">评论 <b class="t-num">'+(data[i].comment_good_count || 0)+'</b></a></span>');
					}
						
					
					goods.push('</div>');
				goods.push('</div>');
			goods.push('</li>');
		}

		this.ajaxGoodsPanel.html(goods.join(''));
		this.root.show();
	}
};
window._onReadyList = (window._onReadyList || []);
window._onReadyList.push(guessYouLike.init);
</script>
	 


