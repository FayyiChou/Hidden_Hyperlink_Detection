    _PreLoadImg([
        $(".full .slides li img").attr("src")
    ],function(){
        FullBg_banner($(".full .slides li"), $(".full .slides li img"));
    })

    $(document).ready(function () {
      slider.init();
      proloader.init();
    })

    get_size();
    $(window).resize(function(){
        get_size();
    })  
    function get_size(){
        var _W = $(window).width(),
            _H = $(window).height();
        $(".banner").height(_H);
        $(".banner1-list").height(_H);
    }
    function FullBg_banner(box, obj){
        box.css("background", "none")
        obj.eq(0).stop().fadeIn(1000)
        function resizeBg() {
            obj.removeClass("w-f").removeClass("h-f").css("margin", 0)
            var boxR = box.width() / box.height(),
                objR = obj.width() / obj.height();
            if ( objR < boxR ) {
                // obj.addClass('w-f').css("margin-top", -(obj.height() - box.height()) / 2);
                obj.addClass('w-f').css("margin-top", 0 );
            } else {
                obj.addClass('h-f').css("margin-left", -(obj.width() - box.width()) / 2);
            }   
        }
        $(window).resize(resizeBg).trigger("resize");
    }
</script>
