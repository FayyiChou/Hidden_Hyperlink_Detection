var _this;
$(".header-se .menu p").hover(function(){
	_this = $(this);
	_this.find(".cn").stop(true,true).animate({"top":"0"},500);
	_this.find(".en").stop(true,true).animate({"top":"-24px"},500);
},function(){
	_this.find(".cn").stop(true,true).animate({"top":"24px"},500);
	_this.find(".en").stop(true,true).animate({"top":"0"},500);
})

$(".bdsharebuttonbox .bds_weixin").hover(function(){
	$(this).find(".weixin").stop(true,true).animate({"width":"90","height":"90"},500);
},function(){
	$(this).find(".weixin").stop(true,true).animate({"width":"0","height":"0"},500);
})
