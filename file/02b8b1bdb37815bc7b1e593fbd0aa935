<script src="http://www.re-shake.com/static/js/plug.preload.js?v=1475808809" type="text/javascript" charset="utf-8"></script><script src="http://www.re-shake.com/static/web/js/jquery.flexslider.js?v=1475808809" type="text/javascript" charset="utf-8"></script><script src="http://www.re-shake.com/static/web/js/modernizr.custom.49256.js?v=1475808809" type="text/javascript" charset="utf-8"></script><script src="http://www.re-shake.com/static/web/js/site.js?v=1475808809" type="text/javascript" charset="utf-8"></script><script>
    setTimeout(function(){
        $(".loader .rect-grey").animate({"height":"0"},1200,"easeInOutExpo",function(){
            $(".loader").hide();
            $(".header").animate({"top":"0"},800,"easeInOutExpo")
