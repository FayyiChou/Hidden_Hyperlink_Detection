<script type="text/javascript">
var islog = false;
var servicepage = "";
function submitLogin(){
	if(islog) {
		return;
	}
	$("#errors").html("");
	if(!$("#account").val()) {
		$("#account").focus();
		$("#errors").html("请输入用户名");
		return;
	}
	if(!$("#password").val()) {
		$("#password").focus();
		$("#errors").html("请输入密码");
		return;
	}
	if(!$("#captcha").val()) {
		$("#captcha").focus();
		$("#errors").html("请输入验证码");
		return;
	}
	islog = true;
	if($("#typeid_per").attr("checked")=="checked"){
		$.post("/login/person_login.do",$("#loginform").serialize(),function(data, textStatus, jqXHR){
						var return_flag = eval("("+data+")");
			             if(return_flag.hasError){
			                 //////removeLodingDiv();//撤销蒙板
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 var str = "";
			                 if(return_flag.errors.account!=undefined){
			                     str += return_flag.errors.account+"<br/>";
			                 }
			                 if(return_flag.errors.password!=undefined){
			                     str += return_flag.errors.password+"<br/>"
			                 }
			                 if(return_flag.errors.captcha!=undefined){
			                    str += return_flag.errors.captcha+"<br/>"
			                 }
			                // new agileui.status(str,"top",600).show(2);
			                showmsg(str);
			                 $("#captcha").val("");
			             }
			             if(return_flag=="wrongaccount"){
			            	 islog = false;
			                 //////removeLodingDiv();//撤销蒙板
			                changesrc();
			                keypress = 0;//防止连续回车事件全局参数
			                $("#captcha").val("");
			                $("#account").focus();
			                //var b = new agileui.status("帐户不存在","top",600);
			                //b.show(2);
			                showmsg("密码和用户名不匹配!");
			             }
			            if(return_flag=="wrongpass"){
			            	islog = false;
			                //////removeLodingDiv();//撤销蒙板
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 $("#captcha").val("");
			                 $("#password").focus();
			                // new agileui.status("密码和用户名不匹配","top",600).show(2);
			                showmsg("密码和用户名不匹配!");
			               }
			            if(return_flag=="captchawrong"){
			            	islog = false;
			                ////removeLodingDiv();//撤销蒙板
			                // mbox.alert("信息提示","该账号已被冻结，请联系管理员！",function(){});
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 $("#captcha").val("");
			                 $("#captcha").focus();
			                // new agileui.status("验证码错误,请重新填写！","top",600).show(2);
			                showmsg("验证码错误,请重新填写！");
			             }
			            if(return_flag=="different"){
			            	islog = false;
			                ////removeLodingDiv();//撤销蒙板
			                // mbox.alert("信息提示","该账号已被冻结，请联系管理员！",function(){});
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 $("#captcha").val("");
			                 $("#captcha").focus();
			                // new agileui.status("验证码错误,请重新填写！","top",600).show(2);
			                showmsg("人员基础库信息和登录校验信息不一致,请重新填写！");
			             }
			            if(return_flag=="aae100false"){
			            	islog = false;
			                ////removeLodingDiv();//撤销蒙板
			                // mbox.alert("信息提示","该账号已被冻结，请联系管理员！",function(){});
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 $("#captcha").val("");
			                 $("#captcha").focus();
			                // new agileui.status("验证码错误,请重新填写！","top",600).show(2);
			                showmsg("该用户在人员基础库中无效,请重新填写！");
			             }
			            if(return_flag=="noperson"){
			            	islog = false;
			                ////removeLodingDiv();//撤销蒙板
			                // mbox.alert("信息提示","该账号已被冻结，请联系管理员！",function(){});
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 $("#captcha").val("");
			                 $("#captcha").focus();
			                // new agileui.status("验证码错误,请重新填写！","top",600).show(2);noperson
			                showmsg("在人员基础库中无该用户信息,请到相关部门核实！");
			             }
			            if(return_flag=="captchaexpire"){
			            	islog = false;
			                ////removeLodingDiv();//撤销蒙板
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 $("#captcha").val("");
			                 //new agileui.status("验证码过期,请重新填写！","top",600).show(2);
			                 showmsg("验证码过期,请重新填写！");
			            }
			            if(return_flag=="isLocked"){
			            	islog = false;
			                ////removeLodingDiv();//撤销蒙板
			                 changesrc();
			                 keypress = 0;//防止连续回车事件全局参数
			                 $("#captcha").val("");
			                // mbox.alert("信息提示","该账号已被冻结，请联系管理员！",function(){});
			                 showmsg("该账号已被冻结，请联系管理员！");
			                 $("#captcha").val("");
			                 $("#captcha").focus();
			             }
			           /*  if(return_flag[0]=="mailVerification"){
			                ////removeLodingDiv();//撤销蒙板
			                 keypress = 0;//防止连续回车事件全局参数
			                 //var aac001 =  return_flag[1];
			                 //$("#aac001").val(aac001);
			                 $("#loginform").submit();
			            } */
			            if(return_flag=="success"){
			            /*      if(servicepage){
			    				window.location.href=servicepage;
			    			}else{
			    				window.location.href="/person/a/pnaa.do";
			    			}
			    			return;
			            }  */
							if(servicepage){
								window.location.href=servicepage;
							}else{
								window.location.href="/cjob/index.do";
							}  
			            return;
						}
					});
	}else if($("#typeid_com").attr("checked")=="checked"){
		ajax("/login/docompany.do", $.formVal("#loginform"), function(data){
			$("#account").select();
			if(data.hasError == 1) {
				islog = false;
				changesrc();
		        keypress = 0;//防止连续回车事件全局参数
		        $("#captcha").val("");
		        $("#captcha").focus();
		       showmsg("用户名或密码不正确,请重新填写！");
				return;
			}
			if(data.hasError == 2) {
				islog = false;
				changesrc();
		        keypress = 0;//防止连续回车事件全局参数
		        $("#captcha").val("");
		        $("#captcha").focus();
		       showmsg("用户账户已被锁定，暂不能登录！");
				return;
			}
			if(data.hasError == 3) {
				islog = false;
				changesrc();
		        keypress = 0;//防止连续回车事件全局参数
		        $("#captcha").val("");
		        $("#captcha").focus();
		       showmsg("请输入用户名、密码和验证码！");
				return;
			}
			if(data.hasError == 4) {
				islog = false;
				changesrc();
		        keypress = 0;//防止连续回车事件全局参数
		        $("#captcha").val("");
		        $("#captcha").focus();
		       showmsg("验证码不正确！");
				return;
			}
			if(data.hasError == 0) {
				//$("#errors").html("登录成功，正在跳转...");
				if(servicepage){
					window.location.href=servicepage;
				}else{
					window.location.href="/company/a/cyaa.do";
				}
				return;
			}
	});
	}
}
//注册
function doreg(){
	if($("#typeid_per").attr("checked")=="checked"){
		window.open("http://card.12333k.cn/siweb/login.do?method=password");
	}
	if($("#typeid_com").attr("checked")=="checked"){
		window.open("/register/company/register.do");
	}
}
//密码修改
function doupdate(){
	if($("#typeid_per").attr("checked")=="checked"){
		window.open("http://card.12333k.cn/siweb/cardregister.do?method=forgetpsw");
	}
	if($("#typeid_com").attr("checked")=="checked"){
		window.open("/login/cfind.do?find=2");
	}
}
	//身份证校验
function isCardID(sId){ 
	 var aCity={11:"北京",12:"天津",13:"河北",14:"山西",15:"内蒙古",21:"辽宁",22:"吉林",23:"黑龙江",31:"上海",32:"江苏",33:"浙江",34:"安徽",35:"福建",36:"江西",37:"山东",41:"河南",42:"湖北",43:"湖南",44:"广东",45:"广西",46:"海南",50:"重庆",51:"四川",52:"贵州",53:"云南",54:"西藏",61:"陕西",62:"甘肃",63:"青海",64:"宁夏",65:"新疆",71:"台湾",81:"香港",82:"澳门",91:"国外"};
	 var iSum=0 ;
	 var info="" ;
	 if(!/^\d{17}(\d|x)$/i.test(sId)){ showmsg("您输入的身份证长度或格式错误！"); flag=false;return;}
	sId=sId.replace(/x$/i,"a"); 
	 if(aCity[parseInt(sId.substr(0,2))]==null) {showmsg("您输入的身份证地区错误！"); flag=false;return;}
	sBirthday=sId.substr(6,4)+"-"+Number(sId.substr(10,2))+"-"+Number(sId.substr(12,2)); 
	 var d=new Date(sBirthday.replace(/-/g,"/")) ;
	 if(sBirthday!=(d.getFullYear()+"-"+ (d.getMonth()+1) + "-" + d.getDate())) {showmsg("您身份证上的出生日期错误！");flag=false; return;}
	 flag=true;
} 
//消息提示
function showmsg(msg){
   mb = mbox({
           title:"系统提示",
           message:msg
   });
   setTimeout(function(){mb.close()}, 3500);
}
//更改验证码
function changesrc(){
   $("#f_svl").attr("src","/captcha.svl?d="+new Date()*1);
}

</script>