<script type="text/javascript">
	$(function() {
		$("#aab004").hide();
		$("#aab004").val('');
		$(".mysorcpic").KinSlideshow();
		$(".login_area").load("/cjob/indexlogin.do",function(){
			$(".login_input").keydown(function(event){
				if(13 == event.which){
					submitLogin();
				}
			});
		});
		
	})
	//工种选择框
	function onWorkTypeTreeSelect() {
		var aa12 = new agileui.ca11Selector({
			objid : "ca11",
			maxSelectItem : "1"
		});
	}

	//地区选择框
	function onRegionTreeSelect() {
		var ca11 = new agileui.aa12Selector({
			objid : "aa12",
			maxSelectItem : "1"
		});
	}

	function spread() {
		$("[name=input1]").toggleClass("Box1Button Box1ButtonH");
		$("[name=input2]").toggleClass("Box1ButtonH Box1Button");
		if ($("[name=input1]").hasClass("Box1ButtonH")) {
			$("#aab004").show();
			$("#acb217").hide();
			$("#acb217").val('');
		} else {
			$("#aab004").hide();
			$("#aab004").val('');
			$("#acb217").show();
		}
	}
	function submitForm() {
		$("#form").submit();

	}
</script>
