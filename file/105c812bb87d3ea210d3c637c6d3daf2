function chk_search() {
   if (document.frm_search.needle.value=='') {
       alert('Не заполнено поле поиска.');
       return false;
   }
   else return true;
}
function check_send_form() {
    var msg="";
    if ((document.frm_send.send_fio.value=='') || (document.frm_send.send_fio.value=='ФИО')) msg="Не заполнено поле Ваше имя.\n";
    if ((document.frm_send.send_email.value=='') || (document.frm_send.send_email.value=='E-mail')) msg+="Не заполнено поле E-mail.\n";
    if (document.frm_send.send_msg.value=='') msg+="Не заполнено поле Вопрос.\n";
    if (document.frm_send.send_control.value=='') msg+="Не заполнено поле Результат сложения.";
    if (msg=="") return true;
    else {
        msg+="\n\nСообщение не может быть отправлено.";
        alert (msg);
        return false;
    }
}
function show_txt(sw) {
   var p_up=document.getElementById('p_up');
   var p_down=document.getElementById('p_down');
   if (sw==1) {
      p_up.style.display='none';
      p_down.style.display='block';
   }
   else {
      p_up.style.display='block';
      p_down.style.display='none';
   }
}
</script>
