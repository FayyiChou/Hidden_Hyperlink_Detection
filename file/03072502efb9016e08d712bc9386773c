<script type="text/javascript">
function DownSoft(siteid,placeid){
if(placeid != "" && placeid != "undefined"){
var url1="http://www.jj.cn/download/download.php?place="+placeid; 
}else{
var url1="http://www.jj.cn/download/download.php";
}
url1 = escape(url1);
clickStat(siteid,6,url1);
}
var ua = navigator.userAgent.toLowerCase(),
ipad = ua.match(/ipad/i) == "ipad",
iphoneos = (ua.match(/iphone os/i) == "iphone os") || (ua.match(/iph os/i) == "iph os"), 
android = (ua.match(/android/i) == "android") || (ua.match(/adr/i) == "adr"),
andr_pad = (ua.match(/android/i) == "mi pad"), 
vm = ua.match(/windows phone/i) == "windows phone",
vm_1 = ua.match(/windows/i) == "windows",
vm_2 = ua.match(/arm/i) == "arm";

window.onload = function (){
json = {
android : 1746,//place	(要改)
ipad :903,//place	(要改)
iphoneos : 734,//place	(要改)
wp : 380,//place	(要改)
pc: 1544//place	(要改)
}

if((android && andr_pad) || android) { 
DownSoft(36198,json["android"]);//36246代表渠道号	(要改)
}else if(ipad) {
DownSoft(4501640,json["ipad"]);//36246代表渠道号	(要改)
}else if(iphoneos){
DownSoft(4501640,json["iphoneos"]);//36246代表渠道号	(要改)
}else if(vm||(vm_1&&vm_2)) {
DownSoft(73005,json["wp"]);//36246代表渠道号	(要改)
}else{ 
DownSoft(36198,json["pc"]);//36246代表渠道号	(要改)
}
}
</script>

