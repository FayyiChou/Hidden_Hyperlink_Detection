    <script type="text/javascript">
        $(function () {

            //购物车数量
            $(".top_menu .cart span").load("/ShopCart/Count");


            //加入购物车
            $(".btn_addCart").on("click", function () {
                var btn_addCart = $(this);
                var cpbh = $(this).val();
                var quantity = "1";
                var txt_quantity = $(this).parent().find(".txt_quantity");
                if (!$.isEmptyObject(txt_quantity))
                    quantity = txt_quantity.val();

                $.ajax({
                    data: $.param({ jxc_cpbh: cpbh, quantity: quantity }, true),
                    type: "POST",
                    url: "/ShopCart/Create/",
                    success: function (data) {
                        if (data == true) {
                            //购物车数量
                            $(".top_menu .cart span").load("/ShopCart/Count");
                            btn_addCart.attr("disabled", "disabled");
                            try {
                                $("#btn_addCart_info").html("加入成功!!");
                            } catch (e) { }
                        }
                        else if (data == false) {
                            location.href = '/Login/Index';
                        }
                        else {
                            alert(data);
                        }
                    },
                    error: function (err) {
                        alert(err.responseText);
                    }
                });
            });

            ////菜单
            //$('.fenlei ul li').mouseenter(function () {
            //    $(this).stop().animate({ 'height': '289px' }, 300).siblings().stop().animate({ 'height': '44px' }, 300);
            //    //$(this).siblings().css('background', '#222');
            //    $('.fenleiright').fadeTo(0, 0.8).stop().animate({ 'width': '289px' }, 300);
            //}).mouseleave(function () {
            //    $('.fenlei ul li').stop().animate({ 'height': '79px' }, 300)
            //    //$(this).siblings().css('background', '#222');
            //});
            //$('.navLeft').mouseleave(function () {
            //    $('.fenleiright').stop().animate({ 'width': '0px' }, 300);
            //});

            ////内页菜单收起
            //$(".navLeft p").bind("click", function () {
            //    $(".navLeft .fenlei").slideToggle(300);
            //});


            //菜单2
            $('.all-sort-list > .item').hover(function () {
                var eq = $('.all-sort-list > .item').index(this),				//获取当前滑过是第几个元素
                    h = $('.all-sort-list').offset().top,						//获取当前下拉菜单距离窗口多少像素
                    s = $(window).scrollTop(),									//获取游览器滚动了多少高度
                    i = $(this).offset().top,									//当前元素滑过距离窗口多少像素
                    item = $(this).children('.item-list').height(),				//下拉菜单子类内容容器的高度
                    sort = $('.all-sort-list').height();						//父类分类列表容器的高度

                if (item < sort) {												//如果子类的高度小于父类的高度
                    if (eq == 0) {
                        $(this).children('.item-list').css('top', (i - h));
                    } else {
                        $(this).children('.item-list').css('top', (i - h) + 1);
                    }
                } else {
                    if (s > h) {												//判断子类的显示位置，如果滚动的高度大于所有分类列表容器的高度
                        if (i - s > 0) {											//则 继续判断当前滑过容器的位置 是否有一半超出窗口一半在窗口内显示的Bug,
                            $(this).children('.item-list').css('top', (s - h) + 2);
                        } else {
                            $(this).children('.item-list').css('top', (s - h) - (-(i - s)) + 2);
                        }
                    } else {
                        $(this).children('.item-list').css('top', 3);
                    }
                }

                $(this).addClass('hover');
                $(this).children('.item-list').css('display', 'block');
            }, function () {
                $(this).removeClass('hover');
                $(this).children('.item-list').css('display', 'none');
            });

            $('.item > .item-list > .close').click(function () {
                $(this).parent().parent().removeClass('hover');
                $(this).parent().hide();
            });

            ////内页菜单收起
            $(".all-sort a").bind("click", function () {
                $(".all-sort-list").slideToggle(300);
            });

            //搜索
            $("#btn-search").bind("click", function () {
                var key = $("#txt-search").val();
                if (key.length > 0) {
                    location.href = "/Search/Index/" + key;
                }
                else {
                    alert("请输入关键词!");
                    $("#txt-search").focus();
                }
            });
        })
    </script>
