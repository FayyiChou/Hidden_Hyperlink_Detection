                float: left;
                position: relative;
                border-bottom: 1px solid #e12428;
            }

                .side ul li .sidebox {
                    position: absolute;
                    width: 54px;
                    height: 54px;
                    top: 0;
                    right: 0;
                    transition: all 0.3s;
                    background: #930000;
                    opacity: 0.8;
                    filter: Alpha(opacity=80);
                    color: #fff;
                    font: 14px/54px "微软雅黑";
                    overflow: hidden;
                }

                .side ul li .sidetop {
                    width: 54px;
                    height: 54px;
                    line-height: 54px;
                    display: inline-block;
                    background: #930000;
                    opacity: 0.8;
                    filter: Alpha(opacity=80);
                    transition: all 0.3s;
                }

                    .side ul li .sidetop:hover {
                        background: #ae1c1c;
                        opacity: 1;
                        filter: Alpha(opacity=100);
                    }

                .side ul li img {
                    float: left;
                }
