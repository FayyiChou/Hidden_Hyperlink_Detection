    <div class="side">
        <ul>
            <li><a href="http://wpa.qq.com/msgrd?V=3&amp;uin=1027709502&amp;Site=vote&amp;Menu=yes"><div class="sidebox"><img src="/Content/images/side_icon04.png">QQ客服</div></a></li>
            <li style="border:none;"><a href="javascript:goTop();" class="sidetop"><img src="/Content/images/side_icon05.png"></a></li>
        </ul>
    </div>
    <script>
        $(function () {
            $(".side ul li").hover(function () {
                $(this).find(".sidebox").stop().animate({ "width": "124px" }, 200).css({ "opacity": "1", "filter": "Alpha(opacity=100)", "background": "#ae1c1c" })
            }, function () {
                $(this).find(".sidebox").stop().animate({ "width": "54px" }, 200).css({ "opacity": "0.8", "filter": "Alpha(opacity=80)", "background": "#930000" })
            });
        });
        //回到顶部函数
        function goTop() {
            $('html,body').animate({ 'scrollTop': 0 }, 300);
        }
    </script>
