<!-- 播放按钮切换 start -->
<script type="text/javascript">
playPartChange();
// 播放按钮根据版本切换显示文案
function playPartChange()
{
	if(browser.version.iPhone)
	{
		// 默认 android ios 时 切换ios的访问		
		            $("#detail_play_pic").attr('data-appup', '1');
			$('#detail_play_button').parent('span').show();
            $('#detail_play_button').attr('data-appup', '1');
						
					var downloadWord = '下载观看';
            $('#playDownLoadBtn').parent('span').show();
			$('#playDownLoadBtn').text(downloadWord);
							$('#playDownLoadBtn').parent('span').removeClass('all');
			                
        var appWord = '';
        var appUrl = 'javascript:void(0);';
                var showUrl = "http://ys.km.com/play/href=http%253A%252F%252Fwww.fun.tv%252Fvplay%252Fg-108902.v-330411%253Falliance%253D2232&title=%BF%D6%C1%FA%CA%C0%BD%E7";
        $('#detail_play_button').attr('data-href', showUrl).removeClass('show_popWin').removeAttr("popwinname");
        $('#detail_play_pic').attr('data-href', showUrl).removeClass('show_popWin').removeAttr("popwinname");
        $('#popWord').html(appWord);
        $('#popHref').attr('href', 'javascript:void(0);').addClass('runApp');
        $('#downHref').attr('href', appUrl);
        	}
}
</script>


