                    <script type="text/javascript">
                        var fmobj = document.calc11;
                        var p = document.location.search.toString();

                        p = p.replace("#", "");
                        p = p.replace("?", "");
                        var dat = p.split("&");
                        var price = areas = '';
                        for (i = 0; i < dat.length; i++) {
                            if (dat[i].indexOf("unit_price=") != -1)
                                price = dat[i].replace("unit_price=", "");
                            if (dat[i].indexOf("buildingarea=") != -1)
                                areas = dat[i].replace("buildingarea=", "");
                        }
                        fmobj.price.value = price;
                        fmobj.sqm.value = areas;

                        var loc = document.location.href;
                        var paramList = loc.split("?");
                        if (paramList[1]) {
                            if (paramList[1] == 'dengebenjin') {
                                $('dengeben2').checked = true;
                                $('dengeben1').checked = false;
                                chanage_type_mm();
                            }
                        }
                    </script>
