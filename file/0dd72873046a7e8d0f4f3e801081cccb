<script type="text/javascript">
    $(document).ready(function () {
        var arr = [['sh', '上海市'],
            ['cd', '成都市'],
            ['cq', '重庆市'],
            ['tj', '天津市'],
            ['gz', '广州市'],
            ['sz', '深圳市'],
            ['wuhan', '武汉市'],
            ['nanjing', '南京市'],
            ['hz', '杭州市'],
            ['suzhou', '苏州市'],
            //['dl', '大连市'],
            //['dg', '东莞市'],
            ['sjz', '石家庄市'],
            ['zz', '郑州市'],
            ['qd', '青岛市'],
            ['jn', '济南市'],
            ['xian', '西安市'],
            //['km', '昆明市'],
            ['wuxi', '无锡市'],
            ['nc', '南昌市'],
            ['fz', '福州市'],
            //['zh', '珠海市'],
            ['cs', '长沙市'],
            ['changchun', '长春市'],
            //['nn', '南宁市'],
            ['sy', '沈阳市']
        ];

        $.each(arr, function (key, val) {
            if (window.location.href.indexOf("8." + val[0] + ".fang.com") >= 0) {
                re = new RegExp("8.fang.com", "g");
                str = $(".tcity").html().replace(re, "8." + val[0] + ".fang.com");
                $(".tcity").html(str);
                $("#cityName").val(val[1]);
                //$(".newnav a").attr("href", "www.baidu.com");
                if ($(this).attr("href").indexOf("home") > 0) {
                    $(this).attr("href", $(this).attr("href").replace("{0}.", "").replace("{0}", ""));
                    $(this).attr("href", $(this).attr("href").replace("%7b0%7d.", "").replace("%7b0%7d", ""));
                    $(this).attr("href", $(this).attr("href").replace("%7B0%7D.", "").replace("%7B0%7D", ""));
                }
                $(this).attr("href", $(this).attr("href").replace("{0}.", val[0]).replace("{0}", ""));
                $(this).attr("href", $(this).attr("href").replace("%7b0%7d.", val[0]).replace("%7b0%7d", ""));
                $(this).attr("href", $(this).attr("href").replace("%7B0%7D.", val[0]).replace("%7B0%7D", ""));
            }
        });
        //北京市处理
        //$(".newnav").find("a").each(function () {
        $("a").each(function () {
            if ($(this).attr("href").indexOf("home") > 0) {
                $(this).attr("href", $(this).attr("href").replace("{0}.", "").replace("{0}", ""));
                $(this).attr("href", $(this).attr("href").replace("%7b0%7d.", "").replace("%7b0%7d", ""));
                $(this).attr("href", $(this).attr("href").replace("%7B0%7D.", "").replace("%7B0%7D", ""));
            }
            if ($(this).attr("href").indexOf("%7B0%7D") >= 0
                || $(this).attr("href").indexOf("%7b0%7d") >= 0
                || $(this).attr("href").indexOf("{0}") >= 0) {
                $(this).attr("href", $(this).attr("href").replace("{0}.", "").replace("{0}", ""));
                $(this).attr("href", $(this).attr("href").replace("%7b0%7d.", "").replace("%7b0%7d", ""));
                $(this).attr("href", $(this).attr("href").replace("%7B0%7D.", "").replace("%7B0%7D", ""));
            }
        });
        //js判断cookie
        $.getJSON("http://8.fang.com/ajax/GetCookie.ashx?type=2&jsoncallback=?", function (data) {
            if (data.username != "") {
                $("#username").text(data.username);
                $("#login").show();
                $("#login1").show();
                $("#login2").show();
                $("#nologin").hide();
                $("#nologin").hide();
                $("#nologin1").hide();
                $("#nologin2").hide();
            } else {
                $("#login").hide();
                $("#login1").hide();
                $("#login2").hide();
                $("#nologin").show();
                $("#nologin1").show();
                $("#nologin2").show();
            }
        });
    });

    //退出
    $("#logout").click(function () {
        $.post("http://8.fang.com/ajax/LoginOut.ashx", function (data) {
            if (data == "ok") {
                //window.location.reload();
            }
        });
        window.location.reload();
    });
</script>

<script type="text/javascript">
    var code;
    var timer;
    function V(id) {
        return document.getElementById(id);
    }
    //var code;
    $(document).ready(function () {

        wydk(); //我要贷款
        $('#wydk_money').focus(function () {
            $('#wydk_money').val('');
        });
        $('#wydk_month').focus(function () {
            $('#wydk_month').val('');
        });
        $('#wydk_money_apply').focus(function () {
            $('#wydk_money_apply').val('');
        });
        //热门产品鼠标经过样式变化
        /*$(".unsecured").hover(function () {
            $(this).toggleClass("tbwhite");
        });*/
        $(".notes01").hide();
        $(".notes02").hide();
    });

    //去往详情页--消费/购房
    function gotodetail(id) {
        window.open("/xiaofei/" + id + "_5_12.htm");
    }
    function gotodetailGF(id) {
        window.open("/goufang/" + id + "_100_240.htm");
    }

    //贷款种类--隐藏/显示
    function DivTypeHide() {
        $("#wydk_type_ul").attr("style", "display:none;left:3px;top:48px;");
    }
    function DivTypeShow() {
        $("#wydk_type_ul").attr("style", "display:block;left:3px;top:48px;");
    }
    //贷款金额--隐藏/显示
    function DivMoneyHide() {
        $("#wydk_money_ul").attr("style", "display:none;left:147px;top:48px;");
    }
    function DivMoneyShow() {
        $("#wydk_money_ul").attr("style", "display:block;left:147px;top:48px;");
    }
    //快速申请贷款金额--隐藏/显示
    function DivApplyMoneyShow() {
        $("#wydk_money_ul_house_apply").attr("style", "display:block;");
    }
    function DivApplyMoneyHide() {
        $("#wydk_money_ul_house_apply").attr("style", "display:none;");
    }
    //贷款期限--隐藏/显示
    function DivMonthHide() {
        $("#wydk_month_ul").attr("style", "display:none;left:291px;top:48px;");
    }
    function DivMonthShow() {
        $("#wydk_month_ul").attr("style", "display:block;left:291px;top:48px;");
    }

    //我要贷款
    function wydk() {
        $("#ssrkbgMoney").click(function () {// 点击弹出和隐藏
            $("#wydk_money_ul").toggle();
        });
        $("#ssrkbgMonth").click(function () {// 点击弹出和隐藏
            $("#wydk_month_ul").toggle();
        });
        $("#ssrkbgType").click(function () {// 点击弹出和隐藏
            $("#wydk_type_ul").toggle();
        });
    }

    //只能输入数字和小数
    function clearNoNum(obj) {
        //obj.value = obj.value.replace(/[^\d.]{5}$/, "")
        obj.value = obj.value.replace(/[^\d.]/g, ""); //清除“数字”以外的字符
        obj.value = obj.value.replace(/^\./g, ""); //验证第一个字符是数字而不是.
        obj.value = obj.value.replace(/\.{2,}/g, "."); //只保留第一个. 清除多余的.
        obj.value = obj.value.replace(".", "$#$").replace(/\./g, "").replace("$#$", ".");
    }

    //快速申请--贷款金额--只能输入数字和小数
    //function clearNoNumApply(obj) { 
    //    obj.value = obj.value.replace(/[^\d.]/g, ""); //清除“数字”以外的字符
    //    obj.value = obj.value.replace(/^\./g, ""); //验证第一个字符是数字而不是.
    //    obj.value = obj.value.replace(/\.{2,}/g, "."); //只保留第一个. 清除多余的.
    //    obj.value = obj.value.replace(".", "$#$").replace(/\./g, "").replace("$#$", ".");
    //}

    //搜索贷款--贷款期限--只能输入数字和小数
    function clearNoNumMon(obj) {
        obj.value = obj.value.replace(/[^\d]/g, ""); //清除“数字”以外的字符
        obj.value = obj.value.replace(".", "$#$").replace(/\./g, "").replace("$#$", ".");
    }

    function MonthFoc() {
        // $('#MonthTs').text('');
        $(".notes02").hide();
    }
    function MoneyFoc() {
        $('#MoneyTs').text('不足一万可用小数表示');
        $('#MoneyTs').css('color', '');
        $(".notes01").hide();
    }

    //贷款金额--取下拉项的值赋给文本框、隐藏下拉列表
    function moneySelect(obj) {
        var reval = $(obj).attr('reval');
        $('#wydk_money').attr('reval', reval);
        $('#wydk_money').val(reval);
        $('#wydk_money_ul').attr("style", "display:none;top:150px;");
    }

    //快速申请贷款金额--取下拉项的值赋给文本框、隐藏下拉列表
    function moneyApplySelect(obj) {
        var reval = $(obj).attr('reval');
        if (reval == "其它") {
            $('#wydk_money_apply').val("");
            $("#wydk_money_apply").focus();
        } else {
            $('#wydk_money_apply').attr('reval', reval);
            $('#wydk_money_apply').val(reval);
        }
        $('#wydk_money_ul_house_apply').attr("style", "display:none;");
    }

    //贷款期限--取下拉项的值赋给文本框、隐藏下拉列表
    function monthSelect(obj) {
        var type = $('#wydk_type').attr('reval');
        var reval = $(obj).attr('reval');
        if (type == "0" || type == "1") {
            $('#wydk_month').attr('reval', reval);
            $('#wydk_month').val(reval);
            $('#wydk_month_ul').attr("style", "display:none;top:220px;");
        }
        else if (type == "2" || type == "3") {
            $('#wydk_month').attr('reval', reval * 12);
            $('#wydk_month').val(reval * 12);
            $('#wydk_month_ul').attr("style", "display:none;top:220px;");
        }
    }

    //贷款种类--对应下拉列表改变
    function typeSelect(obj) {
        var reval = $(obj).attr('reval');
        $('#wydk_type').attr('reval', reval);
        $('#wydk_type').val($(obj).text());
        $('#wydk_type_ul').attr("style", "display:none;top:76px;");
        //0--不限、1--消费贷款
        if (reval == "0" || reval == "1" || reval == "4") {
            $('#wydk_month').attr('reval', "12");
            $('#wydk_month').val("12");

            $('#wydk_money').attr('reval', "5");
            $('#wydk_money').val("5");

            var Hmoney_ul_xiaofei = $('#wydk_money_ul_xiaofei').html(); //贷款金额---隐藏列表ul
            $('#wydk_money_ul').html(Hmoney_ul_xiaofei);// 给下拉列表赋新的html 代码 

            var Hmonth_ul_xiaofei = $('#wydk_month_ul_xiaofei').html(); //贷款金额---隐藏列表ul
            $('#wydk_month_ul').html(Hmonth_ul_xiaofei);// 给下拉列表赋新的html 代码 
        }
        //购房贷款
        if (reval == "2") {
            $('#wydk_month').attr('reval', "240");
            $('#wydk_month').val("240");

            $('#wydk_money').attr('reval', "100");
            $('#wydk_money').val("100");

            var Hmoney_ul_house = $('#wydk_money_ul_house').html(); //贷款金额---隐藏列表ul
            $('#wydk_money_ul').html(Hmoney_ul_house);// 给下拉列表赋新的html 代码 

            var Hmonth_ul_house = $('#wydk_month_ul_house').html(); //贷款金额---隐藏列表ul
            $('#wydk_month_ul').html(Hmonth_ul_house);// 给下拉列表赋新的html 代码 
        }
        //购车贷款
        if (reval == "3") {
            $('#wydk_month').attr('reval', "24");
            $('#wydk_month').val("24");

            $('#wydk_money').attr('reval', "15");
            $('#wydk_money').val("15");

            var Hmoney_ul_auto = $('#wydk_money_ul_auto').html(); //贷款金额---隐藏列表ul
            $('#wydk_money_ul').html(Hmoney_ul_auto);// 给下拉列表赋新的html 代码 

            var Hmonth_ul_auto = $('#wydk_month_ul_auto').html(); //贷款金额---隐藏列表ul
            $('#wydk_month_ul').html(Hmonth_ul_auto);// 给下拉列表赋新的html 代码 
        }
    }

    //我要贷款搜索按钮
    function wydk_search() {
        MoneyBlu();
        MonthBlu();
        if (Smoney == true && Smonth == true) {
            var type = $('#wydk_type').attr('reval');
            var money = $('#wydk_money').attr('reval');
            var month = $('#wydk_month').attr('reval');
            var updateurl = "";
            if (type == 2) {
                updateurl = "/goufang/q" + money + "y" + month + ".htm";
            }
            else if (type == 3) {
                updateurl = "/gouche/q" + money + "y" + month + ".htm";
            }
            else if (type == 4) {
                updateurl = "/jingying/q" + money + "y" + month + ".htm";
            }
            else {
                updateurl = "/xiaofei/q" + money + "y" + month + ".htm";
            }
            window.location.href = updateurl;
            return true;
        }
        else {
            return false;;
        }
    }
    //贷款金额----验证
    function MoneyBlu() {
        var val = $.trim($('#wydk_money').val());
        var valtypename = $.trim($('#wydk_type').val());
        var valtype = 0;
        if (valtypename == "购房贷款") {
            valtype = 2;
        }
        else if (valtypename == "购车贷款") {
            valtype = 3;
        }
        else if (valtypename == "经营贷款") {
            valtype = 4;
        }
        else if (valtypename == "消费贷款") {
            valtype = 1;
        }

        //var ts = $('#MoneyTs');

        var ts = $('.notes01');
        if (val.length == 0) {
            //ts.text('贷款金额不能为空');
            //ts.css('color', 'yellow');
            ts.show();
            Smoney = false;
        } else {
            if (!isNaN(val)) {
                if (parseFloat(val) <= 3000 && parseFloat(val) >= 0.1) {
                    Smoney = true;
                    var money = $('#wydk_money').attr('reval', val);
                    $('#wydk_type').attr('reval', valtype);
                }
                else {
                    //ts.text('请输入0.1至6000的数字');
                    //ts.css('color', 'yellow');
                    ts.show();
                    Smoney = false;
                }
            } else {
                //ts.text('贷款金额请填纯数字');
                //ts.css('color', 'yellow');
                ts.show();
                Smoney = false;
            }
        }
    }
    //贷款期限----验证
    function MonthBlu() {
        var val = $.trim($('#wydk_month').val());
        var ts = $('.notes02');
        if (val.length == 0) {
            //ts.text('贷款期限不能为空');
            //ts.css('color', 'yellow');
            ts.show();
            Smonth = false;
        } else {
            if (!isNaN(val)) {
                if (parseInt(val) <= 360 && parseInt(val) > 0) {
                    Smonth = true;
                    var money = $('#wydk_month').attr('reval', val);
                }
                else {
                    //ts.text('请输入1至360的数字');
                    //ts.css('color', 'yellow');
                    ts.show();
                    Smonth = false;
                }
            } else {
                //ts.text('贷款期限请填纯数字');
                //ts.css('color', 'yellow');
                ts.show();
                Smonth = false;
            }
        }
    }
</script>
