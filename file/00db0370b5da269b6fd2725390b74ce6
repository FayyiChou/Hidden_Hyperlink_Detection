function PC_7_0G3UNU10SD0GHOSD6GG0000000000000_trimString(x) {
	while (x.substring(0,1) == " " )
	{	templength=x.length;
		x = x.substring(1,templength);
	}

	while (x.substring(x.length-1,x.length) == " " )
	{	x=x.substring(0,x.length-1)
	}
	return x
}

function PC_7_0G3UNU10SD0GHOSD6GG0000000000000_selectLogonMode(logonMode) {
	if (logonMode == 1) {
		document.User.__logonMode.value = '1';
		document.User.nextPage.value = 'ITHIDE_hsbc.cam40-myhsbc';
	} else {
		document.User.__logonMode.value = '0';
		document.User.nextPage.value = 'hsbc.myhsbc';
	}
}
function PC_7_0G3UNU10SD0GHOSD6GG0000000000000_chkLogonID(x) {
	if(PC_7_0G3UNU10SD0GHOSD6GG0000000000000_trimString(x).length==0) {
		PC_7_0G3UNU10SD0GHOSD6GG0000000000000_TotalErrorMessage = PC_7_0G3UNU10SD0GHOSD6GG0000000000000_Error_01 + "\n";
		PC_7_0G3UNU10SD0GHOSD6GG0000000000000_alertErrorMessage();
		document.getElementById('submittype').value = 'press';
		return false;
	} else {
		if(document.getElementById('submittype').value != "click") {
			PC_7_0G3UNU10SD0GHOSD6GG0000000000000_TotalErrorMessage = PC_7_0G3UNU10SD0GHOSD6GG0000000000000_Error_02 + '\n';
			PC_7_0G3UNU10SD0GHOSD6GG0000000000000_alertErrorMessage();
			return false;
		} else {
			return true;
		}
	}
}
function PC_7_0G3UNU10SD0GHOSD6GG0000000000000_validate() {
	document.User.userid.value = document.User.u_UserID.value;
	if (!PC_7_0G3UNU10SD0GHOSD6GG0000000000000_chkLogonID(document.User.u_UserID.value)) {
		document.User.u_UserID.focus();
		PC_7_0G3UNU10SD0GHOSD6GG0000000000000_submitForm = "yes";
		return;
	}

	if (PC_7_0G3UNU10SD0GHOSD6GG0000000000000_pressEnter == "yes") {
		PC_7_0G3UNU10SD0GHOSD6GG0000000000000_pressEnter = "no";
		return;
	}

	document.User.submit();
	//IE Macintosh
	if((navigator.appName.indexOf("Microsoft")>-1) && (navigator.appVersion.indexOf("Macintosh")>-1)) {}
	else {
		document.User.reset();
	}
	return;
}

function PC_7_0G3UNU10SD0GHOSD6GG0000000000000_isCookieSupported() {
	document.cookie = "cookie=enable";
	if (document.cookie.length > 0)
		return true;
	return false;
}

function PC_7_0G3UNU10SD0GHOSD6GG0000000000000_isBrowserSupported() {
	if((navigator.appName.indexOf("Netscape")>-1)) //Netscape must greater than version 4
	{
		browserVersion = navigator.appVersion;
		End = browserVersion.indexOf("[");
		End1 = navigator.userAgent.lastIndexOf("Netscape6");

		if(End > -1)
			VersionNo = browserVersion.substring(0,End);
		else if (End1 == -1)
			VersionNo = browserVersion.substring(0,browserVersion.indexOf("("));
		else {
			VersionNo = navigator.userAgent.substring(End1+10, navigator.userAgent.length);
		}
		if ((parseFloat(VersionNo) >= 4.07) && (parseFloat(VersionNo) < 6.0)) {
			return true;
		}
	}
	if((navigator.appName.indexOf("Microsoft")>-1)) //IE must greater than verison 5
	{
		browserName = navigator.appVersion;
		VersionPos=browserName.indexOf("MSIE ") + 5;
		VersionNo = browserName.substring(VersionPos,VersionPos+1);
		if((parseFloat(VersionNo)>=5)  || (parseFloat(VersionNo) < 2)){
			return true;
		}
	}
	return false;
}

function PC_7_0G3UNU10SD0GHOSD6GG0000000000000_alertErrorMessage() {
	if(PC_7_0G3UNU10SD0GHOSD6GG0000000000000_TotalErrorMessage.length > 0) {
		Temp = PC_7_0G3UNU10SD0GHOSD6GG0000000000000_TotalErrorMessage;
		PC_7_0G3UNU10SD0GHOSD6GG0000000000000_TotalErrorMessage = "";
		PC_7_0G3UNU10SD0GHOSD6GG0000000000000_Flag = false;
		PC_7_0G3UNU10SD0GHOSD6GG0000000000000_submitForm = "yes";
		alert(PC_7_0G3UNU10SD0GHOSD6GG0000000000000_errHead + "\n\n" + Temp + "\n" + PC_7_0G3UNU10SD0GHOSD6GG0000000000000_errFoot);
	}
}

