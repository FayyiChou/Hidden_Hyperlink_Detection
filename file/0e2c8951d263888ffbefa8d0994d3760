.script(JSF.swf).wait(function(){
	var $ = jQuery;	
	
	var numF = function(n){
		n = String(n);
		if(n.length<4) return n;
		var a = n.split('').reverse().join('');
		a = a.replace(/(\d{3})/g, '$1,');
		a = a.split('').reverse().join('');
		return a.replace(/^,/,'');
	}

	/**/
	var _userTpl = $('#videoFocus .video-user').find('div.template').html().replace('src3', 'src');
	var videoDef = function(){
		window.homeFocusReady = function(){
			window.focusSwf = swfobject.getObjectById('videoPlayer');
		};		

		var _vBox = $('#videoFocus'),
			_userBox = _vBox.find('.video-user'),
			_li = _vBox.find('li.on'),
			_user = _li.data('user').split(','),
			_flv = _user[2],
			_ruid= _user[7],
			_isPhoneLive = _user[8]>1 ? 1 : 0,
			_bg = _user[9].replace('_s', '_b');

		var _html = _userTpl.replace(/\{rid\}/g, _user[0])
				.replace(/\{link\}/g, _user[6])
				.replace(/\{alias\}/g, _user[1])
				.replace('{pic}', _user[3])
				.replace('{count}', numF(_user[5]));

		_userBox.attr('data-recid', _user[7]);
		_userBox.append(_html);

		var _url = _user[6].indexOf('6.cn')>-1 ? _user[6] : '//v.6.cn'+ _user[6];
		_url = _url + '?' + (isVerB ? 'recid=' + _user[7] : '');
		
		swfobject.embedSWF(__PLAYER, 'videoPlayer', '100%', '100%', '10.1', __NOF, {protocol:location.protocol+'//', ruid:_ruid, fileName:_flv, isPhoneLive:_isPhoneLive, bg:_bg, toRoomUrl:encodeURIComponent(_url + ((window.location.hostname == 'www.6.cn' && $.browser.msie) ? '&_tra_='+VIA_PAGEID+'-ipvafi4f' : '')), readyFun: 'homeFocusReady', chID:0}, {allowscriptaccess:'always', wmode:'opaque', bgcolor:'#333333'});
	}


	if(isVerB){
		var _listBox = $('#videoFocus .video-item'),
			_tpl = _listBox.find('ul.template').html().replace('src3', 'src');

		$.getJSON('/api/index/pc.php', {act:'top', uuid:INDEX_UUID, uid:_puser.uid||0}, function(data){
			if(data.flag=='001'){
				var _html = [];
				window.INDEX_VER = data.content.ver;
				$.each(data.content.data, function(i, user){
					var _u = user.rid+','+user.alias+','+user.flvtitle+','+user.picuser+','
							+user.starttime+','+user.count+','+user.link+','+user.recId;

					var _li = _tpl.replace('{rank}', i+1)
							.replace('{rid}', user.rid)
							.replace(/\{alias\}/g, user.alias)
							.replace('{pic}', user.pic)
							.replace('{user}', _u);

					_li = $(_li).attr('data-recid', user.recId);
					_html.push(_li.prop("outerHTML"));				
				});

				_listBox.find('ul.list').html(_html);
				_listBox.find('ul.list li').eq(4).addClass('on');

				_listBox.find('.text').css('display', 'block');
				_listBox.find('img').on('load', function(){
					$(this).css({
						opacity: 1,
						visibility: 'visible'
					});
				});			
			}else if(data.flag=='403'){
				/* */
				location.href = location.href;
			}else{
				console.log(data.content);

				/* */
				_listBox.find('.text').css('display', 'block');
				_listBox.find('img').css({
					opacity: 1,
					visibility: 'visible'
				});
			}	

			videoDef();	
		});
	}else{
		videoDef();
	}

})
