<!--标题浮动图片-->
<script type="text/javascript"> 
$(document).ready(function(){
	$('.artist div').each(function(){
		$(this).find('.cover').css('top', -$(this).height());
		$(this).hover(function(){
			$(this).find('.cover').animate({
				'top': '0'
			},300);
		},function(){
			$(this).find('.cover').animate({
				'top':$(this).height()
			},{
				duration: 300,
				complete:function(){
					$(this).css('top', -$(this).parent('div').height())
				}
			});
		});
	});
});
</script>
<script type="text/javascript">
$(document).ready(function(){
	$(".advbox").show();
	$(".advbox").animate({
		top:"50%"
	},1000);
	$(".closebtn").click(function(){
		$(".advbox").fadeOut(500);
	})
})
</script>
