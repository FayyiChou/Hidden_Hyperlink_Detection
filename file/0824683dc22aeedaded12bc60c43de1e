<script>
$("#yes").click(function () {    
    if ($('.ifyes').is(':hidden')) {
        $(".ifno").hide();
    }
    $(".ifyes").toggle();
});
$("#no").click(function () {
    if ($('.ifno').is(':hidden')) {
        $(".ifyes").hide();
    }
    $(".ifno").toggle();
});
</script>

