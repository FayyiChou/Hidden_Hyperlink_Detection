                    <script language="javascript" type="text/javascript">
                        var i_p_speed = 40;
                        var i_p_marqueebox11 = document.getElementById("i_p_marqueebox11");
                        var i_p_marqueebox12 = document.getElementById("i_p_marqueebox12");
                        var i_p_marqueebox = document.getElementById("i_p_marqueebox");
                        i_p_marqueebox12.innerHTML = i_p_marqueebox11.innerHTML;
                        function i_p_Marquee() {
                            if (i_p_marqueebox12.offsetHeight - i_p_marqueebox.scrollTop <= 0) { i_p_marqueebox.scrollTop -= i_p_marqueebox11.offsetHeight; }
                            else { i_p_marqueebox.scrollTop++; }
                        }
                        var i_p_MyMar = setInterval(i_p_Marquee, i_p_speed);
                        i_p_marqueebox.onmouseover = function() { clearInterval(i_p_MyMar); }
                        i_p_marqueebox.onmouseout = function() { i_p_MyMar = setInterval(i_p_Marquee, i_p_speed); }
						</script>

