function getNovelIdFromLink(url) {
	var start = url.indexOf("aid=");
	var end = url.indexOf("&", start + 4);
	if (end > start) {
		return parseInt(url.substring(start+4, end));
	} else {
		return 0;
	}
}
function getChapterIdFromLink(url) {
	var start = url.lastIndexOf("cid=");
	if (start > -1) {
		return parseInt(url.substring(start+4));
	} else {
		return 0;
	}
}
function add() {
	var url = document.location.href;
	var nid=getNovelIdFromLink(url);
	var cid=getChapterIdFromLink(url);
	if (cid >0) {
		pl(nid, cid);
	}
	return false;
}
</script>
