	function switchTab(ProTag, ProBox) {
		for (i = 1; i < 3; i++) {
			if ("tab" + i == ProTag) {
				document.getElementById(ProTag).getElementsByTagName("a")[0].className = "on";
			} else {
				document.getElementById("tab" + i).getElementsByTagName("a")[0].className = "";
			}
			if ("con" + i == ProBox) {
				document.getElementById(ProBox).style.display = "";
			} else {
				document.getElementById("con" + i).style.display = "none";
			}
		}
	}
  function UnDisplay(a){
    a=a.parentElement.id;
document.getElementById(a).innerHTML=("<a href=\"#"+a+"\" onclick=\"Display(this);\">观看视频</a>");
  };
  function Display(a){

    a=a.parentElement.id;
document.getElementById(a).innerHTML=("<a href=\"#\" onclick=\"UnDisplay(this);return false;\">隐藏视频<center  style=\"display:;\"><object type=\"application/x-shockwave-flash\" data=\"http://rccchina.com/video/vcastr3.swf\"width=\"700\" height=\"428\" id=\"vcastr3\">        <param name=\"movie\" value=\"http://rccchina.com/video/vcastr3.swf\"/>         <param name=\"allowFullScreen\" value=\"true\" />        <param name=\"FlashVars\" value=\"xml=        <vcastr>          <channel>            <item>              <source>http://files.rccchina.com/files/it/tools/www/flv/"+a+".flv</source>              <duration></duration>              <title></title>            </item>          </channel>          <config>          <controlpanelbgcolor>52479</controlpanelbgcolor>          <isAutoPlay>false</isAutoPlay>          </config>        </vcastr>\"/></center></a>");
  };

