                $('.list_left_fl_ad').css({
                    "top": ($(window).height() - 200)/2 + $(window).scrollTop()
                }).show();
            }
        }else{
            $('.list_left_fl_ad').css({
                "position": "fixed",
                "top": ($(window).height() - 200)/2
            }).show();
        }
    }
});
</script>
