    GJ.use('jquery', function(){
        function procMask(){
            $(".yxgz-guide").hide();
            $("#float_nav_info").css({ "position": "", "z-index": "","background-color": "" })
            $("#seltion").css("position","");
            $("#list_seletion").css("position","");
