<script type="text/javascript">
    Do(function() {


        Do.add('m-slider', {
            path: basedir+'js/slider/jquery.yx_rotaion.js',
            type: 'js'
        });
        Do.add('tinycarousel', {
            path: basedir+'js/tinycarousel/jquery.tinycarousel.js',
            type: 'js'
        });
        Do('m-slider', function() {
            $("#slider").yx_rotaion({
                auto:true,
                btn: true,
                during:5000
            });
        });
        Do('tinycarousel', function() {
            $("#carousel").tinycarousel({
                animation : true,interval: true,intervalTime:3000,animationTime:600
            });
        });
        $('.tab li').hover(function () {
            $(this).addClass('on').siblings().removeClass('on');
            $('.form-box').hide();
            $('.form-box').eq($(this).attr('rel') - 1).show();
        });
        setInterval(function(){
            $(".interval-toggle").find(".item").each(function(){
                if($(this).hasClass("dn")){
                    $(this).removeClass("dn");
                } else {
                    $(this).addClass("dn");
                }
            });
        }, 3000);
        $.ajax({
            type: 'GET',
            url: '/api/ajax/getCsrf',
            success: function(data){
                $('input[name="YII_CSRF_TOKEN"]').val(data);
            }
        });

    });
</script>

