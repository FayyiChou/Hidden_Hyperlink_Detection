			$(document).ready(function() {
				$(".xlist li").hover(function() {
					$('#dl1').children('a').removeClass('xc');
					$('.dl1').removeClass('xc');
					$(this).children('a').addClass('xc');
					$(".dlist").children('.'+this.id).addClass('xc');
					
				}, function() {
					$('#dl1').children('a').addClass('xc');
					$('.dl1').addClass('xc');
					$(this).children('a').removeClass('xc');
					$(".dlist").children('.'+this.id).removeClass('xc');
					
				});
			});
