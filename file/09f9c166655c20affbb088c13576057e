            R: { //Recom
                //BookFormat
                BF: ['<div class="pic"><a href="http://www.qidian.com/Book/{0}.aspx" target="_blank" title="{1}"><img src="http://image.cmfu.com/books/{0}/{0}.jpg" onerror="javascript:EP.R.DC(this);" alt="{1}" width="90" height="112" border="0" /></a></div>',
                           '<div class="txt"><a class="name" href="http://www.qidian.com/Book/{0}.aspx" target="_blank" title="{1}">《{1}》</a><br />',
                                '类型：<a href="{4}" target="_blank" title="{5}">{5}</a><br />作者：<a href="http://me.qidian.com/w/{2}.aspx" target="_blank" title="{3}">{3}</a><br />',
                                '<a class="red" href="http://www.qidian.com/BookReader/{0}.aspx" target="_blank" title="阅读《{1}》">[立刻阅读]</a>',
                            '</div>'].join(''),
                //BookItemFormat
                BIF: '<li onmouseover="this.className=\'active\';EP.R.MO({6});" onmouseout="this.className=\'\'">[<a href="{4}" class="gray" target="_blank" title="{5}">{5}</a>] <a class="name" href="http://www.qidian.com/Book/{0}.aspx" target="_blank" title="{1}">{1}</a></li>',
                B: function() { //Builder
                    if (!StrongRecom) return;
                    var _er = EP.R;
                    var _bif = _er.BF;
                    var _blf = _er.BIF;
                    var sbObj = $('showbook');
                    var lbObj = $('listbook');
                    if (!sbObj || !lbObj) return;

                    var lbHtml = ['<ul>'];
                    for (var i = 0; i < StrongRecom.length && i < 10; i++) {
                        var _b = StrongRecom[i];
                        if (i == 0) {
                            sbObj.innerHTML = _bif.Format(_b.BookId, _b.BookName,
                                                        _b.AuthorId, _b.AuthorName,
                                                        CU(_b.CategoryId), _b.CategoryName);
                            sbObj._i = i;
                        };
                        lbHtml.push(_blf.Format(_b.BookId, _b.BookName,
                                                        0, 0,
                                                        CU(_b.CategoryId), _b.CategoryName, i));
                    };
                    lbHtml.push('</ul>');
                    lbObj.innerHTML = lbHtml.join('');
                    
                    try {
                        $('k').focus();
                    } catch (e) { ; }
                },
                MO: function(i) { //MouseOver
                    if (!StrongRecom || i < 0) return;
                    var sbObj = $('showbook');
                    if (!sbObj || sbObj._i == i) return;
                    var _er = EP.R;
                    var _b = StrongRecom[i];
                    sbObj.innerHTML = _er.BF.Format(_b.BookId, _b.BookName,
                                                _b.AuthorId, _b.AuthorName,
                                                CU(_b.CategoryId), _b.CategoryName);
                    sbObj._i = i;
                },
                DC: function(o) {//DefaultCover
                    if (!o) return;
                    o.src = 'http://image.cmfu.com/books/1.jpg';
                }
            }
        };
        eventBind(window, 'load', EP.R.B);
    </script>
    <script type="text/javascript" src="http://script.cmfu.com/Script/NewDefaultFirst.js"></script>
	


</body></html>