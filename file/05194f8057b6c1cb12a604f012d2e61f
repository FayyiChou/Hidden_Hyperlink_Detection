<script language="javascript">
function check_form()
{
	var obj1 = document.form_denglu.userName;
	var obj2 = document.form_denglu.userPwd;
	if(obj1.value=='输入个人用户名' || obj1.value=='')
	{
		alert('请填写用户名！');
		obj1.focus();
		return false;
	}
	if(obj2.value=='')
	{
		alert('请填写密码！');
		obj2.focus();
		return false;
	}
	document.form_denglu.submit();
	//return true;
}

function search_check()
{
	if(document.form_nav_search.keyWords.value=='输入公司或职位名称')
		document.form_nav_search.keyWords.value='';
	/*if(document.form_nav_search.keyWords.value=='')
	{
		alert('请输入关键词！');
		document.form_nav_search.keyWords.focus();
		return false;
	}*/
	document.form_nav_search.submit();
}

function showLoginHelp(show)
{
	if(show == 1)
	{
		$("#divLoginHelp").show();
	}
	else
	{
		$("#divLoginHelp").hide();
	}
}

function ShowFootLink(id)
{
	for(i=1; i<=3; i++)
	{
		document.getElementById('FootLinkSpan'+i).className='';
		document.getElementById('FootLinkID'+i).style.display='none';
	}
	document.getElementById('FootLinkSpan'+id).className='current';
	document.getElementById('FootLinkID'+id).style.display='block';
}
