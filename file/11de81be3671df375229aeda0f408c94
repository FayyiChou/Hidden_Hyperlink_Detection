		function showHidew($this,status){
			if(status=='on'){
				$this.find("a").css("display","block");
				$this.addClass("i_helpdivon");
			}else{
				$this.find("a:not(:first)").hide();
				$this.removeClass("i_helpdivon");
			}

		}

		$.ajax({
			url: 'http://www.fandongxi.com/index.php?mod=ajax&act=userinfo',
			dataType:'jsonp',
			jsonp:"callback",
			success: function(data){
				if(data.s==1){
					$("#topleft").addClass("col-lg-6");
					topHtml=getTplObj(topHtml,data.user);
					//$('#sign').attr('href','javascript:;'); 
					$('#topleft').html(topHtml).show();
				}
				else{
					$('#topleft').show();
					$('#sign').attr('href','javascript:;');
					//$('#sign').attr('href','/index.php?mod=user&act=login');
				}
			}
		});
		
</script>
