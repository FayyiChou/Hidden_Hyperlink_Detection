$searchInput=$("#s-txt");

$(".have_child").hover(function() {
	thisId=$(this).attr('id');
	$(this).attr('id','navc');
    $(this).find("a").eq(0).addClass("sub_on").removeClass("sub");
    $(this).find("ul").show();
},function() {
	if(typeof(thisId) == "undefined"){
		thisId='';	
	}
	$(this).attr('id',thisId);
    $(this).find("a").eq(0).addClass("sub").removeClass("sub_on");
    $(this).find("ul").hide()
});
$searchInput.focusClear();
$(function(){
	$('#sign').click(function(){
		if(typeof post=='undefined'){
			post=1;
			$.ajax({
		    	url:'http://www.fandongxi.com/index.php?mod=ajax&act=sign&time=1487372194',
				dataType:'jsonp',
				jsonp:"callback",
				success: function(data){
			    	if(data.s==0){
			    		$("#qiandao").show();
			    		alert("请登录后，再进行签到领取集分宝");
			    		if(data.r=='会员未登陆'){
			    			$('#sign').attr('href','/index.php?mod=user&act=login');
			    		}
			    		location.replace(location.href='/index.php?mod=user&act=login');
				    	//alert(errorArr[data.id]);
					}
					else if(data.s==1){
						alert('签到完成，获得1集分宝1积分 ');
				    	location.replace(location.href);
					}
		    	}
			});
		}
	});
});
