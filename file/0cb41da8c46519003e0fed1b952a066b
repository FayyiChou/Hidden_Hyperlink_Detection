          $(document).ready(function(){
             var tnum=$(".news a").length-1;
             var gdnum=1;
             $(".jdnews").jCarouselLite({
                start: 0,
                visible: 1,
                vertical:true,
                speed: 1000,
                auto:3000,
                beforeStart:function(){
                       $(".news a").removeClass("fo");
                       $(".news a").eq(gdnum).addClass("fo");
                },
                afterEnd:function(e){
                    gdnum==tnum?gdnum=0:gdnum++;
                }
             }); 
           });
      </script>
