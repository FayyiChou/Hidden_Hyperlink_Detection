 			document.getElementById('li_id'+id2).style.backgroundImage="url(images/top_addban2_07.gif)";
			AdFeedBack.add(imgno[id2]);
 			index2=id2;
			index=(id2+1)%indexMax;
		}			
 		function cc() {
 			//changeImg(imgsrc[index]);
 			document.getElementById('imageChange').src = 'hotelnews/ad/'+imgsrc[index];
 			document.getElementById('li_id'+index2).style.backgroundImage="url(images/top_addban_07.gif)";
