		function changeImg(id, id2){
			document.getElementById('imageChange').src = 'hotelnews/ad/'+id;
			document.getElementById('_bigBanner').href=linkarray[id2];	
			clearInterval(t);
			document.getElementById('li_id0').style.backgroundImage="url(images/top_addban_07.gif)";
			document.getElementById('li_id1').style.backgroundImage="url(images/top_addban_07.gif)";
