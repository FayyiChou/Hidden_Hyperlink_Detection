<script>
var w_iconurl="https://www.ezhotel.com.tw/hotel/icon/weather/";
var w_date='2017-02-17';
var w_loc='34';
window.onload=function(){
	ajax.request('GET','/weather_data.php?loc='+w_loc+'&date='+w_date,showWeatherInfo,true,false);
}
document.onclick=function(evt){
	var parent=evt ? evt.target : event.srcElement;
	while(typeof(parent)!='undefined' && parent!=null){
		if(parent.className=='w_menu'){
			return false;
		}else{
			parent=parent.parentNode;
		}
	}
	document.getElementById('locMenu').style.display='none';
	document.getElementById('dateMenu').style.display='none';
}
function weatherLocChange(loc){
	ajax.request('GET','/weather_data.php?loc='+loc+'&date='+w_date,showWeatherInfo,true,false);
	w_loc=loc;
	document.getElementById('locMenu').style.display='none';
}
function weatherDateChange(date){
	ajax.request('GET','/weather_data.php?loc='+w_loc+'&date='+date,showWeatherInfo,true,false);
	w_date=date;
	document.getElementById('dateMenu').style.display='none';
}
function showWeatherInfo(str){
	var weatherInfo=JSON.parse(str);
	document.getElementById('w_locname').innerHTML=weatherInfo.LocName;
	document.getElementById('w_date').innerHTML=weatherInfo.HotelDate;
	document.getElementById('w_ltp').innerHTML=weatherInfo.ltemp;
	document.getElementById('w_htp').innerHTML=weatherInfo.htemp;
	document.getElementById('w_desc').innerHTML=weatherInfo.content;
	document.getElementById('w_icon').src=w_iconurl+weatherInfo.d_icon+'.gif';
	document.getElementById('w_icon').title=weatherInfo.content;
}
function weatherOpenMenu(which){
	if(which==1){
		document.getElementById('locMenu').style.display="block";
	}else{
		document.getElementById('dateMenu').style.display="block";
	}
}
</script>
