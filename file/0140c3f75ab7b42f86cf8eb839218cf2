        var playLoggerOptions = {
            apiDomain : 'http://logapi.tvcast.naver.com',
            clipNo : '1426191',
            playlistNo : '',
            beforeClipNo : jindo.$Cookie().get("NTC_BEFORE_CLIP"),
            device : 'PC'
        };

        var oPlaybackOrderController = {};

        window.tvcast = (typeof window.tvcast !== "undefined") ? window.tvcast : {};
        tvcast.PlayLogger = new PlayLogger(playLoggerOptions);

        tvcast.pc.PlayTimeLogger.logPlayTime({
            apiDomain: 'http://logapi.tvcast.naver.com',
            rmcObjId: rmcPlayer.properties.objId,
            clipNo : '1426191',
            playlistNo: ''
        });     
        jindo.$Cookie().remove("NTC_BEFORE_CLIP");

        function initCallbackHandler(sCallbackType, pid, id, data) {
            var playTypeCookie = "",
                    autoPlayFlag = false;

            try {
                oPlaybackOrderController = tvcast.pc.ClipEnd.getClipEndPlaylist().getPlaybackOrderController() || null;
                // 후광고 완료 시
                /**
                 *  arguments
                 *      0 : [String] callbackType
                 *      1 : [String] flashID
                 *      2 : [Boolean] isFullscreen
                 */
                if (sCallbackType === 'ad_complete') {
                    playTypeCookie = oPlaybackOrderController.getCurrentType().sCookie;
                    
                    if(oPlaybackOrderController.isRepeat()){
                 	   jindo.$Cookie().set("NTC_BEFORE_CLIP", ghtEnv.nClipNo, 0);
                	}
                    
                    autoPlayFlag = arguments && arguments[2];

                    if ( autoPlayFlag && playTypeCookie == oPlaybackOrderController.TYPE.REPEAT_PLAYLIST.sCookie ) {
                        getNextClip();
                    }
                    else if ( autoPlayFlag && playTypeCookie == oPlaybackOrderController.TYPE.REPEAT_CLIP.sCookie) {
                        playOtherClip('1426191');
                    }
                    else {
					    oPlaybackOrderController.movePage();				   
                    }
                }

                if(sCallbackType == "beginfullscreen"){
                    if(window.ghtEnv){
                        window.ghtEnv.beginFullscreen = true;
                    }
                }
                // 다시 보기 선택 시
                if (sCallbackType === 'replay') {
                    oPlaybackOrderController.increasePlayCountCookie();
                    oPlaybackOrderController.moveCurrentPage();
                }


            } catch(e) { }

            if(sCallbackType == "nclick"){
                data && clickcr && clickcr("", data, "", "", "");
            }

            // 전체화면 종료
            /**
             *  arguments
             *      0 : [String] callbackType
             *      1 : [String] flashID
             *      2 : [Boolean] continuedInfo     페이지 갱신 후 플래시쪽에 전달해야할 값.
             */
            if (sCallbackType === 'endfullscreen'){
                if( arguments && arguments[2] ){

                    try {
                        var timeMilli = parseInt((new Date().getTime() - nGapTime) / 1000, 10);
                        window.localStorage.setItem("contInfo", arguments[2]);
                        window.localStorage.setItem("searchTime", timeMilli);
                    }catch(e){}
                    
                    if( ghtEnv.nClipNo != nowPlayClipNo){
                        location.href= "/v/"+nowPlayClipNo+"/list/"+ghtEnv.nPlaylistNo;
                    }

                    if(window.ghtEnv) {
                        window.ghtEnv.beginFullscreen = false;
                    }
                }
            }

            // 재생 완료
            if (sCallbackType === 'complete') {
                tvcast.PlayLogger.logCompletePlay();
            }

            // 재생 시작
            if (sCallbackType === 'play') {
                tvcast.PlayLogger.logStartPlay();
                MyPage.addMyPlaylist('http://tv.naver.com', 'recent', '1426191');
            }
        }

        function getNextClip(){
            continuousPlayList.playIndex++;
            if(continuousPlayList.list.length <= continuousPlayList.playIndex){
                continuousPlayList.playIndex = 0;
            }

            ghtEnv.nNextClipNo = continuousPlayList.list[continuousPlayList.playIndex].clipNo;
            // ghtEnv.nClipNo=ghtEnv.nNextClipNo;

            if(continuousPlayList.list[continuousPlayList.playIndex].isAdult == "Y" && ghtEnv.isPlayableAuth){
                rmcPlayer.getElement().closeVideo();
                location.href= "/v/"+ ghtEnv.nNextClipNo +"/list/"+ghtEnv.nPlaylistNo;
                return;
            }

            playOtherClip();
        }
        function playOtherClip(clipNo){
            var clipNo = clipNo || (ghtEnv && ghtEnv.nNextClipNo),
                    playList = ghtEnv && ghtEnv.nNextPlaylistNo;

            if(clipNo){
                new jindo.$Ajax("/api/clipPlayInfo/"+ clipNo +"/list/" + playList, {
                    type : "xhr",
                    method : "get",
                    timeout : 2,
                    onload : function(e){
                        var retData = e.json();
                        nowPlayClipNo = clipNo;
                        
                        tvcast.PlayLogger.reset(nowPlayClipNo, playList, jindo.$Cookie().get("NTC_BEFORE_CLIP"));
                        jindo.$Cookie().remove("NTC_BEFORE_CLIP");

                        if (!!retData.smrTrackingData) {
                            rmcPlayer.properties.externalTrackingData = jindo.$Json(retData.smrTrackingData).toObject();
                        } else {
                            rmcPlayer.properties.externalTrackingData = null;
                        }

                        rmcPlayer.updatePlayInfo(retData.videoId, retData.inKey, {
                            "advertiseInfo" : retData.advertiseInfo,
                            "advertiseUrl": retData.advertiseUrl
                        });
                        rmcPlayer.getElement().playNewVideo({
                            "playapi" : rmcPlayer.getPlayAPIUrl(),
                            "coverImageURL": retData.coverImageUrl
//                            "advertiseInfo": retData.advertiseInfo,
//                            "advertiseUrl": retData.advertiseUrl
                        });
                    },
                    onerror : function(){
                        oPlaybackOrderController && oPlaybackOrderController.movePage();
                    }
                }).request();

            }else{
                oPlaybackOrderController && oPlaybackOrderController.movePage();
            }
        }

        
        function expandPlayer(sIsExpand) {
            var bIsExpand = isGlobalExpand = sIsExpand === 'true';
            var ClipEnd = tvcast.pc.ClipEnd;
            var ClipEndExpand = ClipEnd.getClipEndExpand();

            ClipEndExpand.setExpand(bIsExpand);
        }

        /**
         *  미니플레이어 처리.
         */
        var videoArea = jindo.$Element(jindo.$$.getSingle(".video_area")),
            pipCloseBtn = "",
            hideInterval = 0,
            clipInfo = jindo.$Element("clipInfoArea"),
            clipEndDa = jindo.$Element(jindo.$$.getSingle("#banner ._banner")),
            playerArea = jindo.$Element("playerArea"),
            clipInfoTop = 0,
            isCloseByBtn = false,
