        rmcPlayer.properties.autoPlay = "true";
        
        


        rmcPlayer.properties.skinName = "tvcast_white";
        rmcPlayer.properties.coverImageURL = 'http://tvcast2.phinf.naver.net/20170206_44/jVC6q_1486351275161YWO96_JPEG/b042qOapHQ9DsBqAVST2rrK58Q_rmcvideo_144P_256_100_32_logo.jpg?type=mfullfill_880_495';
        rmcPlayer.properties.isResizableCoverImage = true;
        rmcPlayer.properties.hasRelativeMovie = true;
        rmcPlayer.properties.isP2P = "false";
        
        rmcPlayer.properties.callbackHandler = "initCallbackHandler";
        rmcPlayer.properties.cassiodServiceID = "2010";
