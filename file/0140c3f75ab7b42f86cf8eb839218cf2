        rmcPlayer.properties.protocol="rtmpe";
        rmcPlayer.properties.controls = ghtRmcPlayerControlOption;
        rmcPlayer.properties.advertiseInfo='[{"adSystem":"AMS"}]';
        rmcPlayer.properties.advertiseUrl="http://ams.rmcnmv.naver.com/item/create/2010/NAVER?ams_ctgr=DRAMA&ams_chnl=hrsong&ams_videoId=96C403152D9239C5132892B3E8799DCE9E7A&ams_cp=1245&navervodtype=RMC&unit=1103A&cp=1245&st=1388&chl=hrsong&cat=DRAMA&cl=1426191&svc=tvcast&cc=N&ctry=KR&v_theme=NA&ams_rule=%5B%7B%22type%22%3A%22pre%22%2C%22exposure%22%3Atrue%2C%22replayExposure%22%3Atrue%7D%2C%7B%22type%22%3A%22mid%22%2C%22exposure%22%3Atrue%2C%22replayExposure%22%3Atrue%7D%2C%7B%22type%22%3A%22post%22%2C%22exposure%22%3Afalse%2C%22replayExposure%22%3Afalse%7D%2C%7B%22type%22%3A%22overlay%22%2C%22exposure%22%3Afalse%7D%5D";
        rmcPlayer.properties.vendor = 'NAVER';
        rmcPlayer.properties.countryCode = 'CN';
        rmcPlayer.properties.interfaceLang = 'en_US';

        var milliTime = parseInt((new Date().getTime() - nGapTime)/1000, 10);
        for ( var i = milliTime + 2, nLen = milliTime - 3 ; i > nLen ; i-- ){
            try{
                if( window.localStorage.getItem("searchTime") == i ){
                    rmcPlayer.properties.continuedInfo = window.localStorage.getItem("contInfo");
                    window.localStorage.removeItem("contInfo");
                    window.localStorage.removeItem("searchTime");
                    break;
                }
            }catch (e) {}
        }
        


        
