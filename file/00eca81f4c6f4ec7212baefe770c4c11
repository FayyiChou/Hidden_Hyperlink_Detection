<script type="text/javascript">
	$(document).ready(function(){
		$('#loginbox').click(function(){
			$(this).unbind('click');
			$(this).xLoginBox({show:true,step:2});
		
		});
	});


	function getScript(src,callback){
		var head = document.getElementsByTagName('head')[0];
		var js = document.createElement('script');
		js.setAttribute('src',src);
		js.onload = js.onreadystatechange = function (){
			if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete"){
				head.removeChild(js);
				if (callback) callback();
			}
		}
		head.appendChild(js);
	}

	(function($){
		$(function(){
			if(document.getElementById('listmenu_menu'))document.getElementById('listmenu_menu').innerHTML = '<iframe id="daohang" name="daohang" style="width:822px;" border="0" frameborder="0" marginwidth="0" marginheight="0" scrolling="no"  allowtransparency="true" target="_blank"></iframe>';
			/* 顶部登录经过效果 */
			$(".menubox").hover(function(){
						if(!$(this).data("w"))$(this).data("w",$(this).width());
						$(this).css("width",$(this).data("w"));
						$(this).addClass("hover");
					},
					function(){
						$(this).removeClass("hover");
					});
			$(".mess").hover(function(){
				if(timer){
					clearTimeout(timer)
					if($('#count_msg').html() != '&nbsp;'){
						$('#header_msg').attr('class','menutb')
					}
				}
				if($('#count_msg').html() != '&nbsp;'){
					$(this).css("width",$(this).width());
					$(this).addClass("active");
				}
			},function(){
				if($('#count_msg').html() != '&nbsp;'){
					$(this).removeClass("active");
					if(timer)clearTimeout(timer)
					timer = setTimeout("msgFlicker()",500)
				}
			});
			$(".mbbs").toggle(
					function () {
						if(!$('#daohang').attr('src')){
							var _myDate = new Date();
							var _version = '';
							_version += _myDate.getFullYear();
							_version += _myDate.getMonth()+1;
							_version += _myDate.getDate();
							_version += _myDate.getHours();
							_version += _myDate.getMinutes();
							$('#daohang').attr('src','http://www.xcar.com.cn/bbs/header/bbsnav.htm?action=bbsnav&domain='+document.domain+'&v='+_version);
						}
						$(this).css("width",$(this).width());
						$(this).addClass("hover");
						clickP('131','1','');
					},
					function () {
						$(this).removeClass("hover");
					});
		});
		$(".m_page_select .option li").live('mouseover',function(){
			$(this).addClass("active").siblings().removeClass("active");
		});
	})(jQuery)
</script>
