<script type="text/javascript">
	$(".landed_ul li").click(function(){
		var lis = $(".landed_ul li").index(this);
		$(this).addClass("current").siblings().removeClass("current");
		//console.log(lis)
		$(".landed_con").eq(lis).show().siblings().hide();
	});

	$(document).ready(function(){
		$('#moblielogin').click(function(){
			$('#logintype').val('1');
		});
		$('#namelogin').click(function(){
			$('#logintype').val('2');
		});
	});

	$(document).ready(function(){
		$('#dologin').click(function(){
			var logintype = $('#logintype').val();
			if(logintype == 1){ //手机动态密码登录
				var mobile = $.trim($('#mobile').val());
				var myreg = /^(1+\d{10})$/; 
				if(!myreg.test(mobile)) { 
					$("#errormsg_1_text").html('请您输入正确的手机号');
					$("#errormsg_1").show();
					return false;
				}
				var checkcodenum = $.trim($('#mocode').val());
				if(checkcodenum == '' || checkcodenum == '请输入验证码'){
					$("#errormsg_1_text").html('请输入验证码');
					$("#errormsg_1").show();
					return false;
				}
				var checku = $('#unique').val();
				var mobilepwd = $.trim($('#mobilepwd').val());
				if(mobilepwd == '' || mobilepwd == '请输入6位动态密码'){
					$("#errormsg_1_text").html('请输入动态密码');
					$("#errormsg_1").show();
					return false;
				}
				$.ajax({
					type: "post",
					url: "/ajax/ajax_dologin.php",
					async: false,
					dataType: "json",
					data: 'mobile='+mobile+'&mobilepwd='+mobilepwd+'&logintype=1',
					success: function(data){
							if(data.error){
								$("#errormsg_1_text").html(data.msg);
								$("#errormsg_1").show();
								//$('#mobile').val('');
								//$('#mocode').val('');
								//$('#mobilepwd').val('');
								return false;
							}else{
								if(data.secques == 1){
									location.href = './login_secque.php';
								}else{
									//location.href = 'http://my.xcar.com.cn/index.php';
									location.href='/msg/reply.php';
								}
							}
							return;
					}
				});
			
			}else if(logintype == 2){ //用户名密码登录 
				var username = $.trim($('#username').val());
				if(username == '' || username == '请输入用户名/手机号'){
					$("#errormsg_2_text").html('用户名不能为空');
					$("#errormsg_2").show();
					return false;
				}
				username = encodeURIComponent(username);
				var userpwd = $('#userpwd').val(); //encodeURIComponent($('#userpwd').val());
				if(userpwd == '' || userpwd == '请输入密码'){
					$("#errormsg_2_text").html('请输入密码');
					$("#errormsg_2").show();
					return false;
				}
				userpwd = hex_md5(userpwd); //密码经过md5处理后，在提交
				var unique = $.trim($('#unique').val());
				var checkcode = $('#ucode').val();
								$.ajax({
					type: "post",
					url: "/ajax/ajax_dologin.php",
					async: false,
					dataType: "json",
					data: 'username='+username+'&userpwd='+userpwd+"&logintype=2"+"&checkcode="+checkcode+"&uniquekey="+unique,
					success: function(data){
							if(data.error){
								$("#errormsg_2_text").html(data.msg);
								$("#errormsg_2").show();
								//$('#username').val('');
								$('#userpwd').val('');
								$('#ucode').val('');
								if(data.error_num > 4){
									changecode(1);
									$('#show_check_code_li').show();
								}
								return false;
							}else{
								if(data.secques == 1){
									location.href = './login_secque.php';
								}else{
									//location.href = 'http://my.xcar.com.cn/index.php';
									location.href='/msg/reply.php';
								}
							}
							return;
					}
				});
			}else{
				return false;
			}
		});
	
	});

	//更换验证码
	function changecode(type){
		$.ajax({
			type: "get",
			url: "./login_check_code.php",
			async: false,
			dataType: "json",
			data: 'action=getu&t='+Date.parse(new Date()),
			success: function(u){
				$('#unique').val(u);
				var imgurl='./login_check_code.php?u='+u+'&t='+Date.parse(new Date());
				if(type == 1){
					$('#chk_img').attr('src', imgurl);
				}else{
					$('#chk_mo_img').attr('src', imgurl);
				}
			}
		});
	}

	//获取手机动态密码函数
	function getmobilepwd(){
		var mobile = $.trim($('#mobile').val());
		var unique = $.trim($('#unique').val());
		var mocode = $.trim($('#mocode').val());
		if(mobile == ''){
			$("#errormsg_1_text").html('手机号码不能为空');
			$("#errormsg_1").show();
			return false;
		}
		var myreg = /^(1+\d{10})$/; 
		if(!myreg.test(mobile)) { 
			$("#errormsg_1_text").html('请您输入正确的手机号');
			$("#errormsg_1").show();
			return false;
		}
		if(mocode == ''){
			$("#errormsg_1_text").html('验证码不能为空');
			$("#errormsg_1").show();
			return false;
		}
		if(unique == ''){
			$("#errormsg_1_text").html('动态密码发送失败');
			$("#errormsg_1").show();
			return false;
		}
		$.ajax({
			type: "get",
			url: "/ajax/ajax_get_login_pwd.php",
			async: false,
			dataType: "json",
			data: 'mobile='+mobile+'&mocode='+mocode+'&unique='+unique,
			success: function(data){
					if(data.error){
						$('#mocode').val('');
						$("#errormsg_1_text").html(data.msg);
						$("#errormsg_1").show();
						changecode(2);
					}else{
						$('#getcode_1').hide();
						$('#showcode_1').show();
						settime(60);
					}
					return;
			}
		});
	}

	//时间倒计时 60s
	function settime(num) { 
		if (num == 0) { 
			$('#getcode_1').show();
			$('#showcode_1').hide();
			return;
		} else { 
			$('#showcode_1').html(num+'秒后重新获取');
			num--; 
		} 
		setTimeout(function() { settime(num); }, 1000); 
	}

	//input获得焦点时事件处理
	function focuck(id){
		$("#errormsg_1").hide();
		if(id =='username'){
			if($("#username").val()=="请输入用户名/手机号"){
				$("#username").val('');
			}
		}
		if(id =='userpwd'){
			if($("#userpwd").val()=='请输入密码'){
				$("#userpwd").val('');
				document.getElementById('userpwd').type='password';
			}
		}
		if(id == 'mobile'){
			if($('#mobile').val() == "请输入您的手机号"){
				$('#mobile').val('');
			}
		}
		if(id == 'mobilepwd'){
			if($('#mobilepwd').val() == "请输入6位动态密码"){
				$('#mobilepwd').val('');
			}
		}
		if(id == 'mocode'){
			$('#mocode').val('');
		}
		if(id == 'ucode'){
			$('#ucode').val("");
		}
	}

	function blurmsg(id){
		if(id =='username' && $("#username").val() == ''){
			$("#username").val('请输入用户名/手机号');
		}
		if(id =='userpwd' && $("#userpwd").val() == ''){
			$("#userpwd").val('请输入密码');
		}
		if(id == 'mobile' && $('#mobile').val() == ''){
			$('#mobile').val('请输入您的手机号');
		}
		if(id == 'mobilepwd' && $('#mobilepwd').val() == ''){
			$('#mobilepwd').val('请输入6位动态密码');
		}
		if(id == 'mocode' && $('#mocode').val() == ''){
			$('#mocode').val('请输入验证码');
		}
	}

	$(document).keypress(function(e){
		var eCode = e.keyCode ? e.keyCode : e.which ? e.which : e.charCode;
		if (eCode == 13){
			$('#dologin').click();
		}
	});
</script>
<!-- 登陆切换end -->
