function catesapp(categoryid,isandr,isios,iswp,apptype,ysid,athis){
//var catelist=null;
$("."+ysid+" .cli").attr("class","");
$(athis).attr("class","cli");
var html='';
var apps="<ul>";
$.ajax({
url:"/android.php?categoryid="+categoryid+"&isandr="+isandr+"&isios="+isios+"&iswp="+iswp+"&apptype="+apptype,
type:'get',
dataType:'json',
success:function(json){
for(var i=0;i<json.length;i++){
var info=json[i];
//var cinfo=catelist[info.categoryid];
if(info.star==0){
info.star=30;
}
if(info.downloadcount>100000){
info.downloadcount='10万+';
}
html=html+"<li class=\"dangge_app\"><a href=\""+info.url+"\" class=\"app_icon\" target=\"_blank\"><i class=\"app_img_none\"></i><img alt=\""+info.name+"\" src=\""+info.logo+"\"></a><div class=\"app_name\"><a href=\""+info.url+"\" target=\"_blank\">"+info.name+"</a></div><span class=\"star bigstar"+info.star+"\"></span><span class=\"category\">下载量："+info.downloadcount+"</span><div class=\"app_xiazaib\"><a href=\""+info.dwurl+"\" target=\"_blank\" >下载</a></div></li>";
}
apps=apps+html+ "</ul>";
$('#'+ysid).html(apps);
}

});
}
</script> 
