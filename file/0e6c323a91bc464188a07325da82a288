var speed2=15 //
bbb2.innerHTML=bbb1.innerHTML
function Marquee2(){
if(bbb2.offsetTop-bbb.scrollTop<=0)
bbb.scrollTop-=bbb1.offsetHeight
else{
bbb.scrollTop++
}
}
var MyMrr2=setInterval(Marquee2,speed2)
bbb.onmouseover=function() {clearInterval(MyMrr2)}
bbb.onmouseout=function() {MyMrr2=setInterval(Marquee2,speed2)}
