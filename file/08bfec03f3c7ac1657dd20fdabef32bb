    queryJudgeInfo();

    /**
     * 查询法官信息
     */
    function queryJudgeInfo(){
                var caseId = "114923";
        var url = reqConfig.getInterface("getJudgeInfo");
        url+=caseId;
        $.ajax({
            url:url,
            type:'get',
            dataType:'json',
            data:{caseId:caseId},
            success:function(data){
                if(data.code == 0){
                    var judgeInfo = data.data;
                    var judgeName = judgeInfo.judgeName;
                    if(!judgeName || judgeName.length == 0) {
                        $('#mainJudge').hide();
                        $('#mainJudgeDiv').hide();
                        return ;
                    }
                    var judgeInfoStr = "<span id=\"mainJudge\">"+judgeName+"</span>"+judgeInfo.courtName;
                    var caseList = "";
                    var judgeCases = judgeInfo.cases;
                    if (judgeCases && judgeCases.length > 0) {
                        for(var i = 0; i < judgeCases.length; i++){
                            var temp = judgeCases[i];
                            caseList+="<li>"+
                                "<div class=\"video\" onclick=\"changeCase('"+temp.caseId+"')\"><img src=\""+temp.videoThumbnail+"\"></div>"+
                                "<p onclick=\"changeCase('"+temp.caseId+"')\"><a>"+temp.title+"</a></p>"+

                                "<p class=\"statics\"><span>"+temp.formattedViewCount+"</span> <i>"+moment(temp.beginTime).format('YYYY年MM月DD日 HH:MM')+"</i></p>"+

                            "</li>";
                        }
                    }
                    $("#judgeName").html(judgeInfoStr);
                    $("#jgul").html(caseList);
                }
            }
        });
    }
    
});

require(['page/live/liveDetail'],function(data){
    $("#caseList").find("li").click(function(){
        var caseId = $(this).attr("id");
        data.changeCase(caseId); 
    });
    $("#similarCaseList").find(".video h4").click(function(){
        var caseId = $(this).attr("id");
        data.changeCase(caseId); 
    });
    data.prasie();
    data.clearly();
    data.smooth();
    data.init();
    data.gather();
    data.headerBanner();
});


/**
 * 切换案件
 */
function changeCase(caseId){
    window.location.href="/live/"+caseId;
}

/**
 * 下载裁判文书
 */
function downDoc(url){
    window.open("http://www.sinaimg.cn/dy/slidenews/24_img/2016_31/66316_1387861_468665.jpg","_blank");
}

/**
 * 解析审判长
 */
function parseMainJudge(){
    var mainJudgeStr = "";
    var mainJudge = "".replace("；",";").replace("：",":").replace("，",",");
    var startReg = /^审判长:\S*,.*/;
    var startReg2 = /^审判长:\S*;.*/;
    var blankReg = /\S*\s.*/;
    var commalReg = /\S*,.*/;
    //正常格式
    if(startReg2.test(mainJudge)){
        mainJudge = mainJudge.replace(" ","");
        if(mainJudge.indexOf(";")){
            var temp = mainJudge.split(";")[0];
            mainJudgeStr = temp.split(":")[1];
        }
        $("#mainJudge").text(mainJudgeStr);
        return;
    }
    //类正常格式
    if(startReg.test(mainJudge)){
        mainJudge = mainJudge.replace(" ","");
        if(mainJudge.indexOf(",")){
            var temp = mainJudge.split(",")[0];
            mainJudgeStr = temp.split(":")[1];
        }
        $("#mainJudge").text(mainJudgeStr);
        return;
    }
    //单独人名的
    else if(mainJudge.indexOf(" ") < 0 || mainJudge.indexOf(",") < 0 || mainJudge.indexOf(":") < 0 || 
        mainJudge.indexOf(";") < 0 ||mainJudge.indexOf("：") < 0 || mainJudge.indexOf("；") < 0){
        mainJudgeStr = mainJudge;
        $("#mainJudge").text(mainJudgeStr);
        return;
    }
    //空格分隔的
    else if(blankReg.test(mainJudge)){
        //截取空格前面的名称
        var a = mainJudge.indexOf(" ");
        mainJudgeStr = mainJudge.substring(0,a);
        $("#mainJudge").text(mainJudgeStr);
        return;
    }
    else if(commalReg.test(mainJudge)){
        var a = mainJudge.indexOf(",");
        mainJudgeStr = mainJudge.substring(0,a);
        $("#mainJudge").text(mainJudgeStr);
        return;
    }
    
}
</script> 
 </body>
</html>