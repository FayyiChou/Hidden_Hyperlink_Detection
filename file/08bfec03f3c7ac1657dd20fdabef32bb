;function court(){
    var returnNum;
    if(returnCitySN.cid.length == 6 && typeof(returnCitySN) != "undefined"){
        returnNum = returnCitySN;
    }else{
        returnNum = {cid: '110000', cname:'北京市'};
    }
    areaCode = returnNum.cid.substring(2,0);
    window.location.href = "/court?areaCode=" + areaCode +'0000';
}
function app(){
    $.ajax({
        url: '/live/findFocus',
        type: 'GET',
        dataType: 'json',
    }).done(function(data) {

        if(data.data && data.data.num==0){
            $('#jump').show();
            $('#tel-binding').show();
         var t =  setTimeout(function(){
             window.location.href= '/preview';
        }, 5000)
    }else{
            window.location.href= '/live';
        }
 
    })
}
