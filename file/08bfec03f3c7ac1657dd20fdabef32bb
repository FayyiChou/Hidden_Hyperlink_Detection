    //给审判组织赋值
    var judgeContent = "";
    var judgeStr = "审判长:梁懿;代理审判员:刘生才;代理审判员:贾玉玉;书记员:李航;";
    var judges = judgeStr.replace("；",";").replace("：",":").split(";");
    if(judges){
        for(var i=0; i<judges.length;i++){
            var judge = judges[i];
            if(judge.indexOf(":")>-1){
                var temp = judge.split(":");
                judgeContent+="<li><span>"+temp[0]+"</span><i>"+temp[1]+"</i></li>";
            }
