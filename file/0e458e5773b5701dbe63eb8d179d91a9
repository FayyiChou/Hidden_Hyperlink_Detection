<script>
//职业选项卡
                $(".zy-page a").on("click", function() {
                    var i = $(this).index();
                    var cur = $(".zy-page").attr("data-index");
                    var list = $(".idx-ts-zy").find("img");
                    $(".zy-page").attr("data-index", i);
                    $(this).addClass("on").siblings().removeClass("on");
                    list.css("zIndex", 11);
                    list.eq(i).css("zIndex", 12);
                    list.eq(cur).css("zIndex", 13).stop(true, true).animate({opacity: 0}, 200, function() {
                        $(this).css("zIndex", 11).animate({opacity: 1}, 0);
                    });
                })
</script>
