</script><script src="//image.duoyi.com/js/jquery.min.js" type="text/javascript"></script><script type="text/javascript">
function set_video(obj){
	var vstr='<video id="video" width="640" height="360" poster="'+ obj.replace("mp4","jpg") +'"autoplay controls preload="none"> <source src="'+obj+'" type="video/mp4"  /> <img src="'+ obj.replace("mp4","jpg") +'" width="640" height="360" alt=""title="您的浏览器不支持视频播放，请下载flash或者安装最新浏览器。" /> </video>';
	$('.pop_video_con').html(vstr);
	$('.mask,.pop_video').show();
}
$(".video li").find("a").on("click",function(){
	var current_video = $(this).attr('href');
	set_video(current_video);
	return false;
})
$('.pop_close').on("click",function(){
	$('.mask,.pop_video').hide();
	$('#video').trigger('pause');
})

/** 新服 **/
$.ajax({
    url: '/xml/fullserver.xml',
    dataType: 'xml',
    type: 'GET',
    error: function(xml) {
        return false;
    },
    success: function(xml) {
        var server_dx, server_wt,
            that, id, t, day;
        server_dx = server_wt = {
            'id': 0,
            'date': 0
        };
        var all = [];
        $(xml).find('server').each(function() {
            that = $(this);
            day = (that.find('date').text()).split("-");
            day = day.length > 1 ? new Date(day[0], day[1], day[2]) : 0;
            t = {
                'id': parseInt(that.find('id').text(), 10) || 0,
                'name': that.find('name').text(),
                'url': that.find('url').text(),
                'date': day,
                net: that.find("network").text()
            };
            all.push(t);
        });
        //按日期排序，将最新的排在前面
        all.sort(function(a,b){
            return b.date-a.date;
        });

        server_dx = all[0];
        server_wt = all[1];

        //新服
        var server_new = server_dx,
            server_type = '新服推荐';

        if (server_dx.date < server_wt.date) {
            server_new = server_wt;
            server_type = '新服推荐';
        }

        $(".newser").html(server_dx.name);
        //$("div.wt>a").html(server_wt.name);
        $(".newser").attr('href', server_dx.url || "#");
        //$("div.wt>a").attr('href', server_wt.url || "#");
    }
});
</script>
