//截图投票
function pics_vote(id){
    var Urls = '//w2.duoyi.com/api/swapi.aspx?act=diding&type=didingjiepao&sid=' + id + '&470&format=json';
    $.getJSON(Urls,
            function (r)
            {
                if (r.status == 'error')
                {
                    alert(r.msg);
                } else if (r.status == 'success') {
                    $('#p_' + id).text(r.nums);
                }
            }
        );    
}
