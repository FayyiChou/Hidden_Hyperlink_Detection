<script type="text/jscript">
         $(function(){
           $("input[name=qt]").each(function(){
              $(this).click(function(){
                   var p=$(this).parent("span").parent("p");
                   var ci=p.find("input[name=txtPage]");
                   var c=ci.val();
                   
                   if(c.length==0)
                   {
                     alert("页码不能为空!");return;
                   }
                   
                   if(isNaN(c))
                   {
                     alert("页码必须是数字!");return;
                   }
                   
                   if(c.split(".").length>1)
                   {
                      alert("页码不能为小数!");return;
                   }
                   
                   c=parseInt(c);
                   if(c<0)
                   {
                       alert("页码不能为负数!");return;
                   }
                   
                   var url = window.location.href;
                   url = url.split(".")[0];
                   url= url.split("-")[0]+"-"+url.split("-")[1];
                   url = url+"-"+c.toString()+".html#comment";
                   
                   window.location.href = url;
                   
              });
           });
         });
         
         
         
         
   </script>
   
<script type="text/javascript">





 function delHtmlTag(str){
  return str.replace(/<[^>]+>/g,"");//去掉所有的html标记
 }
//var str = "<span style='display:none;'>This is test</span><img src=''>ss</img><strong></strong><br/>";
//str = delHtmlTag(str);
//alert(str);






 var href=window.location.href;
     
      var hrefs = href.split('/');
      var hrefl = hrefs[hrefs.length-1].substr(0,2);
       //优惠活动页面跳转
	  var href_yh = hrefs[hrefs.length-1].substr(0,6); 
	  if(href_yh=="news-3"){	  
	   //alert(href_yh);
    $(".newslist ul li p a").click(function(){
	var newhref = $(this).parents("p").siblings(".yhurl").html(); 
	
	
	//alert(newhref);
       $(this).attr("href",newhref);
         $(this).attr("target","_blank");
    })	  
	  }
	   //end优惠活动页面跳转     
      
      $(".nav ul li").each(function(){
          var nhref=$(this).find("a").eq(0).attr("href").substr(0,2);          
          if(hrefl == nhref)
          {   //alert(hrefl);
           $(this).eq(0).addClass("hoverLI");
              $(this).find("a").eq(0).addClass("hover");
              return;
          }
          else if(hrefl=="" && nhref=="in") 
          {
              $(this).find("a").eq(0).addClass("hover");
             $(this).eq(0).addClass("hoverLI");
              return;
          }                   
      });

</script>
   
   
