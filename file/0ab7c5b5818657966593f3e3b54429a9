function loadImgs(obj){

var o=obj,imgs=o.getElementsByTagName("img");

for(var i=0; i<imgs.length; i++) {

var s=imgs[i].getAttribute("#src");

if(s) imgs[i].src=s

}

}

loadImgs(document.getElementById("focusPic"));

