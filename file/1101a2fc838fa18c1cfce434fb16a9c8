<!--UrlTips-->
<script type="text/javascript">
var jUT = jQuery.noConflict();
jUT(document).ready(function() {
	var urlTips = jUT('.urltip-item');
	
	function urlTipShow( el ) {
		var parent = jUT(el).parents('.urltip-item');
		var block = parent.find('.urltip-wrap');
		var btn = parent.children('.urltip-item-btn');
		if( block.css('display') == 'none' ) {
			block.show();
			btn.addClass('btncheck');
		} else {
			block.hide();
			btn.removeClass('btncheck');
		}
		
	}
	
	jUT.each( urlTips, function(i,v){
		data = this.dataset;
		title = data.urltipTitle;
		text = data.urltipText;
		img = data.urltipImg;
		
		var wrap = jUT('<div>');
		
		var wrapImgThumb = jUT('<img>');
		wrapImgThumb.attr({'src':img, 'alt':''});
		var wrapImg = jUT('<div>').addClass('urltip-wrap-img').append( wrapImgThumb );
		var wrapInfo = jUT('<div>').addClass('urltip-wrap-info');
		var wrapTitle = jUT('<div>').addClass('urltip-wrap-title').text( title );
		var wrapText = jUT('<div>').addClass('urltip-wrap-text').text( text );
		
		var wrapMoreLink = jUT('<a>').attr({'href':jUT('a',this).attr('href'), 'target':'_blank'}).text('Подробнее');
		var wrapMore = jUT('<div>').addClass('urltip-wrap-more').append( wrapMoreLink );
		
		wrapInfo.append( wrapTitle ).append( wrapText ).append( wrapMore );
		if( img ) {
			wrap.append( wrapImg );
		}
		wrap.append( wrapInfo )
		
		wrap.addClass('urltip-wrap');
		
		jUT('.urltip-item-link',this).append( wrap );
		
		jUT('.urltip-item-link',v).hover(function(){
			var ch = jUT(this).parents().prop('checked');
			if( !ch ) urlTipShow(this);
		}, function(){
			var ch = jUT(this).parents().prop('checked');
			if( !ch ) urlTipShow(this);
		});
		jUT('.urltip-item-btn',v).click(function(){
			var ch = jUT(this).parents().prop('checked');
			jUT(this).parents().prop('checked', !ch);
			urlTipShow(this);
		});
	});
});
</script>
<!--UrlTips-->

