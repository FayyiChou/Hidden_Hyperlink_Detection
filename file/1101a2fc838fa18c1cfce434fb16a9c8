<script type="text/javascript"> // Скрипт обратного отсчета времени CountDown

var eventstr = "Акция закончена"; //Эта строка выводиться по окончанию отсчета
var montharray = new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec");
function CountDowndmn(yr,m,d, id_element){
			cdyear=yr;
			cdmonth=m;
			cdday=d;
			id_elementc =  id_element;
			var countdownid = document.getElementById(id_element); //ID элемента в который выводится время
			var today=new Date();
			var todayy=today.getYear();
			if (todayy < 1000)
			todayy+=1900;
			var todaym=today.getMonth();
			var todayd=today.getDate();
			var todayh=today.getHours();
			var todaymin=today.getMinutes();
			var todaysec=today.getSeconds();
			var todaystring=montharray[todaym]+" "+todayd+", "+todayy+" "+todayh+":"+todaymin+":"+todaysec;
			futurestring=montharray[m-1]+" "+d+", "+yr
			dd=Date.parse(futurestring)-Date.parse(todaystring);
			dday=Math.floor(dd/(60*60*1000*24)*1);
			dhour=Math.floor((dd%(60*60*1000*24))/(60*60*1000)*1);
			dmin=Math.floor(((dd%(60*60*1000*24))%(60*60*1000))/(60*1000)*1);
			dsec=Math.floor((((dd%(60*60*1000*24))%(60*60*1000))%(60*1000))/1000*1);
			if(dday<=0&&dhour<=0&&dmin<=0&&dsec<=1){
			$('#'+id_element).html(eventstr);
		return
		}
		else {
			var lastchar = ""+dsec;	lastchar = lastchar.substring(lastchar.length-1,lastchar.length);
			var dsecstr = "сек";
			if (lastchar=="1") { dsecstr = "сек"; }
			if ((lastchar=="2")||(lastchar=="3")||(lastchar=="4")) { dsecstr = "сек"; }
			
			lastchar = ""+dmin;	lastchar = lastchar.substring(lastchar.length-1,lastchar.length);
			var dminstr    = "мин";
			if (lastchar=="1") { dminstr = "мин"; }
			if ((lastchar=="2")||(lastchar=="3")||(lastchar=="4")) { dminstr = "мин"; }

			lastchar = ""+dhour;	
			firstchar = lastchar.substring(lastchar.length-2, lastchar.length);
			lastchar = lastchar.substring(lastchar.length-1,lastchar.length);	 
			var dhourstr   = "часов";
			if (lastchar=="1") { dhourstr = "час"; }
			if ((lastchar=="2")||(lastchar=="3" && firstchar=="1")||(lastchar=="4")) { dhourstr = "часа"; }
			
			lastchar = ""+dday;	lastchar = lastchar.substring(lastchar.length-1,lastchar.length);
			var ddaystr = "дн";
			if (lastchar=="1") { ddaystr = "дн"; }
			if ((lastchar=="2")||(lastchar=="3")||(lastchar=="4")) { ddaystr = "дн"; }
			$('#'+id_element).html("<span class='text_for_timer'>Осталось: </span> " + dday + ' д ' + dhour + " ч " + dmin + " " + dminstr + " " + dsec + " " + dsecstr);

			
		}
		setTimeout("CountDowndmn(cdyear,cdmonth,cdday, id_elementc)",1000);
}
function CountDowndmnRound(yr,m,d, id_element){
	cdyear=yr;
	cdmonth=m;
	cdday=d;
	id_elementc =  id_element;
	weeks_period = 1;
	var countdownid = document.getElementById(id_element); //ID элемента в который выводится время
	var today=new Date();
	var todayy=today.getYear();
	if (todayy < 1000)
	todayy+=1900;
	var todaym=today.getMonth();
	var todayd=today.getDate();
	var todayh=today.getHours();
	var todaymin=today.getMinutes();
	var todaysec=today.getSeconds();
	var todaystring=montharray[todaym]+" "+todayd+", "+todayy+" "+todayh+":"+todaymin+":"+todaysec;
	futurestring=montharray[m-1]+" "+d+", "+yr
	dd=Date.parse(futurestring)-Date.parse(todaystring);
	dday=Math.floor(dd/(60*60*1000*24)*1);
	dhour=24-todayh-1;			
	dmin=60-todaymin-1;
	dsec=60-todaysec-1;
	var lastchar = ""+dsec;	lastchar = lastchar.substring(lastchar.length-1,lastchar.length);
	var dsecstr = "сек";
	if (lastchar=="1") { dsecstr = "сек"; }
	if ((lastchar=="2")||(lastchar=="3")||(lastchar=="4")) { dsecstr = "сек"; }
	
	lastchar = ""+dmin;	lastchar = lastchar.substring(lastchar.length-1,lastchar.length);
	var dminstr    = "мин";
	if (lastchar=="1") { dminstr = "мин"; }
	if ((lastchar=="2")||(lastchar=="3")||(lastchar=="4")) { dminstr = "мин"; }

	lastchar = ""+dhour;	
	firstchar = lastchar.substring(lastchar.length-2, lastchar.length);
	lastchar = lastchar.substring(lastchar.length-1,lastchar.length);	 
	var dhourstr   = "часов";
	if (lastchar=="1") { dhourstr = "час"; }
	if ((lastchar=="2")||(lastchar=="3" && firstchar=="1")||(lastchar=="4")) { dhourstr = "часа"; }
	
	
	
	period=Math.floor((new Date() - new Date(cdyear, cdmonth-1, cdday))/(24*3600*1000));
	dday=(7*weeks_period)-(period-(Math.floor(period/(7*weeks_period))*7*weeks_period))-1;
	
	lastchar = ""+dday;	lastchar = lastchar.substring(lastchar.length-1,lastchar.length);
	var ddaystr = "дн";
	if (lastchar=="1") { ddaystr = "дн"; }
	if ((lastchar=="2")||(lastchar=="3")||(lastchar=="4")) { ddaystr = "дн"; }
	$('#'+id_element).html("<span class='text_for_timer'>Осталось: </span> " + dday + ' д ' + dhour + " ч " + dmin + " " + dminstr + " " + dsec + " " + dsecstr);
	setTimeout("CountDowndmnRound(cdyear,cdmonth,cdday, id_elementc)",1000);
}
function clear_active_li(){
	$('.exeple_menu li').each(function(){
		$(this).removeClass('active_li');	
	});
}
function clear_active_block(){
	$('.tabs_ex').each(function(){
		$(this).removeClass('active_block');	
	});
}
function setEqualHeight(columns) {
	var tallestcolumn = 0;
	columns.each( function () {
			currentHeight = $(this).height();
			if (currentHeight > tallestcolumn) {
				tallestcolumn = currentHeight;
			}
	});
	columns.height(tallestcolumn);
}
function test_box(id_form_hover,id_form,text_hidden){
	var id_element = id_form_hover;
	if( id_form ) try{
		window.goal('form','show');
		window.goal('form_'+id_form,'show');
	}catch(e){}
	var currentScroll = $(window).scrollTop();
	var windowsHeight = window.innerHeight;
	var height_block = $('#'+id_element).outerHeight();
	var padding_ofset = (parseInt(windowsHeight)-parseInt(height_block))/2;
	if( id_form != '' && (text_hidden != 'undefined' && text_hidden != '' )){
		if( id_form == 1 ) {
			id_form = 0;
		}
		document.getElementById('cf'+id_form+'_field_2').value = text_hidden;
	}
	if(id_form == 29){
		$('#form_title_service').html(text_hidden);
	}
	
	if(padding_ofset > 0){
		el_ofset = parseInt(currentScroll)+parseInt(padding_ofset);
	} else {
		el_ofset = parseInt(currentScroll)+10;
	}
	
	$('#overlay').fadeIn('fast',function(){
		$('#'+id_element).animate({'top':el_ofset+'px'},500);
	});
	return false;
}
function close_all(){
		$('#overlay').fadeOut('fast');
		$('.box_form').each(function(){
			
			$(this).animate({'top':'-2000px'},500);
		});
		return false;
}
