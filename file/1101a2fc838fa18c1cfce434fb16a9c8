	});
	
	$(".next_st").click(function() {				
		var index_element = $('.active_li').index();
		if(index_element==9){ index_next_el = 0; }
		else{ index_next_el = index_element+1; }
		clear_active_li();
		clear_active_block();
		$('#li_example_'+index_next_el).addClass('active_li');
		$('#tab_example_'+index_next_el).addClass('active_block');
		return false; 
	});
	$(".prev_st").click(function() {				
		var index_element = $('.active_li').index();
		if(index_element==0){ index_next_el = 9; }
		else{ index_next_el = index_element-1; }
		clear_active_li();
		clear_active_block();
		$('#li_example_'+index_next_el).addClass('active_li');
		$('#tab_example_'+index_next_el).addClass('active_block');
		return false; 
	});	
	
	carousel = $('#mycarousel').jcarousel({
		wrap: 'circular'
	});
	 setEqualHeight($(".review_single_anons_inner"));
	 setEqualHeight($(".review_single_place"));
	
	techartParallax();
	
	

	$(window).scroll(function () {
		if ($(this).scrollTop() > 50) {
		 $('a#scroll-top').fadeIn();
		} else {
		 $('a#scroll-top').fadeOut();
		}
	   });
	   
	   $('a#scroll-top').on('click', function(){
		$('html, body').animate({scrollTop:0}, 'slow' );
		return false;
	   });

	
	
			
	$('#button_order_scroll').click(function(){
		$('html, body').animate({ scrollTop: $('#form_footer').offset().top }, 500);
		return false;
	});
	
	$('.mobile-main-nav').on('click', 'i', function()
	{ 
		$(this).nextAll('.for_click_menu').slideToggle('fast');
	});
	$('.mobile-main-nav').on('click', '.opener', function()
	{
		
		$(this).next('.for_click_menu').slideToggle('fast');
		return false;
	});
	
	
	$( window ).resize(function() {
		$(".review_single_anons_inner").css('height','inherit');
		$(".review_single_place").css('height','inherit');
		setEqualHeight($(".review_single_anons_inner"));
		setEqualHeight($(".review_single_place"));
		
		
		$('#mycarousel').jcarousel('reload');
		$('#mycarousel').jcarousel('scroll', 0);
		
	});
});
