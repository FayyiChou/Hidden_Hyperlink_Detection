function displayValue() {
	var value_diagramm =  parseInt(document.querySelector('.js-callback').value);
	/* колонка  - сроки */
	var arr_time_text  = new Array('2 недели','1 месяц','1,5 месяца','2 месяца');
	var arr_time_height  = new Array('25','50','75','100');
	$('#text_time_create').html(arr_time_text[value_diagramm-1]);
	$('#column_time_create').css({'height':arr_time_height[value_diagramm-1]+'px'});
	

	/* колонка  - цена */
	var arr_cost  = new Array('29 990','39 990','49 990','69 990');
	var etalon_cost = parseInt(arr_cost[3]);
	var max_height_cost = 100;
	var height_cost = (parseInt(arr_cost[value_diagramm-1])/etalon_cost)*max_height_cost;
	$('#column_cost_create').css({'height':height_cost+'px'});
	$('#text_cost_create').html(arr_cost[value_diagramm-1]+' руб');
	
	/* колонка  - Картинка */
	$('.diaram_image_create img').each(function(){
		$(this).removeClass('active_img');	
	});
	$('#create_human_'+value_diagramm).addClass('active_img');

}
function displayValue2() {
	var value_diagramm =  parseInt(document.querySelector('.js-callback-context').value);
	/* колонка  - сроки */
	var arr_time_text  = new Array('CTR от 1.5%','CTR 1.5-3%','CTR 3-20%');
	var arr_time_height  = new Array('8','15','100');
	$('#text_time_kontext').html(arr_time_text[value_diagramm-1]);
	$('#column_time_kontext').css({'height':arr_time_height[value_diagramm-1]+'px'});
	

	/* колонка  - цена */
	var arr_cost  = new Array('4 990','9 990','19 990');
	var etalon_cost = parseInt(arr_cost[2]);
	var max_height_cost = 100;
	var height_cost = (parseInt(arr_cost[value_diagramm-1])/etalon_cost)*max_height_cost;
	$('#column_cost_kontext').css({'height':height_cost+'px'});
	$('#text_cost_kontext').html(arr_cost[value_diagramm-1]+' руб');
	
	/* колонка  - Картинка */
	$('.diaram_image_kontext img').each(function(){
		$(this).removeClass('active_img');	
	});
	$('#kontext_human_'+value_diagramm).addClass('active_img');

}
function slideShow() {

	//Set the opacity of all images to 0
	$('#gallery div').css({opacity: 0.0});
	
	//Get the first image and display it (set it to full opacity)
	$('#gallery div:first').css({opacity: 1.0});
	
	//Set the caption background to semi-transparent
	$('#gallery .caption').css({opacity: 0.7});

	//Resize the width of the caption according to the image width
	$('#gallery .caption').css({width: $('#gallery div').find('img').css('width')});
	
	//Get the caption of the first image from REL attribute and display it
	$('#gallery .content').html($('#gallery div:first').find('img').attr('rel'))
	.animate({opacity: 0.7}, 400);
	
	//Call the gallery function to run the slideshow, 6000 = change to next image after 6 seconds
	setInterval('gallery()',14000);
	
}
function gallery() {

	//if no IMGs have the show class, grab the first image
	var current = ($('#gallery div.show')?  $('#gallery div.show') : $('#gallery div:first'));

	//Get next image, if it reached the end of the slideshow, rotate it back to the first image
	var next = ((current.next().length) ? ((current.next().hasClass('caption'))? $('#gallery div:first') :current.next()) : $('#gallery div:first'));	
	
	//Get next image caption
	var caption = next.find('img').attr('rel');	
	
	//Set the fade in effect for the next image, show class has higher z-index
	next.css({opacity: 0.0})
	.addClass('show')
	.animate({opacity: 1.0}, 1500);

	//Hide the current image
	current.animate({opacity: 0.0}, 1500)
	.removeClass('show');
	
	//Set the opacity to 0 and height to 1px
	$('#gallery .caption').animate({opacity: 0.0}, { queue:false, duration:50 }).animate({height: '1px'}, { queue:true, duration:300 });	
	
	//Animate the caption, opacity to 0.7 and heigth to 100px, a slide up effect
	$('#gallery .caption').animate({opacity: 0.7},100 ).animate({height: '100px'},500 );
	
	//Display the content
	$('#gallery .content').html(caption);
}
function go_to_element(element){
			$('html, body').animate({ scrollTop: $('#'+element).offset().top }, 500);
			return false;
}
