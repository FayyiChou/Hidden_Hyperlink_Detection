<script type="text/javascript">
jQuery(document).ready(function(){
	var showSaleBlockTop = 400;
	function showSaleBlock( show ) {
		var block = jQuery('#saleblock');
		if( show ) {
			if( !block.hasClass('saleblock-fixed') )
				block.hide().addClass('saleblock-fixed').fadeIn(200);
		} else {
			block.removeClass('saleblock-fixed').show();
		}
	}
	if( window.scrollTop > showSaleBlockTop ) {
		showSaleBlock( true );
	}
	window.onscroll = function() {
		var scrolled = window.pageYOffset || document.documentElement.scrollTop;
		if( scrolled > showSaleBlockTop ) {
			showSaleBlock( true );
		} else {
			showSaleBlock();
		}
	}
});
</script>

