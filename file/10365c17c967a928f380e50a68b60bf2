    <script type="text/javascript">
    $(document).ready(function() {
        $("em.icon").click(function() {
            if ($(this).hasClass("checkhover")) {
                // 未勾选
                $(this).removeClass("checkhover");
                $("#download").hide();
                $("#spdownload").show();
                $(".text1").hide();
                $(".text2").show();
            } else {
                $(this).addClass("checkhover");
                $("#spdownload").hide();
                $("#download").show();
                $(".text2").hide();
                $(".text1").show();
            }
        });
    });
    </script>
    

