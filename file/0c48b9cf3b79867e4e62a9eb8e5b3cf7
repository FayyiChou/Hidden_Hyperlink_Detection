							<script type="text/javascript">
						function showDiv(divID,id) {
            if (divID != null && divID != "") {
                var v = document.getElementById(divID);
                if (v.style.display == "none") {
                    v.style.display = "block";
                    v.parentNode.style.zIndex = "1000";
					$('#'+id).css("display","block")
                }
            }
        }

        function hiddenDiv(divID,id) {
            if (divID != null && divID != "") {
                var vv = document.getElementById(divID);
                if (vv.style.display == "block") {
                    vv.style.display = "none";
                    vv.parentNode.style.zIndex = "100";
					$('#'+id).css("display","none")
					
                }
            }
        }
       
  
</script>
