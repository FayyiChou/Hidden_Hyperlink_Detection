jQuery('.of5qh>.of5qht>a').eq(0).trigger("mouseover");

jQuery('.qh2>.qh2t>a').mouseover(function() { TabSelect(".qh2>.qh2t>a", ".qh2>div.qh2m", "current", jQuery(this)) });
jQuery('.qh2>.qh2t>a').eq(0).trigger("mouseover");

jQuery('.qh3>.qh3t>a').mouseover(function() { TabSelect(".qh3>.qh3t>a", ".qh3>div.qh3m", "current", jQuery(this)) });
