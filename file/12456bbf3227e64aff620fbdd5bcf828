	$.ajaxSetup({
		contentType : "application/x-www-form-urlencoded;charset=utf-8",
		complete : function(xhr, textStatus) {
			//session timeout
			if (xhr.status == 911) {
				//如果超时就处理 ，指定要跳转的页面   
	           top.mainFrame.scs_alert('对不起，登录超时，请重新登录','warn',function(){
	           		top.location.href = pcs.common.path + '/login';
	           });
			}
		}
	});
