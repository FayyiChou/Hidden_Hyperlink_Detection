		function queryHotTag(){
		  	var url = pcs.common.path + "/web/qnzsmsg/getPopularTag";
			$.post(url,{}, function(data){
					if (data.success) {
						var tagsList=data.rs;//是一个list类型	length
						var str='';	
						   $.each(tagsList,function(i,obj){		
						   	    var tag = obj.tags;
						   	    var link = obj.link;
						   	    if(i == 1 || i == 6){
						   	      str += "<a href=\""+link+"\" target=\"_blank\" style=\"background:#ffdcd5\">"+tag+"</a>";
						   	    }else if(i == 2 || i== 7 ){
						   	      str += "<a href=\""+link+"\" target=\"_blank\" style=\"background:#e2efcc\">"+tag+"</a>";
						   	    }else if(i == 3 || i == 8){
						   	      str += "<a href=\""+link+"\" target=\"_blank\" style=\"background:#f9e7d8\">"+tag+"</a>";
						   	    }else if(i == 4 || i == 9){
						   	      str += "<a href=\""+link+"\" target=\"_blank\" style=\"background:#a6e0eb\">"+tag+"</a>";
						   	    }else if(i == 5 || i == 10){
						   	      str += "<a href=\""+link+"\" target=\"_blank\" style=\"background:#dfe2ef\">"+tag+"</a>";
						   	    }	
						   }); 
						   $('.xz2').append(str);
					} else {
						scs_alert(data.errmsg,'error');
					}
				}, 'json');
		}
		function queryHotQuestion(){
		    var url=pcs.common.path + "/web/qnzsmsg/getPopularQuesAns";
				$.post(url,{}, function(data){
					if (data.success) {
						var questions  = data.rs;//是一个list类型	
						 var question = '';
							   //question += "<ul>";		
						   $.each(questions,function(i,obj){
							  					
							   question += "<li class=\"q1\">"+obj.r_q_title+"<span><strong>"+obj.r_q_answernum+"</strong>条回答</li>";
							   question += "<li class=\"a1\"><p>"+obj.r_a_content+"<span>"+to_char2(parseInt(obj.r_a_added)*1000,'yyyy-MM-dd hh24:mi')+"</span><span><img src="+pcs.common.path+"/resources/image/qnzsh_03.png align=\"absmiddle\"/><strong style=\" padding-left:15px;\">"+obj.r_a_agree+"</strong></span></p></li>";	
							   	
							  
						   }); 
						    //question += "</ul>";				
							$('#hotQuestion').html(question);
						    //$('.nlist_lh li:even').addClass('lieven');
						    //$("div.nlist_lh").myScroll({
								//speed:80, //数值越大，速度越慢
								//rowHeight:150 //li的高度
							//});
						   
					} else {
						//scs_alert(data.errmsg,'error');
					}
				}, 'json');
		}
</script>
