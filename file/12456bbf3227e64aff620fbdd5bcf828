			function weibo(){
				var bloguid = "2173144152,2105248117,2093946341,1946469294,2266814964,1392015283,2627707951,1455013232,1992864072,2730549204";
				var iurl= pcs.common.path + "/weibo.jsp?uids=";
				$('#weibocontent').attr('src',iurl+bloguid);
					//var zcfg = document.getElementById("weibocontent");
					//zcfg.src = iurl+bloguid;
			}
