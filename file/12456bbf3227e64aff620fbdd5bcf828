			//八闽轻音
			function queryBmqyListByCount(){
				var url="/web/qnzsmsg/queryByCount?count=10&isHf=1";
				$.post(url,{}, function(data){
					if (data.success) {
						var news=data.rs;//是一个list类型	upNews	
						  
						   $.each(news,function(i,obj){		
						   	    var nickname = obj.lyAcctObj.nickname;
						   	    var userName = obj.lyAcctObj.userName;
								if(checkTextDataForMobile(userName)==true){
									userName = userName.substring(0,3)+'*****'+userName.substring(8,11);
								}	
								var showName = '';
								if(nickname!=null && nickname!='' && nickname!='undefined'){
									showName = nickname;
								}else{
									showName = userName;
								}
								var item='';						
							   item+='<li class="q1">[<strong style="color:#09F">'+obj.msgTypeName+'</strong>]&nbsp;&nbsp;'+obj.lyContent+'<span>提问人：'+showName+'&nbsp;&nbsp;'+to_char2(obj.lyDate,'yyyy-mm-dd hh24:mi:ss')+'</span></li>';
							   item += '<li class="a1">';
							   $.each(obj.answerList,function(i,anObj){
							   		item+='<p>'+anObj.hfContent+'<span>答复人：'+anObj.sysOrg.orgName+'&nbsp;&nbsp;'+to_char2(anObj.hfDate,'yyyy-mm-dd hh24:mi:ss')+'</span></p>';
							   });	
							   item += '</li>';					
							   $("#bmqyList").append(item);
						   }); 
						   $('.list_lh li:even').addClass('lieven');
						   $("div.list_lh").myScroll({
								speed:50, //数值越大，速度越慢
								rowHeight:600 //li的高度
							});
					} else {
						scs_alert(data.errmsg,'error');
					}
				}, 'json');
			}
			//书记问答
			function querySjwdListByCount(){
				var url="/web/qnzsmsg/queryByCount?count=10&isHf=1&isSjwd=1";
				$.post(url,{}, function(data){
					if (data.success) {
						var news=data.rs;//是一个list类型	upNews	
						  
						   $.each(news,function(i,obj){		
						   	    var nickname = obj.lyAcctObj.nickname;
						   	    var userName = obj.lyAcctObj.userName;
								if(checkTextDataForMobile(userName)==true){
									userName = userName.substring(0,3)+'*****'+userName.substring(8,11);
								}	
								var showName = '';
								if(nickname!=null && nickname!='' && nickname!='undefined'){
									showName = nickname;
								}else{
									showName = userName;
								}
								var item='';
								item+='<ul>';						
							   item+='<li class="q3">[<strong style="color:#09F">'+obj.msgTypeName+'</strong>]&nbsp;&nbsp;'+obj.lyContent+'<span>提问人：'+showName+'&nbsp;&nbsp;'+to_char2(obj.lyDate,'yyyy-mm-dd hh24:mi:ss')+'</span></li>';
							   $.each(obj.answerList,function(i,anObj){
							   		item+='<li class="a3">'+anObj.hfContent+'<span>答复人：'+anObj.hfSjName+'&nbsp;&nbsp;'+to_char2(anObj.hfDate,'yyyy-mm-dd hh24:mi:ss')+'</span></li>';
							   });		
							   item+='</ul>';				
							   $("#sjwdList").append(item);
						   }); 
					} else {
						scs_alert(data.errmsg,'error');
					}
				}, 'json');
			}
			
