			function querybmqyByCount(){
				var url="/web/qnzsmsg/queryByCount?count=30";
				$.post(url,{}, function(data){
					if (data.success) {
						var news=data.rs;//是一个list类型	upNews	
						  var item='';
						   $.each(news,function(i,obj){	
						   		var nickname = obj.lyAcctObj.nickname;
						   	    var userName = obj.lyAcctObj.userName;
								if(checkTextDataForMobile(userName)==true){
									userName = userName.substring(0,3)+'*****'+userName.substring(8,11);
								}	
								var showName = '';
								if(nickname!=null && nickname!='' && nickname!='undefined'){
									showName = nickname;
								}else{
									showName = userName;
								}
							   item=   '<li><strong style="color:#060; font-size:16px;">'+showName+'：</strong>'+obj.lyContent+'<span>[<strong>'+obj.msgTypeName+'</strong>]&nbsp;&nbsp;'+to_char2(obj.lyDate,'yyyy-mm-dd hh24:mi:ss')+' </span></li>';
							   $("#bmqyList").append(item);
						   }); 
						   $('.list_lh li:even').addClass('lieven');
						   $("div.list_lh").myScroll({
								speed:50, //数值越大，速度越慢
								rowHeight:600 //li的高度
							});
					} else {
						scs_alert(data.errmsg,'error');
					}
				}, 'json');
			}
 
			function queryNewsByCount(){
				var url="/web/news/queryByCount?count=15";
				$.post(url,{}, function(data){
					if (data.success) {
						var news=data.rs;//是一个list类型	upNews	
						   $.each(news,function(i,obj){					 
						   		var subTitle = pcs.common2.getHertSubStringCH(obj.title,66);			 
								var str = '<li><a href="/news/new.jsp?id='+obj.pkNews+'" target="_blank" title="'+obj.title+'">'+subTitle+' </a></li>';
								$("#newList").append(str);
						   }); 
 
							$('.wz_lh li:even').addClass('lieven');
							$("div.wz_lh").myScroll({
								speed:80, //数值越大，速度越慢
								rowHeight:30 //li的高度
							});
 
					} else {
						scs_alert(data.errmsg,'error');
					}
				}, 'json');
			}

			
