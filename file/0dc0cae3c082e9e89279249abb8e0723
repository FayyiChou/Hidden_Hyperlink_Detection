<script type="text/javascript">
  // jQuery Cycle 2 & SEH Sliders - Plugin Code
  jQuery('.cycle-slideshow').on('cycle-bootstrap', function(e, opts, API) {
    API.getSlideTitle = function(slideOpts, opts, slideEl) {
      if ( $(slideEl).children('a').attr('title') !== undefined ) {
        return $(slideEl).children('a').attr('title');
      } else {
        return $(slideEl).children('div').attr('data-cycle-title');
      }
    }
  });
</script>
