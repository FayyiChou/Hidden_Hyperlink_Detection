<script language="javascript" type="text/javascript">
jQuery(function() {
    // Attributes for progress indicator box
    jQuery("#progress-indicator-id").dialog({
       minWidth: 400,        // width in pixels
       autoOpen: false,      // true or false
       modal: true,
       draggable: false,
       closeOnEscape: false,
       dialogClass: 'no-close'
    });
    
