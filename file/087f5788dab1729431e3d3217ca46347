<script language="JavaScript" type="text/javascript">
var HPUI_URL = '/resource3';
var HPUI_LOCALE = 'zh-CN';
var HPUI_COUNTRY_NAME = '\u4E2D\u56FD';
var HPUI_LANGUAGE_NAME = '\u4E2D\u6587';
</script>
<script type="text/javascript">
jQuery('.js_cselector_trigger').click(function(event){
	jQuery('.js_cselector_target').toggle('slow');
	jQuery('.js_cselector_trigger').addClass('cs_selected');
	jQuery('body').addClass('cs_selected');
	jQuery('div.country_holder ul:first li:first a').focus();
	event.stopPropagation();
});
jQuery('.cselectorbtn').click(function(event){
	jQuery('.js_cselector_target').toggle('slow');
	jQuery('body').removeClass('cs_selected');
	jQuery('.js_cselector_trigger').removeClass('cs_selected');
});
jQuery('.js_cselector_target').click(function(event){
	event.stopPropagation();
});
jQuery('body').click(function(){
	if(jQuery('.js_cselector_target').css('display')==='block') {
		jQuery('.js_cselector_target').toggle('slow');
		jQuery('body').removeClass('cs_selected');
		jQuery('.js_cselector_trigger').removeClass('cs_selected');
	}
});
jQuery('div.country_holder ul li a').keydown(function(event){
	var firstCountry = jQuery('div.country_holder ul:first li:first a').text();
	var lastCountry = jQuery('div.country_holder ul:last li:last a').text();
	var currentCountry = jQuery(this).text();
	if((jQuery(this).is(':focus') && event.keyCode == 27)||(currentCountry == lastCountry && event.keyCode == 9)||(currentCountry == firstCountry && event.shiftKey && event.keyCode == 9)){
		jQuery('.js_cselector_target').toggle('slow');
		jQuery('body').removeClass('cs_selected');
		jQuery('.js_cselector_trigger').removeClass('cs_selected');
		jQuery('.js_cselector_trigger').focus();
		event.stopPropagation();
	}
});
</script>
<script>
jQuery(function() {
