        draggable: false,
        closeOnEscape: false,
        dialogClass: 'no-close'
     });
 });
</script>






<script language="javascript" type="text/javascript">
function psiMoreProductSelectFormSubmit() {
    var selectedValue = document.getElementById("psiMoreProductSelectOption").value;
    var selectedOption = document.getElementById("psiMoreProductSelectOption");
    var selectedID = selectedOption.options[selectedOption.selectedIndex].id;
    //To prevent default action submit on the form
   if(selectedValue=="selectorDefault"){
		return false;
   }
   
    if (selectedValue != "serialModal") {
        //showClassicIndeterminateProgressIndicator("myIndicator1");
		if(selectedID !="#companyNavigationText"){
			hpuiOpenProgressIndicator(jQuery('#progress-indicator-ProfileId'), window);
		}
        window.location.href = selectedValue;
    }
    if (selectedID == "#serialDialog") {
        jQuery(selectedID).dialog("open");
    }
       
}

</script>










                                                                               


 
 

