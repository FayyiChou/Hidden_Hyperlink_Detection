<script>
function checkCode(){
    var code = $("#code").val();
    if (code == "") {
        alert("请输入票面验证码");
        return;
    }
	window.location.href="validateresult.html?code="+encodeURIComponent(code.replace(/#/g, "$"));
}
</script>
