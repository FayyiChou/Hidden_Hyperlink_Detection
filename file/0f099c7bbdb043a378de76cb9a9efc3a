    var contentid = '565393';// 文章ID
    var title = "\u5168\u7403\u9646\u519b\u201c\u4e94\u5f3a\u201d:\u4e2d\u5370\u5175\u529b\u96c4\u539a  \u7f8e\u4fc4\u82f1\u6218\u529b\u5f3a";//文章标题
