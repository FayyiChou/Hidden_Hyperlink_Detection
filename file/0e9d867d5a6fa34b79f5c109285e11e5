		<script type="text/javascript">
		$(function(){
			
			$.get(
				"news.do",
				function(data){
					for(var i=0;i<data.length;i++){
												
						var ul=$('#news');
						var li=$("<li></li>");
						var a=$("<a></a>");
						a.attr("href",data[i].news).attr("target","_blank").text(data[i].title);
						var span=$("<span></span>");
						span.text(data[i].strTime).after(a);
						li.append(a);
						ul.append(li);
						
					}
					//-----------------------新闻轮播---------------------------
	var app_show1 = new ScrollPic();
	app_show1.scrollContId   = "news"; //内容容器ID
	app_show1.arrLeftId      = "";//左箭头ID
	app_show1.arrRightId     = ""; //右箭头ID
	app_show1.dotListId      = "";//点列表ID
	app_show1.dotClassName   = "";//点className
	app_show1.dotOnClassName = "";//当前点className
	app_show1.listType		 = "";//列表类型(number:数字，其它为空)
	app_show1.listEvent      = "onclick"; //切换事件
	app_show1.frameWidth     = 40;//显示框宽度
	app_show1.frameWidth_1   = 5;//显示框宽度
	app_show1.pageWidth      = 40; //翻页宽度
	app_show1.upright        = true; //垂直滚动
	app_show1.speed          = 50; //移动速度(单位毫秒，越小越快)
	app_show1.space          = 10; //每次移动像素(单位px，越大越快)
	app_show1.autoPlay       = true; //自动播放
	app_show1.autoPlayTime   = 1; //自动播放间隔时间(秒)
	app_show1.initialize(); //初始化
					
				}
				
			);
			
		});
		
	</script>
