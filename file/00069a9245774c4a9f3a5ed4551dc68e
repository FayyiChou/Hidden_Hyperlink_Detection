        function collage() {
            $('#gallery-list').removeWhitespace().collagePlus(
    {
        'targetHeight': 300,
        // 'effect': 'effect-6',
        'effect': 'default',
        'allowPartialLastRow': true
    });
        };
        function showMode2() {
            var img = $('#gallery-list img');
            img.each(function () {
                var t = $(this);
                t.attr("src", t.attr('m'));
                t.attr("width", t.attr('w'));
                t.attr("height", t.attr('h'));
            });
            collageHandler = $('#gallery-list').removeWhitespace().collagePlus({
                'targetHeight': 300,
                // 'effect': 'effect-6',
                'effect': 'default',
                'allowPartialLastRow': true
            });
            //绑定resize时的等高排列
            var resizeTimer = null;
            $(window).bind('resize', function () {
                $('#gallery-list li').css("opacity", .8);
                if (resizeTimer) clearTimeout(resizeTimer);
                resizeTimer = setTimeout(collage, 200);
            });

            $('#gallery-list').removeClass('mode1');
        } function FitScreen() {
            $('#content').css({ 'min-height': ($(window).height() - $('.head2016').outerHeight(true) - $('.nav2016').outerHeight(true) - $('#footer').outerHeight(true) - $('.result_bar').height()) + 'px' });
        }
        //小图的tooltip
        function showToolTip() {

            var a = $("#gallery-list li img").tooltip({
                track: true,
                delay: 500,
                showURL: false,
                bodyHandler: function () {
                    //                    console.log(this.lowsrc);
                    return $("<img/>").attr("src", this.lowsrc);
                }
            });
        }
        $(function () {
            showMode2();
            FitScreen();
            showToolTip();
            //购买
            $(".img_bar span.img_buy").click(function () {
                window.open('http://www.quanjing.com/imgbuy/' + $(this).attr('i') + '.html', '_blank');
            })
