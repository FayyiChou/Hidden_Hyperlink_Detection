        var rege= new RegExp("\d+")
        if (rege.test(index)) {
    alert("请输入正确的数字");
    return false;
}        if (index<1 || index>pagecount) {
    alert("请输入正确页码");
        return false;
}   
window.location.href= "/topic/"+kwd+"/"+index+1;
          }  
        })
    </script>
    <p style="position: fixed; bottom: 20px; right: 5px; border: 1px solid #ccc; z-index: 9999;
        text-align: left;">
        <a href="javascript:window.scrollTo(0,0);">
            <img src="/image/go2top.gif" alt="到顶部"></a>
    </p>
    
    </form>


