    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>Авторизация</h3>
    </div>
    <div class="modal-body" style="padding-right: 30px">
        <div id="login-notification" class="alert alert-error" hidden>
            <button id="login-dismiss" type="button" class="close">&times;</button>
            <strong>Ошибка:</strong> <span id="login-error">Неверный логин или пароль.</span>
        </div>
        <form id="login-form" class="form-horizontal">
            <div class="control-group">
                <label class="control-label" for="fAuthEmail">E-Mail</label>
                <div class="controls">
                <input type="text" id="fAuthEmail" placeholder="E-Mail">
                </div>
            </div>

            <div class="control-group">
                <label class="control-label" for="fAuthPassword">Пароль</label>
                <div class="controls">
                    <input type="password" id="fAuthPassword" placeholder="Пароль">
                    <span class="help-block"><a href="https://leagueofgames.net/user/password-reset">Забыли пароль?</a></span>
                </div>
            </div>

            <div class="control-group">
                <div class="controls">
                    <label class="checkbox"><input id="chkKeepLogin" type="checkbox">Запомнить меня</label>
                </div>
            </div>

        </form>
    </div>
    <div class="modal-footer" style="text-align: center">
        <input id="btnLogin" type="submit" class="btn btn-primary" value="Авторизоваться"><span style="margin-left: 4px">или</span>
            <a href="https://leagueofgames.net/registration">Зарегистрироваться</a>

    </div>
    <script language="JavaScript">
        function doLogin()
        {
            var email=$('#fAuthEmail').val();
            var password=$('#fAuthPassword').val();
            var keeplogin = $('#chkKeepLogin').is(":checked")?1:0;

            $.ajax({
                type: 'POST',
                url: 'https://leagueofgames.net/user/login_ajax',
                dataType: 'json',
                data: {email: email, password: password, keeplogin:keeplogin},
                success: function(data){
                    if (data.result=='error'){
                        $('#login-error').text(data.error);
                        $("#login-notification").show();
                    }
                    else
                    {
                        location.reload();
                    }
                }
            })
        }

        $('#btnLogin').on('click', doLogin)

        $("#login-dismiss").on("click", function(){
            $("#login-notification").hide();
        })

        $("#login-form").on("submit", function(){
            doLogin();
            return false;
        });
    </script>
</div>

<script>
    $("#login").on("click",function(e){
        e.preventDefault();
        $("#loginWindow").modal();
    })
</script>


