<script>
  var news_info_1 = new Swiper('#news1 .news_info',{
    speed:600,
	mode : 'vertical',
	simulateTouch : false,
	calculateHeight : true,
    onSlideChangeStart: function(){
      $("#news1 .news_tabs .active").removeClass('active')
      $("#news1 .news_tabs a").eq(news_info_1.activeIndex).addClass('active')  
    }
  });
  $("#news1 .news_tabs a").on('touchstart mousedown',function(e){
    e.preventDefault()
    $("#news1 .news_tabs .active").removeClass('active')
    $(this).addClass('active');
	
     news_info_1.swipeTo( $(this).index() ,1000, false );
  });
  $("#news1 .news_tabs a").click(function(e){
    e.preventDefault()
  });
</script>


