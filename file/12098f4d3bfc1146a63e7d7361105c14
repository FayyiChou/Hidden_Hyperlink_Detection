              footballTap: document.querySelectorAll('ul.toggle-cz li')[0],
              basketballTap: document.querySelectorAll('ul.toggle-cz li')[1],
              matchCenterIcon: document.querySelectorAll('div.headernavR a')[0],
              homeBackIcon: document.querySelectorAll('div.headernavL a')[0],
              hash: location.hash,
              homeTeamDom:document.querySelector('#team_home'),
              awayTeamDom:document.querySelector('#team_away'),
              homeScoreDom:document.querySelector('#home_score'),
              awayScoreDom:document.querySelector('#away_score'),
              concernDom:document.querySelector('div.sk-gz'),
              leagueDom:document.querySelector('div.r-sn'),
              matchStatusInfo:{'4':'完场','2':'中场休息'},
              basketBallMatchStatusInfo:{'11':'完场','12':'改期'},
              homeHref:document.querySelector('a.link-index'),
              detailBackIcon:document.querySelectorAll('div.detailTop a')[0],
              detailHomeIcon:document.querySelectorAll('div.detailTop a')[1],
              sessionId:'',
              init: function () {

                  try {
                      if (window.EsApp && (!(location.search.indexOf('app_online')>-1 || location.search.indexOf('app_home')>-1))) {
                          home.homeBackIcon.style.display = 'none';
                      }
                      if (window.EsApp && window.EsApp.getInfo && window.EsApp.getInfo().apple_hide == 1) {
                          home.matchCenterIcon.style.display = 'none';
                      }
                      if (location.search.indexOf('from=app_bet') > -1 || location.search.indexOf('from=bet') > -1) {
                          home.detailBackIcon.style.display = 'none';
                          home.detailHomeIcon.style.display = 'none';
                      }
                      if (!home.hash || location.hash.indexOf('/football/') > -1 || location.hash.indexOf('/basketball/') > -1) {
                          home.homeHead.style.display = 'block';
                      } else {
                          home.detailHead.style.display = 'block';
                      }
                      if (!home.hash || home.hash.indexOf('football') > -1) {
                          home.footballTap.className = 'cur';
                      } else {
                          home.basketballTap.className = 'cur';
                      }
                  } catch (e) {
                      console.log(e)
                  }
                  home.bindHashChange();
                  home.bindEsEvent();
                  home.handleHomeHash();
                  home.listenR();
              },
              back:function() {
                  var platform = esun.use('esun.util.platform');
                  if (platform.isApp) {
                      return window['EsApp'].invoke('home');
                  }
                  if (document.domain.indexOf("c.m.500.com") > -1 || document.referrer.indexOf("c.m.500.com") > -1) {
                      location.replace("http://c.m.500.com")
                  } else {
                      location.replace("http://m.500.com")
                  }
              },
              initHead:function(){
                  home.leagueDom.innerHTML = '';
              },
              doConcern:function(){
                  var platform = esun.use('esun.util.platform');
                  if (!platform.isLogin()) {
                    return  platform.login();
                  }
                  if(home.hash.indexOf('football')>-1){
                      EsEvent.$emit('football-concern');
                  }else{
                      EsEvent.$emit('basketball-concern');
                  }
                  if(home.concernDom.className.indexOf('cur')>-1){
                      home.concernDom.className = 'sk-gz';
                  }else{
                      home.concernDom.className = 'sk-gz cur';
                  }
              },
              bindHashChange: function () {
                  window.addEventListener('hashchange', function () {
                      try{
                          if(((home.hash.indexOf('/football/') > -1 || home.hash.indexOf('/basketball/') > -1)&&((location.hash.indexOf('/footballdetail/') > -1 || location.hash.indexOf('/basketballdetail/') > -1)))||
                              ((location.hash.indexOf('/football/') > -1 || location.hash.indexOf('/basketball/') > -1)&&((home.hash.indexOf('/footballdetail/') > -1 || home.hash.indexOf('/basketballdetail/') > -1)))){
                              if(document.querySelector('application')&&document.querySelector('application').children){
                                  for(var i= 0,j=document.querySelector('application').children.length;i<j;i++){
                                      document.querySelector('application').removeChild(document.querySelector('application').children[i]);
                                      i--;j--;
                                  }
                              }
                          }
                      }catch(e){
                          console.log(e)
                      }
                      home.hash = location.hash;
                      if (location.hash.indexOf('/football/') > -1 || location.hash.indexOf('/basketball/') > -1) {
                          home.detailHead.style.display = 'none';
                          home.homeHead.style.display = 'block';
                      } else {
                          home.homeHead.style.display = 'none';
                          home.detailHead.className = 'detailTop topBarMove2';
                          home.detailHead.style.display = 'block';
                      }
                      if (location.hash.indexOf('/football/') > -1) {
                          home.footballTap.className = 'cur';
                          home.basketballTap.className = '';
                      } else {
                          home.footballTap.className = '';
                          home.basketballTap.className = 'cur';
                      }
                      home.handleHomeHash();
                  })
              },
              handleHomeHash:function(){
                  if(!home.hash || home.hash.indexOf('football')>-1){
                      home.homeHref.setAttribute('href','#/football/match/jczq')
                  }else{
                      home.homeHref.setAttribute('href','#/basketball/match/jclq')
                  }
              },
              showScore:function(homeScore,awayScore){
                  home.homeScoreDom.innerHTML = homeScore;
                  home.awayScoreDom.innerHTML = awayScore;
                  home.handleMid('block','none');
              },
              handleMid:function(scoreType,vsType){
                  document.querySelector('#type_vs').style.display = vsType;
                  document.querySelector('#type_score').style.display = scoreType;
              },
              bindEsEvent:function(){
                  EsEvent.$on('match',function(match){

                      home.sessionId = match.seasonid;
                      home.initHead();
                      if(home.hash.indexOf('football')>-1){
                          home.homeTeamDom.innerHTML = match.homesxname;
                          home.awayTeamDom.innerHTML = match.awaysxname;
                          home.leagueDom.innerHTML =  match.simpleleague;
                          switch (match.status){
                              case '0': home.leagueDom.innerHTML =  match.simpleleague,home.handleMid('none','block');break;
                              case '1':home.showScore(match.homescore,match.awayscore);break;
                              case '2':home.showScore(match.homescore,match.awayscore);break;
                              case '3':home.showScore(match.homescore,match.awayscore);break;
                              case '4':home.showScore(match.homescore,match.awayscore);break;
                              default :home.handleMid('none','block');
                          }
                      }else{
                          home.homeTeamDom.innerHTML = match.awaysxname;
                          home.awayTeamDom.innerHTML = match.homesxname;
                          home.leagueDom.innerHTML =  match.simpleleague;
                          switch (match.status){
                              case '0':home.handleMid('none','block');break;
                              case '1':home.handleMid('none','block');break;
                              case '12':home.handleMid('none','block');break;
                              default :home.showScore(match.awayscore,match.homescore);
                          }
                      }
                      if(match.isfocus=='0'){
                          home.concernDom.className = 'sk-gz';
                      }else{
                          home.concernDom.className = 'sk-gz cur';
                      }
                  },false)
              },
              showSelect: function () {
                  EsEvent.$emit('filter');
                  _hmt.push(['_trackEvent','zq','click','filter'])
              },
              hrefLeague:function(){
                  _hmt.push(['_trackEvent','zq','click','liansai']);
                 setTimeout(function(){
                     location.href='match_center/index.html?from=web_home';
                 },20)
              },
              goLeague:function(){
                  if(home.hash.indexOf('football')>-1){
                      location.href='match_center/index.html#/footballleague/integral/'+home.sessionId;
                  }else{
                      location.href='match_center/index.html#/basketballleague/rank/'+home.sessionId;
                  }
              },
              timFormat:function (matchAt, isFirstHalf) {
                  var second = Number(matchAt);
                  if (second >= 45 * 60) {
                      return isFirstHalf ? '45+' : '90+';
                  }
                  var minute = Math.ceil(Number(matchAt) / 60);
                  if (minute <= 0) {
                      minute = 1;
                  }
                  return isFirstHalf ? minute : (minute + 45);
              },
              listenR:function(){
                  var send = window.XMLHttpRequest.prototype.send;
                  var open = window.XMLHttpRequest.prototype.open;
                  function monitorOpen(method,url,async,user,password){
                      home.handleRequestIcon('block',url) ;
                      return open.apply(this, arguments);
                  }
                  function monitorSend(data) {

                      if (this.onreadystatechange) {
                          this._onreadystatechange = this.onreadystatechange;
                      }
                      this.onreadystatechange = monitorReadyStateChange;
                      return send.apply(this, arguments);
                  }
                  function monitorReadyStateChange() {
                        
                      if (this.readyState == 4) {
                         
                          home.handleRequestIcon('none') ;
                      }
                      return this._onreadystatechange.apply(this, arguments);
                  }
                  window.XMLHttpRequest.prototype.send = monitorSend;
                  window.XMLHttpRequest.prototype.open = monitorOpen;
              },
              handleRequestIcon:function(type,url){
                  if(document.querySelector('div.loading')&&
                      document.querySelector('div.loading').className.indexOf('hide')==-1&&
                      (!url||(url&&url.indexOf('ews.500.com')>-1&&url.indexOf('/concern/focus')==-1)&&url.indexOf('/concern/state')==-1)){
                      document.querySelector('div.loading').style.display = type;
                  }
              }
          };
          home.init();

          Date.prototype.format = function(partten)
          {
              if(partten ==null||partten=='')
              {
                  partten = 'y-m-d'    ;
              }
              var y = this.getFullYear();
              var m = this.getMonth()+1;
              var d = this.getDate();
              var r = partten.replace(/y+/g,y);
              r = r.replace(/m+/g,(m<10?"0":"")+m);
              r = r.replace(/d+/g,(d<10?"0":"")+d);
              return r;
          }
      </script>
