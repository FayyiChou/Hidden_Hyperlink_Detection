        document.addEventListener('DOMContentLoaded', function () {
            window.FastClick && window.FastClick.attach(document.body);
            
            var platform = esun.use('esun.util.platform');

            platform.dataChanged(function () {
                location.reload();
            });
            
            platform.ready(function () {
               if (platform.isApp) {
                   // 如果是app首页打开的，要让app打开的链接带上&from=app_home这个字段，不显示app的title
                   var from = drunk.querystring.parse(location.search.slice(1))['from'];
                   var isFromAppHome = (from== 'app_home')||(from== 'app_online');//判断来源
                   EsApp.invoke('titleBar', {isShow: isFromAppHome ? '0' : '1', title: '比分详情'});
               }
            });
        });
        // drunk.config.debug = true;
        drunk.Application.start(document.querySelector('application'));
    </script>
