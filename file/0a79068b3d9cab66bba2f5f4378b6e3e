<script type="text/javascript">
var time;
$(function(){
$(".center_wrap").each(function(i){
var cur = $(this).find('.mt8').eq(0);
var w = cur.width();
var h = cur.height();
$(this).find('.mt8 img').LoadImage(true, 950, 950,'statics/images/msg_img/loading.gif');});

$('li.back_t').hover(function (){
var _this = $(this);
time = setTimeout(function () {
_this.find('div.mainNav_b').slideDown();
_this.addClass('onmouse');
}, 500);}, function () {
clearTimeout(time);
$(this).find('div.mainNav_b').slideUp();
$(this).removeClass('onmouse');});


});


</script>


