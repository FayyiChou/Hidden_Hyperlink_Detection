function pageLocation(url){    
    if (window != null){
    	if (window.opener != null){
    		if(window.opener.opener != null){
			    window.close();
			    window.opener.close();
			    window.opener.opener.location.href=url;
			    return true;
    		}
		    window.close();
		    window.opener.location.href=url;
		    return true;
    	}
	    window.top.location.href=url;
	    return true;
    } else {
    	window.top.location.href=url;
    	return true;
    }
}

</script>






