<script type="text/javascript">
function isNum(num){
  	var reNum=/^\d*$/;
 	return(reNum.test(num));
}
var sourceUrl = '';
function go(p){
	if(!isNum(p)){
		alert("输入必须为数字");
		return false;
	}
	if(p > parseInt(jQuery("#pageCount1").val())){
		alert("输入数字不得大于页的总数");
		return false;		
	}
	var b = jQuery("#b").val();
	window.location.href="/product/detail-31111-" + p + "-" + b + "-0.html";
}

function go2(b){
	if(!isNum(b)){
		alert("输入必须为数字");
		return false;
	}
	if(b > parseInt(jQuery("#pageCount2").val())){
		alert("输入数字不得大于页的总数");
		return false;		
	}
	var p = jQuery("#p").val();
	window.location.href="/product/detail-31111-" + p + "-" + b + "-1.html";
}

function countDown(obj,obj2){
	var time=obj/1000
	var day_elem = jQuery(".col_day");
	var hour_elem = jQuery(".col_hour");
	var minute_elem = jQuery(".col_min");
	var second_elem = jQuery(".col_sec");
	var timer = setInterval(function(){
		if (time > 1) {
			time -= 1;
			var day = Math.floor((time / 3600) / 24);//计算天
			var hour = Math.floor((time / 3600) % 24);//计算小时
			var minute = Math.floor((time / 60) % 60);//计算分钟
			var second = Math.floor(time % 60);//计算秒杀
			jQuery(day_elem).text(day+"天");
			jQuery(hour_elem).text(hour<10?"0"+hour+"时":hour+"时");
			jQuery(minute_elem).text(minute<10?"0"+minute+"分":minute+"分");
			jQuery(second_elem).text(second<10?"0"+second+"秒":second+"秒");
		} else {
			clearInterval(timer);
		}
	}, 1000);
	if(jQuery("#storage") && jQuery("#storage").val()){
		jQuery("#surplusTime1").show();
    	jQuery("#surplusTime2").show();
    }
}

//购买
function buyProduct(){
	var isSubmit = true;
	var reg = /^[1-9]\d*$/;
	var num = jQuery("#productNum").val();
	if(!reg.test(num)){
		alert("请输入正确的商品数量");
		return;
	}
	var storage = jQuery("#storage").val();
	if(parseInt(num) > parseInt(storage)){
		alert("购买数量超出库存数量!");
		return;
	}

//校验购物车礼品是否大于10种类型的礼品
jQuery.ajax({
 		type: "post",
		async: false,
		url: "/appController/chkAddCartProductNum",
		success: function(data){
 			if(data != ""){
 				alert(data);
 				isSubmit = false;
 			}
 		}
 	});
	if(isSubmit){
		jQuery("form").submit();
	}
}

//加入购物车
function toAddShopCart() {
	var reg = /^[1-9]\d*$/;
 	var num = jQuery("#productNum").val();
 	var storage = jQuery("#storage").val();
 	var proTypeCode=jQuery("#proTypeCode").val();
 	if (!reg.test(num)) {
 		alert("请输入正确的商品数量");
 		return false; 		
 	} 
 	
			var storage = jQuery("#storage").val();
		if(parseInt(num) > parseInt(storage)){
			alert("购买数量超出库存数量!");
			return false;
		}
		
	var isSubmit = true;
		
	if(isSubmit){
			jQuery.ajax({
				type:"post",
			async:true,
		    	url:"/appController/addShopCart",
		    	data:"pid=31111&productNum=" + num,
				beforeSend:function(){jQuery("#addShopCart").hide();},
		    	success:function(data){
		    		if("beyond" == data){
						alert("购买数量超出库存数量!");
					}else if("beyondNum"==data){
						alert("您兑换的商品数量已达上限（一个商品最多兑换10个），请确认。");
					}else if("beyondType"==data){
						alert("您兑换的商品种类已达上限（种类最多为10个），请确认。");
					}else{
						var proNum = -1;
			    		proNum = parseInt(data);
						if(proNum > -1){
				 			jQuery("strong[name='cartCnt']").text(proNum);
		 		 			jQuery("#addCart").html("购物车共"+proNum+"件商品");
		 		 			jQuery("#buy_tcc").show();
		 		 			jQuery("#numBuy").hide();
						}else{
							alert(data);
						}
					}
						jQuery("#addShopCart").show();  
				}
			}); 		
 	}
} 	

//关闭购物车提示
function offShopCart(){
	jQuery("#buy_tcc").hide();
	jQuery("#numBuy").show();
}

//添加礼品到购物车的数量
function changeNum(obj){
	if(obj==1){
		var oldnum=parseInt(jQuery("#productNum").val());
		if(oldnum>9){
			alert("您兑换的商品数量已达上限，请您提交订单后再次兑换。");
		}else{
	    	jQuery("#productNum").val(oldnum+1);
	    }
	}else{
	    var oldnum = parseInt(jQuery("#productNum").val());
	    if(oldnum > 1){
	    	jQuery("#productNum").val(oldnum-1);
	    }
	}
}

function changeProNum(){
	var reg= /^[1-9]\d*$/;
	var numm =jQuery("#productNum").val();
	if(!reg.test(numm)){
		alert("请输入有效礼品数！");
		jQuery("#productNum").val(1);
	}
	if(parseInt(numm)>10){
		alert("您兑换的商品数量已达上限，请您提交订单后再次兑换。");
		jQuery("#productNum").val(1);
	}
}

//鼠标经过事件
function hov(i){
	document.getElementById("integral"+i).style.display = "block"
}

function out(i){
	document.getElementById("integral"+i).style.display = "none"
}
		
/**
* 验证电话格式是否合法，支持固话和手机
* param str 待处理的字符串
* param type 验证类型串 "1"必须为手机，"2"必须为固话(格式：区号-号码),其他字符串为固话，手机都可以，
* return 返回true表示合法，反之不合法
*/
function validatePhoneNum(str,type){
	var regExp;
	var phone = /^(0(10|2[1-3]|[3-9]\d{2}))-[1-9]\d{6,7}$/g;//固话
	var mobile = /^1\d{10}$/g;//手机
	var mix = /^(1\d{10})|((0(10|2[1-3]|[3-9]\d{2}))-[1-9]\d{6,7})$/g;//都可以
	if(type === "1"){
		regExp = mobile;
	}else if(type === "2"){
		regExp = phone;
	}else{
		regExp = mix;
	}
	return regExp.test(str);
}

//开售提醒
function addMesRemind(pid){
	var remindMobile=jQuery("#remindMobile");
	if(remindMobile.val()== ""){
		alert("手机号不能为空");
		remindMobile.focus();
		return false;
	}else if(!validatePhoneNum(remindMobile.val(),"1")){
		alert("手机格式不合法");
		remindMobile.focus();
		return false;
	}
	jQuery("#remindMes").hide();
	jQuery.ajax({
		type: "post", 
		url: "/appController/isLogin",
		async: true,
		success: function(data){
			var nextPage = "/product/detail-"+pid+".html";
			if(data == "true"){
				jQuery.ajax({
					type: "post",
					async: false,
					url: "/appController/addMesRemind",
					data: "pid="+pid+"&remindMobile="+remindMobile.val(),
					success: function(data){
						if ("Y" == data) {
							jQuery("#remindMes").show();
						} else if ("N" == data) {
							alert("您已经订阅过短信提醒，请勿重复提交");
						}else{
							alert("开售提醒订阅失败，请稍后再试！");
						}
					}
				});
			}else{
				loginToNext(nextPage);
			}
		}
	});  
}

function setTab03Syn ( i ){
	selectTab03Syn(i);
}

function selectTab03Syn ( i ){
	switch(i){
		case 1:
			document.getElementById("TabTab03Con1").style.display="block";
			document.getElementById("TabTab03Con2").style.display="none";
			break;
		case 2:
			document.getElementById("TabTab03Con1").style.display="none";
			document.getElementById("TabTab03Con2").style.display="block";			
	}
}

function changeImage(obj,obj2){
	jQuery(".jqzoom").find("img").attr("jqimg",obj);
	jQuery(".jqzoom").find("img").attr("src",obj2);
}
function continueBuy(){
	//alert("跳转页面："+sourceUrl);
	delSource();
	if(sourceUrl != null && sourceUrl != 'null' && sourceUrl != undefined && sourceUrl != ''){
	 if(sourceUrl == 'carvip'){
	 window.location.href="/carvip/carvip.html";
	 }else{
	 window.location.href= "/carvip/all_list"+sourceUrl+".html";
	 }
	}else{
	 window.location.href="/catalog/all_catalog.html";
	}
}
//删除来源分类url
function delSource(){
	jQuery.ajax({
	   type: "post",
       url: "/appController/delSourceUrl",
       success: function(data){
      	}
      	});
}
jQuery(document).ready(function(){
    //异步加载销量
	jQuery.ajax({
	type: "post",
	data: "pid=31111",
	async: false,
	url: "/appController/selectSalesVolume",
	success: function(data){
				if(null!=data){
				  jQuery("#salesVolume").html("销 量："+data);
				}
	       }
	});
	
	//加载历史记录	
	var page = "/history.html";
	jQuery.post(page,{},function(data){
		jQuery("#history").html(data);
	});
	
    jQuery(".jqzoom").jqueryzoom({
        xzoom:310,  // 放大图的宽
        yzoom:310,  // 放大图的高
        offset:10,   // 放大图距离原图的位置
        position:'right'  // 放大图在原图的右边(默认为right)
    });
    
    //根据库存异步加载购买按钮
	jQuery.ajax({
		type: "post",
		data: "pid=31111",
		async: false,
		url: "/appController/selectProductStorage",
		success: function(data){
			var numdd = jQuery("#prodNum");
			if(data <= 0){
				if(numdd){
					numdd.text("");
					numdd.append("<strong>库存不足</strong>");
				}
				if("" == "jd" || "" == "oil" ||"" == "zc"){
					jQuery("#numBuy").show();
					jQuery("#nowBuy").show();
					jQuery("#addShopCart").show();
				}
			}else{
				if(numdd){
					numdd.text("");
					numdd.append( data + " <input type='hidden' id='storage' value='" + data + "'/>");
				}
				jQuery("#nowBuy").show();
								jQuery("#numBuy").show();
				jQuery("#addShopCart").show();
							}
		}
	});
	
		//获取来源分类url
	jQuery.ajax({
	   type: "post",
       url: "/appController/getSourceUrl",
       success: function(data){
       		//alert(data);
       		if(data != null && data != "" && data != undefined){
    			//$("#contChange").attr("href",data);
    			sourceUrl = data;
    		}
      	}
      	});
});

