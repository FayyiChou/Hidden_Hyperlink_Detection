function zqjsetTransition(){ 
if (document.all){ 
bannerjrrotator.filters.revealTrans.Transition=Math.floor(Math.random()*23); 
bannerjrrotator.filters.revealTrans.apply(); 
} 
} 
function zqjplayTransition(){ 
if (document.all) 
bannerjrrotator.filters.revealTrans.play() 
} 
function nextjr(){ 
if(jrNum<bannerjr.length-1)jrNum++ ; 
else jrNum=0; 
zqjsetTransition(); 
document.images.bannerjrrotator.src=bannerjr[jrNum]; 
zqjplayTransition(); 
theTimer=setTimeout("nextjr()", 4000); 
} 
function zqjjump2url(){ 
jumpUrl=bannerjrlink[jrNum]; 
jumpTarget='_blank'; 
if (jumpUrl != ''){ 
if (jumpTarget != '')window.open(jumpUrl,jumpTarget); 
else location.href=jumpUrl; 
} 
} 
function zqjdisplayStatusMsg() { 
status=bannerjrlink[jrNum]; 
document.returnValue = true; 
} 
function closeDiv(id)
{
   var o = document.getElementById(id);
   o.style.display = "none";
}

