<script language=javascript>
var float_xPos = 0;
var float_yPos = 0;
var float_step = 1;
var float_delay = 30; 
var float_height = 0;
var float_Hoffset = 0;
var float_Woffset = 0;
var float_yon = 0;
var float_xon = 0;
var float_pause = true;
var float_interval;
var float_move = true;
document.getElementById("float_img").style.top = float_yPos;
function float_changePos() {
	if (float_move){
		float_width = document.body.clientWidth;
		float_height = document.body.clientHeight;
		float_Hoffset = document.getElementById("float_img").offsetHeight;
		float_Woffset = document.getElementById("float_img").offsetWidth;
		document.getElementById("float_img").style.left = float_xPos + document.body.scrollLeft+"px";
		document.getElementById("float_img").style.top = float_yPos + document.body.scrollTop+"px";
		if (float_yon) {
		float_yPos = float_yPos + float_step;
		}
		else {
		float_yPos = float_yPos - float_step;
		}
		if (float_yPos < 0) {
		float_yon = 1;
		float_yPos = 0;
		}
		if (float_yPos >= (float_height - float_Hoffset)) {
		float_yon = 0;
		float_yPos = (float_height - float_Hoffset);
		}
		if (float_xon) {
		float_xPos = float_xPos + float_step;
		}
		else {
		float_xPos = float_xPos - float_step;
		}
		if (float_xPos < 0) {
		float_xon = 1;
		float_xPos = 0;
		}
		if (float_xPos >= (float_width - float_Woffset)) {
		float_xon = 0;
		float_xPos = (float_width - float_Woffset);
		  }
	}
}
function float_start() {
  document.getElementById("float_img").visibility = "visible";
  //changePos();
  float_interval = setInterval('float_changePos()', float_delay);
}
float_start();
function div_over(){
	float_move=false;
}
function div_out(){
	float_move=true;
}
function move_close_click()
{
	float_move=false;
	document.getElementById("float_img").style.display = "none";
}
</script> 

