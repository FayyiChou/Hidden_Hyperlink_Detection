<script type="text/javascript">
window.paging_options = {
    "item_selector" : ".comment-item" , 
    "container" : $('#comment-list-box'),
    "count"     : ('37'>>0),
    "total"     : (Math.ceil(('37'>>0)/(('5'>>0)||20)) || 1),
    "data_url"  : "/recipe/comment/comment.php?do=list&type=recipe&itemid=257390&ajax=1&limit=5&p=(*)",
    "callback"  : function(){
        $('#comment-list-box .comment_count').html(window.paging_options.count);
    }
};
window.comment_options = {
    "channel" : 'recipe',
    "item"    : '257390',
    "auto_load" : false};
$(function(){
	(function($, hd){
	    var elements = {
	        "comment_box"  : $('#comment-list-box'),
	        "comment_input_text" : $('#comment-input'),
	        "comment_bt_post"    : $('#comment-send'),
	        "comment_smile"      : $('.smile')
	    };
	    var action_url = 'http://www.haodou.com/comment.php';
	    var comment_widget  = new CommentWidget(elements.comment_box);
	    
	    /** 回复点击方法 */
	    var reply_comment = function(elem){
	        var cid = $(elem).attr('data-cid');
	        var username = $(elem).attr('data-username');
	        comment_options.reply_id = cid;
	        $(elements.comment_input_text).val('回复' + username + ':');
	        $(elements.comment_input_text).get(0).focus();
	    };
	    
	    /** 载入数据 */
	    var load_last_comments = function(options, direction){
	        $.get(window.paging_options.data_url.replace('(*)',1), function(html){
                 var obj = $(html);
                 var item_selector = window.paging_options.item_selector;
                 obj.filter(item_selector).each(function(){
                     var item_key = $(this).attr('data-item-key');
                     if ($(item_selector).filter('[data-item-key="' + item_key + '"]').size() > 0)
                     {
                         obj.filter('[data-item-key="' + item_key + '"]').hide();
                         obj.filter('[data-item-key="' + item_key + '"]').addClass('remove');
                     }
                 });
                 $(item_selector).first().before(obj);
                 $(item_selector).filter('.remove').remove();
                 $('#comment-list-box .comment_count').html(window.paging_options.count);
                 
                 // window.location.href = '#comments';
	        });
	    };
	    
	    /** 提交数据 */
	    var post_coment = function(data)
	    {
			if (window.paging_options.posting){
			    return false;
			}
			window.paging_options.posting = true;
			comment_widget.post(data, function(json){
				if (json.status == '200') {
				    $(elements.comment_input_text).val('');
				    comment_options.reply_id = 0;
				    load_last_comments();
				    $('.comment_none').hide();
				    popup.alert("评论发布成功！");
				}
				else
				{
				    var msg = json.message || '评论发表失败, 请稍后再试!';
				    popup.alert(msg);
				}
				window.paging_options.posting = false;
			});  
	    };
	    
	    /** 点击回复 */
	    $('.comment-item .bt_reply').live('click',function(){
	        reply_comment(this);
	    });
	    
	    /** 评论输入框 */
	    $(elements.comment_input_text).focus(function(){
	        var content = $(this).val();
	        if (content == '说点什么')
	        {
	            $(this).val('');
	        }
			// js 判断登陆，兼容app的 webview 开始
			var uid = /[__]?uid=(\d+)/.exec(document.cookie);
			if (!uid || !uid[1]) {
				location.href = "http://m.haodou.com/recipe/user.php?do=login";
				return false;
			}
	    });
	    $(elements.comment_input_text).change(function(){
	        var content = $(this).val(); 
	        if (content == '说点什么' || !/\S/.test(content))
	        {
	            $(elements.comment_bt_post).removeClass('green');
	        }else{
	            $(elements.comment_bt_post).addClass('green');
	        }
	    });
	    $(elements.comment_input_text).keyup(function(){ $(this).change(); });
	    $(elements.comment_input_text).keydown(function(event){
	        var e = event || window.event || arguments.callee.caller.arguments[0];
	        if (e.keyCode == 13) $(elements.comment_bt_post).click();
	    });
	    
	    /** 点击发送按钮 */
	    $(elements.comment_bt_post).click(function(){
	        var content = $(elements.comment_input_text).val().replace(/(^\s+)|(\s+$)/g,'');
	        if (content == '' || content == '说点什么吧！')
	        {
	            return false;
	        }
	        var data = {
	            "channel" : comment_options.channel,
	            "item"    : comment_options.item,
	            "content" : content,
	            "reply_id" : (
	                (/^回复/.test(content) && comment_options.reply_id)
	                    ? comment_options.reply_id
	                    : 0
	            )
	        };
	        if (data.reply_id)
	        {
	            data.content = data.content.replace(/^回复[^:]+?:/g,'');
	        }
	        post_coment(data);
	    });
	    
	    /** 自动加载 */
	    if (comment_options.auto_load)
	    {
	        window.paging_options.current = 0;
	        $('.paging_more').click();
	        comment_options.auto_load = false;
	    }
	    
	})(jQuery, hd);
});
</script>
