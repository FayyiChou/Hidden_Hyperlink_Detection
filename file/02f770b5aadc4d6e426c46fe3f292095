		function setTab(a){
			for(var i=1;i<=2;i++){
				var oDiv = document.getElementById("main"+i);
				var oA = document.getElementById("a"+i);
				oDiv.style.display="none";
				oA.className = "nochosen";
			}
			var curDiv = document.getElementById("main"+a);
			curDiv.style.display = "block";	
			var curA = document.getElementById("a"+a);
			curA.className = "chosen";
		}
		</script>
