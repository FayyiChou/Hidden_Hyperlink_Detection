    var utp = 0;
    function oAuthUrl(tp) {
        var fromTp = $(".zc-login").attr("rel");
        debugger;
        var addutp = "";
        
        if (utp > 0) {
            switch(utp) {
                case 9:
                addutp = "&UserTp=0";
                break;
            case 10:
                addutp = "&UserTp=1";
                break;
            }
        }
        if (tp=="qq") {
            window.location.href = ("http://p.vobao.com/oAuth/QQLogin?FormTp=" + fromTp + "&goUrl=" + window.location.href + addutp);
        } else if (tp=="sina") {
            window.location.href = ("http://p.vobao.com/oAuth/SinaLogin?FormTp=" + fromTp + "&goUrl=" + window.location.href + addutp);
        } else {
            
        }
    }
