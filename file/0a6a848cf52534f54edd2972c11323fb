                $(function () {
                    $("#asksubmit").click(function () {
                        if ($("input[name=askxingbie]:checked").val() == undefined) {
                            alert("请选择性别");
                            return false;
                        }
                        else {
                            $("input[name=sex]").val($("input[name=askxingbie]:checked").val());
                        }

                        if ($("input[name=age]").val() == "" || isNaN($("input[name=age]").val())) {
                            alert("请填写正确的年龄");
                            return false;
                        }
                        if (parseInt($("input[name=age]").val()) < 0 || parseInt($("input[name=age]").val()) > 100) {
                            alert("岁数限制0-100岁");
                            return false;
                        }

                        if ($.trim($("textarea[name=MainBody]").val()) == "用来治疗什么病，是否有使用其他药品，你对该药有什么疑惑？" || $.trim($("textarea[name=MainBody]").val()).length < 10) {
                            alert("请填写提问内容，长度不小于10个字");
                            return false;
                        }
                        //                            if ($("input[name=askcheckcode]").val().length != 5) {
                        //                                alert("请正确填写验证码");
                        //                                return false;
                        //                            }

                        var usId = $.cookie("pid");
                        var verify = $.cookie("verify");

                        if (usId != undefined) {
                            $("input[name='Pid']").val(usId);
                            $("input[name='Verify']").val(verify);
                        }
                        else {
                            alert("请先登陆！");
                            return false;
                        }

                        $.ajax({
                            type: "POST",
                            url: "/CommentHandle.ashx",
                            data: "act=askadd&" + $("#dpzx_my_a form").serialize(),
                            contentType: "application/x-www-form-urlencoded; charset=utf-8",
                            dataType: 'json',
                            success: function (returnData) {
                                $("body").removeData("mark");
                                if (returnData.status == false) {
                                    alert(returnData.msg);
                                }
                                else {
                                    alert("提问提交成功！");
                                    document.location.href = document.location.href;
                                }

                            },
                            beforeSend: function () {
                                $("#asksubmit").hide();
                                $("#pj_tj_a").append("<label>留言正在提交，请稍候...</label>");
                            }
                        });


                    });
                });
                
