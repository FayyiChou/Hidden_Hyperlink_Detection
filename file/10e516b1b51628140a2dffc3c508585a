		window.addEvent('load', function() {
		var url = '/component/mijosearch/?task=complete&format=raw';
		var completer = new Autocompleter.Ajax.Json($('qr-147'), url, {'postVar': 'q'});
	});
		
	function mijosearchsubmit(){
		var moquery = document.getElementById("qr-147").value.length;
		var text = document.getElementById("qr-147").value;
		var f = "Search this site";
		
		if(text.length < 3 || text == f ){
			return false;
		}
		
		if (moquery >= "3"  ) {
			return true;
		} 
		else {
			alert("Please, make sure that your query length is higher than 3 characters.");
			return false;
		}
	}

            jQuery(document).ready(function() {
            jQuery('#qr-147').keyup(function() {
                ajaxSearch();
            });

            jQuery('#ext').change(function() {
                ajaxSearch();
            });

            jQuery('#order').change(function() {
                ajaxSearch();
            });
        });


        function ajaxSearch(){
			if(jQuery.support.leadingWhitespace == false) { 
				return false;
			}
			
            var text = document.getElementById('qr-147').value;
            if(text.length < 3 || text == 'search...' ){
                return;
            }

            var postdata = jQuery('#mijosearchModule-147').serialize();
            postdata = postdata.replace('&view=search&task=search', '');

            jQuery.ajax({
                type: "GET",
                url: "http://www.yourwardrobeunlockd.com/index.php?option=com_mijosearch&task=ajaxsearch",
                data: postdata,
                beforeSend:function(){
                    jQuery('.mijosearch_input_module_simple').addClass('mijosearch_module_loading');
                    jQuery('.mijosearch_input_module_simple').removeClass('mijosearch_input_module_simple');
                },
                success:function(data){
                    document.getElementById('mijoajax-147').innerHTML = data;
                    jQuery('.mijosearch_module_loading').addClass('mijosearch_input_module_simple');
                    jQuery('.mijosearch_module_loading').removeClass('mijosearch_module_loading');
                }
            });
        }

        function searchSubmit(){
            var form = document.getElementById("mijosearchModule-147");
			var moquery = document.getElementById("qr-147").value.length;

			if (moquery >= 3  ) {
						form.submit();
			} 
			else {
				alert("Please, make sure that your query length is higher than 3 characters.");
				return false;
			}
        }

        		
    </script>

<form id="mijosearchModule-147" action="/component/mijosearch/search" method="post" name="mijosearchModule" onsubmit="return mijosearchsubmit();">
