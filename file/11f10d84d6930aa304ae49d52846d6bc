                                if (res != null) {
                                    if (res.StepOne == "True" && res.StepTwo == "True") {
                                        //已完成  设置cookies 更改完成状态相关样式
                                        var cookName = getCookie("UserID") + "_2016fns1";
                                        var cookName1 = getCookie("UserID") + "_2016fns2";
                                        setCookie(cookName, 1, 365);
                                        setCookie(cookName1, 1, 365);
                                        $("#work2").html("<img src='http://file.safetree.com.cn/special/2016Drowning/Styles/images/2016Drowning/nav_wc.png' />");
                                    }
                                }
                            });
                        }
                    }
                } else {
                    setTab('setnav', 2, 3);
                    finishStatus(0);
                }
                weixin(comefrom, function (a, b, c) {
                    comefrom = a, cityname = b, ipArea = c;
                    $("#weixin_cityname").append("教育百师通" + cityname);
                    if (comefrom >= 0) {
                        $("#wxbox-img1").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                        $("#wxbox-img2").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                        $("#wxbox-img4").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();")
                    }
                });
            });

            //点击完成情况按钮按钮
            $("#FinishSituation").click(function () {
                if (region > 0) {
                    window.location.href = "/2016Drowning_Execution_school.html";
                } else {
                    window.location.href = "/2016Drowning_Execution_family.html";
                }
            });

        });

        //签名
        //isFinish  =>0,未完成    1,已完成
        function tijiao(isFinish) {

            //判断用户是否登录 isLogin=>0:未登录;1:已登入
            if (isLogin == 0) {
                //弹登录框 提示登录
                special.showLogin();
            }
            else {  // 校级、教师
                if (region == 1 || region == 7) {
                    //获取完成状态 
                    try {
                        if (isshiyanqu == 1 && region != 7) {
                            if (comefrom == 20096) {
                                if (videograde == 1 && !(grade == 0 || grade == 13 || grade == 14)) {
                                    alert("请观看您所在年段的视频后再签名确认。");
                                    return;
                                }
                                else if (videograde == 2 && !(grade > 0 && grade < 6)) {
                                    alert("请观看您所在年段的视频后再签名确认。");
                                    return;
                                }
                                else if (videograde == 3 && !((grade > 5 && grade < 13) || grade == 15)) {
                                    alert("请观看您所在年段的视频后再签名确认。");
                                    return;
                                }
                            } else {
                                if (videograde == 1 && !(grade == 0 || grade == 13 || grade == 14)) {
                                    alert("请观看您所在年段的视频后再签名确认。");
                                    return;
                                }
                                else if (videograde == 2 && !(grade > 0 && grade < 7)) {
                                    alert("请观看您所在年段的视频后再签名确认。");
                                    return;
                                }
                                else if (videograde == 3 && !((grade > 6 && grade < 13) || grade == 15)) {
                                    alert("请观看您所在年段的视频后再签名确认。");
                                    return;
                                }
                            }
                        }
                        $("#w_finish").hide();
                        $("#w_finishing").show();
                        $("#step1").hide();
                        //实验区
                        if (isshiyanqu == 1) {
                            //平台写入数据
                            SpecialSign(function (res2) {
                                //写入成功  设置cookies
                                if (res2 != null) {
                                    if (res2.Status == "True") {
                                        var cookName = getCookie("UserID") + "_2016fns";
                                        setCookie(cookName, 1, 365);
                                        //调用主库
                                        $.getJSON(Asmx + "/SpecialSign?jsoncallback=?", { r: Math.random(), Prv: prvCode, City: cityId, County: countryId, School: schoolid, Grade: grade, Class: classroom, UserType: region, Comefrom: comefrom }, function (res3) {
                                            if (res3 != null) {
                                                if (res3.Status == "True") {
                                                    //设cookies 
                                                    var zkcookName = getCookie("UserID") + "_zk2016fns";
                                                    setCookie(zkcookName, 1, 365);
                                                    finishStatus(1);

                                                } else if (res3.Msg.length > 0) {
                                                    alert(res3.Msg);
                                                    finishStatus(0);
                                                }
                                            }
                                        });
                                    }
                                    else if (res2.Msg.length > 0) {
                                        alert(res2.Msg);
                                        finishStatus(0);
                                    }
                                }
                                else {
                                    finishStatus(0);
                                }
                            });
                        }
                        //非实验区
                        else {
                            $("#w_finish").hide();
                            $("#w_finishing").show();
                            $("#step1").hide();
                            $.getJSON(Asmx + "/SpecialSign?jsoncallback=?", { r: Math.random(), Prv: prvCode, City: cityId, County: countryId, School: schoolid, Grade: grade, Class: classroom, UserType: region, Comefrom: comefrom }, function (res4) {
                                if (res4 != null && res4.Status == "True") {
                                    //设置Cookie 
                                    var zkcookName = getCookie("UserID") + "_zk2016fns";
                                    var cookName = getCookie("UserID") + "_2016fns";
                                    setCookie(zkcookName, 1, 365);
                                    setCookie(cookName, 1, 365);
                                    finishStatus(1);
                                }
                                else if (res4.Msg.length > 0) {
                                    alert(res4.Msg);
                                    finishStatus(0);
                                }
                            });
                        }
                    }
                    catch (e) {
                        finishStatus(0);
                        alert("网络异常！请稍后尝试");
                    }
                }
                //非教师、校级
                else {
                    //显示无权限
                    alert("无法进行操作，您没有此权限");
                }
            }
        }

        function finishStatus(qmStatus) {//0:未完成, -1：完成中, 1:已完成
            if (qmStatus == 0) {
                $("#w_finish").show();
                $("#w_finishing").hide();
                $("#step1").show();
            }
            else if (qmStatus == -1) {
                $("#w_finish").hide();
                $("#w_finishing").show();
                $("#step1").hide();
            } else {
                $("#w_finishing").hide();
                $("#w_finish").html("已完成").show();
                $("#step1").show();
                $("#step1").attr("class", "y-wc");
                $("#work1").html("<img src='http://file.safetree.com.cn/special/2016Drowning/Styles/images/2016Drowning/nav_wc.png' />");
            }
        }

