jQuery(function($) {

	$(window).load(function() {
        $('#wrapper').slick({
            centerMode: true,
            centerPadding: '0',
            autoplay: true,
            dots: true,
            arrows: false,
            variableWidth : true
        });
    });

    /*
     info - ссылка на id placeholderа, туда будет падать HTML с сервера
     itab - объект который всё делает,
     itab.config - указание серверу, какой конфиг подключать на этой странице
     itab.update('itab') - запускает процесс обновления информера,
     в параметре имя переменной объекта, который всё делает.
     */
    var itab=null;

    function initInfoTable(){
        itab=new InfoTable('info');
        itab.client="kube";
        itab.config='htable1';
        itab.update('itab');
    }

    setTimeout(initInfoTable,2000);

	
});
