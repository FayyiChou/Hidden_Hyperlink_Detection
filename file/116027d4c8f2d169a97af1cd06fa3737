					var newDateValue = Date.parse(newDate);
					var endDate1= endDate.replace(/-/g, "/");
					var endDateValue =  Date.parse(endDate1);
					if(endDateValue - newDateValue < 0){
						var infoMakerValue = document.getElementById("38_infoMaker");
