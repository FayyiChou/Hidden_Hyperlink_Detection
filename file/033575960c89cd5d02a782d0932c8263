function MakeShop_entryMailMagazine(formElement, mailElement, isInternalForm) {
    if(!mailElement.value.match(/@([A-Za-z0-9-]+\.)+[A-Za-z0-9]{2,}$/)){
        alert('メールアドレスを正しく入力して下さい。');
        mailElement.focus();
        return;
    }
    
    if (!isInternalForm) {
        formElement.email1.value = mailElement.value;
        formElement.valid_htmlmail.value = (document.getElementById('left_validhtmlmail').checked) ? 'Y' : 'N';
    }
    
    formElement.submit();
}
</script>
