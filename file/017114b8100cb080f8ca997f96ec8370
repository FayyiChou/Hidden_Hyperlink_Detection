$("#quick_reply_captcha .close-tc").click(function(){
	$("#qr_captcha_code").val("");
	$("#quick_reply_captcha").hide();
});
//显示弹窗验证码
var show_qr_captcha = '';
function showPopCaptcha(post_id){
	if(!$("#quick_reply_captcha").is(":visible")){
		$("#quick_reply_captcha").show();
		$("#qr_captcha_code").val("");
	}
	
	if(show_qr_captcha != post_id){
		refreshQuickRplyCaptcha();
	}
	show_qr_captcha = post_id;
	
	//绑定确定事件
	$("#quick_reply_captcha a.icon_qd").unbind( "click" );
	if(post_id == 'bottom'){
		$("#quick_reply_captcha a.icon_qd").bind("click", function(){
			submitCheck();
		});
	}else{
		$("#quick_reply_captcha a.icon_qd").bind("click", function(){
			quickReplySubmitCheck();
		});
	}

	return true;
}
//快速回复提交检测
function quickReplySubmitCheck(post_id){
	var flag = true;
	var isreplycheck =  false ;
	post_id = post_id === undefined ? show_qr_captcha : post_id;
	
	if(post_id === undefined || post_id.length <= 0) {
		alert("参数错误");
		return false;
	}
	
	//验证是否重复提交
	if($("#qr_"+post_id).data("disable")){
		return false;
	}
	
	//验证输入
	var ipt = "#qr_"+post_id+" input.text";
	if($(ipt).val() == ""){
		alert("请先输入回复内容");
		return false;
	}
	
	//是否需要验证码
	if (isreplycheck) {
		//是否已显示验证码
		if(!$("#quick_reply_captcha").is(":visible") || show_qr_captcha != post_id){
			showPopCaptcha(post_id);
			return false;
		}
		var code = $("#qr_captcha_code").val();
		$("#qr_" + post_id + " input.qr_ipt_captcha_code").val(code);
		
		//检查验证码是否正确
		flag = checkQuickRplyCaptcha();
	}
	
	if(flag){
		$("#qr_"+post_id).data("disable", true);
		//提交表单
		$("#qr_"+post_id).submit();
	}
	return false;
}
//检查验证码
function checkQuickRplyCaptcha(){
	//检查验证码
	var code = $("#qr_captcha_code").val();
	if(code == ""){
		alert("请输入验证码");
		$("#qr_captcha_code").focus();
		return false;
	}
	if(code.length != 4){
		alert("验证码错误");
		$("#qr_captcha_code").focus();
		return false;
	}
	
	var quickReplyFlag = false;
	$.ajax({
		url: '/checknum',
		type: "post",
		async:false,
		dataType: "json",	  
		data: 'captcha_code='+$("#qr_captcha_code").val(),
		success: function(html){
			if(html.success == '1'){
				alert("验证码错误！");
				refreshQuickRplyCaptcha();
				$("#qr_captcha_code").focus();
				quickReplyFlag = false;
			}else{
				quickReplyFlag = true;
			}
		}
	});
	return quickReplyFlag;
}
//刷新快速回复验证码
function refreshQuickRplyCaptcha(){
	$("#qr_captcha_img").attr("src","/captcha?"+Math.random());
}
</script>

