{
	window.location.href='/board/73-344.html';
}
function openPreview(){
	document.preview.post_body.value=document.getElementById("e_textarea").value;
	document.preview.ubb.value=document.post_form.ubb.checked?"on":"off";
	document.preview.html.value=document.post_form.html.checked?"on":"off";
	document.preview.submit();
}
function submitCheck()
{
	var flag = true;
	autoSave();
	if (1) {
		login_tip();
		return false;
	}
	
	//验证是否重复提交
	if($("#post_form").data("disable")){
		return false;
	}
	
	//var subject = document.getElementById("post_subject");
	if(wysiwyg){
	document.getElementById("e_textarea").value = html2bbcode(getEditorContents());
	}
	var content = document.getElementById("e_textarea");

	//if(subject.value.length<3)
	//{
		//alert('帖子标题不能小于3个字符!');
		//subject.focus();
		//return false;
	//}
	if(content.value.length<3)
	{
		alert("帖子内容不能小于3个字符!");
		//content.focus();
		return false;
	}
		
	if(flag){
		$("#post_form").data("disable", true);
		$("#post_form").submit();
	}
	
	return false;
}			
