function itemsCheck(){
	var op_items = '';
  var i = 0;
  var has_post = false;
  $('#checked_items').val('');
  $('input[name="op_item"]:checked').each(function(){
    if(i>0)
        op_items = op_items+','+$(this).val();
    else
      op_items = $(this).val();
    if($(this).val()!=$('#thread_id').val())
      has_post = true;
    i++;
  });
  if(has_post){
    $('#zt_li1').hide();
    $('#zt_li2').hide();
    $('#zt_li3').hide();
  }else{
    $('#zt_li1').show();
    $('#zt_li2').show();
    $('#zt_li3').show();
  }
    
  if(op_items){
    $('#checked_items').val(op_items);
  }else{
    $('#checked_items').val('');
  }
}
$(function() { 
    var myDate = new Date();
    myDate.setDate(myDate.getDate()+1);

    $('#top_end_date').val(myDate.getFullYear()+"-"+(myDate.getMonth()+1<10 ? '0'+(myDate.getMonth()+1) : myDate.getMonth()+1)+"-"+(myDate.getDate()<10 ? '0'+myDate.getDate() : myDate.getDate())+" "+(myDate.getHours()<10 ? '0'+myDate.getHours() : myDate.getHours())+":"+(myDate.getMinutes()<10 ? '0'+myDate.getMinutes() : myDate.getMinutes()));
    
    $('#top_end_date').datetimepicker({minDate: new Date(), defaultDate: myDate});    
}); 

