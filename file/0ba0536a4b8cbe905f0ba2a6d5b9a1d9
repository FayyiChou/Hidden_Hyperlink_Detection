<script>
	var speed1=20;
	var GDa=document.getElementById('dem'); 
	var GDa1=document.getElementById('dem1');
	var GDa2=document.getElementById('dem2');
	GDa2.innerHTML=GDa1.innerHTML;
	function Marquee1(){
		if(GDa2.offsetWidth-GDa.scrollLeft<=0){
			GDa.scrollLeft-=GDa1.offsetWidth;
		}else{
			GDa.scrollLeft++;
		}
	}
	var MyMar1=setInterval(Marquee1,speed1);
	GDa.onmouseover=function() {clearInterval(MyMar1);}
	GDa.onmouseout=function() {MyMar1=setInterval(Marquee1,speed1);}
</script>

</div>
</div>

