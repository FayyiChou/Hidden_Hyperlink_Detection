<script language="javascript">
function doCheck() {
  var f = document.courseForm;
  if (f.keywords.value=='Enter Course Name...'||f.keywords.value=='') {
    f.keywords.value = '';
	f.keywords.focus();
	return false;
  }
}
</script> 

    
