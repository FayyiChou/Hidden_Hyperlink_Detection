<script language="javascript">
function doLogin() {
  var f = document.loginForm;
  if (f.username.value=="") {
    f.username.focus();
  }
  else if (f.password.value=="") {
    f.password.focus();
  }
  else {
    f.submit();
  }
}

function closeWin() {
  var cw = document.getElementById("tempWin");
  cw.style.display="none";
}

function getPwdTip() {
  alert("Please email us if you fogot your password. Attached your detailed register infomation like first name, surname, mobile and birthday.");
}
</script>
