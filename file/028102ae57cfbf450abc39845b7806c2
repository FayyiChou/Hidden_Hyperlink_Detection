<script>
	String.prototype.Trim = function() { 
		return this.replace(/(^\s*)|(\s*$)/g, ""); 
	}  
		
	function carSearch(){
		var carSearchForm = document.getElementById("carSelectForm");
		var brand = carSearchForm.brand.value; 
		var keyword = carSearchForm.car_key.value.Trim();  		
		var url = "http://m.pclady.com.cn/search/g/";
		if(keyword != "" && keyword.length > 0){
			url = url + keyword + "/";
		}
		if(brand != "" && brand.length > 0){
			url = url +"?t=" + brand;
		} 

		carSearchForm.action=url;
		
		carSearchForm.submit();
	}
</script>

