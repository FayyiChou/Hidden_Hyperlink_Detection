if (np == 1) {
	new popBox({
    	boxClass: 'popBox success',
		content: '发布搭配成功',
		closeDelay: 1000
	});
}

function changeCaptcha(){
	document.getElementById("valid_code").src = "http://captcha.pclady.com.cn/captcha/v.jpg?"+new Date().getTime();
}


function vote(pid){
	$.post("http://m.pclady.com.cn/activity/action/vote_photo.jsp?id="+pid, function(data) {
	  	if(data.indexOf("1")!= -1){
		  	var voteNum = parseInt(document.getElementById("votes_"+pid).value);
	  		document.getElementById("votes_"+pid).value = voteNum+1;
	  		alert("投票成功");
	  		window.location.reload();
	  	}else{
	  		alert("亲，五分钟之后，再来投票哟!");
	  	}
	});
}
