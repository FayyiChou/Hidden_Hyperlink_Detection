<script languege="Javascript">
function checkUserForm()
{
	if (document.UserForm.UserName.value=='')
		{
			alert("请您输入帐号！");
			document.UserForm.UserName.focus();
			return false;
		}
	if (document.UserForm.UserPsw.value=='')
		{
			alert("请您输入密码！");
			document.UserForm.UserPsw.focus();
			return false;
		}
	return true;
}
function opencat(cat,img)
{
	if(cat.style.display=="none")
		{
			cat.style.display="";
			img.src="images/Buttom_GB.jpg";
		}	
	else 
		{
			cat.style.display="none"; 
			img.src="images/Buttom_zK.jpg";
		}
}
</Script>

