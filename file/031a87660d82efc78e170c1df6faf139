        	<!-- 系统消息代码start -->
	<script type="text/javascript" id="cloud" src="http://cloud.u-sooc.com/js.php?m=home&amp;c=js&amp;a=message&amp;host=http://ytwldx.soocedu.com&amp;from=sooc&amp;type=0&amp;cloud=http://cloud.u-sooc.com&amp;uid=&amp;questype=2&amp;identitytype="></script><div class="modal fade" id="cloud_tip_modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog cloud_modal-dialog">
        <div class="modal-content cloud_modal-content">
            <div class="modal-header cloud_modal-header">
                <a aria-hidden="true" data-dismiss="modal" class="close cloud_close"><i class="icons cloud_icons cloud_icon-close-white"></i></a>
                <h4 class="modal-title" id="myModalLabel" style="font-family: '微软雅黑';">系统消息</h4>
            </div>
            <div class="modal-body cloud_modal-body">
                <div style="padding: 15px;background-color: #ffffff;">暂无最新消息！</div>            </div>
        </div>
    </div>
</div>
<script type="text/javascript">

    define("cloud_message",function(require,exports,module) {
        var $ = require('jquery');
        var base = require('base');
        require('box');

        //引入bootstrap
        base.getScript(['lib/bootstrap3/bootstrap.min',seajs.root + '/public/default/js/common'],{
            jquery:true
        });

        //调用云端css
        var head = document.getElementsByTagName('head').item(0);
        var cloudcss = document.createElement("link");
        cloudcss.rel = "stylesheet";
        cloudcss.type = "text/css";
        cloudcss.href = 'http://cloud.u-sooc.com' + '/Public/addons/notice/css/cloud.css?v=1.6.2';
        head.appendChild(cloudcss);
        var msg = eval({"status":1,"data":{"notice":null,"questionnaire":null}});
        var json_cloud = msg.data;
        if (msg.status == '1') {
            var json_cloud = msg.data;
            var html = '<ul>';
            var flag = false;
            $('body').append(
                    '<div class="modal fade" id="tip_notice_modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">' +
                    '<div class="modal-dialog cloud_modal-dialog"><div class="modal-content cloud_modal-content"><div class="modal-header cloud_modal-header">' +
                    '<a aria-hidden="true" data-dismiss="modal" class="close cloud_close"><i class="icons cloud_icons cloud_icon-close-white"></i></a>' +
                    '<h4 class="modal-title" id="myModalLabel" style="font-family: \'微软雅黑\';">系统消息</h4>' +
                    '</div><div class="modal-body cloud_modal-body"></div></div></div></div>');
            if (msg.data.notice && msg.data.notice.length > 0) {
                for (var i in  msg.data.notice) {
                    if (getCookie('_notice_' + msg.data.notice[i].snsid + '_' + msg.data.notice[i].id) != 1) {
                        flag = true;
                        html += '<li class="_notice_' + msg.data.notice[i].snsid + '_' + msg.data.notice[i].id + '"><a title="'+ msg.data.notice[i].title+'" class="cloud_notice" data="'+ i +'"><i class="cloud_tip_png cloud_tip_notice"></i>' + msg.data.notice[i].title + '</a></li>';
                    }
                }
            }
            if (msg.data.questionnaire && msg.data.questionnaire.length > 0) {
                for (var i in  msg.data.questionnaire) {
                    if (getCookie('_questionnaire_' + msg.data.questionnaire[i].snsid + '_' + msg.data.questionnaire[i].id) != 1) {
                        flag = true;
                        html += '<li class="_questionnaire_' + msg.data.questionnaire[i].snsid + '_' + msg.data.questionnaire[i].id + '"><a title="'+msg.data.questionnaire[i].title+'" class="cloud_questionnaire" data="'+ i +'"><i class="cloud_tip_png cloud_tip_questionnaire"></i>' + msg.data.questionnaire[i].title + '</a></li>';
                    }
                }
            }
            if (flag) {
                html += '</ul>';
                var ahref = '<a onclick="cloud_top_tip_close();" class="cloud_top_tip_close">×</a>';
                if(''){
                    html += ahref;
                }
                var divObj = document.createElement("div");
                $(divObj).attr('id', 'cloud_top_tip');
                $(divObj).css('display','none');
                divObj.innerHTML = html;
                var first = document.body.firstChild; //得到第一个元素
                document.body.insertBefore(divObj, first); //在第原来的第一个元素之前插入
                setTimeout(function(){
                    $('body').css('margin-top', '40px');
                    $(divObj).css('display','block');
                    setInterval(function () {
                        cloud_scroll();
                    }, 3000);
                },1000);
            }
        }

        $('body').on('click','.cloud_top_tip_close',function(){
            $('body').animate({'margin-top': '0px'});
            setTimeout(function () {
                var lists = $('#cloud_top_tip ul li');
                for (var i = 0; i < lists.length; i++) {
                    var classname = $(lists[i]).attr('class');
                    setCookie($(lists[i]).attr('class'), 1, 15 * 24 * 60 * 60 * 1000);
                }
                $('#cloud_top_tip').hide();
            }, 1000);
        });

        this.cloud_scroll = function() {
            var height = $('#cloud_top_tip>ul>li:first-child').height();
            var len = $('#cloud_top_tip>ul>li').length;
            if (len != 1) {
                var marginTop = $('#cloud_top_tip>ul>li:first-child').css('margin-top');
                marginTop = (marginTop.replace('px', ''));
                if (-marginTop < (len - 1) * height) {
                    $('#cloud_top_tip>ul>li:first-child').animate({'margin-top': (marginTop - 40) + 'px'});
                } else {
                    $('#cloud_top_tip>ul>li:first-child').animate({'margin-top': '0px'});
                }
            }
        }

        $('body').on('click', '.cloud_notice', function () {
            var i = $(this).attr('data');
            var html = '' +
                    '<div class="cloud_modal-box modal-box">' +
                    '<div class="cloud_resource-list" style="word-wrap: break-word;">' +
                    '<div class="col-md-12">' +
                    '<div class="">' +
                    '<div class="cloud_title_mess"><b><i class="cloud_tip-list-style"></i>通知公告：</b><span style="font-size: 15px;" class="cloud_rights">' + json_cloud.notice[i].format_time + '</span><a style="font-size: 16px;" class="cloud_normal" >' + json_cloud.notice[i].title + '</a></div>' +
                    '</div>' +
                    '<hr style="margin: 0;">' +
                    '<div class="cloud_notice_content">' +
                    '<div class="cloud_tip_png cloud_notice_left"></div>' +
                    '<div class="cloud_notice_middle">' + json_cloud.notice[i].content + '</div>' +
                    '<div class="cloud_tip_png cloud_notice_right"></div>' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '</div>';
            $('#tip_notice_modal .modal-body').html(html);
            $('#tip_notice_modal').modal({
                keyboard: false
            });
        });

        $('body').on('click', '.cloud_questionnaire', function () {
            var i = $(this).attr('data');
            var html = '' +
                    '<div class="cloud_modal-box modal-box">' +
                    '<div class="cloud_resource-list" style="word-wrap: break-word;">' +
                    '<div class="col-md-12">' +
                    '<div class="">' +
                    '<div class="cloud_title_mess">' +
                    '<b><i class="cloud_tip-list-style"></i>问卷调查：</b>' +
                    '<span style="font-size: 15px;" class="cloud_rights">' + json_cloud.questionnaire[i].format_time + '</span>' +
                    '<a style="font-size: 16px;" href="'+ json_cloud.questionnaire[i].url +'" target="_blank">' + json_cloud.questionnaire[i].title + '</a>' +
                    '</div>' +
                    '</div>' +
                    '<hr style="margin: 0;">' +
                    '<div id="cloud_imgcenter">' +
                    '<div class="cloud_center">' +
                    '<img id="cloud_img" alt="" class="pngfix" src="'+ json_cloud.questionnaire[i].code_path +'" style="text-align: center;_border:none;" />' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '</div>';
            $('#tip_notice_modal .modal-body').html(html);
            $('#tip_notice_modal').modal({
                keyboard: false
            });
        });

        function cloud_showTipOfNotice(i) {
            var html = '' +
                    '<div class="cloud_modal-box modal-box">' +
                    '<div class="cloud_resource-list" style="word-wrap: break-word;">' +
                    '<div class="col-md-12">' +
                    '<div class="">' +
                    '<div class="cloud_title_mess"><b><i class="cloud_tip-list-style"></i>通知公告：</b><span style="font-size: 15px;" class="cloud_rights">' + json_cloud.notice[i].format_time + '</span><a style="font-size: 16px;" class="cloud_normal" >' + json_cloud.notice[i].title + '</a></div>' +
                    '</div>' +
                    '<hr style="margin: 0;">' +
                    '<div class="cloud_notice_content">' +
                    '<div class="cloud_tip_png cloud_notice_left"></div>' +
                    '<div class="cloud_notice_middle">' + json_cloud.notice[i].content + '</div>' +
                    '<div class="cloud_tip_png cloud_notice_right"></div>' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '</div>';
            $('#tip_notice_modal .modal-body').html(html);
            $('#tip_notice_modal').modal({
                keyboard: false
            });
        }

        function cloud_showTipOfQuestionnaire(i) {
            var html = '' +
                    '<div class="cloud_modal-box modal-box">' +
                    '<div class="cloud_resource-list" style="word-wrap: break-word;">' +
                    '<div class="col-md-12">' +
                    '<div class="">' +
                    '<div class="cloud_title_mess">' +
                    '<b><i class="cloud_tip-list-style"></i>问卷调查：</b>' +
                    '<span style="font-size: 15px;" class="cloud_rights">' + json_cloud.questionnaire[i].format_time + '</span>' +
                    '<a style="font-size: 16px;" href="'+ json_cloud.questionnaire[i].url +'" target="_blank">' + json_cloud.questionnaire[i].title + '</a>' +
                    '</div>' +
                    '</div>' +
                    '<hr style="margin: 0;">' +
                    '<div id="cloud_imgcenter">' +
                    '<div class="cloud_center">' +
                    '<img id="cloud_img" alt="" class="pngfix" src="'+ json_cloud.questionnaire[i].code_path +'" style="text-align: center;_border:none;" />' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '</div>';
            $('#tip_notice_modal .modal-body').html(html);
            $('#tip_notice_modal').modal({
                keyboard: false
            });
        }

        //获得coolie 的值
        function getCookie(name){
            var cookieArray=document.cookie.split("; "); //得到分割的cookie名值对
            var cookie=new Object();
            for (var i=0;i<cookieArray.length;i++){
                var arr=cookieArray[i].split("=");       //将名和值分开
                if(arr[0]==name)return unescape(arr[1]); //如果是指定的cookie，则返回它的值
            }
            return "";
        }

        //删除cookie
        function delCookie(name){
            var exp = new Date();
            exp.setTime(exp.getTime() - 1);
            var cval=getCookie(name);
            if(cval!=null) document.cookie= name + "="+cval+"; expires="+exp.toGMTString()+ "; path=/";
        }

        //两个参数，一个是cookie的名字，一个是值
        function setCookie(name,value,expires){
            var timers = 30*24*60*60*1000; //此 cookie 将被保存 30 天
            if(typeof(expires)!='undefined' && expires>0){
                timers = expires;
            }
            var exp = new Date();    //new Date("December 31, 9998");
            exp.setTime(exp.getTime() + timers);
            document.cookie = name + "="+ escape (value) + "; path=/; expires=" + exp.toGMTString();
        }

    });
    seajs.use("cloud_message");
</script>

