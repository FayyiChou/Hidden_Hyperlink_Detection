    function fakeProgress(v, el) {
    if (v >= 51)
    location.href="/8";
    else {
    setSB(v, el, infor, "欢迎光临，页面载入中，请稍等！");
    window.setTimeout("fakeProgress(" + (v + 1) + ", document.all['" + el.id + "'])", 30);
    }
