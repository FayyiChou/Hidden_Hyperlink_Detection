  require.config(map);
  var route = require.getRouteFromLocation() || '';
  var moduleId = findModuleId(route, map);
  if(moduleId){
    require.async([moduleId, 'framework/uae/wa', 'framework/uae/perf'], function(renderFn, wa, perf){
      //统计
      wa.setSiteId('0196fa7ce7dd');
      wa.setTrackerUrl('http://track.uc.cn:9080/collect?uc_param_str=dnfrcpvess');
      wa.setRedirectHost('http://track.uc.cn:9080/lct?uc_param_str=dnfrcpvess');
      window.wa = wa;

      renderFn(document.getElementById('view'));

      // 上报恶意注入的情况
      setTimeout(function() {
        if (Array.isArray(window.injectionReportQueue) && window.injectionReportQueue.length) {
          window.injectionReportQueue.forEach(function(item) {
            wa.addActionStat('carrier_injection', item.string, item.nw);
          });
        }
        perf.config({
          app: 'uae406',
          path: route.toLowerCase().replace(/^\//, '').replace(/\//g, '_'),
          percent: 100
        });

        perf.add('ws', window._headtime);
        perf.send();
      }, 1000);


    });
  }else{
    document.body.style.backgroundColor = '#fff';
    document.getElementById('loading').style.display='none';
    document.getElementById('iosLoading').style.display='none';
    document.getElementById('error').style.display = 'block';
  }

  function findModuleId(route, map){
    if(route) {
      route = route.replace(/^\//, '').replace(/\/$/, '');
      var ua = navigator.userAgent;
      var isIOS = ua && (ua.toLowerCase().indexOf("ios") > -1 || ua.indexOf("iPhone") > -1 || ua.indexOf("iPad") > -1 || ua.indexOf("iPod") > -1);
      var androidModuleId = 'modules/android/' + route;
      var iosModuleId = 'modules/ios/' + route;
      var moduleId = isIOS ? iosModuleId : androidModuleId;
      var platform= require.getURLParameters(window.location.href, 'platform');
      if(platform){
        moduleId = 'modules/' + platform+'/' + route;
      }
      if(isIOS){
        document.getElementById('iosLoading').style.display='';
      }else{
        document.getElementById('loading').style.display='';
      }
      //TODO: 目前当PC查看时, 如果一个模块同时存在android和ios, 会优先显示android. 是否需要修复?
      //根据ua找到对应的模块
      if (map.alias.hasOwnProperty(moduleId)) {
        return moduleId;
      }else if(map.alias.hasOwnProperty(androidModuleId)){
        //找不到时, 优先选android
        return androidModuleId;
      }else if(map.alias.hasOwnProperty(iosModuleId)){
        //最后找ios
        return iosModuleId;
      }
    }
  }
  // loading状态 ，当loading被元素隐藏停止loading
  (function() {
      var i=1;

      var loadState = setInterval(function(){
          if(i == 10) {
              i=1;
          }
          document.getElementById('load').className = 'loading-img load-' + i;
          i++;
          if(document.getElementById('loading').style.display == 'none') {
              clearInterval(loadState);
          }

      },100);
  })();
  document.getElementById('loading').style.display='none';

  //TODO:临时解决劫持导致页面不显示问题 和 流量球的问题
  window.addEventListener('load', function(){
    if(!window.angular){
      document.body.removeAttribute('data-ng-cloak');
    }
  });
</script>

