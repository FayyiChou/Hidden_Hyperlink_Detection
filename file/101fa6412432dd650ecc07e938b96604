//		jQuery("#sn002024").css("display","block");
//		jQuery("#sn002024").adFloat({width:98,height:155,top:0,left:0});	
//	}
	
//});


</script>

<script type="text/javascript">
	$(function(){
		var url=$("#url").val();
		//初始化登陆用户信息
		investorLogin(url);
		//加载回复的得分数
		//loadReplyScore();
	});

	$(document).ready(function(){
		initTip('popDiv');
		
		buildHQMoudle();
		
		
		var language = queryLanguageForLogin();
		if(language=="zh"){
			document.getElementById('englishlang').innerHTML = "English";
		}else if(language=="en"){
			document.getElementById('englishlang').innerHTML = "中文";
		}else{
			document.getElementById('englishlang').innerHTML = "English";
		}
	});
	
	$(function ($) {
		$('.artZoom').artZoom({
			path: 'http://irm.cninfo.com.cn/ircs/ssgs/images',	// 设置artZoom图片文件夹路径
			preload: true,		// 设置是否提前缓存视野内的大图片
			blur: true,			// 设置加载大图是否有模糊变清晰的效果
			
			// 语言设置
			left: '左旋转',		// 左旋转按钮文字
			right: '右旋转',		// 右旋转按钮文字
			source: '看原图'		// 查看原图按钮文字
		});
		
		$('#myEmail').html(1987 + '.tangbin@' + 'gmail.com');
	});

	function buildHQMoudle(){
		var company_stockCode = "000050";
        var company_shortname = '深天马Ａ';
		if(company_stockCode){
            var target_url = "http://api.cninfo.com.cn/service/timeLine";
			var quote = new PriceChart("#hq_show_area",company_shortname,company_stockCode,"sz"+company_stockCode,target_url ,{"codelist":"sz"+company_stockCode});
			quote.trigger();
		}
	}
</script>
