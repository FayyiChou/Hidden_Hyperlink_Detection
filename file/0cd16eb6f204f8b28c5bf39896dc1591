    <div id="win_login" class="winbox" style="display:none;">
        <div class="win-wrap">
            <div class="win-inner">
                <div class="win-cont">
                    <div class="erweima">
                        <img src="http://img.58cdn.com.cn/jxedt/listing/images/erweima.jpg">
                    </div>
                    <ul class="user-login">
                        <li class="title">用户登录</li>
                        <li class="user">
                            <i></i><input data-node="user" type="text" value="" placeholder="手机号/用户名">
                        </li>
                        <li class="pwd">
                            <i></i><input data-node="pwd" type="password" value="" placeholder="密码">
                        </li>
                        <li class="forgetpwd"><a href="http://user.jxedt.com/findpass">忘记密码?</a></li>
                        <li class="token">
                            <div class="code-value"><input data-node="token" type="text" value="" placeholder="验证码"></div>
                            <div class="code-pic"><img data-action="gettoken" src="http://user.jxedt.com/inc/checkcode?r=0.7212022461042582"></div>
                        </li>
                        <li class="action"><a data-action="login" href="javascript:">登 录</a></li>
                        <li class="other">
                            <div class="qq-login"><a href="javascript:qqlogin()"><img src="http://j2.58cdn.com.cn/jxedt/user/images/qq.png"></a></div>
                            <div class="registe"><a href="http://user.jxedt.com/register">注册账号</a></div>
                        </li>
                    </ul>
                </div>
                <div data-action="close" class="win-close"><i class="icon-close"></i></div>
            </div>
        </div>
        <div class="win-bg"></div>
    </div>
    <script>
        document.domain = "jxedt.com";
        var loginDom = $("#win_login");
        loginDom.find('[data-action="close"]').on("click", function(){
            loginDom.hide();
        })
        loginDom.find('[data-action="login"]').on("click", function(){
            login();
        });
        loginDom.find('[data-action="gettoken"]').on("click", function(){
            getToken(this);
        });
        function getToken($this){
            $this.src = "http://user.jxedt.com/inc/checkcode?r=" + (+new Date());
        }
        function login(){
            var user = loginDom.find('[data-node="user"]').val();
            var pwd = loginDom.find('[data-node="pwd"]').val();
            var token = loginDom.find('[data-node="token"]').val();

            if(!$.trim(user)){
                alert("请填写用户名")
                return;
            }
            if(!$.trim(pwd)){
                alert("请填写密码")
                return;
            }
            if(!$.trim(token)){
                alert("请填写验证码")
                return;
            }

            var postData = function (params) {
                var callback = "postp_" + new Date().getTime();
                var targetName = "postp_iframe";

                var iframe = window[targetName];
                if (!iframe) {
                    iframe = document.createElement("iframe");
                    iframe.style.display = "none";
                    iframe.name = iframe.id = targetName;
                    document.body.appendChild(iframe)
                }

                var form = document.getElementById("postp");
                if (!form) {
                    form = document.createElement("form");
                    form.id = "postp";
                    form.style.display = "none";
                    form.method = "post";
                    form.action = params.url;
                    form.target = targetName;
                    document.body.appendChild(form);
                }
                form.innerHTML = "";
                var data = params.data;
                var input = "", $form = $(form);
                for (var key in data) {
                    input += '<input type="hidden" name="' + key + '" value="' + data[key] + '">';
                }
                input += '<input type="hidden" name="callback" value="' + callback + '">';
                $form.append(input);
                form.submit();
                window[callback] = function (data) {
                    params.success(data);
                }
            };


            postData({
                url: "http://user.jxedt.com/ulogin",
                data: {
                    phone: user,
                    password: pwd,
                    indexccode: token
                },
                success:function(res){
                    console.log(res)
                    if(res.reType == 0){
                        loginDom.hide();
                        $(".header .not-login").hide();
                        $(".header .logged").show();
                        $(".header .logged .username").html(user + '<i class="icons"></i>');
                        if($.cookie('jxedt_uname')){
                            $('.huida_1_right').show();
                            $(".huida_1_right").text("你好，"+decodeURIComponent($.cookie('jxedt_uname')))
                        }else{
                            $('.huida_m').show();
                            $('.huida_1_right1').show();
                        }
                        submit();
                    }else{
                        alert(res.msg)
                    }
                }
            })
        }

        function ask(){
            var text = $.trim($('.huida_2').val());
            if (!text) {
                confirm("请填写问答内容")
                return;
            }
            if (text.length > 100) {
                confirm("内容不能超过100个字");
                return;
            }
            var reg = /jxedt_uid=.+?;?/;
            if(reg.test(document.cookie)){
                submit();
            }else{
                loginDom.show();
            }
        }
        function qqlogin(){
            window.open('http://user.jxedt.com/home/qqlogin?v=2.1', 'TencentLogin', 'width=740,height=450,menubar=0,scrollbars=0, status=1,titlebar=0,toolbar=0,location=1')
        }
    </script>


</body></html>