<script type="text/javascript">
function make_sub_nav(){
	var bg_ = $('<div id="page_body_cont_winbg"></div>');
	bg_.width($(document).width()).height($('#page_body').height());
	$('#page_body').append(bg_);
	return bg_;
}
$(function(){
	$('#nav_main_list li').has('div.nav_sub_box').mouseenter(function(){
		var ind_ = $(this).index();
		$('#nav_main_list li').removeClass('cur');
		$(this).addClass('cur');
		var bg_ = $('#page_body_cont_winbg');
		if(bg_.length<=0){bg_ = make_sub_nav();}
		bg_.show();
	});
	$('#nav_main_list li').has('div.nav_sub_box').mouseleave(function(){
		$('#nav_main_list li').removeClass('cur'); 
		$('#page_body_cont_winbg').hide(); 
	});
});
</script>
</div>
