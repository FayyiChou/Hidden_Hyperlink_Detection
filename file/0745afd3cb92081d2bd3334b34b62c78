<script type="text/javascript">
function readCookie(name)
{
  var cookieValue = "";
  var search = name + "=";
  if(document.cookie.length > 0)
  { 
    offset = document.cookie.indexOf(search);
    if (offset != -1)
    { 
      offset += search.length;
      end = document.cookie.indexOf(";", offset);
      if (end == -1) end = document.cookie.length;
      cookieValue = decodeURI(decodeURI(document.cookie.substring(offset, end)));
    }
  }
  return cookieValue;
}
function login_init(){
	if(readCookie('sys_cookie')!=''){
		show_logoutbox();
	}else{
		show_loginbox();
	}
}
function login_(){
$.colorbox({innerWidth:"643px", innerHeight:'328px;', opacity:'0.3',  inline:true, href:"#web_user_login_win",onClosed:function(){win_close();},onComplete:function(){$('#user_jacc_btn').click();}});
	if(readCookie('auto_cookie')!=''){$('#user_name_val').val(readCookie('auto_cookie'));}	
}
function win_close(){
	$('#user_name_val').val('');
	$('#user_pwd_val').val('');
	$('#user_jacc_val').val('');
}
function login_f(){
	var n_ = $('#user_name_val').val();
	var p_ = $('#user_pwd_val').val();
	var l_ = $('#user_jacc_val').val();
	if($.trim(n_)==''){alert('用户名不能为空'); $('#user_name_val').focus(); return false;}
	if($.trim(l_)==''){alert('验证码不能为空'); $('#user_jacc_val').focus(); return false;}
	$.getJSON('http://www.poten.cn:8080/security/login?callback=?',{'username':n_,'password':p_,'nologincode':'false','ajax':'true','loginCode':l_},function(d){
		if(d&&d.state=='success'){
			show_logoutbox();
			$.colorbox.close();
			$('#user_name_val').val('');
			$('#user_pwd_val').val('');
			$('#user_jacc_val').val('');
			try{topbar_login_LD();}catch(e){}
			return;
		}else if(d.state){
			alert(decodeURI(decodeURI(d.state)));
			$('#user_pwd_val').val('');
			$('#user_jacc_val').val('');
			return;
		}else{
			alert('失败，请重试');
			$('#user_pwd_val').val('');
			$('#user_jacc_val').val('');
			return;
		}
	});
}
function is_iframe(){
	if(self==top){return false;}else{return true;}
}
function logout_f(){
	$.getJSON('http://www.poten.cn:8080/logout?ajax=true&callback=?',function(){
		show_loginbox();
		try{topbar_logout_LD();}catch(e){}
	});
}
function show_logoutbox(){
	$('#topbar_loginbox').html('<a href="http://www.poten.cn:8080/pass/myperson.jsp" target="_blank">'+readCookie('sys_cookie')+'</a> | <a href="javascript:logout_f();">退出</a>');
}
function show_loginbox(){
	$('#topbar_loginbox').html('<a class="color_006ebc" href="javascript:login_();">登录</a>|<a class="color_006ebc" href="http://www.poten.cn:8080/regist/registnew.jsp" target="_blank">注册</a>');
}


$(function(){
	login_init();
	$('#user_jacc_val').keypress(function(e){if(e.keyCode==13){login_f();}});
	$('#user_jacc_btn').click(function(){
		$('#user_jacc_img').attr('src',$('#user_jacc_img').attr('data')+'?'+Math.random());
	});
	$("#web_user_login_win .i_input input").focus(function(){
		$(this).parent(".i_input").addClass("cur_f");	
	}).blur(function(){
		$(this).parent(".i_input").removeClass("cur_f");	
	});
})
