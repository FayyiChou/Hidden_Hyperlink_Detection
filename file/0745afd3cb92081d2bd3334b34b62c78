<script type="text/javascript">
$(function(){
	$('#sel_btn_box2').click(function(){
		$('#sel_list_box2').toggle();
		$('#sel_btn_box2').toggleClass('sel_btn_box2_cur',$('#sel_list_box2').is(':visible'));
	});
	$('#sel_list_box2 li').click(function(){
		$('#sel_input_box2').html($(this).html());
		$('#sel_list_box2').hide();
		$('#sel_btn_box2').toggleClass('sel_btn_box2_cur',$('#sel_list_box2').is(':visible'));
	});
	$('#sel_btn_box').click(function(){
		$('#sel_list_box').toggle();
		$('#sel_btn_box').toggleClass('sel_btn_box_cur',$('#sel_list_box').is(':visible'));
	});
	$('#sel_list_box li').click(function(){
		$('#sel_input_box').html($(this).html());
		$('#sel_list_box').hide();
		$('#sel_btn_box').toggleClass('sel_btn_box_cur',$('#sel_list_box').is(':visible'));
	});
	$(document).mousedown(function(e){
		var sel_list_box2 = $(e.target).closest("#sel_list_box2"); 
		var sel_list_box = $(e.target).closest("#sel_list_box");
		var sel_btn_box = $(e.target).closest("#sel_btn_box");
		var sel_btn_box2 = $(e.target).closest("#sel_btn_box2");
		var dingyue_box = $(e.target).closest("#vb_yanzheng_box");
		if(sel_list_box2.length>0||sel_list_box.length>0||sel_btn_box.length>0||sel_btn_box2.length>0||dingyue_box.length>0){return;}
		$('#sel_list_box2').hide();
		$('#sel_list_box').hide();
		$('#vb_yanzheng_box').hide();
		$('#sel_btn_box2').toggleClass('sel_btn_box2_cur',$('#sel_list_box2').is(':visible'));
		$('#sel_btn_box').toggleClass('sel_btn_box_cur',$('#sel_list_box').is(':visible'));
	});
});
</script>
<script type="text/javascript">
function check_ismail(mail){
	return(new RegExp(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/).test(mail));
}
function email_dingyue_fun(){
	var e_ = $('#vb_box_inptu_val').val();
	if($.trim(e_)==''){alert('邮箱地址不能为空'); $('#vb_box_inptu_val').focus(); return false;}
	if(!check_ismail(e_)){alert('邮箱格式不正确'); $('#vb_box_inptu_val').focus(); return false;}
$('#dingyue_code_img').click();
	$('#vb_yanzheng_box').show();
}
$(function(){
	$("#dingyue_code_val").keyup(function(){
	  if($.trim($(this).val()).length==4){
		var e_ = $('#vb_box_inptu_val').val();
		if($.trim(e_)==''){alert('邮箱地址不能为空'); $('#vb_box_inptu_val').focus(); return false;}
		if(!check_ismail(e_)){alert('邮箱格式不正确'); $('#vb_box_inptu_val').focus(); return false;}
		 $.getJSON('http://www.poten.cn:8080/addEmailDingYue?callback=?',{'email':e_,'loginCode':$.trim($(this).val())},function(t_){
				 if(t_=='codeFalse'){alert("验证码错误！")}else if(t_=='true'){alert('订阅成功！'); $('#vb_yanzheng_box').hide(); $('#vb_box_inptu_val').val('');}else{alert('失败，请重试');}
			});  
		}
	}); 
	$('#dingyue_code_img').click(function(){
		$('#dingyue_code_img').attr('src',$('#dingyue_code_img').attr('data')+'?'+Math.random());
	});
})
</script>
	
				
