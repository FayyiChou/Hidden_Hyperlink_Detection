    <script type="text/javascript">
        var speed = 2000
        marquePic2.innerHTML = marquePic1.innerHTML
        var myindex = 4;
        var isStop = false;
        function Marquee() {
            //if (demo.scrollLeft >= marquePic1.scrollWidth) {
            //    demo.scrollLeft = 0
            //} else {
            //    demo.scrollLeft++
            //}
            if (isStop) {
                return;
            }
            myindex--;
            if (myindex < 0) {
                myindex = 4;
            }
            var title = $('#NewsPic a:eq(' + myindex + ') img').attr("title");
            $('#NewsPicTxt a').text(title);
            $('#NavDiv span').removeClass("Cur").addClass("Normal");;
            $('#NavDiv span:eq(' + myindex + ')').addClass("Cur");
            $('#NewsPic a').hide();
            $('#NewsPic a:eq(' + myindex + ')').show();
        }
        var MyMar = setInterval(Marquee, speed)
        demo.onmouseover = function () { clearInterval(MyMar) }
        demo.onmouseout = function () { MyMar = setInterval(Marquee, speed) }
    </script>
    <script type="text/javascript">
        //$('#hotpic').liteNav(1000);
        $('#NewsPic a').each(function (key, obj) {

            $(obj).mouseenter(function () {
                isStop = true;
            }).mouseout(function () {
                isStop = false;
            });

        });
        $('#NavDiv span').each(function (key, obj) {
            $(obj).click(function () {
                $('#NavDiv span').removeClass("Cur").addClass("Normal");;
                $('#NavDiv span:eq(' + key + ')').addClass("Cur");
                var title = $('#NewsPic a:eq(' + key + ') img').attr("title");
                $('#NewsPicTxt a').text(title);
                $('#NewsPic a').hide();
                $('#NewsPic a:eq(' + key + ')').show();
                myindex = key;
             
            });
        });

    </script>
    <script type="text/javascript">
        var speed = 50
        marquePic2.innerHTML = marquePic1.innerHTML
        function Marquee() {
            if (demo.scrollLeft >= marquePic1.scrollWidth) {
                demo.scrollLeft = 0
            } else {
                demo.scrollLeft++
            }
        }
        var MyMar = setInterval(Marquee, speed)
        demo.onmouseover = function () { clearInterval(MyMar) }
        demo.onmouseout = function () { MyMar = setInterval(Marquee, speed) }
    </script>
