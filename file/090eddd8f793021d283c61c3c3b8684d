    <script type="text/javascript">
        var index = 0;
        $(function () {
            $('#Tab ul li').each(function (key, obj) {
                if (key < 2) {
                    $(obj).mouseenter(function () {
                        $('#Tab ul li').removeClass("avt");
                        $(obj).addClass("avt");
                        //alert(key);
                        $('#moos ul.moo').hide();
                        $('#moos ul.moo:eq(' + key + ')').show();
                        index = key;
                    }).mouseout(function () {
                        $('#moos ul.moo:eq(' + index + ')').show();
                    });;
                }
            });
        });
    </script>
    <script type="text/javascript">
