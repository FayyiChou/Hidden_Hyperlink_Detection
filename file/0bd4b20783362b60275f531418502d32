	//热点专题
	
	var rdztIndex=1;
	var rdzt='rdzt_';
	function goPre(){
		if(rdztIndex>1){
			var temp=rdztIndex;
			temp=rdztIndex+3;
			$("#"+rdzt+temp).hide();
			temp=rdztIndex-1;
			$("#"+rdzt+temp).show();
			rdztIndex=rdztIndex-1;
		}
		

	}
	
	function goAft(){
		if(rdztIndex<6-4){
			var temp=rdztIndex;
			$("#"+rdzt+temp).hide();
			temp=rdztIndex+4;
			$("#"+rdzt+temp).show();
			rdztIndex=rdztIndex+1;
		}
		
	}

</script>


