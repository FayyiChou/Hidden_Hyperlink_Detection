function openBz(i){
	if(i==1){
 		window.open("http://www.beian.gov.cn/portal/registerSystemInfo?recordcode=61010302000111");
	}
	if(i==2){
 		window.open("http://bszs.conac.cn/sitename?method=show&id=0D889F0C73083489E053022819AC3D04");
	}
}

function omoverQyhhb(i){
	if(i==1){
		$("#divhb1").show();$("#qyhb1").addClass('nav_p');
		$("#divhb2").hide();$("#qyhb2").removeClass('nav_p');
		$("#divhb1").show();
		$("#divhb2").hide();
	}
	if(i==2){
		$("#divhb2").show();$("#qyhb2").addClass('nav_p');
		$("#divhb1").hide();$("#qyhb1").removeClass('nav_p');
		$("#divhb2").show();
		$("#divhb1").hide();
	}
	
}

