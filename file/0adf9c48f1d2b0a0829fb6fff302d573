	<script>
		// function goTop(){
		// $("html,body").animate({
		// 	'scrollTop':"0px"
		// },600);
		// }
		// var scrolldelay; 
		function pageScroll(){
			document.body.scrollTop=0;
		}
		$(".sidetop").hover(function() {
			$(this).find(".sidebox").stop().animate({
				"width" : "60px",
				"left" : "-60px"
			}, 200).css({
				"opacity" : "1",
				"filter" : "Alpha(opacity=100)",
				"background" : "#4c5465",
				"display":"block"
			})
		}, function() {
			$(this).find(".sidebox").stop().animate({
				"width" : "0px",
				"left" : "0px"
			}, 200).css({
				"opacity" : "0",
				"filter" : "Alpha(opacity=0)",
				"background" : "#4c5465"
			})
		});
		$(window).on("scroll",function(){
			if($(document).scrollTop() >= $(window).height()/3){
				$(".sidetop").fadeIn("slow");
			}else{
				$(".sidetop").fadeOut("slow");
			}
		});

	</script>