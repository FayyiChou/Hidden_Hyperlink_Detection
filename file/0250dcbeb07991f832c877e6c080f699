<script>
    (function(jQuery){
        function slide(){
            jQuery('.slide1').bxSlider({
                mode:'fade',
                slideWidth: 1920,
                slideMargin: 0,
                captions: true,
                auto: true,
                infiniteLoop: true,
                autoHover: true,
                speed:1000,
                pause: 3000,
                controls:false
            });
        }
        function slideOnOff(){
            var len = $('.slide1 .slide').length;
            if(len > 1){
                slide();
            }
        }
        slideOnOff();
        function clickFn(tab,con,tab_c_css,i){
            jQuery(tab).removeClass(tab_c_css);
            jQuery(tab).eq(i).addClass(tab_c_css);
            jQuery(con).hide();
            jQuery(con).eq(i).show();
        }
        jQuery(".wb-b5-list1 li").each(function(i){
            jQuery(this).click(function(){
                clickFn('.wb-b5-list1 li','.wb-con','current',i);
            });
        });
    })(jQuery);
    document.onselectstart=function(){return false;}
    //借钱
    var time_b=null;
    var time_m =null;
    $('.borrow').hover(function(){
        clearTimeout(time_b)
        time_b = setTimeout(function(){
            $('.nav-wrap').show();
            $('.nav_borrow').show();
        },200) 
    },function(){
        time_b = setTimeout(function(){
            $('.nav-wrap').hide();
            $('.nav_borrow').hide();
        },200)
    })
    $('.nav_borrow').hover(function(){
        clearTimeout(time_b)
        clearTimeout(time_m)
        $('.nav-wrap').show();
        $('.nav_borrow').show();
        $('.borrow').css({
            color: '#00bcc2',
            background: '#f1f4f4'
        })
    },function(){
       time_b = setTimeout(function(){
            $('.nav-wrap').hide();
            $('.nav_borrow').hide();
        },200)
        $('.borrow').css({
            color: '',
            background: ''
        })
    })
    //理财
    $('.manage').hover(function(){
        clearTimeout(time_m)
        time_m = setTimeout(function(){
            $('.nav-wrap').show();
            $('.nav_manage').show();
        },200) 
    },function(){
        clearTimeout(time_m)
        time_m = setTimeout(function(){
            $('.nav-wrap').hide();
            $('.nav_manage').hide();
        },200) 
    })
    $('.nav_manage').hover(function(){
        clearTimeout(time_m)
        $('.nav-wrap').show();
        $('.nav_manage').show();
        $('.manage').css({
            color: '#00bcc2',
            background: '#f1f4f4'
        })
    },function(){
        time_m = setTimeout(function(){
            $('.nav-wrap').hide();
            $('.nav_manage').hide();
        },200)   
        $('.manage').css({
            color: '',
            background: ''
        })
    })
</script>
<script></script>
 
</body></html><!--#end-->