      <form name="search" method="post" onsubmit="return pageOKButton(this.pageNumber.value,this.maxpage.value);">
       <input name="pageNumber" class="txt" size="2" type="text" />
       <input name="maxpage" value="6" type="hidden" />
       <input type="submit" class="fysr_enter" value="跳转" />
      </form>
      <script type="text/javascript">
$(document).ready(function(){
$("[name=pageNumber]").keyup(function (){
if(!/^\d*$/.test($(this).val())){
$(this).val("")
}
});})
function pageOKButton(a,b){
var pageNumberStr = a;
var maxNumber = b;
var pageNumber=~~pageNumberStr;//强制转化为数字
if(pageNumberStr != pageNumber || pageNumberStr.indexOf(".") > -1  ){
alert("跳转页码必须为数值");
$(pageNumberStr).prev().prev().focus();
return false;}
if(pageNumber > ~~maxNumber ){
alert("跳转页码大于最大页数");
$(pageNumberStr).prev().prev().focus();
return false;
}
if(pageNumber < 1){
pageNumber = 1;
}
var url = "http:" + "//www.lfbxw.com/forum/thread-6538365-tid-0-back-0-{page}-1.html".replace("\{page\}",pageNumber);
window.location = url;return false;
}
</script> 
