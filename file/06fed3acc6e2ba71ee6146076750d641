    <script type="text/javascript" src="js/jquery.roundabout-1.0.min.js"></script>
    <script type="text/javascript" src="js/jquery.easing.1.3.js"></script>

    <script type="text/javascript">
    var aBtn1 = $('.out_btn a');
    var aBox1 = $('.out_con>div');
    var callbackArr = [
    	function(){
    		$('#featured-area1 ul').roundabout({
    		  easing: 'easeOutInCirc',
    		  duration: 600,
    		  cb: function(k){ rollMinPic('minp1',k); }
    		});
    	},
    	function(){
    		$('#featured-area2 ul').roundabout({
    		  easing: 'easeOutInCirc',
    		  duration: 600,
    		  cb: function(k){ rollMinPic('minp2',k); }
    		});
    	},
    	function(){
    		$('#featured-area3 ul').roundabout({
    		  easing: 'easeOutInCirc',
    		  duration: 600,
    		  cb: function(k){ rollMinPic('minp3',k); }
    		});
    	},
    	function(){
    		$('#featured-area4 ul').roundabout({
    		  easing: 'easeOutInCirc',
    		  duration: 600,
    		  cb: function(k){ rollMinPic('minp4',k); }
    		});
    	},
    	function(){
    		$('#featured-area5 ul').roundabout({
    		  easing: 'easeOutInCirc',
    		  duration: 600,
    		  cb: function(k){ rollMinPic('minp5',k); }
    		});
    	}
    ];
    callbackArr[0]();

    aBtn1.click(function(){
    	 var k = $(this).index();
    	 aBtn1.removeClass('active').eq(k).addClass('active');
    	 aBox1.hide().eq(k).show();
    	 if(!this.firstBind && k!=0){
    		 callbackArr[k] && callbackArr[k]();
    		 this.firstBind=true;
    	}
    });

    function rollMinPic(sid,key){
    	var aPics = $('#'+sid+' .text_list li');
    	var aBtns = $('#'+sid+' .text_btn a');
    	aPics.animate({opacity:0}).eq(key).animate({opacity:1});
    	aBtns.removeClass('active').eq(key).addClass('active');
    }

    var num1 = 0;
   var count1 = $("#featured-area1 ul li").length - 1;
   $("#featured-area1>ul>li").click( function () {
   	num1 = $(this).attr('id').match(/\d+/g);
   });
   function left1(){
   	num1++;
   	if(num1>count1){
   		num1 = 0;
   	}
   	$("#a"+ num1).stop().trigger("click");
   }
   function right1(){
   	num1--;
   	if(num1<0){
   		num1 = count1;
   	}
   	$("#a"+ num1).stop().trigger("click");
   }


    var num2 = 0;
   var count2 = $("#featured-area2 ul li").length - 1;
   $("#featured-area2>ul>li").click( function () {
   	num2 = $(this).attr('id').match(/\d+/g);
   });
   function left2(){
   	num2++;
   	if(num2>count2){
   		num2 = 0;
   	}
   	$("#b"+ num2).stop().trigger("click");
   }
   function right2(){
   	num2--;
   	if(num2<0){
   		num2 = count2;
   	}
   	$("#b"+ num2).stop().trigger("click");
   }

    var num3 = 0;
   var count3 = $("#featured-area3 ul li").length - 1;
   $("#featured-area3>ul>li").click( function () {
   	num3 = $(this).attr('id').match(/\d+/g);
