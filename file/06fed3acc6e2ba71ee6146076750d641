   		num3 = count3;
   	}
   	$("#c"+ num3).stop().trigger("click");
   }

    var num4 = 0;
   var count4 = $("#featured-area4 ul li").length - 1;
   $("#featured-area4>ul>li").click( function () {
   	num4 = $(this).attr('id').match(/\d+/g);
   });
   function left4(){
   	num4++;
   	if(num4>count4){
   		num4 = 0;
   	}
   	$("#d"+ num4).stop().trigger("click");
   }
   function right4(){
   	num4--;
   	if(num4<0){
   		num4 = count4;
   	}
   	$("#d"+ num4).stop().trigger("click");
   }
    var num5 = 0;
   var count5 = $("#featured-area5 ul li").length - 1;
   $("#featured-area5>ul>li").click( function () {
   	num5 = $(this).attr('id').match(/\d+/g);
   });
   function left5(){
