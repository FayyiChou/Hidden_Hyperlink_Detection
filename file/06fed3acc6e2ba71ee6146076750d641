    <script type="text/javascript">
    function getByClass(obj,sClass)
    {
      if(obj.getElementsByClassName)
      {
        return obj.getElementsByClassName(sClass);
      }
      var result=[];
      var arr=obj.getElementsByTagName('*');
      var re=new RegExp('\\b'+sClass+'\\b');
      for(var i=0;i<arr.length;i++)
      {
        if(re.test(arr[i].className))
        {
          result.push(arr[i]);
        }
      }
      return result;
    }
    function getId(id){
  		return document.getElementById(id);
  	}
    var getStyle=function(obj, name)
  	{
  		if(obj.currentStyle)
  		{
  			return obj.currentStyle[name];
  		}
  		else
  		{
  			return getComputedStyle(obj, false)[name];
  		}
  	}
  	var move=function(obj, json, fnEnd)
  		{
  			clearInterval(obj.timer);
  			obj.timer=setInterval(function (){
  			var all=true;	//鍋囪

  			for(var name in json)
  			{
  				if(name=='opacity')
  				{
  					var cur=Math.round(parseFloat(getStyle(obj, name))*100);
  				}
  				else
  				{
  					var cur=parseInt(getStyle(obj, name));
  				}

  				var speed=(json[name]-cur)/6;
  				speed=speed>0?Math.ceil(speed):Math.floor(speed);

  				if(name=='opacity')
  				{
  					obj.style.filter='alpha(opacity:'+(cur+speed)+')';
  					obj.style.opacity=(cur+speed)/100;
  				}
  				else
  				{
  					obj.style[name]=cur+speed+'px';
  				}

  				if(cur!=json[name])
  				{
  					all=false;
  				}
  			}

  			if(all==true)
  			{
  				clearInterval(obj.timer);
  				fnEnd && fnEnd();
  			}
  		}, 30);
  	}
    // 第一屏 轮播
    myWrap();
    function myWrap(){
      var aBtn=getByClass(document,"btn_list")[0].getElementsByTagName('a');
    	var aCon=getByClass(document,"img_list")[0].getElementsByTagName('li');
    	var now=0;
    	var oDiv=getByClass(document,'wrap')[0];
      var tab=function()
    	{
    		for(var j=0;j<aBtn.length;j++)
    		{
    			move(aCon[j],{opacity:0});
    			aBtn[j].className="";
    			aCon[j].style.zIndex=2;
    		}
    		move(aCon[now],{opacity:100});
    		aBtn[now].className="active";
    		aCon[now].style.zIndex=3;
    	};
    	for(var i=0;i<aBtn.length;i++)
    	{
    		(function(index){
    			aBtn[i].onclick=function()
    			{
    				now=index;
    				tab();
    			};
    		})(i)
    	}
    	var next=function()
    	{
    		now++;
    		if(now==aBtn.length)
    		{
    			now=0;
    		}
    		tab();
    	};
    	oDiv.timer=setInterval(next,2000);
    		oDiv.onmouseover=function ()
    		{
    			clearInterval(oDiv.timer);
    		};
    		oDiv.onmouseout=function ()
    		{
    			oDiv.timer=setInterval(next, 2000);
    		};
    }
    // imgTab();
    function imgTab(){
      var aImg=getId('featured-area').getElementsByTagName('li');
      var aText=getByClass(document,'text_btn')[0].getElementsByTagName('a');
      var n=0;
      for(var i=0;i<aImg.length;i++){
        aImg[i].index=i;
        aImg[i].onclick=function(){
          for(var j=0;j<aImg.length;j++){
              aText[j].className='';
          }
          aText[this.index].className='active';
        }
      }
    }
    </script>
