<script type="text/javascript">setcss();

function swap_tab1(n){

	for(var i=1;i<=5;i++){

		var curC=document.getElementById("tabview1"+i);

		var curB=document.getElementById("tab1"+i);

		if(n==i){

			curC.style.display="block";

			curB.className="dh_index1";

		}

		else{

			curC.style.display="none";

			curB.className="dh_index2";

	    }

	}

} 



function swap_bmdt(n){

	for(var i=1;i<=7;i++){

		var curC=document.getElementById("jrhy"+i);

		var curB=document.getElementById("bmdt"+i);

		if(n==i){

			curC.style.display="block";

			curB.className="news_bj1";
			

		}

		else{

			curC.style.display="none";

			curB.className="yhsp_index";

	    }

	}

} 

function swap_mxgr(n){

	for(var i=1;i<=4;i++){

		var curC=document.getElementById("mxqy"+i);

		var curB=document.getElementById("mxgr"+i);

		if(n==i){

			curC.style.display="block";

			curB.className="index_wsdc3";

		}

		else{

			curC.style.display="none";

			curB.className="index_xxgk3";

	    }

	}

} 



</script></head>

