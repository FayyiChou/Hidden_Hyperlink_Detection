		<script>
var jumpurl_1 = "http://lixiangjiawk.fang.com/bbs/2811172064~-1/515655695_515655695_{page}.htm";
function jumppage_1(page){
	var tpage = parseInt("1",10);
	if(event.keyCode == 13){
		var p = parseInt(page,10);
		if( p<1||p!=page ){
			alert("页号应该是一个数字");
			return false;
		}
		if( p>tpage ){
			p=tpage;
		}
		location.href = jumpurl_1.replace(/{page}/,page);
	}
}
function jumppage1_1(page){
	var tpage = parseInt("1",10);
	var p = parseInt(page,10);
	if( p<1||p!=page ){
		alert("页号应该是一个数字");
		return false;
	}
	if( p>tpage ){
		page=tpage;
	}
	location.href = jumpurl_1.replace(/{page}/,page);
}
</script>
