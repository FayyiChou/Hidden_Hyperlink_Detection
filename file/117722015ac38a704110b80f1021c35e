<script type="text/javascript">
	//获取cookie
	function getCookie(name)
	{
		var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
		if (arr = document.cookie.match(reg))
			return unescape(arr[2]);
		else
			return null;
	}
	function errorBox2013Login() 
	{
		if (jQuery("input[name='boxusername']").val() == "") {
			alert("请输入手机号/邮箱/用户名！");
			jQuery("input[name='boxusername']").focus();
			return false;
		}
		if (jQuery("input[name='boxpassword']").val() == "") {
			alert("请输入密码！");
			jQuery("input[name='boxpassword']").focus();
			return false;
		}
		var password = jQuery("input[name='boxpassword']").val();

		//基于https的通行证登录接入
		var uid = jQuery("input[name='boxusername']").val();
		var backurl = jQuery("#backurl").val();
		var httpsToken = getCookie('token');
		var httpsService = "soufun-bbs-web";
		
		//alert("正式接口测试、检查遗漏，error_box_2013.html，标注代码：P第  65 行"+uid+'---'+password+'------'+httpsToken+'----'+httpsService+'-----------'+backurl);
		jQuery.ajax({
			type: "get",
			url: "https://passport.fang.com/login.api",  //正式    
			//url: "https://passporttest.fang.com/login.api",//测试
			//url: '/NormalLoginInterface/Login',
			dataType: "jsonp",
			jsonp: "callback",
			data: { "Uid": escape(uid), "Pwd": escape(password), "Service": escape(httpsService), "Token": escape(httpsToken), "backurl":escape(backurl),"AutoLogin":"1" },
			xhrFields: {
				withCredentials: true
			},
			crossDomain: true,
			success: function (json) {
				if( json.Message=="Success" )
				{
					window.location.reload();
				}
				else
				{
					alert(json.Tip);
					jQuery("input[name='boxusername']").val("");
					jQuery("input[name='boxpassword']").val("");
				}
			}
		});
	}
</script>
