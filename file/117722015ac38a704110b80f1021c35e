<script type="text/javascript">
	/*大首页推广文案*/
	
	//top_nav_2014.js中空搜索跳转top_nav_2014.js必须在该文件下面加载
	var lcityt = 'http://gzbbs.fang.com/index.php?_c=Managerindex&_m=EliteList';
	var bbs_phbt = 'http://gzbbs.fang.com/rank.html';
	//var arr = lcityt.split('/');
	//var host = lcityt.indexOf('http://') > -1 ?  (arr[0]+'//'+arr[2]) : lcityt;
	var lcity = CurJsBbsHref+'asp/index/Rank.aspx';
	var bbs_phb = CurJsBbsHref+'phb_day.htm';
	//搜索事件
	function  multiSearchUB() {
		var selectIndex = document.getElementById('bs_idtype').selectedIndex;
		var searchType = document.getElementById('bs_idtype').options[selectIndex].text;
		var keyword = Trim(document.getElementById('multiSearchInput').value);
		var url = document.getElementById("defaultSearchUrl").value;
				if(searchType=='搜论坛'){
			if(keyword=="请输入关键字"){
				keyword='';
			}
			keyword = Trim(encodeURIComponent(keyword));
			_ub.collect(1,{'vwg.page':'bbspostpage','vwb.forumkey':keyword});
		}else{
			// var postkey = document.getElementById('multiSearchInput').value;
			if(keyword=="请输入关键字"){
				keyword='';
			}
			keyword = Trim(encodeURIComponent(keyword));
			_ub.collect(1,{'vwg.page':'bbspostpage','vwb.postkey':keyword});
		}

		multiSearch2014();//重新运行search函数
	}
	function Trim(str)
	{
		return str.replace(/(^\s*)|(\s*$)/g, "");
	}
</script>
