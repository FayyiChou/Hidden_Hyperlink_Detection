//发表评论
    function C() {
var user_status = 2;
    if(user_status == 1) {
       layer.msg('您的会员组没有评论权限', {time: 1200});
        return false;
        }
    if(user_status == 2) {
open_alertbox("login");
        return false;
        }
    if(Dd('content').value.length < 10) {
       layer.msg('内容最少需要10字', {time: 1200});
        Dd('content').focus();
        return false;
        }
if(Dd('content').value.length > 200) {
       layer.msg('内容最多200字', {time: 1200});
        Dd('content').focus();
        return false;
        }
$('#addcomment').attr('onclick','');
$('#subBtn').html('正在提交...');

var content = Dd('content').value;
$.post('/extend/comment.php',{
submit:'submit',
mid:21,
content:content,
itemid:113745,
items:12,
page:1,
qid: 0,
ip:'140.205.205.32'
},function(data) {
$("#comment-list").prepend(data);
if(ac_flag){
//恢复页面
Inner('chars', Dd('content').value.length);
$('#addcomment').attr('onclick','C();');
$('#subBtn').html('评论');
//刷新总评论 数
Dd('content').value = "";
document.getElementById("comment_count").innerHTML=parseInt(document.getElementById("comment_count").innerHTML)+1;
$("#success_layer").remove();
}else{
$("#error_layer").remove();
}
},'text');
    return false;
    }
        
//展示回复框
function reply(obj,cid,lev,hf_id){
var ncUser = "";
if(lev){
ncUser = "回复 "+$("#user_"+hf_id).html()+"：";
}
$("#del_"+cid).find('.comment-reply').remove();
$('#del_'+cid).append("<div class='comment-reply'>"+
"<span class='close-reply' onclick='close_reply(this)'>关闭</span>"+
"<div class='textarea'><textarea class='reply_area' id='replycontent' name='replycontent'>"+ncUser+"</textarea></div>"+
"<a href='javascript:' id='replyBtn' class='reply-btn' onclick='Q("+cid+",this)'>回复</a>"+
"</div>");
}
//关闭回复框
function close_reply(obj){
$(obj).parent().remove();
}

//发表回复(及  回复的回复)
function Q(qid,obj){
var user_status = 2;
    if(user_status == 1) {
       layer.msg('您的会员组没有评论权限', {time: 1200});
        return false;
        }
    if(user_status == 2) {
open_alertbox("login");
        return false;
        }
    if(Dd('replycontent').value.length < 10) {
       layer.msg('内容最少需要10字', {time: 1000});
        Dd('replycontent').focus();
        return false;
        }
if(Dd('replycontent').value.length > 200) {
       layer.msg('内容最多200字', {time: 1000});
        Dd('replycontent').focus();
        return false;
        }
$('#replyBtn').attr('onclick','').html('回复中...');

var content = Dd('replycontent').value;
$.post('/extend/comment.php',{
submit:'submit',
mid:21,
content:content,
itemid:113745,
items:12,
page:1,
qid: qid,
clientIP:'140.205.205.32'
},function(data) {
//console.log(data);return;
//拼接页面
var rObj = document.getElementById("subcomment_"+qid);
if(rObj == null){
$("#show_comment_hf"+qid).append("<div class='subcomment' id='subcomment_"+qid+"'>"+data+"<p class='subtotal'>共<span id='c_r_count_"+qid+"'>1</span>条回复</p></div>");
$("#r_count_"+qid).html(1);
}else{
//计算并更新 该评论的回复数量
var count = parseInt($("#c_r_count_"+qid).html())+1;
$("#c_r_count_"+qid).html(count);
$("#r_count_"+qid).html(count);

$("#c_r_count_"+qid).parent().before(data);
}
//关闭 回复框
close_reply(obj);
},'text');

    return false;
}

//评论框输入事件
function S() {
    Inner('chars', Dd('content').value.length);
    }
//评论框获得焦点事件
        function F() {
var user_status = 2;
if(user_status == 2) {
open_alertbox("login");
}
//H();
}
        //点 “支持” 或 “反对”
var v_id = 0;
var v_op = 1;
var v_nm = 0;
var v_lev = 0;
function V(id, op, nm,lev) {
if (!0) {
                open_alertbox("login");
                return;
            }
v_id = id;
v_op = op;
v_nm = nm;
v_lev = lev;
if(get_cookie('comment_vote_21_113745_'+id)) {
layer.msg('您已经对此评论表过态了', {time: 1200});
return;
}
var result = makeRequest('action=vote&mid=21&itemid=113745&cid='+id+'&op='+op, '/extend/comment.php?', '_V');
}
function _V() {
if(xmlHttp.readyState==4 && xmlHttp.status==200) {
if(xmlHttp.responseText == -2) {
layer.msg('抱歉，您没有投票权限', {time: 1200});
} else if (xmlHttp.responseText == -1) {
//confirm('');
layer.msg('您已经对此评论表过态了', {time: 1200});
} else if (xmlHttp.responseText == 0) {
layer.msg('参数错误，如有疑问请联系管理员', {time: 1200});
} else if (xmlHttp.responseText == 1) {
if(v_op == 1) {
Inner(v_lev+v_id+'_1', ++v_nm);
layer.msg("支持成功");
} else {
Inner(v_lev+v_id+'_0', ++v_nm);
layer.msg("反对成功");
}
}
}
}

