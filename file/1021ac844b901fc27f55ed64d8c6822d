  <script>
    function open_alertbox(alertname){
            $('.alertbox .bg').fadeIn("slow");
            $('.'+alertname).fadeIn("slow");
            ajaxlock = false;
        }
    function close_alertbox(alertname){
        $('.alertbox .bg').fadeOut("slow");
        $('.'+alertname).fadeOut("slow");
    }
    $('.login .tab-nav li').on('click',function(){
                var _index = $(this).index();
                $('.login .tab-nav li span').removeClass('active').eq(_index).addClass('active');
                $('.login-item').hide().eq(_index).show();
});
//手机验证码
var text = '获取验证码';
var init_str = '60 秒';
var can_send = true;
var ii;
function countdown() {
    var str = $('#send_scode').text();
    if (str == text) {
        str = init_str;
    }
    var seconds = str.match(/\d+/);
    var time = parseInt(seconds) - 1;
    if (time <= 0) {
        stopcount();
    } else {
        $('#send_scode').text(time + ' 秒');
    }
}
function stopcount() {
    can_send = true;
    $('#send_scode').text(text);
    $('#send_scode').css('background-color', '#e1090e');
    clearInterval(ii);
}
function SendSCode() {
var mobile = $('#mobile').val();
if (!mobile) {
    layer.msg('请填写手机号码');
    return;
}
if (!can_send) {
    return;
} else {
    can_send = false;
}
ii = setInterval(countdown, 1000);
$('#send_scode').text(init_str);
$('#send_scode').css('background-color', 'gray');
$.post('/member/register.php', {action: '23a1e5eb1e69a84f4cc11defb2ef7fc3', value: mobile}, function(data) {
    if(data == 1){
        layer.msg('短信发送成功');
    }else if(data == 2) {
        layer.msg('手机号码格式错误，请检查');
        stopcount();
    } else if(data == 3) {
        layer.msg('手机号码已存在，请更换');
        stopcount();
    } else if(data == 5) {
        layer.msg('短信发送过快，请稍后再试');
    } else if(data == 6) {
        layer.msg('尝试发送次数太多，请稍后再试');
    } else if (data == -1) {
        layer.msg('action invalid');
    } else {
        layer.msg('未知错误，请重试');
    }
});
}
//生成微信登陆二维码
