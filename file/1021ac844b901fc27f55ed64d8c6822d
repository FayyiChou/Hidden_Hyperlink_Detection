     <script type="text/javascript">
$(document).ready(function(e) {
t = $('.adfixed').offset().top;
mh = $('.main').height();
fh = $('.adfixed').height();
$(window).scroll(function(e){
s = $(document).scrollTop();
if(s > t - 10){
$('.adfixed').css('position','fixed');
if(s + fh > mh){
$('.adfixed').css('top',20+'px');
}
}else{
$('.adfixed').css('position','');
}
})
});
