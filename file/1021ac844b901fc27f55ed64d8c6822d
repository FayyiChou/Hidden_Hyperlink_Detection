//}
//H();

//未知事件
function comment_each(page){
$('.comment_page_'+page+' .inner').each(function(){
var height = $(this).find('.comment-text').height();
if(height>130){
$(this).find('.comment-text').css('max-height','96px');
$(this).find('.comment-text').after("<a href='javascript:' class='c_more' style='color: #1c88e3;'>查看更多</a>");
}
});
}
$(document).on('click','.c_more',function(){
if($(this).hasClass("active")){
$(this).prev('.comment-text').css('max-height','96px');
$(this).html('查看更多');
$(this).removeClass("active");
    }else{
        $(this).prev('.comment-text').css('max-height','100%');
$(this).html('收起评论');
$(this).addClass("active");
    }
})
</script> 
     <script>
document.getElementById("admin_flag").value=""
//删除评论
function deleteComment(cid,r_id){
if(!confirm('确定要删除此评论吗？')){
layer.msg("取消了删除");
return;
}
var data = {
mid:"21",
itemid:item_id,
page:"1",
cid:cid
};
$.post('/extend/comment.php?action=delete',data, function(data) {
//          console.log(data);return;
            var json = JSON.parse(data);
if (json.status == 1) {
layer.msg("删除成功!",{time:2000},function(){
//删除回复  计算回复数
if(r_id != 0){
var rcountObj = $("#c_r_count_"+r_id);
var ccount = parseInt(rcountObj.html())-1;
if(ccount<= 0){
$("#subcomment_"+r_id).remove();
$("#r_count_"+r_id).html(0);
}else{
$("#r_count_"+r_id).html(ccount);
rcountObj.html(ccount);
}
}
//删除对应元素
$("#del_"+cid).remove();
});
}else{
switch (json.error_code){
case 0:
layer.msg(" 数据异常，稍后再试 ");
window.location.reload();
break;
case 1:
layer.msg("该评论不存在!");
break;
case 2:
layer.msg("没有权限删除!");
break;
default:
layer.msg("系统繁忙，请稍后再试!");
break;
}
}
            });
}
</script> 
