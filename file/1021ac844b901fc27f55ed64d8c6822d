  <script type="text/javascript" src="http://cdn.staticfile.org/jquery-cookie/1.4.1/jquery.cookie.min.js"></script> 
  <script>
            $(function(){
            var zan_itemid = $.cookie('zan_'+113745);
            if(zan_itemid) {
                $('#pt2 a').css('background', 'url(http://images.yzmg.com/css/shiyong/images/zan2.png)');
            }
        });
        function add_favorite(mid, itemid, title, url) {
        var userid = 0;
        if (!userid) {
            open_alertbox('login');return;
        }
        if (!mid || !itemid || !title || !url) {
            return;
        }
        $.post('/member/favorite.php',
            {isajax: true, action: 'add', submit: 'submit', post: {'userid': 0, 'item_mid': mid, 'item_id': itemid, 'title': title, 'url': url}},
            function(data) {
                $('#pt1 a').css('background', 'url(http://images.yzmg.com/css/shiyong/images/sc2.gif)');
                if (data == 'ok') {
                    favorite(mid, itemid);
                } else if (data == 'exists') {
                    layer.msg('收藏过了', {time: 3600});
                } else {
                    layer.msg(data, {time: 3600});
                }
        });
    }
    function favorite(mid, itemid) {
        if (!mid || !itemid) {
            return;
        }
        $.post('/api/tj.php', {action: 'favorite', item_mid: mid, item_id: itemid}, function(data) {
            var json = JSON.parse(data);
            if (json.status == 'ok') {
                var num = parseInt($('#favorite_num').text());
                $('#favorite_num').text(num+1);
                if (json.credit) {
                    layer.msg('收藏成功+' + json.credit + ' 蘑菇', {time: 1800});
                } else {
                    layer.msg('收藏成功', {time: 1800});
                }
            }
        });
    }
    function zan(mid, itemid, author, title, linkurl, callback, e) {
        var userid = 0;
        // if (!userid) {
        //     open_alertbox('login');return;
        // }
        if (!mid || !itemid || !title || !linkurl) {
            return;
        }
                    if ($.cookie('zan_'+113745)) {
                layer.msg('已赞过了', {time: 1800});
                return;
            }
                $.post('/api/tj.php', {action: 'zan', item_mid: mid, item_id: itemid, 'author': author, 'title': title, url: linkurl, guest: 1}, function(data) {
            // console.log(data);
            var json = JSON.parse(data);
            if (json.status == 'ok') {
                if (callback) {
                    callback(e);
                } else {
                    var num = parseInt($('#zan_num').text());
                    $('#zan_num').text(num+1);
                    $('#pt2 a').css('background', 'url(http://images.yzmg.com/css/shiyong/images/zan2.png)');
                }
                if (json.credit) {
                    layer.msg('感谢你的大拇指+' + json.credit + ' 蘑菇', {time: 1800});
                } else {
                    layer.msg('感谢你的大拇指', {time: 1800});
                }
                                    $.cookie('zan_'+item_id,item_id);
                            } else if (json.status == 'exists') {
                layer.msg('已赞过了', {time: 1800});
            }
        });
    }
    function share(mid, itemid) {
        if (!mid || !itemid) {
            return;
        }
        $.post('/api/tj.php', {action: 'share', item_mid: mid, item_id: itemid}, function(data) {
            var json = JSON.parse(data);
            if (json.status == 'ok') {
                var num = parseInt($('#share_num').text());
                $('#share_num').text(num+1);
            }
        });
    }
    function exist_favorite(mid, itemid, domid) {
        if (!domid) {domid = 'pt1_a';}
        $.post('/member/favorite.php', {isajax: true, action: 'add', submit: 'submit', post: {'userid': 0, 'item_mid': mid, 'item_id': itemid, 'title': '', 'url': ''}, just_check: true}, function(data) {
            if (data == 'exists') {
                $('#'+domid).css('background-image', 'url(http://images.yzmg.com/css/shiyong/images/sc2.gif)');
            }
        });
    }
    function exist_zan(mid, itemid, author, linkurl, domid) {
        if (!domid) {domid = 'pt2_a';}
        $.post('/api/tj.php', {action: 'zan', item_mid: mid, item_id: itemid, 'author': author, 'title': '', url: linkurl, just_check: true}, function(data) {
            var json = JSON.parse(data);
            if (json.status == 'exists') {
                $('#'+domid).css('background-image', 'url(http://images.yzmg.com/css/shiyong/images/zan2.png)');
            }
         });
    }
</script> 
