  <script>
//判断用户是否登陆
var ajaxlock = true;
var ajaxhandle;
function synclogin(){
    if (!ajaxlock) {
        $.get("/api/ajax/code.php?state=d7b798ac86f045ca5c694e3e0a83812f", function(data){
            if(data==1){
                location.reload();
                clearInterval(ajaxhandle);
            }
        });
    }
}
$(function(){
    ajaxhandle = setInterval("synclogin()",1000);
    $('#logoBtn').click(function(){
        ajaxlock = false;
    });
    $('.login_win_close_wrap a').click(function(){
        $('.login_wechat').css('display','none');
        ajaxlock = true;
    });
});
</script> 
