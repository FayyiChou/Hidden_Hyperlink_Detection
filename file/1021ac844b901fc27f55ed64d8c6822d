//打赏
function reward(itemid,moduleid,username){
var amount = $('.ds_mun li.active').html();
amount = parseInt(amount);
   $.post('/api/reward.php', {action: 'reward', itemid: itemid, moduleid: moduleid,username:username,amount:amount}, function(data) {
console.log(data);
var json = JSON.parse(data);
if (json.status == 1) {
           layer.msg("打赏成功");
           var rewardNum = parseInt($("#rewardNum").html());
           $("#rewardNum").html(rewardNum+1);
           
           //获取用户的 用户名、昵称
           var username = "";
           var nickname = "";
           var showUsername = "";
           //判断用户昵称是否为空
           if(nickname == ""){
           var qianString = username.substr(0,3);
           var houString = username.substr(7,4);
           showUsername = qianString+"****"+houString;
           }else{
           showUsername = nickname;
           }
           
           $("#rewardUser").prepend("<span><img src='http://www.yzmg.com/api/avatar/show.php?userid=0&size=large' title='"+showUsername+"' ></span>");
           layer.closeAll('page'); //关闭所有页面层
}else{
switch (json.error_code){
case 0:
layer.msg("一次打赏只能0~50蘑菇哦~");
break;
case 1:
layer.msg("蘑菇不足，无法打赏哦~");
break;
case 2:
layer.msg("数据异常,文章不存在!");
break;
case 3:
layer.msg("本篇已打赏过，去看看其他文章吧!");
break;
case 4:
//插入 打赏日志失败
layer.msg("服务器繁忙，请等会儿再来打赏!");
break;
case 5:
login_show();
break;
default:
layer.msg("操作异常!请重试!");
break;
}
}
       });
   }
</script> 
