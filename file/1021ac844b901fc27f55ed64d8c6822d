//打开分享弹窗
function open_share(itemid,bid){
if(bid){
$('.shareurl').val("http://www.yzmg.com/mall/show.php?itemid="+itemid+"&bid="+bid);
$('#copyBtn').attr('data-clipboard-text',"http://www.yzmg.com/mall/show.php?itemid="+itemid+"&bid="+bid);
}else{
$('.shareurl').val("http://www.yzmg.com/mall/show.php?itemid="+itemid+"&type=1");
$('#copyBtn').attr('data-clipboard-text',"http://www.yzmg.com/mall/show.php?itemid="+itemid+"&type=1");
}
$("#sharebox").attr('data-itemid',itemid);
$("#sharebox").show();
$('.sharebox_bg').show();
$('body').css('overflow','hidden')
}
//关闭分享弹窗
function close_share(){
$("#sharebox").hide();
$('.sharebox_bg').hide();
$('body').css('overflow','auto')
}
function order(itemid) {
$.post('/api/ajax/mall.php', {action:'order', itemid:itemid}, function(data) {
var json = JSON.parse(data);
if (json.status == 1) {
location.href = '/mall/buy.php?itemid='+json.orderid;
} else if (json.status == 2) {
location.href = 'http://www.yzmg.com/api/pay/weixin/jsapi.php?itemid='+json.orderid;
} else if (json.status == 3) {
location.href = '/mall/buy.php?itemid='+json.orderid;
} else {
location.href = '/member/login.php';
}
});
}
