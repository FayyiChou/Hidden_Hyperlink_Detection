//滚动加载
var flag = 1;
    var page = 2; //设置当前页数
    var winHeight = $(window).height() + 120;
    $(window).on('scroll', function () {
        var scrollTop = $(this).scrollTop();
        var diff = $(document).height() - winHeight - scrollTop;
        if (diff < 0 && flag) {
            flag = 0;
            loadnext(page++); //动态加载内容
        }
});
function loadnext(page) {
$.get('/extend/comment.php?mid=21&itemid=113745&loadmore=1&page=' + page, function(data) {//
// console.log(data);
if (data && data.trim() != '') {
$('#comment-list').append(data);
//H();
comment_each(page);
flag = 1;
} else {
page--;
}
});
}
