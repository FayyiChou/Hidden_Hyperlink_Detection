	<script>
        $(function() {
            stats('click', 'http://res.youyunad.com/game/qyj_ad/apk_sem/bd_qyj_ad_195_113.apk')
            
            $('a').click(function(event) {
                var url = $(this).attr('href');
                if (url == 'http://res.youyunad.com/game/qyj_ad/apk_sem/bd_qyj_ad_195_113.apk') {
                    var down_count = $('.down_count').val();
                    if (down_count == 1) {
                        $('.down_count').val(2);
                        stats('down', 'http://res.youyunad.com/game/qyj_ad/apk_sem/bd_qyj_ad_195_113.apk')
                    }
                }
            });

            function stats(event, url) {
                var timestamp = Date.parse(new Date());
                $.get("http://youyundata.ijunhai.com/collect.gif?act=" + event + "&url=" + url + "&timestamp=" + timestamp, function(data) {});
            }
        });
    </script>
    
