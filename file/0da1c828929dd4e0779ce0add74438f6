_mvq.push(['$setAccount', 'm-20396-0']);
var wurl = wurl || document.location.href;
if (wurl.indexOf("item") > -1) { // 详情页代码
	if (typeof prdInfo != "undefined") {
		_mvq.push(['$setGeneral', 'goodsdetail', '', /*用户名*/ '', /*用户id*/ '']);
		_mvq.push(['$logConversion']);
		_mvq.push(['$addGoods',
			/*分类id*/ "", 
			/*品牌id*/ prdInfo.brandID || "", 
			/*商品名称*/ prdInfo.itemName || prdInfo.prdName || "",
			/*商品ID*/ prdInfo.itemId || prdInfo.prdId || "",
			/*商品售价*/ prdInfo.price, 
			/*商品图片url*/ "https:" + $(".j-bpic-b").attr("src"),
			/*分类名*/ "", 
			/*品牌名*/ prdInfo.breadName || "", 
			/*商品库存状态1或是0*/ "", 
			/*网络价*/ '',
			/*收藏人数*/ '', 
			/*商品下架时间*/ '',
			prdInfo.firstCategoryName || "",
			prdInfo.secondCategoryName || "",
			prdInfo.thirdCategoryName || ""
			]);
		_mvq.push(['$addPricing', /*价格描述*/ '']);
		_mvq.push(['$logData']);
	}
} else if (wurl.indexOf("cart.jsp") > -1) { // 购物车页代码
	_mvq.push(['$setGeneral', 'cartview', '', /*用户名*/ '', /*用户id*/ '']);
	_mvq.push(['$logConversion']);
	var mediavAddItem = [];
	(function() {
		var smStr = "";
		for(var k = 0, len = shippingGroups.length; k < len; k++) {
			if(k == 0) {
				var normalCartGroups = shippingGroups[0].normalCartGroups;
				var items = shippingGroups[0].items;
				//国美在线商品存在
				if(normalCartGroups) {
					var normalLen = normalCartGroups.length;
					$.each(normalCartGroups, function (index, item) {
						var generalItems = item.generalItems;
						var generalLen=generalItems.length;
						$.each(generalItems, function (i,item2) {
							mediavAddItem.push(item2.productId);
						});
					});
				}
				//如果国美在线商品不存在，则第一个为店铺商品或其他
				if(items) {
					var len = items.length;
					$.each(items, function (index, item2) {
						mediavAddItem.push(item2.productId);
					});
				}
			} else {	
				if(typeof(shippingGroups[k]) == 'undefined') continue;
				var items = shippingGroups[k].items;
				if(items) {
					var len = items.length;
					$.each(items, function (index, item2) {
						mediavAddItem.push(item2.productId);
					});
				}
			}
		}
	})()
	mediavAddItem = mediavAddItem.join(",");
	_mvq.push(["$addItem", "", mediavAddItem, "", ""]);
	_mvq.push(['$logData']);
} else if (wurl.indexOf("commitSuccess.jsp") > -1) { // 订单页代码
	_mvq.push(['$setGeneral', 'ordercreate', '', /*用户名*/ '', /*用户id*/ '']);
	_mvq.push(['$logConversion']);
	_mvq.push(['$addOrder',/*订单号*/ orderId, /*订单金额*/ totalPrice]);
	shippingGroups = eval(shippingGroups);
	if(shippingGroups){
		var shipLen=shippingGroups.length;
  		$.each(shippingGroups,function (index,item2){
			_mvq.push(['$addItem',
				/*订单号*/ orderId, 
				/*商品id*/ item2.productId, 
				/*商品名称*/ item2.productName, 
				/*商品价格*/ item2.price, 
				/*商品数量*/ item2.quantity, 
				/*商品页url*/ '', 
				/*商品页图片url*/ '']);
		});
  	}
}
_mvq.push(['$logConversion']);
(function() {
    var mvl = document.createElement('script');
    mvl.type = 'text/javascript'; mvl.async = true;
    mvl.src = ('https:' == document.location.protocol ? 'https://material-ssl.mediav.com/bjjs/mba_ka.js' : 'http://material.mediav.com/bjjs/mba_ka.js');
