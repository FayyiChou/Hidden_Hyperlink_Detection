		function qie(data,id){

		$(id).children("a").addClass("hover");
		$(id).siblings().children("a").removeClass("hover");
		$.post("pro_ajax.php",{id_lm:data},function(val){

		$("#slide").html(val);
		new ZoomPic("slide");
		
		});
		
		}
