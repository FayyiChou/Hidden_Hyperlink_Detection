<script>

	function syin(data){
	
	$(data).css("display","none");
	
	}
	function start(data,val){
	
	$(data).css("display","block");
	$(".tan_sou").css("display","block");
	$(".yu").css("display","none");
	if(val=="yu"){
	$(".tan_sou").css("display","none");
	$("#tan_nei").css("display","none");
	$(".yu").css("display","block");
	}
	}
	function sou(data){
	if(data==''){
	return false;
	}
	$.post("sou_ajax.php",{keyword:data},function(zhi){
	
	$("#tan_nei").html(zhi);
	$("#tan_nei").css("display","block");
	
	});
	}
</script>

