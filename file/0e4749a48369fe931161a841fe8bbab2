function selectitem(obj,showid)
{
	if(showid=="shownews")
	{
		document.getElementById(showid).style.display="none";
		document.getElementById(showid+"more").style.display="none";
		document.getElementById("shownewsimg").src="/templates/default/images/n1a.gif";
		document.getElementById("showdt").style.display="";
		document.getElementById("showdtmore").style.display="";
		obj.src="/templates/default/images/n2.gif";
		}
		else
		{
		document.getElementById(showid).style.display="none";
		document.getElementById(showid+"more").style.display="none";
		document.getElementById("showdtimg").src="/templates/default/images/n2a.gif";
		document.getElementById("shownews").style.display="";
		document.getElementById("shownewsmore").style.display="";
		obj.src="/templates/default/images/n1.gif";
			}
	}
function selectitem2(obj,showid)
{
	if(showid=="shownews2")
	{
		document.getElementById(showid).style.display="none";
		document.getElementById(showid+"more").style.display="none";
		document.getElementById("shownewsimg2").src="/templates/default/images/2014-a1.gif";
		document.getElementById("showdt2").style.display="";
		document.getElementById("showdt2more").style.display="";
		obj.src="/templates/default/images/2014-b.gif";
		}
		else
		{
		document.getElementById(showid).style.display="none";
		document.getElementById(showid+"more").style.display="none";
