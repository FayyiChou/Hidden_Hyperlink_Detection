var tID=0;   















function ShowTabs(ID){  















    if(ID!=tID){ 















        eval("document.getElementById('Tabs"+[tID]+"').style.display='none';");   















        eval("document.getElementById('Tabs"+[ID]+"').style.display='';");   















        tID=ID;   















    }   















} 















</script> 















                      <script> 















var tID=0;   















function ShowDay(ID){  















    if(ID!=tID){ 















        eval("document.getElementById('Day"+[tID]+"').style.display='none';");   















        eval("document.getElementById('Day"+[ID]+"').style.display='';");   















        tID=ID;   















    }   















} 















