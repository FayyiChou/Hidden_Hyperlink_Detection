</div>















<script type="text/javascript">















function ChannelSlide(Name,Class){















	$(Name+' ul.photo li').sGallery({















		titleObj:Name+' div.title p',















		thumbObj:Name+' .thumb li',















		thumbNowClass:Class















	});















}















new ChannelSlide(".channel-slide","on");















</script>















