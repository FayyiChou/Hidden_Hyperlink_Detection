                $(function(){
                    var timer=null;
                    var $navDown=$(".navBar-down");
                    $navDown.hover(function(){
                        var $this=$(this);
                        if(timer){
                            clearTimeout(timer);
                            timer=null;
                        }
                        timer=setTimeout(tanchu,500);
                        function tanchu(){
                            $(".down-arr").show();
                            $this.find("ul").slideDown();
                        }

                    },function(){
                        timer=setTimeout(shouqi,1000);
                        var $this=$(this);
                        function shouqi(){
                            $(".down-arr").hide();
                            $this.find("ul").slideUp();
                        }
                    });

                });
