	  <!-- 开班滚动js-->
    
	<script type="text/javascript">
var dome=document.getElementById("dome");
var dome1=document.getElementById("dome1");
var dome2=document.getElementById("dome2");
var speed=50;//设置向上轮动的速度
dome2.innerHTML=dome1.innerHTML;//复制节点
function moveTop(){
if(dome1.offsetHeight-dome.scrollTop<=0){
dome.scrollTop=0;
}else{
dome.scrollTop++;
}
}
var myFunction=setInterval("moveTop()",speed);
dome.onmouseover=function(){
clearInterval(myFunction);
}
dome.onmouseout=function(){
myFunction=setInterval(moveTop,speed);
}
</script>

 <!-- qiehuanjs-->
  <script>
$(function(){
	$(".container>div").hide();
	$(".container>div:eq(0)").show();	
	
	$(".navContent a").click(function(){
		var n = $(".navContent a").index(this);
		$(".navContent a").index(this);
		$(".container>div").hide();
		$(".container>div:eq("+n+")").show();	
	})
	
	
})
</script>
<script type="text/javascript">
function setContentTab(name, curr, n) {
    for (i = 1; i <= n; i++) {
        var menu = document.getElementById(name + i);
        var cont = document.getElementById("con_" + name + "_" + i);
        menu.className = i == curr ? "hover" : "";
        if (i == curr) {
            cont.style.display = "block";
        } else {
            cont.style.display = "none";
        }
    }
}
</script>
	 <!-- bannerjs-->
	
	
	
	 <script type="text/javascript">
        // 轮播图划过切换
        $(document).ready(function() {
            var lunbo=$("#lunbo");
            var imgBox=$("#imgBox");
            var buttons=$("#buttons").find('li');
            var index=0;
            var timer=null;
            var delay=2000;
            var imgWidth=300;
            timer=setInterval(autoSwitch, delay)
             // 遍历所有数字导航实现划过切换至对应的图片
            buttons.each(function(i) {
                buttons.eq(i).attr('id', i);
                $(this).mouseover(function(event) {
                    clearInterval(timer);
                    changePic($(this).attr("id"));
                });
            });
            lunbo.hover(function() {
                clearInterval(timer);
            }, function() {
                timer=setInterval(autoSwitch,delay);
            });
            function changePic (currentIndex){
              buttons.eq(currentIndex).addClass("on").siblings().removeClass("on");
              // imgBox.animate({'left':-currentIndex*imgWidth}, 2000);
              imgBox.animate({left:-currentIndex*imgWidth},"normal");
              index=currentIndex;
            }
            function autoSwitch (){
              index++;
              if(index>=buttons.length){
                index=0;
              }
              changePic(index);
            }
        });
    </script>
