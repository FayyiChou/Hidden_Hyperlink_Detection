	<script>
    $(function () {
        //左侧导航
        $('.siderNav').each(function (index, element) {
            $(element).animate({ top: $(document).scrollTop() + (($(window).height() - $(element).height()) / 2) }, 500);
            $(window).scroll(function () {
                $(element).animate({ top: $(document).scrollTop() + (($(window).height() - $(element).height()) / 2) }, 30);
            })
        });

        //默认加载显示广告2015.8.26新增
        popup('becPopAdid', 'becPopAd');
        var dH = $(document).height();
        $('#becmask').css({ 'height': dH, 'display': 'block' })
        $('#becclose').click(function () {
            $('#becPopAdid').hide();
            $('#becmask').hide();
        });

        $('.oral_ul1 li').hover(function () {
            $(this).find('.oral_show').show();
            $(this).find('.imgbox').hide();
        }, function () {
            $(this).find('.oral_show').hide();
            $(this).find('.imgbox').show();
        });
        //选择合适的在线学习方式对学习效果的影响将会事半功倍
        $('#Area5Ul li').hover(function () {
            $(this).find('dl').show();
            $(this).find('h4').hide();
            $(this).find('div:last').css('background-color', '#2887ab')
        }, function () {
            $(this).find('dl').hide();
            $(this).find('h4').show();
            $(this).find('div:last').css('background-color', '#2887ab')
        });
        /*在线测评*/
        $('.schemecs li').hover(function () {
            var _txt = $(this).find('.scheme_dv1');
            $(this).find('.scheme_dv').fadeIn(10, function () {
                _txt.fadeIn(10);
            });
        }, function () {
            var _bg = $(this).find('.scheme_dv');
            $(this).find('.scheme_dv1').fadeOut(10, function () {
                _bg.fadeOut(10);
            });
        });

        //
        $('.oral_s_nav li').hover(function () {
            var index = $(this).index();
            $(this).addClass('on').siblings('li').removeClass('on');
            $('.oral_vi').find('li').eq(index).show().siblings('li').hide();
        });

    });
    </script>
	
