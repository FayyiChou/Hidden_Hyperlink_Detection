    <script type="text/javascript">
    $(function () {
        var hydt = $("#hydt");
        var jyxw = $("#jyxw");
        var info1 = $("#info1");
        var info2 = $("#info2");
        var more = $("#newsmore");
        var url1 = "/ac/";
        var url2 = "/ac/";

        hydt.mousemove(function () {
            more.attr("href", url1);
            info1.show();
            info2.hide();
            hydt.removeClass().addClass("cur");
            jyxw.removeClass();
        });

        jyxw.mousemove(function () {
            more.attr("href", url2);
            info2.show();
            info1.hide();
            jyxw.removeClass().addClass("cur");
            hydt.removeClass();
        });
    });
</script> 
    
