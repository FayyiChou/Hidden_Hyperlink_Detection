   }); 
   function onnav(my,id){
        var ul = document.getElementById('navul-'+id);
		var width = $(my).width();
		$(my).addClass('navdown');
        $(ul).css('display','block');
		$(ul).width(width); 
}
   function outnav(my,id){
         var ul = document.getElementById('navul-'+id);
		 $(my).removeClass('navdown');
		 $(ul).css('display','none');
   }

