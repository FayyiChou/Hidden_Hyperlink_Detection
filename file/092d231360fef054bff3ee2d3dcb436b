(function ($){
var id=0,page = 1, isLoading = false;
    loadData();
    function onScroll(event){
        if (!isLoading) {
         var closeToBottom = ($(window).scrollTop() + $(window).height() > $(document).height() - 100);
            if(closeToBottom) {
                loadData();
            }
        }
    };
    function loadData(){
        isLoading = true;
        $.ajax({
            url: '/index.php?s=news&c=api&m=template&name=list_data&pagesize=10&catid=' + id,
            data: { page: page }, 
            success: onLoadData
        });
    };
    function onLoadData(data){
        isLoading = false;
        page++;
        $('.dr_ajax_result').append(data);
        $(".loadmore").show().html("努力加载中...");

    };
    $(document).on('scroll', onScroll);
})(jQuery);


</script>
