jQuery(".moreBox").hover(function(){jQuery(this).find(".more-channels").stop(true,true).fadeTo("show",1) },function(){ jQuery(this).find(".more-channels").fadeOut() });
jQuery(".tb-item-more").hover(function(){jQuery(this).find(".layer").stop(true,true).fadeTo("show",1) },function(){ jQuery(this).find(".layer").fadeOut() });
// jQuery("#tabRank li").hover(function(){ jQuery(this).addClass("on").siblings().removeClass("on")},function(){ });
if(jQuery('.topbarWrapper').length>0){
    $(function(){
      m = $('#pagelet-nnav');
      b = $('.index-left');
      t = $(document).scrollTop();
      s = m.outerHeight();
      $(window).scroll(function() {
        var h = $(document).scrollTop();
        if (h > s){ m.addClass('scroll'); } else { m.removeClass('scroll'); }
        if (h > 40){ b.addClass('fixed'); } else { b.removeClass('fixed'); }
        t = $(document).scrollTop();
      });
    });
}
