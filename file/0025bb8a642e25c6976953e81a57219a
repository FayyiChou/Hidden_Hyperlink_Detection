var galleries = $('.ad-gallery').adGallery();
var z_n = $('.zhiwei_nav');
var z_n_t = z_n.offset().top;
var off_array = new Array();
off_array[0] = $('#fwxx').offset() ? $('#fwxx').offset().top : Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
off_array[1] = $('#fwdt').offset() ? $('#fwdt').offset().top : Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
off_array[2] = $('#txqfy').offset() ? $('#txqfy').offset().top : Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
off_array[3] = $('#fjfy').offset() ? $('#fjfy').offset().top : Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
off_array[4] = $('#jgxjfy').offset() ? $('#jgxjfy').offset().top : Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
off_array[5] = $('#xqsj').offset() ? $('#xqsj').offset().top : Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
$(window).scroll(function(e) {
	var top = window.pageXOffset || document.documentElement.scrollTop || document.body.scrollTop;
	if (top >= z_n_t) {
		z_n.addClass('z_n_fix');
	} else {
		z_n.removeClass('z_n_fix')
	}
	for (var i = 0; i < 6; i++) {
		if (top > off_array[i] - 200) {
			$('.zhiwei_nav li').removeClass('active').eq(i).addClass('active')
		}
	}
});
