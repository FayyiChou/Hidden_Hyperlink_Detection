    <script type="text/javascript">
$(function(){
   $('#PostCode').focus(function() {
       if ($(this).next().attr('src') == 'http://img.pccoo.cn/post/images/yammr.gif') {
           $(this).next().click();
       }
   })
});
function postResubmit(reid) {
   var test = '既然路过就说些什么吧（理性上网 文明留言 200个字符以内）';
   reid = parseInt(reid);
   if (reid > 0) {
       var HtmlContent = $('#HtmlContent' + reid);
       var PostCode = $('#PostCode' + reid);
       if (HtmlContent.val() == '' || HtmlContent.val() == test) {
           alert('回复信息不能为空!');
           HtmlContent.focus();
           return;
       }
       if (HtmlContent.val().length > 200) {
           alert('回复信息过长，200字以内!');
           HtmlContent.focus();
           return;
       }
       if (PostCode.val() == '') {
           alert('验证码不能为空!');
           PostCode.focus();
           return;
       }
       $.post('/post/ajax/addbook.asp', {
           stype: 'chuzu',
           HtmlContent: escape(HtmlContent.val()),
           PostCode: escape(PostCode.val()),
           newsid: $('#newsid').val(),
           reid: reid
       }, function(data) {
           if (data == 1 || data == '1') {
               alert('留言提交成功！')
               getBookPage('bookpage', 464633 , 1, 'chuzu');
           } else {
               alert(data);
           }
       });
   }
   return false;
}
</script> 
