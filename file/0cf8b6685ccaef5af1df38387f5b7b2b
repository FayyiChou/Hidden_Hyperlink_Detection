 <script>
         var change = false; 
        $(function(){
          $.jsonp({
			async : false,
	        url: "http://logininfo.nxeduyun.com/online.jsp",
	        callback:"callbackHandler",
	        success: function (result) {
	        	$("#online").html(result.online);
	        	$("#history").html(result.history);	        	
	        },
	        error: function (xOptions, textStatus) {
	        }
		});	
           $("#username").val('');
           $("#password").val('');
			$("#username").click(function(){if(!change){$("#captchaimg").click();change = true;}});
			$("#password").click(function(){if(!change){$("#captchaimg").click();change = true;}});
          	function noLogin(){
            	$(".nx-ban-login").show();
            }
			$.RepsLogin.isLogin($.RepsLogin.loginSuccess,noLogin);
		});
        </script>
