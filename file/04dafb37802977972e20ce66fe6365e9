<script>
    $(function () {
        var QInterval = 1;
        var QDirectionTB = 0; //上0下1
        var QDirectionLR = 0; //左0右1
        var QMoveValue = 1;
        var QTime = 50;
        var obj = $(".pc_1");
        var closeObj = $(".gb_1");

        function FloatDiv() {
            if (obj.offset().left >= ($(window).width() - obj.width() - QMoveValue)) {
                QDirectionLR = 0;
            }
            if (obj.offset().left <= 0) {
                QDirectionLR = 1;
            }
            if (obj.offset().top >= ($(window).height() - obj.height() - QMoveValue)) {
                QDirectionTB = 0;
            }
            if (obj.offset().top <= 0) {
                QDirectionTB = 1;
            }
            obj.css({ top: QDirectionTB == 0 ? obj.offset().top - QMoveValue : obj.offset().top + QMoveValue, left: QDirectionLR == 0 ? obj.offset().left - QMoveValue : obj.offset().left + QMoveValue });
        }

        obj.mouseover(function () {
            clearInterval(QInterval);
        });

        obj.mouseout(function () {
            QInterval = setInterval(FloatDiv, QTime);
        });

        closeObj.click(function () {
            clearInterval(QInterval);
            obj.remove();
        });

        QInterval = setInterval(FloatDiv, QTime);
    })
</script>


