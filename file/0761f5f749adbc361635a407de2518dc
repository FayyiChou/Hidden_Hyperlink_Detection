       <script type="text/javascript">
                function onAppointment() {
                    var parms = new Object();
                    parms["Name"] = $("#name").val();
                    parms["Mobile"] = $("#mobile").val();
                    parms["Email"] = $("#email").val();
                    parms["Description"] = $("#description").val();
                    $.ajax({
                        type: "Post",
                        url: "/appointment/createuser?rnd=" + Math.random() * 10,
                        data: parms,
                        success: function (data) {
                            if (data.isSuccess) {
                                alert("预约成功");
                            }
                            else {
                                alert(data.Message);
                            }
                        }
                    });
                }
            </script> 
