<script>
function star(obj,num){
  if(num<=5){
    for(var n=0;n<num;n++){
      $(obj).append('<img src="/themes/news/images/star_03.png"/>')
    }
    for(var o=0;o<(5-num);o++){
      $(obj).append('<img src="/themes/news/images/star_05.png"/>')
    }
  }
}
star('.star',4)
</script>
<script>
var b5_html=$('.b5l_inner').html();
var b5_width=$('.b5l_box').width()*2;
var j=0;
var timer;

$('.b5l_inner').append(b5_html);
$('.b5l_inner').prepend(b5_html);
$('.b5_arrow_left').click(function(){
  if($('#label').length>0) return false;
  j++
  if(j>2)j=0;
  if(j<0)j=2;
  move_left()  
  chg();
  //console.log(j)
})
$('.b5_arrow_right').click(function(){
  if($('#label').length>0) return false;
  j--
  if(j>2)j=0;
  if(j<0)j=2;
  move_right()  
  chg()
  //console.log(j)
})

timer=setInterval(auto_play,6000)
$('.box5').mouseenter(function(){
  clearInterval(timer);  
})
$('.box5').mouseleave(function(){
  clearInterval(timer);
  timer=setInterval(auto_play,6000)
})
function auto_play(){
  if($('#label').length>0) return false;
  j++
  if(j>2)j=0;
  if(j<0)j=2;
  move_left()  
  chg();
}

$('.b5r_boxt').each(function(k){
  $(this).mouseover(function(){
    j=k;    
    $('.b5_left').stop().animate({
      //scrollLeft:left1+b5_width/5
      scrollLeft:j*b5_width/2
    },150)
    chg()
  })
})


function chg(){
  for(var i=0;i<4;i++){
    $('.b5r_filter').eq(i).css({display:'none'})
    $('.b5rb_box').eq(i).css({display:'none'})
  }
  $('.b5r_filter').eq(j).css({display:'block'})
  $('.b5rb_box').eq(j).css({display:'block'})
}

function move_left(){
  var left1=$('.b5_left').scrollLeft();
  if(left1<2*b5_width){
    $('body').append('<span id="label" style="display:none"></span>')
    $('.b5_left').stop().animate({
      //scrollLeft:left1+b5_width/5
      scrollLeft:j*b5_width/2
    },150,function(){$('#label').remove()})    
  }else{
    $('.b5_left').scrollLeft(left1-b5_width)
    move_left()
  }
  
}
function move_right(){
  
  var left1=$('.b5_left').scrollLeft();
  if(left1>(b5_width-b5_width/2)){
    $('body').append('<span id="label" style="display:none"></span>')
    $('.b5_left').stop().animate({
      //scrollLeft:left1-b5_width/5
      scrollLeft:j*b5_width/2
    },150,function(){$('#label').remove()})
  }else{    
    $('.b5_left').scrollLeft(left1+b5_width)
    move_right()
  }
  
}
</script>

