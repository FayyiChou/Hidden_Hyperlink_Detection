<script>

	var finish=true;
	$('input[type=button]').click(function(){
		$('#formdiv').submit();
	});
	$(window).bind('scroll',function(){
		
		
		if($(document).scrollTop() + $(window).height() > $(document).height()-10){
			
			if(finish){
			finish=false;
			var page=$('input[name=page]').val();
			var dishi=$('input[name=dishi]').val();
			var bx=$('input[name=bx]').val();
			var kksj=$('input[name=kksj]').val();
			var lx=$('input[name=lx]').val();
			var s=$('input[name=s]').val();
			
				$.ajax({
					'url':'/listnext',
					'data':{'page':page,'dishi':dishi,'kksj':kksj,'bx':bx,'lx':lx,'sousuo':s},
					'type':'get',
					success:function(data){
						
						//console.log(data);
						
						for(i=0;i<(data.length);i++){
							if(data[i]['pingjia']=='wu'){
								$('.list').append('<li><dl class="zb_open"><dt><a href="/kc/'+data[i]['aid']+'.html"><img src="/pic/'+data[i]['bjhb']+'_x.jpg"></a></dt><dd><a href="/kc/'+data[i]['aid']+'.html"><strong>'+data[i]['title']+'</strong></a><p><i>'+data[i]['xf']+'</i></p><p><span>'+data[i]['click']+'人关注</span><img src="/images/str1.png" /><img src="/images/str1.png" /><img src="/images/str1.png" /><img src="/images/str1.png" /><img src="/images/str1.png" /></p></dd></dl></li>');
							}else{
								$('.list').append('<li><dl class="zb_open"><dt><a href="/kc/'+data[i]['aid']+'.html"><img src="/pic/'+data[i]['bjhb']+'_x.jpg"></a></dt><dd><a href="/kc/'+data[i]['aid']+'.html"><strong>'+data[i]['title']+'</strong></a><p><i>'+data[i]['xf']+'</i></p><p><span>'+data[i]['click']+'人关注</span><img src="/images/str1.png" /><img src="/images/str1.png" /><img src="/images/str1.png" /><img src="/images/str1.png" /></p></dd></dl></li>');
							}
							
						}
						
						if(data.length!=0){
							$('input[name=page]').val(parseInt(page)+1);
							finish=true;
						}else{
							finish=false;
						}
						
						
						
					}

				})
			
			}
		}
	
	})
	
	function jiazai(){
		
		$('input[name=page]').val('2');
	
	}
	
	function xuanzhong(obj){
	
		
		$('#kclx').html($(obj).find('span').html())
	}
