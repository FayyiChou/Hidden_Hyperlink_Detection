<script type="text/javascript">
$(function(){
	var num01=0;
	var num02=0;
	$('.member_right').click(function(){
		num01++
		if(num01 > 7){
			$('ul').css('left',0);
			num01=1;
			$('ul').stop().animate({left:-num01*225},500);
		}else{
			$('ul').stop().animate({left:-num01*225},500);
		}
		/*---------------以上是所有图片工作------------------*/
		
	})	
	
	$('.member_left').click(function(){
		num01--
		if(num01<0){
			$('ul').css('left',-1575);//瞬间把图片拉回到某个位置
			num01=7;
			$('ul').stop().animate({left:-num01*225},500);
		}else{
			$('ul').stop().animate({left:-num01*225},500);
		}
		/*---------------以上是所有图片工作------------------*/
	})
})
</script>	
	
