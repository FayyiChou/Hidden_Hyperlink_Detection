        type = Sizzle('#search_type')[0].value;
    }
	var keyword = Sizzle('#search_keyword')[0].value;
    while (keyword.indexOf('.') == 0) {
        keyword = keyword.substring(1);
    }
    document.location = 'http://www.verycd.com/search/' + type + '/'+ encodeURIComponent(keyword);
	return false;
}
