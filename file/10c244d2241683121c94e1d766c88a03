	function ShowUl(ulid){
		if($("#li_"+ulid).attr("class")=='collapsable'){
			$("#li_"+ulid).removeClass('collapsable');
			$("#li_"+ulid).addClass('expandable');
			$("#ul_"+ulid).hide();
		}else{
			$("#li_"+ulid).addClass('collapsable');
			$("#li_"+ulid).removeClass('expandable');
			$("#ul_"+ulid).show();
		}
	}
</script>
