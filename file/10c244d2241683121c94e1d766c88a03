//语言跳转菜单
$(document).ready(function(){
	$(".lang_menu").hover(function(){
		$(this).find("ul.lang_list").show();//鼠标滑过查找div.lang_menu下面的ul然后显示；
	},function(){
		$(this).find("ul.lang_list").hide();//鼠标离开隐藏div.lang_menu下面的ul；
	});
	
	//nav
	$("#navarea li").hover(function () { $(this).find("div:eq(0)").show(); }, function () { $(this).find("div:eq(0)").hide(); }); 
	$("#navarea li ul li:last-child").css("background","none");
	
	//滚动
	$('.scroll_t').click(function(){$('html,body').animate({scrollTop: '0px'}, 500);}); 

	//九宫格图片相册
	$("a[rel=img_group]").fancybox({
		'transitionIn'		: 'none',
		'transitionOut'		: 'none',
		'titlePosition' 	: 'over',
		'titleFormat'		: function(title, currentArray, currentIndex, currentOpts) {
			return '<span id="fancybox-title-over">图片 ' + (currentIndex + 1) + ' / ' + currentArray.length + (title.length ? ' &nbsp; ' + title : '') + '</span>';
		}
	});

})

