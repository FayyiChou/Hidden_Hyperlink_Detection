        </div>
        <!--注册结果弹出DIV End-->
        <script type="text/javascript">
            loadCheckCode(2,'login_img');
            loadCheckCode(1,'reg_img');
            var submitFlag=1;
            function loadCheckCode(at, tar) {
                var settings = {
                    type: "post",
                    url: "/enroll/dyn/checkcode.json",
                    dataType: "json",
                    contentType: 'application/json;charset=UTF-8',
                    data: JSON.stringify({
                        active_type: at
                    }),
                    success: function(data) {
                        $("#" + tar).attr("src", data.data.url);
                    },
                    error: function() { }
                };
                $.ajax(settings);
            }
            function validateRegisterParam() {
                var user_name = $("#user_name").val();
                var password = $("#password").val();
                var repeat_password = $("#repeat_password").val();
                var people_name = $("#people_name").val();
                var mobile = $("#mobile").val();
                var email = $("#email").val();
                var checkcode = $("#check_code").val();
                if (!$("#agree")[0].checked) {
                    alert("请您认真阅读用户注册协议,并选择同意");
                    return false;
                }
                if (user_name == "") {
                    alert("用户名不能为空！");
                    return false;
                }
                if (isUserName(user_name) == "") {
                    alert("用户名只能为字母、数字、下划线！");
                    return false;
                }
                if (!checkLength(user_name, 4, 18)) {
                    alert("用户名长度为4-18个字符！");
                    return false;
                }
                if (password == "") {
                    alert("密码不能为空！");
                    return false;
                }
                if (!checkLength(password, 6, 16)) {
                    alert("密码长度为6-16个字符！");
                    return false;
                }
                if (repeat_password == "") {
                    alert("确认密码不能为空！");
                    return false;
                }
                if (password != repeat_password) {
                    alert("两次输入的密码不一致！");
                    return false;
                }
                if (people_name == "") {
                    alert("真实姓名不能为空！");
                    return false;
                }
                if (!checkLength(people_name, 1, 50)) {
                    alert("真实姓名长度为1-50个字符！");
                    return false;
                }
                if (!isChinaStr(people_name)) {
                    alert("真实姓名必须为汉字！");
                    return false;
                }
                if (mobile == "") {
                    alert("手机号不能为空！");
                    return false;
                }
                if (!isMobile(mobile)) {
                    alert("手机号格式不正确！");
                    return false;
                }
                if (email == "") {
                    alert("邮箱不能为空！");
                    return false;
                }
                if (!checkLength(email, 5, 45)) {
                    alert("email长度为5-45个字符！");
                    return false;
                }
                if (!isEmail(email)) {
                    alert("邮箱格式不正确！");
                    return false;
                }
                if (checkcode == "") {
                    alert("验证码不能为空！");
                    return false;
                }
                return true;
            }
            function getRegisterParam() {
                var obj = {
                    "user_name": $("#user_name").val(),
                    "password": $("#password").val(),
                    "repeat_password": $("#repeat_password").val(),
                    "people_name": $("#people_name").val(),
                    "mobile": $("#mobile").val(),
                    "email": $("#email").val(),
                    "checkcode": $("#check_code").val()
                };
                return obj;
            }
            function register() {
              
                if (!validateRegisterParam()) {
                    return;
                }
                if( submitFlag==1){        
                   submitFlag=2;
                } else{
                  return;
                }
                var username = $("#user_name").val();
                var settings = {
                    type: "post",
                    url: "/enroll/dyn/register.json",
                    dataType: "json",
                    contentType: 'application/json;charset=UTF-8',
                    data: JSON.stringify(getRegisterParam()),
                    success: function(data) {
                        if (data.flag == 1) {
                            afterRegisterSuccess();
                        } else {
                            alert(data.message);
                        }
                         submitFlag=1;
                    },
                    error: function() {
                         submitFlag=1;
                    }
                };
                $.ajax(settings);
            }
            function afterRegisterSuccess(){
                window.scrollTo(0, 0);
                $("#box-popup").css("display", "block");
                $("#user_name").val("");
                $("#password").val("");
                $("#repeat_password").val("");
                $("#people_name").val("");
                $("#mobile").val("");
                $("#email").val("");
                $("#check_code").val("");
            }
            function validateLoginParam() {
                var username = $("#l_username").val();
                var password = $("#l_password").val();
                var checkcode = $("#l_check_code").val();
                if (username == "") {
                    alert("用户名不能为空！");
                    return false;
                }
                if (password == "") {
                    alert("密码不能为空！");
                    return false;
                }
                if (checkcode == "") {
                    alert("验证码不能为空！");
                    return false;
                }
                return true;
            }
            function getLoginParam() {
                var obj = {
                    "username": $("#l_username").val(),
                    "password": $("#l_password").val(),
                    "checkcode": $("#l_check_code").val()
                };
                return obj;
            }
            function login() {
                if (!validateLoginParam()) {
                    return;
                }
                var settings = {
                    type: "post",
                    url: "/enroll/dyn/login.json",
                    dataType: "json",
                    contentType: 'application/json;charset=UTF-8',
                    data: JSON.stringify(getLoginParam()),
                    success: function(data) {
                        if (data.flag == 1) {
                            location.href = "/enroll/dyn/enroll/viewEnroll.htm";
                        } else {
                            alert(data.message);
                        }
                    },
                    error: function() { }
                };
                $.ajax(settings);
            }
            function go2Login(){
                $("#box-popup").css("display", "none");
                $("#l_username")[0].focus();
            }
        </script>
    
</body></html>