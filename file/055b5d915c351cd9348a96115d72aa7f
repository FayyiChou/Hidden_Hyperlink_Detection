	<script>
		$(function(){
			$(".loading_box_bj").css({height:$(window).height()+"px",width:$(window).width()+"px"});
			yz();//表单验证
			qq();//表单验证

			// 鼠标移动至下载APP展示图片
			$(".c_shows").hover(function(){
				$(".c_down_kik").slideDown(200);
			},function(){
				$(".c_down_kik").slideUp(200);
			}) 
			
			//表单验证
			function yz() {
				$("input").blur(function() {
					var index = $(this).index("input");
					if ($(this).val() == "") {
						$(this).parent().css({
							border : "1px solid #E86969"
						})
						$($(".b_add_yz i")[index]).css({
							background : "url(/static/img/icon/false.png)"
						});
						// 以下处理登录按钮不会增加边框
						$("#nowLogin").parent().css({border : "0px solid #E86969"})
						$("#imgCodeSure").parent().css({border : "0px solid #E86969"})
					} else {
						$(this).parent().css({
							border : "1px solid #0697DA"
						})
						$($(".b_add_yz i")[index]).css({
							background : "url(/static/img/icon/true.png)"
						});
						// 以下处理登录按钮不会增加边框
						$("#nowLogin").parent().css({border : "0px solid #E86969"})
						$("#imgCodeSure").parent().css({border : "0px solid #E86969"})
					}
				})
			}

			//qq弹出框
			function qq() {
				$(".b_qq_login").width($("body").width());
				$(".b_qq_login").height($("html").height());
				if ($(window).width() > 1200) {
					$(".b_qq_login_con").css(
							{
								right : ($(window).width() - $(".b_login_con")
										.width()) / 2 + 43
							});
				} else {
					$(".b_qq_login_con").css({
						right : 0
					});
				}
				$(window)
						.resize(
								function() {
									$(".b_qq_login").width($("body").width());
									$(".b_qq_login").height($("html").height());
									if ($(window).width() > 1200) {
										$(".b_qq_login_con")
												.css(
														{
															right : ($(window)
																	.width() - $(
																	".b_login_con")
																	.width()) / 2 + 43
														});
									} else {
										$(".b_qq_login_con").css({
											right : 0
										});
									}
								})
			}

			//点击QQ执行弹出框
			/* $(".b_login_xz_qq").click(function() {
				$(".b_qq_login").css({
					display : "block"
				});
			}) */
			//点击关闭弹出框
			$(".b_qq_close").click(function() {
				$(".b_qq_login").css({
					display : "none"
				});
			})
		
			// S 表单验证 2015-11-13 添加
			$(".b_login_input input").blur(function() {
				if ($(this).val() == "") {
					$(this).nextAll(".b_error").show().delay(1500).hide(0); //错误信息显示1秒后自动隐藏
				}
			})
			//点击错误信息时，错误信息隐藏，即可输入信息
			$(".b_login_input p").click(function() {
				$(this).hide();
			})
			//E 表单验证 2015-11-13 添加
		})
	</script>
