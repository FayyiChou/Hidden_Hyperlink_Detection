	<script>
		var messenger = new Messenger('parent', 'LIKEWED');
		messenger.listen(function(msg) {
			var href = msg + window.location.href;
			window.location.href = href;
		});

		function showLogin(redirect_uri) {
			dialogLogin(redirect_uri);
		}

		$(function() {
			var global_login_type;

			function jump(href) {
				window.location.href = href;
			}

			//顶部搜索
			$("#js_topsearch_submit").click(function() {
				var type = $("#js_search_type").text();
				var $form = $("#js_topsearch_form");
				if (type == "商家")
					$form.attr("action", "/partners/filter/search/lists");
				else
					$form.attr("action", "/posts/filter/search");

				$form.submit();
			});


			$(".search_type").click(function() {
				//作品
				if ($(this).data("value") == "shop") {
					var $form = $("#js_topsearch_form");
					$form.attr("action", "/partners/filter/search/lists");
					$form.submit();
				}
				//商家
				else if ($(this).data("value") == "work") {
					var $form = $("#js_topsearch_form");
					$form.attr("action", "/posts/filter/search");
					$form.submit();
				} else if ($(this).data("value") == "article") {
					var $form = $("#js_topsearch_form");
					$form.attr("action", "/articles/filter/search");
					$form.submit();
				} else {
					return false;
				}
			});
		});
	</script>
