<script language="javascript">
var now = new Date();
var _year = now.getFullYear();
var _m = now.getMonth()+1;
var _d = now.getDate();
var  _yi='';
var  _ji='';

_month = _m<10?"0"+_m:_m;
_day = _d<10?"0"+_d:_d;
_index = "d"+_month+_day;
_y = lmanac[_year][_index]['y'];
for(_i=0;_i<=_y.length-1;_i++){
	_yi+='<em style="cursor: pointer;" onmouseout="unhuangliinfo()" onmouseover="huangliinfo(\''+_y[_i]+'\',this)">'+_y[_i]+'</em>';
}

_j = lmanac[_year][_index]['j'];
for(_i=0;_i<=_j.length-1;_i++){
	_ji+='<em style="cursor: pointer;" onmouseout="unhuangliinfo()" onmouseover="huangliinfo(\''+_j[_i]+'\',this)">'+_j[_i]+'</em>';
}
$("#yi").html(_yi);
$("#ji").html(_ji);
function huangliinfo(name,evt){
	var top = Math.ceil($(evt).offset().top);
	var left = Math.ceil($(evt).offset().left);
	$("#tip").css("left",(left+5)+"px");
	$("#tip").css("top",(top+15)+"px");
	$("#tip").show();
	$("#huangli_content").html(huangli[name]);
}
function unhuangliinfo(){
	$("#tip").hide();
	$("#huangli_content").html('');
}
</script>
