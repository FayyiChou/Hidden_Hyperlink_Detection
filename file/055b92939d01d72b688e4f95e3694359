<script type="text/javascript">
function ietester() {
var undef,
	ie,
	v = 3,
	div = document.createElement('div'),
	all = div.getElementsByTagName('i');
while (
		div.innerHTML = '<!--[if gt IE ' + (++v) + ']><i></i><![endif]-->',
		all[0]
	);
v > 4 ? ie = v : ie = undef;
return ie;
}

$(function () {
if ($(".fix_div").length > 0) {
	var top = $(".fix_parent").offset().top;
	var left = $(".fix_parent").offset().right;

	var ie = ietester();

	$(window).scroll(function () {
		var limit = $(".clearfix").offset().top + $(".clearfix").height() - $(".fix_div").height();
		if ($(window).scrollTop() >= limit) {
			$(".fix_div").css({ position: "absolute", top: limit, right:"50%", marginRight: "-481px"  });

		} else {
			if (top <= $(window).scrollTop()) {
				if (ie <= 6) {
					$(".fix_div").css({ position: "absolute", top: ($(window).scrollTop()), right: "50%", marginRight:"-481px" });
				} else {
					$(".fix_div").css({ position: "fixed", top: 0, right:"50%", marginRight: "-481px" });
				}
			} else {
				$(".fix_div").css({ position: "static", top: 0, marginRight: "0px" });
			}
		}
	});
}
});
</script>





