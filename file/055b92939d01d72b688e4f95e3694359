$.getJSON('/plugins/rili/action.php',{"date":cdate,"type":"unload"},function(data){
	var s='';
	var d='';
	$.each(data.good,function(i,n){s+=d+n;d='，';});
	$("div.hl p.yi").html(s);
	s='';d='';
	$.each(data.bad,function(i,n){s+=d+n;d='，';});
	$("div.hl p.ji").html(s);
});

