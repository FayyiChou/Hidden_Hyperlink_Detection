<script type="text/javascript">
function tab_m(obj,name)
{
var p = obj.parentNode.getElementsByTagName("li");
var p1 = document.getElementById(name).getElementsByTagName("div");
for(i=0;i<p1.length;i++)
{
if(obj==p[i])
{
p[i].className = "s";
p1[i].className = "dis";
}
else
{
p[i].className = "#";
p1[i].className = "undis";
}
}
}
</script>



