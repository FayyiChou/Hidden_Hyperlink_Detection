<script type="text/javascript">
	loginhander("v_6.0", {base : "http://fd.llzg.cn",res : "http://image.llzg.cn"});
	function loadBoundary() {
		jQuery.ajax({
			type : "GET",
			url : "http://fd.llzg.cn/map/loadsiteboundary",
			data : {},
			success : function(result) {
				if (typeof result.boundary != "undefined"
						&& result.boundary.length > 0) {
					drawBoundary(result.boundary);
				} else {
					alert("读取地区失败!");
				}
			}
		});
	}
	function drawBoundary(boundary) {
		if (boundary != null && boundary.length > 6) {
			var bps = new Array();
			var bs = boundary.split(",");
			//myMap.clearOverlays();
			for ( var i = 0; i < bs.length; i = i + 2) {
				bps.push(new BMap.Point(bs[i], bs[i + 1]));
			}
			var ply = new BMap.Polygon(bps, {
				strokeWeight : 1,
				strokeColor : "#0000cc",
				fillColor : "#ccccff",
				fillOpacity : 0.3
			}); //建立多边形覆盖物
			myMap.addOverlay(ply); //添加覆盖物
			myMap.setViewport(ply.getPath());
		}
	}
	$(function() {
		// 下拉菜单
		$(".dropdown").each(function(i, e) {
			e = $(e);
			e.mouseenter(function() {
				e.addClass("open");
			}).mouseleave(function() {
				e.removeClass("open");
			});
		});
		$(".slitherWrap").hover(function() {
			$(this).find(".slither").stop().slideDown(500);
		}, function() {
			$(this).find(".slither").stop().slideUp(500);
		});
	})
	function closewin() {
		$(".floatWindow1").hide();
	}
	$(document).ready(function() {
		//点击提交
		$("#button2").click(function() {
			var startaddress  = $("#textfield2").val();
			var destinationaddress  = $("#textfield3").val();
			var url = "http://fd.llzg.cn/xinxi/pc/"+startaddress+"-"+destinationaddress+"-0-0-0.html";
			encodeUrl(url);
		});
	});
</script>
