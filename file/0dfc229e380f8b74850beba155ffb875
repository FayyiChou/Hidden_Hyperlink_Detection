        <script>
            var signInBottom=document.getElementById('signInBottom');
            var timer=null;
            clearTimeout(timer);
            timer=setTimeout(function(){
                signInBottom.style.display='block';
                $(".footerWrap").css("margin-bottom",'100px')
            },15000);
            var oClose=document.getElementById('close');
            oClose.onclick=function(){
                signInBottom.style.display='none';
            };
            function login(){
                var username=$('#j_username').val()
                var password=$('#j_password').val()
                if(username.length==0){
                    alert('请输入用户名')
                    return
                }
                if(password.length==0){
                    alert('请输入密码')
                    return
                }
                $("#loginFrame").attr('action',CAS_SERVER + "/login?type=ajax&service="+escape(CAS_SERVICE)).submit()

            }
        </script>
