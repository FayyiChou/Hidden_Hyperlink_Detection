<script type="text/javascript">
var nowtime = "02/18/2017 06:12:57";
var noimage = "/include/common/images/noimage.jpg";
$(function(){
//--中栏tab切换--
$(".board:not(.box-3)").each(function(i){
var $panel = $(this);
var $tabs = $panel.find(".title").find(".tabs");
var $more = $panel.find(".title").find(".more");
var $lists = $panel.find(".lists");
//标签切换
$tabs.on("mouseover", "a", function(){
var i = $tabs.find("a").index(this);
$(this).addClass("active").siblings("a").removeClass("active");
$more.find("a").eq(i).addClass("active").siblings("a").removeClass("active");
$lists.find(".uls").eq(i).addClass("active").siblings(".uls").removeClass("active");
});
});
//--右边栏政务公开tab切换--
$(".box-4").each(function(i){
var $panel = $(this);
var $tabs = $panel.find(".title");
var $lists = $panel.find(".lists");
//标签切换
$tabs.on("mouseover", "a", function(){
var i = $tabs.find("a").index(this);
$(this).addClass("active").siblings("a").removeClass("active");
$lists.find(".uls").eq(i).addClass("active").siblings(".uls").removeClass("active");
});
});
//--右边栏政务公开tab切换--
$(".jz-show").each(function(i){
var $panel = $(this);
var $tabs = $panel.find(".page dd");
var $lists = $panel.find("table");
//标签切换
$tabs.on("mouseover", "a", function(){
var i = $tabs.find("a").index(this);
$(this).addClass("active").siblings("a").removeClass("active");
$lists.find("tbody").eq(i).addClass("active").siblings("tbody").removeClass("active");
});
});
//--底部网站链接tab切换--
$(".link, .box-6").each(function(i){
var $panel = $(this);
var $tabs = $panel.find(".tabs");
var $lists = $panel.find(".lists");
//溢出可见
$panel.on("mouseenter", function(){
$(".uls", this).addClass("expand");
}).on("mouseleave", function(){
$(".uls", this).removeClass("expand");
});
//标签切换
$tabs.on("mouseover", "a", function(){
var i = $tabs.find("a").index(this);
$(this).addClass("active").siblings("a").removeClass("active");
$lists.find(".uls").eq(i).addClass("active").siblings(".uls").removeClass("active");
});
}).find(".lists").find("a").each(function(j){
this.title = this.innerText;
});
//--给列表标上“新”图标--
$(".news-list ul,.box-2 ul,.box-3 ul").each(function(i){
var $ul = $(this);
var marknewtimes = parseInt($ul.attr("marknewtimes")) || 1;
$ul.find("li").each(function(j){
var dtStr = $("span", this).attr("time"); //发文时间 String
if (Math.abs(dtDiff(dtStr, window.nowtime, "day")) <= marknewtimes) { //最新24小时的标new
$(this).addClass("new");
}
});
});
//--滚动标语--
jsScroll(20, document.getElementById("notice-obox"), "ul", "H");
Cms.siteFlow("", location.href, document.referrer);
});
</script>
