<!-- 业务查询 -->
<div class="yw-search">
<div class="yw-hd"><a href="/inquiry/index.jspx" target="_blank">业务查询</a></div>
<div class="yw-bd">
<div class="yw-bd-t"><div class="line"></div>▪ <span>业务办理进展查询</span></div>
<script type="text/javascript">
//--“业务进展查询”表单验证--
function validateTaskSearchForm(form) {
var eTitle = form.elements["title"];
if (eTitle.value == "") {
alert("请输入任务名称");
eTitle.focus();
return false;
}
return true;
}
</script>
<div class="box bx1">
<form name="taskSearchForm" action="/inquiry/task/search.jspx" method="post" target="_blank" onsubmit="return validateTaskSearchForm(this);">
<kbd><label>任务名称：</label><input type="text" name="title" class="txt" /></kbd>
<button type="submit">提 交</button><button type="reset">重 输</button>
</form>
</div>
<div class="yw-bd-t"><div class="line"></div>▪ <span>登记公开查询</span></div>
<script type="text/javascript">
//--“土地登记公开查询”表单验证--
function validateRegisterSearchForm(form) {
var eParcelId = form.elements["parcelId"];
var eOwnerName = form.elements["ownerName"];
var eCertNo = form.elements["certNo"];
var eParcelLocation = form.elements["parcelLocation"];
var count = 0;
if (eParcelId.value != "") count++;
if (eOwnerName.value != "") count++;
if (eCertNo.value != "") count++;
if (eParcelLocation.value != "") count++;
if (count < 2) {
alert("请至少输入两个查询条件");
return false;
}
return true;
}
</script>
