<script type="text/javascript" src="/include/common/js/jquery.jcryption.js"></script>
<script type="text/javascript">
function checkLogin(form) {
if (form.elements["username"].value == "") {
alert("请输入用户名！");
return false;
} else if (form.elements["password"].value == "") {
alert("请输入密码！");
return false;
} else if (form.elements["captcha"].value == "") {
alert("请输入验证码！");
return false;
} else {
try { form.elements["submitBtn"].disabled = true; } catch(e) {}
}
$.jCryption.getKeys(form.getAttribute("RSAKeysUrl"), function(receivedKeys) {
$.jCryption.encrypt(form.elements["password"].value, receivedKeys, function(encryptedPasswd) {
form.elements["password"].value = encryptedPasswd;
form.submit();
});
});
return false;
}
$(function(){
setTimeout(function(){document.forms["loginForm"].reset();}, 100); //消除浏览器的“自动完成”填写的用户密码
});
</script>
<form name="loginForm" action="/login.jspx" RSAKeysUrl="/getRSAKeys.jspo" method="post" autocomplete="off" onsubmit="return checkLogin(this);">
<input type="hidden" name="returnUrl" value="http://219.129.94.42:8008/"/>
用户名:<input type="text" class="txt" name="username" maxlength="100" autocomplete="off" />
密码:<input type="password" class="txt" name="password" maxlength="64" autocomplete="off" />
验证码:<input type="text" name="captcha" class="txt" maxlength="4" autocomplete="off" style="width:40px;" placeholder="输入" onkeypress="if(event.keyCode==13){this.form.elements['submitBtn'].click();}"
/><img class="capimg" src="/captcha.svl" onclick="this.src='/captcha.svl?d='+new Date()*1" width="60" height="20" />
