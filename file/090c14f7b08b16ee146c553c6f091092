<script type="text/javascript">
$(function(){
//当选中某个栏目时，将其名显示到下拉箭头旁
$(".search").find("input:radio[name='cgId']").on("change", function(){
$(".search .select .title").text($(this).parent().find("label").text());
})
//鼠标经过下拉箭头展示栏目列表
.end().find(".select").hover(function(){
$(this).addClass("active");
},function(){
$(this).removeClass("active");
});
});
</script>
