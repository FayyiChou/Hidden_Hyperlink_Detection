    /*我司设计稿基准w:640 @2x
     *前端计算时页面宽度基准320，font-size:20px
     *依此递增iphone6: fs = 375/320*20
     */
    ;
    (function(win, doc) {
        var UA = navigator.userAgent,
            isAndroid = /android|adr/gi.test(UA),
            isIos = /iphone|ipod|ipad/gi.test(UA) && !isAndroid;
        var docEl = doc.documentElement;
        var refreshRem = function() {
                var w = docEl.getBoundingClientRect().width || 320;
                var fs = w / 320 * 20;
                fs = fs > 40 ? 40 : fs;
                docEl.style.fontSize = fs + 'px';
                window.fontSize = fs;
            },
            refreshRemId;
        // alert(docEl.getBoundingClientRect().width + '--'+ document.documentElement.clientHeight)
        win.addEventListener('resize', function() {
            clearTimeout(refreshRemId);
            refreshRemId = setTimeout(refreshRem, 100);
        }, false);
        win.addEventListener('pageshow', function(e) {
            if (e.persisted) {
                clearTimeout(refreshRemId);
                refreshRemId = setTimeout(refreshRem, 100);
            }
        }, false);
        refreshRem();
    })(window, document);
    </script>
