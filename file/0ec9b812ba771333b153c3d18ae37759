        <script>
            function addStyle(e, style) {
                e.className += ' ' + style;    
            }
            
            function removeStyle(e, style) {
                var s = '';
                var cs = e.className.split(' ');
                for (var i = 0; i < cs.length; i ++)
                    if (cs[i] != style) s += cs[i];
                e.className = s;
            }
            
            function hasStyle(e, style) {
                var cs = e.className.split(' ');
                for (var i = 0; i < cs.length; i ++)
                    if (cs[i] == style) return true;
                return false;    
            }

            var seqs = document.getElementById('seqs');
            function fixCurrent() {
                if (seqs) {
                    var as = seqs.getElementsByTagName('a');
                    for (var i = 0; i < as.length; i ++)
                        if (as[i].className == 'current') {
                            seqs.scrollLeft = as[i].offsetLeft - parseInt(seqs.clientWidth * 0.03);
                            break;
                        }
                }
            }
            if (seqs && seqs.scrollWidth > seqs.clientWidth) {
                removeStyle(document.getElementById('foot'), 'none-more');
                fixCurrent();
            }

            function showMore() {
                addStyle(document.getElementById('foot'), 'more');
            }

            function hideMore() {
                removeStyle(document.getElementById('foot'), 'more');
                fixCurrent();
            }
        </script>
    </div>
        
