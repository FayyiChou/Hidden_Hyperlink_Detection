<script>
(function(scope){
	scope.getElement('button[type=submit]').addEvent('click', function(e){
		if(scope.getElement('input').value.trim() === '') {
			e.stop();
		}
		//cnzz 全景统计
		dplus.define('inSearch', function(inSearch){
			inSearch.hitSearch({
			  keyword:scope.getElement('input').value.trim(),
			  '$st':'明星周边-输入搜索词',
			  '$cg':document.title
			});
		});
	});
	var search_keywords = decodeURIComponent(Memory.get('search_key'));
	if(search_keywords == 'null'){
		search_keywords = '';
	}
	//scope.getElement('input[name=search_keywords]').value = search_keywords;

		
	//cnzz搜索框下方关键词事件
	$('hot-keywords').getElements('a').each(function(obj){
		obj.addEvent('click', function(e){
			dplus.define('inSearch', function(inSearch){
				inSearch.hitSearch({
					keyword:obj.get('text'),
					'$st':'明星周边-搜索框下提示词',
					'$cg':document.title
				});
			});
		});
	});
	
})($('searchbar_71'));
