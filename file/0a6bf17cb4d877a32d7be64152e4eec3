		var advChannel = $.cookie('advChannel');
	} else {
		var advChannel = document.referrer;
		var advDomain  = 'wasu.cn';
		for(var i = 0; i <= channels.length; i++) {
			if(advChannel.indexOf(channels[i]) != -1) {
				advChannel = channels[i];
				$.cookie('advChannel',advChannel,{domain:advDomain});
				break;
			}
		}
	}
	var ac_content_targeting = $.inArray(advChannel,channels) == -1 ? "" : "付费引流渠道:" + advChannel + "";
</script>
