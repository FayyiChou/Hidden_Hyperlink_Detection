<script language="javascript">
$(document).ready(function (){
    var object,timeout;
    $(".nav_a").hover(
      function () {
        object = $(this).siblings("ul");
        l = $(this).position().left;
        t = $(this).position().top + $(this).height();
        object.css({"left":l,"top":$(this).position().top,"opacity":0,"display":"block"});
        object.animate( { top: '+'+t,opacity:1 } , 500 );
      },
      function () {
        var oldObject = $(this).siblings("ul");
        timeout = setTimeout(function (){oldObject.animate({top: '0px',opacity:0 ,queue: false},100,function (){oldObject.hide();});},100);
      }
    ); 
    $(".show_nav_ul").hover(
    function (){
        clearTimeout(timeout);
    },
    function (){
        var oldObject = $(this);
        timeout = setTimeout(function (){oldObject.animate({top: '0px',opacity:0 ,queue: false},100,function (){oldObject.hide();});},100);
    }
    );
    
    $(".showTag").hover(function (){
        $(".showTag").removeClass("t_online t2_online");
        i = $(".showTag").index($(this));
        className = (i == 0) ? "t_online" : "t2_online";
        $(this).addClass(className);
        $(".showMarquee").hide();
        $(".showMarquee").eq(i).show();
    },function (){
        
    });
});
</script>
