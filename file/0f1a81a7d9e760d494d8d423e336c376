<script>
//评论框获取焦点
jQuery('#commentq .txt').bind('focus',function(){
	jQuery(this).animate({height:'46px',marginBottom:'20px'},500);
	jQuery('#commentq .list').animate({height:'281px'},500);
	jQuery(this).next().addClass('submit');
	jQuery('#commentq .face').show();
	jQuery('.smilies').hide();
});
//评论框失去焦点
jQuery('#commentq .txt').bind('blur',function(){
	var ts = jQuery(this);
	if(ts.val()==""){
		var t = setTimeout(function(){
    		ts.animate({height:'26px',marginBottom:'0'},500);	
    		jQuery('#commentq .list').animate({height:'321px'},500);
    		ts.next().removeClass('submit');
    		jQuery('#commentq .face').hide();
		},500);
		jQuery('#commentq .face').click(function(){
			clearTimeout(t);
		});
	}
	else{
		return false;
		}
});


</script>
<script>
        //文字展开
       (function($){
    	   var pBox = $('.main-tit dd p');
           var p_flog = true;
           $('.main-tit dd p').find('i').on('click',function(){
               if(p_flog){
                   $(this).addClass('close');
                   pBox.css({'height':'auto'})
                   p_flog = false;
               }else{
                   $(this).removeClass('close');
                   pBox.height(24);
                   p_flog = true;
               }
           })
           var nowpage = 1;
           if(nowpage<=2){
           	nowpage = 0;
           }else{
           	nowpage-=2;
           }
           //右侧轮播
           $('.scrollpics_dome').jCarouselLite({
               btnPrev: '#scrollPrev',
               btnNext: '#scrollNext',
               visible:3,
               speed:500,
               vertical:true,
               circular:false,
               start:nowpage
           });
           $('#scrollPrev').on('click',function(){
           	var topval = parseInt($('.scrollpics_dome ul').css('top'));
           	if(Math.abs(parseInt(topval/133))!=0){
           		var index = Math.abs(parseInt(topval/133))-3;
           		var imgload = $('.scrollpics_dome ul li').eq(index-1).find('img');
           		imgLoadFn(imgload,index,true);
           	}
           });
           $('#scrollNext').on('click',function(){
           	var topval = parseInt($('.scrollpics_dome ul').css('top'));
           	var index = Math.abs(parseInt(topval/133))+3;
           	if(Math.abs(parseInt(topval/133))<=$('.scrollpics_dome ul li').length-3){
           		var imgload = $('.scrollpics_dome ul li').eq(index).find('img');
           		imgLoadFn(imgload,index,false);
           	}
           });
       })(jQuery)
        function imgLoadFn(imgEle,index,loadtype){
        	if(imgEle.attr('src')==''){
        		var tid    = jQuery("#tid").val();
            	var limit  = jQuery("#limit").val();
            	var limitNew = loadtype ? index - 1 : index;
        		jQuery.ajax({
    				'type':'POST',
    				'dataType':'json',
    				'data':{'tid':tid,'limit':limitNew,'num':1000000,'size':1},
    				'url':'/ajaxAttachments.php',
    				'success':function(data){
    					if(data){
    						for(var key in data['scrs']){
    							if(!jQuery("#pic"+key).attr("src")) jQuery("#pic"+key).attr("src",data['scrs'][key]);
    						}
    					}
    				}
    			})
        	}
        }
