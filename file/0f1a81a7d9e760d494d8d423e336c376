	<script>
    	(function($){
    		//设置cookie
    		function setCookie(cname, cvalue, exdays) {
    		    var d = new Date();
    		    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    		    var expires = "expires="+d.toUTCString();
    		    document.cookie = cname + "=" + cvalue + "; " + expires;
    		}
    		//获取cookie
    		function getCookie(cname) {
    		    var name = cname + "=";
    		    var ca = document.cookie.split(';');
    		    for(var i=0; i<ca.length; i++) {
    		        var c = ca[i];
    		        while (c.charAt(0)==' ') c = c.substring(1);
    		        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
    		    }
    		    return "";
    		}
    		//清除cookie  
    		function clearCookie(name) {  
    		    setCookie(name, "", -1);  
    		}
    		var flog     = !getCookie('autoplay');
    		var timer    = null;
    		var hrefs    = "bbs.55bbs.com/gallery-5602706-1.html";
    		var nowPage  = 1;
    		var numPage  = 1;
    		var timeValue = 10000;
    		var notLastPage = nowPage==numPage ? false :true;
    		if(flog && notLastPage){
    			timer = setTimeout(function(){
    				window.location.href="http://"+hrefs ;
    			},timeValue);
    		}else if(flog && nowPage==numPage){
    			setTimeout(function(){
    				$("#out-of-range").show();
    			},timeValue);
    			
    		}
    		$('.pouse-btn').on('click',function(){
    			var flog = !getCookie('autoplay');
    			if(flog){
    				setCookie("autoplay",1,1);
    				clearTimeout(timer);
    				$(this).find('span').text('播放');
    				$(this).find('i').addClass('playicon');
	    			
    			}else{
    				timer = setTimeout(function(){
	    				window.location.href="http://"+hrefs ;
	    			},timeValue)
    				clearCookie("autoplay");
    				$(this).find('span').text('暂停');
	    			$(this).find('i').removeClass('playicon');
    			}
        	});
    		
    		//按键跳转下一页
        	document.onkeydown = function(e){
        		var prevPage = "/gallery-5602706-1.html";
        		var nextPage = "/gallery-5602706-1.html";
            	var e = window.event || e,
                curKey =  e.keyCode || e.charCode;
                if(curKey == 37) {
                    window.location.href = prevPage;
                }
                if(curKey == 39) {
                	if(nowPage==numPage){
                		showDiv();
                		return true;
                	}
                    window.location.href = nextPage;
                }
            }
    	})(jQuery)
    </script>
