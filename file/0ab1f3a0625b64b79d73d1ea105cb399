$(document).ready( function() {



    var lang = 'cn';



    var format = 'yyyy-mm-dd';



    var localize_cn = {



      months:        '一月,二月,三月,四月,五月,六月,七月,八月,九月,十月,十一月,十二月',



      shortMonths:   '一月,二月,三月,四月,五月,六月,七月,八月,九月,十月,十一月,十二月',



      days:          '周日,周一,周二,周三,周四,周五,周六',



      shortDays:     '日,一,二,三,四,五,六'



    };







    if (lang == 'cn')



       $.tools.dateinput.localize(lang, localize_cn);



    $('#pubDate').dateinput( {format: format, lang: lang , selectors: true} );



});



 $(document).ready( function() {



    $("#cate").val("11");



  });











var PanSelect   = function(){



    this.id     = 'ppid';



    this.arr    = [];



    this.arr.push({ 'category' : '12', 'id' : '544', 'name' : '白银' });



        this.arr.push({ 'category' : '12', 'id' : '767', 'name' : '铟' });



        this.arr.push({ 'category' : '12', 'id' : '923', 'name' : '锗' });



        this.arr.push({ 'category' : '12', 'id' : '765', 'name' : '铋' });



        this.arr.push({ 'category' : '12', 'id' : '135', 'name' : '钨精矿' });





    };







PanSelect.prototype.set = function( category ){



    $("#" + this.id ).empty();



//    $("<option value=''></option>").appendTo( $("#" + this.id ) );



		var index = 0;



    for( var i = 0; i < this.arr.length; i++ ){



			//if( _id == this.arr[i].id){



			//	index = i;



			//}



        if( this.arr[i].category == category || ! category ){



             $("#" + this.id ).append("<option value='" + this.arr[i].id + "'>" + this.arr[i].name + "</option>");



        }



    }



	//alert(index);



	$('#ppid').val("");



};



$(document).ready( function() {



    var panselect   = new PanSelect();



    $('#cate').change(function(){



        panselect.set(this.value);



    }).val('');



    panselect.set('');    //$('#ppid').val('490');



});



</script> 


