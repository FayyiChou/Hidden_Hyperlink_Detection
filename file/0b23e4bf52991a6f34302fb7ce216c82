    <script type="text/javascript">
        function Search() {
            var value = $("#searchKey").val();
            if (value == "" || value == undefined) {
                alert("请输入搜索关词键");
            } else {
                window.location = "/Article/Search?search=" + escape(value);
            }
        }
        $(document).ready(function () {
            $("#searchKey").keypress(function (event) {
                if (event.keyCode == 13) {
                    Search();
                }
            });
        });
    </script>


    
