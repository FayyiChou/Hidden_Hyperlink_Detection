<script type="text/javascript">
    $(document).ready(function () {
        $("#captcha").click(function () {
            resetCaptcha();
        });
        $("#icaptcha").click(function () {
            resetCaptcha();
        })
    })

    function resetCaptcha() {
        if ($('#captcha').length > 0) {
            $("#login-vcode").val('');
            //    $("#login-vcode").focus();
            $("#captcha").attr('src', '/Admin/Login/GetVCode?' + Math.round(Math.random() * 10000));
        }
    }
</script>
