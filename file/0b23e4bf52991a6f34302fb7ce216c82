<script type="text/javascript">
    $(document).ready(function () {
        jQuery.fn.extend({
            TitleShow: function (strHTML) {
                var xOffset = 10;
                var yOffset = 30;
                var preview = $("#preview_container");
                if (preview.length <= 0) {
                    $("body").append("<div id='preview_container'></div>");
                    preview = $("#preview_container");
                }
                /* 在这里设置漂浮容器的样式 */
                preview.css({
                    "display": "none"
                    , "position": "absolute"
                    , "width": "300px"
                });
                return this.each(function () {
                    var _this = $(this);
                    _this.hover(
                        function (e) {
                            preview.html(strHTML);
                            preview
                                .css("top", (e.pageY - xOffset) + "px")
                                .css("left", (e.pageX + yOffset) + "px")
                                .css("opaticy", 0)
                                .show()
                                .stop()
                                .animate({ "opacity": 0.9 }, 300);

                        }, function () {
                            preview
                                .stop()
                                .animate({ "opacity": 0 }, 300, function () {
                                    $(this).hide();
                                });
                        }
                    )
                    _this.mousemove(function (e) {
                        preview
                            .css("top", (e.pageY - xOffset) + "px")
                            .css("left", (e.pageX + yOffset) + "px");
                    });
                });
            }
        });
        var initCapacity = function () {
            $.ajax({
                type: 'post',
                url: '/Rainfall/GetRealWaterData',
                success: function (result) {
                    if (result.success) {
                        $("#img_fozilin").TitleShow("<div class='floatdiv'>佛子岭：" + result.data.fozilin + "</div>");
                        $("#img_mozitan").TitleShow("<div class='floatdiv'>磨子潭：" + result.data.mozitan + "</div>");
                        $("#img_bailianya").TitleShow("<div class='floatdiv'>白莲崖：" + result.data.bailianya + "</div>");
                        $("#img_fozilinbx").TitleShow("<div class='floatdiv'>佛子岭坝下：" + result.data.fozilinbx + "</div>");
                        $("#img_mozitanbx").TitleShow("<div class='floatdiv'>磨子潭坝下：" + result.data.mozitanbx + "</div>");
                        $("#img_bailianyabs").TitleShow("<div class='floatdiv'>白莲岩坝上：" + result.data.bailianyabs + "</div>");
                        $("#img_blianyancf").TitleShow("<div class='floatdiv'>白莲岩厂房：" + result.data.blianyancf + "</div>");
                        $("#img_hweihe").TitleShow("<div class='floatdiv'>黄尾河：" + result.data.hweihe + "</div>");
                    }
                }
            });
        };
        var initSituation = function () {
            $.ajax({
                type: 'post',
                url: '/Rainfall/GetRealWaterSituationData',
                success: function (result) {
                    if (result.success) {
                        $("#ss_fozilin").TitleShow("<div class='floatdiv'>佛子岭：" + result.data.fozilin + "</div>");
                        $("#ss_mozitan").TitleShow("<div class='floatdiv'>磨子潭：" + result.data.mozitan + "</div>");
                        $("#ss_bailianya").TitleShow("<div class='floatdiv'>白莲崖：" + result.data.bailianya + "</div>");
                        $("#ss_zhufoan").TitleShow("<div class='floatdiv'>诸佛庵：" + result.data.zhufoan + "</div>");
                        $("#ss_yuerjie").TitleShow("<div class='floatdiv'>与儿街：" + result.data.yuerjie + "</div>");
                        $("#ss_changshanchong").TitleShow("<div class='floatdiv'>长山冲：" + result.data.changshanchong + "</div>");
                        $("#ss_manshuihe").TitleShow("<div class='floatdiv'>漫水河：" + result.data.manshuihe + "</div>");
                        $("#ss_qianlisi").TitleShow("<div class='floatdiv'>千笠寺：" + result.data.qianlisi + "</div>");
                        $("#ss_shangtusi").TitleShow("<div class='floatdiv'>上土寺：" + result.data.shangtusi + "</div>");
                        $("#ss_taiyang").TitleShow("<div class='floatdiv'>太阳：" + result.data.taiyang + "</div>");
                        $("#ss_taipingfan").TitleShow("<div class='floatdiv'>太平畈：" + result.data.taipfan + "</div>");
                        $("#ss_kuotanhe").TitleShow("<div class='floatdiv'>阔滩河：" + result.data.kuotanhe + "</div>");
                        $("#ss_toutuo").TitleShow("<div class='floatdiv'>头陀：" + result.data.toutuo + "</div>");
                        $("#ss_hekousi").TitleShow("<div class='floatdiv'>河口寺：" + result.data.hekousi + "</div>");
                        $("#ss_hweihe").TitleShow("<div class='floatdiv'>黄尾河：" + result.data.hweihe + "</div>");
                        $("#div_time").html("(" + result.data.time + ")");
                    }
                }
            });
        };
        initCapacity();
        initSituation();
    });
</script>

