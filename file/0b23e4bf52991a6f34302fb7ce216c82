<script type="text/javascript">
    var realWater = function () {
        $.ajax({
            type: 'post',
            url: '/Rainfall/RealWater',
            success: function (result) {
                if (result.success) {
                    $("#fzilin").html(result.data.fozilin + '(m)');
                    $("#mzitan").html(result.data.mozitan + '(m)');
                    $("#blianya").html(result.data.bailianya + '(m)');
                    $("#fzilinW").css("width", result.data.preFozilin);
                    $("#mzitanM").css("width", result.data.preMozitan);
                    $("#blianyaB").css("width", result.data.preBailianya);
                }
            }
        });
    };
    $(document).ready(function () {
        //水库水位
        realWater();
    });
</script>
