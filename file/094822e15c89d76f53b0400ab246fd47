   </div> 
   <!--container结束--> 
  </div> 
  <div class="clearboth"></div> 
  <script>
var speed = 30;
	var tab = getElementsByClass('demo')
	var tab1 = getElementsByClass('demo1');
	var tab2 = getElementsByClass('demo2');
	for(var i = 0; i < tab.length; i++){
		tab2[i].innerHTML = tab1[i].innerHTML;
	}
	function Marquee(){
		for(var i = 0; i < tab.length; i++){
			if(tab2[i].offsetWidth-tab[i].scrollLeft<=0)
				tab[i].scrollLeft-=tab1[i].offsetWidth
			else{
				tab[i].scrollLeft++;
			}
		}
	}
	var MyMar=setInterval(Marquee,speed);
	
	for(var i = 0; i < tab.length; i++){
		tab[i].onmouseover=function() {
			clearInterval(MyMar)
		};
		tab[i].onmouseout=function() {
			MyMar=setInterval(Marquee,speed)
		};
	}
	
	function getElementsByClass(tClass){
		var tElementArray = document.getElementsByTagName('*');
		var tObj = [];
		for ( var i = 0, len = tElementArray.length; i < len; i++ ){
			if(tElementArray[i].className == tClass){
				tObj.push(tElementArray[i]);
			}
		}
		return tObj;
	}
</script> 
