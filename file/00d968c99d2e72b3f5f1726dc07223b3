$().ready(function() {  		
    $("#btn_price_form_submit").click(function() {
	  var v=$("#weight").val();
	  if(v==""){
		  alert("请填写重量");
		  $("#weight").focus();
		  return false;
	  }

	  if(isNaN(v)){
		  alert("重量必须是数字");
		  $("#weight").focus();
		  return false;
	  }
	  
	  $("#form_price_search").submit();
    });  

	$("#btn_login").click(function(){
		if($("#system_version").val()=="new"){
			$("#login_form").attr("action","http://ishipper.harpost.com:8080/ishipper/application/authenticate");
			$("#loginName").val($("#login_name").val());
			$("#loginPassword").val($("#password").val());
		}else{
			$("#login_form").attr("action","http://im.harpost.com/imail/haradmin/login_do.php");
		}
		$("#login_form").submit();
	});

   
}); 
