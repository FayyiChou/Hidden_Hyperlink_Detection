<script type="text/javascript">

  $().ready(function() {
    //$(".flexselect").flexselect();
    $("#btn_price_form_submit").click(function() {
	  var v=$("#weight").val();
	  if(v==""){
		  alert("请填写重量");
		  $("#weight").focus();
		  return false;
	  }

	  if(isNaN(v)){
		  alert("重量必须是数字");
		  $("#weight").focus();
		  return false;
	  }
	  
	  $("#form_price_search").submit();
    });  
   
}); 

</script>
