<script type="text/javascript">

//签到 AJAX
function ajaxCheckin(obj){
$(obj).unbind('click');
$(obj).removeAttr('onclick');
$(obj).html('签到...');
$.ajax({
		type: 'GET', 
        url: '/usersigndo.asp', 
        timeout: 8000,
		dataType:'json',
		data:{"action":'sign',"tmp":Math.random()},
        cache: false, 
        error: function(){
            alert('由于签到人数过多，导致拥堵，请稍后再试'); 
            return false; 
        },
		success:function(rtn) {
			if(rtn.error == "success"){
			$(obj).html('成功');
			$(obj).attr("className","mrqd_un");
			$(obj).unbind('click');
            $(obj).removeAttr('onclick');
			layer.tips(rtn.msg,document.getElementById("sign-btn"),{
                 style: ['background-color:#f40; color:#fff', '#f40'],
				 time: 6,
                 maxWidth:180
                });
				
			}else{
				layer.tips(rtn.msg, document.getElementById("sign-btn"), {
                 style: ['background-color:#f40; color:#fff', '#f40'],
				 time: 6,
                 maxWidth:180
                });
			}
		}
});
}
</script>
<script>
$(function(){
	function bannerFadeFun(){
		var banw=$("#banner1_wrap"), bans = banw.find(".banner"),points=$(".banners_point .point"),len=bans.length,index=1,timer;
		var switchBanner = debounce(function(){
			var $pre=bans.filter(".bancur"),$this = bans.eq(index),$point=points.eq(index);
			index++;
			bans.not(".bancur").css("z-index",8);
			$this.css("z-index",9);
			$pre.fadeOut('slow', function() {
				if (index>=len) {
					index=0;
				}
				$pre.removeClass("bancur").css("z-index",9).show();
				$this.addClass("bancur").css("z-index",10);
				points.removeClass("cur");
				$point.addClass("cur");
			});
		},200);
		var startSwitch = debounce(function(){
			if(!timer)
				timer=setInterval(switchBanner,2000);
		},200);
		var stopSwitch=debounce(function(){
			window.clearInterval(timer);
			timer = null;
		},200);
		startSwitch();
		points.hover(function(){
			stopSwitch();
			index = points.index($(this));
			switchBanner();
		},function(){
			startSwitch();
		});
		banw.hover(stopSwitch,startSwitch);
		bans.hover(stopSwitch,startSwitch);
	};
	bannerFadeFun();
});
</script><div class="login1_bg user_info_wrap">
	
