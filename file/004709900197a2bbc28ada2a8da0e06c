	<script>
$(function(){
	var imgWrap=$(".news_imgs_wrap"),
		bigImgs=imgWrap.find(".news_imgs > a"),
		imgNewsTitle=imgWrap.find("#news_title_a"),
		smallImgs = imgWrap.find(".ns_img"),
		pre=imgWrap.find(".ns_pre"),
		next=imgWrap.find(".ns_next"),
		smalls=imgWrap.find(".small_images"),
		smallImgCount = smallImgs.length,
		smallsWidth=smalls.width(),
		sImgLi=imgWrap.find(".sImgLi"),
		sImgLiWidth=sImgLi.width(),
		max=smallsWidth - sImgLiWidth,
	 	timeIndex = 1,
	 	timerFun=function(){
	 		if(timeIndex >= smallImgCount){
				timeIndex=0;
			}
			var bi = bigImgs.hide().eq(timeIndex).show();
			smallImgs.removeClass("on").eq(timeIndex).addClass("on");
			imgNewsTitle.html(bi.attr("title")).attr("href",bi.attr("href"));
			var left = parseInt(timeIndex/3) * -1 *smallsWidth;
			if(left < max){
				left = max;
			}
			sImgLi.css("left",left);
			timeIndex++;
		},
	 	timer;
	timer = setInterval(timerFun,2000);
	imgWrap.hover(function(){
		clearInterval(timer);
	},function(){
		timer=setInterval(timerFun,2000);
	});
	pre.click(function(){
		var left = parseInt(sImgLi.css("left"));
		if(left<0){
			left = left+smallsWidth;
			if(left>0)left=0;
			sImgLi.css("left",left);
		};
	});
	next.click(function(){
		var left = parseInt(sImgLi.css("left"));
		if(left > -1*sImgLiWidth){
			left = left - smallsWidth;
			if(left < max){
				left = max;
			}
			sImgLi.css("left",left);
		}
	});
	smallImgs.click(function(){
		var it = $(this),index = smallImgs.index(it);
		var bi = bigImgs.hide().eq(index).show();
		smallImgs.removeClass("on");
		it.addClass("on");
		timeIndex = index+1;
		imgNewsTitle.html(bi.attr("title")).attr("href",bi.attr("href"));
	});
});
</script>

