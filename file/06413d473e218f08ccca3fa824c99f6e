<script type="text/javascript">
$(function(){
	var $ld5 = $(".pc-select-L_2");					  
	$ld5.ld({ajaxOptions : {"url" : "http://www.yzwood.com/api.php?op=get_linkage&act=ajax_select&keyid=1"},defaultParentId : 0})	 
	var ld5_api = $ld5.ld("api");
	ld5_api.selected();
	$ld5.bind("change",onchange);
	function onchange(e){
		var $target = $(e.target);
		var index = $ld5.index($target);
		$("#L_1-3").remove();
		$("#L_2").val($ld5.eq(index).show().val());
		index ++;
		$ld5.eq(index).show();	
}
})
</script>
