<script>
function formSubmit(formId,textid)
{
	var Obj = document.getElementById("typeid");
	var Value = Obj.options[Obj.selectedIndex].innerText;
	if(Value=='搜索类型')
	{
		alert("请选择搜索类型");
		return false;
	}
	var content = trim(document.getElementById(textid).value);
	if(content=="" || content=="站内搜索")
	{
		alert("请填写搜索内容");
		return false;
	}
	document.getElementById(formId).submit();
}
function trim(str)
{
	return str.replace(/(^\s*)|(\s*$)/g, "");  
}

function getSelectValue2()
{
	var Obj = document.getElementById("L_1-T");
	var Value = Obj.options[Obj.selectedIndex].innerText;
	var Child_Obj = document.getElementById("L_1-T2");
	var Child_Value = Child_Obj.options[Child_Obj.selectedIndex].innerText;
	if(Value=="请选择" || Child_Value=="请选择")
	{
		alert("请选择您的城市哦！");
		return false;
	}
	document.getElementById("cityNameT").value = Value+Child_Value;
	document.getElementById("form2").submit();
}
</script>
