<script language="javascript" type="text/javascript"> 
var f_i=0,f_tmpAd;
function f_rollImg(){
    var tnum=2
    if(tnum>1){
    if(tnum==0){tnum=1;}
    var x=0,dstr=1; 
    f_i=f_i*1+dstr*1;
        if(f_i==tnum)
        {
            f_i=0;
        }
    x=(0-f_i)*140;
    $("#firendlink>ul").animate({marginLeft:x+"px"},600);
	f_tmpAd=setTimeout("f_rollImg()",3000);
	}
}
$(document).ready(function(){	
	$("#content_box>ul>li").hover(function(){
		$(this).children("div").show();
	},
	function(){
		$(this).children("div").hide();
	});
	f_tmpAd=setTimeout("f_rollImg()",3000);
	
})
</script>
