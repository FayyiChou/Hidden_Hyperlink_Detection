	   else if(window.sidebar){
	   window.sidebar.addPanel('和讯德扑 ', 'http://hoker2013.blog.hexun.com', "");
	   }
	   else{ 
	  alert('您可以通过快捷键' + ctrl + ' + D 加入到收藏夹');
	}
	   })
});</script>
