            $('.anserdh li').click(function() {
                cid = $(this).attr("id");
            });



        })
        function sech() {

            var key = $("#keys").val();
            var a = $(".input").html();
            if (a == '产品') {
                a = 1;
            }
            else {
                a = 0;
            }
            location.href = "/search/search.aspx?key=" + key + "&a=" + a;
        }
    </script>

