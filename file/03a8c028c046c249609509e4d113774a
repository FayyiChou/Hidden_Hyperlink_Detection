        $(function() {
            $("ul.tabTit li").click(function() {
                var index = $(this).index();
                $("ul.tabTit li").removeClass("cur");
                $(this).addClass("cur");
                $(".tabCon").hide();
                $(".tabCon").eq(index).show();
            }).eq(0).click();
        });
