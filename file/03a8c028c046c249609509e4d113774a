<script>

    $(function() {
        $(".nav li").hover(function() {

            if ($(this).index() != 2) {
                $(this).find('.sub').show();
            }

        }, function() {
            $(this).find('.sub').hide();
        });



        //        $(".nav li").find('.sub').find('ul li').hover(function() {
        //            $(this).find('ul').show();
        //        }, function() {
        //            $(this).find('ul').hide();
        //        });


        $(".nav li").find('.sub').find('ul li').find('p').find('a').hover(function() {

            //获得当前被点击的元素索引值
            var Index = $(this).parent().parent().index();

            //显示对应的div
            $(".nav li").find('.sub').find('ul li').find('p').find('a').parent().parent().parent().parent().children('div').eq(Index).show().siblings('div').hide();

        });
    });
</script>

