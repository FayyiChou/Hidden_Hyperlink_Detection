<script type="text/javascript">
$('.top span').click(function(){
	var spanhtml = $('.top span').html();
	if(spanhtml=="点击展开联系方式"){
		$('.top span').html('点击收起联系方式');
		$('.top').animate({
			width:'100%',
			height:'200px'
		},200);
		//图片展开
		$('.top .imgbox img').animate({
			width:'100px',
			height:'100px'
		},200);
		
	}else{
		$('.top span').html('点击展开联系方式');
		$('.top').animate({
			width:'1002px',
			height:'50px'
		},200,function(){
			//图片缩小
			$('.top .imgbox img').animate({
				width:'94px',
				height:'53px'
			},200);	
		});
	}
});
</script>

