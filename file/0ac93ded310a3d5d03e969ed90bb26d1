<script language="javascript">
	function get_smallclass(){
	var b=document.getElementById("x_b");
		for(var i=1;i<b.childNodes.length;i++){
		  if(b.childNodes[i].selected==true){
		  
				if (window.XMLHttpRequest){// code for IE7+, Firefox, Chrome, Opera, Safari
				  xmlhttp=new XMLHttpRequest();
				  }else{// code for IE6, IE5
				  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
				 }
				 
				xmlhttp.onreadystatechange=function(){
				  if(xmlhttp.readyState==4 && xmlhttp.status==200){
				   var result=xmlhttp.responseText;
				   var arr_result=result.split(",");
				   
				   change_option(arr_result,"x_s")
						
				  //alert(xmlhttp.responseText);
				  
/*						if(xmlhttp.responseText=="right"){
							changeBoxStyle('j_id');
							alert("操作成功！")
						}*/
					}
				}
				
				xmlhttp.open("GET","search_sonclass.asp?x_b="+b.childNodes[i].value+"&m="+Math.random(),true);
				xmlhttp.send();			   
		  
		  }
		}
	}

function change_option(arr,domid){
 var dom_id=document.getElementById(domid);
 dom_id.options.length=0;
 for(var i=0;i<arr.length; i=i+2){
  dom_id.options.add(new Option(arr[i],arr[i+1]));
 }
}
function so_key(){
 var x_b=document.getElementById("x_b").value;
 var x_s=document.getElementById("x_s").value;
 var x_m=document.getElementById("x_m").value;
 var x_k=document.getElementById("x_k").value;
 var to_url="http://so.51jiameng.com/?";
	 if (x_b!=''){
	   to_url+="hangye="+x_b;
	 }
	 if(x_s!=''){
	  to_url+="&hangyezilei="+x_s;
	 }
	 if (x_m!=''&& x_m.indexOf("-")>0){
	   var arr_xm=x_m.split("-");
	   to_url+="&money_down="+arr_xm[0]+"&money_up="+arr_xm[1];
	 }
	 if(x_k!=''){
	  to_url+="&s_key="+escape(x_k);
	 }
	// alert(to_url);
	// return;
  window.location.href=to_url;
}
