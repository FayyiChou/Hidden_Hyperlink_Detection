        + '<span class="fl mt04"><a href="/space/{uid}"><img onerror="this.onerror=null;this.src=\'http://'+"nuc.m1905.com"+'/images/noavatar_small.gif\'"  src="{face}" title="{uname}" uid="{uid}" imgtype="head" height="40" width="40" /></a></span>' 
        + ' <dl class="sar_Info"><p><span class="fr">{ctime}</span><a href="{space_url}">{uname}</a></p>'
        + '<p class="topic-card-content">{content}</p></dl>'
        + '<p class="topic-card-actions"><a href="javascript:;" onclick="weibo.{fav_status}({weibo_id},this)">{fav_text}</a><a class="pl20" href="/weibo/{weibo_id}">评论({comment})</a><a class="pl20" href="javascript:weibo.transpond({weibo_id})" tnum="{{weibo_id}}"  id="transpond_{{transpond}}"><i class="fs_normal">转发</i>({transpond})</a><span class="botton fr"><a class="green_bot02 btn" href="javascript:weibo.sendPrivateMsg({uid},\'{uname}\', $(\'[data-weiboid={weibo_id}]\'))">发私信</a></span></p>' 
        + '</div></div></div>',
        nocardTpl: '<div class="starcoment_Box_a"><div class="card-arrow"></div><div class="sp_coment"><div class="coment_D"><p class="ta_c">微博未审核或已删除</p></div></div></div>'
    });
});
</script>
    
