        <script type="text/javascript">
    $(function() {
            var is_tiped = true;
            function noticeHide(){
                if(is_tiped){
                    $.post("http://d.1905.com/index.php?app=home&mod=User&act=clearTipNumber");
                }
                $("#notice-new-content").animate({
                    "top": "19px"},
                    300, function() {  
                      $("#notice-new-content").hide();  
                });
                is_tiped = false;
            }
            $("#notice-new-content").click(function(e) {
                noticeHide();
                $(".seach-content").trigger('click');                  
                $(".seach-content").trigger('keyup');  
                 e.stopPropagation();             
            });
            $(".seach-content").keyup(function() {
                 noticeHide();                
                var dataQ = $('.seach-content').val();
                if (dataQ == '') {
                    dataQ = ' ';
                }
          $(".sh-sug").html('');
                $.post(U('films/Index/recomSearch'), {
                    q: dataQ
                }, function(data) {
              if ( data && data.length) {
                var UL = "<ul>";
                UL+='<li class="hd">最近热门</li>';
                $.map(data, function(item){
                            UL += '<li><a href="' + item.content + '"  target="_blank">' + item.title + '</a></li>';
                        });
                UL += "</ul>";
                $(".sh-sug").html(UL);
                        $("#fix_q").css({
                            "border": "1px solid #CDCDCD",
                            "padding": " 0",
                            "*height" :"auto"
                        });
                       
                         var  n = data.length < 3 ? data.length : 3;
                        for(i=1;i <= n;i++){                          
                            $(".n-header-middle .sh-sug").find('li').eq(i).append('<span class="li-marks">'+i+'</span>');
                            $(".n-header-middle .sh-sug").find('li').eq(i).children('a').append('<span class="seach-new"></span>');
                        }
                    } else {
                        $("#fix_q").css({
                            "border": "0 none",
                            "padding": "0",
                            "*height":"0"
                        });
                    }
                }, 'json');
				
            }); 
            $(document).delegate('#content_publish', 'focus', function(event) {
                $("#fix_q").hide();
                $(".search-classify").hide();
            });



    })
    function load_quick_login( type ){
     if(type == 'reg'){ 
        __popReg.initialize({type:2});//注册
        } else {
            __popReg.initialize({
                type: 1
            }); //登录 
        }
    }
</script>
