<script type="text/javascript">
	var num=1;
	function readMore(){
		num=num+1;
		$.ajax({    
	        type:'get',        
	        url:'http://m.cngold.org/api/channel/readmore/3010/'+num,    
	        success:function(data){
	        	$('#docs').append(data);
	        	if((data.split("</li>").length - 1) < 20){
	        	   $(".view-more").remove();
	        	}
	        }
	    });  
	}
</script>

