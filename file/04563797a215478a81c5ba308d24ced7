<script>
var scrollreset = false;
function scroll_func(){
scrolldiv = document.getElementById('scrollforumname');
scrolltext = document.getElementById('forumnametext');
if(scrolltext.offsetWidth - 400 <= scrolldiv.scrollLeft){
if(scrollreset){
scrollreset = false;
setTimeout(function(){scrolldiv.scrollLeft=0;},2000);
}
}
else {
scrollreset = true;
scrolldiv.scrollLeft++;
}
}
setInterval(scroll_func,50);
</script>
