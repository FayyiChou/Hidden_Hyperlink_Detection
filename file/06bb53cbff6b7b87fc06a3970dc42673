    <script language="JavaScript">
        //首页浮动广告，将上面3行提到脚本外

        var xPos = 0;
        //var yPos = document.body.clientHeight;
        var yPos = 0;
        var step = 1;
        var delay = 20;
        var height = 0;
        var Hoffset = 0;
        var Woffset = 0;
        var yon = 0;
        var xon = 0;
        var pause = true;
        var interval;
        if (!("undefined" == typeof imgs))
            imgs.style.top = yPos;
        function changePos() {
            if (!("undefined" == typeof imgs)) {
                width = document.body.clientWidth;
                height = document.body.clientHeight;
                Hoffset = imgs.offsetHeight;
                Woffset = imgs.offsetWidth;
                imgs.style.left = xPos + document.body.scrollLeft;
                imgs.style.top = yPos + document.body.scrollTop;
                if (yon) {
                    yPos = yPos + step;
                }
                else {
                    yPos = yPos - step;
                }
                if (yPos < 0) {
                    yon = 1;
                    yPos = 0;
                }
                if (yPos >= (height - Hoffset)) {
                    yon = 0;
                    yPos = (height - Hoffset);
                }
                if (xon) {
                    xPos = xPos + step;
                }
                else {
                    xPos = xPos - step;
                }
                if (xPos < 0) {
                    xon = 1;
                    xPos = 0;
                }
                if (xPos >= (width - Woffset)) {
                    xon = 0;
                    xPos = (width - Woffset);
                }
            }
        }
        function start() {
            if (!("undefined" == typeof imgs))
                imgs.visibility = "visible";
            interval = setInterval('changePos()', delay);
        }

        var k = 1;
        if (k) {
            start();
        }
        else {
            if (!("undefined" == typeof imgs))
                imgs.style.display = "none";
        }
            </script>

    </div>
