            <script type="text/javascript">
                if ($("#NewsInfo td").length > 4) {
                    var NewsInfo = document.getElementById("NewsInfo");
                    var NewsInfo2 = document.getElementById("NewsInfo2");

                    var demoNews = document.getElementById("demoNews");
                    var newsspeed = 60;
                    NewsInfo2.innerHTML = NewsInfo.innerHTML;
                    function NewsMarquee() {
                        if (NewsInfo2.offsetHeight - demoNews.scrollTop <= 0) {

                            demoNews.scrollTop -= NewsInfo.offsetHeight;

                        }
                        else {
                            demoNews.scrollTop++;

                        }
                    }
                    var NewsInterval = setInterval(NewsMarquee, newsspeed);
                    demoNews.onmouseover = function () {
                        clearInterval(NewsInterval);
                    }
                    demoNews.onmouseout = function () {
                        NewsInterval = setInterval(NewsMarquee, newsspeed);
                    }
                }
            </script>
