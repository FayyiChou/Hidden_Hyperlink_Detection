    <script type="text/javascript">
        //首页搜索、有提示
        function clickinput(n) {
            var serachId = document.getElementById("serach");
            if (n == 0 && serachId.value == "") { serachId.value = ""; }
            if (n == 1 && serachId.value == "") { serachId.value = ""; }
        }
        function trim_(value_) {
            var re = /\s+/gi;
            return value_.replace(re, "");
        }
        function CheckLogin() {
            if (trim_(document.getElementById("serach").value) == '请输入关键字') {
                alert("请输入需要查询的关键字");
                return;
            }
            else {
                window.open("/search.aspx?title=" + escape(document.getElementById("serach").value));
                document.getElementById("serach").value = '';
            }
        }
        $(function () {
         var title='';
            if (title!= '') {
                document.getElementById("serach").value = title;
            }

            $("#serach").bind('click', function () {
                if ($(this).val() == "请输入关键字") {
                    $(this).val("");
                    $(this).css('color', '#000000');
                }
            });
            $("#serach").bind('blur', function () {
                if ($(this).val() == "") {
                    $(this).val("请输入关键字");
                    $(this).css('color', '#b8b8b8');
                }
            });
        });
    </script>
    
