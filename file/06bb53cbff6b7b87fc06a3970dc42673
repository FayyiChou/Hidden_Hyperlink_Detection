                 <script type="text/javascript">
                     if ($("#itd td").length > 1) {
                         var speeds = 20
                         var marquePic2 = document.getElementById("marquePic2");
                         var marquePic1 = document.getElementById("marquePic1");
                         var demo = document.getElementById("demo");
                         marquePic2.innerHTML = marquePic1.innerHTML
                         function Marquees() {
                             if (marquePic2.offsetWidth - demo.scrollLeft <= 0)
                                 demo.scrollLeft -= marquePic1.offsetWidth
                             else {
                                 demo.scrollLeft++
                             }
                         }
                         var MyMars = setInterval(Marquees, speeds)
                         demo.onmouseover = function () { clearInterval(MyMars) }
                         demo.onmouseout = function () { MyMars = setInterval(Marquees, speeds) }
                     }
            </script>
