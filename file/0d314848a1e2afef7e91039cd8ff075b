<script>

var $tt,$t,$jxs_t;

$(function(){

	svalue=$("input[name='s']").val()

	$("input[name='s']").focus(function(){

		if($(this).val()==svalue){

			$(this).val("");

		}

	});

	$("input[name='s']").blur(function(){

		if($(this).val()==""){

			$(this).val(svalue);

		}

	});

	$("#tops").submit(function(){

		if($("input[name='s']").val()=="" || $("input[name='s']").val()==svalue){

			alert("Search Content can not be empty");return false;

		}

	});

	$(".la").each(function(){

		$(this).css("visibility","visible").height($(this).find(".la_ul").height()).hide();

	});

	$(".nav_a:not(#pro_a)").hover(function(){

		clearTimeout($t);

		$(".la").hide();$(".la_pro").hide();

		$la=$(this).parent().find(".la");

		$la.show();

	},function(){

		$t=setTimeout(la,200);

	});

	$(".la").hover(function(){

		clearTimeout($t);

	},function(){

		$t=setTimeout(la,200);

	});

	$("#pro_a").hover(function(){

		clearTimeout($t);

		$(".la").hide();

		$la=$(this).parent().find(".la_pro");

		$la.show();

	},function(){

		$t=setTimeout(la_pro,200);

	});

	$(".la_pro").hover(function(){

		clearTimeout($t);

	},function(){

		$t=setTimeout(la_pro,200);

	});

	

	$(".btn_jxs").hover(function(){

		$(".jxs").show();

	},function(){});

	$(".jxs").hover(function(){clearTimeout($jxs_t);},function(){$jxs_t=setTimeout(la_jxs,100);});

});

function la(){clearTimeout($t);$(".la").hide();}

function la_jxs(){clearTimeout($jxs_t);$(".jxs").hide();}

function la_pro(){clearTimeout($t);$(".la_pro").hide();}

function addfavorite()

{

   if (document.all)

   {

      window.external.addFavorite('http://www.wlbckj.com/','温岭绿健环保科技');

   }

   else if (window.sidebar)

   {

      window.sidebar.addPanel('温岭绿健环保科技', 'http://www.wlbckj.com/', "");

   }

} 

</script>

