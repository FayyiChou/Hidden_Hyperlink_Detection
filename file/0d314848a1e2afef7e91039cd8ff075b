<script>
	/*
		全屏背景轮播切换
	  第一步：让切换按钮动起来
	  第二步：让图片跟着切换按钮动起来
	  第三步：让它自动动起来，定义一个定时任务。
	*/
 $("#banner .imglist").eq(0).show().siblings("div").hide();
	$("#banner .btn li").mouseover(function(){
			clearInterval(timer);//当鼠标滑动时，则停止定时任务
		    var _index = $(this).index();//获取当前索引（相对应的位置）
			/*当鼠标滑动到li相对应的位置上面，则添加一个样式（小圆点）*/
			$(this).addClass("hover").siblings().removeClass("hover");
			/*相对应的背景图片显示和隐藏*/
			$("#banner .imglist").eq(_index).fadeIn(1000).siblings("div").fadeOut(1000);
	}).mouseout(function(){
		 autoplay();//当鼠标离开时，则执行定时任务
		});
	 var _index = 0;
	 var timer = null;
	 /*定义一个定时任务*/
	 function autoplay(){
		timer=setInterval(function(){
			 _index++;
			  if(_index<9){
				$("#banner .btn li").eq(_index).addClass("hover").siblings().removeClass("hover");
				$("#banner .imglist").eq(_index).fadeIn(700).siblings("div").fadeOut(700);
			  }else{_index=-1;}
			},3000); 
	 };
	 autoplay();
	</script>

