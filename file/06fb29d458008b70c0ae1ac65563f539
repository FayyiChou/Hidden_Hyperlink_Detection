    function gtmAddToCart(data){
        if(typeof(data)=='undefined' || typeof(data.name)=='undefined'){
            return true ;
        }
        //根据url截取路径（如yixun,com）
        if(data.dimension10!=undefined && data.dimension10!=null && data.dimension10!=''){
            var point = data.dimension10.indexOf(".");
            if(point > 0) {
                var point1 = data.dimension10.indexOf("/", data.dimension10.indexOf("//") + 2);
                if (point1 > point) {
                    data.dimension10 = data.dimension10.substring(point + 1, point1);
                }
            }
        }

