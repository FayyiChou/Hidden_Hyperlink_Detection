        function keypress(e) {
            var currKey = 0, e = e || event;
            if (e.keyCode == 13) {
                if (document.getElementById("Register_Btn") != null) {
                    $("#Register_Btn").click();
                }
                else if (document.getElementById("LoginAfter").style.display == "none") {
                    $("#Btn_Login").click();
                }

            }
        }
        document.onkeypress = keypress;
