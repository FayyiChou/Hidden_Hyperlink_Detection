    <script type="text/javascript">

        function CopyName(objName, Name) {
            $('input#MyCopyName').val(Name);
            var offsettop = $('#' + objName).offset().top;
            var offsetleft = $('#' + objName).offset().left;
            $("#CopyNameDiv").show();
            $("#CopyNameDiv").css({ position: "absolute", 'top': offsettop + 10, 'left': offsetleft + 10, 'z-index': 1 });
            $('#MyCopyNameBtn').zclip({
                path: 'js/ZeroClipboard.swf',
                copy: function () {
                    return $('input#MyCopyName').val();
                }
            });
        }
        function CopyNameClose() {
            $("#CopyNameDiv").hide();
        }
        


        google.load("visualization", "1", { packages: ["corechart"] });


        function AD_Show() {
           // alert("aaaaaa");
           // if ($("#miaov_float_layer").css("display") != "none")
           // {
               // alert("bbbbb");
                var LeftP = $(window).width() - 258;
                var LeftT = $(window).height() - 261;
                $("#miaov_float_layer").css("display", "block");
                $("#miaov_float_layer").css("position", "fixed");
                $("#miaov_float_layer").css("top", LeftT);
                $("#miaov_float_layer").css("left", LeftP);
           // }
        }
        function AD_Close() {
            var LeftP = $(window).width() - 258;
            var LeftT = $(window).height() - 261;
            $("#miaov_float_layer").css("display", "none");
        }
        

    </script>
