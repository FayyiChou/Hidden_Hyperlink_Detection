<script type="text/javascript">
    $(function () {
        $(".tab_Tit a").each(function (index) {
            $(this).mouseover(function () {
                $(".tab_Tit a.selected").removeClass("selected");
                $(this).addClass("selected");
                $(".tab_Con > ul:visible").hide();
                $(".tab_Con ul:eq(" + index + ")").show();
                $("#C_Stakes").show();
            })
        })
    })
</script>
