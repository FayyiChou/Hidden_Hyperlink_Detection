<script type="text/javascript">
jQuery(function($){
		$(".blk1_pay_attention").bind("click",function(){
			var _self = $(this);
			var id = _self.attr("bsiteid");
			$.verifyLogin(function(){
			// 如果不存在关注的class，则调用关注接口，反之调用取消关注
			if(!_self.hasClass("blk1_attention")){
				_self.attr("disabled","disabled");
				$.ajax({
					type : 'POST',
					url : "/bsite/follow/" + id,
					//data: data,
					success : function(data) {
						//console.log(data);
						if (data.errorCode == 0 ){
							$(".blk1_pay_attention").text("已关注");
							$(".blk1_pay_attention").addClass("blk1_attention");
							//$(".blk1_pay_attention").alert(getMsg(data.errorCode));
							$(".bsiteFansCount").text(Number($(".bsiteFansCount").text())+1);
						} else {
							$(".blk1_pay_attention").alert(getMsg(data.errorCode));
						}
						$(".blk1_pay_attention").removeAttr("disabled");
					},
					dataType: "json"
				});
			}else{
				// 取消关注
				$(this).attr("disabled","disabled");
				$.ajax({
					type : 'POST',
					url : "/bsite/unfollow/" + id,
					//data: data,
					success : function(data) {
						//console.log(data);
						if (data.errorCode == 0 ){
							$(".blk1_pay_attention").empty().html("<span></span>+关注");
							//$(".blk1_pay_attention").alert(getMsg(data.errorCode));
							$(".blk1_pay_attention").removeClass("blk1_attention");
							$(".bsiteFansCount").text(Number($(".bsiteFansCount").text())-1);
						} else {
							$(".blk1_pay_attention").alert(getMsg(data.errorCode));
						}
						$(".blk1_pay_attention").removeAttr("disabled");
					},
					dataType : "json"
				});
			}
		
	},function(){
		$("body").alert(getMsg(100));
	});
	});
	
	$(".blk1_pay_attention").hover(function(){
  		if($(this).hasClass("blk1_attention")){
			$(this).text("取消关注");
		}},function(){
			if($(this).hasClass("blk1_attention")){
				$(this).text("已关注");
	}});
});
</script>        <input type="hidden" id="webroot" value="">
