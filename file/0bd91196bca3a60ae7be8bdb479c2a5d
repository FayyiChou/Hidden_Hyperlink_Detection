            <!-- 推荐用户 -->

<script type="text/javascript">
jQuery(function($){
	$(".flow_btn").bind("click",function(){
		if(!$(this).hasClass("flowed")){
		var id = $(this).attr("personid");
		var item = $(this);
		$.verifyLogin(function(){
			if(item.hasClass("flow_btn")){ // 关注操作
				item.attr("disabled", "disabled");
				$.ajax({
                    type : 'POST',
                    url : "/u/follow/" + id,
					//data: data,
					success : function(data) {
						//console.log(data);
						if (data.errorCode == 0 ){
							item.text("已关注");
							var _num = item.closest(".flow_item").find(".flow_text span.num");
							var num = parseInt(_num.text());
							num = num + 1;
							_num.text(num);
							item.removeClass("flow_btn").addClass("flowed");
							item.alert("操作成功！");
						} else {
							item.alert(getMsg(data.errorCode));
							item.removeAttr("disabled");
						}
					},
					error:function(){
						item.alert("关注失败，系统异常，请你稍后再试！");
						item.removeAttr("disabled");
					},
					dataType : "json"
				});
			}
		},function(){
			$("body").alert(getMsg(100));
			return false;			
		});
		}
	});
});
</script>            <!-- 侧边栏 video -->
