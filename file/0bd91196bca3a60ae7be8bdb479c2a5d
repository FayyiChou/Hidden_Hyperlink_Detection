<script type="text/javascript">
//实例化编辑器
var serverPath = '',
um = UM.getEditor('commentEdit', {
    imageUrl:serverPath + "/umedit/reply/umeditupload",
    imagePath:""
});
um.execCommand('cleardoc');
$(".tns_input").focus();
var floor = $.hash("floor");
// 是否是管理员
function isAdmin(){
    var admin = false;
    if (userInfo.errorCode == 0) {
        if (userInfo.erpManager) {
            return true;
        } else {
            for(var idx in userInfo.roles){
                var item = userInfo.roles[idx];
                if (item.roleId == 1) {
                    if (item.siteId == $("#bsiteId").val()) {
                        admin = true;
                        break;
                    }   
                } else if (item.roleId == 2 || item.roleId == 3) {
                    if (item.siteId == $("#bsiteId").val() && item.blockId == $("#blockId").val()) {
                        admin = true;
                        break;
                    }
                }
            }
        }
    }
    return admin;
}

juicer.register("foolrText", function foolrText(index){
    var pageNum = parseInt($("#pageNum").val());
    if (pageNum === 1) {
        if (parseInt(index) === 0){
            return '<div class="views_1st">沙发</div>';
        } else if (parseInt(index) === 1){
            return '<div class="views_2nd">板凳</div>';
        } else if (parseInt(index) === 2){
            return '<div class="views_3rd">地板</div>';
        } else {
            return '<div class="views_normal">#'+(parseInt(index)+1)+'</div>';
        }
    } else {
        return '<div class="views_normal">#'+(parseInt(pageNum-1)*20+parseInt(index)+1)+'</div>';
    }
});

juicer.register("foolr", function foolr(index){
    var pageNum = parseInt($("#pageNum").val());
    if (pageNum === 1) {
        return parseInt(index) + 1;
    } else {
        return (parseInt(pageNum-1)*20+parseInt(index)+1);
    }
});

juicer.register("showDelBtn", function showDelBtn(uid){
    if (userInfo.uid == uid) {
        return "inline";
    } else {
        return "none";
    }
});

juicer.register("stamp2Time", function stamp2Time(stmp){
    var d = new Date(stmp);
    var year = d.getFullYear();
    var month = d.getMonth()+1;
    var day = d.getDate();
    var hours = d.getHours();
    var minutes = d.getMinutes();
    var seconds = d.getSeconds();
    var timeStr = "" + year + "-" + month + "-" + day +" " + hours + ":" + minutes + ":" + seconds;
    return timeStr;
});

$(function(){
    // 验证码
    $(".kaptcha-container").hide();
    $(".edui-body-container").on("focus", function(){
        var kaptchaIsShowing = $(".kaptcha-container").is(":visible");
        if(!kaptchaIsShowing){
            $('.kaptcha-img').attr('src', '/captcha/replyCode?' + Math.floor(Math.random()*100)).show();
            $(".kaptcha-container").fadeIn();
        }
    });
    $(".kaptcha-container").on("click", ".kaptcha-change, .kaptcha-img",function(){
        $('.kaptcha-img').attr('src', '/captcha/replyCode?' + Math.floor(Math.random()*100)).fadeIn();
        $('.kaptcha-text').val("");
    });
    
    $(".topic_cmnt,.btn_comment").bind("click", function(){
        um.execCommand('cleardoc');
    });
    
    var _comment_tpl = $('#_comment_tpl').html();
    var _movebox_tpl = $('#_movebox_tpl').html();
    var _blockTag_select = $("#_blockTag_select").html();
    // 获取评论数
    function getReplys(pageNum){
        $("#pageNum").val(pageNum);
        $(".views_list").showLoading();
        $.ajax({
            type: "POST",
            dataType: "json",
            url:"/open/bsite/postreply/56477_"+pageNum,
            data: {},
            cache:false,
            success: function(result){
                var html = juicer(_comment_tpl, result);
                $(".views_list").html(html);
                // 加载完用户回复数据后的校验
                // 校验是否点过赞
                var replyList = $("body").find(".views_up");
                var postData = [];
                $(replyList).each(function(i, item) {
                    postData.push($(item).attr("data-id"));
                });
                //console.log("获取到评论的数组是：" + postData);
                $.ajax({
                    type: 'GET',
                    url: contextPath + "/post/checkreplypraised",
                    data: {"postdata": postData.join(",")},
                    cache: false,
                    success: function(data) {
                        //console.log("校验完的结果是："+data);
                        $(replyList).each(function(i, item) {
                            $(data).each(function(j, dataObj){
                                if($(item).attr("data-id") == dataObj){
                                    $(item).addClass("views_upd");
                                }                               
                            });
                        });
                    },
                    error: function(){
                    },
                    dataType: "json"
                });
                // 验证是否该显示删除按钮
                if (isAdmin()) {
                    $(".delete_reply").show();
                    $(".report_reply").hide();
                } else {
                    // $(".report_reply").show();
                    var t;
                    var objBtn;
                    $(".views_item.clearfix").mouseenter(function(){
                        objBtn = $(this);
                        t = setTimeout(function(){
                            objBtn.find(".report_reply").fadeIn();
                            }, 1000);
                    }).mouseleave(function(){
                        objBtn.find(".report_reply").fadeOut();
                        clearTimeout(t);
                    });
                }
                // 是否是楼层定位
                if (floor != null) {
                    var offset = $("#"+floor).offset();
                    if(offset){
                        $("html,body").animate({scrollTop:offset.top}, 50);
                    }
                }
                // 生成分页按钮
                if (result.replysCnt > 20) {
                    $("#page_box").show();
                    laypage({
                        cont: 'page_box',
                        pages: (result.replysCnt%20) == 0 ? result.replysCnt/20 : result.replysCnt/20 + 1, //总页数
                        curr: pageNum, //当前页
                        first:"首页",
                        last: "尾页",
                        prev: '上一页',
                        next: '下一页',
                        jump: function(obj){
                            if(obj.curr != pageNum){
                                getReplys(obj.curr);
                                $('html, body').animate({
                                    scrollTop: $(".views_tit").offset().top + "px"
                                },400);
                            }
                        }
                    });                 
                }else{
                    $("#page_box").hide();
                }
            },
            error: function () {
            }
        });
    }
    
    $("a.btn_operate").mouseenter(function(){
        //if($("div.btn_active").length > 0){
        //  $("div.btn_ops").removeClass("btn_active");
        //}else{
        $("div.btn_ops").addClass("btn_active");
        //}
    });

    $("div.btn_ops").mouseleave(function(){
        $(this).removeClass("btn_active");
    });
        
    // 发表评论
    $(".comment_r .comment_publish").click(function(){
        var isRefReply = $("#refId").val().length == 0 ? false : true;
        $.verifyLogin(function(){
            //获取html内容，返回: <p>hello</p>
            var replyCode = $(".kaptcha-text").val();
            if ($(".kaptcha-text").val() == "") {
                $().alert(getMsg(176));
                return;
            } else {
                $("#replyCode").val($(".kaptcha-text").val());
            }
            var contentHtml = um.getContent();
            //获取纯文本内容，返回: hello
            var contentTxt = um.getContentTxt();
            var tempHtml = um.getContent();
            var blockquoteHtml = "";
            if(!um.hasContents()){
                $("body").alert("请输入评论内容后再提交");
                return;
            }else{
                if (isRefReply) {
                    blockquoteHtml = $(".refcomment-right").html();
                    contentHtml = contentHtml;
                    contentTxt = $(contentHtml).text();
                } else {
                    isRefReply = false;
                    $("#refId").val("");
                }
                // if (!$.trim(contentTxt).length > 0) {
                if($(contentHtml).html() == null || contentHtml.replace(/<br\/?>/g,"").replace(/<p><\/p>/g,"").length == 0) {
                    $("body").alert("评论内容为空哦 :)");
                    return;
                }
                if (contentTxt.length > 10000) {
                    $("body").alert("超出了最大字数限制，字数应在【10000】字以内");
                    return;
                }
                $("#replyContent").val(contentHtml);
                var param = {};
                param.refId = $("#refId").val();
                param.replyContent = contentHtml;
                param.replyCode = $(".kaptcha-text").val();
                $.ajax({
                    type : 'POST',
                    url : "/bsite/reply/56477?_=" + new Date().getTime(),
                    data: param,
                    cache:false,
                    success : function(data) {
                        $("#replyCode").val("");
                        if(data.errorCode == 0){
                            $("#refId").val("");
                            um.execCommand('cleardoc');
                            //更新评论数量
                            var _tit = $(".views_tit");
                            var num = parseInt(_tit.html());
                            if(num){
                                num = num + 1;
                            }else{
                                num = 1;
                            }
                            $("p.views_tit").attr("style","");
                            _tit.html(num + "个评论");
                            $(".postReplyNum").text(num);
                            var _views_comments = $("#_views_comments").html();
                            var commentsData = {
                                user : {uid:user.uid,nickName:user.info.nickName,headImg:user.info.headImgUrl},
                                comments_content : contentHtml,
                                blockquote : blockquoteHtml,
                                floorNum : num,
                                id : data.info,
                                postUId : $("#postUserId").val()
                            };
                            var content = juicer(_views_comments, commentsData);
                            $(".views_list").append(content);
                            // 清空回复框
                            $(".refcomment-wrap").slideUp();
                            // 刷新验证码
                            $(".kaptcha-img").click();
                            if(!isAdmin()){
                                $(".report_reply_btn[data-id="+data.info+"]").show();
                            }
                            $("body").alert(getMsg(data.errorCode), function(){});
                        } else {
                            $("body").alert(getMsg(data.errorCode));
                            $(".kaptcha-img").click();
                        }
                    },
                    error:function(){
                        $("#replyCode").val("");
                    },
                    dataType : "json"
                });
            }
        },function(){
            $("body").alert(getMsg(100));
            return false;
        });
    });
    
    // 移动
    $(".operate_move").click(function(){
        // 去校验是否具备移动帖子的权限
        if ($(".component_move").length) {
            $(".component_move").animate({opacity:"1"});
            $(".component_move").css("left",$(".btn_ops").offset().left);
            $(".component_move").fadeIn();
            $("div.btn_ops").removeClass("btn_active");
            return;
        }
        $.ajax({
            type : 'POST',
            url : "/open/bsite/getblocks",
            data: {"bsiteId": "18351"},
            cache:false,
            success : function(data) {
                var moveDiv = '';
                if(data.model.blocks.length > 0){
                    moveDiv = juicer(_movebox_tpl, data.model);
                } else {
                    moveDiv = juicer(_movebox_tpl,{blocks:[]});
                }
                var _moveDiv = $(moveDiv);
                _moveDiv.css("left","-500px").css("top",$(".btn_ops").offset().top).css("z-index",1001);
                $("body").append(_moveDiv);
                $(".moveSelect1").click(function(){
                    if ($(".component_move div.select").hasClass("active")) {
                        $(".component_move div.select").removeClass("active");
                    } else {
                        $(".component_move div.select").addClass("active");
                    }
                });
                
                $(".moveSubmitBtn").bind("click", function(){
                    var blockId,blockTagId=0;
                    if(!$(".moveSelect1").attr("blockid")){
                        $("body").alert("请选择板块信息");
                        return;
                    }
                    blockId = $(".moveSelect1").attr("blockid");
                    if($(".blockTagSelect").length > 0){
                        if(!$(".blockTagSelect").attr("blocktagid")){
                            $("body").alert("请选择二级板块信息");
                            return;
                        }
                        blockTagId = $(".blockTagSelect").attr("blocktagid");
                    }
                    $.ajax({
                        type: 'POST',
                        url: "/post/movepost/56477" ,
                        data: {"blockId":blockId,"blockTagId":blockTagId},
                        cache:false,
                        success: function(data){
                            if(data.errorCode == 0){
                                $().alert(getMsg(data.errorCode), function(){
                                    location.reload();
                                    $(".component_move a.close").click();
                                    $(".btn_operate").click();
                                });
                            }else{
                                $().alert(getMsg(data.errorCode));
                            }                           
                        } ,
                        dataType: "json"
                    });
                });
                
                $(".blockBtn").bind("click", function(){
                    var blockId = $(this).attr("blockid");
                    $(".moveSelect1").text($(this).text());
                    $(".moveSelect1").click();
                    $(".moveSelect1").attr({"blockid":blockId});
                    $.ajax({
                        type: 'POST',
                        url: "/open/bsite/getblocktags" ,
                        data: {"blockId":blockId} ,
                        cache:false,
                        success: function(data){
                            if (data.model.blockTags.length > 0) {
                                $(".tagsSelect").remove();
                                var tagSelect = juicer(_blockTag_select, data.model);
                                $(".blocksSelect").after(tagSelect);
                                
                                $("div.tagsSelect a.cur").click(function(){
                                    if($("div.tagsSelect").hasClass("active")){
                                        $("div.tagsSelect").removeClass("active");  
                                    }else{
                                        $("div.tagsSelect").addClass("active");
                                    }
                                });
                                $(".blockTagBtn").bind("click", function(){
                                    $("div.tagsSelect a.cur").text($(this).text());
                                    $("div.tagsSelect a.cur").click();
                                    $("div.tagsSelect a.cur").attr({"blocktagid": $(this).attr("blocktagid")});
                                });
                            } else {
                                $(".tagsSelect").remove();
                            }
                        } ,
                        dataType: "json"
                    });
                });
                
                _moveDiv.animate({left:$(".btn_ops").offset().left + 150});
                _moveDiv.animate({left:$(".btn_ops").offset().left},"fast");
                
                $("div.btn_ops").removeClass("btn_active");
                
                $(".component_move a.close").bind("click",function(){
                    $(".component_move").fadeOut();
                });
            },
            error:function(){},
            dataType : "json"
        });
    })
    
    // 删除帖子
    $(".operate_delete").bind("click", function(){
        $("body").confirm("帖子删除后需要到后台管理端恢复，是否确认删除？" ,function(){
            $(".btn_operate").click();
            $.ajax({
                type: 'POST',
                url: "/post/delpost/56477",
                cache:false,
                success: function(data) {
                    if (data.errorCode == 0) {
                        $().alert(getMsg(data.errorCode), function(){
                            window.location.href = "/open/bsite/index/18351.html"
                        });
                    } else {
                        $().alert(getMsg(data.errorCode));
                    }
                },
                error:function(){},
                dataType: "json"
            });
        });
    });
    
    $(".btn_delete").bind("click", function(){
        $("body").confirm("帖子删除后需要到后台管理端恢复，是否确认删除？" ,function(){
            $(".btn_operate").click();
            $.ajax({
                type: 'POST',
                url: "/post/delpost/56477",
                cache:false,
                success: function(data) {
                    if (data.errorCode == 0) {
                        $().alert(getMsg(data.errorCode), function(){
                            window.location.href = "/open/bsite/index/18351.html"
                        });
                    } else {
                        $().alert(getMsg(data.errorCode));
                    }
                },
                error:function(){},
                dataType: "json"
            });
        });
    });
    
    // 删除评论
    $("body").delegate(".delete_replay_btn", "click", function(){
        var item = $(this);
        $.verifyLogin(function(){
            $("body").confirm("评论删除后需要让管理员恢复，是否确认删除？" ,function(){
                $.ajax({
                    type: 'POST',
                    url: "/bsite/delreply/" + $(item).attr("data-id"),
                    cache:false,
                    success: function(data) {
                        if (data.errorCode == 0) {
                            $().alert(getMsg(data.errorCode), function(){
                                $(item).parent().siblings("div.views_text").empty().text("该评论已被删除。");
                                $(item).parent().empty().html("&nbsp;");
                            });
                        } else {
                            $().alert(getMsg(data.errorCode));
                        }
                    },
                    error:function(){},
                    dataType: "json"
                });
            });
        },function(){
            $("body").alert(getMsg(100));
            return false;
        });
    // 评论点赞
    }).delegate(".views_up", "click", function(){
        var item = $(this);
        $.verifyLogin(function(){
            var type = 1;
            if($(item).hasClass("views_upd")){
                type = -1;
            }
            $.ajax({
                type: 'POST',
                url: "/bsite/heartreply/" + $(item).attr("data-id"),
                data:{"type": type, "bsiteId": 18351},
                cache:false,
                success: function(data) {
                    if (data.errorCode == 0) {
                        if (!$(item).hasClass("views_upd")) {
                            $(item).addClass("views_upd");
                            if($(item).find(".bestBuf").is(":hidden")){
                                $(item).find(".bestBuf").show();
                            }
                        } else {
                            $(item).removeClass("views_upd");
                        }
                        var currNum = Number($(item).find(".bestSpan").text());
                        if(isNaN(currNum)){
                            currNum = 0;
                        }
                        var nextNum = currNum + type;
                        if(nextNum<0){
                            nextNum = 0;
                        }
                        $(item).find(".bestSpan").text(nextNum);
                    } else {
                        $().alert(getMsg(data.errorCode));
                    }
                },
                error:function(){},
                dataType: "json"
            });
        },function(){
            $("body").alert(getMsg(100));
        });
        // 评论回复
    }).delegate(".views_reply", "click", function(){
        um.execCommand('cleardoc');
        $(".tns_input").focus();
        var _self = $(this).closest(".views_r");
        $("#refId").val($(this).attr("data-id"));
        var _refId = $(this).attr("data-id");
        var tempNode = _self;
        $.verifyLogin(function(){
            var commentsInfo = {
                "headline" : _self.find(".views_info").eq(0).text().replace(/\ |&nbsp;/g,''),
                "contents" : _self.find("div.views_text").length > 1 ? _self.find("div.views_text").eq(1).html() : _self.find("div.views_text").eq(0).html()
            };
            var _replyTpl = $("#_reply_comments").html();
            var html = juicer(_replyTpl, commentsInfo);
            html = UM.utils.html(html);
            um.focus();
            $(".refcomment-wrap").slideDown().find(".refcomment-right").attr({
                "data-refid" : _refId
            }).html(html);
                        $("#refReplyContext").val(html);
            return false;
        },function(){
            $("body").alert(getMsg(100));
        });
    });
    
    // 管理员回复
    $(".manage_reply").bind("click", function(){
        var official_comment = $(".official_comment").html();
        if(!$(".official_comment").text().length > 0){
            $("body").alert("请输入回复内容");
            return;
        }
        $.ajax({
            type: 'POST',
            url: "/bsite/managerReply/56477",
            data:{"replyContent":official_comment},
            cache:false,
            success: function(data) {
                if (data.errorCode == 0) {
                    $(".official_comment").blur();
                    $(".manage_reply").hide();
                    $().alert(getMsg(data.errorCode));
                    $(".official_comment").attr("contenteditable", null);
                } else {
                    $().alert(getMsg(data.errorCode));
                }
            },
            error:function(){},
            dataType: "json"
        });
    });
    // 加精/取消加精
    $(".operate_best").bind("click", function() {
        if($(".operate_best").hasClass("isbest")){
            $.ajax({
                type: 'POST',
                url: "/post/unbestpost/56477",
                cache:false,
                success: function(data) {
                    if (data.errorCode == 0) {
                        $(".operate_best").removeClass("isbest");
                        $(".operate_best").text("加精");
                        $().alert(getMsg(data.errorCode));
                        $(".btn_operate").click();
                    } else {
                        $().alert(getMsg(data.errorCode));
                    }
                },
                error:function(){},
                dataType: "json"
            });
        } else {
            $.ajax({
                type: 'POST',
                url: "/post/bestpost/56477",
                cache:false,
                success: function(data) {
                    if (data.errorCode == 0) {
                        $(".operate_best").addClass("isbest");
                        $(".operate_best").text("取消加精");
                        $().alert(getMsg(data.errorCode));
                        $(".btn_operate").click();
                    } else {
                        $().alert(getMsg(data.errorCode));
                    }
                },
                error: function(){},
                dataType: "json"
            });
        }
    });
    
    // 置顶/取消置顶
    $(".operate_top").bind("click", function() {
        if($(".operate_top").hasClass("istop")){
            $.ajax({
                type : 'POST',
                url : "/post/untoppost/56477",
                cache:false,
                success : function(data) {
                    if (data.errorCode == 0) {
                        $(".operate_top").removeClass("istop");
                        $(".operate_top").text("置顶");
                        $().alert(getMsg(data.errorCode));
                        $(".btn_operate").click();
                    } else {
                        $().alert(getMsg(data.errorCode));
                    }
                },
                error:function(){},
                dataType : "json"
            }); 
        } else {
        $.ajax({
            type : 'POST',
            url : "/post/toppost/56477",
            cache:false,
            success : function(data) {
                if (data.errorCode == 0) {
                    $(".operate_top").addClass("istop");
                    $(".operate_top").text("取消置顶");
                    $().alert(getMsg(data.errorCode));
                    $(".btn_operate").click();
                } else {
                    $().alert(getMsg(data.errorCode));
                }
            },
            error:function(){},
            dataType : "json"
        });}
    });
    
    // 举报
    $(".report").bind("click", function(){
        $("#reportType").val("1");
        $("#reportBox").css("left", $(this).offset().left-306).css("top",$(this).offset().top).css("z-index",1001).show();
    });
    $("#reportBox .close").bind("click", function(){
        $("#reportBox").hide();
    });
    $("#reportBox a.btn").bind("click", function(){
        $.verifyLogin(function(){
            if(!$("input[name='report']").is(":checked")){
                $("body").alert("请选择举报类型再提交");
            }else{
                var postUrl = "/post/reportpost";
                if ($("#reportType").val() == 2) {
                    postUrl = "/post/reportreply";
                }
                $.ajax({
                    type: 'POST',
                    url: postUrl,
                    cache:false,
                    data: {"tag":$("input[name='report']:checked").val(),
                        "bsiteId":18351,
                        "postId":56477,
                        "replyId": $("#replyId").val()},
                        success : function(data) {
                        if (data.errorCode == 0) {
                            $().alert(getMsg(data.errorCode));
                            $("#reportBox .close").click();
                        } else {
                            $().alert(getMsg(data.errorCode));
                        }
                    },
                    error:function(){},
                    dataType : "json"
                });
            }
        },function(){
            $("body").alert(getMsg(100));
            return false;
        });     
    });
    
    // 举报评论
    $("body").delegate(".report_reply", "click", function(){
        $("#reportType").val(2);
        $("#replyId").val($(this).attr("data-id"));
        $("#reportBox").css("left", $(this).offset().left-306).css("top",$(this).offset().top).css("z-index",1001).show();
    });
    
    // 点击用户头像触发
    $("body").delegate(".userLink", "click", function(){
        if (checkLogin) {
            window.open("/u/profile/"+$(this).attr("userid")+".html");
        } else {
            window.open("/open/u/profile/"+$(this).attr("userid")+".html");
        }
        return false;
    }).delegate(".operate_reply", "click", function(){
        $(".official_comment").attr("contenteditable", "");
        $(".manage_reply").show();
        $("#topic_reply").show().find("p").focus();
        $(".btn_operate").click();
    });

    void function(){
        var curPage = 1;
        if (floor != null) {
            curPage = parseInt(floor) % 20 == 0 ? parseInt(floor) / 20 : parseInt(parseInt(floor) / 20 + 1);
        }
        getReplys(curPage);
    }();
    
    
    // 加载完基础数据后初始化动态校验数据
    $.verifyLogin(function(){
        var postList = $("body").find(".topic_up");
        var postData = [];
        $(postList).each(function(i, item) {
            postData.push($(item).attr("postid"));
        });
        $.ajax({
            type: 'GET',
            url: contextPath + "/post/checkpostpraised",
            data: {"postdata": postData.join(",")},
            cache: false,
            success: function(data) {
                if (data.length > 0) {
                    $(data).each(function(index, postid){
                        $(postList).each(function(i, item) {
                            if(postid == $(item).attr("postid")){
                                $("a.topic_up").addClass("topic_up_selected");
                                $("a.btn_up").addClass("btn_upd");
                                $("a.btn_up").text("已赞");
                            }
                        });
                    });
                }
            },
            error: function(){
            },
            dataType: "json"
        });
    });
    var postHeartSave = function(){
        var item = $(this);
        $.verifyLogin(function(){
            // 已登陆
            var type = 1;
            // 如果存在点赞的数据则为取消赞
            if (item.hasClass("topic_up_selected") || item.hasClass("btn_upd")) {
                type = -1;
            } else {
                type = 1;
            }
            $.ajax({
                type: "POST",
                dataType: "json",
                url:"/bsite/saveheart/56477.html",
                data: {"type":type, "bsiteId":18351},
                cache:false,
                success: function(result) {
                    if (result.errorCode == 0) {
                        if (item.hasClass("topic_up_selected") || item.hasClass("btn_upd")) {
                            $("a.btn_up").removeClass("btn_upd");
                            $("a.btn_up").text("赞");
                            $("a.topic_up").removeClass("topic_up_selected");
                            $(".topic_up_num").text(Number($(".topic_up_num").text())-1);
                        } else {
                            $("a.btn_up").addClass("btn_upd");
                            $("a.btn_up").text("已赞");
                            $("a.topic_up").addClass("topic_up_selected");
                            $(".topic_up_num").text(Number($(".topic_up_num").text())+1);
                        }
                    } else {
                        $().alert(getMsg(result.errorCode));
                    }
                },
                error: {
                }
            });
        },function(){
            // 未登陆
            $("body").alert(getMsg(100));
        });
    }
    
    // 点赞
    $(".topic_up").bind("click", postHeartSave);
    $(".btn_up").bind("click", postHeartSave);
    //使用Ctrl+Enter提交评论
    $(window).bind("keydown", function(e){
        if(e.keyCode==13&&e.ctrlKey){
            $(".comment_publish").click();
        }
    });
    (function(){
        //删除当前登陆用户的加关注按钮
        //userinfo(function(user){
        //    user = user;
        //    $(".flow [personid="+user.uid+"]").hide();
        //});
        if(userInfo.errorCode!=100){
    		$(".flow [personid="+userInfo.uid+"]").hide();
		}
    }());
    
    if(location.hash=="#_comment"){//解决IE下跳来跳去的问题
        setTimeout(function(){
            $('html, body').animate({
                scrollTop: $("#_comment").offset().top + "px"
            },500);
        },1000);
    }
    
});
$.get("//jingzhi.jd.com/index/viewpost/56477");
</script>

