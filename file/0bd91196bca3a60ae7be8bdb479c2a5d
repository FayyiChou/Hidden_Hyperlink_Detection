var isDisDel = false;
var crrUserid;
var checkLogin = false;
$(function(){
$.verifyLogin(function(){ //login callback 
    // 如果登陆了，获取用户的操作按钮
    $.ajax({
        type: "POST",
        dataType: "json",
        async: false,
        cache:false,
        url: "/u/getmenu/56477",
        success: function(result){
            // 校验是否关注小站
            $.checkFocusBsite("blk1_attention","已关注");
            // 校验是否关注过推荐用户
            $.checkFocusPerson("flow_btn","flowed","已关注");
            if (result.state) {
                checkLogin = true;  
                switch(result.auth)
                {
                case 1://站长
                //if (userStatus.errorCode == 0) { 
                    $(".btn_edit").show();
                    $(".btn_ops").show();
                    isDisDel = true;
                //}
                break;
                case 2://版主
                //if (userStatus.errorCode == 0) { 
                    $(".btn_edit").show();
                    $(".btn_ops").show();
                    isDisDel = true;
                //}
                break;
                case 3://实习版主
                //if (userStatus.errorCode == 0) { 
                    $(".btn_edit").show();
                    $(".btn_ops").show();
                    isDisDel = true;
                //}
                break;
                case 4://发帖人
                    $(".btn_edit").show();
                    $(".btn_delete").show();
                break;
                default://普通用户
                    $(".btn_edit").hide();
                }
            }
        },
        error: function(e){
            //console.log(e);
        }});});
});
</script>
