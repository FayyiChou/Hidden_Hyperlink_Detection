<script id="_comment_tpl" type="text/template">
{@each replys as reply, index}
<div class="views_item clearfix" id="$${index|foolr}">
$${index|foolrText}
<a class="views_portrait userLink" userid="${reply.cmsAccountWrap.id}" style="cursor: pointer;">
    <img src="${reply.cmsAccountWrap.headImgUrl}">
    <span class="views_cover"></span>
</a>
<div class="views_r">
    <span class="views_info">
        <a class="userLink" userid="${reply.cmsAccountWrap.id}" style="cursor: pointer;">{@if reply.authorId === 2352899}楼主{@else}${reply.cmsAccountWrap.nickName}{@/if}</a> <span title="${reply.createdDate|stamp2Time}">${reply.createdDateStr}</span> 发表
    </span>
    {@if reply.yn == "N"}
        {@if reply.refReplyId }
            <blockquote class="views_quote">
                <span class="views_info">{@if reply.cmsReplyWrap.authorId === 2352899}楼主{@else}${reply.cmsReplyWrap.cmsAccountWrap.nickName}{@/if}</a> ${reply.cmsReplyWrap.createdDateStr} 发表</span>
                <div class="views_text">$${reply.cmsReplyWrap.replyContent}</div>
            </blockquote>
        {@/if}
        <div class="views_text">
            $${reply.replyContent}
        </div>
    {@else}
        <div class="views_text">
            该评论已被删除。
        </div>
    {@/if}
    {@if reply.yn == "N"}
        <div class="views_panel">
            <a href="javascript:void(0);" class="views_delete delete_replay_btn delete_reply" data-id="${reply.id}" userid="${reply.cmsAccountWrap.id}" style="display:${reply.cmsAccountWrap.id|showDelBtn}">删除</a>
            <a href="javascript:void(0);" class="views_delete report_reply" data-id="${reply.id}" style="color:#999;display:none;">举报</a>
            <a href="javascript:void(0);" class="views_up" data-id="${reply.id}" style="cursor:pointer;">赞<span class="bestBuf" {@if !reply.heartNum}style="display:none;"{@/if}>(<span class="bestSpan">${reply.heartNum}</span>)</span></a>
            <a href="#_comments" class="views_reply" data-id="${reply.id}">回复</a>
        </div>
    {@else}
        <div class="views_panel">&nbsp;</div>
    {@/if}
</div>
</div>
{@/each}
</script>
