<script type="text/javascript">
jQuery(function($) {
	$("body").on("click", "a.publish,a.publishBtn", function() {
		var item = $(this);	
		$.verifyLogin(function() {
			$.ajaxSetup({cache:false});
			$.ajax({
				type : 'GET',
				url : "/post/validatepublish/18351",
				cache : false,
				async : false,
				success : function(data) {
					if (data.errorCode == 0) {
						$(item).attr("target", "_blank");
						$(item).attr("href", "/bsite/publish/18351.html?blockId=" + $('#blockId').val());
					} else {
						$(item).removeAttr("target");
						$(item).attr("href", "javascript:void(0);");
						$().alert(getMsg(data.errorCode));
					}
				},
				error : function() {}
			});
		}, function() {
			$().alert(getMsg(100));
		});
	});
});
</script>
