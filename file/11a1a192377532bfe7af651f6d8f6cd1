<script type="text/javascript">
		function eTOc(str){

	   var arr = new Array("!","@","#","\\$","\%","\\^","\&","\\*","\\(","\\)","\\_","\\+","\-","\=","\\{","\\}","\\|","\\[","\\]","\\\\",";","'",":","\"","\<","\>","\\?","\,","\\.","\/");
	   var arr1 = new Array("！","◎","＃","￥","％","……","※","×","（","）","?D?D","＋","－","＝","『","』","§","【","】","÷","：","“","；","‘","《","》","？","，","\.","、");
	   var _str = str;

	   for(var i=0;i<arr.length;i++){
		 var re = new RegExp(arr[i],"img");
		 _str = _str.replace(re,arr1[i]);
	   }

		return _str;

	}
   function doSearch(theForm){
      if(theForm.searchwordvalue.value==''){
	    alert("请输入检索词！");
		return false;
	  }
var title = eTOc(theForm.searchwordvalue.value.replace(/^\s+|\s+$/g,""));
var searchword1 = ""
		var titleArray = title.split(" ");
		if(title != "")
		{
			for(var i=0; i<titleArray.length;i++)
			{
				if(i==0)
					searchword1 += titleArray[i];
				else
					searchword1 += " AND " + titleArray[i];								
			}
		}
		//alert(searchword1);
theForm.action = "http://search.jyb.cn:8080/was40/search?searchword="+encodeURI(searchword1);
  theForm.submit();
   }
</script>


