		var videoListTemplate = Handlebars.compile($("#_video_list_template").html());
		var curVideoTabIndex = 0;
		var curFromVideoIndex = 0;
		var curToVideoIndex = 0;
		var ONE_PAGE_VIDEO_COUNT = 5;

		function makeTemplateData(tabIndex, fromVideoIndex, toVideoIndex) {
			var oriVideoList = centerVideoListJson[tabIndex].videoList;
			var tplVideoData = {
				"videoList": []
			};
			for(var i = 0; i < oriVideoList.length; i++){
				if(i >= fromVideoIndex && i <= toVideoIndex){
					tplVideoData.videoList.push(oriVideoList[i]);
				}
			}
			return tplVideoData;
		}

		function drawNavBtn(totalVideoCount, fromVideoIndex, toVideoIndex) {
			var navBtns = $("#_home_vod_list_wrap").find(".nav");
			var onPrevBtn = navBtns.eq(0);
			var offPrevBtn = navBtns.eq(1);
			var onNextBtn = navBtns.eq(2);
			var offNextBtn = navBtns.eq(3);

			navBtns.hide();
			if(totalVideoCount < ONE_PAGE_VIDEO_COUNT){
				return;
			}

			if(fromVideoIndex === 0){
				offPrevBtn.show();
			}else{
				onPrevBtn.show();
			}

			if(toVideoIndex === totalVideoCount - 1){
				offNextBtn.show();
			}else{
				onNextBtn.show();
			}
		}

		function getCurTabVideoCount(tabIndex) {
			return centerVideoListJson[tabIndex].videoList.length;
		}

		function drawVideoList(tabIndex, fromVideoIndex){
			var toVideoIndex = fromVideoIndex + ONE_PAGE_VIDEO_COUNT - 1;
			var videoListData = makeTemplateData(tabIndex, fromVideoIndex, toVideoIndex);
			$("#_home_vod_list_wrap").html(videoListTemplate(videoListData));

			drawNavBtn(getCurTabVideoCount(tabIndex), fromVideoIndex, toVideoIndex);

			curFromVideoIndex = fromVideoIndex;
			curToVideoIndex = toVideoIndex;
		}

		function changeCenterVideoTab(tabIndex){
			drawVideoList(tabIndex, 0);
			curVideoTabIndex = tabIndex;
		}

		function initCenterVideo(){
			$("#_home_vod_tab").on("click", "a", function(e){
				e.preventDefault();
				$("#_home_vod_tab a").removeClass("selected");
				$(e.currentTarget).addClass("selected");

				changeCenterVideoTab($(e.currentTarget).attr("data-id"));
			});
			$("#_home_vod_list_wrap").on("click", "a.nav", function(e){
				e.preventDefault();
				var clicked = $(e.currentTarget);
				var videoListCount = getCurTabVideoCount(curVideoTabIndex);
				var fromVideoIndex = 0;

				if(clicked.hasClass("prev")){
					fromVideoIndex = Math.max(0, curFromVideoIndex - ONE_PAGE_VIDEO_COUNT);
				}else if(clicked.hasClass("next")){
					fromVideoIndex = Math.min(curToVideoIndex + 1, videoListCount - ONE_PAGE_VIDEO_COUNT);
				}

				drawVideoList(curVideoTabIndex, fromVideoIndex);
			});

			drawVideoList(0, 0);
		}
		initCenterVideo();
	})();
</script>



