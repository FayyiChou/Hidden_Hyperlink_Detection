<script type="text/javascript">
	for(var i=1; i<top_menu_list.length; i++){
		if(document.getElementById("lnbTopMenu_" + i).className == "on"){
			document.getElementById("lnb_menu_link").innerHTML = "<a href='#lnbSubMenu_" + i + "' onclick=\"document.getElementById('lnbSubMenu_" + i + "').tabIndex=-1;document.getElementById('lnbSubMenu_" + i + "').focus();\"><span>하위 메뉴로 바로가기</span></a>";
			break;
		}
	}

	var gnb_searchbox = "on";
	(function() {
		function loadGNB(){
			getGNB();
		}
		window.attachEvent ? window.attachEvent('onload', loadGNB) : window.addEventListener('load', loadGNB, false);
	})();
</script>
