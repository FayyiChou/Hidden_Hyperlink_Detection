<script>
    Handlebars.registerHelper('headLineTypeTag', function(type) {
        var typeToName = {
            "none": "",
            "video": "<span class=\"ico_vod\">영상</span>",
            "poll": "<span class=\"tag\">투표</span>",
            "radio": "<span class=\"tag\">라디오</span>",
            "photo": "<span class=\"tag\">포토</span>",
            "column": "<span class=\"tag\">칼럼</span>",
            "tv":"<span class=\"ico_vod\">중계</span>"
        };
        if (type == null || type === '') {
            return "";
        }
        return new Handlebars.SafeString(typeToName[type] || "");
    });
</script>


    
    
        
        <script>
            naver = window.naver || {};
            naver.sports = naver.sports || {};
            naver.sports.pc = naver.sports.pc || {};
            naver.sports.pc.Headline = eg.Class({
                construct: function (htOption) {
                    this._initVars(htOption);
                    this._compileTemplate();
                    this._setEventListener();
                },

                _initVars: function (htOption) {
                    this._headlineItems = htOption;
                    this._lastHeadlinePage = htOption.length > 4 ? 3 : htOption.length -1;
                    this._currentHeadlinePage = 0;
                    this._headlineRolling = true;
                },

                _setEventListener: function() {
                    var self = this;
                    $('#_pcSectionHeadLine')
                        .mouseenter(function() {
                            self._headlineRolling = false;
                        })
                        .mouseleave(function() {
                            self._headlineRolling = true;
                        })
                        .focusin(function() {
                            self._headlineRolling = false;
                        })
                        .focusout(function() {
                            self._headlineRolling = true;
                        })
                    $('#_pc_headline_thmb_list a').focusin(function() {
                        self.setHeadlinePage($(this).data('page'));
                    });
                },

                rolling: function() {
                    if (!this._headlineRolling) {
                        setTimeout($.proxy(function(){
                            this.rolling();
                        }, this), 1000);
                    } else {
                        this.nextHeadline();
                        setTimeout($.proxy(function(){
                            this.rolling();
                        }, this), 15000);
                    }
                },

                _compileTemplate: function() {
                    this._headlineHtmlContent = [];

                    for (itemindex = 0; itemindex <= this._lastHeadlinePage; itemindex++) {
                        var headlineData = this._headlineItems[itemindex];
                        var elTemplate = $("#_" + headlineData.templateId);
                        if (elTemplate.length) {
                            var template = Handlebars.compile(elTemplate.html());
                            this._headlineHtmlContent.push(template(headlineData.dataList));
                        }
                    }
                },

                nextHeadline: function () {
                    var nextPage = this._currentHeadlinePage + 1;
                    if (this._currentHeadlinePage == this._lastHeadlinePage) {
                        nextPage = 0;
                    }
                    this.setHeadlinePage(nextPage);
                },

                prevHeadline: function () {
                    var prevPage = this._currentHeadlinePage - 1;
                    if (this._currentHeadlinePage == 0) {
                        prevPage = this._lastHeadlinePage;
                    }
                    this.setHeadlinePage(prevPage);
                },

                setHeadlinePage: function (page) {
                    this._currentHeadlinePage = Number(page);
                    $('#_pc_headline_content').html(this._headlineHtmlContent[this._currentHeadlinePage]);
                    $('#_pc_headline_thmb_list li').removeClass("selected");
                    $('#_pc_headline_thmb_list li .mask').remove();
                    $('#_pc_headline_thmb_list li .blind').remove();
                    $('#_pc_headline_sub_thumb_' + this._currentHeadlinePage).addClass("selected");
                    $('#_pc_headline_sub_thumb_' + this._currentHeadlinePage).append("<span class=\"blind\">선택됨</span><span class=\"mask\"></span>");
                    this._setMainThumbnail();
                },

                _setMainThumbnail: function () {
                    var mainThumbnailDataIndex = 0;
                    //경기형 헤드라인은 dataList에 모바일용 title이 들어가기에 0번째 pass
                    if (this._headlineItems[this._currentHeadlinePage].templateId === 'section_home_headline_game') {
                        mainThumbnailDataIndex = 1;
                    }
                    $('#_pc_headline_main_thumb').attr('src', 'http://dthumb.phinf.naver.net/?src=' + this._headlineItems[this._currentHeadlinePage].dataList[mainThumbnailDataIndex].image + '&type=nf320_234');
                    $('#_pc_headline_main_thumb').parent().attr("href", $('#_headlineMainArticle').attr("href"));
                    $('#_pc_headline_main_thumb').parent().attr("target", $('#_headlineMainArticle').attr("target"));
                }


            });
            var pcSectionHeadline = new naver.sports.pc.Headline([{"itemId":"1490309843631","isEnable":"Y","title":"[TV중계] ANA 인스퍼레이션 3R","templateId":"section_home_headline","isFixed":"N","fixedPos":null,"dataTitleStyle":null,"dataSetForm":[{"name":"헤드라인포토","count":"1","form":[{"label":"포토제목","type":"text","var":"title"},{"label":"리드문","type":"textarea","var":"subContent"},{"label":"이미지(320x234)","type":"image","var":"image","option":[{"width":"320","height":"234","caption":"PC 종홈 (320 x 234)","type":"nf320_234"},{"width":"420","height":"264","caption":"PC 스홈 (420 x 264)","type":"nf420_264"},{"width":"576","height":"277","caption":"모바일 (576 x 277) 잘리면 모바일이미지로 편집해 주세요","type":"nf576_277"}]},{"label":"모바일 이미지(576x277)","type":"image","var":"mobileImage"},{"label":"URL","type":"text","var":"url"},{"label":"링크","var":"linkType","type":"select","option":[["","본창"],["_blank","새창"]],"defaultOption":""},{"label":"리그정보","type":"select","var":"sectionId","required":"true","bo":"sectionsFO","params":{}},{"label":"아이콘타입","var":"iconType","type":"select","option":[["none","없음"],["video","영상"],["tv","중계"],["poll","POLL"],["radio","라디오"],["photo","포토"],["column","칼럼"],["issue","이슈"]],"defaultOption":"none"}]},{"name":"기사","count":"3","form":[{"label":"제목","type":"text","var":"title"},{"label":"URL","type":"text","var":"url"},{"label":"링크","var":"linkType","type":"select","option":[["","본창"],["_blank","새창"]],"defaultOption":""},{"label":"아이콘타입","var":"iconType","type":"select","option":[["none","없음"],["video","영상"],["tv","중계"],["poll","POLL"],["radio","라디오"],["photo","포토"],["column","칼럼"],["issue","이슈"]],"defaultOption":"none"}]}],"contentId":"item.1490309843631","sectionId":"golf","newsType":"","mixPos":null,"template":{"templateId":"section_home_headline","templateName":"헤드라인","dataSetId":"home/section_home_headline","pushPos":"1","isAuto":"N","isGroup":"N","viewPc":{"view":"pc/home_headline","textView":null,"width":0,"id":"pc/home_headline"},"viewMobile":{"view":"mobile/section