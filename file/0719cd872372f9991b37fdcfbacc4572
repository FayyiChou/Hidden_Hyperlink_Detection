    $(document).ready(function (e) {
        $('.dcbg').click(function () {
            $('.lh-qdbox').hide();
            $(this).hide();
        });
        $('[gban]').click(function () {
            $('.lh-qdbox').hide();
            $('.dcbg').hide();
        });
        $('.dzp').click(function () {
            $('.lh-qdbox').hide();
            $('.dzp_box').show();
            $('.gban1').show();
            $(".dzp_box1").load("/Lottery/Lottery.aspx");
        });
        $('.gban1').click(function () {
            //$('.dzp_box').hide();
            //$(this).hide();
            window.location = window.location;
        });
    });
</script> 
