<script> 
	$(function(){
		$(".menu li a").each(function(){
			var menuH = $(this).outerWidth();
			$(this).find("em").css("width",menuH);
			$(this).hover(function(){
				if($(this).hasClass("cur")){
				} else {
					$(this).stop().find("em").slideDown(300);
				}
			},function(){
				if($(this).hasClass("cur")){
				} else {
					$(this).stop().find("em").slideUp(300);
				}
			});
		});
		
		$(".menu li").hover(function(){
			$(this).find("a.navPro").addClass("navProH");
			$(this).find("dl").fadeIn(300);
		},function(){
			$(this).find("a.navPro").removeClass("navProH");
			$(this).find("dl").fadeOut(300);
		});
		
		$(".menu li dl").hover(function(){
			$(this).parent("li").find("a.navPro em").slideDown(50);
		},function(){
			$(this).parent("li").find("a.navPro em").slideUp(50);
		});
		
		 $(".proSide li a").each(function () {
			if ($(this).attr("href") == window.location.pathname) {
				$(this).addClass('cur');
				return false;
			}
		});
		
		$(".inProTit li a").each(function () {
			if ($(this).attr("href") == window.location.pathname) {
				$(this).addClass('cur');
				return false;
			}
		});
		
		$(".iproWarp .typeList").hover(function(){
			$(".proBg").show();
			$(this).find(".proBg").hide();
		},function(){
			$(".proBg").hide();
		});

	})
</script>


