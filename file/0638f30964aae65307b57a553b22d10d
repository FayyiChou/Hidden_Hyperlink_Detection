function wq_ad_all_close(id, time, page, adposition) {
if (page == '0') {
$('#' + id).fadeOut(350);
} else {
$('#' + id + page + adposition).fadeOut(350);
}

time = parseInt(time);
if (time > 0) {
setcookie(id, 1, time);
}
}
