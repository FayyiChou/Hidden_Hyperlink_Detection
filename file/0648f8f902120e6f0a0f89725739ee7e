<script> 
(function(){
    var vfromObj = {
        getCookie: function(name){
            return (name=RegExp("(^| )"+name+"=([^;]*)(;|$)").exec(document.cookie)) ? name[2] :null;
        },
        setCookie: function(name, value){
            document.cookie = name+"="+value+"; domain=video.duowan.com; path=/";
        },
        getVfrom: function(){
            var vfrom = (window.location.href.match(/[^\w]*vfrom=(\w+web)/) || [,])[1];
            if(vfrom){
                this.setCookie('vfrom', vfrom);
            }else if( /http:\/\/v\.baidu\.com/.test(document.referrer) ){
                vfrom = 'vbaiduweb';
                this.setCookie('vfrom', vfrom);
            }else{
                vfrom = this.getCookie('vfrom');
            }
            if( vfrom && /\w+web$/.test(vfrom) ) return vfrom;
            return null;
        },
        replaceEmbed: function(vfrom){
            var _playObj = $('#video_embed');
            var _embedObj = _playObj.find('embed');
            var _flashvarsFrom = (_embedObj.attr('flashvars').match(/from=(\w+web)/) || [,])[1];
            if( vfrom !=_flashvarsFrom){
                var _flashvars =  _embedObj.attr('flashvars').replace(/from=\w+/, 'from='+vfrom);
                _embedObj.attr('flashvars', _flashvars);
                var _html = _playObj.html();
                _embedObj.remove();
                _playObj.html(_html);
            }
        },
        init: function(){
            if( vfrom = this.getVfrom() ){
                this.replaceEmbed(vfrom);
            } 
        }
    }
    vfromObj.init();            
})();
</script>
