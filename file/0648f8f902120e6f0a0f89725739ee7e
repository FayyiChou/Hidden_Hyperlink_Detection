<script>!window.jQuery && document.write(unescape('%3Cscript src="http://assets.dwstatic.com/common/js/jquery.js"%3E%3C/script%3E'));</script>

<script>        
$(function(){
        var vid = "3586235";
        var jsapi = 'http://video.duowan.com/jsapi/';

        //标签样式
        $(".tagvideo").eq(0).addClass('active');
        var text = $(".tagvideo").text();
        if ('' == text) {
            $(".tagvideo").hide();
        };
        $('.video-tag-list  a').bind('mouseover',function(){
            var index = $(this).index();
            $(this).addClass('active').siblings().removeClass('active');
            $('.sel_tag').eq(index).show().siblings().hide();
        });



        var CookieUtil = {
            get : function(name) {
                var arr = document.cookie.match(new RegExp("(^| )" + name
                        + "=([^;]*)(;|$)"));
                if (arr != null) return decodeURIComponent(arr[2]);
                return null;
            },
            set : function(name, value, expire, path, domain) {
                var exp = new Date();
                exp.setTime(exp.getTime() + expire * 1000);
                document.cookie = [ name, '=', encodeURIComponent(value),
                    expire ? '; expires=' + exp.toGMTString() : '',
                    path ? '; path=' + path : '',
                    domain ? '; domain=' + domain : '', ].join('');
            }
        };
     
    
        //数字格式转换
        function numFormat(num){
            var rs;
            if(num > 999999){
                rs = Math.round( num / 10000 ) + "万";
                return rs;
            }else if(num > 9999){
                rs = Math.round(num / 10000 * 10) / 10 + "万";
                return rs;
            }else if( num > 999 ){
                return parseFloat(num).toLocaleString();
            }else{
                return num;
            }
        }
        
        function fixVideoPlayNum(vid, playNum, channel){
            var salt = parseInt(parseInt(vid) * 1.2);
            var saltStr = ""+salt;
            salt = parseInt( saltStr.substr(saltStr.length - 3, 3) );
            salt = salt > 500 ? salt : (1000 - salt);
            return parseInt(playNum*1.8) + salt;        
        }
        
        //更新播放数 
        var updateVideoPlayNum = {
            init: function(vid){
                var _this = this;
                $.get('http://playstats.v.duowan.com/index.php?r=api/get', {vid:vid},
                    function(json){
                        if( !json[vid] ) return;
                        var new_num = numFormat(fixVideoPlayNum(vid, json[vid], 'kan'));
                        var old_num = $("#video_play_num").data('play-num');
                        $('#video_play_num').html(new_num);
                        if ( new_num != old_num ) {
                            _this.requestUpdate(vid);
                        }
                }, 'jsonp');
            },
            
            //请求服务器更新 
            requestUpdate:function(vid){
                 $.get(jsapi+'updateVideoPlayNum', {vid:vid}, function(){}, 'jsonp');         
            }
        };

        //更新评论数
        var updateVideoCommentNum = {
            init: function(vid){
                var _this = this;
                var uniqid = window.comment3Uniqid;
                var domain = window.qianwangbieyongzhegebianliang_domain || 'video.duowan.com';
                var url = window.qianwangbieyongzhegebianliang_url || '/play/'+vid+'.html';
            
                var apiUrl = 'http://comment3.duowan.com/index.php?r=comment/totaljson&uniqid='+uniqid+'&domain='+domain+'&url='+url+'&huya=1';
                $.get(apiUrl, {vid:vid},
                    function(json){
                        if( !json['show'] || !json['show']['total_num']) return;
                        var new_num = numFormat(json['show']['total_num']);
                        var old_num = $("#first_comment_count").data('comment-num');                            
                        if ( new_num != old_num ) {
                           //$('#first_comment_count').html(new_num);
                            _this.requestUpdate(vid);
                        }
                }, 'jsonp');
            },
            
            //请求服务器更新
            requestUpdate:function(vid){
                 $.get(jsapi+'updateVideoCommentNum', {vid:vid}, function(){}, 'jsonp');          
            }
        };

        //提供给播放器的全局变量，需要谨慎处理
        window.vhuyaPlayerNextVideo = {'title':'', 'url':''};
        var setNextVhuyaPlayerNextVideo = function(data){
            var videoInfo = $('#v-alist-list').find('.curPage ~ a');
            var alisNextUrl = videoInfo.attr('href');
            var alisNextTitle = videoInfo.eq(0).text();
            if ( typeof alisNextUrl !== 'undefined' &&  alisNextUrl !='') {
                vhuyaPlayerNextVideo.title = alisNextTitle;
                vhuyaPlayerNextVideo.url = alisNextUrl;   
            }else if(data){
                vhuyaPlayerNextVideo.title = data.video_title;
                vhuyaPlayerNextVideo.url = data.video_url;  
            }
        };
           
        //加载相关视频
        var videoRelate = {
            nextVideo: null,
            tpl_render : function(tpl, data){
                return tpl.replace(/\{([\w\_]+)\}/g, function(){
                    return (typeof data[arguments[1]]==='undefined')?'':data[arguments[1]];
                });
            },
            
            getRelateVid : function(vid){
                var _arr = window.location.href.match(/relate_vid=(\d+)/);
                return _arr && _arr[1] ? _arr[1] : vid;
            },
            
            getRealPlayNum : function(json, callback) {
                if( !json || !json[0] ) return ;
                //获取实时播放量
                var vidArr = [];
                for(var i in json){
                    vidArr.push(json[i]['vid']);
                }
                var vids = vidArr.join(',');
                var isRun = false;
                
                //超时回调
                setTimeout(function(){
                    if(false==isRun){
                        isRun = true;
                        callback([]);
                    }
                }, 5*1000);
                
                $.get('http://playstats.v.duowan.com/index.php?r=api/get', {vid:vids},
                    function(playNumJson){
                        if(false==isRun){
                            isRun = true;
                            callback(playNumJson);
                        }
                }, 'jsonp');           
            },
            
            renderData : function(vid, relate_vid, json, playNumJson){
                if( !json || !json[0] ) return ;                
                var tpl = '{onlive}\
                            <a href="{video_url}" data-dwclick="dwclick" data-ckstat="play_video_relate{k}" target="_blank" class="video-right-list-img">\
                                <img src="{video_cover}" alt="">\
                                {playing}\
                                <div class="video-right-list-mask"></div>\
                                <i class="video-right-list-icon"></i>\
                            </a>\
                            <div class="video-right-list-info">\
                                <a href="{video_url}" class="video-right-list-title" data-dwclick="dwclick" data-ckstat="play_video_relate{k}" >{video_title}</a>\
                                <span class="video-right-list-num"><i></i>{video_play_num}</span>\
                                <span class="video-right-list-comment"><i></i>{video_comment_num}</span>\
                            </div>\
                        </div>' ;                                             
                var html = '', curIndex=0;
                for(var i in json){
                    json[i]['k'] = i;
                    
                    //处理实时播放量
                    var _vid = json[i]['vid'];
                    if( typeof playNumJson[ _vid ] != 'undefined' ){
                        json[i]['video_play_num'] = numFormat(fixVideoPlayNum(_vid, playNumJson[ _vid ], json[i]['video_channel']));
                    }
                                       
                    if( vid == json[i]['vid'] ){
                        curIndex = i;
                        json[i]['playing'] = '<span class="video-right-list-time playing">正在播放</span>';
                        json[i]['onlive'] = '<div class="video-right-list-item clearfix onlive">';
                        if( typeof json[i-0+1] !== 'undefined' && !this.nextVideo){
                            this.nextVideo = json[i-0+1];
                        }
                    }else{
                        json[i]['playing'] = '<span class="video-right-list-time">'+json[i]['video_duration']+'</span>';
                        json[i]['onlive'] = '<div class="video-right-list-item clearfix">';
                    }
                    
                    if(i<15){
                        json[i]['video_url'] = json[i]['video_url'] + '#relate_vid='+relate_vid;
                    }           
                    html += this.tpl_render(tpl, json[i]); 
                }
                if( !this.nextVideo && vid != json[0]['vid']){
                    this.nextVideo = json[0];
                }
                $('#videoRelate').html( html );
                //Set ScrollTop
                var top = (curIndex-1)*100;
                if( top>0 ){
                    $('#relateRight').niceScroll().doScrollTop(top);
                }
            },
            
            init: function(vid){
                try{
                    var _this = this;
                    var relate_vid = _this.getRelateVid(vid);
                    $.get(jsapi + 'getVideoRelate', {relate_vid:relate_vid}, function(json){
                        _this.getRealPlayNum(json, function(playNumJson){
                            _this.renderData(vid, relate_vid, json, playNumJson);                            
                            setNextVhuyaPlayerNextVideo(_this.nextVideo);
                        })
                    }, 'jsonp');
                }catch(e){}                
            }
        };
                
        var addPlayRecord = function(vid){
            $.get(jsapi+'addPlayRecord', {vid:vid}, function(){},  'jsonp'); 
        }
        
        updateVideoPlayNum.init(vid);
        updateVideoCommentNum.init(vid);
        videoRelate.init(vid);
        addPlayRecord(vid);
                
        window.videoRelate = videoRelate;

        window.stop_cb = function(){
            if( window.vhuyaPlayerNextVideo.url ){
                var image = new Image;
                var statUrl = 'http://clickstats.duowan.com/index.php?r=stats/index&event=vhuya_continue_play';
                image.src= statUrl; 
                window.location.href = window.vhuyaPlayerNextVideo.url;
                return ;
            }
        };

        $('.zan').on('click', function(){
            var vid = $(this).data('vid');
            if( false == vid )return false;
            $.get('/index.php?r=video/support', {vid:vid}, function(data){
                if(data.rs){
                    var support = $('.zan').text();
                    support = parseInt(support)+1;
                    $('.zan').html("<i></i>"+ support );
                    $('.zan').addClass('zan--actived');
                }
            }, 'json')
        });
        
        function getIsLive(){
            var user_id = '2168163474';
            $.get('/index.php?r=video/huyalive', {user_id:user_id}, function(data){
                if( 1 == data['code'] ){
                    $("#isLive").attr('href',data['data']).show();
                }
            }, 'json')
        }
        getIsLive();


        /**
         * 记录多玩懂你业务所需cookie：
         * 已播放视频的最新五个专区
         */
         ~ function(){
             var ckName = 'DwKnowYouChannels';
             var channel = 'kan';
             var cStr = CookieUtil.get(ckName) || '';
             var cArr = cStr.split(',', 5);
             if (-1 == cArr.indexOf(channel)) {
                cArr.unshift(channel);
                for (var i in cArr) {
                    if (cArr[i] == '') {
                        delete cArr[i];
                    }
                }
             }
             if (cArr.length > 0) {
                cStr = cArr.join(',');
                CookieUtil.set(ckName, cStr);
             }
         }();

});

</script>
