<script>
    (function($){

        if( !$ ) return;
        var cont = {};
        cont.shareText = $.trim($(".play-title").text());
        cont.sharePic = $(".logo").eq(0).attr("src");
        cont.shareDes = $.trim($('#video-des').find('span').text());
        cont.shareUrl = window.location.href;

        var sina = "http://service.weibo.com/share/share.php?url="+cont.shareUrl+"&searchPic=false&title="+cont.shareText+"&appkey=2937485425&ralateUid=5617445110&pic=";
        //var sina = "http://service.weibo.com/share/share.php?url="+cont.shareUrl+"&searchPic=false&title="+cont.shareText+"&appkey=&ralateUid=&pic=";
        var bd = "http://tieba.baidu.com/f/commit/share/openShareApi?url="+cont.shareUrl+"&title="+cont.shareText+"&desc="+cont.shareDes+"&pic="+cont.sharePic;
        var qzone = "http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url="+cont.shareUrl+"&title="+cont.shareText+"&pics="+cont.sharePic;
        $('.bds_tsina').attr('href',sina)
        $('.bds_baidu').attr('href',bd)
        $('.bds_qzone').attr('href',qzone)

    })(jQuery)

</script>
