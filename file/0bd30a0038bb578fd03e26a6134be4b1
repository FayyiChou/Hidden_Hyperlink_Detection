var currentTime = new Date();
var month = currentTime.getMonth() + 1;
var day = currentTime.getDate();
var year = currentTime.getFullYear()



if ((day < '31' && year == '2016' && month == '7')) {
	window.addEvent('domready', function() {
		setTimeout('show_pop_alert()', 1000);
	});
}
function show_pop_alert() {
	var showalert = (document.cookie.indexOf('__pop_alert=1') == -1);
	showalert = 1;
	if(!showalert) return;

	window._pop_alert_overlay = new OverlayDiv(
		{
			duration:		800,
			transition:		Fx.Transitions.Cubic.easeOut,
			color:			'#ffffff',
			opacity:		0.8,
			close_on_click:		false
		}

	);


	var div = $('div_pop_alert');

	div.style.left = ((getPageSize().pageWidth / 2)-225) + 'px';
	div.style.top = '40px';
	div.setStyle('opacity', 0);
	div.setStyle('display', 'block');

	new Fx.Morph(div, {
		duration: 800
	}).start({
		opacity: 1
	});

	window._pop_alert_overlay.show();

}

function close_div_pop_alert() {
	$('div_pop_alert').style.display = 'none';

	window._pop_alert_overlay.hide();

	document.cookie = '__pop_alert=1';
}
</script>

