<script src="https://plutus.wumii.cn/javascripts/external/jquery-2.0.3.min.fe994ecb.js"></script>
<script>
    'use strict';

    $('.download-btn').on('click', function(){
        $('.popup-wrapper').show();
    });

    $('.close-btn').on('click', function(){
        $('.popup-wrapper').hide();
    });

    var currentIndex = 0;
    var nextIndex ;

    addMouseWheel();

    function MouseWheelHandler(e){
        var ele = window.event || e;
        var delta = Math.max(-1, Math.min(1, (ele.wheelDelta || -ele.detail)));

        removeMouseWheel();
        setTimeout(function(){
            addMouseWheel();
        },600);

        currentIndex = $('.main.current').index();

        if(delta>0){
            e.preventDefault();
        }

        if(delta>0){
            if(currentIndex===0){
                return ;
            }else{
                nextIndex = currentIndex-1;
            }
        }else{
            if(currentIndex===4){
                return ;
            }else{
                nextIndex = currentIndex+1;
            }
        }

        $('.main').eq(currentIndex).removeClass('current');
        $('.main').eq(nextIndex).addClass('current');
        $('.count-icon').eq(currentIndex).removeClass('current');
        $('.count-icon').eq(nextIndex).addClass('current');
    }

    function addMouseWheel(){
        if(window.addEventListener){
            window.addEventListener('mousewheel',MouseWheelHandler,false);
            window.addEventListener('DOMMouseScroll',MouseWheelHandler,false);
        }else{
            window.attachEvent('onmousewheel',MouseWheelHandler);
        }
    }

    function removeMouseWheel(){
        if(window.removeEventListener){
            window.removeEventListener('mousewheel',MouseWheelHandler,false);
            window.removeEventListener('DOMMouseScroll',MouseWheelHandler,false);
        }else{
            window.detachEvent('onmousewheel',MouseWheelHandler);
        }
    }

    $('.count-icon').on('click', function(){
        if($(this).hasClass('current')){
            return ;
        }

        currentIndex = $('.count-icon.current').index();
        nextIndex = $(this).index();

        $('.main').eq(currentIndex).removeClass('current');
        $('.main').eq(nextIndex).addClass('current');
        $('.count-icon').eq(currentIndex).removeClass('current');
        $('.count-icon').eq(nextIndex).addClass('current');
    });
</script>
