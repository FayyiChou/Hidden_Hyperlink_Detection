    document.writeln("<br><a onClick=\"right2.style.display = 'none'\" title=\"关闭上面的广告\"><img src=http://news.hdjwww.com/img/news/default/guanbi.gif border=0 /><\/a>");
	document.writeln("<\/div>");
    document.writeln("<\/div>");
    //------左侧各块结束
    document.writeln("<\/div>");
    document.writeln("<div id=\"rightDiv\" style=\"top:330px;left:5px\">");
