<script type="text/javascript" >
        //function $(id) { return document.getElementById(id) }

        var tab = document.getElementsByName("tip");
        //var spanname = $(".image-maps-conrainer div").find("span");

        //var spanname = document.getElementsByName("spantip"); //
        //var spanname = $("span[class='spantip']");
        //$("input[name='mobile']").val()


//        alert(tab.length);
//        alert(spanname.length);
        var current = 0; //设置当前帧的变量容器
        var speed = 2000; //速度设置

        function disAll() {//初始所有标签样式
            for (var i = 0; i < tab.length; i++) {
                tab[i].className = "";
                tab[i].style.visibility = "hidden";
                tab[i].style.height = "0px";
                tab[i].style.width = "0px";
//                spanname[i].className = "";
//                spanname[i].style.display = "none";
                //            alert(spanname[0].innerHTML);
            }
        }
        function setNow() {//获取当前帧的索引值
            for (var i = 0; i < tab.length; i++) {
                //alert("classname"+tab[i].className);
                if (tab[i].className == "tip") {
                    current = i;
                } else {
                    //current +=1;
                }
            }
        }
        for (var j = 0; j < tab.length; j++) {//设置手动切换
            tab[j].onmouseover = function () {
                clearInterval(h);
                disAll();
                this.className = "tip";
                setNow();
                tab[current].style.visibility = "visible";
                tab[current].className = "tip";
//                spanname[current].style.display = "block";
//                spanname[current].className = "now";
                $(".tip").animate({ height: "30px", width: "150" });
            }
            tab[j].onmouseout = function () {
                setNow();
                h = setInterval("goNext()", speed);
            }
        }
        function goNext() {//自动切换
            setNow(); //获取当前帧索引

            current += 1; //帧自增1
            // alert(" 当前 qian " + current);
            if (current >= parseInt(tab.length)) {//判断：如果当前帧索引值是否大于切换按钮总数，如果大于按钮总数则回到初始状态
                current = 0;
                //alert(" 当前current >= parseInt(tab.length) " + current);
                disAll();
                tab[0].style.visibility = "visible";
                tab[0].className = "tip";
//                spanname[0].className = "now";
//                spanname[0].style.display = "block";
                $(".tip").animate({ height: "30px", width: "150" });
                $(".tip").height(0);
                $(".tip").width(0);
            }
            else {

                disAll();
                tab[current].style.visibility = "visible";
                tab[current].className = "tip";
//                spanname[current].className = "now";
//                spanname[current].style.display = "block";
                $(".tip").animate({ height: "30px", width: "150" });
                $(".tip").height(0);
                $(".tip").width(0);
            }

        }
        var h = setInterval("goNext()", speed)//开始自动切换
        //    var MyMar = setInterval(Marquee, speed);


        //    tab.onmouseover = function () { clearInterval(MyMar) };
        //    tab.onmouseout = function () { MyMar = setInterval(Marquee, speed) };
</script>
