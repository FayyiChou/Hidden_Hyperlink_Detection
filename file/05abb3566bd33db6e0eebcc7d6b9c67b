<script type="text/javascript">
    $.ajax({
        type: "POST",
        url: "/ajax/submit_ajax.ashx?action=user_check_login",
        dataType: "json",
        timeout: 20000,
        success: function(data, textStatus) {
            if (data.status == "y") {
                $("#login_st").prepend('<li><a href="/logout.aspx">退出</a></li>');
                $("#login_st").prepend('<li><a href="/member/usercenter.aspx">会员中心(Hi，' + data.username + ')</a></li>');
            } else {
                $("#login_st").prepend('<li><a href="/register.aspx">注册</a></li>');
                $("#login_st").prepend('<li><a href="/login.aspx">登录</a></li>');
            }
        }
    });
</script>
    
    

<link rel="stylesheet" type="text/css" href="/css/jquery.suggest.css" />
<script src="/js/jquery/jquery.suggest.js" type="text/javascript"></script>
<script type="text/javascript">
    var searchTxt = '请输入您要搜索的商品关键字';
    function searchFocus(e) {
        if (e.value == searchTxt) {
            e.value = '';
            $('#keyword').css("color", "");
        }
    }
    function searchBlur(e) {
        if (e.value == '') {
            e.value = searchTxt;
            $('#keyword').css("color", "#999999");
        }
    }
    $('#keyword').css("color", "#999999");
    $(function() {
        var act = $("#act").val();
        $("#keyword").blur();
        suggest(act);

        $("#scbar_type").click(function(e) {
            $('#scbar_type_menu').show();
            e.stopPropagation(); //阻止事件冒泡，否则事件会冒泡到下面的文档点击事件
        });
        $(document).click(function() {
            $('#scbar_type_menu').hide();
        });
        $("#scbar_type_menu li").click(function(e) {
            $('#act').val($(this).attr("act"));
            $("#scbar_type").text($(this).text());
            $("#scbar_type_menu").hide();
            e.stopPropagation();
        });
    });
    function suggest(act) {
        $("#keyword").unbind();
        $("#keyword").suggest("/ajax/submit_ajax.ashx?action=ajaxback&t=" + act, {
            onSelect: function () {
                $("#search_act").submit();
            }
        });
    }
    function searchInput() {
        if ($("#keyword").val() == "") {
            alert("请输入搜索关键字");
            $("#keyword").focus();
            return false;
        }
    }</script>
