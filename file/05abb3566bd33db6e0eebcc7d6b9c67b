    <script type="text/javascript">
        $(function () {
            n = $("body"),
            n.find("[data-href]").bind("click", function () {
                window.location.href = $(this).data("href");
            });
            // body...
            $(".Categorylist ul li").hover(function (event) {
                /* Act on the event */
                $('.CategoryPop').hide();
                $(this).find('.CategoryPop').show();
                $(this).find('div').first().removeClass('item');
                $(this).find('div').first().addClass('itemHover');
            }, function (event) {
                // body...
                $('.CategoryPop').hide();
                // alert($(this).html());
                $(this).find('div').first().removeClass('itemHover');
                $(this).find('div').first().addClass('item');
            });

            // body...
            $(".nav-shop ul li").hover(function (event) {
                $(this).parent().find('.shop-goods').hide();
                $(this).find('.shop-goods').show();

            });
            $(".pic_list").css({ 'opacity': '1' });
            $('.pic_list a').hover(
                function () {
                    $(this).find('img').stop().fadeTo(500, 0.5);
                },
                function () {
                    $(this).find('img').stop().fadeTo(500, 1);
                }
            )
            $("img.lazy").lazyload({ effect: "fadeIn", event: "scroll" });
        })
    </script>
