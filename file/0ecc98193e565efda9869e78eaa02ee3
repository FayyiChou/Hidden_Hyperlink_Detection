<div id="sliderCourseFindernew">
                            <script type="text/javascript">
$(document).ready(function() {
    function checkWindowSize() {
        if (jQuery(window).width() < 978) {

            $('#course-finder-wrap').hide;
            $('#course-finder-wrap').stop();
                $('.courseCalc h3').unbind('click').click(function(){
                    $('#course-finder-wrap').toggle();
                });

        } else {

          $( ".courseCalc h3" ).unbind();
            $('#course-finder-wrap').show();    
            
        }
    }
    jQuery(window).resize(function () {

        checkWindowSize();
    });
    checkWindowSize();
});
</script>

