//***********模板设置***************
function mod_editFriblog(url)
{
	g_pop=new Popup({ contentType:1, isReloadOnClose:false, width:960, height:530 });
	g_pop.setContent("title","");
	g_pop.setContent("contentUrl",url);
	g_pop.build();
	g_pop.show();
	return false;
}
//***********编辑相册***************

