	<script>
	   function searchkey(){
          var searchkey = $("#searchkey").val();
          if(searchkey == ''){
             return false;
           }
          $.ajax({
                url: "http://218.95.173.238:8080/api/json/cms/list",
                type: "POST",
                dataType: "json",
                data : {"siteId":'297eff54532b771001542d31485c0003',"channelId":'',"searchkey":searchkey},
                async: false,
                success: function(data){
                  var conData = data;
                  var conList = conData.data;
                  var conHtml = "<div class=\"s-home\"><p><span class=\"s-homewz\">检索关键字："+searchkey+"，为您找到相关结果 "+conList.length+" 个</span></p></div>";
				  conHtml += "<div class=\"s-box\"><div class=\"s-con01Left fl\">";
                  for(i in conList){
                      var name = conList[i]["内容标题"];
                      var context = conList[i]["内容"];
                      var releaseDate = conList[i]["发布日期"];
                      var htmlUrl = conList[i]["HTML地址"];
					  htmlUrl = "http://www.nxeduyun.com/jcjy//jcjy/"+htmlUrl+".html";
                      conHtml +="<div class=\"s-nrlist01\">";
                      conHtml +="<h3><a href=\""+htmlUrl+"\" title=\""+name+"\" target=\"_blank\">"+name+"</a></h3>";
                      conHtml +=""+context+"";
                      conHtml +="<span class=\"s-date fr\">["+releaseDate+"]</span>";
                      conHtml +="</div>";
                  }
                  conHtml +="</div></div>";
                  $("#middle").html(conHtml);
               }
          });
      }
	</script>
