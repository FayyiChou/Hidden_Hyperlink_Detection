function showmsg(type,msg){
	fusion2.ui.showMsgbox
	({
		type : type,
		msg : msg,
		timeout :1500		
	})
}
function postBadGood(ftype,fid)
{
	var taget_obj = document.getElementById(ftype+fid);
	var saveid = GetCookie('badgoodid');
	if(saveid != null)
	{
		var saveids = saveid.split(',');
		var hasid = false;
		saveid = '';
		j = 1;
		for(i=saveids.length-1;i>=0;i--)
		{
			if(saveids[i]==fid && hasid) continue;
			else {
				if(saveids[i]==fid && !hasid) hasid = true;
				saveid += (saveid=='' ? saveids[i] : ','+saveids[i]);
				j++;
				if(j==10 && hasid) break;
				if(j==9 && !hasid) break;
			}
		}
		if(hasid) { fusion2.ui.showMsgbox({type: 3,msg: "这条评论您已表决过了喔",timeout: 1500}); return false;}
		else saveid += ','+fid;
		SetCookie('badgoodid',saveid,1);
	}
	else
	{
		SetCookie('badgoodid',fid,1);
	}
	myajax = new DedeAjax(taget_obj,false,false,'','','');
	myajax.SendGet2("/plus/feedback.php?aid="+fid+"&action="+ftype+"&fid="+fid);
}
function openShutManager(oSourceObj,oTargetObj,shutAble,oOpenTip,oShutTip){
var sourceObj = typeof oSourceObj == "string" ? document.getElementById(oSourceObj) : oSourceObj;
var targetObj = typeof oTargetObj == "string" ? document.getElementById(oTargetObj) : oTargetObj;
var openTip = oOpenTip || "";
var shutTip = oShutTip || "";
if(targetObj.style.display!="none"){
   if(shutAble) return;
   targetObj.style.display="none";
   if(openTip && shutTip){
    sourceObj.innerHTML = shutTip; 
   }
} else {
   targetObj.style.display="block";
   if(openTip && shutTip){
    sourceObj.innerHTML = openTip; 
   }
}
}
function getHttpParams(name) {
	var r = new RegExp("(\\?|#|&)"+name+"=([^&#]*)(&|#|$)");
	var m = location.href.match(r);
	return decodeURIComponent(!m?"":m[2]);
}
var openid = getHttpParams("openid");
var openkey = getHttpParams("openkey");
if(openid != "" && openkey != ""){
	SetCookie('openid',openid,1);
	SetCookie('openkey',openkey,1);
}
function LoadCommets(aid,page) {
	var taget_obj = document.getElementById('c' + aid);
	var waithtml = "<div style='line-height:50px'><img src='/images/loadinglit.gif' />评论加载中...</div>";
	var myajax = new DedeAjax(taget_obj, true, true, '', 'x', waithtml);
	myajax.SendGet2("/plus/feedback_ajax_nvliumang_count.php?dopost=getlist&aid=" + aid + "&page=" + page);
	DedeXHTTP = null
}
function PostComment(mgid) {
	var f = document.getElementById('feedback' + mgid);
	var msg = document.getElementById('commentc' + mgid).value;
	var nface = '6';
	var nfeedbacktype = 'feedback';
	var nvalidate = '';
	var nnotuser = '';
	var nusername = '';
	var npwd = '';
	var taget_obj = $DE('commetcontentNew' + mgid);
	var waithtml = "<div style='line-height:30px'><img src='/images/loadinglit.gif' />正在发送中...</div>";
	var patrn=/[\u4E00-\u9FA5]|[\uFE30-\uFFA0]/gi;
	if (msg == '' || msg == null || msg.length == 0) {
		showmsg(3,"评论内容不能为空");
		return
	};
	if (msg.length > 300) {
		showmsg(3,"请填写300字以内的评论");
		return
	};
	if(!patrn.exec(msg)){
		showmsg(3,"请填写评论文字");
		return
	}
	var myajax = new DedeAjax(taget_obj, false, true, '', '', waithtml);
	myajax.sendlang = 'gbk';
	myajax.AddKeyN('aid', mgid);
	myajax.AddKeyN('dopost', 'send');
	myajax.AddKeyN('notuser', 0);
	myajax.AddKeyN('fid', f.fid.value);
	myajax.AddKeyN('feedbacktype', nfeedbacktype);
	myajax.AddKeyN('msg', msg);
	myajax.SendPost2('/plus/feedback_ajax_nvliumang_count.php');
	document.getElementById('commentc' + mgid).value = '';
	f.fid.value = 0;
	showmsg(2,"评论成功");
}
function GoTo() {
	var PageNo = $("#PageNo").val();
	var totalpage = $("#totalpage").html();
	tmp = parseInt(PageNo);
	path = '';
	if (isNaN(tmp)) {
		alert('请输入有效页数，1—' + totalpage);
		PageNo.select();
		return false
	} else {
		if (tmp < 1 || tmp > totalpage) {
			alert('请输入1—' + totalpage + '之间的数字');
			PageNo.select();
			return false
		} else {
			if (tmp == 1) {
				alert('呃，第1页就懒得转了；点"首页"试试？');
				return false
			} else {
				path = 'index_' + tmp + '.html';
				location.href = path
			}
		}
	}
}
