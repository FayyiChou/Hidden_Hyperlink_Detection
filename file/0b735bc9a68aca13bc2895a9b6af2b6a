    $(function(){
        $(document).unbind().bind('click',function(){
            hideTips();
        });

    });
    var $iframe = $('#iframeRight'),iframeOffset = $iframe.offset();
    function showTips(offset, content,qlid,info,myKey,hits,miss){
        var $obj = $('#service-assess');
        /* if($obj.length==0){
         $('body').append('<div id="tooltip"></div>');
         $obj = $('#tooltip');
         $obj.unbind().bind('click',function(e){
         e.stopPropagation();
         });
         }
         $obj.html(content).css({
         "top":(offset.top+iframeOffset.top)+"px",
         "left":(iframeOffset.left-$obj.width()-30)+"px"
         }).show();*/
        $(".like-icon").text(hits);
        $(".unlike-icon").text(miss);
        $(".detailed-text").addClass("hide");
        $(".service-assess-main p").removeClass("hide");
        $(".feedback-form").removeClass("hide");
        $obj.unbind().bind('click',function(e){
            e.stopPropagation();
            $(".feedback-frame-menu").addClass("hide");
        });
        $('.unlike-icon').click(function(){
            $(".feedback-frame").removeClass("hide");
        });
        $(".like-icon").unbind().bind("click",function(){
            var dealType=1;
            $(".feedback-frame").addClass("hide");
            Helper.getRemoteJsonData(Helper.Urls.submitQusetion,{myKey:myKey,info:info,dealType:dealType,questionLogId:qlid},function(d){

                if(d.message==""){
                    $(".detailed-text").removeClass("hide");
                    $(".service-assess-main p").addClass("hide");
                    $(".feedback-form").addClass("hide");
                }else{
                    banWindow("不能重复评价")
                }

            })
        });
        $(".feedback-frame-menu").find("li").unbind().bind("click",function(){
            var dealType=2;
            var reasonId=$(this).attr("reasonid");
            Helper.getRemoteJsonData(Helper.Urls.submitQusetion,{myKey:myKey,info:info,dealType:dealType,questionLogId:qlid,reasonId:reasonId},function(d){

                if(d.message==""){
                    $(".detailed-text").removeClass("hide");
                    $(".service-assess-main p").addClass("hide");
                    $(".feedback-form").addClass("hide");
                    $(".feedback-frame-text").text($(this).text()).attr("reasonid",$(this).attr("reasonid"));
                    $(".feedback-frame-menu").addClass("hide");
                    $(".feedback-frame").addClass("hide");
                }else{
                    banWindow("不能重复评价");
                    $(".feedback-frame-text").text($(this).text()).attr("reasonid",$(this).attr("reasonid"));
                    $(".feedback-frame-menu").addClass("hide");
                }

            });
            return false;

        });
        $(".feedback-frame-menu").addClass("hide");
        $(".feedback-frame-text").text("请选择原因");
        $(".service-content").html(content);
        $("#service-assess").css({
            "top":(offset.top+iframeOffset.top-20)+"px",
            "left":(iframeOffset.left-$obj.width()-50)+"px"
        }).show();

    }
    function hideTips(){
        /* $("#tooltip").hide();*/
        $("#service-assess").hide();
        $(".service-content").text("");
        $(".feedback-frame-menu").addClass("hide");
        $(".feedback-frame-text").text("请选择原因");
        $(".detailed-text").addClass("hide");
        $(".service-assess-main p").removeClass("hide");
        $(".feedback-form").removeClass("hide");
    }
    function banWindow(a){
        var click=true;
        if(click==true){
            click=false;
            $(".success").text(a);
            $(".success").fadeIn(1000,function(){
                var sss=function(){
                    $(".success").fadeOut(2000,function(){
                        click=true;
                    });
                };
                setTimeout(sss,2200)
            });
        }
        return false;

    }
</script>
