                    html=' <iframe src="'+src+'" id="main" name="main" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes" width="680" height="600"></iframe>';
                    $(".nboxleft").html(html)
                }
            }



        },
        /**
         * 在线客服右侧**
         **/
        serviceRight:function(){
            var _this=this;
            var gameType="";
            /*Helper.getRemoteJsonData(Helper.Urls.allgameMenu,{},function(d){
               console.log(d);
            });*/
            Helper.getRemoteJsonData(Helper.Urls.hotQusetion,{typeId:40},function(d){
                var data=d.data;
                _this.createHtml("serviceHotGuestion",{list:data},"service-questionmenu1");
                _this.clickEvent();
                /*  _this.hotJump();*/

            });
            Helper.getRemoteJsonData(Helper.Urls.hotQusetion,{typeId:38},function(d){
                var data=d.data;
                _this.createHtml("serviceHotGuestion",{list:data},"service-questionmenu2");
                _this.clickEvent();
                /*  _this.hotJump();*/

            });
            Helper.getRemoteJsonData(Helper.Urls.hotQusetion,{typeId:3},function(d){
                var data=d.data;
                _this.createHtml("serviceHotGuestion",{list:data},"service-questionmenu3");
                _this.clickEvent();
                /*  _this.hotJump();*/

            });
            var userName=$.cookie('username');
            Helper.getRemoteJsonData(Helper.Urls.allgameMenu,{userId:userName},function(d){
                if(d.status==200){

                    if(d.data.lastGameName!=null){

                        var typeName= d.data.lastGameName;
                        for(var key in d.data.typeMap){
                            for(var i=0;i<d.data.typeMap[key].length;i++){
                                if($.trim(typeName)==d.data.typeMap[key][i].typeName){
                                    var typeId=d.data.typeMap[key][i].typeId;
                                    Helper.getRemoteJsonData(Helper.Urls.hotQusetion,{typeId:typeId},function(d){
                                        var data=d.data;
                                        _this.createHtml("serviceHotGuestion",{list:data},"service-questionmenu4");
                                        _this.clickEvent();
                                    });
                                }
                            }

                        }
                    }else{
                       var typeId=d.data.typeMap.ABCD[0].typeId;
                        Helper.getRemoteJsonData(Helper.Urls.hotQusetion,{typeId:typeId},function(d){
                            var data=d.data;
                            _this.createHtml("serviceHotGuestion",{list:data},"service-questionmenu4");
                            _this.clickEvent();
                        });
                    }
                }



                /*  _this.hotJump();*/

            });
            function clickMenu(a,b){
                $(a).bind("click",function(){
                    $(".service-questionmenu").addClass("hide");
                    $(b).removeClass("hide");
                })
            }
            clickMenu("#working","#service-questionmenu1");
            clickMenu("#recharge","#service-questionmenu2");
            clickMenu("#account-icon","#service-questionmenu3");
            clickMenu("#game-icon","#service-questionmenu4");
