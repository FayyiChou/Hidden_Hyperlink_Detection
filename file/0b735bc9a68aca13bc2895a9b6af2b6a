    var Index=function(){
        var _this=this;
        seajs.use("common.helper",function(helper){
            window.Helper=helper;
            _this.init();
        })
    };
    $.extend(Index.prototype, {
        init: function () {
            var _this = this;
            _this.serviceRight();
            _this.serviceDataSearch();
            _this.routineHtml();
            _this.answerMenu();
            _this.serviceLeft();
            _this.loadEvent();
        },
        createHtml: function (type, data, id, fn) {
            var html = Template[type](data);
            $('#' + id).html(html);
            fn && fn();
        },
        /**
         * 新登录退出判断
         */
        loadEvent:function(){
            $("#logins").click(function(){
                var url="";
                YPCSDK.checkLogin(function(d){
                    if(d){
                        if ((navigator.userAgent.indexOf('MSIE') >= 0)
                            && (navigator.userAgent.indexOf('Opera') < 0)){
                            jump1()
                        }else if (navigator.userAgent.indexOf('Firefox') >= 0){
                            jump1()
                        }else if (navigator.userAgent.indexOf('Opera') >= 0){
                            jump1()
                        }else{
                            jump2()
                        }

                        function jump1(){
                            if($.trim(document.location.host)=="kf.duowan.com"){
                                url='index.html';
                                YPCSDK.YYLogOut('http://kf.duowan.com/s/test/'+url);

                            }else if($.trim(document.location.host)=="kf.4366.com"){
                                url='index.html';
                                YPCSDK.YYLogOut('http://kf.4366.com/s/test/'+url);
                            }else{
                                url='index.html';
                                YPCSDK.YYLogOut('http://kf.duowan.com/s/test/'+url);
                            }
                        }
                        function jump2(){
                            if($.trim(document.location.origin)=="http://kf.duowan.com"){
                                url='index.html';
                                YPCSDK.YYLogOut('http://kf.duowan.com/s/test/'+url);
                            }else if($.trim(document.location.origin)=="http://kf.4366.com"){
                                url='index.html';
                                YPCSDK.YYLogOut('http://kf.4366.com/s/test/'+url);
                            }else{
                                url='index.html';
                                YPCSDK.YYLogOut('http://kf.duowan.com/s/test/'+url);
                            }
                        }
                    }else{
                        if(typeof(window.external)!='undefined'&&typeof(window.external.IsGuestMode)!='undefined') {
                            window.external.OpenLoginWnd();
                        }else{
                            YPCSDK.YYLogin();
                        }
                    }
                })
            });

        },
        /**
         * 客服左侧渲染
         */
        serviceLeft:function(){
            var _this=this;
            var typeId=_this.getParams('typeId');
            var product=_this.getParams('product')? _this.getParams('product'):"";
            var source=_this.getParams('source')? _this.getParams('source'):"";
            var src='',html="";
            if ((navigator.userAgent.indexOf('MSIE') >= 0)
                && (navigator.userAgent.indexOf('Opera') < 0)){
                types1()
            }else if (navigator.userAgent.indexOf('Firefox') >= 0){
                types1()
            }else if (navigator.userAgent.indexOf('Opera') >= 0){
                types1()
            }else{
                types2()
            }
            function types1(){

                if($.trim(document.location.host)=="kf.duowan.com"){
                    src='http://kf.game.yy.com/webim/user/toUserWebimServeForGmCenter.do?product='+product+'&source='+source+'&typeId='+typeId;
                    html=' <iframe src="'+src+'" id="main" name="main" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes" width="680" height="660"></iframe>';
                    $(".nboxleft").html(html)
                }else if($.trim(document.location.host)=="kf.4366.com"){
                    src='http://kf.game.4366.com/webim/user/toUserWebimServeForGmCenter.do?product='+product+'&source='+source+'&typeId='+typeId;
                    html=' <iframe src="'+src+'" id="main" name="main" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes" width="680" height="660"></iframe>';
                    $(".nboxleft").html(html)
                }else{
                    src='http://kf.game.yy.com/webim/user/toUserWebimServeForGmCenter.do?product='+product+'&source='+source+'&typeId='+typeId;
                    html=' <iframe src="'+src+'" id="main" name="main" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes" width="680" height="600"></iframe>';
                    $(".nboxleft").html(html)
                }
            }
            function types2(){
                if($.trim(document.location.origin)=="http://kf.duowan.com"){
                    src='http://kf.game.yy.com/webim/user/toUserWebimServeForGmCenter.do?product='+product+'&source='+source+'&typeId='+typeId;
                    html=' <iframe src="'+src+'" id="main" name="main" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes" width="680" height="600"></iframe>';
                    $(".nboxleft").html(html)
                }else if($.trim(document.location.origin)=="http://kf.4366.com"){
                    src='http://kf.game.4366.com/webim/user/toUserWebimServeForGmCenter.do?product='+product+'&source='+source+'&typeId='+typeId;
                    html=' <iframe src="'+src+'" id="main" name="main" frameborder="no" border="0" marginwidth="0" marginheight="0" scrolling="no" allowtransparency="yes" width="680" height="600"></iframe>';
                    $(".nboxleft").html(html)
                }else{
