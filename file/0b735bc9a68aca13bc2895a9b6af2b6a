             */
            qusetionType2:"http://gm.4366.com/gmCenter/user/data/productIssueTypes.do",
            /**
             * 我要提问添加问题接口（3.3.1）
             */
            addQusetion:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/issue/add.do",
            /**
             * 查看所有问题记录（3.4.1）
             */
            saveMenu:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/issue/list.do",
            /**
             * 提问记录详情（3.5.1）
             */
            askDetails:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/issue/detail.do",
            /**
