$('.login').on('click',function(){
    if(Helper.is4366){
        U4366SDK.checkLogin(function(d){
            if(d){
                U4366SDK.logout('');

            }else{
                U4366SDK.showLogin({type:2,style:2})
            }
        })
    }else{
        YPCSDK.checkLogin(function(d){
            if(d){
                YPCSDK.YYLogOut()
            }else{
                if(typeof(window.external)!='undefined'&&typeof(window.external.IsGuestMode)!='undefined') {
                    window.external.OpenLoginWnd();
                }else{
                    YPCSDK.YYLogin();
                }

            }
        })



    }
});
$(".register").on('click',function(){
    if(Helper.is4366){
        U4366SDK.showReg({style:2})
    }else{

        window.location='https://udb.duowan.com/register.do';

    }
});
