var istest=location.href.toLowerCase().indexOf('test')!=-1?true:false;

$(".query-i1").click(function(){



    if(Helper.is4366){
        var url = 'http://kf.4366.com/s/'+(istest?'test':'home')+'/indulge.html';
        U4366SDK.checkLogin(function(d){
            if(d){
                window.location = url;
            }else{
                U4366SDK.showLogin({type:2,style:2,url: url})
            }
        });
    }else{
        var url='http://kf.duowan.com/s/'+(istest?'test':'home')+'/indulge.html';
        YPCSDK.checkLogin(function(d){
            if(d){
                window.location=url;
            }else{
                if(typeof(window.external)!='undefined'&&typeof(window.external.IsGuestMode)!='undefined') {
                    window.external.OpenLoginWnd();
                }else{
                    YPCSDK.YYLogin(url);
                }

            }
        })
    }

});
