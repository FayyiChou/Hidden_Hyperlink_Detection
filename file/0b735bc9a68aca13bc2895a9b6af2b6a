(function($){

    var Index=function(){
        var _this=this;
        seajs.use("common.helper",function(helper){
            window.Helper=helper;
            _this.init();
        })
    };
    $.extend(Index.prototype, {
        init: function () {
            var _this = this;
            _this.topNew();

        },
        createHtml: function (type, data, id, fn) {
            var html = Template[type](data);
            $('#' + id).html(html);
            fn && fn();
        },
        topNew:function(){
            var html="";
            Helper.getRemoteJsonData(Helper.Urls.newContent,{pageSize:3,page:1},function(d){
                var data= d.data.noticeList;
                for(var i=0;i<data.length;i++){
                    html+='<li>\
                            <div class="drop"></div>\
                            <a href="new.html?id='+data[i].id+'" class="news-nav">'+data[i].title+'</a>\
                            <span class="news-date"></span>\
                          </li>'
                }
                $(".news-menu").html(html);

            })
        }
    });
    new Index();
})(jQuery);
</script>
<script>/**
 * Created by Administrator on 2015/8/12 0012.
