.footer_content div,.footer_content div a{color:#666;font-size:12px;font-family: "SimSun";line-height:21px;}
.footer_content div a{text-decoration: none;}
.footer_content div a:hover{color:#333;}
.footer_content .footer_pd{margin-left:-3px;}
.footer_content .footer_pd a,.footer_content .footer_pd span{padding:0 3px}
