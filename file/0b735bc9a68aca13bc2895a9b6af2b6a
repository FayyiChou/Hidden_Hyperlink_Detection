            starSystem:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/issue/grade.do",
            /**
             * 新获取myKey接口(二期在线客服右侧)
             */
            newGetKey:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/selfHelp/getKeySign.do",
            /**
             * 在线客服菜单
