/*菜单变换*********************************************************************************/
jQuery.fn.tabs = function(control){
    var el = $(this);
    var control = $(control);
    el.delegate("li", "click", function(){
        var tabName = $(this).data("tab");
        el.trigger("change.tabs", tabName);
    });
    el.bind("change.tabs", function(e, tabName){
        el.find("li").removeClass("activity-nav-c");
        el.find(">[data-tab='"+tabName+"']").addClass("activity-nav-c");
    });
    el.bind("change.tabs", function(e, tabName){
        control.children(".activity-menu").addClass("hide");
        control.find(">[data-tab='"+tabName+"']").removeClass("hide");
    });

};
$(".activity-top").tabs(".activity-main");
/*我要提问问题显示隐藏********************************************************************************************************/
function menuDisplay(a,b,c,d,e,f){
    $(a).click(function(){
        $(this).addClass(b);
        $(e).removeClass(f);
        $(c).removeClass("hide");
        $(d).addClass("hide");
    });
}
menuDisplay(".quiz-game","quiz-gclick",".quiz-game-main",".quiz-product-main",".quiz-product","quiz-pclick");
menuDisplay(".quiz-product","quiz-pclick",".quiz-product-main",".quiz-game-main",".quiz-game","quiz-gclick");
/*图片轮播************************************************************************************************************/
function imgPlay(a,b){
    var le=parseInt($(a).find(b).length);
    $(a).css("width",parseInt($(a).css("width"))*le);
    function turn() {
        $(document).ready(function () {
            $(a).animate({left:"-="+parseInt($(b).css("width"))},function(){
                var c=$(b).eq(0).html();
                $(b).eq(0).remove();
                $(b).eq(le-2).after('<li class="img">'+c+'</li>');
                $(a).css("left",0);
            })
        });
    }
    setInterval(turn,5000);
}
imgPlay(".img-menu",".img");
