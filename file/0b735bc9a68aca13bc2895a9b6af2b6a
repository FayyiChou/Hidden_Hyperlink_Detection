             */
            addToQusetion:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/issue/supply.do",
            /**
             * 撤销问题（3.7.1）
             */
            deleteQusetion:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/issue/cancel.do",
            /**
             * 评分系统
