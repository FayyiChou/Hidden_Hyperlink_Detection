/**
 * Created by Administrator on 2015/5/22 0022.
 */
define('common.helper',function(require){
    var is4366 = location.href.toLowerCase().indexOf('.4366.com')!=-1?true:false;
    var Helper={
        is4366: is4366,
        getRemoteData: function (/*string*/ url, /*object*/ dataObj, /*function*/fn, /*boolean*/ isCache) {
            $.ajax({
                url: url,
                type: 'get',
                dataType: 'script',
                data: dataObj || '',
                cache: isCache || false,
                success: function (data, textStatus, xhr) {
                    fn && fn(data);
                },
                error: function (xhr, textStatus, errorThrown) {
                    // lobby.console('getRecommendData:get data error')
                }
            });
        },
        //获取数据json格式
        getRemoteJsonData:function(/*string*/ url, /*object*/ dataObj, /*function*/fn, /*boolean*/ isCache){
            $.ajax({
                url:url,
                type:'post',
                dataType:'jsonp',
                data: dataObj || '',
                cache: isCache || false,
                success:function(data){
                    fn && fn(data);
                },
                error: function (xhr, textStatus, errorThrown) {
                    // lobby.console('getRemoteJsonData:get data error')
                }
            });
        },
        getJsonpCallBack:function(/*string*/ url, /*object*/ dataObj, /*function*/fn, /*boolean*/ isCache){
            $.ajax({
                url: url,
                type: 'get',
                dataType: 'jsonp',
                jsonp: 'jsonpcallback',  //回调的时候数据名称固定了才需要使用
                data: dataObj || '',
                success:function(data){
                    fn && fn(data);
                },
                error: function (xhr, textStatus, errorThrown) {
                    // lobby.console('getRemoteJsonData:get data error')
                }
            });
        },
        //获取页面参数
        getParams: function(key, url) {
            var search = url || location.search;
            if(search.indexOf('?')>=0){
                search=search.slice(1);
            }
            var paramsArray = search.split('&'), params , tmp;
            if (paramsArray && paramsArray.length) {
                params={};
                for (var i = 0, len = paramsArray.length; i < len; i++) {
                    tmp = paramsArray[i];
                    tmp && (tmp = tmp.split('='));
                    if (tmp && tmp.length) {
                        params[tmp[0]] = (tmp[1]&&decodeURIComponent(tmp[1]))||'';
                    }
                }
            }

            if (key) {
                return params[key];
            }
            return params;
        },
        Urls:{
            /**
             *所有游戏菜单(1.1.1)
             */
            allgameMenu:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/webService/listSelfTypes.do",
            /**
             * 搜索问题搜索提示过程(1.2.1)
             */
            searchProcess:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/webService/listTip.do",
            /**
             * 热门问题(1.3.1)
             */
            hotQusetion:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/webService/listHotQuestion.do",
            /**
             * 问题结果页(1.4.1)
             */
            qusetionResult:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/webService/answer.do",
            /**
             * 自助服务统计接口（新增）
             */
            selfHelpRecord:"http://robot.game."+(is4366?'4366':'yy')+".com//gmCenter/selfHelp/selfHelpRecord.do",
            /**
             * 新闻公告（1.5.1）
             */
            newContent:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/notice/list.do",
            /**
             *答案详情（1.6.1）
             */
            detailsMenu:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/webService/questionItem.do",
            /**
             * 游戏资料子菜单（2.1.1）
             */
            gameDataMenu:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/selfHelp/listTypes.do",
            /**
             * 游戏资料内容（2.2.1）
             */
            gameDataContent:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/selfHelp/listQuestions.do",
            /**
             * 游戏资料搜索（2.3.1）
             */
            gameSearchInput:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/selfHelp/search.do",
            /**
             * 问题上传反馈(2.4.1)
             */
            submitQusetion:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/selfHelp/feedbackReason.do",
            /**
             * 直接点击左侧子菜单（2.5.1）
             */
            getKey:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/selfHelp/getKeyAndSign.do",
            /**
             * 防沉迷查询(2.6.1)
             */
            indulgeQuery:"http://fcm."+(is4366?'4366':'duowan')+".com/user/fcm_get.do",
            /**
             * 防禁封页面加载时（2.7.1）
