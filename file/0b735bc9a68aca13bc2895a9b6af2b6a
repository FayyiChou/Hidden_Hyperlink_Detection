             */
            gslist:"http://udblogin.duowan.com/ext/gslist.do?game=fy&online&jsonp&weight=0"
        }
    };
    return Helper;
});;
/**
 * 客服中心首页
 */
(function($){
    var Template={
        topInputMenu:function(obj){
var __t,__p='',__j=Array.prototype.join,print=function(){__p+=__j.call(arguments,'');};
with(obj||{}){
__p+='';
var data;
__p+='\r\n';
for(var i=0,len=list.length;i<len;i++){
__p+='\r\n';

data=list[i];

__p+='\r\n    <li tnum='+
((__t=(i))==null?'':__t)+
'>'+
((__t=(data))==null?'':__t)+
'</li>\r\n';
}
__p+='';
}
return __p;
}
    };
    var Index=function(){
        var _this=this;
        seajs.use("common.helper",function(helper){
            window.Helper=helper;
            _this.init();
        })
    };
    $.extend(Index.prototype,{
        init:function(){
            var _this=this;

            _this.topInputMenu();
            _this.routineHtml();

        },
        createHtml: function (type, data, id, fn) {
            var html = Template[type](data);
            $('#' + id).html(html);
            fn && fn();
        },
        /**
         * 头部输入框
         * @constructor
         */
        topInputMenu:function(){
            var _this=this;
            var sl=(-1);
            $(".t-search-text").keyup(function(e){
                var change=true;
                var question=$(this).val();
                var keycode=e.keyCode;
                Helper.getRemoteJsonData(Helper.Urls.searchProcess,{question:question},function(d){
                    var data= d.data;
                    var le=$("#t-search-guide").find("li").length;
                    if(keycode==40){
                        change=false;
                        if(sl<le-1){
                            sl++;
                            $("#t-search-guide").find("li").removeClass("ts-click");
                            $("#t-search-guide").find("li").eq(sl).addClass("ts-click");
                            $(".t-search-text").val($("#t-search-guide").find("li").eq(sl).text());


                        }else if(sl>le-1 || sl==le-1){
                            $("#t-search-guide").find("li").removeClass("ts-click");
                            $("#t-search-guide").find("li").eq(le-1).addClass("ts-click");
                            $(".t-search-text").val($("#t-search-guide").find("li").eq(le-1).text());
                        }
                    }
                    if(keycode==38){
                        change=false;
                        if(sl>0){
                            sl--;
                            $("#t-search-guide").find("li").removeClass("ts-click");
                            $("#t-search-guide").find("li").eq(sl).addClass("ts-click");
                            $(".t-search-text").val($("#t-search-guide").find("li").eq(sl).text());
                        }else{
                            $("#t-search-guide").find("li").removeClass("ts-click");
                            $("#t-search-guide").find("li").eq(0).addClass("ts-click");
                            $(".t-search-text").val($("#t-search-guide").find("li").eq(0).text());
                        }
                    }
                    $(".t-search-text").blur(function(){
                        sl=(-1);
                        $("#t-search-guide").find("li").removeClass("ts-click");

                    });
                    $("#t-search-guide").find("li").click(function(){            /*点击菜单输入框显示内容*/
                        $("#t-search-guide").addClass("hide");
                        $(".t-search-text").val($(this).text());
                        return false;
                    });
                    if(change==true){
                        sl=(-1);
                        _this.createHtml("topInputMenu",{list:data},"t-search-guide");
                    }
                    topInput("#t-search-guide li");
                    if(keycode==13){
                        var passport="";
                        var question=$(".t-search-text").val();
                        if($.cookie('username')==null){
                            passport="";
                        }else{
                            passport=$.cookie('username')
                        }
                        /* _this.openUrl("question.html"+"?&question="+question+"&passport="+passport);*/
                        if(question==""){
                            var click=true;

                            if(click==true){
                                click=false;
                                $(".success").text("请输入问题");
                                $(".success").fadeIn(1000,function(){
                                    var sss=function(){
                                        $(".success").fadeOut(2000,function(){
                                            click=true;
                                        });
                                    };
                                    setTimeout(sss,2200)
                                });
                            }
                            return false;
                        }else{
                            document.location="question.html"+"?&question="+question+"&passport="+passport;
                        }

                    }
                })
            });
            function topInput(e){
                $(e).click(function(){
                    var passport="";
                    var question=$(".t-search-text").val();
                    if($.cookie('username')==null){
                        passport="";
                    }else{
                        passport=$.cookie('username')
                    }
                   /* _this.openUrl("question.html"+"?&question="+question+"&passport="+passport);*/
                    if(question==""){
                        var click=true;

                        if(click==true){
                            click=false;
                            $(".success").text("请输入问题");
                            $(".success").fadeIn(1000,function(){
                                var sss=function(){
                                    $(".success").fadeOut(2000,function(){
                                        click=true;
                                    });
                                };
                                setTimeout(sss,2200)
                            });
                        }
                        return false;
                    }else{
                        document.location="question.html"+"?&question="+question+"&passport="+passport;
                    }

                });
            }
            topInput(".t-search-icon2");
        },
        openUrl: function(url){
            if(/msie/g.test(navigator.userAgent.toLowerCase())){
                window.open(url, '_blank');
            }else{
                var a = $("<a href='"+url+"' target='_blank'>Game</a>").get(0);
                var e = document.createEvent('MouseEvents');
                e.initEvent( 'click', true, true );
                a.dispatchEvent(e);
            }
        },
        routineHtml:function(){

            $(".t-search-guide").find("li").click(function(){                 /*头部输入框渲染*/
                $(".t-search-guide").addClass("hide");
                $(".t-search-text").val($(this).text());
                return false;
            });
            $(".t-search-text").focus(function(){
                /*$(this).attr("value","");*/
                $(this).val("");
            });
            $('input').focus(function(){
                $(this).css('outline','none');//answer1
                $(this).css('outline-color','red');//answer2
            })
        }

    });
    new Index();
})(jQuery);</script>
