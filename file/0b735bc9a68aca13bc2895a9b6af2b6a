                var typeId=$(this).attr("typeId");
                $(".hm-bl-nav dt").removeClass("bxz");
                $(this).addClass("bxz");


            });
            $(".hm-bl-nav dt:eq(0)").click();

        },
        serviceDataSearch:function(){                                          //?????????????????????????
            var _this=this;
            var sl=(-1);
            $(".service-search-text").keyup(function(e){
                var question=$(this).val();
                var keycode=e.keyCode;
                var changes=true;
                Helper.getRemoteJsonData(Helper.Urls.searchProcess,{question:question},function(d){

                    var data= d.data;
                    var le=data.length;

                    if(keycode==40){
                        changes=false;
                        if(sl<le-1){
                            sl++;
                            $("#service-search-guide").find("li").removeClass("ts-click");
                            $("#service-search-guide").find("li").eq(sl).addClass("ts-click");
                            $(".service-search-text").val($("#service-search-guide").find("li").eq(sl).text());


                        }else if(sl>le-1 || sl==le-1){
                            $("#service-search-guide").find("li").removeClass("ts-click");
                            $("#service-search-guide").find("li").eq(le-1).addClass("ts-click");
                            $(".service-search-text").val($("#service-search-guide").find("li").eq(le-1).text());
                        }
                    }
                    if(keycode==38){
                        changes=false;
                        if(sl>0){
                            sl--;
                            $("#service-search-guide").find("li").removeClass("ts-click");
                            $("#service-search-guide").find("li").eq(sl).addClass("ts-click");
                            $(".service-search-text").val($("#service-search-guide").find("li").eq(sl).text());
                        }else{
                            $("#service-search-guide").find("li").removeClass("ts-click");
                            $("#service-search-guide").find("li").eq(0).addClass("ts-click");
                            $(".service-search-text").val($("#service-search-guide").find("li").eq(0).text());
                        }
                    }
                    $(".service-search-text").blur(function(){
                        sl=(-1);
                        $("#service-search-guide").find("li").removeClass("ts-click");

                    });
                    $(document).click(function(){
                        $("#service-search-guide").addClass("hide");
                    });
                    if(changes==true){
                        sl=(-1);
                        _this.createHtml("InputMenu",{list:data},"service-search-guide");
                        $("#service-search-guide").find("li").click(function(){            /*点击菜单输入框显示内容*/
                            $("#service-search-guide").addClass("hide");
                            $(".service-search-text").val($(this).text());
                            return false;
                        });
                    }
                });
                if(keycode==13){
                    $(".service-search-icon").click();
                    $("#service-search-guide").addClass("hide");
                }
            });
        },
        answerMenu:function(){
            var _this=this;
            $(".service-search-icon").click(function(){
                var html="";
                var question=$(".service-search-text").val();
                Helper.getRemoteJsonData(Helper.Urls.qusetionResult,{question:question},function(d){
                    if(d.data==""){
                        window.parent.banWindow("亲！请搜索其他问题试试");
                    }else if(d.result==false){
                        window.parent.banWindow("请输入问题");
                    }

                    var adata= d.data.answerList;
                    var rdata= d.data.relativeAnswerList;
                    var len1=adata.length;
                    var len2=rdata.length;
                    $(".hm-bl-nav dt").removeClass();
                    $("#game-icon").addClass("bxz");
                    $("#game-icon").addClass("dt-click");
                    $(".service-questionmenu").addClass("hide");
                    $("#service-questionmenu4").removeClass("hide");
                    if(len1!=undefined){
                        for(var i=0;i<len1;i++){
                            html+='<li>\
                                <a questionid="'+adata[i].questionId+'" >'+adata[i].content+'</a>\
                                <div class="hide" hits="'+adata[i].hits+'" miss="'+adata[i].miss+'">'+adata[i].answer+'</div>\
                            </li>';
                        }

                        $("#service-questionmenu4").html(html);
                        _this.clickEvent()
                    }
                    if(len2!=undefined){
                        for(var a=0;a<len2;a++){
                            html+='<li>\
                                <a questionid="'+rdata[a].questionId+'">'+rdata[a].content+'</a>\
                                 <div class="hide" hits="'+rdata[i].hits+'" miss="'+rdata[i].miss+'">'+rdata[i].answer+'</div>\
                            </li>';
                        }
                        $("#service-questionmenu").html(html);
                        _this.clickEvent()
                    }
                })
            })
        },
        clickEvent:function(){
            var info="";
            var myKey="";
            var questionLogId="";
            $(".service-questionmenu").find("li").find('a').unbind().bind('click', function(e){
                var question=$(this).text();
                var questionId=$(this).attr("questionid");
                $(".service-questionmenu").find("li").removeClass("down");
                $(this).parent("li").addClass("down");
                var _this=this;
                    Helper.getRemoteJsonData(Helper.Urls.newGetKey,{questionId:questionId},function(d){
                        info= d.data.info;
                        myKey= d.data.myKey;
                        questionLogId= d.data.questionLogId;
                        Helper.getRemoteJsonData(Helper.Urls.qusetionResult,{question:question},function(d){

                            if(d.data==""){
                                window.parent.banWindow("亲！请选择其他问题");
                            }else{
                                var hits=d.data.answerList[0].hits;
                                var miss= d.data.answerList[0].miss;
                                $(_this).parent("li").parent("#service-questionmenu").find("li").removeClass("down");
                                $(_this).parent("li").addClass("down");
                                window.parent.showTips($(_this).offset(), $(_this).parent("li").find("div").html(),questionLogId,info,myKey,hits,miss);
                            }
                        });
                    });
                e.stopPropagation();

            });
            $(document).unbind().bind('click',function(){
                window.parent.hideTips();
            });
        },
        /**
         * 获取页面参数
         * @param key
         * @param url
         * @returns {*}
         */
        getParams: function (key, url) {
            var search = url || location.search;
            if(search.indexOf('?')>=0){
                search=search.slice(1);
            }
            var paramsArray = search.split('&'), params , tmp;
            if (paramsArray && paramsArray.length) {
                params={};
                for (var i = 0, len = paramsArray.length; i < len; i++) {
                    tmp = paramsArray[i];
                    tmp && (tmp = tmp.split('='));
                    if (tmp && tmp.length) {
                        params[tmp[0]] = (tmp[1]&&decodeURIComponent(tmp[1]))||'';
                    }
                }
            }
            if (key) {
                return params[key];
            }
            return params;
        },
        routineHtml:function(){
            $(".service-search-text").click(function(){
                $("#service-search-guide").removeClass("hide");
                return false;
            });

            $(".t-search-guide").find("li").click(function(){                 /*头部输入框渲染*/
                $(".t-search-guide").addClass("hide");
                $(".t-search-text").val($(this).text());
                return false;
            });
            $(".t-search-text").focus(function(){
                $(this).attr("value","");
            });

        }
    });
    new Index();
})(jQuery);</script>
