<script>/**
 * Created by Administrator on 2015/7/22 0022.
 */
var is4366 = location.href.toLowerCase().indexOf('.4366.com')!=-1?true:false;
if(is4366){
    $(".home-main-tr").remove();
    U4366SDK.getUserInfo(function(d){

        if(d.status==200){
            $("#username").text("您好  "+ d.data.account);
            $(".login").text("退出");
            $(".register").addClass("hide");
            $(".hm-tr-title").addClass("hide");
            $(".hm-tr-text").addClass("hide");
            $(".hm-tr-button").addClass("hide");
           /* $(".logined-menu").removeClass("hide");*/
        }else{
            $(".login").text("登陆");
            $(".register").removeClass("hide");
            $("#username").text("");
            $(".hm-tr-title").removeClass("hide");
            $(".hm-tr-text").removeClass("hide");
            $(".hm-tr-button").removeClass("hide");
           /* $(".logined-menu").addClass("hide");*/
        }

    });
}else{

    if($.cookie('username')==null){
        $(".login").text("登陆");
        $(".register").removeClass("hide");
        $("#username").text("");
        $(".hm-tr-title").removeClass("hide");
        $(".hm-tr-text").removeClass("hide");
        $(".hm-tr-button").removeClass("hide");
      /*  $(".logined-menu").addClass("hide");*/
    }else{
        $("#username").text("您好  "+$.cookie('username'));
        $(".login").text("退出");
        $(".register").addClass("hide");
        $(".hm-tr-title").addClass("hide");
        $(".hm-tr-text").addClass("hide");
        $(".hm-tr-button").addClass("hide");
      /*  $(".logined-menu").removeClass("hide");*/
    }


}

</script>
<script>/**
 * Created by Administrator on 2015/8/1 0001.
 */
/**
 * Created by Administrator on 2015/5/22 0022.
 */
define('common.helper',function(require){
    var is4366 = location.href.toLowerCase().indexOf('.4366.com')!=-1?true:false;
    var Helper={
        is4366: is4366,
        getRemoteData: function (/*string*/ url, /*object*/ dataObj, /*function*/fn, /*boolean*/ isCache) {
            $.ajax({
                url: url,
                type: 'get',
                dataType: 'script',
                data: dataObj || '',
                cache: isCache || false,
                success: function (data, textStatus, xhr) {
                    fn && fn(data);
                },
                error: function (xhr, textStatus, errorThrown) {
                    // lobby.console('getRecommendData:get data error')
                }
            });
        },
        //获取数据json格式
        getRemoteJsonData:function(/*string*/ url, /*object*/ dataObj, /*function*/fn, /*boolean*/ isCache){
            $.ajax({
                url:url,
                type:'post',
                dataType:'jsonp',
                data: dataObj || '',
                cache: isCache || false,
                success:function(data){
                    fn && fn(data);
                },
                error: function (xhr, textStatus, errorThrown) {
                    // lobby.console('getRemoteJsonData:get data error')
                }
            });
        },
        getJsonpCallBack:function(/*string*/ url, /*object*/ dataObj, /*function*/fn, /*boolean*/ isCache){
            $.ajax({
                url: url,
                type: 'get',
                dataType: 'jsonp',
                jsonp: 'jsonpcallback',  //回调的时候数据名称固定了才需要使用
                data: dataObj || '',
                success:function(data){
                    fn && fn(data);
                },
                error: function (xhr, textStatus, errorThrown) {
                    // lobby.console('getRemoteJsonData:get data error')
                }
            });
        },
        //获取页面参数
        getParams: function(key, url) {
            var search = url || location.search;
            if(search.indexOf('?')>=0){
                search=search.slice(1);
            }
            var paramsArray = search.split('&'), params , tmp;
            if (paramsArray && paramsArray.length) {
                params={};
                for (var i = 0, len = paramsArray.length; i < len; i++) {
                    tmp = paramsArray[i];
                    tmp && (tmp = tmp.split('='));
                    if (tmp && tmp.length) {
                        params[tmp[0]] = (tmp[1]&&decodeURIComponent(tmp[1]))||'';
                    }
                }
            }

            if (key) {
                return params[key];
            }
            return params;
        },
        Urls:{
            /**
             *所有游戏菜单(1.1.1)
             */
            allgameMenu:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/webService/listSelfTypes.do",
            /**
             * 搜索问题搜索提示过程(1.2.1)
             */
            searchProcess:"http://robot.game."+(is4366?'4366':'yy')+".com/gmCenter/webService/listTip.do",
            /**
             * 热门问题(1.3.1)
             */
