            loadMain:"http://udblogin.duowan.com/ext/gslist.do?online&jsonp",
            /**
             * 用户最近玩过的游戏（2.8.1）
             */
            recentlyGame:"http://userdata.udblogin."+(is4366?'4366':'duowan')+".com/ext/recently.do?javascript",
            /**
             * 点击查询（2.9.1）
             */
            queryContent:(is4366?'http://robot.game.yy.com/webService/searchSelfHelp.do':"http://garbage.game.yy.com/query/userStatus.do"),
            /**
             * 活动查询接口(2.11.1)
             */
            searchPlatform:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/inquiry/searchPlatform.do",
            /**
             * 活动查询加载（新增）
             */
            loadPlatformActivity:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/inquiry/loadPlatformActivity.do",
            /**
             * 全部奖励记录（2.12.1）
             */
            searchPlatformReward:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/inquiry/searchPlatformReward.do",
            /**
             * 客服专区进入我要提问时请求（3.1.1）
             */
            checkCanAdd:"http://gm."+(is4366?'4366':'duowan')+".com/gmCenter/user/issue/checkCanAdd.do",
            /**
