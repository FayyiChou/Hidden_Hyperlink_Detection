<script type="text/javascript">

var sUserAgent = navigator.userAgent;
//네이버 앱에서 사용하는  글로벌 변수입니다. (삭제시 네이버앱에 문의.)
var naverAppFloatingToolBarBottomMargin = 57;
window.onpageshow = function(evt) {
 	if(!!sUserAgent &&  sUserAgent.indexOf("NAVER") > -1  &&  sUserAgent.indexOf("search") > -1){
 		function setFloatingToolbarBottomMargin(bottomMargin) {
		    var tempIframe = document.createElement('iframe');
		    tempIframe.style.display = 'none';
		    tempIframe.src = 'naverapp://floatingtoolbar?bottomMargin=' + bottomMargin;
		    document.documentElement.appendChild(tempIframe);
		}
 		//네이버앱보다 로딩이 빠르면 툴바가 겹치는 현상 발생
 		setTimeout(function(){setFloatingToolbarBottomMargin(naverAppFloatingToolBarBottomMargin);}, 500);
	}
};
</script>







 <script type="text/javascript">
var nJsRetryCnt = 5;

//onload시점에 call
jQuery(function() {
	if(!!oCafeUtil){
		oCafeUtil.makeJsElement("splugin", "http://spi.naver.net" , "/js/release/ko_KR/splugin.m.js",  "utf-8");
		//splugin.m.js(31KB) 평균(Good 3G - 1.5Mb/s)에서 wait포함 download가 1.5초정도 소요됨.  
		setTimeout(makeSocialPlugInCore, 500);   
	}
});
     
     
function makeSocialPlugInCore() {
   	if(typeof SocialPlugIn_Core == 'undefined' ) {
   		if(nJsRetryCnt > 0) {
			nJsRetryCnt = -- nJsRetryCnt;
			setTimeout(makeSocialPlugInCore, 500); 
		}   			
		return;
 	}
    
    window.__splugin = SocialPlugIn_Core({
        "evKey"       : "cafe",
        "serviceName" : "카페",
        "sourceName"  : "[롤 더 난투] 공식 카페",
        "dimmed"      : "default"
    });
}

window.resize = function() {
    if (window.__splugin) {
        window.__splugin.resize();
    }
}

if ("onorientationchange" in window) {
    window.onorientationchange = function() {
        if (window.__splugin) {
            window.__splugin.resize();
        }
    }
}

function splugin_oninitialize() {
    return htParameter;
}
</script>
