<script type="text/javascript">
function adCR(adcrUrl, p, hrefUrl) {
    var l = adcrUrl + "&p=" + p;
    if (document.images) {
        var img = new Image();
        if (typeof hrefUrl != "undefined") {
            img.onerror = function (event) {
                document.location = hrefUrl;
            };
        }
        img.src = l;
    } else {
        document.location = l;
    }
};
</script>


	

