    <script type="text/javascript">
        function ToSelectUrl(obj) {
            var url = obj.value;
            if (url != "") {
                window.open(url, null, "top=5,left=5,width=600,resizable=yes,height=400,menubar=yes,toolbar=yes,scrollbars=yes,status=yes,location=yes");
            }
        }

        function getNames(obj, name, tij) {
            var p = document.getElementById(obj);
            var plist = p.getElementsByTagName(tij);
            var rlist = new Array();
            for (i = 0; i < plist.length; i++) {
                if (plist[i].getAttribute("name") == name) {
                    rlist[rlist.length] = plist[i];
                }
            }
            return rlist;
        }

        function butong_net(obj, name) {
            var p = obj.parentNode.getElementsByTagName("td");
            var p1 = getNames(name, "f", "div");
            for (i = 0; i < p1.length; i++) {
                if (obj == p[i]) {
                    p[i].className = "s";
                    p1[i].className = "dis";
                }
                else {
                    p[i].className = "e";
                    p1[i].className = "undis";
                }
            }
        }

    </script>

    <script type="text/javascript">
        function String.prototype.Trim() {
            return this.replace(/(^\s*)|(\s*$)/g, "");
        }
        function GetInSiteSearchText(linkObj) {
            var searchText = window.document.getElementById("txtKeyWordInSite").value;
            searchText = searchText.toString().Trim();
            if (searchText == "") {
                alert("关键字不能为空");
                window.event.returnValue = false;
            }
            else {
                linkObj.href = "Search/SearchResult.aspx?key=" + escape(searchText);
            }
        }
        function GetGoogleSearchText(linkObj) {
            linkObj.href = "http://www.google.cn/search?hl=zh-CN&q=" + window.document.getElementById("txtKeyWordGoogle").value;
        }
        function keyDownInSite() {
            if (window.event.keyCode == 13) {
                document.getElementById("linkInSite").click();
                event.returnValue = false;
            }
        }
        function keyDownGoogle() {
            if (window.event.keyCode == 13) {
                document.getElementById("linkGoogle").click();
                event.returnValue = false;
            }
        }
        function document.onkeydown() {
            if (event.keyCode == 13) {
                event.returnValue = false;
            }
        }
        function valiLogin() {
            var name = document.getElementById('TxtUserName');
            var pwd = document.getElementById('TxtPassword');
            if (name.value == "" || pwd.value == "") {
                alert("请输入用户名和密码");
                return false;
            }
            else {
                return true;
            }
        }

    </script>

