<script type="text/javascript">
function changebutton(){
var url = location.search;
            if (url.indexOf("?lang-cn") != -1) {
	var urltext = location.href.replace("?lang-cn","?lang-en"); 
window.open(urltext,"_self");
}else{
	var urltext = location.href.replace("?lang-en","?lang-cn"); 
window.open(urltext,"_self");
			};};
</script>
