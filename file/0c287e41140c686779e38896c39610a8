<!---- Modal Form Ajax request---->
<script>

    $("#myform").submit(function(e)
    {
		var error=true;
		var value = $('#input-4').val();

		if (value=="") {
			$('#name-error').html('Required field.').fadeIn();
			$('#input-4').focus();
			error=false;
		}else if (value.trim() !== value) {
			$('#name-error').html('Empty spaces before the first character are not allowed.').fadeIn();
			$('#input-4').focus();
			error=false;
		}else{
			$('#name-error').fadeOut();
		}
		var value1 = $('#input-5').val();
		if (value1=="") {
			$('#company-error').html('Required field.').fadeIn();
			$('#input-5').focus();
		error=false;
		}else if (value1.trim() !== value1) {
			$('#company-error').html('Empty spaces before the first character are not allowed.').fadeIn();
			$('#input-5').focus();
		error=false;
		}else{
			$('#company-error').fadeOut();
		}
		var value6 = $('#input-6').val();
		var filter = /^[\w\-\.\+]+\@[a-zA-Z0-9\.\-]+\.[a-zA-z0-9]{2,4}$/;
		if (value6=="") {
			$('#email-error').html('Required field.').fadeIn();
			$('#input-6').focus();
		error=false;
		}else if (!filter.test(value6)) {
			$('#email-error').html('Please enter valid email.').fadeIn();
			$('#input-6').focus();
		error=false;
		}else{
			$('#email-error').fadeOut();
		}
		var value7 = $('#input-7').val();
		if (value7=="") {
			$('#number-error').html('Required field.').fadeIn();
			$('#input-7').focus();
		error=false;
		}else if (isNaN(value7)) {
			$('#number-error').html('Only accept numeric value.').fadeIn();
			$('#input-7').focus();
		error=false;
		}else{
			$('#number-error').fadeOut();
		}
		var value8 = $('#input-8').val();
		if (value8=="") {
			$('#tellus-error').html('Required field.').fadeIn();
			$('#input-8').focus();
		error=false;
		}else{
			$('#tellus-error').fadeOut();
		}
		if(error){
        var postData = $(this).serializeArray();
        var formURL = $(this).attr("action");
        $.ajax(
                {
                    url: formURL,
                    type: "POST",
                    data: postData,
                    success: function(data, textStatus, jqXHR)
                    {
                        $('#final-wrapper').fadeOut();
                        $('#final-msg').text('Thank you! We will be in touch very shortly.');
                        //var messageEl = theForm.querySelector( '.final-msg' );
                        //messageEl.innerHTML = 'Thank you! We\'ll be in touch.';

                    },
                    error: function(jqXHR, textStatus, errorThrown)
                    {
                        //if fails
                    }
                });
		}
        e.preventDefault(); //STOP default action
        //e.unbind(); //unbind. to stop multiple form submit.
    });

</script>

<!------End Request------>

<script>
    (function($) {

        $.fn.visible = function()
        {

            var $element = $('.statistics-wrapper'),
                    $win = $(window),
                    elemTop = $element.position().top,
                    elemBottom = elemTop + $element.height() + 200,
                    doc = window.screen.height,
                    winTop = $win.scrollTop(),
                    winBottom = winTop + doc;
            var windowWidth = $(window).width();
            if (windowWidth < 767) {

                if (elemTop <= winTop && elemBottom >= winBottom) {
                    return true;
                }
                else {
                    return false;
                }
            } else {
                if (elemTop > winTop && elemBottom < winBottom) {
                    return true;
                } else {
                    return false;

                }
            }
        };

    })(jQuery);

    function padNum(num) {
        if (num < 10) {
            return " " + num;
        }
        return num;
    }


    var $count1 = $('.statistics-wrapper');


// Scrolling Functions
    $(window).scroll(function(event)
    {

        var first = 215; // Count up to 25x for first
        var second =61385; // Count up to 4x for second
        var third = 205; // Count up to 25x for first
        var fourth =22391; // Count up to 4x for second
        var fifth = 8927100; // Count up to 25x for first
        var sixth = 2567; // Count up to 4x for second

        function countStuffUp(points, selector, duration)
        {
            //Animate count
            var $selector = $(selector);
            $selector.addClass('counting');

            var $counter = $({
                countNumber: $selector.text()
            }).animate({
                countNumber: points
            }, {
                duration: duration,
                easing: 'linear',
                step: function(now) {
                    //console.log(now);
                    $selector.data('remaining', (points - now) * (duration / points));
                    $selector.text(padNum(parseInt(this.countNumber)));
                },
                complete: function() {
                    $selector.removeClass('counting');
                    $selector.text(points);

                }
            });

            $selector.data('counter', $counter);
        }

        // Output to div
        $(".first-count").each(function(i, el) {
            var el = $(el);
            if (el.visible() && !el.hasClass('counting')) {
                var duration = el.data('remaining') || 1600;
                console.log('duration', duration);
                countStuffUp(first, '.first-count', duration);
            } else if (!el.visible() && el.hasClass('counting')) {
                el.data('counter').stop(true);
                el.removeClass('counting');
            }
        });

        // Output to div
        $(".second-count").each(function(i, el) {
            var el = $(el);
            if (el.visible() && !el.hasClass('counting')) {
                var duration = el.data('remaining') || 1000;
                countStuffUp(second, '.second-count', duration);
            } else if (!el.visible() && el.hasClass('counting')) {
                el.data('counter').stop(true);
                el.removeClass('counting');
            }
        });

        $(".third-count").each(function(i, el) {
            var el = $(el);
            if (el.visible() && !el.hasClass('counting')) {
                var duration = el.data('remaining') || 2000;
                countStuffUp(third, '.third-count', duration);
            } else if (!el.visible() && el.hasClass('counting')) {
                el.data('counter').stop(true);
                el.removeClass('counting');
            }
        });

        $(".fourth-count").each(function(i, el) {
            var el = $(el);
            if (el.visible() && !el.hasClass('counting')) {
                var duration = el.data('remaining') || 1600;
                countStuffUp(fourth, '.fourth-count', duration);
            } else if (!el.visible() && el.hasClass('counting')) {
                el.data('counter').stop(true);
                el.removeClass('counting');
            }
        });
        $(".fifth-count").each(function(i, el) {
            var el = $(el);
            if (el.visible() && !el.hasClass('counting')) {
                var duration = el.data('remaining') || 1000;
                countStuffUp(fifth, '.fifth-count', duration);
            } else if (!el.visible() && el.hasClass('counting')) {
                el.data('counter').stop(true);
                el.removeClass('counting');
            }
        });

        $(".sixth-count").each(function(i, el) {
            var el = $(el);
            if (el.visible() && !el.hasClass('counting')) {
                var duration = el.data('remaining') || 3000;
                countStuffUp(sixth, '.sixth-count', duration);
            } else if (!el.visible() && el.hasClass('counting')) {
                el.data('counter').stop(true);
                el.removeClass('counting');
            }
        });

        var windowWidth = $(window).width();
        if (windowWidth > 991) {
            if ($(this).scrollTop() > 10) {
                $('.logo-1').addClass('logo-fade');
                $('.logo-1').removeClass('logo-in');
                $('.logo-2').addClass('logo-in');
                $('.logo-2').removeClass('logo-fade');

                $('.header').addClass('small-padding')

            } else
            {
                $('.logo-2').addClass('logo-fade');
                $('.logo-2').removeClass('logo-in');
                $('.logo-1').addClass('logo-in');
                $('.logo-1').removeClass('logo-fade');

                $('.header').removeClass('small-padding');
            }
        }


    });
    $(document).ready(function() {

        $('.logo-1').addClass('logo-in');
        $('.logo-2').addClass('logo-fade');
    })




</script>


<script>
            (function() {
                if (!String.prototype.trim) {
                    (function() {
                        // Make sure we trim BOM and NBSP
                        var rtrim = /^[\s\uFEFF\xA0]+|[\s\uFEFF\xA0]+$/g;
                        String.prototype.trim = function() {
                            return this.replace(rtrim, '');
                        };
                    })();
                }

                [].slice.call(document.querySelectorAll('input.input__field')).forEach(function(inputEl) {
                    // in case the input is already filled..
                    if (inputEl.value.trim() !== '') {
                        classie.add(inputEl.parentNode, 'input--filled');
                    }

                    // events:
                    inputEl.addEventListener('focus', onInputFocus);
                    inputEl.addEventListener('blur', onInputBlur);
                });

                function onInputFocus(ev) {
                    classie.add(ev.target.parentNode, 'input--filled');
                }

                function onInputBlur(ev) {
                    if (ev.target.value.trim() === '') {
                        classie.remove(ev.target.parentNode, 'input--filled');
                    }
                }
            })();
</script>





<script>
    var menuRight = document.getElementById('cbp-spmenu-s2'),
            showRightPush = document.getElementById('showRightPush'),
            body = document.body;


    showRightPush.onclick = function() {
        classie.toggle(this, 'active');
        classie.toggle(body, 'cbp-spmenu-push-toleft');
        classie.toggle(menuRight, 'cbp-spmenu-open');
        if ($('.SnapABug_Button.active').length > 0) {
            $('#SnapABug_Button').removeClass('active');

        } else {
            $('#SnapABug_Button').addClass('active');
        }
        //disableOther( 'showRightPush' );
    };

</script>

