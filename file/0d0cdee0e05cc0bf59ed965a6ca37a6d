<script type="text/javascript">

	jQuery(function($){
		var id1 = '#subscribe-modal';
		var id2 = '#subscribe-modal1';

		if ('/index.php/sitestroy' == window.location.pathname || '/index.php/forex' == window.location.pathname) {
			setTimeout(function(){
			
			    if('/index.php/sitestroy' == window.location.pathname) id = id1;
				else id = id2;
				var maskHeight = $(document).height();
				var maskWidth = $(window).width();
				  
				$('#mask').css({'width':maskWidth,'height':maskHeight});
				  
				$('#mask').fadeIn(1000); 
				$('#mask').fadeTo("slow",0.8); 
				  
				var winH = $(window).height();
				var winW = $(window).width();
				  
				$(id).css('top',  winH/2-$(id).height()/2);
				$(id).css('left', winW/2-$(id).width()/2);
				  
				$(id).fadeIn(2000); 
			}, 5000);
		}
		
		$('#mask').click(function () {
			$(this).hide();
			$(id).hide();
		}); 
	});

</script>    
