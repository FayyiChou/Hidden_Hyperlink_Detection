<script src="//static.youku.com/v1.0.132/js/jquery.js"></script>
<script>
    $(document).ready(function () {
        if(Terminal.platform.iPad){
            $('body').prepend('<style>.web{display: none;}.pad {display: block;}</style>');
        }
        function resize(){
            
            var winHeight = $(window).height();
            var winWidth = $(window).width();
            var portrait = winHeight > winWidth ;
            if(portrait){
                $('.top-h').css('display','none');
                $('.top-s').css('display','inline-block');
            }else{
                $('.top-h').css('display','inline-block');
                $('.top-s').css('display','none');
            }
            
            
            var height = $('.bg-box-1').height();
            $('.btn-box img').height( 136 * (height / 1465) ); // 1465 为PSD高度
            $('.btn-box img').show();
            $('.btn-box').css({bottom: 245 * (height / 1465)});
            
            
            var bodyHeight = $(document.body).height();
            if(portrait){
                $('.app-btn img').height( 110 * (bodyHeight / 1563) ); // 1465 为PSD高度
                $('.app-btn').css({bottom: 460 * (bodyHeight / 1563)});
            }else{
                $('.app-btn img').height( 150 * (bodyHeight / 1563) ); // 1465 为PSD高度
                $('.app-btn').css({bottom: 350 * (bodyHeight / 1563)});
            }
            $('.app-btn img').show();
        }
        
        $(window).resize(resize);
        resize();
    
        var WeiXin = (function() {
            var ua = navigator.userAgent.toLowerCase();
            $(".share-btn").click(function(){
                if (ua.match(/MicroMessenger/i) == "micromessenger" || ua.match(/qq/i) == "qq" || ua.match(/WeiBo/i) == "weibo") {
                    $('.share').show();
                } else {
                    if(/alipay/i.test(navigator.userAgent)){
                        location.href = 'itms-apps://itunes.apple.com/cn/app/xiao-xiao-you-kuhd/id1067741050?mt=8';
                    }else{
                        location.href = 'download.html';
                    }
                }
            });
        })();
        
        $('.btn-box .btn-android').on('click',function (argument) {
            if(fromAdv){
                window.open('//h5.pl.youku.com/www/kids_apk/Kids_Android_Phone_'+fromAdv+'.apk','_self');
            }else{
                window.open('//h5.pl.youku.com/www/kids_apk/Kids_Android_Phone_youkuzhuzhan.apk','_self');
            }
        });

        $('.btn-box img').on({
            mousedown: function(){
                var defaultImg = $(this).attr('src'),
                hoverImg = $(this).attr('data-hoverImg');
                $(this).attr('src',hoverImg);
                $(this).attr('data-hoverImg',defaultImg)
            },
            mouseup: function(){
                var defaultImg = $(this).attr('src'),
                hoverImg = $(this).attr('data-hoverImg');
                $(this).attr('src',hoverImg);
                $(this).attr('data-hoverImg',defaultImg)
            }
        })
    });
</script>
