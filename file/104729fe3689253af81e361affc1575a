    var iNow = count = 0;

    //打点公用方法
    function bhTongJi(spm) {
        baihe.bhtongji.tongji({'event':3,'spm':spm});
    }

    function toRun(){
        iNow = iNow >= (count-1) ? 0 : iNow+1;
        $('.icon li').eq(iNow).trigger('click').addClass('active').siblings('li').removeClass('active');
        jQuery('#indexPic').children().filter(':visible').fadeOut(500).parent().children().eq(iNow).fadeIn(1000);
    }

    jQuery(function() {
        var iHeight = jQuery(window).innerHeight();
        var iWidth = jQuery(window).width();
        var arrImg = new Array();
        var url = new Array();
//        var spm = ['baihe.bhtongji.tongji({\'event\':3,\'spm\':\'6.29.605.393.5187\'});setCookie(\'ordersource\', \'14080101\', 86400);','baihe.bhtongji.tongji({\'event\':3,\'spm\':\'6.29.605.393.5188\'});setCookie(\'ordersource\', \'14080102\', 86400);','',''];
        var spm = [
            'bhTongJi(\'28.1.1.3153.8338\');',
            'bhTongJi(\'28.1.1.3154.8339\');',
            'bhTongJi(\'28.1.1.3155.8340\');',
            'bhTongJi(\'28.1.1.3156.8341\');',
            'bhTongJi(\'28.1.1.3157.8342\');',
            'bhTongJi(\'28.1.1.3158.8343\');',
        ];
                arrImg[0] = 'http://public.baihe.com/hunli/2017/03/29/58db5470c2fa1.jpg';
        url[0] = 'http://m.hunli.baihe.com/outer/topic/bj?type=bsqrj';
                arrImg[1] = 'http://public.baihe.com/hunli/2017/03/13/58c6730d47d6a.jpg';
        url[1] = 'http://m1.hunli.baihe.com/outer/topic/bj?type=bjdlb';
                arrImg[2] = 'http://public.baihe.com/hunli/2017/03/14/58c7a69e69895.jpg';
        url[2] = 'http://m.hunli.baihe.com/outer/celebration/sellerDetail?sid=1306';
                jQuery(arrImg).each(function(i,elem){
            var img = new Image();
            var $li=$('<li/>');
            var $a=$('<a/>');
            if($(url)){
                $a.attr('href',$(url).eq(i)[0]);
                if($(url).eq(i)[0]==''){
                    $a.attr('href','javascript:;');
                }
            }else{
                $a.attr('href','javascript:;');
            }

            if($(spm)){
                $a.attr('onclick',$(spm).eq(i)[0]);
                if($(spm).eq(i)[0]==''){
                    $a.attr('onclick','');
                }
            }else{
                $a.attr('onclick','');
            }

            var $oImg=$('<img src="" alt="" class="topic_img"/>');
            $a.append($oImg);
            $li.append($a);
            $('#indexPic').append($li);

            img.src=$(arrImg).eq(i)[0];
            img.onload=function(){
                $oImg.attr('src',img.src);
                var picHeight = iWidth*0.5;
                $('.indexFocus,#indexPic,#indexPic ul li').height(picHeight);
            }
        });
                count = arrImg.length;//$('#indexPic li').size();

        if(count==1){
            jQuery('#indexPic li').eq(0).show();
        }else if(count>1){
            for(var i=1;i<count+1;i++){
                var newli = '<li>'+i+'</li>';
                $('#scrollerPageNum').append(newli);
            }
            jQuery('.icon li').eq(0).addClass('active');
            jQuery('#indexPic li').eq(0).show();
            jQuery('.icon li').click(function(){
                jQuery(this).addClass('active').siblings('li').removeClass('active');
                var i = jQuery(this).text()-1;
                iNow = i;
                if(i>=count) return;
                jQuery('#indexPic').children().filter(':visible').fadeOut(500).parent().children().eq(i).fadeIn(1000);
            });
            timer = setInterval('toRun()',5000);
        }

        function swipePanel(panel,icon,iNow,count){
            jQuery(panel).fadeOut(500).siblings().eq(iNow).fadeIn(1000);
            jQuery(icon).eq(iNow).addClass('active').siblings().removeClass('active');
        }

        function swipeleftHandler(){
            if(iNow >= count-1) {
                return;
            }else{
                iNow++;
            };
            swipePanel($('#indexPic').children(),$('.icon li'),iNow,count);
        }

        function swiperightHandler(){
            if(iNow <= 0) {
                return;
            }else{
                iNow--;
            };
            swipePanel($('#indexPic').children(),$('.icon li'),iNow,count);
        }

        $('#indexPic li').swipeLeft(function(e){
            e.preventDefault();
            swipeleftHandler();
        });

        $('#indexPic li').swipeRight(function(e){
            e.preventDefault();
            swiperightHandler();
        });

    });
</script>
