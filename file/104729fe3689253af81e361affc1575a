    /** 活动 */
    function sellerActivityData(obj){

        var bigTypeSpmA = new Array();
            bigTypeSpmA["photo"] = '28.1.1.3177.8362', //婚照
            bigTypeSpmA["celebration"] = '28.1.1.3175.8360', //婚庆
            bigTypeSpmA["banquet"] = '28.1.1.3176.8361', //婚宴
            bigTypeSpmA["lvpai"] = '28.1.1.3178.8363' //旅拍

        var sellerActivityHtml = '';
            sellerActivityHtml +='<li>';
            sellerActivityHtml +='	<div class="topBox">';
            sellerActivityHtml +='		<a href="javascript:;" class="gz j_gz" data-sid=' + obj['sid'] + '>' + collectFun(obj['isCollect']) + '</a><img src='+obj['logo'] + ' alt="" />';
            sellerActivityHtml +='		<a href=' + obj['storeDetailUrl'] + '?arg3g=\{%22sid%22:%22' + obj['sid'] + '%22\}' + '>' + obj['title'] + '</a>';
            sellerActivityHtml +='		<span class="labelT pink">' + obj['bigTypeName'] + '</span>';
            sellerActivityHtml +='	</div>';
            sellerActivityHtml +='	<div class="bottomBox">';
            sellerActivityHtml +='		<div class="link">';
            sellerActivityHtml +='			<a  onclick="bhTongJi(\''+ bigTypeSpmA[obj['bigType']] + '\');" href=' + obj['dataUrl'] + '?arg3g=\{%22couponNumber%22:%22' + obj['couponNumber'] + '%22\}' + ' class="pic"><img src=' + obj['picUrl'] + ' onload="imgWidth()"></a>';
            sellerActivityHtml +='			<span>商家活动</span>';
            sellerActivityHtml +='		</div>';
            sellerActivityHtml +='		<a href=' + obj['dataUrl'] + '?arg3g=\{%22couponNumber%22:%22' + obj['couponNumber'] + '%22\}' + ' class="title">' + obj['recommend']['rTitle'];
            sellerActivityHtml +='		    <span>' + obj['recommend']['rStartTime'] + ' - ' + obj['recommend']['rEndTime'] + '</span>';
            sellerActivityHtml +='		</a>';
            sellerActivityHtml +='	</div>';
            sellerActivityHtml +='</li>';
        return sellerActivityHtml;
    }

    /* 全国站（未开城市） */
    function QGlvPaiSeriesData(obj){
        var QGlvPaiSeriesHtml = '';
            QGlvPaiSeriesHtml += '<li>';
            QGlvPaiSeriesHtml += '  <div>';
            QGlvPaiSeriesHtml += '      <div class="link">';
            QGlvPaiSeriesHtml += '          <a href=' + obj['dataUrl'] + '?arg3g={"gid":"' + obj['gid'] + '"}' + ' class="pic"><img src="' + obj['picUrl'] + '" onload="imgWidth()"></a>';
            QGlvPaiSeriesHtml += '          <a href="javascript:;" class="keep '+ (parseInt(obj['collectCount']) > 1 ? 'active' : '') +'" data-sid="' + obj['sid'] +'"><em></em><strong>' + obj['collectCount'] + '</strong></a>';
            QGlvPaiSeriesHtml += '          <a class="tit">' + obj['seriesLightspot'] + '</a>';
            QGlvPaiSeriesHtml += '      </div>';
            QGlvPaiSeriesHtml += '      <span class="price">¥'+ obj['price'] + '<del>¥'+ obj['oriPrice'] + '</del></span>';
            QGlvPaiSeriesHtml += '      <a href="'+ obj['dataUrl'] + '?arg3g={"gid":"' + obj['gid'] + '"}' +'" class="title">' + obj['title'] + '<span>' + obj['sname'] + '</span></a>';
            QGlvPaiSeriesHtml += '      <div class="text">';
            QGlvPaiSeriesHtml += '          <span class="location">' + obj['placeStr'] + '</span>';
            QGlvPaiSeriesHtml += '          <span class="' + (parseInt(obj['includePlane']) == 1 ? 'stay' : 'travel') + '">' + (parseInt(obj['includePlane']) == 1 ?  '包' : '不包') + '机票</span>';
            QGlvPaiSeriesHtml += '          <span class="' + (parseInt(obj['includeHotel']) == 1 ? 'stay' : 'travel') + '">' + (parseInt(obj['includeHotel']) == 1 ? '包' : '不包') + '酒店</span>';
            QGlvPaiSeriesHtml += '      </div>';
            QGlvPaiSeriesHtml += '  </div>';
            QGlvPaiSeriesHtml += '</li>';
        return QGlvPaiSeriesHtml;
    }

    /**
     * 关注与否
     * @param isCollect
     */
    function collectFun(isCollect){
        return parseInt(isCollect) == 1 ? "已关注" : "+关注";
    }

    function imgWidth(){
        $('.big_pic').height(document.body.clientWidth / 2.142);
    }
</script>
