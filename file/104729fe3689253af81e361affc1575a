<script type="text/javascript">
    var isLogin = false;
    var page =  1;
    //下拉
    function pullUpAction(){
        page += 1;
        $.ajax({
            type: "get",
            url: '/bj/index/index?arg3g={"page":"'+page+'"}',
            success: function(data, textStatus){
                var dhtml = "";
                var list = $.parseJSON(data);
                if(list.length > 0){
                    for (var i = 0; i < list.length; i++) {
                        var obj = list[i];
                        if(obj['smallType'] == "sellerActivity"){ //商家活动
                            dhtml += sellerActivityData(obj);
                        }else if(obj['smallType'] == "seller"){ //商家
                            dhtml += sellerData(obj);
                        }else if(obj['smallType'] == "series"){ //套系
                            dhtml += seriesData(obj);
                        }else if(obj['smallType'] == "case"){ //案例
                            dhtml += caseData(obj);
                        }
                        else if(obj['smallType'] == "QGlvPaiSeries"){ //全国站旅拍套系
                            dhtml += QGlvPaiSeriesData(obj);
                        }
                    }
                    /** 渲染到页面 */
                    scrollAddHtml(dhtml);
                    if(list.length < 20){
                        addComplete();
                    }
                }else{
                    addComplete();
                }
            },
            error: function(){
                //请求出错处理
            }
        });
    }

    /** 商家 */
    function sellerData(obj){
        var bigTypeSpmS = new Array();
            bigTypeSpmS["photo"] = '28.1.1.3167.8352'; //婚照
            bigTypeSpmS["celebration"] = '28.1.1.3165.8350'; //婚庆
            bigTypeSpmS["banquet"] = '28.1.1.3166.8351'; //婚宴
            bigTypeSpmS["lvpai"] = '28.1.1.3168.8353'; //旅拍

        var sellerHtml = '';
            sellerHtml +='<li>';
            sellerHtml +='	<div class="topBox">';
            sellerHtml +='		<a href="javascript:;" class="gz j_gz" data-sid=' + obj['sid'] + '>' + collectFun(obj['isCollect']) + '</a><img src="'+obj['logo'] + '" alt="" />';
            sellerHtml +='		<a href=' + obj['storeDetailUrl'] + '?arg3g={"sid":"' + obj['sid'] + '"}' + '>' + obj['title'] + '</a>';
            sellerHtml +='		<span class="labelT yellow">' + obj['bigTypeName'] + '</span>';
            sellerHtml +='	</div>';
            sellerHtml +='	<div class="bottomBox">';
            sellerHtml +='		<div class="link">';
            sellerHtml +='			<a onclick="bhTongJi(\''+ bigTypeSpmS[obj['bigType']] + '\');" href=' + obj['storeDetailUrl'] + '?arg3g={"sid":"' + obj['sid'] + '"}' + ' class="pic"><img src="' + obj['picUrl'] + '" onload="imgWidth()"></a>';
            sellerHtml +='			<span>精选商家</span>';
            sellerHtml +='		</div>';
            sellerHtml +='	</div>';
            sellerHtml +='</li>';
        return sellerHtml;
    }

    /** 套系 */
    function seriesData(obj){
        var bigTypeSpmSe = new Array();
            bigTypeSpmSe["photo"] = '28.1.1.3170.8355'; //婚照
            bigTypeSpmSe["celebration"] = '28.1.1.3169.8354'; //婚庆
            bigTypeSpmSe["lvpai"] = '28.1.1.3171.8356'; //旅拍

        var seriesHtml = '';
            seriesHtml +='<li>';
            seriesHtml +='	<div class="topBox">';
            seriesHtml +='		<a href="javascript:;" class="gz j_gz" data-sid=' + obj['sid'] + '>' + collectFun(obj['isCollect']) + '</a><img src=' + obj['logo'] + ' alt="" />';
            seriesHtml +='		<a href=' + obj['storeDetailUrl'] + '?arg3g={"sid":"' + obj['sid'] + '"}' + '>' + obj['title'] + '</a>';
            seriesHtml +='		<span class="labelT blue">' + obj['bigTypeName'] + '</span>';
            seriesHtml +='	</div>';
            seriesHtml +='	<div class="bottomBox">';
            seriesHtml +='		<div class="link">';
            seriesHtml +='			<a onclick="bhTongJi(\''+ bigTypeSpmSe[obj['bigType']] + '\');" href=' + obj['dataUrl'] + '?arg3g={"gid":"' + obj['gid'] + '"}' + ' class="pic"><img src=' + obj['picUrl'] + ' onload="imgWidth()"></a>';
            seriesHtml +='			<span>精选套系</span>';
            seriesHtml +='		</div>';
            seriesHtml +='		<a href=' + obj['dataUrl'] + '?arg3g={"gid":"' + obj['gid'] + '"}' + ' class="title">' + obj['recommend']['rTitle'] + '<span>' + obj['recommend']['rType'] + '</span>';
                if(obj['recommend']['lvpaiPlaceStr'] != ""){
                    seriesHtml +='		    <span class="local">' + obj['recommend']['lvpaiPlaceStr'] + '</span>';
                }
            seriesHtml +='		</a>';
            seriesHtml +='		<span class="price">¥' + obj['recommend']['rPrice'] + '<del>¥' + obj['recommend']['rMarketPrice'] + '</del></span>';
            seriesHtml +='	</div>';
            seriesHtml +='</li>';
        return seriesHtml;
    }

    /** 案例 */
