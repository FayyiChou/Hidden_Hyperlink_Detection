                                        ]
                                    }
                                }
                            ]
                        };
                        myChart2.setOption(option2);
                    }
                );
            }

            $(function() {
            //布伦特   hangqing
                viewLineChartRight('JO_429','blt_1_a');
            //WTI   hangqing
                viewLineChartRight('JO_357','wti_1_a');
