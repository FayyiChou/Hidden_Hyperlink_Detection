        <script language="javascript">
          	function quote_oil(flag){
             var table = ["q67","q63","q70","q80","q1"];
             var page = $("#currentPage").val();
             var codes = "JO_429,JO_357,JO_60828,JO_92273";
              //SearchQuot 参数说明  1、查询的code 2、查询的类型 3、所要显示列  4、显示位置的ID 5、当前页码  6、每页数据数   
              QuotList.SearchQuot(codes,"",table,"market",page,10,2,"",flag);
               }
              $(function(){
                 quote_oil("true");
               });
               window.setInterval(function(){
                 quote_oil("false");
               },60000);
		 </script>
        <script type="text/javascript">
            $('#tzData').change(function(){
                var len = $('#tzData option').length;
                for (i=0;i<len;i++) {
                    if ($('#tzData option')[i].selected == true) {
                        $('#oilzs'+(i+1)).show();
                    }else{
                        $('#oilzs'+(i+1)).hide();
                    }
                }
            });
        </script>
