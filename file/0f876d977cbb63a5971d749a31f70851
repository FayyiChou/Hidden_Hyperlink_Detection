        <script type="text/javascript">
            var QUOT_API_URL = "http://api.jijinhao.com";
            function convertDate(data){
                if(data<10){
                    return "0"+data;
                }
                    return data;
            }
            var date = new Date();
            var dateStr = date.getFullYear()+"-"+convertDate(date.getMonth())+"-"+convertDate(date.getDate());
            var oilTtitle ;
            function viewLineChartRight(code,chartId){
                var oilDataq2 = new Array();
                var timeStr = new Array();
                var oilTUrl = QUOT_API_URL+"/history/quotejs.htm?codes="+code+"&currentPage=1&pageSize=5000&begin=" + dateStr + "&style=0";
                $.getScript(oilTUrl,function(){
                    var data = quot_str[0].data;
                    if(data != null ){
                        for(var i=0; i<data.length; i++)
                        {
                            var time =  data[i].quote.q59;
                            if(i==0){
                                oilTtitle = "分时线  "+time+"\n最新价："+data[i].quote.q63;
                            }
                            if(time!=""){
                                timeStr[i]=parseFloat(time.substring(11,16).replace(":","."));
                            }
                            oilDataq2[i]= parseFloat(data[i].quote.q63);
                        }
                        renderLineChartRight(timeStr,oilDataq2,chartId,oilTtitle);
                    }
                });
            }
            function renderLineChartRight(date, data, chartId,title){
                require.config({
                    paths: {
                        echarts: 'https://res.cngoldres.com/energy/js/'
                    }
                });
                require([
                        'echarts',
                        'echarts/chart/line'
                    ],
                    function (ec) {
                        var myChart2 = ec.init(document.getElementById(chartId));
                        var  option2 = {
                                tooltip : {
                                    trigger: 'axis'
                                },
                                grid: {
                                    x:56,
                                    y:54,
                                    x2:56,
                                    width:180,
                                    height:110,
                                    borderWidth:1,
                                    borderColor:'#9b9b9b'
                                },
                                title : {
                                    text: title,
                                    x:20,
                                    y:14,
                                    textStyle: {
                                        color: '#000',
                                        fontSize: 12,
                                        fontWeight: 'normal',
                                        fontFamily: 'arial'
                                    }
                                },
                                toolbox: {show : false},
                                xAxis : [
                                    {
                                        scale:true,
                                        type : 'category',
                                        axisLine:{show:false},
                                        axisTick: {show:false},
                                        splitLine: {
                                            show:true,
                                            lineStyle:{
                                                color: '#d3d3d3',
                                                width: 1,
                                                type: 'dashed'
                                            }
                                        },
                                        data : date
                                    }
                                ],
                                yAxis : [
                                    {
                                      scale:true,
                                      type : 'value',
                                      axisLine:{show:false},
                                      axisTick:{show:false},
                                      splitLine: {
                                        show:true,
                                        lineStyle:{
                                            color: '#d3d3d3',
                                            width: 1,
                                            type: 'dashed'
                                        }
                                    },
                                    axisLabel:{
                                        margin:6,
                                        textStyle:{
                                            color:'#00005f',
                                            fontSize:7
                                        }
                                    }
                                }
                            ],
                            series : [
                                {
                                    name:'收盘',
                                    type:'line',
                                    symbol:'none',
                                    itemStyle: {
                                        normal: {
                                            lineStyle: {
                                                width: 1,
                                                color: '#1084cc',
