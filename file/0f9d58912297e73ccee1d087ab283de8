                    url: "/u/util/getbdthreadlist",
                    data: {bids:bid,stick:0,count:30},
                    dataType: "json",
                    success: function(res){
                        var items = res.data[bid];
                        $.each(items,function(i){
                            var title = this.title,
                                e = "]",
                                obj = title.match(e),
                                headTitle = "",
                                titleCont = title,
                                stick = this.stick,
                                now = new Date(),
                                date = now.getFullYear()+""+(now.getMonth()+1)+""+now.getDate();
                            if(obj){
                                headTitle = "["+title.substring(1,obj.index)+"]";
                                titleCont = title.substring(obj.index+1,title.length);
                            }
                               //取置顶帖
                            if(stick != 0){
                                stickData.push('<li><a href="'+this.url+'" ttname="bbs_bk_wnyj_'+date+'_'+this.refTid+'" target="_blank"><span>'+headTitle+'</span>'+titleCont+'</a></li>');
                            }else{
                                allData.push('<li><a href="'+this.url+'" ttname="bbs_bk_wnyj_'+date+'_'+this.refTid+'" target="_blank"><span>'+headTitle+'</span>'+titleCont+'</a></li>');
                            }
                        })
                        var data = getArrayItems(allData,(8-stickData.length));
                        var html =stickData.join("") + data.join("");
                        $(".wnList").append(html).attr("ajaxstat", true);
                    }
                });
            }
            $(".wanneng-menu").css('visibility','hidden');
            $(".wanneng-list").show();
        }
        ).mouseleave(function() {
            $(".wanneng-menu").css('visibility','visible');
            $(".wanneng-list").hide();
        })

        //随机取值
        function getArrayItems(arr, num) {
            var temp_array = new Array();
            for (var index in arr) {
                temp_array.push(arr[index]);
            }
            var return_array = new Array();
            for (var i = 0; i<num; i++) {
                if (temp_array.length>0) {
                    var arrIndex = Math.floor(Math.random()*temp_array.length);
                    return_array[i] = temp_array[arrIndex];
                    temp_array.splice(arrIndex, 1);
                } else {
                    break;
                }
            }
            return return_array;
        }

    })
</script>
