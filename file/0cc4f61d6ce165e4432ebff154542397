	      	<td valign="top"  class="right-box"> 
			  		<script language="javascript">
	function chk_submit() {
		if(login_form.uid.value == "") {
			alert("請輸入帳號！");
			login_form.uid.focus();
			return;
		}
		
		if(login_form.passwd.value == "") {
			alert("請輸入密碼！");
			login_form.passwd.focus();
			return;
		}
		
		login_form.submit();
	}
	
	function KeyDown(event) {
    if(event.keyCode == 13) {
      event.returnValue=false;
      event.cancel = true;
      chk_submit();
    }
	}
</script>
