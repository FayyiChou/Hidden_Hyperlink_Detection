<script language="javascript">
function KeyDownSearch(event,myType) {
   if(event.keyCode == 13) {
      event.returnValue=false;
      event.cancel = true;
      simpleSearch(myType);
   }
}
function keywordSearch(k){
	document.query_form.keyword.value= k;
	document.query_form.action = "/main/doKeywordSearch.php?hotkey=yes";
	document.query_form.submit();
}
function simpleSearch(type){	
	if(type == 'maga'){
		if(top_form.query2.value == '') {
			alert("請輸入搜尋字串！");		
			return;
		}
		document.query_form.keyword.value= document.top_form.query2.value;
		document.query_form.action = "/main/doMagaKeywordSearch.php";
	}else if(type == 'article'){
		if(top_form.query1.value == '') {
			alert("請輸入搜尋字串！");		
			return;
		}
		document.query_form.keyword.value= document.top_form.query1.value;
		document.query_form.action = "/main/doKeywordSearch.php";
	}else{
		if(top_form.query3.value == '') {
			alert("請輸入搜尋字串！");		
			return;
		}	
		document.query_form.keyword.value= document.top_form.query3.value;
		document.query_form.action = "/main/doFigureSearch.php";		
	}	
	
	document.query_form.submit();
}

function chg_search_tab(tab) {
	if(tab == 'article') {
		document.query_form.action = "/main/doKeywordSearch.php";
	}
	else if(tab == 'maga') {
		document.query_form.action = "/main/doMagaKeywordSearch.php";
	}
	else if(tab == 'pic') {
	}
}
</script>

