<script language="javascript">
	function news_smb(act) {
		if(newspaper_form.email.value == '') {
			alert("請輸入EMAIL!");
			newspaper_form.email.focus();
			return;
		}
		
		if(!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(newspaper_form.email.value))){
      alert("請輸入正確的EMAIL!");
      newspaper_form.email.focus();
      return;
    }
		
		newspaper_form.action.value = act;
		newspaper_form.submit();
	}
</script>					        
