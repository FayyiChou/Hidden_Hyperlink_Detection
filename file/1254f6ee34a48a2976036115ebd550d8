<script>

$(document).ready(function(){
	var _idx = 0;
	var _len = $('#banner div').length;
	var timer = setInterval(function(){
		
		$('#banner div').eq(_idx).fadeTo('normal', 0, function(){
			_idx ++;
			if(_idx == _len) {
				_idx = 0;
			}
			$('#banner div').eq(_idx).fadeTo('normal', 1);
		}); 
		
	}, 5000);
})

</script>

