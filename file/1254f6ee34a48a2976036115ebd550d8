<script>
$(function(){
	$("#content2").hide();
	$("ul.gallery li").find(".title").fadeTo('normal', 0.7);
	$("ul.gallery li").hover(function() {
		$(this).find(".mask").stop().fadeTo('normal', 0.7); 
		$(this).find(".move").show();
		$(this).find(".title").css("background-color","#f39800")
		$(this).find(".title").fadeTo('normal', 1);;
	} , function() {
		$(this).find(".mask").stop().fadeTo('normal', 0); 
		$(this).find(".move").hide();
		$(this).find(".title").css("background-color","#0193c4");
		$("ul.gallery li").find(".title").fadeTo('normal', 0.7);
	});
});
function left(){
	if( $('.left span').attr("name") == "yes" ){
		$("#content2").hide();
		$("#content1").show();		
		$('.right span').attr("name","yes")
		$('.left span').attr("name","no")		
	}else{
		$("#content2").show();
		$("#content1").hide();		
		$('.right span').attr("name","no")
		$('.left span').attr("name","yes")			
	}
}
function right(){
	if( $('.right span').attr("name") == "yes" ){
		$("#content1").hide();
		$("#content2").show();
		$('.left span').attr("name","yes")
		$('.right span').attr("name","no")
	}else{
		$("#content1").show();
		$("#content2").hide();
		$('.left span').attr("name","no")
		$('.right span').attr("name","yes")		
	}
}
</script>   
