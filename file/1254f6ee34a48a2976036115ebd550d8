<script>
	function doInquirySubmit(){
		if( $("input[name=subject]").val() == "" ){
			alert("请输入您的留言主题");
			$("input[name=subject]").focus();
			return false;
		}
		if( $("input[name=name]").val() == "" ){
			alert("请输入您的姓名");
			$("input[name=name]").focus();
			return false;
		}
		if( $("input[name=email]").val() == "" ){
			alert("请输入您的邮箱");
			$("input[name=email]").focus();
			return false;
		}
		if( $("input[name=tel]").val() == "" ){
			alert("请输入您的留言电话");
			$("input[name=tel]").focus();
			return false;
		}
		if( $("textarea[name=content]").val() == "" ){
			alert("请输入您的留言内容");
			$("textarea[name=content]").focus();
			return false;
		}
		if( $("input[name=confirmText]").val() == "" ){
			alert("请输入您的验证码");
			$("input[name=confirmText]").focus();
			return false;
		}        
        var temp = $("input[name=email]").val();
        var myreg = /(\S)+[@]{1}(\S)+[.]{1}(\w)+/;
		if(!myreg.test(temp)){
			alert("请输入您的正确的邮箱");
			return false;
		}
		alert("留言成功!!");
		$("input[name=subject]").val("");
		$("input[name=name]").val("");
		$("input[name=email]").val("");
		$("input[name=tel]").val("");
		$("textarea[name=content]").val("");
		$("input[name=confirmText]").val("");
	}
</script>			
			
