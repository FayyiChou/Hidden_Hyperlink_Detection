	function clear_gfrm()
	{
		clearFormHiddenParams_gfrm('gfrm');
	}
	
	function clearFormHiddenParams_gfrm(currFormName)
	{
		var f = document.forms['gfrm'];
		f.elements['gfrm:_link_hidden_'].value='';
		f.elements['localeID'].value='';
		f.elements['gfrm:_idcl'].value='';
		f.target='';
	}
	
