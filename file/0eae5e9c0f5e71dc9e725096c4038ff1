function test_iever(){ //检测是否是IE6
	if (window.navigator.appName!='Microsoft Internet Explorer'){
		alert("由于您用的不是Microsoft Internet Explorer浏览器，所以不能顺利报名，请更换。")
	}
}
test_iever()
</script>
