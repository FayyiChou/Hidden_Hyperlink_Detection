<script>
ecode=new Array('25','49')
ebmurl=new Array('','')
bmurl=''
function sf(n){
	if (n==1){
		if (document.form1.sf1.value=='0'){
			alert('请选择报考省份')
			return
		}
		else{
			sfcode=document.form1.sf1.value;
			sfname=document.form1.sf1.options[document.form1.sf1.selectedIndex].text;
			if (confirm('您选择的是“'+sfname+'”，确定吗？')){
				for (i=0;i<2;i++){
					if (ecode[i]==sfcode){
						bmurl=ebmurl[i]
						break
					}
				}
				document.form1.action=bmurl+"/ksbm/lc.jsp"
				document.form1.xsf.value='1'
			}
			else{
				return
			}
		}
	}
	//alert(document.form1.action)
	document.form1.submit();
}
function showsfmsg(){
	var msg_35_45="昆明、玉溪、楚雄考生请选择“云南（后审）”并自行网上缴费，其他州、市考生请选择“云南（前审）”并到报名点确认、缴费。"
	sfcode=document.form1.sf1.value;
	if (sfcode=='35' || sfcode=='45'){
		//alert(msg_35_45)
	}
}
</script>
