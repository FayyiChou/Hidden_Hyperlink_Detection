    function syn(form) {
        var query = document.userForm.query.value;


        if (query == "" || query == "请输入关键字") {
            alert("请输入关键字！");
            return false;
        }
        else {

            if (form.radiobutton[0].checked) {
                form.action = '/cat/cat69/index.html';
                form.siteid.value = document.userForm.siteid.value;
                form.q.value = form.query.value;
                form.method = "get";
                document.charset = "UTF-8";



            }
            else if (form.radiobutton[1].checked) {


                var href = document.getElementById("link");

                var url = 'http://60.190.2.12/jsearch/index.jsp?';
                param = 'pagemode=result&appid=2&style=2&pos=title%2Ccontent&pg=10&ck=0&tmp_od=0&radiobutton=1&Submit.x=19&Submit.y=12&q=';

                url += param + encodeURI(form.query.value);

                href.setAttribute("href", url);
                href.setAttribute("target", "_blank");
                href.click();

                return false;
            }
            else if (form.radiobutton[2].checked) {
                //document.charset = 'GBK';
                //form.wd.value = form.query.value;
                //form.method = "get";
                //form.action = 'http://www.baidu.com/s'
                var href = document.getElementById("link");
                var url = 'http://www.baidu.com/s?';
                param = 'wd=';
                url += param + encodeURI(form.query.value);
                href.setAttribute("href", url);
                href.setAttribute("target", "_blank");
                href.click();
                return false;

            }
        }

    }
