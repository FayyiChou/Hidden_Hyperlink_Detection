<script>
function googleAnalytices(path , referer , param , async){
    var url = _rootDomain+"/ga/url/?path="+path+"&referer=" + (typeof(referer) == 'undefined' ? "" : referer) + (typeof(param) == 'undefined' ? "" : param)+"&callback=?";
    var tempasync = true;
    if(async == false)tempasync = async ;
    $.ajax({type:"JSONP", url: url , async : tempasync , success:function(data){
            var tempurl = _rootDomain+data.url ;
		        var image = document.createElement("img") ;
				image.src = tempurl ;
				image.className = "googleanalyze" ;
				document.body.appendChild(image) ;
    },timeout : 30000, error:function(msg){
    }});
}
</script>

