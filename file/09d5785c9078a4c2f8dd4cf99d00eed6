<script>
	
	$("#warn_input_mobile").bind("keyup", function(){
		$(this).val($(this).val().replace(/[^0-9]/g,''));
	}).bind("blur",function(){
		$(this).val(passport.QtoB(this.value));
		if (!passport.validMobileFormat(this.value)) {
			$("#errortip").show();
			$("#errortip").html("您输入的手机号不正确！");
		}
	}).bind("click",function(){
		$("#errortip").hide();
	});
	
	function dofeature(){
		//验证手机号
		phone = $("#warn_input_mobile").val();
		if (!passport.validMobileFormat(phone)) {
			$("#errortip").show();
			$("#errortip").html("您输入的手机号不正确！");
			return false;
		}
		code = $("#warn_input_yzm").val();
		//提交数据
		url = "https://passport.58.com/feature/dologin?warn_input_mobile=" + phone + "&warn_input_yzm=" + code + "&callback=?";
		$.getJSON(url, function(result) {
			if(result.code == '0'){
				pwdLogin();			}else{
				$("#errortip").show();
				$("#errortip").html(result.msg);
			}
		});
	}
	
	function showvalidcode(){
		$("#validatecodediv").show();
		$("#validatecodeimg").attr("src", "https://passport.58.com/validatecode?temp=123" + (new Date().getTime().toString(36)));
	}
	
	function hidevalidecode(){
		$("#validatecodediv").hide();
	}
	
	function succsendcode(result){
		if(result.code == '0'){
			$("#warn_input_yzm").show();
		}else{
			$("#errortip").show();
			$("#errortip").html(result.msg);
		}
	}
	
	$("#btn_sendyzm").bind("click", function(){
		//验证手机号
		phone = $("#warn_input_mobile").val();
		if (!passport.validMobileFormat(phone)) {
			$("#errortip").show();
			$("#errortip").html("您输入的手机号不正确！");
			return false;
		}
		//调用接口，成功后，显示手机确认码的框
		//提交数据
		url = "https://passport.58.com/feature/code?warn_input_mobile=" + phone + "&callback=?";
		$.getJSON(url, function(result) {
			succsendcode(result);
		});
	});
	$("#btn_commit").bind("click", dofeature);
</script>
