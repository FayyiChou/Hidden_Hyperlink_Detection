    <script type="text/javascript">

        try {
            var effect = ('').split(',');
            for (var i = 0; i < effect.length; i++) {
                getEffect(effect[i]);
            }
        }
        catch (e) { }

        function getEffect(ef) {
            if (ef.indexOf("bg") > -1) {
                $(".signeffect-id").addClass("sign-bg");
            }
            else if (ef.indexOf("head") > -1) {
                $(".signeffect-head").addClass("sign-ico").attr("title", "签到精英");
            }
        }
        $(document).ready(function () {
            try {
                Tx_InitTopPostDigBar("contentsbelowbar", ThreadID, Subject, '0');
            }
            catch (e)
            { }
            TxBBS_AdminAndUserPanel();
            TxBBS_PostShowAdminButtonPanel();
        });
    </script>
            
