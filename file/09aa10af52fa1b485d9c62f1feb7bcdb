<script type="text/javascript" language="javascript"> 

//搜索
function searchForm(){
	
	if(document.getElementById('contentKey').value=='请输入关键字'){
		alert('请输入关键字');
		return false;
	}
	
	if(document.getElementById('newsType').value=='5'){
		
		document.all("seachformjs").setAttribute("action","http://shop.snupg.com/resource/resource-center!indexSearch.action");
	}
	return true
}

$(function(){
 $(".list>li:has(div)").hover(function(){
   $(this).children('a').addClass('red').end().find('div').slideDown("fast");},
  function(){
   $(this).children('a').removeClass('red').end().find('div').css("display","none");;
  });
 });
</script>

