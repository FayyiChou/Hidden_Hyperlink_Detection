<script type="text/javascript">
//jQuery(".slideBox").slide( { mainCell:".bd ul",effect:"left",autoPlay:true} );
var theSlideBoxTimmer =null;
var theSlideBoxTime =3000;
var theSlideBoxRolling=false;
$(function(){
	$(".slideBox .hd li").mouseover(function(){
		var i =$(".slideBox .hd li").index($(this)[0]);
		var j = $(".slideBox .bd li").index($(".slideBox .bd li:visible")[0]);
		if(i==j)return;
		if(theSlideBoxRolling)return;
		theSlideBoxRolling =true;
		window.clearInterval(theSlideBoxTimmer);
		$(".slideBox .bd li").fadeOut(300);
		$(".slideBox .bd li").eq(i).fadeIn(300,function(){
			window.clearInterval(theSlideBoxTimmer);
			$(".slideBox .hd li").removeClass("on")
			$(".slideBox .hd li").eq(i).addClass("on")
			theSlideBoxRolling =false;
			theSlideBoxTimmer=setInterval("showTheSlideBoxNext()",theSlideBoxTime);
		});
	});
	theSlideBoxTimmer=setInterval("showTheSlideBoxNext()",theSlideBoxTime);
})
function showTheSlideBoxNext(){
	if(theSlideBoxRolling)return;
	theSlideBoxRolling =true;
	window.clearInterval(theSlideBoxTimmer);
	var i = $(".slideBox .bd li").index($(".slideBox .bd li:visible")[0]);
	i=parseInt((i+1)%$(".slideBox .bd li").length);
	$(".slideBox .bd li").fadeOut(300);
	$(".slideBox .bd li").eq(i).fadeIn(300,function(){
		window.clearInterval(theSlideBoxTimmer);
		$(".slideBox .hd li").removeClass("on")
		$(".slideBox .hd li").eq(i).addClass("on")
		theSlideBoxRolling =false;
		theSlideBoxTimmer=setInterval("showTheSlideBoxNext()",theSlideBoxTime);
	});
}
</script>
</div>
