<script>
    $(function () {
        //如果已经登录，则隐藏注册登录按钮
        var isshow = 'True';
        if (isshow == "False") {
            $("#loginInfo").hide();
        }
    });
</script>
<script type="text/javascript">
    /* 扫码登陆相关JS dick 2015.08.24 */
    $(".tabbtn1").click(function () {
        $("#imgLoginQRCode").attr("src", "");
        $(".ok-mask").hide();
        SetScanQRCodeImg();
    });

    /*根据值判断是否先显示二维码登录  mocha 2015年9月29日*/
    function lick() {
        var isSub = $("#hid_issub").val();
        if (isSub != "") {
            return;
        }
        var msg = getCookies();
        if (msg > 0) {
            $(".tabbtn1").click();
        }
    }
    //加载后调用lick（）
    $(document).ready(function () {
        lick();
    });
    //获取前台
    function getCookies() {
        var cookieName = "LyqLoginType";
        var cookieValue = null;//返回cookie的value值 
        if (document.cookie && document.cookie != '') {
            var cookies = document.cookie.split(';');//将获得的所有cookie切割成数组 
            for (var i = 0; i < cookies.length; i++) {
                var cookie = cookies[i];//得到某下标的cookies数组 
                if (cookie.substring(0, cookieName.length + 2).trim() == cookieName.trim() + "=") {//如果存在该cookie的话就将cookie的值拿出来 
                    cookieValue = cookie.substring(cookieName.length + 2, cookie.length);
                    break
                }
            }
        }
        if (cookieValue == "ScanCode") {//如果存在指定的cookie值 
            return 1;
        } else {
            return 0;
        }
    }
    $(".J-help").hover(
		function () {
		    $(".login-app .state").hide().eq(2).show();
		},
		function () {
		    $(".login-app .state").hide().eq(0).show();
		}
	);
    $(".J-down").hover(
		function () {
		    $(".login-app .state").hide().eq(1).show();
		},
		function () {
		    $(".login-app .state").hide().eq(0).show();
		}
	);
    //生成二维码图片
    var apiDomain = 'http://jsonp.lvyouquan.cn/';
    var recordId = "";
    var objInterval = null;//定时器
    function SetScanQRCodeImg() {
        if (objInterval != null) {
            clearInterval(objInterval); //清除定时器
        }
        clearInterval(objInterval);
        $.ajax({
            type: 'POST',
            url: apiDomain + 'app/GetScanCodeForLoginLYQ',
            data: { "LoginType": 1 },
            cache: false,
            dataType: "jsonp",
            success: function (data) {
                if (data != null && data.Result == true) {
                    $("#imgLoginQRCode").attr("src", data.QRCodeImg);
                    recordId = data.RecordId;
                    objInterval = setInterval(CheckScanLoginStatus, 3000);//5秒扫描一次
                }
            }
        });
    }
    //定时扫描的方法
    function CheckScanLoginStatus() {
        $.ajax({
            type: 'POST',
            url: apiDomain + 'app/GetAppScanCodeStatusLoginLYQ',
            data: { "RecordId": recordId },
            cache: false,
            dataType: "jsonp",
            success: function (data) {
                if (data != null && data.Result == true) {
                    var status = data.Status + "";
                    if (status == "2") {
                        //已扫描
                        $(".ok-mask").show();//扫描成功执行事件
                        $(".ok-mask").css("background-image", "url(http://r.lvyouquan.cn/static/lyq/images/loginapp3.png)");
                    } else if (status == "3") {
                        //已登陆成功
                        if (objInterval != null) {
                            clearInterval(objInterval); //清除定时器
                        }
                        //快捷登陆
                        $(".ok-mask").css("background-image", "url(http://r.lvyouquan.cn/static/lyq/images/loginapp4.png)");
                        ScanCodeQuickLogin();
                    } else if (status == "3") {
                        //已取消登陆
                        if (objInterval != null) {
                            clearInterval(objInterval); //清除定时器
                        }
                    }
                } else {
                    if (objInterval != null) {
                        clearInterval(objInterval); //清除定时器
                    }
                }
            }
        });
    }
    //APP扫码快捷登录
    function ScanCodeQuickLogin() {
        //alert("RecordId=" + recordId + "&CallBackUrl=" + escape($("#BusinessesLogin1_hid_callbackurl").val()) + "##" + $("#BusinessesLogin1_hid_callbackurl").val());
        $.ajax({
            type: 'POST',
            url: "/Common/ScanCodeQuickLogin",
            data: "RecordId=" + recordId + "&CallBackUrl=" + escape($("#BusinessesLogin1_hid_callbackurl").val()),
            cache: false,
            success: function (data) {
                if ((data + "").split("@@")[0] == "1") {
                    //跳转到登陆跳转页
                    window.location.href = unescape((data + "").split("@@")[1]);
                }
            }
        });
    }
</script>
<script type="text/javascript">

    $(function () {
        $("#_Forget").bind("mouseover", function () {
            GetName();
        });
        var msg = $.trim($("#hidMsg").val());
        if (msg != "") {
            showMsg();
            $(".J_LoginTip").html(msg);
        }
    });
    function GetName() {
        $("#_Forget").attr("href", "/Home/Forget?email=" + $("#txtUserName").val());
        $("#_Forget").attr("href", "/Home/ForgetByPhone");
    }

    function showMsg() {
        $(".J_LoginTip").addClass("curr").show();
    }
</script>
