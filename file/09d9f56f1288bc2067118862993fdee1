<script>
	function checkForm(obj){
		if(obj.q.value == ''){obj.q.focus();return false;}
		return true;
	}
	function audioPlay(obj){
		var daudio = document.getElementById("daudio");
		daudio.src = "http://audio.dict.cn/output.php?id="+obj.getAttribute("audio");
		daudio.play();
	}
	</script>
<audio style="position:absolute;top:-1000%;width:1px;height:1px;-khtml-opacity:0;-moz-opacity:0;opacity:0;filter:progid:DXImageTransform.Microsoft.Alpha(opacity=0);" id="daudio" controls="controls"></audio>

	
