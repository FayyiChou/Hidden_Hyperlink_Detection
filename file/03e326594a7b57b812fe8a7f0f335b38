<script type="text/javascript">
$(function(){



    $('form[name="search_form"]').submit(function(){
        var fkw = $('input[name="keywords"]');
        var url = $(this).attr("action");
        if(fkw.val() != ''){
            window.location.href = url+encodeURIComponent(fkw.val());
        }else{
            fkw.focus();
        }
        return false;
    });


    $('#weixinBox').hover(function(){
        var wtop = $(this).offset().top+32;
        var wleft = $(this).offset().left-40;
        $('#weixinPopup').css({'position':'absolute','display':'block','top':wtop,'left':wleft,'z-index':'9999'});
    }).mouseleave(function(){
        $('#weixinPopup').hide();
    })

    $('#weixinPopup').hover(function(){
        $(this).show();
    }).mouseleave(function(){
        $(this).hide();
    })

});
</script>








