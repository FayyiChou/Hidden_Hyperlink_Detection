    <script type="text/javascript">

         $(function(){

             $('.theme_head').find('li:eq(0)').addClass('on');
             $('.theme_box_0').show();
             var theme_a = $('.theme_head').find('li');
             theme_a.on('mouseover',function(){
                 var new_data = $(this).attr('data');
                 $(this).addClass('on').siblings('li').removeClass('on');
                 var data = $(this).attr('data-k');
                 var elemet = 'theme_box_'+data ;
                 $(this).closest('.theme_content').find('.theme_box_em').css({'display':'none'});
                 $(this).closest('.theme_content').find('.'+elemet).css({'display':'block'});
             })
         });

        $(function(){

            //头部幻灯片切换
            $('#slides').slideBox({
                duration : 0.3,//滚动持续时间，单位：秒
                easing : 'linear',//swing,linear//滚动特效
                delay : 5,//滚动延迟时间，单位：秒
                hideClickBar : false,//不自动隐藏点选按键
                clickBarRadius : 10
            });

            $('.listNav').mouseenter(function(){
                $(this).siblings().find('.selected').removeClass('selected').hide();
                $(this).find('.zongdao').show().addClass('selected');
                $(this).closest('.chanfen_con').find('.listNav').removeClass('cat_selected');
                $(this).addClass('cat_selected');
            });
            $('.chanfen_con').mouseleave(function(){
                $('.listNav').find('.zongdao').hide();
            });
        });
</script>
