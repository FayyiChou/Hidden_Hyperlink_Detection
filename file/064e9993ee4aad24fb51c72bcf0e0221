<script type="text/javascript">
$(document).ready(function(){
    //文本框默认值
    $(":input.moren").each(function(){
        $(this).css("color","#999");
        $(this).focus(function(){
            if($(this).val() ==this.defaultValue){   
                $(this).val("").css("color","#000");            
            }  
        }).blur(function(){
            if ($(this).val() == '') {
                $(this).val(this.defaultValue).css("color","#999");
            }
        });
    });
    

});
</script>
