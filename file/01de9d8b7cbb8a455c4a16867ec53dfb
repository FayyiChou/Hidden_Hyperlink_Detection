			function check_currchecked() {
				var all_checkbox = document.getElementById('duostamp_mods').getElementsByTagName("input");
				for(var x=0,len=all_checkbox.length;x<len;x++) {
					if(all_checkbox[x].type == 'checkbox') {
						all_checkbox[x].checked = false;
					}
				}
				
				if(global_selected.length > 0){					
					for(var i=0,len=global_selected.length;i<len;i++) {
						if($('rss_'+global_selected[i])) {
							$('rss_'+global_selected[i]).checked = true;
						}
					}
				}
				all_checkbox = x = len = i = null;
			}
			
			function closeHideWindow(mod) {
				if($('duostamp_' + mod)) {
					$('append_parent').removeChild($('duostamp_' + mod));
					//$('duostamp_' + mod).style.display = "none";
				}
			}
			
			function checksubmit() {
				var fid = 338;
				var tid = 1521379;
				var dzversion = 'X3.2';
				var reason = document.getElementById('reason').value,url = 'plugin.php?id=duostamp:duostamp';
				if(dzversion == 'X2.5') {
					var x = new Ajax('HTML');
				} else {
					var x = new Ajax('JSON');
				}
				var vk=[],num=0,all_checkbox = document.getElementById('duostamp_mods').getElementsByTagName("input");
				for(var i=0,len=all_checkbox.length;i<len;i++) {
					if(all_checkbox[i].type == 'checkbox' && all_checkbox[i].checked) {
						vk.push(all_checkbox[i].value);
						num += 1;
					}
				}
				
				if(num > 4) {
					window.alert('您最多只能选择4个图章!');
					return false;
				}
				
				if($(threadstamp));
				else {
					var pid = 12275120,threadstamp = document.createElement('div');
					threadstamp.id = 'threadstamp';
					if($('post_'+pid) && $('pid'+pid)) {
						$('post_'+pid).insertBefore(threadstamp,$('pid'+pid));
					}
				}
				
				var wid=[0,335,500,665],stampurls = new Array();
				stampurls[0] = '001.gif';stampurls[1] = '002.gif';stampurls[4] = '005.gif';stampurls[5] = '006.gif';stampurls[6] = '007.gif';stampurls[7] = '008.gif';stampurls[8] = '009.gif';stampurls[9] = '010.gif';
				x.post(url,'inajax=1&fid='+fid+'&tid='+tid+'&reason='+reason+'&vk='+vk,function(s) {
					if(dzversion == 'X2.5') {
						//X2.5
						s = eval('(' + s + ')');
					}
					global_selected = [];
					if(s && s[0] !== "") {
						var k,stamphtml='',j=0;
						for(k in s) {
							if(j == 0) {
								stamphtml += '<img src="static/image/stamp/'+stampurls[s[k]]+'">';
							} else {
								stamphtml += '<img src="static/image/stamp/'+stampurls[s[k]]+'" style="right:'+wid[j]+'px;">';
							}
							global_selected.push(s[k]);
							j += 1;
						}
						$('threadstamp').innerHTML = stamphtml;
					} else {
						$('threadstamp').innerHTML = "";
					}
					closeHideWindow('mods');
				});
			}
