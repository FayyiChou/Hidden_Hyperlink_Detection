			(function(){
				var stamphtml='',j=0,wid=[0,335,500,665],hastamp = {};
				
				for(var i in hastamp) {
					if(j == 0) {
						stamphtml += '<img src="static/image/stamp/'+hastamp[i]+'">';
					} else {
						stamphtml += '<img src="static/image/stamp/'+hastamp[i]+'" style="right:'+wid[j]+'px;">';
					}
					j += 1;
				}
				if(j > 0) {
					if($(threadstamp));
					else {
						var pid = 12275120,threadstamp = document.createElement('div');
						threadstamp.id = 'threadstamp';
						if($('post_'+pid) && $('pid'+pid)) {
							$('post_'+pid).insertBefore(threadstamp,$('pid'+pid));
						}
					}
					$('threadstamp').innerHTML = stamphtml;
				}
			})();
			
			var global_selected = [];
			
			function createDiv() {
				var divobj,oFragment;
				if($('duostamp_mods')) {
					$('duostamp_mods').style.display = "block";
					return false;
				}
								
				oFragment = document.createDocumentFragment();
				divobj = document.createElement('div');
				divobj.id = "duostamp_mods";
				divobj.className = "fwinmask";
				divobj.style.position = 'fixed';
				divobj.style.left = '520px';
				divobj.style.top = '149px';
				divobj.style.zIndex = 201;
				divobj.style.display = 'block';
				divobj.setAttribute('initialized','true');
				var divhtml = ['<style type="text/css">object{visibility:hidden;} .labelspan {float:left;text-align:left;white-space:nowrap;margin:3px;}</style>',
