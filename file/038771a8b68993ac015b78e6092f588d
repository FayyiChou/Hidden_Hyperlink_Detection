        $(function () {
            $('tr.trshow').mouseenter(function () {
                $(this).children().css('background', '#F8F8F8');
                $(this).next('tr.trhide').show();
            }).mouseleave(function () {
                $(this).children().css('background', '#fff');
                $(this).next('tr.trhide').hide();
            });

            var wait = 60;

            function time(o) {
                if (wait == 0) {
                    if ($("#phone").next(".sem_gou").css("display") != "none") {
                        o.removeAttribute("disabled");
                    } else {
                        o.setAttribute("disabled", true);
                    }

                    o.value = "获取验证码";
                    wait = 60;
                } else {
                    o.setAttribute("disabled", true);
                    o.value = "(" + wait + ")";
                    wait--;
                    setTimeout(function () {
                        time(o);
                    }, 1000);
                }
            }
            $("#getvefydata").click(function () {
                var data = { imgCode: $("#validateCode").val(), mobilePhone: $('#phone').val() };
                $.getJSON("http://ac.ppdai.com/ValidateCode/MobileWithImgCode?callback=?", data, function (result) {
                    if (result != null && result.Code != "1") {
                        alert(result.Message);
                    }
                });
                time(this);
            });

            var isSubmit = false;
            $("#sem_btn").click(function () {

                if ($(".sem_gou:visible").length == 4 && isSubmit == false) {
                    isSubmit = true;
                    $('#sem_btn').attr("disabled", true);
                    $('#sem_btn').attr("value", "正在提交...");
                    var gateway = new servicestack.ClientGateway("http://" + window.location.host);
                    var regSource = 0;
                    if (document.location.href.indexOf("rongdao") > 0) regSource = 101;
                    var data = {
                        UserName: $("#userName").val(),
                        Email: $("#email").val(),
                        Password: $("#Password").val(),
                        RegisterValidateType: $("#RegisterValidateType1").val(),
                        RegSource: regSource
                    };

                    $('#regform').submit();
                } else if (isSubmit == false) {
                    $("#phone , #validateCode,#Password,#verfyData").blur();
                }
            });
        });
    </script> 
