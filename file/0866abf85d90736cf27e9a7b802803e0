<script>
    function show_swt() {
        $("#swt_center").css("display", "block");
    }

    $(document).ready(function () {

        var allwidth = document.body.offsetWidth;
        var allheight = document.documentElement.clientHeight;
        if (allwidth > 200) {
            $("#swt_center").css("left", ((allwidth - 411) / 2) + "px");
        }
        if (allheight > 100) {
            $("#swt_center").css("top", ((allheight - 217) / 2) + "px");
        }

        setTimeout(show_swt, 5000);

        $("#swt_center_close,#swt_center_close1").click(function () {
            $("#swt_center").css("display", "none");
            setTimeout(show_swt, 5000);
        });

    });

    setTimeout(show_swt, 5000);
   
</script>

