                                var x = $(this).index();
                                if (x == 0) {
                                    $("#li2,#li4,#li8,#li6,#li12,#li9").css("display", "none");
                                    $("#li10,#li1,#li3,#li5,#li7,#li11").css("display", "block");
                                    $("#ul10").slideToggle("slow");
                                    $("#ul2,#ul4,#ul6,#ul8,#ul12").slideUp("slow");
                                }
                                if (x == 1) {
                                    $("#li9").css("display", "block");
                                    $("#li10").css("display", "none");
                                }
                            });
                        });
                    </script>
