                        $(function () {
                            $("#li1").css("display", "none");
                            $("#li2").css("display", "block");
                            $("#ul2").css("display", "block");
                            $("#ul1 li").click(function () {
                                var x = $(this).index();
                                if (x == 0) {
                                    $("#li1,#li4,#li6,#li8,#li10,#li12").css("display", "none");
                                    $("#li2,#li3,#li5,#li7,#li9,#li11").css("display", "block");
                                    $("#ul2").slideDown("slow");
                                    $("#ul4,#ul6,#ul8,#ul10,#ul12").slideUp("slow");
                                }
                                if (x == 1) {
                                    $("#li1").css("display", "block");
                                    $("#li2").css("display", "none");
                                    $("#ul2").slideUp("slow");
                                }
                            });
                        });
