                    $(".center1_top_center_ul li").mouseenter(function () {
                        var x = $(this).index();
                        if (x == 0) {
                            $(".center1_top_center_ul_li").css("background", "url(/WebUsing/img/index_img15.png)");
                            $(".center1_top_center_ul_li").css("color", "#FFF");
                            $(".center1_top_center_ul_li2").css("background", "none");
                            $(".center1_top_center_ul_li2").css("color", "#333333");
                            $(".center1_top_center_div2").css("display", "none");
                            $(".center1_top_center_div").css("display", "block");
                        }
                        if (x == 1) {
                            $(".center1_top_center_ul_li2").css("background", "url(/WebUsing/img/index_img15.png)");
                            $(".center1_top_center_ul_li2").css("color", "#FFF");
