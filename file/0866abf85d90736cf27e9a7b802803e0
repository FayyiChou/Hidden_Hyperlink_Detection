            <script type="text/javascript">
                var a = 0;
                var speed = 40
                gpic2.innerHTML = gpic1.innerHTML
                function Marquee4() {
                    if (gpic2.offsetWidth - gpic.scrollLeft <= 0)
                        gpic.scrollLeft -= gpic1.offsetWidth
                    else {
                        a = gpic.scrollLeft++
                    }
                    /*document.write("gpic2.offsetWidth");*/
                    if (a == 1) {
                        document.getElementById("center3_right_p").style.backgroundImage = "url(/WebUsing/img/index_img68.png)";
                    }
                    if (a == 90) {
                        document.getElementById("center3_right_p").style.backgroundImage = "url(/WebUsing/img/index_img69.png)";
                    }
                    if (a == 192) {
                        document.getElementById("center3_right_p").style.backgroundImage = "url(/WebUsing/img/index_img70.png)";
                    }
                    if (a == 292) {
                        document.getElementById("center3_right_p").style.backgroundImage = "url(/WebUsing/img/index_img71.png)";
                    }
                    if (a == 392) {
                        document.getElementById("center3_right_p").style.backgroundImage = "url(/WebUsing/img/index_img68.png)";
                    }

                }
                var MyMar4 = setInterval(Marquee4, speed)
                gpic.onmouseover = function () { clearInterval(MyMar4) }
                gpic.onmouseout = function () { MyMar4 = setInterval(Marquee4, speed) }
            </script>
