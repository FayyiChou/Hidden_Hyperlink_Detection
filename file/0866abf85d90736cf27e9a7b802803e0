        $(function () {
            var met;
            var x;
            $(".daohang_div_ul>li").hover(function () {
                x = $(this).index();
                var s = $(this);
                $('.daohang_div_ul>li').css("background-image", "none");
                $(this).css("background-image", "url(/WebUsing/img/nav_bg_move.png)");
                met = setTimeout(function () {
                    if (s.find('#daohang_div_ul' + x + '' != null)) {
                        s.find('#daohang_div_ul' + x + '').slideDown('slow');
                        
                        met = null;
                    }
                }, 100);
            }, function () {
                if (met != null) {
                    clearTimeout(met);
                }
                $(this).find('ul').css('display', 'none');
            });
        });
    </script>
