            <script>
                /*ul的显示隐藏*/
                $(function () {
                    var a = 0;
                    $(".center1_top_left_p").click(function () {
                        a++;
                        if (a == 2) {
                            a = 0;
                        }
                        if (a == 0) {
                            $(".center1_top_left_ul2").css("display", "block");
                            $(".center1_top_left_ul3").css("display", "none");
                        }
                        if (a == 1) {
                            $(".center1_top_left_ul3").css("display", "block");
                            $(".center1_top_left_ul2").css("display", "none");
                        }
                    });
                    $(".center1_top_left_p2").click(function () {
                        a++;
                        if (a == 2) {
                            a = 0;
                        }
                        if (a == 0) {
                            $(".center1_top_left_ul2").css("display", "block");
                            $(".center1_top_left_ul3").css("display", "none");
                        }
                        if (a == 1) {
                            $(".center1_top_left_ul3").css("display", "block");
                            $(".center1_top_left_ul2").css("display", "none");
                        }
                    });
                });
            </script>
