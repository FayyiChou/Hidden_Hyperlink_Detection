                    var speed = 30
                    gpic11.innerHTML = gpic10.innerHTML

                    function Marquee() {
                        if (gpic11.offsetWidth - gpic9.scrollLeft <= 0)
                            gpic9.scrollLeft -= gpic10.offsetWidth
                        else {
                            gpic9.scrollLeft++
                        }
                    }
                    var MyMar = setInterval(Marquee, speed)
                    gpic9.onmouseover = function () { clearInterval(MyMar) }
                    gpic9.onmouseout = function () { MyMar = setInterval(Marquee, speed) }
                </script>
