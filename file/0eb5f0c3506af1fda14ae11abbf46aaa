<script>
var ycssnr=document.getElementById("ycssnr");
var ssnr=document.getElementById("ssnr");
function qhss(n){
	var ycssnrn=document.getElementById("ycssnr"+n);
	ssnr1=ssnr.innerHTML
	ssnr.innerHTML=ycssnrn.innerHTML;
	ycssnrn.innerHTML=ssnr1;
	ycssnr.style.display="none";
	var hyformnew2=document.getElementById("hyformnew2");<!--20130321微门户搜索 新声明变量-->
	
	//获得当前显示的文本
	var ssnr_txt = ssnr.innerHTML;
	var showtext = document.getElementById("w");
	var searchtype = document.getElementById("c");
		if(ssnr_txt == '微门户'){
		hyformnew2.action="http://miniportal.b2b.hc360.com/miniportal/turbine/template/miniportal,search.html";
	}else{
		hyformnew2.action="http://s.hc360.com/iframe_ls.cgi";
	}<!--20130321微门户搜索 新增加判断-->
	if(ssnr_txt == '求购'){
		searchtype.value = "求购信息";
		showtext.value= "请输入搜索求购内容";	
	} else if(ssnr_txt == '公司'){
		searchtype.value = "企业库";
		showtext.value="请输入搜索公司内容";
	} else if(ssnr_txt == '资讯'){
		searchtype.value = "行业资讯";
		showtext.value="请输入搜索资讯内容";
	} else if(ssnr_txt == '标王'){
		searchtype.value = "";
		hyformnew.action="http://my.b2b.hcf360.com/my/turbine/template/pubinfo%2Ckeyword%2Cchampion_query2.html";
		showtext.value="请输入搜索标王内容";
	}else if(ssnr_txt == '微门户'){
		searchtype.value = "微门户";
		showtext.value="请输入搜索微门户内容";
	}<!--20130321微门户 新增-->  
	else {
		searchtype.value = "供应信息";
		showtext.value="请输入搜索供应内容";
	}
}
function hideYcssnr(){
	ycssnr.style.display = "none";
}
function showYcssnr(){
	ycssnr.style.display = "";
}
</script>
      <form action="http://s.hc360.com/iframe_ls.cgi" method="get" name="hyformnew" target="_blank" id="hyformnew2" onsubmit="return check(this);">
