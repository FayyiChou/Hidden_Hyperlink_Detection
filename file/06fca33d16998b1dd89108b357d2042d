            <script type="text/javascript">
                var slideInt;
                function slideOutHome(el) {
                    if ($(el).data('lock') == 1) {
                        $(el).children('.rollover').slideDown('normal', 'easeOutExpo',
                        function() {
                            $(el).find('.shadow-left').animate({
                                'margin-left': 0
                            },
                            125);
                            $(el).find('.shadow-right').animate({
                                'margin-right': 0
                            },
                            125);
                        });
                    }
                }
                function slideUpHome(el) {
                    if ($(el).data('lock') == 0) {
                        $(el).find('.shadow-left').animate({
                            'margin-left': 15
                        },
                        'slow');
                        $(el).find('.shadow-right').animate({
                            'margin-right': 15
                        },
                        'slow');
                        $(el).children('.rollover').slideUp('normal', 'easeInExpo',
                        function() {});
                    }
                }
                $(document).ready(function() {
                    $('.splash-items .item').find('.shadow-left').css({
                        'margin-left': 15
                    });
                    $('.splash-items .item').find('.shadow-right').css({
                        'margin-right': 15
                    });
                    $('.splash-items .item').mouseover(function() {
                        $(this).data('lock', 1);
                        var el = this;
                        setTimeout(function() {
                            slideOutHome(el);
                        },
                        200);
                    });

                    $('.splash-items .item').mouseleave(function() {
                        $(this).data('lock', 0);
                        var el = this;
                        setTimeout(function() {
                            slideUpHome(el);
                        },
                        200);
                    });
                });

                
            </script>
