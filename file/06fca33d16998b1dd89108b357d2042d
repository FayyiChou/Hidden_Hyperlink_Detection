<script type="text/javascript">
    function mypop() {
        if ($('#showvod').css("display") == "none") {
            $('#showvod').css("display", "block");
            $('#hidevod').css("display", "none");
            $('#rbcontact').css("display", "none");
        } else if ($('#hidevod').css("display") == "none") {
            $('#showvod').css("display", "none");
            $('#hidevod').css("display", "block");
            $('#rbcontact').css("display", "block");
        }
    }
</script>
    
  

