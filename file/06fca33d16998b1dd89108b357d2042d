<script type="text/javascript">
    $(function () {
        $('#callback').show();
        $('body').on('click', '#callback > a, #callback a.close', function (e) {
            $('#callback > a').toggleClass('active');
            $('#callback > div').fadeToggle();
            e.preventDefault();
        });
       
        
        $("#let_btn").click(function () {
            emailsubmit("email", "let_btn")
        });
        $("#let_btn_bottom").click(function () {
            emailsubmit("email_bottom", "let_btn_bottom")
        });

        function emailsubmit(v, b) {
            v1 = $("#" + v).val();
            if (v1 != "") {
                $("#" + b).attr("disabled", "disabled");
                $.ajax({
                    type: "POST",
                    url: "index.aspx",
                    data: "v1=" + encodeURIComponent(v1) + "&op=email",
                    success: function (data) {
                        $("#" + b).removeAttr("disabled");//
                        $("#btn1").css("display", "block");
                        if (data == "1") {
                            alert("谢谢!稍后会有专业的销售与您联系 .");
                            $("#" + v).val("");
                            
                        }
                    }
                });
            } else {
                $("#" + v).focus();
            }
        }
    });

        </script>
       

