<script>
function addflash(infoid,focus_width,focus_height,pics,links,texts){
	var interval_time=5 	
	 var focus_width=focus_width?focus_width:300;
	 var focus_height=focus_height?focus_height:225;
	 var text_height=15;
	 var swf_height = focus_height+text_height;	
	 var text_align= 'center' //标题文字对齐方式(left、center、right)
	  var so = new SWFObject("themes/rational/js/pixviewer.swf", "swfb1",focus_width,swf_height, "10");
	so.addParam('FlashVars', 'pics='+pics+'&links='+links+'&texts='+texts+'&borderwidth='+focus_width+'&borderheight='+focus_height+'&textheight='+text_height+'&interval_time='+interval_time+'&text_align='+text_align);
	so.addParam("allowScriptAccess", 'sameDomain');
	so.addParam("quality", 'high');
	so.addParam("bgcolor", '#ffffff');
	so.addParam("menu", 'true');
	so.addParam("wmode", 'opaque');
	so.write(document.getElementById(infoid));
}
/*$(document).ready(function(){
	var huandengid_pics="";var huandengid_texts="";var huandengid_links="";
	$("#flashid img").each(function(i){
   		if(this.src&&this.title)
		{
			huandengid_pics+=this.src+"|";
			huandengid_texts+=this.title+"|";
		}
	 });
	$("#flashid a").each(function(i){
   		if(this.href)
		{
			huandengid_links+=this.href+"|";
		}
	 });
	 addflash("flash",300,210,huandengid_pics,huandengid_links,huandengid_texts);
})*/
</script>
