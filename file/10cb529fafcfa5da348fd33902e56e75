var DivFriendActionContainer=document.getElementById("DivFriendActionContainer");
var DivFriendsAction=document.getElementById("DivFriendsAction");
var DivFriendsAction2=document.getElementById("DivFriendsAction2");
var speed=40 
DivFriendsAction2.innerHTML=DivFriendsAction.innerHTML 
function Marquee(){ 
if(DivFriendActionContainer.scrollLeft>=DivFriendsAction.scrollWidth){ 
DivFriendActionContainer.scrollLeft=0 
}else{ 
DivFriendActionContainer.scrollLeft++; 
} 
} 
var MyMar=setInterval(Marquee,speed) 
DivFriendActionContainer.onmouseover=function() {clearInterval(MyMar)} 
DivFriendActionContainer.onmouseout=function() {MyMar=setInterval(Marquee,speed)} 

