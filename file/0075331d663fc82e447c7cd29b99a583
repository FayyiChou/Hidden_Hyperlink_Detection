<script>
function setsite(cityid,name){
	$.post("http://www.erapower.cn/index.php?m=ajax&c=SiteCity",{cityid:cityid,cityname:name},function(data){
		if(data!=""){
			window.location.href=data;
		}
	});
}
function setsite_hy(hyid){
	$.post("http://www.erapower.cn/index.php?m=ajax&c=SiteHy",{hyid:hyid},function(data){
		if(data!=""){
			window.location.href=data;
		}
	});
}
function show_site(div_id){
	if(div_id=='site_city'){
		$("#site_city").show();
		$("#site_hy").hide();
	}else{
		$("#site_hy").show();
		$("#site_city").hide();
	}
}
</script>
<script>
function alertsite(){
var pageii = $.layer({
  type: 1,
  title: false,
  closeBtn: [0],
  area: ['auto', 'auto'],
  page:  {dom : '#site'}
});
$('#pagebtn').on('click', function(){
  layer.close(pageii);
});
}

</script>
