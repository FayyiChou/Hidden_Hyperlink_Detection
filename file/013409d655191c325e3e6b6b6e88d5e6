        <script>
    $(function () {
        Pagination('news_content');
        $("body").css("margin-top", '44px');
        $(document).ready(function () {
            $("#news_content img").css('width', '100%');
            $('#news_content img').css('height', 'auto');
        });
        $(".share").hide();
        $(".share_in").hide();
        $("#share").click(function () {
            $(".share").fadeIn();
            $(".share_in").fadeIn();
        });

        $(".share").click(function () {
            $(".share").fadeOut();
            $(".share_in").fadeOut();
        });
    });
    //top
    (function gotop(anchor) {
        window.addEventListener("load", function () {
            window.setTimeout(function () { window.scrollTo(0, 1); }, 300);
        }, false)
        document.body.scrollTop = 0;
        var screenheight = 480;
        var imgH = 45;//滑动元素的高度
        var gotop = document.querySelector(anchor)
        function addStyle(p, s) {
            var ps = p['style'], s;
            for (var k in s) { ps[k] = s[k]; }
        }
        gotop.addEventListener('click', function () {
            addStyle(gotop, { 'opacity': '0' })
            window.setTimeout(function () { window.scrollTo(0, 1); }, 300);
        }, false)
        document.body.addEventListener('touchstart', function () {
            addStyle(gotop, {
                '-webkit-transition-duration': '',
                '-webkit-transition-timing-function': '',
                'opacity': '1'
            })
        }, false);
        document.addEventListener('scroll', function () {
            if (document.body.scrollTop < 100) return;
            var screenheight = parseInt(window.innerHeight) - imgH;
            var scrollLength = document.body.scrollTop + screenheight;
            addStyle(gotop, {
                '-webkit-transition-duration': '400ms',
                '-webkit-transition-timing-function': 'ease-out-in',
                'opacity': '0',
