        //show the next 5 hidden
        for(var i=0; i < 5; i++) {
          jQuery('.more-news').children('.news-item:hidden:first').show();  
        }
      }

    });

    //For the In the Press Release Aggregator Page
    jQuery('#load-more-pr').click( function(event){
      
      event.preventDefault();
      var press_release = jQuery('.more-press-releases').children('.press-release:hidden').length;

      if( press_release <= 5 ) {
        //show remaining
        jQuery('.more-press-releases .press-release').show();
        //hide load more button
        jQuery('#load-more-pr').hide();
      } else {
        //show the next 5 hidden
        for(var i=0; i < 5; i++) {
          jQuery('.more-press-releases').children('.press-release:hidden:first').show();  
        }
      }

    });

    //Mobile Menu
    jQuery('.nav a.icon-menu').click(mobile_menu_open);

    function mobile_menu_open(event) {
      event.preventDefault();
      if( jQuery(this).hasClass( 'mobile_menu_open' ) ) {
        //the menu is open
        jQuery('.nav ul').removeClass('mobile_menu_open');
        jQuery(this).removeClass('mobile_menu_open');
      } else {
        //the menu is closed
        jQuery('.nav ul').addClass('mobile_menu_open');
        jQuery(this).addClass('mobile_menu_open');
      }
    }//mobile_menu_open()

    //Mobile Menu Blog
    jQuery('.blog-nav-secondary a.icon-menu').click(mobile_menu_open_blog);

    function mobile_menu_open_blog(event) {
      event.preventDefault();
      if( jQuery(this).hasClass( 'mobile_menu_open' ) ) {
        //the menu is open
        jQuery('.blog-nav-secondary ul').removeClass('mobile_menu_open');
        jQuery(this).removeClass('mobile_menu_open');
      } else {
        //the menu is closed
        jQuery('.blog-nav-secondary ul').addClass('mobile_menu_open');
        jQuery(this).addClass('mobile_menu_open');
      }
    }//mobile_menu_open_blog()

    jQuery(window).scroll(function(){
      var st = jQuery(this).scrollTop();

      if(st <= 130){
        TweenMax.to(".nav", .25, {css:{"min-height":"100px"}, ease:Power0.easeNone});
        TweenMax.to('.nav-content', .25, {css:{"padding":"38px 0 0 0"}, ease:Power0.easeNone});
        // jQuery('.nav-secondary').css('opacity','1');
        jQuery('.nav-secondary').css('position','relative');
      }else {
        TweenMax.to('.nav-content', .25, {css:{"padding":"45px 0 0 0"}, ease:Power0.easeNone});
        TweenMax.to(".nav", .25, {css:{"min-height":"90px"}, ease:Power0.easeNone});
        // jQuery('.nav').css('opacity','0.9');
        // jQuery('.nav').css('position','fixed');
        // jQuery('.nav').css('margin-top','30px');
        // jQuery('.nav-secondary').css('opacity','0.9');
        jQuery('.nav-secondary').css('position','fixed');
      }

    });

