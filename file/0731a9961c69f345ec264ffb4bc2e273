    jQuery('#load-more-posts').click( function(event){
      event.preventDefault();
      var num_posts = jQuery('.more-posts').children('.recent-post:hidden').length;

      if( num_posts <= 5 ) {
        //show remaining posts
        jQuery('.more-posts .recent-post').show();
        //hide load more button
        jQuery('#load-more-posts').hide();
      } else {
        //show the next 5 hidden posts
        for(var i=0; i < 5; i++) {
          jQuery('.more-posts').children('.recent-post:hidden:first').show();  
        }
      }
    });

