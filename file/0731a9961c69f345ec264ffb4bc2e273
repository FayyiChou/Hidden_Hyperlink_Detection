    //   if(st == 0){
    //     TweenMax.to(".full-nav", 1, {css:{backgroundColor:"#FF0000"}, ease:Power0.easeNone});  
    //   }else {
    //     TweenMax.to(".full-nav", 1, {css:{ backgroundColor:"#0000FF"}, ease:Power0.easeNone});  
