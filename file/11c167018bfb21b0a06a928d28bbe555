<script>

    $(function () {
        /*-------------------------------------------------------------------------
        * author:师亚伟
        * date:2016-01-15
        * desc：更新登录验证码（三种类型）
        -------------------------------------------------------------------------*/
        var yzmType = function () {
            //IE6下默认不缓存背景图片，CSS里每次更改图片的位置时都会重新发起请求，用这个方法告诉IE6缓存背景图片 
            var isIE6 = /msie 6/i.test(navigator.userAgent);
            if (isIE6) {
                try { document.execCommand('BackgroundImageCache', false, true); } catch (e) { }
            }

            window.chinaCaptcha = new ChinaCaptcha($("#form_Pass"), {
                bid: "login"
            });
            chinaCaptcha.init();
        }
        yzmType();



        /* 加载轮播图json */
        $.ajax({
            url: "/Advertisement/GetAdInfo",
            type: "GET",
            dataType: "json",
            data: { r: Math.random() },
            success: function (d) {
                if (d.IsSuccess && d.DataAdInfo.length > 0) {
                    var s = "", v = "";
                    for (var i = 0; i < d.DataAdInfo.length; i++) {
                        if (i == 0) {
                            if (d.DataAdInfo[i].url != "") {
                                s += '<li class="current" style="display:block"><div class="div_focbg" style="background-image: url(' + d.DataAdInfo[i].img + ');"><a href="' + d.DataAdInfo[i].url + '" target="_blank" title="' + d.DataAdInfo[i].name + '"></a></div></li>';
                            } else {
                                s += '<li class="current" style="display:block"><div class="div_focbg" style="background-image: url(' + d.DataAdInfo[i].img + ');"></div></li>';
                            }
                            v += '<a class="current" href="javascript:;"></a>';
                        } else {
                            if (d.DataAdInfo[i].url != "") {
                                s += '<li><div class="div_focbg" style="background-image: url(' + d.DataAdInfo[i].img + ');"><a href="' + d.DataAdInfo[i].url + '" target="_blank" title="' + d.DataAdInfo[i].name + '"></a></div></li>';
                            } else {
                                s += '<li><div class="div_focbg" style="background-image: url(' + d.DataAdInfo[i].img + ');"></div></li>';
                            }
                            v += '<a href="javascript:;"></a>';
                        }
                    }
                    var str = '<div class="sg_focus">' +
                              '<ul class="ul_focus">' + s +
                              '</ul>' +
                              '</div>' +
                              '<div class="focsel clearFix">' + v +
                              '</div>';
                    $(".sg_main").append(str);


                    /*图片淡隐淡现*/
                    var defaultOpts = { interval: 8000, fadeInTime: 300, fadeOutTime: 200 };
                    var _titles = $(".focsel a");
                    var _bodies = $("ul.ul_focus li");
                    var _count = _titles.length;
                    var _current = 0;
                    var _intervalID = null;

                    var stop = function () {
                        window.clearInterval(_intervalID);
                    };

                    var slide = function (opts) {
                        if (opts) {
                            _current = opts.current || 0;
                        } else {
                            _current = (_current >= (_count - 1)) ? 0 : (++_current);
                        };
                        _bodies.filter(":visible").fadeOut(defaultOpts.fadeOutTime, function () {
                            _bodies.eq(_current).fadeIn(defaultOpts.fadeInTime);
                            _bodies.removeClass("current").eq(_current).addClass("current");
                        });
                        _titles.removeClass("current").eq(_current).addClass("current");
                    };

                    var go = function () {
                        stop();
                        _intervalID = window.setInterval(function () {
                            slide();
                        }, defaultOpts.interval);
                    };

                    var itemMouseOver = function (target, items) {
                        stop();
                        var i = $.inArray(target, items);
                        slide({ current: i });
                    };

                    _titles.click(function () {
                        if ($(this).attr('class') != 'current') {
                            itemMouseOver(this, _titles);
                            go();
                        }
                    });

                    _bodies.hover(stop, go);

                    go();
                    /*图片淡隐淡现 end*/
                }
            }
        });
        /* 加载轮播图json end */

        /* 底部广告 */
        $(".sg_ad").find("li").hover(function () {
            $(this).addClass("ad_cur");
        }, function () {
            $(this).removeClass("ad_cur");
        });
        /* 底部广告 end */

        /* 登陆层切换 */
        $(".js_tablog a").click(function () {
            var index = $(this).index();
            $(".js_tablog a").removeClass("sign_cur");
            $(this).addClass("sign_cur");
            $(".formbox > div").hide().eq(index).show();
            if ($(this).attr("data-id") == "js_ewm") {
                if ($("#js_ewm_box").attr("data-status") == "1") {
                    $("#js_ewm_box").attr('data-status', '0');
                    getqrcode($("#js_ewm_box"));
                }
            }
        });
        getqrcode($("#js_ewm_box"));
        function ewmLoginFn(oEwmbox) {
            $.ajax({
                url: "/QRCode/QRCodeValid",
                type: 'GET',
                data: { r: Math.random() },
                success: function (d) {
                    if (d.success) {
                        if (getParamValue('orderId') != "") {
                            window.location.href = "/Payment/Alipay/Index/?gpayOrderId=" + getParamValue('orderId');
                        } else {
                            window.location.href = "/UserCenter/index/";
                        }
                    } else {
                        $('.ewmOverDue_box').show();
                        $('.ewmtxt').html('二维码失效<br/>请点击刷新');
                        oEwmbox.attr('data-status', '1');
                    }
                },
                error: function () {
                    $('.ewmOverDue_box').show();
                    $('.ewmtxt').html('网络异常<br/>请点击刷新');
                    oEwmbox.attr('data-status', '1');
                }
            })
        }
        function getqrcode(oEwmbox) {
            if (oEwmbox.attr('data-status') == '1') {
                oEwmbox.attr('data-status', '0');
                $('.ewmOverDue_box').hide();
                ajaxqrcode(oEwmbox)
            } else {
                $('.ewmOverDue_box').hide();
                ajaxqrcode(oEwmbox)
            }
        };
        function ajaxqrcode(oEwmbox) {
            $.ajax({
                url: "/QRCode/GetLoginQRCode",
                type: 'GET',
                data: {
                    r: Math.random()
                },
                success: function (d) {
                    if (d.success) {
                        $('#js_ewm_box').empty();
                        var qrcode = new QRCode('js_ewm_box', {
                            text: d.content,
                            width: 177,
                            height: 177,
                            colorDark: '#000',
                            colorLight: '#ffffff',
                            correctLevel: QRCode.CorrectLevel.H
                        });
                        if (oEwmbox.find('table').length == 1) {
                            ewmLoginFn(oEwmbox)
                        } else {
                            oEwmbox.find('img').unbind('load').bind('load', function () {
                                ewmLoginFn(oEwmbox)
                            });
                        }
                    }
                },
                error: function () {
                    $('.ewmOverDue_box').show();
                    $('.ewmtxt').html('获取二维码失败<br/>请点击刷新');
                    oEwmbox.attr('data-status', '1');
                }
            })
        };
        $('.ewmOverDue_box').unbind('click').bind('click', function () {
            $(this).hide();
            getqrcode($("#js_ewm_box"));
        })


        /* 登陆层切换 end */

        /* 对输入框提示文字的处理 */
        setTimeout(function () {
            $.each($(".d_iptw input"), function (i, item) {
                if ($(this).val() == "") {
                    $(this).parents(".d_iptw").find(".tip_word").show();
                } else {
                    $(this).parents(".d_iptw").find(".tip_word").hide();
                }
            });
        }, 1000);

        $(".d_iptw input").focus(function () {
            $(this).parents(".d_iptw").removeClass("d_iptw_err").addClass("d_iptw_foc");

        }).blur(function () {
            $(this).parents(".d_iptw").removeClass("d_iptw_foc");
        }).live("keydown keyup", function () {
            var _this = $(this);
            if ($(this).val() != "") {
                $(_this.parent(".d_iptw").find(".tip_word")).hide();
            } else {
                $(_this.parent(".d_iptw").find(".tip_word")).show();
            }
        });

        $(".d_iptw input").bind("paste", function (e) {
            var _this = $(this);
            setTimeout(function () {
                if (_this.val() != "") {
                    $(_this.parent(".d_iptw").find(".tip_word")).hide();
                } else {
                    $(_this.parent(".d_iptw").find(".tip_word")).show();
                }
            }, 100);
        });

        $(".tip_word").click(function () {
            $(this).parents(".d_iptw").find("input").focus();
        });
        /* 对输入框提示文字的处理 end */

        $(":input[data-id=txtCaptchaCode]").live("keyup paste focus blur", function () {
            var val = $(this).val();
            var pattern = new RegExp("[^A-Za-z0-9]", "g");
            $(this).val(val.replace(pattern, ''));
        });

        /* 帐号密码登陆 */
        $("#form_Pass").submit(function () {
            $(".js_ekey_err").hide().html("");
            var exp = new RegExp("^[\u4e00-\u9fa5]{0,}$");

            var account = $(":input[data-id=txtUserName]");
            var password = $(":input[data-id=txtUserPassword]");
            var captchaCode = $(":input[data-id=txtCaptchaCode]");

            if (account.val() == "") {
                $("#form_Pass").find(".errorbox").show().html("账号不能为空");
                account.parent(".d_iptw").addClass("d_iptw_err");
                return false;
            } else if (exp.test(account.val())) {
                $("#form_Pass").find(".errorbox").show().html("账号格式错误");
                account.parent(".d_iptw").addClass("d_iptw_err");
                return false;
            } else if (password.val() == "") {
                $("#form_Pass").find(".errorbox").show().html("密码不能为空");
                password.parent(".d_iptw").addClass("d_iptw_err");
                return false;
            }

            /*登录ajax*/
            var ajaxLoginFn = function () {
                $.ajax({
                    url: "/Login/Index",
                    type: "post",
                    dataType: "json",
                    data: {
                        Account: account.val(),
                        Password: password.val(),
                        CaptchaCode: captchaCode.val(),
                        KeyType: $(":input[name=KeyType]").val(),
                        orderId: getParamValue('orderId'),
                        r: Math.random()
                    },
                    beforeSend: function () { $("#form_Pass").find(":submit").attr("disabled", "disabled").val("正在登录..."); },
                    success: function (d) {
                        $("#form_Pass").find(":submit").removeAttr("disabled").val("登  录");
                        if (d.IsSuccess) {

                            window.location.href = d.ReturnUrl;

                        } else {
                            if ($('.js_login_type').val() == 1) {
                                alert(d.Message);
                            }
                            $('#captcha_image').attr('src', '/Login/Create?bid=login&r=' + Math.random());
                            chinaCaptcha.refreshCaptcha();//刷新
                            $("#form_Pass").find(".errorbox").show().html(d.Message);
                        }
                    },
                    error: function () {
                        $("#form_Pass").find(":submit").removeAttr("disabled").val("登  录");
                        $("#form_Pass").find(".errorbox").show().html("登录失败，请稍后重试。");
                    }
                });
            }

            if ($('.js_login_type').val() == 0) {
                if (captchaCode.val() == "") {
                    $("#form_Pass").find(".errorbox").show().html("验证码不能为空");
                    captchaCode.parent(".d_iptw").addClass("d_iptw_err");
                    return false;
                }
                ajaxLoginFn();
            } else if ($('.js_login_type').val() == -1) {
                ajaxLoginFn();
            } else {
                chinaCaptcha.createCaptchaPop();//创建弹层
            }


            ChinaCaptcha.prototype.subFn = ajaxLoginFn;
            return false;
        });
        /* 帐号密码登陆 end */

        /* 手机认证登陆 */
        $("#form_PhoneValidate").submit(function () {

            $(".js_phone_err").hide().html("");
            var PhonePatt = /^[a-zA-Z0-9]{5}$/;
            var exp = new RegExp("^[\u4e00-\u9fa5]{0,}$");
            var account = $(":input[data-id=txtUserNamePhone]");
            var dynamicKey = $(":input[data-id=txtPhoneNumber]");
            var captchaCode = $(":input[data-id=txtCaptchaCodePhone]");

            if (account.val() == "") {
                $("#form_PhoneValidate").find(".errorbox").show().html("账号不能为空");
                account.parent(".d_iptw").addClass("d_iptw_err");
                return false;
            } else if (exp.test(account.val())) {
                $("#form_PhoneValidate").find(".errorbox").show().html("账号格式错误");
                account.parent(".d_iptw").addClass("d_iptw_err");
                return false;
            } else if (dynamicKey.val() == "") {
                $("#form_PhoneValidate").find(".errorbox").show().html("手机认证码不能为空");
                dynamicKey.parent(".d_iptw").addClass("d_iptw_err");
                return false;
            } else if (!PhonePatt.test(dynamicKey.val())) {
                $("#form_PhoneValidate").find(".errorbox").show().html("手机认证码错误");
                dynamicKey.parent(".d_iptw").addClass("d_iptw_err");
                return false;
            } else {
                $.ajax({
                    url: "/Login/Index",
                    type: "post",
                    dataType: "json",
                    data: { Account: account.val(), DynamicKey: dynamicKey.val(), CaptchaCode: captchaCode.val(), KeyType: $(":input[name=KeyTypephone]").val(), orderId: getParamValue('orderId'), r: Math.random() },
                    beforeSend: function () { $("#form_PhoneValidate").find(":submit").attr("disabled", "disabled").val("正在登录..."); },
                    success: function (d) {
                        $("#form_PhoneValidate").find(":submit").removeAttr("disabled").val("登  录");
                        if (d.IsSuccess) {
                            window.location.href = d.ReturnUrl;
                        } else {
                            $("#form_PhoneValidate").find(".errorbox").show().html(d.Message);
                            $("#captcha_image2").attr("src", "/Login/Create?fileName=" + Math.random());
                        }
                    },
                    error: function () {
                        $("#form_PhoneValidate").find(":submit").removeAttr("disabled").val("登  录");
                        $("#form_PhoneValidate").find(".errorbox").show().html("登录失败，请稍后重试。");
                    }
                });

                return false;
            }

            return false;
        });
        /* 手机认证登陆 end */

        /* 判断获取验证码按钮是否能用 */
        $(":input[data-id=txtUserNamePhone],:input[data-id=txtUserPasswordPhone]").keyup(function () {
            if ($(":input[data-id=txtUserNamePhone]").val() != "" && $(":input[data-id=txtUserPasswordPhone]").val() != "") {
                $(".js_sendbtn").removeClass("disb").removeAttr("disabled");
            } else {
                $(".js_sendbtn").addClass("disb").attr("disabled", "disabled");
            }
        });

        $(":input[data-id=txtUserNamePhone],:input[data-id=txtUserPasswordPhone]").bind("paste", function () {
            setTimeout(function () {
                if ($(":input[data-id=txtUserNamePhone]").val() != "" && $(":input[data-id=txtUserPasswordPhone]").val() != "") {
                    $(".js_sendbtn").removeClass("disb").removeAttr("disabled");
                } else {
                    $(".js_sendbtn").addClass("disb").attr("disabled", "disabled");
                }
            }, 300);
        });
        /* 判断获取验证码按钮是否能用 end */

        /* 发送验证码 end */
        $(".js_sendbtn").click(function () {
            $("#form_PhoneValidate").find(".errorbox").hide().html("");

            $.ajax({
                url: "/Login/SenderSms",
                type: "post",
                dataType: "json",
                data: { Account: $(":input[data-id=txtUserNamePhone]").val(), Password: $(":input[data-id=txtUserPasswordPhone]").val(), r: Math.random() },
                beforeSend: function (d) {
                    $(".js_sendbtn").addClass("disb").attr("disabled", "disabled").val("发送中...");
                },
                success: function (data) {
                    if (data.IsSuccess) {
                        $(":input[data-id=txtUserNamePhone]").attr("disabled", "disabled");
                        $(".js_sendbtn").addClass("disb").attr("disabled", "disabled").val("重新获取（" + data.PushTime + "）");
                        countDown($(".js_sendbtn"), data.PushTime);
                    } else {
                        $(":input[data-id=txtUserNamePhone]").removeAttr("disabled");
                        $(".js_sendbtn").removeClass("disb").removeAttr("disabled").val("获取验证码");
                        $("#form_PhoneValidate").find(".errorbox").show().html(data.Message);
                    }

                },
                error: function () {
                    $(":input[data-id=txtUserNamePhone]").removeAttr("disabled");
                }
            });
        });
        /* 发送验证码 end */
    });

    // 获取地址栏的参数数组
    function getUrlParams() {
        var search = window.location.search;
        // 写入数据字典
        var tmparray = search.substr(1, search.length).split("&");
        var paramsArray = new Array;
        if (tmparray != null) {
            for (var i = 0; i < tmparray.length; i++) {
                var reg = /[=|^==]/;    // 用=进行拆分，但不包括==
                var set1 = tmparray[i].replace(reg, '&');
                var tmpStr2 = set1.split('&');
                var array = new Array;
                array[tmpStr2[0]] = tmpStr2[1];
                paramsArray.push(array);
            }
        }

        // 将参数数组进行返回
        return paramsArray;
    }

    // 根据参数名称获取参数值
    function getParamValue(name) {
        var paramsArray = getUrlParams();
        if (paramsArray != null) {
            for (var i = 0 ; i < paramsArray.length ; i++) {
                for (var j in paramsArray[i]) {
                    if (j == name) {
                        return paramsArray[i][j];
                    }
                }
            }
        }
        return "";
    }

    /* 短信定时器倒计时*/
    function countDown(obj, seconds) {
        var timer = setInterval(function () {
            if (seconds > 1) {
                seconds = seconds - 1;
                obj.val("重新获取（" + seconds + "）");
            } else {
                clearInterval(timer);
                $("#form_PhoneValidate").find(".errorbox").hide().html("");
                $(":input[data-id=txtUserNamePhone]").removeAttr("disabled");
                obj.removeAttr("disabled").removeClass("disb").val("获取验证码");
            }
        }, 1000);
    }
</script>

