    <script type="text/javascript">
        $(function () {
            /* 加载底部信息json */
            $.ajax({
                url: "/Advertisement/GetAdInfo/",
                type: "GET",
                dataType: "json",
                data: { r: Math.random() },
                success: function (d) {
                    if (d.IsSuccess) {
                        var h = '',
                            n = '',
                            a = '';
                        if (d.DataHelp.length > 0) {
                            h += '<li><dl class="sg_guide"><dt>3分钟学会光宇Go购</dt><dd>';
                            for (var i = 0; i < d.DataHelp.length; i++) {
                                if (i != d.DataHelp.length - 1 && d.DataHelp[i].url != "") {

                                    h += '<a href="' + d.DataHelp[i].url + '" hidefocus="true" target="_blank">' + d.DataHelp[i].word + '</a> |';

                                } else if (i != d.DataHelp.length - 1 && d.DataHelp[i].url == "") {

                                    h += d.DataHelp[i].word + '|';

                                } else if (i == d.DataHelp.length - 1 && d.DataHelp[i].url != "") {

                                    h += '<a href="' + d.DataHelp[i].url + '" hidefocus="true" target="_blank">' + d.DataHelp[i].word + '</a>';

                                } else if (i == d.DataHelp.length - 1 && d.DataHelp[i].url == "") {

                                    h += d.DataHelp[i].word;
                                }
                            }
                            h += '</dd></dl></li>';

                        }

                        if (d.DataActivity.length > 0) {
                            a += '<li><dl class="sg_camp"><dt>最新活动</dt><dd>';
                            for (var i = 0; i < d.DataActivity.length; i++) {
                                if (i != d.DataActivity.length - 1 && d.DataActivity[i].url != "") {

                                    a += '<a href="' + d.DataActivity[i].url + '" hidefocus="true" target="_blank">' + d.DataActivity[i].word + '</a> |';

                                } else if (i != d.DataActivity.length - 1 && d.DataActivity[i].url == "") {

                                    a += d.DataActivity[i].word + '|';

                                } else if (i == d.DataActivity.length - 1 && d.DataActivity[i].url != "") {

                                    a += '<a href="' + d.DataActivity[i].url + '" hidefocus="true" target="_blank">' + d.DataActivity[i].word + '</a>';

                                } else if (i == d.DataActivity.length - 1 && d.DataActivity[i].url == "") {

                                    a += d.DataActivity[i].word;
                                }

                            }
                            a += '</dd></dl></li>';
                        }
                        if (d.DataNotice.length > 0) {
                            n += '<li><dl class="sg_notice"><dt>公告</dt><dd>';
                            if (d.DataNotice[0].url != "") {
                                n += '<a href="' + d.DataNotice[0].url + '" hidefocus="true" target="_blank">' + d.DataNotice[0].word + '</a>';
                            } else {
                                n += d.DataNotice[0].word;
                            }
                            n += '</dd></dl></li>';
                        }
                        $(".sg_ad").append(h).append(a).append(n);
                        $(".sg_ad li").hover(function () {
                            $(this).addClass("ad_cur");
                        }, function () {
                            $(this).removeClass("ad_cur");
                        });
                    }
                }
            });
            /* 加载底部信息json end */
        })

    </script>
