function settab_zzjs(name,num,n){
 for(i=1;i<=n;i++){
  var menu=document.getElementById(name+i);
  var con=document.getElementById(name+"_"+"tabmenu"+i);
  menu.className=i==num?"hover":"";
    con.style.display=i==num?"block":"none";
 }
}
</script>
