<div style="display:none;">
    <script type="text/javascript">
        /* 搜索框效果实现 */
        !function(){
            $sear = $("#J_search");

            $sel  = $sear.find(".J_sel");
            $placeholder = $('input[name=key]').attr('placeholder');
            // 初始化搜索框标签选中效果
            if ($('.J_type').val() == 'goods') {
                $sel.find(".goods").addClass("z-crt");
                $sel.find(".shop").removeClass("z-crt");
            } else if ($('.J_type').val() == 'shop') {
                $sel.find(".shop").addClass("z-crt");
                $sel.find(".goods").removeClass("z-crt");
                $('input[name=key]').attr('placeholder','');
            }
            $sel.find("a").click(function(){
                var key_placeholder = $('#key_value').val();
                $this = $(this);
                if(!$this.hasClass("z-crt")){
                    $sel.find("a").removeClass("z-crt");
                    $this.addClass("z-crt");
                    $sear.find(".J_type").val($this.data("type"));
                }
                if ($('.J_type').val() == 'shop') {
                    $('input[name=key]').attr('placeholder','');
                } else {
                    $('input[name=key]').attr('placeholder',key_placeholder);
                }
            });
            $sear.find("form").submit(function(){
                if ($sear.find(":text").val() == '' && $('.J_type').val() == 'goods') {
                    $sear.find(":text").val($placeholder)
                };
                return $sear.find(":text").val()!=="";
            });
        }();
        // 搜索框前的二维码显示效果
        $('.min-qrcode .close').on('click',function(){
            $('.min-qrcode').hide();
        });

    </script>
    <!-- 百度统计 -->
