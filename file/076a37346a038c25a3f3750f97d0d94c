<script type="text/javascript">
function ToSearch() {
    var txtContent = document.getElementById("txtSearch");
    if (txtContent == null || checkSpace(txtContent.value)) {
        alert("请输入查询关键字");
        return false
    }
    else {
        var newText = filter(txtContent.value);
        if (trimTxt(document.getElementById("selectSearch").value) == "2") {
            window.location.href = "/searchProduct.aspx?k=" + encodeURI(newText) + "&p=1";
        }
        else if (trimTxt(document.getElementById("selectSearch").value) == "1") {
            window.location.href = "/searchNews.aspx?k=" + encodeURI(newText) + "&p=1";
        }
        return false;
    }
}

//判断空格
function checkSpace(checkValue) {
    if (checkValue.length == 0) {
        return true;
    }
    else {
        checkValue = trimTxt(checkValue);
        if (checkValue.length == 0) {
            return true;
        }
        else {
            return false;
        }
    }
}
function trimTxt(txt) {
    return txt.replace(/(^\s*)|(\s*$)/g, "");
}

function filter(s) {
    var pattern = new RegExp("[`~!@#$^&*()=|{}':;',\\[\\].<>/?~！@#￥……&*（）&;—|{}【】‘；：”“'。，、？]")
    var rs = "";
    for (var i = 0; i < s.length; i++) {
        rs = rs + s.substr(i, 1).replace(pattern, '');
    }
    return rs.replace(/^\s+|\s+$/g, "");
}

function GetParaValue(url, paras) {
    var paraString = url.substring(url.indexOf("?") + 1, url.length).split("&");
    var paraObj = {}
    for (i = 0; j = paraString[i]; i++) {
        paraObj[j.substring(0, j.indexOf("=")).toLowerCase()] = j.substring(j.indexOf("=") + 1, j.length);
    }
    var returnValue = paraObj[paras.toLowerCase()];
    if (typeof (returnValue) == "undefined") {
        return "";
    } else {
        return returnValue;
    }
}
var url = window.location.href;
if (url.indexOf("search.aspx?") != -1) {
    var value = GetParaValue(url, "k");
    if (!checkSpace(value)) {
        document.getElementById("txtSearch").value = decodeURIComponent(value);
    }
}
</script>
