    //TAG
    function onThisTag(obj) {
        var as = obj.parentNode.getElementsByTagName("a");
        for (var i = 0; i < as.length; i++) {
            as[i].className = "nobg";
        }
        obj.className = "havebg";
        var uls = obj.parentNode.parentNode.getElementsByTagName("ul");
        for (var i = 0; i < uls.length; i++) {
            uls[i].style.display = "none";
        }
        document.getElementById("ul_" + obj.id).style.display = "block";
    }
    function onPTag(obj) {
        var as = obj.parentNode.getElementsByTagName("a");
        for (var i = 0; i < as.length; i++) {
            as[i].className = "nobg";
        }
        obj.className = "havebg";
        var uls = obj.parentNode.parentNode.getElementsByTagName("div");
        for (var i = 0; i < uls.length; i++) {
            uls[i].style.display = "none";
        }
        document.getElementById("p_" + obj.id).style.display = "block";
    }

</script>
