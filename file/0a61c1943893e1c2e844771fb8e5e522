//写入cookie
function setCookies(name,value)
{
   var Days = 30; //此 cookie 将被保存 30 天
   var expdate   = new Date();
   expdate.setTime(expdate.getTime() + Days*24*60*60*1000); 
   document.cookie = name + "="+ escape(value) +";expire*="+ expdate.toGMTString();
}

//读取cookie
function getCookies(name)
{
   var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
   if(arr != null) return unescape(arr[2]); return "null";
}


function maxok()
{
   setCookies("max","on");sfmax();
}


function maxno()
{
   setCookies("max","off");sfmax();
}


function sfmax(){
var sCookie = getCookies("max");
if (sCookie == "null" || sCookie == "off")
  {
	  sCookie = "<input type='checkbox' name='zidmax' value='1' onclick='maxok()'>下集自动全屏 关";
  }
else
  {
	  sCookie = "<input type='checkbox' name='zidmax' value='1' onclick='maxno()' checked>下集自动全屏 开";
  }
document.getElementById('max').innerHTML=sCookie;
}


