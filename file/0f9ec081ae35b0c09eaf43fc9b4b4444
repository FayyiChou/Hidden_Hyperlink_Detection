<!--//--><![CDATA[// ><!--

			$(document).ready(function() {
		
				$('.iosSlider').iosSlider({
					desktopClickDrag: false,
					snapToChildren: true,
					infiniteSlider: true,
					snapSlideCenter: true,
					navSlideSelector: '.sliderContainer .slideSelectors .item',
