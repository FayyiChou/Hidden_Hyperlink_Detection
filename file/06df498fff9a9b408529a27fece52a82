		<script>
		function changeinit(arr){
			var num = arr.length;
			for(var i=1;i<num;i++){
			$('#'+arr[i]).removeClass('current');
			//$().css('display','none');
			$('#'+arr[i]+'content').css('display','none');
			//$('#'+arr[i]+'content1').css('display','none');
			}
		}

		function change(id){
			changeinit(arguments);
			$('#'+id).addClass('current');
			$('#'+id+'content').css('display',''); 
			//$('#'+id+'content1').css('display',''); 
		}
		</script>
