<script type="text/javascript">
    function setAnchors(){
        var oIos = 'http://www.jj.cn/download/download.php?place=1753',
                oAnz = 'http://www.jj.cn/download/download.php?place=626',
                oIpad = 'http://www.jj.cn/download/download.php?place=1754',
                others = 'http://www.jj.cn/download/download.html',
                u = navigator.userAgent,
                ua = navigator.userAgent.toLowerCase(),
                ipad = ua.match(/ipad/i) == "ipad",
                iphoneos = (ua.match(/iphone os/i) == "iphone os") || (ua.match(/iph os/i) == "iph os"),
                android = (ua.match(/android/i) == "android") || (ua.match(/adr/i) == "adr"),
                andr_pad = (ua.match(/android/i) == "mi pad");

        var a_list = document.getElementsByTagName("a"),
                i = 0,
                len = a_list.length,
                get_par = "",
                get_par = function get_par(par){
                    var local_url = document.location.href;
                    var get = local_url.indexOf(par + "=");
                    if(get == -1){
                        return false;
                    }
                    var get_par = local_url.slice(par.length + get + 1);
                    var nextPar = get_par.indexOf("&");
                    if(nextPar != -1){
                        get_par = get_par.slice(0, nextPar);
                    }
                    return get_par;
                };

        place = get_par("place") || "626";

        for(i = 0; i < len; i++){
            (function(index){
                if (u.indexOf('Android') > -1 || u.indexOf('Linux') > -1) {
                    a_list[index].addEventListener('click',function(){
                        DownSoft(39480,place);
                    },false);

                }else if (u.indexOf('iPhone') > -1) {
                    a_list[index].addEventListener('click',function(){
                        DownSoft(4501641,1753);
                    },false);

                }else if(u.indexOf("ipad") > -1){
                    a_list[index].addEventListener('click',function(){
                        DownSoft(4501641,1754);
                    },false);
                }else{
                    a_list[index].href = others;
                }
            })(i)
        }

        function DownSoft(siteid,placeid){
            if(placeid != "" && placeid != "undefined"){
                var url1= "http://www.jj.cn/download/download.php?place=" + placeid;
            }else{
                var url1= "http://www.jj.cn/download/download.php";
            }
            url1 = escape(url1);
            clickStat(siteid,6,url1);
        }
    }
    window.addEventListener('DOMContentLoaded',setAnchors,false);
</script>

