$(document).ready(function(){
	$('.newstitle div').click(function(){
		$('.newstitle div').removeClass("cur");
		$(this).addClass("cur");
		$('.newsclist').removeClass("cur");
		$('.newsclist').eq($(this).index()).addClass("cur");
	});
});
