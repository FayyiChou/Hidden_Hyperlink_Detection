<script type="text/javascript">
function Local(val)
{
	if(val !='#' && val!='')
	{
		window.location.href=val;
	}
}
function addToFavorite() {
	var a = 'http://www.to-grace.com';
	var b = '至恩科技|光耦|光宝代理|松下光耦|光宝LED |COSMO光耦|亿光光耦|光宝光耦';
	if (document.all) {
		window.external.AddFavorite(a, b)
	} else if (window.sidebar) {
		window.sidebar.addPanel(b, a, "")
	} else {
		alert("对不起，您的浏览器不支持此操作!\n请您使用菜单栏或Ctrl+D收藏本站。")
	}
}
$(document).ready(function(){
	$indexlength=0;
	$($('.slide .banner-img-list li').get(0)).show();
	$('#bannerCtrl li').eq(0).addClass("current");
	$('#bannerCtrl li').mousemove(function(){
		$('#bannerCtrl li').removeClass("current");
		$('.slide .banner-img-list li').hide();
		$('.newslist ul li').hide();
		$(this).addClass("current");
		var index=$('#bannerCtrl li').index(this);
		$($('.slide .banner-img-list li').get(index)).show();	
		$('.newslist ul li').eq(index).show();
	});
	s();
});	 
$i=0;
function s()
{
		var timer = setInterval(
		function(){
			var $length=$('#bannerCtrl li').length;
			if($i<$length-1){
				$('#bannerCtrl li').removeClass("current");
				$('.slide .banner-img-list li').hide();
				$('.newslist ul li').hide();
				$($('.slide .banner-img-list li').get($i+1)).show();	
		    	$($('#bannerCtrl li').get($i+1)).addClass("current");
				$('.newslist ul li').eq($i+1).show();
				$i++;				
			}else{
				$i=0;
				$('#bannerCtrl li').removeClass("current");
				$('.slide .banner-img-list li').hide();
				$('.newslist ul li').hide();
				$($('.slide .banner-img-list li').get(0)).show();	
		    	$($('#bannerCtrl li').get(0)).addClass("current");
				$('.newslist ul li').eq(0).show();
			}
		},
		5000
		);
}
</script>
