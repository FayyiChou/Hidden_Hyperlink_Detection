(function(){
	var s1,s2,s3,timea;
	function init(){
		s1=getid("ODiv");
		s2=getid("ODiv1");
		s3=getid("ODiv2");
		s3.innerHTML=s2.innerHTML;
		timea=setInterval(mar,30)
		s1.onmouseover=function(){
			clearInterval(timea);
		}
		s1.onmouseout=function(){
			timea=setInterval(mar,30);
		}
	}
	function mar(){
		if(s2.offsetWidth<=s1.scrollLeft){
			s1.scrollLeft-=s2.offsetWidth;
		}else{
			s1.scrollLeft++;
		}
	}
	function getid(id){
		return document.getElementById(id);
	}
	init();
})();
</script>
