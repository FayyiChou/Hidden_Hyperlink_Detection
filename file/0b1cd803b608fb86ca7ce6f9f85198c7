											var gglspeed=30;
											ggl2.innerHTML=ggl1.innerHTML;
											function Marqueeggl(){
												if(ggl2.offsetTop-ggl.scrollTop<=0)
													ggl.scrollTop-=ggl1.offsetHeight;
												else{
													ggl.scrollTop++;
												}
											}
											var gglMar=setInterval(Marqueeggl,gglspeed);
											ggl.onmouseover=function() {clearInterval(gglMar)}
											ggl.onmouseout=function() {gglMar=setInterval(Marqueeggl,gglspeed)}
