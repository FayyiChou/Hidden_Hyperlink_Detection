	function printData_li(data,objid){
		$.each(data,function(i,val){
			var html  = "<li><a href=\""+val.url+"\" target=\"_blank\">"+val.title+"</a></li>";
			$('#'+objid).append(html);
		});
	}
	$.getJSON("http://xuexiao.eol.cn/public/js/trsdata20.php?channelid=11021&callback=?",function(data){
		printData_li(data,'11021');
	});
	$.getJSON("http://xuexiao.eol.cn/public/js/trsdata20.php?channelid=11019&callback=?",function(data){
