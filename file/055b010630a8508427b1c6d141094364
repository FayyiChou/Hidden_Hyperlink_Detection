            <script type="text/javascript">   
                    var t = n = count = 0;   
                    $(function(){   
                        count = $("#play_list a").size();   
                        $("#play_list a:not(:first-child)").hide();   
                        $("#play_info").html($("#play_list a:first-child").find("img").attr('alt'));   
                        $("#play_text li:first-child").css({'background-image':'url(themes/index/images/1.png)','color':'#ffffff'});   
                        $("#play_info").click(function(){window.open($("#play_list a:first-child").attr('href'), "_blank")});   
                        $("#play_text li").click(function() {   
                            var i = $(this).index();   
                            n = i;   
                            if (i >= count) return;   
                            $("#play_info").html($("#play_list a").eq(i).find("img").attr('alt'));   
                            $("#play_info").unbind().click(function(){window.open($("#play_list a").eq(i).attr('href'), "_blank")})   
                            $("#play_list a").filter(":visible").fadeOut(1000).parent().children().eq(i).fadeIn(500);   
                            $(this).css({'background-image':'url(themes/index/images/1.png)','color':'#ffffff'}).siblings().css({'background-image':'url(themes/index/images/2.png)','color':'#fff'});   
                        });   
                        t = setInterval("showAuto()", 5000);   
                        $("#play").hover(function(){clearInterval(t)}, function(){t = setInterval("showAuto()", 5000);});   
                    })   
                      
                    function showAuto()   
                    {   
                        n = n >= (count - 1) ? 0 : n + 1;   
                        $("#play_text li").eq(n).trigger('click');   
                    }   
                    </script>
            <style type="text/css">      
                     #play img {border:0px}   
                     #play {width:324px;height:136px;overflow:hidden;margin:0px 0 0 0px;}   
                     #play_info{position:absolute;margin-top:141px;padding:8px 0 0 20px;height:42px;width:270px;color:#fff;z-index:1001;cursor:pointer}   
                     #play_info b{font-size:14px;display:block}   
                     #play_bg {position:absolute;background-color:#000;margin-top:314px;height:25px;width:1000px;filter: Alpha(Opacity=70);opacity:0.7;z-index:1000}   
                     #play_text {position:absolute;margin:20px 0 0 450px;height:25px;width:127px;z-index:1002}   
                     #play_text ul {list-style-type:none; width:127px;height:50px;display:block;padding-top:1px;_padding-top:0px;filter: Alpha(Opacity=80);opacity:0.8;}   
                     #play_text ul li {width:12px;height:12px;float:left;line-height:20px;display:block;color:#FFF;text-align:center;margin:1px;cursor:pointer;font-family:"Courier New";background:url('themes/index/images/2.png') no-repeat;}   
                     #play_list a{display:block;width:324px;height:136px;position:absolute;overflow:hidden}   
                    </style>
