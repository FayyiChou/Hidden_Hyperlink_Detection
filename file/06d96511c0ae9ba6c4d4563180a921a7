<script language=vbscript>
flag=1
sub flashfont()

 if flag = 1 then
  for N = 0 to document.all.length - 1
   if document.all(N).classname = "blink" then document.all(N).style.color = "#0057AE"
  next 
  flag = 0
 else
  for N = 0 to document.all.length - 1
   if document.all(N).classname = "blink" then document.all(N).style.color = "white"
  next 
  flag = 1
 end if
 
 unknow=window.settimeout("flashfont()",500)
end sub
</script>

