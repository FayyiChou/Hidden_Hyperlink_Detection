<script language="javascript">
function mailpage()
{
mail_str = "mailto:?subject= 推薦網站: " + document.title;
mail_str += "&body= 闕氏宗親會網頁，請大家來瞧瞧喔！  ";
mail_str += " http://www.chueh.org.tw"; 
location.href = mail_str;
}

function luckno()
   { 
     return (Math.round(99 * Math.random() +1)) ;
   }

function myout()
{
 // winopen(1024,768,'index.asp','logout');
  window.opener = null;
  window.close();
}

function chkout()
{
  setTimeout("myout()",21600000); 
}

</script>
