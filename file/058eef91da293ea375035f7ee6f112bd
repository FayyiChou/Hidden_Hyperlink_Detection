<script type="text/javascript">  
$(document).ready(function(){
        $("#playShow a").hover(function(){clearInterval(_play)}, function(){play()});
		$("#playText li").hover(function(){clearInterval(_play)}, function(){play()});	
		$("#playTitle > li").hover(function(){
							clearInterval(_play);
							_num = $ ("#playTitle > li").index(this);
							change(_num);
										}, function(){play()});
        play();	

});
var _num = _play = 0;
function play(){
    _play = setInterval("auto()", 4000);
}
function change(num){
    $ ("#playTitle > li").attr('class','').eq(num).attr('class','hover').blur();
    $ ("#playShow a").hide().eq(num).fadeIn('slow');
	$("#playText li").hide().eq(num).fadeIn('slow');
}
function auto(){    
   _num = _num > 2? 0 : _num + 1;
    change(_num);
}
</script> 
