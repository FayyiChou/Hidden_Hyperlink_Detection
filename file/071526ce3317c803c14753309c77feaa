      <script type="text/javascript">
    checkUrl();
    /*头部搜索*/
    $('.seach-select').hover(function () {
        $(this).find("li").show();
    }, function () {
        $(this).find("li:last-child").hide();
    });
    $('#searchBox > li').click(function () {
        var selected = $.trim($(this).find('span').html());
        var currentType = $.trim($(this).siblings().find('span').html());
        $('#searchBox').find("li:first-child > span").html(selected);
        $('#searchBox').find("li:last-child > span").html(currentType);
        $('#searchBox').find("li:first-child").show();
        $('#searchBox').find("li:last-child").hide();

    });
    //function globelQuery(e) {
    //    if (!e)
    //        e = window.event;
    //    if ((e.keyCode || e.which) == 13) {
    //        alert(2);
    //       ;
    //    }
    //} 
    function btnSearch_Click() {
        var keywords = $.trim($("#txtSearchKeywords").val());
        if (keywords == "") {
            alert("请输入您要找的内容！");
            $("txtSearchKeywords").focus();
            return;
        }

        var currentType = $.trim($("#searchBox").find("li:first-child > span").html());

        if (currentType == "课程") {
            window.open("/search/Lessons.aspx?kw=" + escape(keywords) + "&p="+4);
        }
        else if (currentType == "资讯") {
            window.open("/search/Informations.aspx?kw=" + escape(keywords) + "&p="+4);
        }
}

function txtSearchKeywords_KeyPress() {
    if (event.keyCode == 13) {
        event.keyCode = 0;
        btnSearch_Click();
    }
}
    
    /*添加百度统计：
    页面需求：在http://www.wenduedu.com中全部属于资讯页面的部分中添加代码。
    url:http://172.168.254.231/Lists/List2/DispForm.aspx?ID=2938&ContentTypeId=0x0108002A0A09C6C43213428A93CF97462B925A
    检测url*/
    function checkUrl() {  
        var path='/yszg/142548.html';
    var strReg ="/.+/\\d+\\.html";
        var re = new RegExp(strReg);  
        if(re.test(path)) { 
         
            var _hmt = _hmt || [];
            (function() {
                var hm = document.createElement("script");
                hm.src = "https://hm.baidu.com/hm.js?8190f36aa73dbb14e7cc095acf0259cd" ;
                var s = document.getElementsByTagName("script")[0]; 
                s.parentNode.insertBefore(hm, s);
            })();
        } else { 
            return false; 
        }  
    }  

</script> 
