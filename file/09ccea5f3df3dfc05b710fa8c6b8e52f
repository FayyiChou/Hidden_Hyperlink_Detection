</div><SCRIPT> 
  var rollspeed=60
  rolllink2.innerHTML=rolllink1.innerHTML 
  function Marquee(){ 
  if(rolllink2.offsetWidth-rolllink.scrollLeft<=0) 
  rolllink.scrollLeft-=rolllink1.offsetWidth 
  else{ 
  rolllink.scrollLeft++ 
  } 
  } 
  var MyMar1=setInterval(Marquee,rollspeed) 
  rolllink.onmouseover=function() {clearInterval(MyMar1)} 
  rolllink.onmouseout=function() {MyMar1=setInterval(Marquee,rollspeed)} 
                                     </SCRIPT> </td>
