 <script language="javascript">
	function ftab(f,id,max)
	{
	   var max = arguments[2] ? arguments[2] : 4;
	   for(i=0;i<max;i++){
	      if(i==id){
		      document.getElementById("tab"+f+"_"+id).className="hover";
			  document.getElementById("con"+f+"_"+id).style.display="block";
		  }
		  else{
		      document.getElementById("tab"+f+"_"+i).className="";
			  document.getElementById("con"+f+"_"+i).style.display="none";
		  }
	   }
	}
	</script>
