<script type="text/javascript">

function addOption (citynumber) {

   if (citynumber!='0' && citynumber!='1' && citynumber!='2') {

      if (document.getElementById('locationid').checked) {
         document.getElementById("categoryid").disabled = false;
         document.getElementById("plantingid").disabled = false;
      }
      else{
         document.getElementById("categoryid").disabled = true;
         document.getElementById("plantingid").disabled = true;
      }

      var newOpt1 = document.createElement ('option');
      newOpt1.value = 'ëþáàÿ';
      newOpt1.text = 'ëþáàÿ';
      newOpt1.selected = true;
      document.getElementById ('categoryid').options.add (newOpt1);

      var newOpt2 = document.createElement ('option');
      newOpt2.value = 'ëþáàÿ';
      newOpt2.text = 'ëþáàÿ';
      newOpt2.selected = true;
      document.getElementById ('plantingid').options.add (newOpt2);

   }
   else{

      if (document.getElementById('locationid').checked) {
         document.getElementById("categoryid").disabled = true;
         document.getElementById("plantingid").disabled = true;
      }
      else{
         document.getElementById("categoryid").disabled = false;
         document.getElementById("plantingid").disabled = false;
      }
      objSel1 = document.search.category;

      for (var i=objSel1.options.length-1; i >= 4; i--) {
         objSel1.remove(i);
      }

      objSel2 = document.search.planting;

      for (var o=objSel2.options.length-1; o >= 6; o--) {
         objSel2.remove(o);
      }

   }   
}

