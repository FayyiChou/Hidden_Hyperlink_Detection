function doSearch(){
 /*    if(!$("#search_input").val()){
        $("#search_input").val($("#search_input").attr("systemValue"));
    } */

    var url="/ser/search.action?key="+$("#search_input").val()+"&page=1&serviceSortCode=default&serviceSortType=desc";
    window.location.href = url;
}



