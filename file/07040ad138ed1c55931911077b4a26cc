                url : "/validate/isLogin.action?v="+Math.random(),
                dataType : 'json',
                success : function(data) {
                    // 判断是否登录，如果没有登录跳转到登录页面
                    if (!data.success) {//未登录
                        window.location.href = "http://passport.jd.com/new/login.aspx?ReturnUrl="+ window.location.href;
                        return;
