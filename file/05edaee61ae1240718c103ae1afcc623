	function getSubMenuHoverCsslayer39CDBF25D6ADA78EB9039DB4502A313E(css_pro,regex,isLi)
	{
				var navStyle = $.trim($("#layer39CDBF25D6ADA78EB9039DB4502A313E").data("datastys_"));
				if(navStyle.length > 0)
		{
			var patt1 =new RegExp(regex,'i');
			var tmp = patt1.exec($.trim(navStyle));
			if(tmp)
			{
				return $.trim((tmp[0].match(/{[^:]+:[^;]+/)[0]).match(/:[^;]+/)[0].replace(':',''));
			}
		}
						navStyle = $.trim($("#layer39CDBF25D6ADA78EB9039DB4502A313E").data("datasty_"));
			 
		
		if(navStyle.length > 0)
		{
			var patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s+a\\s*\\s*{[^}]+}",'i');
			if(isLi)
				patt1 = new RegExp("#nav_layer[0-9|a-z|A-Z]+\\s+ul\\s+li\\s*\\s*{[^}]+}",'i');
			var tmp = patt1.exec(navStyle);
			
			if(tmp)
			{
				var tmp1 = tmp[0].match(/{[^}]+}/)[0];
				var patt2 = new RegExp(css_pro+"\\s*:\\s*[^;]+;",'i');
				tmp = patt2.exec(tmp1);
				if(tmp) return $.trim(tmp[0].replace(/[^:]+:/,'').replace(';',''));
			}
		}
		return false;
	}
	
	$("#layer39CDBF25D6ADA78EB9039DB4502A313E li").hover(function(){ 			
			var thisleft=$(this).offset().left;
			var thiswidth=$(this).outerWidth();	
			//父中心点坐标
			var thiswidthcenter=0;
			//子中心点坐标
			var thischildwidthcenter=0;			
			var orgleft = $("#layer39CDBF25D6ADA78EB9039DB4502A313E").offset().left;
			var orgright = $("#layer39CDBF25D6ADA78EB9039DB4502A313E").outerWidth();
			orgright=orgright+orgleft;
			thiswidthcenter=thisleft+thiswidth/2;

			//计算子菜单总宽度
			var childlidom=$(this).children('ul').children('li');
			var childliwidth=0;
			childlidom.each(function(){
				childliwidth=childliwidth+$(this).outerWidth();			
			});
			
			thischildwidthcenter=childliwidth/2;			
			var chavalue=thiswidthcenter-thischildwidthcenter;
			var charightvalue=thiswidthcenter+thischildwidthcenter;			
			
			var ulwidth=$("#layer39CDBF25D6ADA78EB9039DB4502A313E").width();
			if(chavalue<orgleft && charightvalue>orgright){ 
				//超出边界不做处理			
			}else{
				if(chavalue>orgleft && charightvalue<orgright){
					$(this).children("ul:eq(0)").css("padding-left", (chavalue-orgleft)+"px");				
					if(ulwidth>0){
						$(this).children("ul:eq(0)").css("width", (ulwidth-chavalue+orgleft)+"px");
					}	
				}else{
					if(chavalue>orgleft && charightvalue>orgright){
						$(this).children("ul:eq(0)").css("padding-left", (ulwidth-childliwidth)+"px");
						if(ulwidth>0){
							if(childliwidth>ulwidth) childliwidth=ulwidth;
							$(this).children("ul:eq(0)").css("width", (childliwidth)+"px");
						}
					}
				}		
			}
			
			if(!$(this).hasClass('wp_subtop'))
			{
				$(this).css('background-image',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-image',3));
				$(this).css('background-repeat',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-repeat',3));
				$(this).css('background-color',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-color',3));
				$(this).css('background-position',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-position',3));
			}
			if($(this).children('ul').length > 0)
			{
				$(this).children('ul').css('background-image',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-image',2));
				$(this).children('ul').css('background-repeat',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-repeat',2));
				$(this).children('ul').css('background-color',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-color',2));
				$(this).children('ul').css('background-position',layer39CDBF25D6ADA78EB9039DB4502A313E_getSubMenuHoverCss('background-position',2));

				$(this).children('ul').children('li').css({'background-image':'none','background-color':'transparent'});
			}
			var type=$("#layer39CDBF25D6ADA78EB9039DB4502A313E").find('.wp-new_navigation_content').attr('type');
			if(type==2){
				var self = $(this);
				var pos = 0 ;
				var loops = 0;
				$('#nav_layer39CDBF25D6ADA78EB9039DB4502A313E').find('li').each(function(){
					if(loops == 1) return true;
					if(self.html() == $(this).html()){
						loops = 1;
						return true;
					}else{
						pos = pos + $(this).outerWidth();
					}	
					 
				})
				 
				$("#layer39CDBF25D6ADA78EB9039DB4502A313E").find('.ddli').hide();
				var this_width = $('#nav_layer39CDBF25D6ADA78EB9039DB4502A313E').outerWidth();
				var thisul_left = $('#nav_layer39CDBF25D6ADA78EB9039DB4502A313E').css("padding-left");
				thisul_left = parseInt(thisul_left);
				$(this).children('.ddli').outerWidth(this_width).css("margin-left","-"+(thisul_left+pos)+"px");
				$(this).children('.ddli').eq(0).slideDown();
			}			
 
	},function(){ 
			if(!$(this).hasClass('wp_subtop'))
			$(this).css({'background-color':'transparent','background-image':'none'});
			$(this).children("ul:eq(0)").css("left", "-99999px").hide(); 
							var type=$("#layer39CDBF25D6ADA78EB9039DB4502A313E").find('.wp-new_navigation_content').attr('type');
			if(type==2){
				$("#layer39CDBF25D6ADA78EB9039DB4502A313E").find('.ddli').slideUp();
			}
	});

