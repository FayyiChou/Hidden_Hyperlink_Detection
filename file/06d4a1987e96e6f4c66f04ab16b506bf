function goTop()
 {
 jQuery(window).scroll(function(e) {
if(jQuery(window).scrollTop()>100) jQuery(".returnTop").show();
else jQuery(".returnTop").hide();
 });
 };
 jQuery(function($){
 jQuery(".returnTop").click(function(e) {
jQuery('body,html').scrollTop(0);
 });
 goTop();
 });
