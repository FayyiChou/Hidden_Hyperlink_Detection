	  <!--------------------------------------------lee 5.27 城市搜索--------------------------------------------------------------------->
       <script type="text/javascript">
           
           $(document).ready(function(){
                $('.search-city-submit').click(function(){
                    if($('#searchCity').val != '请输入城市名' && $('#searchCity').val() != ''){
                       var  seachVal =$('#searchCity').val();
                       //得到所有的city-right里的a城市名
                       var allA = $('.city-right').find('a');
  
                       for(var i=0;i<allA.length;i++){
                             if(allA[i].innerHTML.substr(0,1) == seachVal.substr(0,1)){
                                 
                                 if(allA[i].parentElement.parentElement.previousSibling.nodeType == 3){
        
                                     var caseL=allA[i].parentElement.parentElement.previousElementSibling.firstChild.innerHTML;
                                 }else {
                                     
                                     var caseL=allA[i].parentElement.parentElement.previousSibling.firstChild.innerHTML;
                                 }                                 
                             }
                       }
                       var allAs = $('#s-engsh').find('a');
                        for(var i=0;i<allAs.length;i++){
                             var numHeight = 0;
                                    
                             if(allAs[i].innerHTML == caseL ){
                                   var caseI  = i;                                        
                             }                                  
                         }
                        for(var i = 0; i < caseI;i++){
                           numHeight += $("#cityC div.city-a").eq(i).height(); 
                        }
                        $("#cityC").scrollTop(numHeight);
                     }
                 });
           });
          
       </script>
     </div>
    <!--搜索--> 
    <div class="search">
      <div class="search-locate">
        <div class="search-locate-head"><span>定位</span></div>
		<script type='text/javascript'>
			function trim(str){   
				str = str.replace(/^(\s|\u00A0)+/,'');   
				for(var i=str.length-1; i>=0; i--){   
					if(/\S/.test(str.charAt(i))){   
						str = str.substring(0, i+1);   
						break;   
					}   
				}   
				return str;   
			}  
			function check_search_site_form(){
				var key_values = trim(document.searchForm.keywords.value);
				if(key_values=='' || key_values=='请输入地址 定位您周边店铺位置'){
					alert('请输入关键词搜索店铺!');
					document.searchForm.keywords.focus();
					return false;
				}
				return true;
			}
		</script>
			<form name='searchForm' action="http://stione.cimimall.cn/search_site/index" method="post" style='padding:0px;margin:0px;'>
				<input type="text" value="请输入地址 定位您周边店铺位置" id="searchLocate" name="keywords" class="input-locate searchLocate" style=""/>
				<input type="submit" onclick="return check_search_site_form();" value="搜索" class="locate-bg"/>
			</form>
        <div class="search-locate-explain"><span>定位：输入地址或小区名-定位-周边店铺一目了然</span></div>
      </div>
      <div class="search-right">
      <div id='search_godds_cf' class="search-right-head-1" onclick="changeGoods();"><a href="javascript:void(0);" onclick="changeGoods();">商品</a></div>
      <div id='search_shops_cf' class="search-right-head-2 search-right-head" onclick="changeShop();"><a href="javascript:void(0);" onclick="changeShop();">店铺 </a></div>   
		<!--范超 5.6 切换店铺/商铺搜索框内容---> <!-- Lee 5.30 完善特效 -->
		<script type="text/javascript">
			function go_searchFormTwo(){
				var searchformtwo_name_cf = trim(document.searchFormTwo.searchformtwo_name_cf.value);
				if(searchformtwo_name_cf=='' || searchformtwo_name_cf=='请输入店铺名称或行业分类' || searchformtwo_name_cf=='请输入商品名称或商品分类'){
					alert('请输入关键词搜索!');
					document.searchFormTwo.searchformtwo_name_cf.focus();
					return false;
				}
				return true;
			}
			$(document).ready(function(){
				var href_url = window.location.href;
				if(href_url.indexOf("/search_goods") >= 0){
					changeGoods();
				}
			})
			var searchGoods = '请输入商品名称或商品分类';
			var searchShop = '请输入店铺名称或行业分类';
			function changeGoods() {
				$("#searchformtwo_name_cf").val(searchGoods);                              
				$("#search_shops_cf").removeClass('search-right-head');
				$("#search_godds_cf").addClass('search-right-head');
				document.searchFormTwo.action='/search_site/search_goods';//Lee 6.02 搜索的地址变更         
			}
			function changeShop() {
				$("#searchformtwo_name_cf").val(searchShop);
				$("#search_godds_cf").removeClass('search-right-head');
				$("#search_shops_cf").addClass('search-right-head');                       
				document.searchFormTwo.action='/search_site/search_shop';//lee 6.02 搜索的地址变更         
			}
			function clearContent(obj) {
				$("#searchformtwo_name_cf").attr("style","color:#000;");
				if($(obj).val() == '请输入商品名称或商品分类' || $(obj).val() == '请输入店铺名称或行业分类'){
					$(obj).val('');
				}
			}
            function content(obj){
				if($(obj).val()==''){
					if($("#search_godds_cf").attr('class') == 'search-right-head-1 search-right-head'){
						$(obj).val(searchGoods);
					}else {
						 $(obj).val(searchShop);
					}
				}
			}
			// Lee   ajax cookie 0607 --------------------------------------------------------------------------
			$(document).ready(function(){
				
				$('.city-right li').each(function(e){
					$(this).click(function(e){
				
						var index = $(this).index();
						var city_id = $(this).find('a').attr('date-id');
						var city_name = $(this).find('a').html();
						//alert(city_id + city_name);
						
						if(city_id){
							
							$.post("http://stione.cimimall.cn/index/get_city_cookie",{city_id:city_id,city_name:city_name},function(){
								location.reload();
							})
						}
					})
				})
				$("#searchLocate").click(function(){
					$("#searchLocate").attr("style","color:#000;");
				})
			})
		</script>
        <form name='searchFormTwo' action="/search_site/search_shop" method="post" style='padding:0px;margin:0px;'>
		  <input type="hidden" name="search_type_two" id="search_type_two" value="1" />	
          <input type="text" onblur="content(this)" onfocus="clearContent(this);" id="searchformtwo_name_cf" name="searchformtwo_name_cf"  
		  value="请输入店铺名称或行业分类" id="search_shop_or_shiti" class="input-search search_shop_or_shiti" style=""/>
          <input type="submit" onclick="return go_searchFormTwo();" value="搜索" class="search-bg"/>
		</form>
        
      </div>
      
