					<script type="text/javascript">
						$(document).ready(function(){
							$('.hot-city li').each(function(e){
								$(this).click(function(e){
									var index = $(this).index();
									var city_id = $('.hot-city a:eq('+index+')').attr("date-id");
									var city_name = $('.hot-city a:eq('+index+')').html();
									
									if(city_id){
										$.post("http://stione.cimimall.cn/index/get_city_cookie",{city_id:city_id,city_name:city_name},function(){
												location.reload();
										})
									}
								})
							});
							$('#nowcity').click(function(){
								
									var city_id = $(this).attr("date-id");
									var city_name = $(this).html().replace('市','');
									if(city_id){
										$.post("http://stione.cimimall.cn/index/get_city_cookie",{city_id:city_id,city_name:city_name},function(){
												location.reload();
										})
									}
							});
						
						})
					</script>
