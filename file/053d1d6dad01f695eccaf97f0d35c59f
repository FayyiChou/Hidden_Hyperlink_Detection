<script type="text/javascript">
$(document).ready(function(){

	var pre_sc = gc('n_ow_sc');
    sc("p_re_sc",pre_sc,0,'cimimall.cn');
    var pathname = window.location.pathname;
	sc("n_ow_sc",encodeURIComponent(pathname),0,'cimimall.cn');
	//搜索标题切换 
	$("#search_godds_cf").click(function(){
		$("#searchformtwo_name_cf").attr("style","");
		document.searchFormTwo.action='/search_site/search_goods';
		$(this).addClass("search-right-head");
		$("#search_shops_cf").attr('className','search-right-head-2');
		
	});
	
	$("#search_shops_cf").click(function(){
		$("#searchformtwo_name_cf").attr("style","");
		document.searchFormTwo.action='/search_site/search_shop';
		$(this).addClass("search-right-head");
		$("#search_godds_cf").attr('className','search-right-head-1');
		
	});
		$("#searchformtwo_name_cf").focus(function(){
            //console.log($("#search_godds_cf").attr('class'));
            
		if($(this).val()=='请输入店铺名称或行业分类'){
			$(this).val('');
		}
	});
	
	$("#searchformtwo_name_cf").blur(function(){
            // 5.30 lee 探索切换
		if($(this).val()==''){
                    if($("#search_godds_cf").attr('class') == 'search-right-head-1 search-right-head'){
			$(this).val('请输入商品名称或商品分类');
                    }else {
                        $(this).val('请输入店铺名称或行业分类');
                    }
		}
	});

	/*鼠标移过，左右按钮显示*/
	$(".mall_banner").hover(function(){
		$(this).find(".prev,.next").fadeTo("show",0.1);
	},function(){
		$(this).find(".prev,.next").hide();
	})
	/*鼠标移过某个按钮 高亮显示*/
	$(".prev,.next").hover(function(){
		$(this).fadeTo("show",0.7);
	},function(){
		$(this).fadeTo("show",0.1);
	})
	$(".mall_banner").slide({ titCell:".num ul" , mainCell:".buypic" , effect:"fold", autoPlay:true, delayTime:700 , autoPage:true });

	$("#taggle a").mouseover(function(){
		var index = $(this).index();
		$("#tagglef ul").attr("style","display:none").eq(index).attr("style","display:block");
		$(this).siblings().attr("style","background-color:#f5f5f5;color:#a7a7a7");
		$(this).attr("style","border-right:none;border-bottom:none;background-color:#fff;color:#595959");
	});

});
	

</script>
