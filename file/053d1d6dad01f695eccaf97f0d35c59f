$(document).ready(function(){
	$("#descimg").click(function(){
		$("#descindex").addClass("descdisplay");
		$(this).addClass("descdisplay");
	});
	$("#descindex").click(function(){
		$(this).addClass("descdisplay");
		$("#descimg").addClass("descdisplay");
	});
	
	setTimeout(function(){
		$("#descindex").addClass("descdisplay");
		$("#descimg").addClass("descdisplay");
 	},5000)
