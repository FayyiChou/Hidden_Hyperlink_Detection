<script type="text/javascript"> 
	// 當網頁載入完
	Ext.EventManager.on(window, 'load', function(){
		var $win = Ext.get(document),
			$ad = Ext.get('abgne_float_ad').setOpacity(0).show(),	// 讓廣告區塊變透明且顯示出來
			_width = $ad.getWidth(),
			_height = $ad.getHeight(),
			_diffY = 200, _diffX = 10,	// 距離右及下方邊距
			_moveSpeed = 800;	// 移動的速度
		
		// 先把 #abgne_float_ad 移動到定點
		$ad.setStyle({
			top: Ext.lib.Dom.getDocumentHeight() + 'px',
			left: (Ext.lib.Dom.getViewWidth() - _width - _diffX)  + 'px',
			opacity: 1
		});
 
		// 幫網頁加上 scroll 及 resize 事件
		Ext.EventManager.on(window, {
			scroll: moveAd,
			resize: moveAd
		});
 
		function moveAd(){
			// 控制 #abgne_float_ad 的移動
			$ad.stopFx().animate({
				top: {to: $win.getScroll().top + Ext.lib.Dom.getViewHeight() - _height - _diffY},
				left: {to: $win.getScroll().left + Ext.lib.Dom.getViewWidth() - _width - _diffX}
			}, _moveSpeed / 1000);
		}
 
		// 關閉廣告
		Ext.select('#abgne_float_ad .abgne_close_ad').on('click', function(){
			$ad.hide();
		});
 
		moveAd();// 觸發一次 scroll()
	});
</script>
