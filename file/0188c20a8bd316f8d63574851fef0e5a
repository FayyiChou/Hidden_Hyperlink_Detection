                <script type="text/javascript">
				
					function search(){
						
					   num = $('#selec').val();	
					   
					   if(num==0){
						   site();
					   }else {
						   google();
						   
						   }
						
					}
				
					function google(){
					
					
					$('.site').css('disabled','enable');
					$("#pesquisa_noticia").attr("action","http://www.google.com.br/cse");
					$("#pesquisa_noticia").attr("target","_blank");
					$("#cmp").attr("name","q");
					
				
				
					}
					function site(){
					$('.site').css('disabled','disabled');
					$("#pesquisa_noticia").attr("action","/busca");
					$("#pesquisa_noticia").attr("target","_self");
					$("#cmp").attr("name","pesq");
					}
					</script>
                                
                
                
                
				
