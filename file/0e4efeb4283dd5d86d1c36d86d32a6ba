<script>
$(document).ready(function(){
	$(".help-nav li a").click(function(){
	      if($(this).hasClass("help-off")){
                $(".help-on").removeClass("help-on").addClass("help-off");
                $(this).addClass("help-on").removeClass("help-off");
            }	
     });
	$(".cont-left li a").click(function(){
	      if($(this).hasClass("left-off")){
                $(".left-on").removeClass("left-on").addClass("left-off");
                $(this).addClass("left-on").removeClass("left-off");
            }	
     });
	
	
	$('.textarea').click(function(){
		if($.trim($(this).val()) == '您对我们的产品有什么改进的建议'){
			$(this).val('');
			$(this).css('color','#333');
		}
	});
	$('.textarea').blur(function(){
		if($.trim($(this).val()) == ''){
			$(this).val('您对我们的产品有什么改进的建议');
			$(this).css('color','#9f9f9f');
		}
	});
	$('.input-text').click(function(){
		if($.trim($(this).val()) == 'QQ或者Email'){
			$(this).val('');
			$(this).css('color','#333');
		}
	});
	$('.input-text').blur(function(){
		if($.trim($(this).val()) == ''){
			$(this).val('QQ或者Email');
			$(this).css('color','#9f9f9f');
		}
	});
	$(".submit").click(function(){
		var texts  = $('textarea[name=text]').val();
		var conects = $('input[name=conect]').val(	);

		if(texts == "" || texts == "您对我们的产品有什么改进的建议"){
			alert('请填写您的意见后再进行提交');
		}else if(conects == "" || conects == 'QQ或者Email'){
			alert('请留下您的联系方式（QQ/Email/电话），以便我们与您联系');
		}else{
			$.ajax({
					url: "/opinion.php",
					type:"post",
					data:{
						text:texts,
						conect:conects,
						action: "submit"
					},
					dataType:"json",
					success: function(msg){
						alert('信息已成功提交,非常感谢您的支持，我们会尽快处理您所提出的问题。');
						$('textarea[name=text]').val('');
						$('input[name=conect]').val('');
					}
				});
		}
		
		return false;
	})
})

function hasClass(ele,cls) { 
return ele.className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)')); 
} 
function index(){
  var obox=document.getElementById("box"); 
  var lis=obox.getElementsByTagName("li"); 
  for(var index=0;index<lis.length;index++){ 
  if(hasClass(lis[index], "on")){
	  break;	  
   }
  } 
  return index;
}
function changeDiv(index){
	var index = index();
	 var url=document.getElementById("aboutus");
	 url.href = url+"?var="+index;
}
</script>
