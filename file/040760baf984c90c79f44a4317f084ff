	<script type="text/javascript" src="http://js4.jobui.com/js/jquery/jquery-1.7.2.min.js--template_1/js/plugin/goBackTop.js,Cookie.js--template_1/js/mobile/common.js--template_1/js/mobile/tipsList.js--template_1/js/mobile/lenovoPrompt.js--v617.51.js"></script>
	<script type="text/javascript" charset="utf-8" src="http://js4.jobui.com/template_1___js___mobile___people___index.js--template_1___js___plugin___Cookie.js--v617.50.js"></script>
	    <script type="text/javascript">
    
    $(".j-people-fresh-resume").live("click", function(){
    	$.ajax({
    		type: "post",
    		url: "/process/",
    		data: {
    			formAction: "user_profile_updateLoginDay"
    		},
    		dataType: "json",
    		success: function(result){
    			if(result["result"] > 0){
    				alert("刷新成功");
    				$(".j-updateTimeText").text(result["param"]["updateTime"]);
    			}else if(result["result"] == -11){
    				alert("刷新次数频繁");
    			}else{
    				alert("刷新失败");
    			}
    		}
    	})
    });
    
    if(document.cookie.indexOf("isloginStatus") == -1){
    	var html = '<a href="http://m.jobui.com/people/login/?source='+window.location.href+'" class="check-infomation j-check-infomation">查看联系方式</a>';
    	$(".j-zhuye").after(html);
    }

	/**
	 * [popupPoints 消耗积分弹框]
	 * @param  {string} html [弹框内容]
	 * @return {undefined}     
	 */
	function popupPoints (html) {
		$("body").append($(html));
		var top = $(".j-mobilePoint-box").offset().top;
		$(".j-mobilePoint-box").css({
			top : $(document).scrollTop() + top + "px",
		});
	};
	/**
	 * [consumePointsSucc 成功消费积分弹框]
	 * @param  {string} html  [弹框内容]
	 * @param  {number} point [积分数]
	 * @return {undefined}      
	 */
	function consumePointsSucc (html , point , profileID , profileKeyID) {
		$.ajax({
			type : "post",
			url : "/process/",
			data : {
				point : point,
				profileID : profileID,
				profileKeyID : profileKeyID,
				formAction : "user_profile_checkByPoint"
			},
			dataType : "json",
			success : function (result) {
				if (result['result'] > 0) {
					$("body").append($(html));
					var top = $(".j-points-consumeBox").offset().top;
					$(".j-points-consumeBox").css({
						top : $(document).scrollTop() + top + "px",
					});
					$('.j-zhuye').before($(result['html']));
					$('.j-check-mobilePoint').css('display' , 'none');
				} else {
					alert('网络错误,请稍后重试');
				}
			}
		});
	};
	$(".j-points-button").live("click" , function () {
		var $this = $(this);
		$this.parent().remove();
		var html = '<div class="j-points-consumeBox" style="background-color:#fff;text-align:center;padding:20px;position:absolute;top:50%;margin-top:-44px;z-index:200;width:100%;box-sizing:border-box">'+
						'<div class="fs20 fwb box">购买成功</div>'+
						'<button class="j-points-succBtn" style="margin-right:10px;color:#fff;background-color:#0cc;padding:8px 20px;border-radius:5px;border:none;cursor:pointer;font-size:18px;width:100%">完成</button>'+
					'</div>';
		var point = praseFloat($(".j-total-points").text()) - 1;
		var profileID = $("#profilePage").attr("data-followedUserID");
		var profileKeyID = $(".j-mobilePoint-btn").attr("data-profileKeyID");
		consumePointsSucc(html , point , profileID , profileKeyID);
	});
	
	//引导用户登录
	$(".j-mobilePoint-btn").live("click", function(){
		if(document.cookie.indexOf("isloginStatus") == -1){

		} else {
			var point = praseFloat($(this).attr("data-point"));
			var pointText = point ? "查看联系方式，需消耗 1 积分" : "积分不足，无法查看联系方式";
			var pointClassName = point ? "j-points-button" : "j-nopoints-button";
			var html = '<div class="j-mobilePoint-box" style="background-color:#fff;text-align:center;padding:40px 20px 20px;position:absolute;top:50%;margin-top:-60px;z-index:200;width:100%;box-sizing:border-box">'+
							'<div style="font-size:18px;font-weight:bold;margin-bottom:20px;">'+pointText+'</div>'+
							'<div style="margin-bottom:20px;font-size:16px;">你目前持有 <em style="color:#f60" class="j-total-points">'+point+'</em> 积分</div>'+
							'<button class='+pointClassName+' style="margin-right:10px;color:#fff;background-color:#0cc;padding:8px 20px;border-radius:5px;border:none;cursor:pointer;font-size:18px;width:100%">确定</button>'+
							'<span class="j-mobilePoint-delete" style="position:absolute;right:15px;top:5px;color:#999;font-size:20px">X</span>'+
						'</div>'+
						'<div class="j-mobilePoint-mask" style="position:fixed;top:0;left:0;right:0;bottom:0;background-color:rgba(0,0,0,.2)"></div>';
			if (point > 0) {
				popupPoints(html);
			} else {
				popupPoints(html);
			}
		}
	});
	// 点击按钮关闭弹框
	$(".j-mobilePoint-delete , .j-points-succBtn , .j-nopoints-button").live("click" , function () {
		var $this = $(this);
		$this.parent().remove();
		$(".j-mobilePoint-mask").remove();
	});

	// HR查看用户简历    start
	if(document.cookie.indexOf("isloginStatus") != -1){
		if($.cookie("jobui_userProfileVisitor") == 1||$.cookie("jobui_userProfileVisitor")==null){
			var profileUserID = $("#profilePage").attr("data-followedUserID");
			$.ajax({
				type : 'post',
				url : '/process/',
				data : {
					formAction : 'user_profile_userProfileVisitor',
					profileUserID : profileUserID
				},
				dataType : 'json',
				success : function(result) {
					if(result['result']>0){
						$(".j-mobilePoint-btn").attr("data-profileKeyID" , result['param']['profileKeyID']);	
					}
				}
			});
		}
	}
	// HR查看用户简历    end
    </script> 

	 
