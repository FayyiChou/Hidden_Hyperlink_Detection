                function ShowCheckTemplateProductGroup011(src) {
                    var htmla = $(src).attr("id");
                    $(".chkTemplateProductGroup011").removeClass("cur_01");
                    $("#" + htmla).addClass("cur_01");
                    var Index = $(src).attr("id");
                    Index = Index.substring(3, Index.length);
                    var ShowIndex = "#shk" + Index;
                    $(src).parent().parent().parent().children(".nr").hide();
                    $(src).parent().parent().parent().find(ShowIndex).show();
                    $("#hiddenTemplateProductGroup011").val($("#" + htmla).attr("href"));
                }
                function ClickTemplateProductGroup011() {
                    window.open($("#hiddenTemplateProductGroup011").val());
                }
                var CheckIndexTemplateProductGroup011 = "0";
                var SwichIDTemplateProductGroup011 = "#TemplateProductGroup011";
                $("#chkTemplateProductGroup011" + CheckIndexTemplateProductGroup011).addClass("cur_01");
                $("#hiddenTemplateProductGroup011").val($("#chkTemplateProductGroup011" + CheckIndexTemplateProductGroup011).attr("href"));
                $("#shkTemplateProductGroup011" + CheckIndexTemplateProductGroup011).show();
            </script>
