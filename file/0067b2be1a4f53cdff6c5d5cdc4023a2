(function($){
$(function(){
	$(".block1 .div:first").show();
	$(".block1 .tabs a").each(function(index, element) {		
		$(this).click(function(){
			$(".block1 .tabs a").removeClass("now");
			$(this).addClass("now");
	        $(".block1 .div").hide();
			$(".block1 .div").eq(index).fadeIn();
		});
    });
	
	var tabi=0;
	setInterval(function(){
		tabi++;
		if(tabi==3){tabi=0};
		$(".block1 .tabs p").children('a').removeClass("now").end().eq(tabi).children('a').addClass("now");
		$('.block1 .div').eq(tabi).fadeIn().siblings('.div').hide();
	},3000);
	
	$(window).resize(function(){
		//console.log($('#focus,.focus_img').height());
		$('#focus,.focus_img').css('height',$('#focus .bannar_img img').height());
		$('.bannar_text').each(function(index, element) {
			var h=($(this).height()-$(this).children('img').height())/2;
			$(this).children('img').css('margin-top',h);
		});
	});
	
	bScrollt($('.focus_img'));
	bScroll($('.pro_cont'));
	
	$('.bannar_text').each(function(index, element) {
		var h=($(this).height()-$(this).children('img').height())/2;
		$(this).children('img').css('margin-top',h);
	});
	
	var slideImg=0;
	$('.slideBtn').click(function(){
		if(slideImg==0){
			slideImg=1;
			$('.slide_img').animate({height:0},1000,'easeOutQuad');
		}else{
			slideImg=0;
			$('.slide_img').animate({height:'404px'},1000,'easeOutQuad');
		};
	});
	
	setInterval(arrow,1000);
	
	function arrow(){
		$('.arrow').fadeIn().animate({backgroundPosition:'0 0'},{duration:2000}).fadeOut(function(){$(this).css('background-position','0 -76px');});
	};
	
	$('.arrow').click(function(){
		var sTop=$('.block1').position().top;
		$($.browser.webkit?"body":"html").animate({scrollTop:sTop},400);
	});
	
	var timeScroll=null;
	$('html,body').on('mousewheel', function(event, delta) {
		var $this = $(this),
		timeoutId = $this.data('timeoutId');
		if (timeoutId) {
			clearTimeout(timeoutId);
		};
		$this.data('timeoutId', setTimeout(function() {
			if($(window).scrollTop()>=$('.block5').offset().top-200 && !$('.map').hasClass('scrollshow')){
				$('.map').addClass('scrollshow');
				caiyangIn();
			};
			if($(window).scrollTop()<=$('.block4').offset().top+200 && $('.map').hasClass('scrollshow')){
				$('.map').removeClass('scrollshow');
				caiyangOut();
			};
			///
			$this.removeData('timeoutId');
			$this = null;
		}, 400));
		//return false;
	});
	function caiyangOut(){
		$('.map .caiyang').find(':animated').stop(true);
		$('.map .caiyang .img').stop().animate({width:'0',height:'0',left:'60px',top:'60px'},'fast').siblings().fadeOut();;
	};
		
	function caiyangIn(){
		var num=0;
		var times=setInterval(function(){
			caiyangShow(num);
			num++;
			if(num>3){clearInterval(times);};
		},1200);
		
	};
	
	function caiyangShow(num){
		var obj=$('.map .caiyang').eq(num);
		obj=$('.map .caiyang').eq(num);
		
		obj.children('.img').stop().animate({width:'121px',height:'121px',left:0,top:0},function(){
			$(this).siblings('.list').stop().fadeIn('slow');
			$(this).siblings('span').show('fast');
		});
		
/*		var caiyangqu=[
			function(){obj.children('.img').animate({width:'121px',height:'121px',left:0,top:0},'s','easeOutBounce',_takeOne);},
			function(){obj.children('.list').delay(100).fadeIn('slow',_takeOne);},
			function(){obj.children('span').delay(300).show('fast',_takeOne);}
		]
		$('.caiyang').queue('slideList',caiyangqu);
		var _takeOne=function(){
			$('.caiyang').dequeue('slideList');
		};
		_takeOne();  
*/
	};


///
});
window.onload=function(){
	$('#focus,.focus_img').css('height',$('#focus .bannar_img img').height());
};

$(document).ready(function () {
	// 读取window
	$window = $(window);
	// 遍历包含“data-type”的元素，读取并设置Y偏移量、X位置、滚动速度等参数
	$('[data-type]').each(function () {
		$(this).data('offsetY', parseInt($(this).attr('data-offsetY')));
		$(this).data('Xposition', $(this).attr('data-Xposition'));
		$(this).data('speed', $(this).attr('data-speed'));
	});
	// 遍历符合 section[data-type="background"] 选择器的元素
	$('div[data-type="background"]').each(function () {
		// 存储基础变量
		var $self = $(this),
			offsetCoords = $self.offset(),
			topOffset = offsetCoords.top;
		// 当窗口滚动时
		$(window).scroll(function () {
			// 通过计算滚动条高度和窗口高度判断当前元素是否在视野中
			if (($window.scrollTop() + $window.height()) > (topOffset) &&
					((topOffset + $self.height()) > $window.scrollTop())) {
				// 以设定的速度滚动背景
				// 因为我们是向上滚动，所以背景的yPos是负值
				var yPos = $window.scrollTop() / $self.data('speed');
				 //如果此元素有一个Y偏移，将其添加上
				//console.log(yPos);
				if ($self.data('offsetY')) {
					yPos += $self.data('offsetY');
				}
				// 最终的背景位置
				var coords = 'center ' + yPos + 'px';
				// 滚动背景
				$self.css({ backgroundPosition: coords });
				
				// 遍历本节点中的其他元素
/*				$('[data-type="sprite"]', $self).each(function () {
					// 读取data-type="sprite"规则的元素
					var $sprite = $(this);
					// 使用同样的方法进行滚动
					var yPos = -($window.scrollTop() / $sprite.data('speed'));
					var coords = $sprite.data('Xposition') + ' ' + (yPos + $sprite.data('offsetY')) + 'px';
					$sprite.css({ backgroundPosition: coords });
				}); 
				// 遍历需要滚动的视频
				$('[data-type="video"]', $self).each(function () {
					// 读取data-type="video"规则的元素
					var $video = $(this);
					var yPos = -($window.scrollTop() / $video.data('speed'));
					var coords = (yPos + $video.data('offsetY')) + 'px';
					$video.css({ top: coords });
				});
*/				 
			}; // in view
		}); // window scroll
	}); // each data-type
}); // document ready 



setInterval(function(){
		var m=$(".slideBtn");
		if(m.hasClass("click")){
			m.removeClass("click")
		}else{
			m.addClass("click")
		}
	},400);
	
/*轮播器函数*/
function bScroll(sDiv){
	var sUl=sDiv.children('ul');
	var suLi=sUl.children('li');
	var sImg=suLi.find('img');
	
	//sImgWH(sImg);
	if(suLi.length<2){return};
	
	//sUl.children('li:gt(0)').css('left',suLi.width());
	sUl.children('li:eq(0)').fadeIn();
	
	/*创建页签*/
	var tip='<li></li>';
	for(var i=0;i<suLi.length;i++){	sDiv.append(tip)};
	sDiv.children('li').wrapAll('<ol></ol>');
	
	var sOl=sDiv.children('ol');
	var soLi=sOl.children('li');
	var tNow=0;
	var time=null;
	var iNow=0;
	sOl.css('margin-left',-sOl.width()/2+'px');
	soLi.eq(tNow).addClass('cur');
	
	soLi.click(function(){
		var tIndex=$(this).index();
		if(tIndex>tNow){
			$(this).addClass('cur').siblings('li').removeClass('cur');
			//suLi.eq(tNow).stop().animate({left:-suLi.width()});
			suLi.eq(tNow).fadeOut();
			//suLi.eq(tIndex).css('left',suLi.width()).stop().animate({left:0});
			suLi.eq(tIndex).fadeIn();
			}else if(tIndex<tNow){
				$(this).addClass('cur').siblings('li').removeClass('cur');
				//suLi.eq(tNow).stop().animate({left:suLi.width()});
				suLi.eq(tNow).stop().fadeOut();
				//suLi.eq(tIndex).css('left',-suLi.width()).stop().animate({left:0});
				suLi.eq(tIndex).fadeIn();
				};
		tNow=tIndex;
		iNow=tIndex;
		});
	
	time=setInterval(slider,5000);
	
	sDiv.hover(function(){
		clearInterval(time);
	},function(){
		time = setInterval(slider,5000);
	});

	function slider(){
		if(iNow<suLi.length-1){
			//suLi.eq(iNow).stop().animate({left:-suLi.width()});
			iNow++;
			soLi.eq(iNow).addClass('cur').siblings('li').removeClass('cur');
			//suLi.eq(iNow).css('left',suLi.width()).stop().animate({left:0});
			suLi.eq(iNow).stop().fadeIn().siblings('li').fadeOut();
			tNow=iNow;
		}else{
			//suLi.eq(iNow).stop().animate({left:-suLi.width()});
			suLi.eq(iNow).stop().fadeOut();
			iNow=0;
			soLi.eq(iNow).addClass('cur').siblings('li').removeClass('cur');
			//suLi.eq(iNow).css('left',suLi.width()).stop().animate({left:0});
			suLi.eq(iNow).stop().fadeIn();
			tNow=iNow;
		};
	};
		
	/*根据判断设定图片长宽*/
	function sImgWH(sImg){
	if(sImg.width()<sImg.height()){
		sImg.css({
			'width':'auto',
			'height':'100%'
			});
		}else{
		sImg.css({
			'width':'auto',
			'height':'100%'
			});
		};
	};
};

/*轮播器函数*/
function bScrollt(sDiv){
	var sUl=sDiv.children('ul');
	var suLi=sUl.children('li');
	var sImg=suLi.find('img');
	
	//sImgWH(sImg);
	if(suLi.length<2){return};
	
	sUl.children('li:gt(0)').css('left',suLi.width());
	
	/*创建页签*/
	var tip='<li></li>';
	for(var i=0;i<suLi.length;i++){	sDiv.append(tip)};
	sDiv.children('li').wrapAll('<ol></ol>');
	
	var sOl=sDiv.children('ol');
	var soLi=sOl.children('li');
	var tNow=0;
	var time=null;
	var iNow=0;
	sOl.css('margin-left',-sOl.width()/2+'px');
	soLi.eq(tNow).addClass('cur');
	
	soLi.mouseenter(function(){
		var tIndex=$(this).index();
		if(tIndex>tNow){
			$(this).addClass('cur').siblings('li').removeClass('cur');
			suLi.eq(tNow).stop().animate({left:-suLi.width()});
			suLi.eq(tIndex).css('left',suLi.width()).stop().animate({left:0});
		}else if(tIndex<tNow){
			$(this).addClass('cur').siblings('li').removeClass('cur');
			suLi.eq(tNow).stop().animate({left:suLi.width()});
			suLi.eq(tIndex).css('left',-suLi.width()).stop().animate({left:0});
		};
		tNow=tIndex;
		iNow=tIndex;
	});
	
	time=setInterval(function(){
		if(iNow==0){
			setTimeout(slider,1000);
		}else{
			slider();
		};
	},5000);
	
	var prev=sDiv.children('.prev');
	var next=sDiv.children('.next');
	prev.click(function(){
		suLi.eq(iNow).stop().animate({left:suLi.width()},1000,'easeInSine');
		if(iNow<suLi.length-1){
			iNow++;
		}else{
			iNow=0;
		};
		soLi.eq(iNow).addClass('cur').siblings('li').removeClass('cur');
		suLi.eq(iNow).css('left',-suLi.width()).stop().animate({left:0},1000,'easeInSine');
		tNow=iNow;
	});
	
	next.click(function(){
		suLi.eq(iNow).stop().animate({left:-suLi.width()},1000,'easeInSine');
		if(iNow>0){
			iNow--;
		}else{
			iNow=suLi.length-1;
		};
		soLi.eq(iNow).addClass('cur').siblings('li').removeClass('cur');
		suLi.eq(iNow).css('left',suLi.width()).stop().animate({left:0},1000,'easeInSine',function(){
		});
		tNow=iNow;
	});

	sDiv.hover(function(){
		clearInterval(time);
	},function(){
		time = setInterval(slider,5000);
	});
	
	function slider(){
		if(iNow<suLi.length-1){
			suLi.eq(iNow).stop().animate({left:-suLi.width()},1000,'easeInSine');
			iNow++;
			soLi.eq(iNow).addClass('cur').siblings('li').removeClass('cur');
			suLi.eq(iNow).css('left',suLi.width()).stop().animate({left:0},1000,'easeInSine');
			tNow=iNow;
		}else{
			suLi.eq(iNow).stop().animate({left:-suLi.width()},1000,'easeInSine');
			iNow=0;
			soLi.eq(iNow).addClass('cur').siblings('li').removeClass('cur');
			suLi.eq(iNow).css('left',suLi.width()).stop().animate({left:0},1000,'easeInSine',function(){
			});
			tNow=iNow;
		};
	};
};
})(jQuery);
</script>
</body>
</html>
