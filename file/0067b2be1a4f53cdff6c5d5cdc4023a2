<script>
(function($){
	$('#sub_nav_template').show().appendTo('#headerWarp');
	
	function in_array(search,array){
        for(var i in array){
            if(array[i]==search){
                return true;
            }
        }
        return false;
    }
    var menulist = $$('.ft-nav a');
    if(typeof(NTKF_PARAM.ntalkerparam)=="undefined"){
        NTKF_PARAM.ntalkerparam={item:{id:"0"}};
    }
	
	typeof(NTKF_PARAM.ntalkerparam.item) != 'undefined' && menulist.each(function(el){
        var ft_por_id=NTKF_PARAM.ntalkerparam.item.id;
        navobj=el.get('navcon');
        var strs= new Array(); 
        strs=navobj.split(","); 
        if(ft_por_id && navobj && in_array(ft_por_id,strs)) el.addClass('cur');
    });
})(jQuery);
</script>
