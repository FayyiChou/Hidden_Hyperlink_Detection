									if(currentEle.parent().prop("tagName").toLocaleLowerCase()=="a") {
										currentEle.parent().attr(
												{
													href  : targetHref,
													target: "_self"
												}
										);
									}
									else {
										currentEle.wrap("<a href='"+targetHref+"'>"+"</a>")
									}
									var naviA=$("<a>");
									naviA.css(
											{
												textAlign: "center",
												color    : "#b61606",
												display  : "block",
												marginTop: "10px"
											}
									);
									naviA.html("点击图片进入下一页");
									if(targetHref.length>3) {
										currentEle.after(naviA)
									}
								}
						);
					})()
				</script>
