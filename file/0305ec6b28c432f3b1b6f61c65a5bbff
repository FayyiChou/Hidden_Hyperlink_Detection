<script>
	function goBack(source) {
		var target=$(source);
		$(window).scroll(
				function() {
					if($(window).scrollTop()>100) {
						target.fadeIn(1500);
					}
					else {
						target.fadeOut(1500);
					}
				}
		);
		//当点击跳转链接后，回到页面顶部位置
		target.click(
				function() {
					$('body,html').animate({scrollTop: 0}, 800);
					return false;
				}
		);
	}
	goBack("#go_back");

</script>
