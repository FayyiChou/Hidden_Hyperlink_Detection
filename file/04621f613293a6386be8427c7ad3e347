<script>
$(function(){
        var notice = $("#J_notice");
        if(notice.length==1)
        {
           var is_close = getCookie('account_close_gonggao');
           if(is_close)
               notice.hide();
            else
               notice.fadeIn();
        }
    });
$(".gonggao-close").click(function(){
        $("#J_notice").hide();
        var expires = new Date();
        expires.setTime(expires.getTime() + 20 * 60 * 1000);
        setCookie('account_close_gonggao',1,expires);
 });
</script>
