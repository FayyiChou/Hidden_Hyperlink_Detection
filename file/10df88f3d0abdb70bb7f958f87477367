    <script type="text/javascript">
    $(function () {

        var cookieVal = getCookie("abcde");

        if (cookieVal != "") {
            $("#" + cookieVal).addClass("selectedpart");
        } else {
            $("#ii1").addClass("selectedpart");
        }
        $(".mainmenu").find("li").each(function () {
            $(this).click(function () {
                setCookie("abcde", $(this).attr("id"));
            });
        });
    });
</script> 
