		if(img1 != ""){
			if(img1.indexOf("500x500")!= -1){
				arrPic.push({ smallPic: '<a rel="nofollow" href="javascript:void(0);"><img width="70" height="70" src="'+ img1.replace("500x500", "80x80")+'" /></a>', originPic: img1, zoomPic: img1 });
			}
			if(img1.indexOf("_m2")!= -1){
				arrPic.push({ smallPic: '<a rel="nofollow" href="javascript:void(0);"><img width="70" height="70" onload=\"DrawImage(this,70,70)\" src="'+ img1.replace("_m2","_m")+'" /></a>', originPic: img1.replace("_m2","_m"), zoomPic: img1 });
			}
		}
		
		if(img2 != ""){
			if(img2.indexOf("500x500")!= -1){
				arrPic.push({ smallPic: '<a rel="nofollow" href="javascript:void(0);"><img width="70" height="70" src="'+ img2.replace("500x500", "80x80")+'" /></a>', originPic: img2, zoomPic: img2 });
			}
			if(img2.indexOf("_m2")!= -1){
				arrPic.push({ smallPic: '<a rel="nofollow" href="javascript:void(0);"><img width="70" height="70" onload=\"DrawImage(this,70,70)\" src="'+ img2.replace("_m2","_m")+'" /></a>', originPic: img2.replace("_m2","_m"), zoomPic: img2 });
			}
		}
		
		if(img3 != ""){
			if(img3.indexOf("500x500")!= -1){
				arrPic.push({ smallPic: '<a rel="nofollow" href="javascript:void(0);"><img width="70" height="70" src="'+ img3.replace("500x500", "80x80")+'" /></a>', originPic: img3, zoomPic: img3 });
			}
			if(img3.indexOf("_m2")!= -1){
				arrPic.push({ smallPic: '<a rel="nofollow" href="javascript:void(0);"><img width="70" height="70" onload=\"DrawImage(this,70,70)\" src="'+ img3.replace("_m2","_m")+'" /></a>', originPic: img3.replace("_m2","_m"), zoomPic: img3 });
			}
		}
		
		list.innerHTML = "";
		$$A.forEach(arrPic, function(o, i){
			var li = list.appendChild(document.createElement("LI"));
			li.innerHTML = o.smallPic;
			li.onclick = function(){
			iz.reset({ originPic: o.originPic, zoomPic: o.zoomPic });
			$$A.forEach(list.getElementsByTagName("LI"), function(li){ li.className = ""; });
			li.className = "cur";
		}
		hoverDelay(li, function(){
		if( !this.className ){
		var self = this;
		iz.reset({ originPic: o.originPic, zoomPic: o.zoomPic });
		$$A.forEach(list.getElementsByTagName("LI"), function(li){ li.className = ""; });
		this.className = "cur";
		image.src = o.originPic;
		}
		},function(){},20
		);
		if(!i){li.onclick();}
		})
	})();
	//执行产品大图操作结束
	</script>

