var colorId = 0; //选中颜色ID
var packageId = 0; //选中套餐ID 

function custom(m){
	var handle = g(m);
	var ele = handle.getElementsByTagName('a');
	var num = ele.length;
	for(var i=0;i<num;i++){
		ele[i].newId = i;
		ele[i].onclick = function(){
			//清除其它没有被点击的样式
			for(var j=0;j<num;j++){
				if(ele[j].className != 'grayChoose'){
					ele[j].className = '';
				}else{ele[j].className = '';}
				
			}
			//添加有点击选中的样式
			if(ele[this.newId].className != 'grayChoose'){
				ele[this.newId].className = 'cur';
				var ctmp = $(this).attr("color");
				var ptmp = $(this).attr("package");
				//给当前颜色赋值
				if(ctmp != undefined && parseInt(ctmp) > 0){
					colorId = ctmp;
				}
				// 选中颜色时的动作:
				if(colorId > 0){
					var colorAmount = 0;
					//alert("packageId= " + packageId);
					
					//选中颜色是改变库存量
					$("#amounts").text(colorAmount);
				}
				
				//给当前套餐赋值
				if(ptmp != undefined  && parseInt(ptmp) > 0){
					packageId = ptmp;
				}
				
				$("#packageChoose a").each(function(i){
					if($(this).attr("class") == 'cur'){
						packageId = parseInt($(this).attr("package"));
					}
				});
				
				//如果当前只选中一个,则不执行下面这段代码,用于计算库存及价格
				if(colorId > 0 && packageId > 0){
					
				}
			}
		}
	}
}
if($("#colorChoose").length > 0){
	//定制商品颜色
	custom('colorChoose');
}
if($("#packageChoose").length > 0){
	//定制套餐
	custom('packageChoose');
}
</script>
<script>
 $("#icon-beCarefulClose").click(function(){
            $("#beCareful").hide(400);
        });

//滚动栏固定在顶部
(function(){
	var mpHead = document.getElementById('J_scrollMenuBar');
	var h = mpHead.getBoundingClientRect().top;

	window.onscroll = function(){
		var sh = document.body.scrollTop || document.documentElement.scrollTop;
		if(sh > h){
			mpHead.className = 'scrollMenuBarTop';
		}else{
			mpHead.className = 'scrollMenuBar';
		}
	}
})();
</script>	

<script>
//检查购买数量的合法性
function checkLegal(num){
	var count = parseInt($("#amounts").text());
	if(num > count){
		alert('该商品最多可购买'+count+'件');
		$("#_count").val(1);
	}else if(num <=0){
		alert('出错啦！购买商品时请输入需要的数量。');
		$("#_count").val(1);
	}
}

function doShowProduct(pInfo){
	var items = pInfo.items;
	var html = '';
	for(var i=0;i<items.length;i++)
	{
		html += '<li><b>'+items[i].title+'：</b><i>'+items[i].value+' </i></li>';
	}
	document.getElementById("ul_Pdetail").innerHTML = html;
	$("#detailUrl").attr("href",pInfo.detailUrl);
}


</script>

