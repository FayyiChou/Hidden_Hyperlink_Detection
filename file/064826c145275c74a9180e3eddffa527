tabs[i].className = '';
}
}
if (curUrl.indexOf('p2/star1.html') > -1) {
clearClass();
tabs[1].className = 'current';
document.ts.action = 'http://m.pconline.com.cn/p2/star1.html';
} else if (curUrl.indexOf('s/list.html') > -1) {
clearClass();
tabs[2].className = 'current';
document.ts.action = 'http://m.pconline.com.cn/s/list.html';
} else {
clearClass();
tabs[0].className = 'current';
}
