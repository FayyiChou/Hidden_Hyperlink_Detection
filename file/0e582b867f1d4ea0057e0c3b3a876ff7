  <script language="javascript">
var arr="重庆时时彩";
function AddOptions(dltObj,arrObj)
{
dltObj.innerHTML="";
var arrLocation=arrObj.split("|");
for(var i=0;i<arrLocation.length;i++){
var opt=document.createElement("OPTION");
dltObj.add(opt);
opt.value=arrLocation[i];
opt.text=arrLocation[i];
}
}
function init(){
AddOptions(document.formf.lt,eval('arr'));
}
function checkform()
{
if(formf.cp_num.value==""){alert("期数不能为空！");return false;}
}
</script> 
