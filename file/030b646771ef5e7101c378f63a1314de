<script type="text/javascript">
$(function(){
	 $.ajaxSetup({   
       contentType:"application/x-www-form-urlencoded;charset=utf-8",   
       complete:function(XMLHttpRequest,textStatus){ 
	       	if('getResponseHeader' in XMLHttpRequest){
		         var sessionStatus=XMLHttpRequest.getResponseHeader("sessionStatus"); //通过XMLHttpRequest取得响应头，sessionstatus，  
		             if(sessionStatus=="timeout"){ 
		             	//如果超时就处理 ，指定要跳转的页面  
		                location.href="https://www.xiangjins.com/login.htm?referer="+window.location.href;   
		             }
		          }  
	         }
     });
});

function logout(){
	$.post("https://www.xiangjins.com/logout.htm",function(data){
		if(data==1){
			window.location.href="https://www.xiangjins.com/";
		}
    });
}


function toLogin(){
			window.location.href = 'https://www.xiangjins.com/login.htm?referer='+window.location.href;
		}
/**
function register(){
			window.location.href = 'https://www.xiangjins.com/register.htm';
		}*/
