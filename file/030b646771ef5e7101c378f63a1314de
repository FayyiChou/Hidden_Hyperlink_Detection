<script>
    //kefu
//    var kefu = $('.keifu_box');
//    kefu.hover(function() {
//        $(this).stop().animate({right: 0, opacity: 1}, 200);
//    }, function() {
//        $(this).stop().animate({right: -80+'px', opacity: 0.9}, 200);
//    }).animate({right: -80+'px', opacity: 0.9}, 200);

    /*收藏*/
function addBookmark(){
    var ctrl = (navigator.userAgent.toLowerCase()).indexOf('mac') != -1 ? 'Command/Cmd': 'CTRL';
    try{
        if (document.all) { //IE类浏览器
            try {
                window.external.toString(); //360浏览器不支持window.external，无法收藏
                wind.alert("提示","您可以尝试通过快捷键 ctrl+D 加入到收藏夹~");
            }
            catch (e){
                try{
                    window.external.addFavorite(window.location, document.title);
                }
                catch (e){
                    window.external.addToFavoritesBar(window.location, document.title);  //IE8
                }
            }
        }
        else if (window.sidebar) { //firfox等浏览器
            window.sidebar.addPanel(document.title, window.location, "");
        }
        else {
            wind.alert("提示",'您可以尝试通过快捷键 ctrl+D 加入到收藏夹~');
        }
    }
    catch (e){
        wind.alert("提示","您可以尝试通过快捷键 ctrl+D 加入到收藏夹~");
    }
}
</script>

<script src="https://www.xiangjins.com/themes/default/js/jquery.min.js" type="text/javascript"></script>
<script src="https://www.xiangjins.com/themes/default/js/jquery.SuperSlide.2.1.1.js" type="text/javascript"></script>
<script src="https://www.xiangjins.com/themes/default/js/index.js" type="text/javascript"></script>
<script src="https://www.xiangjins.com/themes/default/js/countdown/countdown.js" type="text/javascript"></script>
<script src="https://www.xiangjins.com/themes/default/js/wind.js" type="text/javascript"></script>
<script src="https://www.xiangjins.com/themes/default/js/common.js" type="text/javascript"></script>


<script>
    $(function(){

        $("#indexMenu").addClass("cur");

        $(document).scrollTop(0);
        /*轮播*/

        var bannerbg = $(".bannerbg");
        var hdli = bannerbg.find(".bd li").length;
        var hdlih = "";
        var bannerbd = bannerbg.find(".bd");
        var bannerhtml = bannerbd.html();
        for(var i = 0; i<hdli; i++){hdlih+="<li></li>";}
        bannerbg.find(".num").html(hdlih);
        bannerbg.slide({mainCell:".bd ul",effect:"left",autoPlay:true,interTime:"3000"});
        $(window).resize(function(){
            bannerbd.html(bannerhtml);
            bannerbg.slide({mainCell:".bd ul",effect:"left",autoPlay:true,interTime:"3000"});
        });
        $(".dtbox-cen .cen").slide({mainCell:".bd ul",effect:"left"});
        $(".bzbox-cen").slide({mainCell:".bd ul",effect:"left",autoPage:true,autoPlay:true,vis:4});
        $(".zybox .fn-clear").each(function(){
            var ldiv = $(this).find(".ldiv img");
            var rdiv = $(this).find(".rdiv p");
            var bdiv = $(this).find(".bdiv");
            var i = 0;
            bdiv.find(".prev").click(function(){
                i--;
                if(i<0){i=0;}
                ldiv.stop().animate({"left":(-i*190)+"px"},300);
                rdiv.stop().hide(300).eq(i).stop().show(300);
            });
            bdiv.find(".next").click(function(){
                i++;
                if(i>=rdiv.length){i=rdiv.length-1;}
                ldiv.stop().animate({"left":(-i*190)+"px"},300);
                rdiv.stop().hide(300).eq(i).stop().show(300);
            });
        });
        /**/
        $(".numic").each(function(i,e){
            var o = $(this);
            var t = 10;
            var num = o.text();
            var val = parseInt(num.replace(/,/g,""))/t;
            var setIntervalId = window.setInterval(function(){
                if(t--){
                    var v = parseInt(val*(10-t)).toString();
                    var varr = v.split("");
                    for (var i = varr.length-4; i >= 0; i -= 3) varr[i] += ",";
                    if(num.indexOf(".")<0){
                        o.text(varr.join(""));
                    }else{
                        o.text(varr.join("")+"."+num.split(".")[1]);
                    }
                }
                else{clearInterval(setIntervalId);}
            },100);
        });

        /*收藏*/
        function addBookmark() {
            var ctrl = (navigator.userAgent.toLowerCase()).indexOf('mac') != -1 ? 'Command/Cmd' : 'CTRL';
            try {
                if (document.all) { //IE类浏览器
                    try {
                        window.external.toString(); //360浏览器不支持window.external，无法收藏
                        window.alert("您可以尝试通过快捷键 ctrl+D 加入到收藏夹~");
                    } catch (e) {
                        try {
                            window.external.addFavorite(window.location, document.title);
                        } catch (e) {
                            window.external.addToFavoritesBar(window.location, document.title); //IE8
                        }
                    }
                } else if (window.sidebar) { //firfox等浏览器
                    window.sidebar.addPanel(document.title, window.location, "");
                } else {
                    alert('您可以尝试通过快捷键 ctrl+D 加入到收藏夹~');
                }
            } catch (e) {
                window.alert("您可以尝试通过快捷键 ctrl+D 加入到收藏夹~");
            }
        };

        /*圆环进度*/
        $(function() {
            $(".td5-m1").each(function(index, el) {
                var $val = parseInt($(this).text());
                $val ? $val = $val : $val = 0;
                var $key = -54 * $val;
                $(this).css({
                    "background-position": "" + ($key - 0) + "px 0"
                });
            });
        });

        //tabs
        $.jqtab = function(tab_tit, tab_content, tab_event) {
            $(tab_tit).children(".tab-all-tits").on(tab_event, function() {
                $(this).addClass("sel").siblings(".tab-all-tits").removeClass("sel");
                var activeindex = $(tab_tit).children(".tab-all-tits").index(this);
                $(tab_content).children().eq(activeindex).show().siblings().hide();
                return false;
            });
        };
        $.jqtab(".tab-a", ".tab-a-con", "click");

       //收缩
		$(function() {
			var slideHeight = 140; // px
			var defHeight = $('.x-list-e').height();
			var liste = $('.x-list-e');
			if (defHeight > slideHeight) {
				liste.css('height', slideHeight + 'px');
				liste.next('.list_more_a').children('a').click(function() {
					var curHeight = liste.height();
					if (curHeight == slideHeight) {
						liste.animate({
							height: defHeight
						}, "normal");
						$(this).html('收起<i class="ic_new ico_up ml5"></i>');
					} else {
						liste.animate({
							height: slideHeight
						}, "normal");
						$(this).html('查看更多合作机构<i class="ic_new ico_down ml5"></i>');
					}
					return false;
				});
			}else{			
				liste.next('.list_more_a').hide();
			}
		});
    });
</script>
