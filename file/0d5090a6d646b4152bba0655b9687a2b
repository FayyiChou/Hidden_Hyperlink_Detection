<!--javascript-->        <script type="text/javascript" src="http://www.sh-juyun.com/js/jquery.plugin.js"></script>
<script type="text/javascript" src="http://www.sh-juyun.com/js/main.js"  ></script>
<script>
    function validateLogin() {
        if ($('#top_login_username').val() == '') {
            alert('请输入用户名');
            return false;
        }
        else if ($('#top_login_password').val() == '') {
            alert('请输入密码');
            return false;
        } else {
            $.post('http://www.sh-juyun.com/index.php/site/signin', {username: $('#top_login_username').val(), password: $('#top_login_password').val()}, function(result) {
                if (result == 'ok') {
                    location.reload();
                } else {
                    alert(result);
                }
            });
        }

    }
    function validateRegister() {
        if ($('#top_reg_username').val() == '') {
            alert('请输入用户名');
            return false;
        }
        else if ($('#top_reg_password').val() == '') {
            alert('请输入密码');
            return false;
        } else if ($('#top_reg_email').val() == '') {
            alert('请输入邮箱');
            return false;
        } else {
            $.post('http://www.sh-juyun.com/index.php/site/reg_save', {username: $('#top_reg_username').val(), password: $('#top_reg_password').val(), email: $('#top_reg_email').val()}, function(result) {
                if (result == 'ok') {
                    alert('注册成功');
                    location.reload();
                } else {
                    alert(result);
                }
            });
        }
    }
</script>    </body>
</html>
