$(document).ready(function() {
	function a() {
		var a = $(window).scrollTop(),
		b = !window.XMLHttpRequest,
		c = $("#headMenu"), 
		d = {
			position: "fixed",
			top: "0"
		},
		e = $("#yd_menu"),
		f = {
		      display: "none"
		};
		a >= 505 ? b ? c.css({
			position: "absolute"
		}) : c.css(d) : (c.css({
			position: "relative"
		}), c.css({
			top: "0"
		}) ),
		a >= 505 ? b ? c.css({
			position: "absolute"
		}) : e.css(f) : (e.css({
			display: "block"
		}))
	}
	$(window).scroll(function() {
		a()
	})
})
