     function multiLevelMenu_college11(level){
        obj=document.getElementById(arrSel_college6[level]);
		if(obj)
		{
			subMenu_Name = "menu_college6";
			for(i=0;i<level;i++)
			{
				subMenu_Name += "[" + (document.getElementById(arrSel_college6[i]).selectedIndex*2 + 1) + "]";
			}
			
			subMenu = eval(subMenu_Name);
			//alert (subMenu_Name);
			//if(!subMenu) return false;
			
			
			sel_len = obj.length;
			for(i=0;i<sel_len;i++)
			{
				obj.options.remove(0);
			}

			new_sel_len = Math.floor(subMenu.length/2);

			for(i=0;i<new_sel_len;i++)
			{
				obj.options.add(new Option(subMenu[i*2],subMenu[i*2+1]));
			}
			
			obj.onchange = Function("multiLevelMenu_college11(" + (level+1) + ")");

			multiLevelMenu_college11(level+1);
		}
		else
		{

			return false;
		}	
     }
     
     function multiLevelMenu_college12(){
        var i;
	    switch(document.all.college_provid6.options[document.all.college_provid6.selectedIndex].text){
	       case menu_college6[document.all.college_provid6.selectedIndex*2]:
	          { switch(document.all.collegeid6.options[document.all.collegeid6.selectedIndex].text){
	                case menu_college6[document.all.college_provid6.selectedIndex*2 + 1][document.all.collegeid6.selectedIndex*2]:
	                   if(menu_college6[document.all.college_provid6.selectedIndex*2 + 1][document.all.collegeid6.selectedIndex*2 + 1]){
	                      window.open(menu_college6[document.all.college_provid6.selectedIndex*2 + 1][document.all.collegeid6.selectedIndex*2 + 1]);
	                   }
	                   else alert('请做出您的选择!');
	            } 
	          }
	    }
     }
     
     multiLevelMenu_college11(0);
</SCRIPT></p>
    </div>
