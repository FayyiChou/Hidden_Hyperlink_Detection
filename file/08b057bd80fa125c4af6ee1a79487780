					});
					t = setInterval("showAuto()", 5000);
					$("#Pic").hover(function(){clearInterval(t)}, function(){t = setInterval("showAuto()", 5000);});
				})
				function showAuto()
				{
					n = n >= (count - 1) ? 0 : ++n;
					$("#PicNum a").eq(n).trigger('click');
