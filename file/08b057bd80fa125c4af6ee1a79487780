				var t = n = 0, count = $("#PicShow a").size();
				$(function(){
					$("#PicShow a:not(:first-child)").hide();
					$("#PicNum a:first").css({"background":"#FF0000",'color':'#ffffff'});
					$("#PicNum a").click(function() {
					   var i = $(this).text() - 1;//取出当前图片位置值
					   n = i;
					   if (i >= count) return;
					   $("#PicShow a").filter(":visible").hide().parent().children().eq(i).fadeIn("slow");//关键
