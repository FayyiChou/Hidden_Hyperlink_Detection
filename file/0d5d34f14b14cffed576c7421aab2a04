$(document).ready(function () {
    $('.reco_wrap .tab a').mouseenter(function () {
        var index = $(this).index();
        var $reco = $(this).parents('.reco_wrap').find('.recommend');
        $(this).parents('.tab').children('a').removeClass('on');
