 $(document).ready(function(){
	  //$(".tab-box .tab:not(:first)").hide();
	  //$(".tab-nav .item:first").addClass("current");
      $(".tab-nav").each(function (i) {
		 var s = $(this).attr("id").split("-");
		 var num = s[2];
		 var tabN = "#tab-box-" + num;
		 $(this).find(".item").each(function(i){
               var index = $(this).parent().find(".item").index(this);
			   $(this).hover(function(){
					$(this).parent().find(".item").removeClass("current");
					$(this).addClass("current");
					$(tabN).find(".tab").css("display","none");
					$(tabN).find(".tab").eq(index).css("display","block");
					});
		});

     });

});
