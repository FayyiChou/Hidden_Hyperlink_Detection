        </div>        
        <script language="javascript">
        $(".spces-box").each(function(event){
            if($(this).find("tr").length==0){
                $(this).remove();
            }
        }
        );

        function submit_correct(){
            var options = {
                url: "ajax.php?t=new_correct&id=9539",
                beforeSubmit: function (formData, jqForm, options){
                    if ($.trim($("#correct_title").val())=='' || $.trim($("#correct_content").val())=='' || $.trim($("#vcode").val())==''){
                        alert("请填写完整信息后重试");
                        return false;
                    }
                },
                success: function (responseText, statusText){
                    if (responseText==1){
                        alert("提交成功，谢谢");                        
                    }else if(responseText==2){
                        alert("请正确填写验证码");
                        newvcode();
                    }else if(responseText==3){
                        alert("请勿重复提交信息");
                        newvcode();
                    }else{
                        alert("请填写完整信息后重试");
                    }
                    return false;
                }
            };
            $("#correct_form").ajaxSubmit(options);
        }

                </script>        
