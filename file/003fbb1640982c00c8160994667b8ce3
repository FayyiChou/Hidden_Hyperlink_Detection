<script language="javascript" type="text/javascript">
    var Check = new CheckInput();
    var cmpCheckCode = "";

    if($('#loginCategory').val()==0){
        $('.LIBByUsname').show();
        $('.LIBByPhone').hide();
    }  else if( $('#loginCategory').val()==6){
        $('.LIBByUsname').hide();
        $('.LIBByPhone').show();
    }

    function ckUID() {
        if($('#loginCategory').val()==0){
            return !Check.IsTextEmpty('username') && Check.checkLength('username', 2, 50);
        } else if($('#loginCategory').val()==6){
            return !Check.IsTextEmpty('phone') && Check.checkLength('phone', 11,11)&&(/^(1)\d{10}$/).test($("#phone").val());
        }
        return false;
    }

    $("#username").focus(function () {
        $("#nameErrorCode").hide();
        return;
    });

    $("#username").blur(function () {
        if (!ckUID()) {
            $("#nameErrorCode").show();
            return;
        }
    });

    $("#phone").focus(function () {
        $("#phoneErrorCode").hide();
        return;
    });

    $("#phone").blur(function () {
        if (!ckUID()) {
            $("#phoneErrorCode").show();
            return;
        }
    });

    function chkCode() {
        if ($('#randomPageId').length) {
            return Check.checkLength('checkCode', 4, 4);
        }
        return true;
    }
    function loginSubmit() {
        $("#errorMsg").hide();
        setTongdunRequestID();//同盾请求设置请求参数
        var res = Check.endAutoCheck(true);
        if (res) {
            if (!chkCode()) {
                $("#errorCode").show();
                return false;
            } else if ($.trim(cmpCheckCode) != ""){
                return false;
            } else if ($.trim($("#password").val()) == ""){
                $("#errorMsg").text("密码不正确。");
                return false;
            }

            $("#encryption").val($.md5($("#password").val()));
            if($("#encryption").val() == "") {
                $("#errorMsg").text("脚本出错，请使用其他浏览器尝试登录或联系客服人员。");
                return false;
            }

            $("#password").val("");
            $("#loginForm").submit();

            return true;
        }else {
            return false;
        }
    }

    $(function () {
        $('.LIBturnIcon').click(function(){
            $('#errorMsg').hide();
            if($(this).parent().attr("class")=="LIBByUsname"){
                $('.LIBByUsname').hide();
                $('.LIBByPhone').show();
                $('#loginCategory').val(6);
            }else{
                $('.LIBByUsname').show();
                $('.LIBByPhone').hide();
                $('#loginCategory').val(0);
            }
        });

        $("#codeImage").click(function() {
            $("#codeImage").attr("src","/sso/code?randomPageId=" + "&c=" + Math.random());
        });

        $("#checkCode").focus(function () {
            $("#errorCode").hide();
            cmpCheckCode = "";
            return;
        });

        $("#checkCode").blur(function () {
            var checkCode = $("#checkCode").val();
            if (checkCode == "" || checkCode.length != 4) {
                $("#errorCode").show();
                cmpCheckCode = checkCode;
                return;
            }
            $("#checkCode").attr("ptitle", "");
            checkVCode();
        });

        function checkVCode() {
            var checkCode = $("#checkCode").val();
            if (checkCode == "" || checkCode.length != 4) return;
            $.ajax({
                type: "Post",
                url: "/sso/checkCode?checkCode=" + checkCode + "&randomPageId=" + $("#randomPageId").val(),
                dataType: "json",
                success: function(data) {
                    if(data == 0){
                        $("#errorCode").show();
                        cmpCheckCode = checkCode;
                    } else if (data == 2){
                        $("#errorCode").show();
                        $("#errorCode").text("图形验证码已失效，请重新获取。");
                        cmpCheckCode = checkCode;
                    } else {
                        $("#errorCode").hide();
                        cmpCheckCode = "";
                    }
                }
            });
        }
    });
</script>


</body></html>