<script type="text/javascript">
    jQuery(function () {
        function setSiteCookie(name, value) {
            var Days = 1;
            var exp = new Date();
            exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);
            document.cookie = name + "=" + value + ";expires=" + exp.toGMTString();
        }
        function getSiteCookie(name) {
            var cookies = document.cookie.split("; ");
            for (var i = 0; i < cookies.length; i++) {
                var cookieName = cookies[i].split("=");
                if (cookieName[0] == name) {
                    return cookieName[1];
                }
            }
        }
        if (getSiteCookie("dpcontext") != undefined)
            jQuery("#context").val(unescape(getSiteCookie("dpcontext")));
        
        jQuery("#context").keyup(function (event) {
            if (jQuery(this).val().length > 0) {
                jQuery("#publish").attr('style', 'cursor:hand');
            }
        });

        function getByteLen(str){
            return String(str).replace(/[^\x00-\xff]/g,'aa').length;
        }

    });
</script>
