	   agedata = jQuery.parseJSON(data);
	   var age = [];var amount = [];var pre= [];var tem;
	   for (var i = 0; i < agedata.length; i++) {
		   tem = 0
		   tem = Math.floor(agedata[i].amount*agedata[i].pre/100);
		   pre.push(tem);
		   amount.push(agedata[i].amount-tem);
	   }
	   age.push({'data':amount,color:"#FFBAC3",name:"点评人数"},{'data':pre,color:"#FF9E57",name:"好评人数"});
	   getbar('agechart', age, ["25\u5c81\u4ee5\u4e0b","26-30\u5c81","31-35\u5c81","36\u5c81\u4ee5\u4e0a"], agedata, agetype);
	   
	   
