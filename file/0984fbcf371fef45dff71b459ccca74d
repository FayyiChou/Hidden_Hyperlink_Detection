   $(function(){

	    //var colors = Highcharts.getOptions().colors,
	    data = '[["5\u661f",51.2],["4\u661f",43.9],["3\u661f",4.9],["2\u661f",0],["1\u661f",0]]';
	    data = jQuery.parseJSON(data);
	    colors = ["#f8a1aa", "#ff9e57", "#79c4a5", "#72c8d7", "#a7c0de"];
	    
