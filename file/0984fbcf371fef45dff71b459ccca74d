<script>
function pagetarget2(){
    var page1 = document.getElementById("cpage");
    if(page1!=null){
        if(page1.children.length > 0){
            var childs = page1.children.item(0).childNodes;
            for (i=0; i<childs.length; i++){
                if(childs[i].nodeName == 'A'){
                    childs[i].setAttribute('target','_self')
                }
            }
        }
    }
}

window.onload=pagetarget2;

</script>
