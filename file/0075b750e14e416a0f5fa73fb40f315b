		<script language="javascript">
			/***************滚动图片效果开始***************/
			var speed = 30;
			demo2.innerHTML = demo1.innerHTML;
			function MarqueeL() {
				//设置 oldLeft变量，是因为 当图片较少时，会出现 demo.scrollLeft不可能大于 demo2.offsetWidth。(可以设置个文本框观察下参数就明白了)
				var oldLeft = demo.scrollLeft;
				if (demo2.offsetWidth - demo.scrollLeft <= 0) {
					demo.scrollLeft -= demo1.offsetWidth;
				} else {
					demo.scrollLeft++;
				}
				if (oldLeft == demo.scrollLeft) {
					demo.scrollLeft = 0;
				}

			}
			var MyMar = setInterval(MarqueeL, speed);
			demo.onmouseover = function() {
				clearInterval(MyMar);
			}
			demo.onmouseout = function() {
				MyMar = setInterval(MarqueeL, speed);
			}
			/***************滚动图片效果结束***************/
			function query() {
				document.getElementById("title").value = document
						.getElementById("title").value.trim().filter();
				var keyName = document.getElementById("title").value.trim()
						.filter();
				if (keyName == null || keyName.length == 0) {
					alert("查询内容不能为空！");
					document.getElementById("title").focus();
					return false;
				}
				document.researchForm.target = "_blank";
				document.researchForm.action = "articleFront/articleQueryByTitle.action?tempId="
						+ Math.random();
				document.researchForm.submit();
			}
		</script>
		


