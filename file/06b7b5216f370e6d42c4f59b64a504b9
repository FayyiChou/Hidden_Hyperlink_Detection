	}
	var durl="/movie/17091/"+res_format+"-"+page;
	$.get(durl,
		function(result){
			$("#cpdl2list").html(result);
		});
	if(jump){
	//$("html,body").animate({scrollTop:$("#cpage").offset().top},300);
	}
}
