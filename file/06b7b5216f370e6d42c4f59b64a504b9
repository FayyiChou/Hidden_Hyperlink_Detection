<script>
var app = 2
function changedaz(mid)
{
	$("#sidebar_related_movies").html(" 努力加载中...");
	var msg 	= $.ajax({
		  url: "/ajax_request/sidebar_related_movies/"+mid+'/'+app,
		  global: false,
		  type: "GET",
		  dataType: "html",
		  async:false
	   }
	).responseText;
	
	$("#sidebar_related_movies").html(msg);
	
	if(app==1){
		app=1;
	}else{
		app++;
	}
	
}
</script>



