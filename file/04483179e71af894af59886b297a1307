<a href="http://www.gdjkfupin.org.cn/yfwn/index.asp"  target="_blank" onclick="javascript:adclose()">
<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,19,0" width="300" height="155">
          <param name="movie" value="swf/yfwn.swf" />
          <param name="quality" value="high" />
          <embed src="swf/top3.swf" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" width="300" height="155"></embed>
