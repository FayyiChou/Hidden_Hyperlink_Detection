<script>
    
    // $('.menu li:eq(0)').children('a').addClass('itemselect');

    var strs = new Array();
    var url = window.location['pathname'];
    var strs = url.substring(0,url.indexOf('.html')).split('\/');

    $('.'+strs[4]).children('a').addClass('lableSelect');
    if (strs.length == 7) {
        $(".news-tab2 td a").eq(parseInt(strs[6])).addClass('lableSelect');
    }else{
        $(".news-tab2 td a").eq(0).addClass('lableSelect');
    }

    function remark(pid) {
        window.location.href="/quotation/remark/id/"+pid;
    }

    // 遮罩层
    $(document).ready(function() {
        
        
        var windowHeight=$(window).height();
        var dialogHeight=$(".cu-alter_to").outerHeight(true);
        var titleHeight=$(".cu-alter_to  h1").outerHeight(true);
        
        var windowWidth=$(window).width();
        var dialogWidth=$(".cu-alter_to").outerWidth(true);
        var left=(windowWidth-dialogWidth)/2;
        
    
    
        if(dialogHeight>windowHeight)
        {
            dialogHeight=windowHeight*0.8;
            var contentHeight=dialogHeight-titleHeight;
             $("#content2").height(contentHeight);
            
        }
        dialogHeight=$(".cu-alter_to").outerHeight();

        var top=(windowHeight-dialogHeight)/2;
        
        
        $(".cu-alter_to").css({top:top,left:left});
        
        
        $("#showTip").click(function()
        {
            
            $("#zn_mask_alter").css('visibility','visible');
            $('body,html').css('overflow-y','hidden');
            return false;
        });
        
        $(".closeDialog").click(function(e) {
            $("#zn_mask_alter").css('visibility','hidden');
            $('body,html').css('overflow-y','');
            return false;
        });
        
        
    });


</script>
