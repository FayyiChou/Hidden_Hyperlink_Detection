        // });

        /* 当滚动条的位置处于距顶部100像素以下时，跳转链接出现，否则消失 */
        $(window).scroll(function () {
            var Offset = $(window).scrollTop(); // 滚轮转动高度
            // console.log(Offset);
            if(Offset > 100){
                $('#btnTop').fadeIn(1500);
            }else{
                $('#btnTop').fadeOut(1500);
            }
        });

        /* 点击返回顶部*/
        $('#btnTop').click(function(){
            $('body,html').animate({scrollTop:0},1000); 
            return false;
        });

    });

    // 获取新闻数据
    $(function () {
        var stop = true;
        var metalType = "sn";
        // if(metalType != 'futures'){
            var strs = new Array();
            var url = window.location['pathname'];
            var strs = url.substring(0,url.indexOf('.html')).split('\/');
            $('.'+strs[4]).children('a').addClass('lableSelect');

            //导航栏选中
            // $('.lableSelect').parent().siblings().show();
            // $('.lableSelect').parent().parent().show().siblings('ul').hide();
            // $('.menu li:eq(0)').children('a').addClass('itemselect');

            var page = 1;
            $(window).scroll(function () {
                var html = '';
                $('#con2 tr:last td').css('border-bottom-width','0px');
                var height = $(window).height(); // 获取当前浏览器高度
                var Offset = $(window).scrollTop(); // 滚轮转动高度
                var docHeight = $(document).height()-70; // 获取页面的文档高度
                var totalheight = parseFloat(height) + parseFloat(Offset); // 获取当前浏览器高度+滚轮转动高度
                var a = "sn";
                if(a == 'futures'){
                    url = '/news/loadingNews';
                }else{
                    url = '/news/loadingProductNews';
                }
                // url = '/news/loadingNews';
                if (docHeight <= totalheight) {
                    // if (docHeight - $(this).scrollTop() - $(this).height() <= 0) { // 当前页面高度小于已滚动出去的高度进行加载数据
                        // $('body').append('<div>正在加载中...</div>');
                    if(stop==true)
                    {
                        stop=false;
                        page++;
                        $.get(url, {
                            page: page,
                            id: '24'
                        }, function (data) {

                            var json = eval(data);
                            for (var i = 0; i < json.length; i++) {
                                html += '<tr>';
                                html += '<td width="30%"><img width="110px" src="' + json[i].img + '"></td>';
                                html += '<td width="70%">';
                                html += '<p>';
                                html += '[' + json[i].news_type_name + ']';
                                html += '<a href="/news/info/id/'+json[i].news_id+'">' + json[i].title_name + '</a>';<!--/metaltype/'+metalType+'-->
                                html += '</p>';
                                html += '<span>' + json[i].renew_date + '</span>';
                                html += '</td>';
                                html += '</tr>';
                            }

                            if(json.length == 0){
                                $('#tianchong').css('display','none');
                                $('.footer').css('display','block');
                            }

                            setTimeout(function(){
                                $('#con2 tr:last td').css('border-bottom-width','1px');
                                $('.container').append(html);
                                stop=true;
                            },800);

                        });
                    // }
                    }
                }
            });
        // }
        

    });
</script>
