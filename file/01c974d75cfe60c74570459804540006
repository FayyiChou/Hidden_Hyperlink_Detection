<script type="text/javascript">
function F(id){
  return document.getElementById(id);
}
var showOrHide=getcookie('gamechat_show_hide');
if(showOrHide==''){
  showOrHide='onez_gamechat_hide';
}
F('onez_gamechat_box').className=showOrHide;

var MsgTipBool=true;
var MsgTipTitle=top.document.title;
function MsgTip(){
  if(top.document.hasFocus()){
    top.document.title=MsgTipTitle;
    return;
  }
  top.document.title=MsgTipBool?'【有新消息】':'【　　　　】';
  MsgTipBool=!MsgTipBool;
  setTimeout('MsgTip()',800);
}
function OnezGameChat(type,a){
  if(type=='newmsg'){
    MsgTip();
  }else if(type=='system'){
    if(a=='show'){
      F('onez_gamechat_box').className='onez_gamechat_show';
      setcookie('gamechat_show_hide','onez_gamechat_show');
    }else{
      F('onez_gamechat_box').className='onez_gamechat_hide';
      setcookie('gamechat_show_hide','onez_gamechat_hide');
    }
  }
}
function InsertFlash(Flash,Vars,Width,Height,ID,BoxName){
  Vars+='&wmode='+gamechat_wmode;
  Vars+='&tokename='+(document.title.split(' - ')[0]);
  Vars+='&qrcode='+encodeURIComponent(encodeURIComponent('http://www.lzmrmfw.com//plugin.php?id=onez_gamechat&action=mobile&token=8bb26b2bc632220f8afe'));
  var FlashHtml='<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" ';
  FlashHtml+='codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,29,0" ';
  FlashHtml+='width="' + Width + '" height="' + Height + '" id="' + ID + '">';
  FlashHtml+='<param name="movie" value="' + Flash + '">';
  FlashHtml+='<param name="quality" value="high">';
  FlashHtml+='<param name="allowFullScreen" value="true">';
  FlashHtml+='<param name="wmode" value="'+gamechat_wmode+'">';
  FlashHtml+='<param name="allowscriptaccess" value="always">';
  FlashHtml+='<param name="FlashVars" value="'+Vars+'">';
  FlashHtml+='<embed src="' + Flash + '" name="' + ID + '" quality="high" allowscriptaccess="always" pluginspage="http://www.macromedia.com/go/getflashplayer" ';
  FlashHtml+='type="application/x-shockwave-flash" width="' + Width + '" FlashVars="'+Vars+'" wmode="'+gamechat_wmode+'" allowFullScreen="true" height="' + Height + '"></embed>';
  FlashHtml+='</object>';
  F(BoxName).innerHTML=FlashHtml;
}
var gamechat_wmode=getcookie('gamechat_wmode');
if(gamechat_wmode==''){
  gamechat_wmode='transparent';
}
function OnezGameChatShow(wmode){
  gamechat_wmode=wmode;
  InsertFlash('source/plugin/onez_gamechat/template/SiteChat.swf?t=1354534426','uid=0&uname=&token=8bb26b2bc632220f8afe&theme=default&soundif=current,world,user&circle=1&background=#000000&alpha=0.2&channel=world&switch_js=1&success_tip=1&page_tip=1','100%','100%','_gamechat_obj','onez_gamechat');
  setcookie('gamechat_wmode',wmode);
}
OnezGameChatShow(gamechat_wmode);
</script>
