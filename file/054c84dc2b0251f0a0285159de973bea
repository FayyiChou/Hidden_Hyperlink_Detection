<script>
    //var jQuery = dfsj_jq;
jQuery.noConflict();
var sh;
    function openMsgArea() {
        jQuery('#msg-area').stop(true).animate({
            'bottom': 20
        }, 750);
    }
    function closeMsgArea() {
        jQuery('#msg-area').stop(true).animate({
            'bottom': -237
        }, 300);
    }

    function showMessage(msg) {
console.log(msg);
        var data = jQuery.parseJSON(msg['content']);
        jQuery('#msg-area').attr('class', '').addClass('game' + data['game_id']);
        jQuery('#msg-area .title-area .title').html(data['title']);
        jQuery('#msg-area .content .head-image').attr('style', 'background-image: url(' + data['headimage'] + ')');
        jQuery('#msg-area .content .text-area.author').html('作者：' + data['username']);
        jQuery('#msg-area .content .text-area.description').html(data['description']);
        jQuery('#msg-area .btn-detail a').attr('href', data['url']).attr('target', '_blank');
        openMsgArea();
jQuery.post('http://bbs.colg.cn/plugin.php?id=api&action=msgcenter&do=setread', {
'mid': data['id']
}, function(data) {
console.log(data);
});
//setTimeout(closeMsgArea, 60000);
    }
</script>
<script src="http://www.colg.cn/public/zhuanti/201605/rename/js/rename.js" type="text/javascript"></script>
<script src="http://www.colg.cn/public/zhuanti/201605/vip/js/vip.js" type="text/javascript"></script>
