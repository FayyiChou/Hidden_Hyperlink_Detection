<script>
function setShowHiddenTextStatus(thread_id) {
jQuery.post('/plugin.php?id=api&action=bbs&do=setShowHiddenTextStatus', {
tid: thread_id
}, function(data) {
var jsonDATA = jQuery.parseJSON(data);
if (jsonDATA.REV == true) {
location.reload();
} else {
if (jsonDATA['ERROR']['MSG'] != '') {
alert(jsonDATA['ERROR']['MSG']);
}
}
});
}

    jQuery(function () {
jQuery.post('http://bbs.colg.cn/plugin.php?id=api&action=access&do=login&ping=1', {pid: 0});
        jQuery('#msg-area .title-image .btn.close').click(function () {
            closeMsgArea();
        });

jQuery('#bbs-right-nav').hover(function() {
jQuery(this).find('.headimg').stop(true).animate({
width: 64, height: 64
}, 120);
setTimeout(function() {
jQuery('#bbs-right-nav').addClass('show');
}, 120);
}, function() {
jQuery('#bbs-right-nav').removeClass('show');
jQuery(this).find('.headimg').stop(true).animate({
width: 48, height: 48
}, 120);
});

jQuery('#bbs-right-nav .menu-area > div').hover(function() {
jQuery(this).find('.sub-menu-panel').show();
}, function() {
jQuery(this).find('.sub-menu-panel').hide();
});

jQuery('#bbs-right-nav .sub-menu > div').hover(function() {
jQuery(this).find('.tri-menu-panel').show();
}, function() {
jQuery(this).find('.tri-menu-panel').hide();
});

jQuery('#bbs-right-nav .page a').click(function() {
var index = parseInt(jQuery(this).index()) + 1;
jQuery('div[class^=job-in-page]').hide();
jQuery('.job-in-page' + index).show();
jQuery('.page a').removeClass('on').eq(index - 1).addClass('on');
});
jQuery('.rename-boxer .btn-close').click(function() {
closeBoxer(jQuery(this).parent());
});
getScoreVals(10, 1);
jQuery('#fight9th_live-btn-close').click(function() {
closeLive();
});
jQuery('.btn-open-mobile-forum').click(function() {
openBindForum();
});
jQuery.post('http://bbs.colg.cn/plugin.php?id=api&action=mobile&do=getmobile', '', function(data) {
if (data.length > 0) {
jQuery('#user-bind-mobile').html(data);
jQuery('#btn-open-mobile-forum-a').hide();
} else {
jQuery('#user-bind-mobile').html('');
jQuery('#btn-open-mobile-forum-a').show();
}
});
    });
function idpk(uid) {
jQuery.get('http://bbs.colg.cn/plugin.php?id=api&action=idpk', {uid: uid}, function(data) {
jQuery('#exDialogArea').html(data);
});
}
function openBindForum() {
jQuery.get('http://bbs.colg.cn/plugin.php?id=api&action=mobile&do=forum', '', function(data) {
jQuery('#exDialogArea').html(data);
});
}
</script>
