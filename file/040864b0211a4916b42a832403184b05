<script type="text/javascript">
    function GetPost(id)
    {
        $("#ajax-get-post-overlay").fadeIn();
        $("#ajax-get-post").fadeIn();
        $("#ajax-get-post").html("<div id='post-loading'></div>");
        $.ajax({
            type: "POST",
            url: "http://arkhyz24.ru/ajax-get-post",
            data: { 'post_id': id },
            success: function(d) {
                $("#ajax-get-post").html(d);
            }
        });
    }
    $(function(){
        $('#ajax-get-post-close').live('click',function(){
            $("#ajax-get-post-overlay").fadeOut('fast');
            $("#ajax-get-post").fadeOut('fast',function(){
                $(this).html('');
            });
            return false;
        });
        $('#ajax-get-post-overlay').live('click',function(){
            $(this).fadeOut('fast');
            $("#ajax-get-post").fadeOut('fast',function(){
                $(this).html('');
            });
            return false;
        });
