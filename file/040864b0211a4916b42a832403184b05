        var tvproWgt = $('.tvpro-wgt');
        var tvproWgtDiv = $(tvproWgt).children('div').eq(0);
        var offset = $(tvproWgt).offset().top;
        var tvproWgtHeight = $(tvproWgtDiv).height();
        var ScrollPercent = parseInt( 21 / 24 * 100 );
        var scrollto = tvproWgtHeight * (ScrollPercent/100);
        //$(tvproWgt).parent().append('<p>1 / 24 * 100 = '+ScrollPercent+'</p>');
        //$(tvproWgt).parent().append('<p>'+offset+' + '+tvproWgtHeight+' * ('+ScrollPercent+'/'+'100) = '+scrollto+'</p>');
        //$(tvproWgt).parent().append('<p>Height: '+tvproWgtHeight+'px, TopOffset: '+offset+'px, ScrollPercent: '+ScrollPercent+'%, ScrollTo: '+scrollto+'px</p>');
        $(tvproWgt).animate({scrollTop: scrollto}, 1000);

