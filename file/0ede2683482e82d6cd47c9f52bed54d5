<script>
	$(function() {
		$("#tabs_con .tab_content").hide(); //Hide all content
		$("#tabs_ul li:first").addClass("active").show(); //Activate first tab
		$("#tabs_con .tab_content:first").show(); //Show first tab content

		//On Click Event
		$("#tabs_ul li").click(function() {
			//換圖
			for(i = 0;i < 4;i++){
				$("#tabs_ul li:eq("+i+") img").attr("src","images/goldenplus_select_0"+(i+1)+".jpg");
			}
			var v=$("a img", this).attr("src");
			$("a img", this).attr("src","images/goldenplus_select_down_"+v.substr(v.length-6,6));

			
			$("#tabs_ul li").removeClass("active"); //Remove any "active" class
			$(this).addClass("active"); //Add "active" class to selected tab
			$("#tabs_con .tab_content").hide(); //Hide all tab content
			
			var activeTab = $(this).find("a").attr("href"); //Find the href attribute value to identify the active tab + content
			$(activeTab).fadeIn(100); //Fade in the active ID content
			$(activeTab+" #tabs1_con .tab_content:first").show(); //Show first tab content
			//alert($(activeTab+" #tabs1_ul ul li :eq(0) img").attr("src"));
			for(i = 0;i < 3;i++){
				$(activeTab+" #tabs1_ul ul li:eq("+i+") img").attr("src","images/goldenplus_button_0"+(i+1)+".jpg");
			}
			var v=$(activeTab+" #tabs1_ul ul li:eq(0) img").attr("src");
			$(activeTab+" #tabs1_ul ul li:eq(0) img").attr("src","images/goldenplus_button_down_"+v.substr(v.length-6,6));
			return false;
		});

		$("#tabs1_con .tab_content").hide(); //Hide all content		
		$("#tabs1_ul li:first").addClass("active").show(); //Activate first tab
		$("#tabs1_con .tab_content:first").show(); //Show first tab content
		
		$("#tabs1_ul li").click(function() {
			//換圖
			
			for(i = 0;i < 3;i++){
				$(this).parent().children(":eq("+ i +")").children("a").children("img").attr("src","images/goldenplus_button_0"+(i+1)+".jpg");
			}
			var v=$("a img", this).attr("src");
			//alert($(this).children('a').children('img').attr("src"));
			$("a img", this).attr("src","images/goldenplus_button_down_"+v.substr(v.length-6,6));

			
			$("#tabs1_ul li").removeClass("active"); //Remove any "active" class
			$(this).addClass("active"); //Add "active" class to selected tab
			$("#tabs1_con .tab_content").hide(); //Hide all content	
			
			var activeTab = $(this).find("a").attr("href"); //Find the href attribute value to identify the active tab + content
			$("#tabs1_con "+activeTab).fadeIn(100); //Fade in the active ID content
			return false;
		});
		
		$('.h3').next().hide();
		$('.h3href').click(function() {
			location=$(this).attr("href")
			return false;
		});			
		$('.h3').click(function() {			
			//alert("11");
			$(this).next().toggle();
			return false;
		});

	});	
</script>
