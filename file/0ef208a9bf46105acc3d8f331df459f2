<script>
function saveToDesk(){
	var query = location.search.toLowerCase().substr(1);
	if (query.charAt(0)&&query.charAt(0)=='k' ) {
		var match = /k\d+$/.exec(query);
		if(match && match!==null){
			window.open('http://download.2345.com/2345_common/2345_'+match[0]+'_desktool.exe','_self');
		}else{
			window.open('http://download.2345.com/2345desk/2345desktool.exe','_self');
		}
	}else{
		window.open('http://download.2345.com/2345desk/2345desktool.exe','_self');
	}
}
</script>
