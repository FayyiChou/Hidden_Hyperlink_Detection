(function (exports){
    var logoWrappper =  $('websiteLogo').parentNode,timer = null;
    if ($CONFIG['pushZhuti'] &&  $CONFIG['pushZhuti'] != g_cookie('theme')) {
       logoWrappper.style.cssText = 'height:62px;margin-top:8px;background:url(i/pushZhuti/'+$CONFIG['pushZhuti']+'.png) no-repeat;_background:none;_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src=\'i/pushZhuti/'+$CONFIG['pushZhuti']+'.png\',sizingMethod=\'crop\');';
       logoWrappper.onclick  = function (){
          mystyleInit.set("theme", $CONFIG['pushZhuti']);
          mystyleInit.setZT();
          mystyleInit.show(1);
          clickCount('pushZhuti_95');
          return false;
       };
       return;
   }else{
      logoWrappper.style.cssText = 'width:135px;height:57px;margin-top:5px;background:url(images/logo/logo_normal.png) no-repeat;_background:none;_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src=\'images/logo/logo_normal.png\',sizingMethod=\'crop\');';
   };
    var  obj =  $('J_tipSet_ie6') , 
          ua = navigator.userAgent ,
          hasSetHomePage = function (obj){
            if (!/MSIE/.test(ua)) {return false;}
			try{
				if (obj.isHomePage('http://www.2345.com/?to=2345') ||
					  obj.isHomePage('http://www.2345.com/?t=2345') ||
					  obj.isHomePage('http://www.2345.com/') || 
					  obj.isHomePage('http://www.2345.com') || 
					  obj.isHomePage('http://www.2345.com/?to=ie6') || 
					  obj.isHomePage('http://www.2345.com/?t1=2345')
				) {return true;}
				if(window.location.href.indexOf('2345.com')>-1&&obj.isHomePage(window.location.href)){
					return true;
				}
			}catch(e){}
            return false;
          };
    obj.style.behavior = 'url(#default#homepage)';
    exports.hasSetHomePage = hasSetHomePage(obj);
	//if(true!==exports.hasSetHomePage){
		logoWrappper.title="把2345网址导航设为主页";
		logoWrappper.onclick=function(){
			T.setHomePage(this,'http://www.2345.com/?t=2345');
			clickCount('homepage1');
			return false;
		};
	//}
    if (!/MSIE 6\./.test(ua) || window.opera ) { return;};
    if (!exports.hasSetHomePage && !localStorage.getItem('__tipSet_ie6__')) {
      obj.style.display = '';
      obj.onclick =  function (){
          T.setHomePage(obj,'http://www.2345.com/?to=ie6');
          clickCount('homepage_ie6');
      }
    }  
}(this));
