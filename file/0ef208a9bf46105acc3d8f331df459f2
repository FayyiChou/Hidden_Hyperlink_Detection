			if (target.nodeName ==='SPAN' || target.nodeName ==='I') {
				target = target.parentNode;
				return;
			}
			if ((target.nodeName === 'DIV') && (target.className == "btn-area" || target.className == "txt-area" || target.className == "pop-zfclose-con" || target.className == "mod pop-zfclose")){
				return;
			}
			if ((target.nodeName === 'A') && target.className.indexOf("mod-arrow") > -1){
			    return;
			}
			$("modZfSite").className = "mod mod-zfsite";
			$("popZfClose").style.display = "none";
		});
	</script>
  </div>
</div>
<!--政府网站关闭提示弹层 end-->

