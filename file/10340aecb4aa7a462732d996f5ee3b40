<script type="text/javascript" language="javascript">
var changeOpacityTimeCounter=0, imageIndex=1, t1, t2, headImageId, nextHeadImageId, headImage, nextHeadImage;
var DangJiJingXuanImage, nextDangJiJingXuanImage, DangJiJingXuanImageId, nextDangJiJingXuanImageId;
function changeImageTimer()
{
    t1=setTimeout("changeOpacity()",3000);
}
function changeOpacity()
{
    headImageId="HeadImage"+imageIndex;
    DangJiJingXuanImageId="DangJiJingXuanImage"+imageIndex;
    headImage=document.getElementById(headImageId);
    DangJiJingXuanImage=document.getElementById(DangJiJingXuanImageId);
    if(imageIndex>=5)
    {
        nextHeadImageId="HeadImage1";
        nextDangJiJingXuanImageId="DangJiJingXuanImage1";
    }
    else
    {
        nextHeadImageId="HeadImage"+(imageIndex+1);
        nextDangJiJingXuanImageId="DangJiJingXuanImage"+(imageIndex+1);
    }
    nextHeadImage=document.getElementById(nextHeadImageId);
    nextDangJiJingXuanImage=document.getElementById(nextDangJiJingXuanImageId);
    headImage.style.display="";
    DangJiJingXuanImage.style.display="";
    nextHeadImage.style.display="";
    nextDangJiJingXuanImage.style.display="";
    
    changeOpacityTimer();
}
function changeOpacityTimer()
{
    if(changeOpacityTimeCounter>=5)
    {
        changeOpacityTimeCounter=0;
        if(imageIndex>=5)
        {
            imageIndex=1;
        }
        else
        {
            imageIndex=imageIndex+1;
        }
        headImage.style.display="none";
        DangJiJingXuanImage.style.display="none";
        changeImageTimer();
    }
    else
    {
        headImage.style.opacity=1-(changeOpacityTimeCounter/5);
        nextHeadImage.style.opacity=changeOpacityTimeCounter/5;
        DangJiJingXuanImage.style.opacity=1-(changeOpacityTimeCounter/5);
        nextDangJiJingXuanImage.style.opacity=changeOpacityTimeCounter/5;
        changeOpacityTimeCounter=changeOpacityTimeCounter+1;
        t2=setTimeout("changeOpacityTimer()",200);
    }
}
</script>




