<script type="text/javascript">
    var header_mouseOnUser = false;
    var showUserCenter = function () {
        header_mouseOnUser = true
        document.getElementById("userCenterDiv").style.display = 'block';
    }
    var hideUserCenter = function () {
        header_mouseOnUser = false;
        window
                .setTimeout(
                function () {
                    if (!header_mouseOnUser) {
                        document.getElementById("userCenterDiv").style.display = 'none';
                    }
                }, 300);
    }


    /**
     * 表单提交
     */
    var searchFormSubmit = function (pageNo) {
        var key = $("#searchkey").val().trim();
        var entry = $("#entryInput").val();
        if(key == ""){
        	window.top.location.href = "http://www.shuidixy.com/company_info_16047328054379265480713921730569.html";
        	return
        }
        if ((entry != "1" && entry != "2") && key.indexOf("*") >= 0) {
            $("#searchkey").val("");
            $("#searchkey").attr("style", "border: red 2px solid;font-size:16px;color:#fff;");
            return;
        } else {
            if (typeof(pageNo) == 'undefined' || pageNo == null || isNaN(parseInt(pageNo))) {
                pageNo = 0;
            }
            $("#npageInput").val(pageNo);
            $mark = $("#pageListParams input[name=customParam" + pageNo + "]").val();
            $("#markInput").val($mark);
            if (pageNo == 0) {
                $("#totalInput").val("");
            }
            $("#searchForm").submit();
        }
    }

    /**
     * 单击搜索，清空里面的查询条件
     */
    var searchBtnFn = function () {
        if ($("#entryInput").val() != '1') {
            $("#provinceCodeInput").val("");
        }
        if ($("#entryInput").val() != '2') {
            $("#secondLevelIndustryTypeInput").val("");
        }
        $("#searchTypeInput").val("all");
        $("#sortInput").val("");
        $("#npageInput").val("0");
        $("#capitalNumberInput").val("");
        $("#establishDurationInput").val("");
        $("#markInput").val("");
        $("#totalInput").val("");

        searchFormSubmit();
    }
  	//判断是否是子账号登录
	 var subLoginAjax = function(){
		var url= basePath + "/company/subLoginCode";
		var ajaxObj=new AjaxObj(url,null,subLoginAjaxCallback);
		
		Pingan.ajax(ajaxObj);
	}
	var subLoginAjaxCallback = function(data){
		if(data.statusCode=="1"){
			$("#toUserAccountSet").empty();
		}
	}  
    $(document).ready(
            function () {
                $.ajax({
                    url: basePath + "/getUserSession",
                    data: null,
                    dataType: "json",
                    type: "POST",
                    async: true,
                    success: function (data) {
                        if (data.statusCode == 1) {
                            //给页面的userId赋值
                            $("#baseCfg").attr("u", data.userId);

                            $("#userCenterHref").html(
                                    "hi," + data.data.userDigestedName
                                    + "<i></i>");
                            $("#noLogin").hide();
                            $("#alreadyLogin").show();
                            //详情页评论模块显示登陆div
                            $("#comment_list_login").show();
                            $("#comment_list_no_login").hide();
                             subLoginAjax(); 
                            return;
                        } else {
                            $("#alreadyLogin").hide();
                            $("#noLogin").show();
                            //详情页评论模块隐藏登陆div
                            $("#comment_list_no_login").show();
                            $("#comment_list_login").hide();
                            
                        }
                    }
                });
            })
		 
  
    $("#searchBtn").click(searchBtnFn);
    $("#searchkey").keydown(function (e) {
        if (e && e.keyCode == 13) {
            searchBtnFn();
        }
    });
   
</script>




