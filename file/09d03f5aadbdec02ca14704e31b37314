<script type="text/javascript">
	var functionName='detail.Ajax.loadChangeAjax';
	function goPage(){
		var page=document.getElementById("pageNo").value;
		if(page==""){
			return;
		}
		page=parseInt(page);
		if(isNaN(page)){
			return;
		}
		//输入的page数如果有误
		if(page<=0){
			page=0;
			document.getElementById("pageNo").value=1;
		}
		var totalPages=parseInt('1');
		if(page>totalPages){
			page=totalPages;
			document.getElementById("pageNo").value=page;
		}
		//npage从0开始,所以此处用page<=totalPages,而不是page<totalPages
		if(page>0 && page<=totalPages){
			page=page-1;
		}
		var fnc=eval(functionName);
		new fnc(page);
	}
	
</script>

                        </div>
                    </div>
                
                
            
        </div>
