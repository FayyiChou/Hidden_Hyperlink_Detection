<script>
var length = 3;
	var step = 1000;
	var c;
	function autoClose(){
		if(length > 0){
			length-- ;
			c = setTimeout("autoClose()",step);
		}else{
			clearTimeout(c);
			if(jQuery("#winmask").css("display")=="block"){
				togglenew1();
			}
		}
	}
	jQuery(document).ready(function(){
		autoClose();
	});
function togglenew2(){
	jQuery("#rightclose").animate({width:"show", opacity:"show"}, "normal" ,function(){
		jQuery("#rightclose").show();
	});
	
	jQuery("#rightclose2").attr("style","display:none");
	}
function togglenew1(){
	Effect.openwin('payshowpnew');
	document.getElementById("rightclose2").style.display = "";
	}
</script>
