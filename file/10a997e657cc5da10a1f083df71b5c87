<script type="text/javascript">

	$(function($) {
		initToken();
	});

	
	function initToken(){
		jQuery.ajax({
			url : '/htzq/tokenAction.do',
			type : 'GET',
			data : {'method':'getToken','key':'TOKEN_TRADE_USER_LOGIN'},
			dataType : 'json',
			success : function(data){
				if(data.success){
					jQuery("#token").attr("name",data.token_key);
					jQuery("#token").val(data.token_value);
				}
			}
			
		});
	}
	
	function changeCode(){
		document.getElementById("verifyCodeImg").src = "/htzq/verifyCode.do?method=getVerifyCode&ran="+Math.random();	
  	}
</script>
