})();$(window).scroll(function(){var top =$(document).scrollTop();var menu =$("#float-nav");var items =$("#z-con").find(".item");var curId ="";items.each(function(){var m =$(this);var itemsTop =m.offset().top;if(top >itemsTop-200){curId ="#" + m.attr("id");}else{return false;}
});var curLink =menu.find(".cur");if(curId &&curLink.attr("href") !=curId ){curLink.removeClass("cur");menu.find("[href=" + curId + "]" ).addClass("cur");}
 });$(window).scroll(function(){var top =$(document).scrollTop();var menu =$("#float1");var items =$("#z-con").find(".item");var cur1Id ="";items.each(function(){var m =$(this);var itemsTop =m.offset().top;if(top >itemsTop-200){cur1Id ="#" + m.attr("id");}else{return false;}
});var cur1Link =menu.find(".cur1");if(cur1Id &&cur1Link.attr("href") !=cur1Id ){cur1Link.removeClass("cur1");menu.find("[href=" + cur1Id + "]" ).addClass("cur1");}
 });</script>
