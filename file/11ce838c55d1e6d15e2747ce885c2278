function resetShopCart(ppid,count,cb){$.post('http://www.228.com.cn/ajax/updateshopcart.json',{productplayid:ppid,num:count},function(data){if (data.status ==1) {alert_suss("提示！","请先选择商品！");}else if (data.status ==2) {alert_suss("提示！","您的购物车已经空了！");}else if (data.status ==3) {alert_suss("提示！","更新购物车失败，请稍候再试！");}else{cb(data);}
});}
	function reloadImage(o){var img =document.getElementById(o);img.src ="http://www.228.com.cn/jsp/common/image.jsp?rm=" + Math.random();$("img[class='imagCode']").each(function(){$(this).attr("src",img.src);});}
