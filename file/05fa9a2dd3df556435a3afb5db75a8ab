  <script type="text/javascript">	
	function addCart(event) {
		var offset = $('#end').offset(), flyer = $('<img style="display: block;width: 50px;height: 50px;border-radius: 50px;position: fixed;z-index: 9999;" src="http://www.macklin.cn/static/images/cartsmall.png"/>');
		flyer.fly({
		    start: {
		        left: event.clientX,
		        top: event.clientY-25
		    },
		    end: {
		        left: offset.left,
		        top: 0,
		        width: 0,
		        height: 0
		    },
		    speed:0.7
		});
	}
	
	function addFav(event) {
		var offset = $('#fav_end').offset(), flyer = $('<img style="display: block;width: 50px;height: 50px;border-radius: 50px;position: fixed;z-index: 9999;" src="http://www.macklin.cn/static/images/star.png"/>');
		flyer.fly({
		    start: {
		        left: event.clientX,
		        top: event.clientY-25
		    },
		    end: {
		        left: offset.left-38,
		        top: 0,
		        width: 0,
		        height: 0
		    },
		    speed:0.7
		});
	}
	
	function changeLang(lang){
		$.ajax({
		   type 	: "get",
		   url 		: base_url + "/ajax/changLang/" + lang,
		   cache	: false,
		   success: function(response) {
						location.reload();
		   			}
		});
	}
</script>   
