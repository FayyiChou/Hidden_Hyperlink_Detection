  <script>
	$(function() {
		$("li.mytuniuArea").hover(function () {
							var obj_id = $(this).attr("id");//获得触发的id
				$("#"+obj_id).addClass('rc_hover');
			        }, function () {
            $(this).removeClass('rc_hover');
        });
	});
	$(function() {
		$("li.rc_li,li.rcBackToTopSty").hover(function () {
            $(this).addClass('rc_hover');
        }, function () {
            $(this).removeClass('rc_hover');
        });
	});
	$(function() {
		$("li.hoverClick").hover(function () {
            $(this).addClass('rc_mouseover');
        }, function () {
        	        	//alert($(this).attr('id'));
            $(this).removeClass('rc_mouseover');
            $(this).removeClass('rc_click');
        });
	});
	$(function() {
		$("li.hoverClick").click(function () {
			if($(this).attr('class') == 'hoverClick' || $(this).attr('class') == 'hoverClick rc_click' || $(this).attr('class') == 'hoverClick rc_mouseover rc_click' || $(this).attr('class') == 'hoverClick rc_click rc_mouseover'){
				return;
			}
			//alert($(this).attr('class'));
			//销毁之前加载的div
							//未登录,先保存已输入的内容
				var e_name = $(this).find('#email').val();
				var e_password = $(this).find('#password').val();
				$(this).find('div.rc_click_event').remove();//销毁div
						var obj_id = $(this).attr("id");//获得触发的id
			$("#"+obj_id).removeClass('rc_mouseover');
			var idJson = {
				id:obj_id,
			}
			$.ajax({
			   type: "post",
			   url: base_url + "/ajax/right_common",
			   data: idJson,
			   cache : false,
			   success: function(responseHTML) {
			       	$("#"+obj_id).append(responseHTML);
			       			       		$("#"+obj_id).find('#email').val(e_name);
		       		$("#"+obj_id).find('#password').val(e_password);
		       		           			$("#"+obj_id).addClass('rc_click');
			   }
			});
        });
	});
	
	//右侧快速登录
	function right_login(obj){
		//alert($(obj).parent().parent().attr("id"));
		var email = $(obj).parent().parent().find("input[id$='email']").val();
		var password = $(obj).parent().parent().find("input[id$='password']").val();
		var loginJson = {
			email:email,
			password:password,
		}
		$.ajax({
		   type: "post",
		   url: base_url + "/ajax/login",
		   data: loginJson,
		   cache : false,
		   dataType : 'json',
		   success: function(responseJSON) {
		       	if(responseJSON.result){
		       		window.location.reload();//刷新当前页面.
		       	}else{
		       		$("p.help-block").html(responseJSON.error);
		       	}
		   }
		});
	}
	
	//右侧快速退出
	function right_logout(){
		$.ajax({
		   type: "post",
		   url: base_url + "/ajax/logout",
		   data: {},
		   cache : false,
		   dataType : 'json',
		   success: function(responseJSON) {
		       	if(responseJSON.result){
		       		window.location.reload();//刷新当前页面.
		       	}else{
		       		alert('退出失败，请重新尝试！');
		       	}
		   }
		});
	}
	
	//关闭快速订购
	function close_easybuy(){
		$("#easyBuy").removeClass('rc_mouseover');
        $("#easyBuy").removeClass('rc_click');
        return false;
	}
</script> 
