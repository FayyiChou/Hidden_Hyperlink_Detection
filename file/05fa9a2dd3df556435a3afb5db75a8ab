  <script>
	$().ready(function() {
    	$('#searchspiritinput').focus(function() {
    		$('.rep-search .search-bg').css('background-color','#1BABF8');
		    $('.adv-search').show();
		    $(document).bind('focusin.adv-search click.adv-search',function(e) {
		        if ($(e.target).closest('.adv-search, #searchspiritinput').length) return;
		        $(document).unbind('.adv-search');
		        $('.rep-search .search-bg').css('background-color','#c0c0c0');
		        $('.adv-search').fadeOut('medium');
		    });
		});

		$("#searchspiritinput").keyup(function(){
			$('.rep-search .search-bg').css('background-color','#c0c0c0');
			$('.adv-search').hide();
		});
    });
</script> 
