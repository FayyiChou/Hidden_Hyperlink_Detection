<script src="/front/yangquanshiqu/js/jquery.cookie.js" type="text/javascript"></script>
<script type="text/javascript">
	$(document).ready(function(){
		var browserUptTip = $.cookie("browserUptTip");
		var userAgent = navigator.userAgent;
		//ie 
		if (!!window.ActiveXObject || "ActiveXObject" in window){
			//alert("ie");
			$(".browserUptTip2").hide();
		}
		//firefox 
		else if (userAgent.indexOf("Firefox") > -1) {
			//alert("Firefox");
			$(".browserUptTip2").hide();
		}
		//Chrome
		else if(userAgent.indexOf("Chrome") > -1 && userAgent.indexOf("Safari") > -1 && userAgent.indexOf("QQBrowser")==-1){
			//alert("Chrome");
			$(".browserUptTip2").hide();
		}else{
			if(browserUptTip==undefined||browserUptTip==""||browserUptTip=="false"||browserUptTip==false){
				$(".browserUptTip2 .browUptTipCloseBtn").click(function(){
					if(confirm("关闭提示条，1个月内将不再显示，确定关闭吗？")){
						$(this).parent().hide();
						$.cookie("browserUptTip","true",{expires:30,path:"/",domain:'www.yqedu.cn'});
					}
				}).parent().show();
				$(".browserUptTip2 .browserUptDownBtn").click(function(){
					var form=$("<form>");//定义一个form表单
					form.attr("style","display:none");
					form.attr("target","");
					form.attr("method","post");
					form.attr("action","/browser/chrome.zip");
					$("body").append(form);//将表单放置在web中
					var input1=$("<input>");
					input1.attr("type","hidden");
					input1.attr("name","exportDate");
					input1.attr("value",(new Date()).getMilliseconds());
					form.append(input1);
					form.submit();//表单提交
					form.remove(); 
					$.cookie("browserUptTip","true",{expires:30,path:"/",domain:'www.yqedu.cn'});
					$(this).parent().hide();
				});
			}
		}
		var footWrap=document.getElementById('footWrap');
		$(window).resize(function(){  
			var winHeight = $(window).height();
			var contentHeight = document.body.scrollHeight;
			if(contentHeight>winHeight){
				footWrap.style.position="relative";
			}else{
				footWrap.style.position="absolute";
				footWrap.style.width="100%";
			}
	    });
		$("body").bind('DOMNodeInserted', function(e) {
			var winHeight = $(window).height();
			var contentHeight = document.body.scrollHeight;
			if(contentHeight>winHeight){
				footWrap.style.position="relative";
			}else{
				footWrap.style.position="absolute";
				footWrap.style.width="100%";
			}
		});
	});
</script>
<div class="footer" id="footWrap">
</div>
