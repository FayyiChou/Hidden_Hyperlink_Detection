<script type="text/javascript">
//导航
$('.head li').hover(function(){
$(this).find('a.tit').addClass('readerhover');
$(this).find('.navTop').show();	
},function(){
$(this).find('a.tit').removeClass('readerhover');	
$(this).find('.navTop,.navSearch').hide();
});
//搜索 按钮
$('.s_btn').hover(function(){
	$(this).removeClass().addClass('s_btn11');							 							 
},function(){
	$(this).removeClass('s_btn11').addClass('s_btn');							 
	
});
//导航搜索表单
$('.navSearch label input').disappearDefault({
	vals:"",
	defaultColor:"#FFF",
	color:"#FFF",
	showDefault:true
});
//替换url中的参数的特殊、复杂参数值，中文、url链接等等做参数值时都需要这样处理。
function replaceParam(obj,attr,datas){
	var attrValue = obj[attr]==undefined?obj.getAttribute(attr):obj[attr];
	if(datas!=null&&datas!=undefined){
		for(var i in datas){
			if(i!=undefined){
				//href属性写法
				//attrValue = attrValue.replace(eval("/%5B"+i+"%5D/g"),encodeURIComponent((datas[i])));
				//非href属性写法
					//编码写法 attrValue = attrValue.replace(eval("/\\["+i+"\\]/g"),encodeURIComponent(datas[i]));
					//不编码写法
					attrValue = attrValue.replace(eval("/\\["+i+"\\]/g"),datas[i]);
			}
		}
	}
	if(obj[attr]!=undefined){
		obj[attr] = attrValue;
	}else{
		obj.setAttribute(attr,attrValue);
	}
}
//正式通知公告（非新闻分类）的显示
$(document).ready(function(){
	 $.ajax({ 
		type: "get", 
		url : "/api/front/notices/curList",
		dataType:'html',
		data: 'date='+new Date(), 
		async:true,
		success: function(data){
			if($.trim(data)!=''){
				$('#ads_wrap>ul').html(data);
				var noticesWindClose = $.cookie("noticesWindClose");
				if(noticesWindClose==undefined||noticesWindClose==""){
					$.cookie("noticesWindClose","false",{expires:1,path:"/",domain:'www.yqedu.cn'});
					$('.maskLayer').show();
					$('#ads_wrap').addClass("ads_wrap_show").show();
					$("#adsClose").removeClass("adsOpen").addClass("adsClose");
				}else{
					$.cookie("noticesWindClose","true",{expires:1,path:"/",domain:'www.yqedu.cn'});
					$('#ads_wrap').removeClass("ads_wrap_show").addClass("ads_wrap_hide").show();
					$('#ads_wrap').css({"top":$(window).height()-37,"width":"300px","position":"fixed"});
					$("#adsClose").removeClass("adsClose").addClass("adsOpen");
				}
				$("#adsClose").click(function(){
					if($(this).hasClass("adsClose")){
						$('#ads_wrap').removeClass("ads_wrap_show").addClass("ads_wrap_hide").css("top","100%");
						$('#ads_wrap').css({"top":$(window).height()-37,"width":"300px","position":"fixed"});
						$('.maskLayer').hide();
						$(this).removeClass("adsClose").addClass("adsOpen");
						$.cookie("noticesWindClose","true",{expires:1,path:"/",domain:'www.yqedu.cn'});
					}else{
						$('#ads_wrap').removeClass("ads_wrap_hide").addClass("ads_wrap_show").css({"top":"30%","width":"600px"});
						$('.maskLayer').show();
						$(this).removeClass("adsOpen").addClass("adsClose");
						$.cookie("noticesWindClose","false",{expires:1,path:"/",domain:'www.yqedu.cn'});
					}
				});
			}
		}	
	});
});
</script>
