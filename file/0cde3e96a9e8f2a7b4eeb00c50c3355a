var showListSub = function(pid){
	$.ajax({ 
		type: "get", 
		url : "/api/front/index/showListSubFrontMenu?pid="+pid,
		dataType:'html',
		data: '', 
		async:true,
		success: function(data){
			data = data.replace(/(^\s*)|(\s*$)/g,"");
			var str = document.getElementById("frontMenu"+pid).innerHTML;
			document.getElementById("frontMenu"+pid).innerHTML = str+data;
		} 
	});
};

var showRowSub = function(pid){
	$.ajax({ 
		type: "get", 
		url : "/api/front/index/showRowSubFrontMenu?pid="+pid,
		dataType:'html',
		data: '', 
		async:true,
		success: function(data){
			data = data.replace(/(^\s*)|(\s*$)/g,"");
			var doc = document.getElementById("submenuRow");
			doc.innerHTML = data;
			if(doc.innerHTML==''){
				document.getElementById("submenuRowshow").style.display="none";
			}else{
				document.getElementById("submenuRowshow").style.display="block";
				var sunchannelid = '';
				if(sunchannelid!=null&&sunchannelid!=''){
					document.getElementById("subrowlistli"+sunchannelid).className="cur";
				}
			}
		} 
	});
};

var loginout=function(){
window.location.href='/api/front/localfexit';
};
var changeChannel =function(tourl,channelid,sunchannelid,openBlank,needLogin){
	
	//fromUrl = window.location.href;
	var refer = "http://"+'www.yqedu.cn'+"/api/front/setFrongSession?fromUrl="+tourl;
	
	var domain = 'www.yqedu.cn';
	
	if(domain=='aoxiang.bj80.chaoxing.com'){
		
		if(tourl!='/api/front/index/first'){
			$.ajax({ 
				type: "get", 
				url : "/api/front/changeChannel?channelid="+channelid+"&tourl="+tourl+"&sunchannelid="+sunchannelid+"&date="+new Date(),
				dataType:'html',
				data: '', 
				async:true,
				success: function(data){
					needLogined(tourl,openBlank);
				} 
			});
		}else{
			window.location.href='/api/front/index/first';
		}
		
		
	}else{
	 if(tourl=='zyvideo.whjy.net/author/remoteLogin.jspx'){
	 $.ajax({ 
				type: "get", 
				url : "/api/front/joinWhjy",
				dataType:'json',
				data: 'date='+new Date(), 
				async:true,
				success: function(data){
				if(data.success==true){
				tourl='http://'+tourl+'?userid='+data.userid+'&key='+data.key;
				}
			  if(needLogin&&needLogin!=undefined&&needLogin==1){
					needLogined(tourl,openBlank);
				}else{
					if(openBlank&&openBlank!=undefined&&openBlank==1){
				 		window.open(tourl);
				 	}else{
				    	window.location.href=tourl;
				    }
				}
				} 
			});
	 }
	else{
		$.ajax({ 
			type: "get", 
			url : "/api/front/changeChannel?channelid="+channelid+"&tourl="+tourl+"&sunchannelid="+sunchannelid+"&date="+new Date(),
			dataType:'html',
			data: '', 
			async:true,
			success: function(data){
				if(needLogin&&needLogin!=undefined&&needLogin==1){
					needLogined(tourl,openBlank);
				}else{
					if(openBlank&&openBlank!=undefined&&openBlank==1){
				 		window.open(tourl);
				 	}else{
				    	window.location.href=tourl;
				    }
				}
			} 
		});
		}
	}
	
	
	
	
	
};
</script>
