<script>
	$(document).ready(function(){
		$.ajax({ 
			type: "get", 
			url : "/api/front/index/getResourceRank?date="+new Date().getTime(),
			dataType:'html',
			data: '', 
			async:true,
			success: function(data){
				data = data.replace(/(^\s*)|(\s*$)/g,"");
				$("#rightResourceRank").html(data);
			} 
		});
		$("div.gxzyRes").each(function(index,el){
			if($(el).find(".menuLi").length>0){
				var $div_li =$(el).find("ul li");
				$div_li.hover(function(){
					$(this).addClass("selected").siblings().removeClass("selected");  //去掉其它同辈<li>元素的高亮
           		var index1 =  $div_li.index(this);  // 获取当前点击的<li>元素 在 全部li元素中的索引。
           		 	$(el).parent().parent().find(".tab_box > div").eq(index1).show().siblings().hide();
				});
			};
			if($(".gxzyRes>ul>li").length>1){
				$(".gxzyRes>ul>li:first").addClass("selected").trigger("mouseover").siblings().removeClass("selected");
			}else{
				$(".gxzyRes>div").show();
			}
		});
		$(".gxzyRes>ul>li").each(function(i,ele){
			$.ajax({ 
			 type: "get", 
				url : "/api/front/index/getYQIndexRecRes?date="+new Date().getTime(),
				dataType:'html',
				data: {eduLevel:$(ele).attr("edul")},
				async:true,
				success: function(data){
					data = data.replace(/(^\s*)|(\s*$)/g,"");
					if($(ele).attr("edul")=='1'){
						$("#leftSharedresXiao").html(data);
					}else if($(ele).attr("edul")=='2'){
						$("#leftSharedresChu").html(data);
					}else if($(ele).attr("edul")=='3'){
						$("#leftSharedresGao").html(data);
					}
				} 
			});
		})
	});
</script>
