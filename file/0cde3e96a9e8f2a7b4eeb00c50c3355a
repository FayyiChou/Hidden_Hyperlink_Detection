<script>
	$.ajax({ 
		type: "get", 
		url : "/api/front/index/getAppStore?date="+new Date().getTime(),
		dataType:'html',
		data: '', 
		async:true,
		success: function(data){
			data = data.replace(/(^\s*)|(\s*$)/g,"");
			$("#appsData").html(data);
			left_right_scroll();
		} 
	});
	function appAccessLog(appId,appName){
		$.ajax({ 
			type: "post", 
			url : "/api/front/index/appAccessLogForYQ?date="+new Date(),
			dataType:'json',
			data: {appId:appId,appName:appName}, 
			async:true,
			success: function(data){
				//alert("app访问日志记录成功！");
			} 
		});
	}
	function left_right_scroll(){
		//切换
		$('.s_right').each(function(index, element) {
			var $parent = $(this).parents('.content')
				,	$uls =  $parent.find('ul')
				,	parent = $parent[0];
	    	parent.len = $uls.length;
			parent.now = 0;
			parent.width = $uls.outerWidth();
			$(this).click(function(){
				parent.now++;
				if(parent.now>parent.len-1){
					parent.now=parent.len-1;
					return;
				}
				parent.now===parent.len-1?$(this).css('backgroundPosition','-30px 0'):$(this).css('backgroundPosition','-30px -45px');
				parent.now===0?$(this).siblings('.s_left').css('backgroundPosition','0 0'):$(this).siblings('.s_left').css('backgroundPosition','0 -45px');
				$parent.find('.srcoll').stop(true).animate({marginLeft:-parent.now*parent.width+'px'});
			});
	  	});
		$('.s_left').click(function(){
			var $parent = $(this).parents('.content')
				,	parent = $parent[0];
			parent.now--;
			if(parent.now<0){
				parent.now=0;
				return;
			}
			parent.now===parent.len-1?$(this).siblings('.s_right').css('backgroundPosition','-30px 0'):$(this).siblings('.s_right').css('backgroundPosition','-30px -45px');
			parent.now===0?$(this).css('backgroundPosition','0 0'):$(this).css('backgroundPosition','0 -45px');
			$parent.find('.srcoll').stop(true).animate({marginLeft:-parent.now*parent.width+'px'});
		});
	};
</script>
