<script>

//<![CDATA[
	$(function(){
		$("div.szlgRes").each(function(index,el){
			if($(el).find(".menuLi").length>0){
				var $div_li =$(el).find("ul li");
				$div_li.hover(function(){
					$(this).addClass("selected").siblings().removeClass("selected");  //去掉其它同辈<li>元素的高亮
           		var index1 =  $div_li.index(this);  // 获取当前点击的<li>元素 在 全部li元素中的索引。
           		 	$(el).parent().parent().find(".tab_box > div").eq(index1).show().siblings().hide();
				});
			};
		});
	});
//]]>

function peoplemore(roleId){
	window.open("/api/front/index/more?roleId="+roleId);
}
function classmoree(){
	window.open("/api/front/index/classmore");
}
</script>
