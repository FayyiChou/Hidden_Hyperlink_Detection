 <script type="text/javascript">
 
 /**
 	根据机构空间类型，不同的登录方式
 */
var logintype = '1';
if(logintype=='1'){
	 $("#loginStart").click(function(){
	 	showLoginDialoge();
	});
}else if(logintype=='2'){
	$('#loginStart').click(function () {
		fromUrl = window.location.href;
		var refer = "http://"+'www.yqedu.cn'+"/api/front/setFrongSession?fromUrl="+fromUrl;
		refer=escape(encodeURI(refer));
		window.location.href='http://passport.yqedu.cn/login?fid=1057&refer='+refer;
	});
}
 var needLogined = function(tourl,openBlank){
 	 if(tourl==undefined||tourl.replace(/ /g,"")==''){
 	 	tourl = window.location.href;
 	 }
 	 if(tourl!=undefined&&tourl.replace(/ /g,'')!=''){
		tourl = tourl.replace(/\${sessionScope\.FRONT_USER\.name}/g,escape(''));
		tourl = tourl.replace(/\${sessionScope\.FRONT_USER\.displayName}/g,escape(''));
	 }
	 var frontu = '';
	 if(frontu==''||frontu=='null'||frontu==null||frontu=='undefined'||frontu==undefined){
	    showLoginDialoge(); 
	 }else{
	 if(openBlank&&openBlank!=undefined&&openBlank==1){
	 		window.open(tourl);
	 }else{
	    	window.location.href=tourl;
	   }
	 }
};
 
 
 var showLoginDialoge = function(){
 		var codeUrl = $("#numVerCode").attr("src");
 		$("#numVerCode").attr("src",codeUrl.indexOf("?")>0?codeUrl+"&_t="+new Date().getTime():codeUrl+"?_t="+new Date().getTime());
		$("#loginBox").show();
		$(".maskLayer").show();			
    $("#loginCloseBtn").click(function(){
		$("#loginBox").hide();
		$(".maskLayer").hide();		
	});
 }; 
 var showWrongMessage = function(){
 		var msg = '';
		var $errTip = $("#err_m");
		$errTip.show();
		if(msg!=null&&msg!=""&&msg=="noexist"){
			$errTip.html("当前机构下不存在该用户！");
		}else{
	        $errTip.html(msg&&msg!=""?msg:"用户名或密码错误");
		}
        setTimeout(function () { $errTip.empty();$errTip.hide(); }, 5000);
	};
	
	/* loginfailed 
	   1:弹窗登录，用户名或者密码有误
	   2：这个页面必须登录才可以，有框弹框，无框跳转
	*/
	
 var flag = '';
 if(flag!=''&&flag!=undefined&&flag=='1'){
	  showLoginDialoge();
	  showWrongMessage();
 }else if(flag!=''&&flag!=undefined&&flag=='2'){
	 var temp = window.location.href;
	 temp = temp.replace("loginfailed=2","");
	 needLogined(temp);
 }
 
 
 
</script>
