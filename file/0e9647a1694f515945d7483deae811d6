        <script type="text/javascript">
	    initLogin(0)
	    function initLogin(num){
	        $('.tabLg').eq(num).css('display','block').siblings('.tabLg').css('display','none');
	    }
        var T = 180,Tout;
        function countDown(obj, tTime){
            if(!$('#'+obj).hasClass('Time')){
                //clearInterval(Tout);
                $('#'+obj).html('('+T+'S)重新获取');
                $('#'+obj).addClass('Time');
                Tout = setInterval(function(){
                    if(T == 0){
                        clearInterval(Tout);
                        $('#'+obj).html('获取验证码');
                        $('#'+obj).removeClass('Time')
                        T = tTime;
                    }else{
                        T--;
                        $('#'+obj).html('('+T+'S)重新获取');
                    }
                },1000);
            }else{
                if(T == tTime){
                    clearInterval(Tout);
                }
            }
        }
            function styleClk(obj) {
                var p = $(obj).parents('.tabForm');
                var T = 0;
                var time = 0,
                    prev = $(p).prev('.tabForm'),
                    next = $(p).next('.tabForm');

                if($(prev).length > 0) {
                    $(prev).css('display', 'block');
                    $(p).css('display', 'none');
                    $('#loginScan').css('display', 'none');

                } else {
                    $(next).css('display', 'block');
                    $(p).css('display', 'none');
                    $('#loginTab').css('display', 'none');

                    $.ajax({
                        type: "GET",
                        url:  "https://passport.cnfol.com/userlogin/getouid?appid=zjd007905e50109284&sid=1&tid=1491050768&rt=aHR0cDovL21wLmNuZm9sLmNvbS9Mb2dpbi9pbmRleA==",
                        dataType: "json",
                        cache: false,
                        success: function(minfo) {
                            var ouid = minfo.msg;
                            if(minfo.flag == 1) {
                                if(ouid && ouid != "") {
                                    $('.LerBigImg').attr('src', 'https://passport.cnfol.com/userlogin/qrcode/'+ouid+'?t=1491050768');
                                    timeoutName = setTimeout(function(){qrCodeScan(T, ouid);},5000);//flag!=1，则5秒后继续发送ajax请求

                                } else {
                                    alert('参数错误，请刷新');
                                    return;
                                }
                            } else {
                                alert(ouid);
                                return;
                            }

                    clearInterval(T);
                },
                    error: function(XMLHttpRequest, textStatus, errorThrown) {
                        alert('数据提交失败，请重新提交！');
                        return;
                    }
                });
                
            }
            
        }
        
        function formClk(obj){
            var p = $(obj).parents('.tabLg');
            $(p).css('display','none').siblings('.tabLg').css('display','block');
            $('.tabTip').prev('h3').css('visibility','visible');
            $('.tabTip').css('display','none');
        }
        function qrCodeScan(t, ouid){
            var  st = $('#loginScan').css('display');
            if(st=='none'){
                return false;
            }
             $.ajax({
               type: "GET",
               url:  "https://passport.cnfol.com/userlogin/qcscan/"+ouid,
               dataType: "json",
               timeout:5000,
               error:function(){
                     alert('数据提交失败，请重新提交！');
                     return;
               },
               success:function(data){
                  var flag=data.flag;
                  if(flag==1){
                      $('.LerSpan').html('已扫描，请在手机端点确定登录');
                      timeoutName =setTimeout(function(){qrCodeConfirm(t, ouid);},3000);//flag!=1，则2秒后继续发送ajax请求
                  }else if(flag == 5 || flag == 4){
                      timeoutName = setTimeout(function(){qrCodeScan(t, ouid);},5000);//flag!=1，则2秒后继续发送ajax请求
                  } else {
                      clearTimeout(timeoutName);
                      $('.LerSpan').html('参数错误，请重新扫描');
                  }
                  clearInterval(t);
                  
                       }

                })
            }

            function qrCodeConfirm(t, ouid) {
                var st = $('#loginScan').css('display');
                if(st == 'none') {
                    return false;
                }
                $.ajax({
                    type: "GET",
                    url: "https://passport.cnfol.com/userlogin/qcconfirm/"+ouid,
                    dataType: "json",
                    timeout: 5000,
                    error: function() {
                        alert('数据提交失败，请重新提交！');
                        return;
                    },
                    success: function(data) {
                        var flag = data.flag;
                         if(flag==1){
                            qrCodeLogin(ouid);
                         }else if(flag == 5){
                             timeoutName = setTimeout(function(){qrCodeConfirm(t, ouid);},5000);//flag!=1，则2秒后继续发送ajax请求
                         } else {
                             clearTimeout(timeoutName);
                             $('.LerSpan').html('登录失败，请重新扫描');
                         }

                        clearInterval(t);
                    }
                })
            }

            function qrCodeLogin(ouid) {

                $.ajax({
                    type: "GET",
                    url:  "https://passport.cnfol.com/userlogin/qclogin/"+ouid,
                    dataType: "json",
                    cache: false,
                    success: function(minfo) {
                        //alert(minfo.toUrl);
                        //return;
                        if(minfo.flag == 1) {
                            //alert(minfo.toUrl);
                            window.location.href = minfo.toUrl;
                        } else {
                            alert('登录失败');
                            window.location.href = minfo.toUrl;
                        }
                    },
                    error: function(XMLHttpRequest, textStatus, errorThrown) {
                        alert('数据提交失败，请重新提交！');
                        return;
                    }
                });
            }

            inpFocus('#username');
            inpBlur('#username');
            inpFocus('#password');
            inpBlur('#password');

            function inpFocus(obj) {
                if($(obj).prevAll().length > 0) {
                    $(obj).prev().css('display', 'none');
                } else {
                    $(obj).css('display', 'none');
                    $(obj).next().focus();
                }
            }

            function inpBlur(obj) {
                if($(obj).val() == '') {
                    $(obj).prev().css('display', 'block');
                }
                $(obj).parent().prev().html()
            }

            function errorStr(num) {
                var str = '';
                switch(num) {
                    case 0:
                        {
                            str = '请输入您的帐号！'
                            break;
                        }
                    case 1:
                        {
                            str = '密码不能为空!';
                        }
                }
                return str;
            }

            //绑定回车键
            document.body.onkeydown = function(e) {
                e = e || window.event;
                if(e.keyCode==13){
	            	var form = $('form:visible').attr('id');
	            	if(form == 'smsForm'){
	            		smsFormSub();
	            	} else {
	            		formSubmit();
	            	}
            	}
        	}

            function formSubmit() {

                var flag = true,
                    num = -1;
                if($('#username').val() == '') {
                    flag = false;
                    num = 0;
                } else if($('#password').val() == '') {
                    flag = false;
                    num = 1;
                }

                if(!flag) {
                    str = errorStr(num);
                    $('.tabTip').prev('h3').css('visibility', 'hidden');
                    $('.tabTip').css('display', 'block');
                    $('.tabTip').children('em').html(str);
                }
                if(flag) {
                    $('#loginform').submit();
                }
            }
			
			

        /*===================================================*/
        //$('#loginMobile').val('');
        $('#loginCode').val('');
        function tipShow(txt){
            $('.tabTip').prev('h3').css('visibility','hidden');
            $('.tabTip').css('display','block');
        	$('.tabTip').children('em').html(txt);
        	return false;
        }
        var toolTest={
                IsPhoneNum:function(str){
                        var reg = /^(((13[0-9]{1})|(15[0-9]{1})|(17[0-9]{1})|(18[0-9]{1}))+\d{8})$/;
                        return reg.test(str)||this.IsHongNum(str);    
                    },
                IsHongNum:function(str){
                        var reg=/^00852(\d{8})$/;
                        return reg.test(str);
                },
                ValMsgCode:function(str){
                        return str.length==6&&parseInt(str);
                    }
            };
        //20161212 start
        function getCode(){
        	
            if($('#getCode').hasClass('Time')){
                return false;
            }

            if($('#getCode').hasClass('disClick')){
            	tipShow('请求已提交，请耐心等待！');
                return false;
            }

            var loginMobile = $('#loginMobile').val();
            if(!(toolTest.IsPhoneNum(loginMobile)))
            {
                tipShow("请输入正确的手机号！");
                return false;
            }

            $('#getCode').addClass('disClick');
            $.ajax({
                url:  "https://passport.cnfol.com/userlogin/sendlogincode",
                cache:false,
                type: 'post',
                dataType: 'json',
                data: 'tid=1491050768&mid=pcec8e099020372198&mobile=' + loginMobile,
                success : function(minfo){
                     $('#getCode').removeClass('disClick');
                     if(minfo.flag == '10000'){
                         tipShow(minfo.msg);
                         $('#tstr').val(minfo.info['tstr']);
                         $('#vstr').val(minfo.info['vstr']);

                         countDown('getCode', 180);

                     } else {
                            tipShow(minfo.msg);
                         return false;
                     }
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    $('#getCode').removeClass('disClick');
                    alert('数据提交失败，请重新提交！');
                    return;
                }
            });
        }
        
        function smsFormSub(){
            var loginMobile = $('#loginMobile').val();
            if(!(toolTest.IsPhoneNum(loginMobile)))
            {
                tipShow("请输入正确的手机号！");
                return false;
            }
            var tstr = $('#tstr').val();
            var vstr = $('#vstr').val();
            if(!tstr || !vstr){
                tipShow("请重新获取验证码！");
                return false;
            }
            var loginCode = $('#loginCode').val();
            if(!(toolTest.ValMsgCode(loginCode)))
            {
                tipShow("验证码输入不对！");
                return false;
            }
            if($('#subBtn').hasClass('disClick')){
                tipShow("请求已提交，请耐心等待！");
                return false;
            }
            $('#subBtn').addClass('disClick');
            $.ajax({
                url:  "https://passport.cnfol.com/userlogin/verifylogincode",
                cache:false,
                type: 'post',
                dataType: 'json',
                data: 'tid=1491050768&mid=pcec8e099020372198&rt=aHR0cDovL21wLmNuZm9sLmNvbS9Mb2dpbi9pbmRleA==&mobile=' + loginMobile+'&tstr=' + tstr+'&vstr=' + vstr+'&code='+loginCode,
                success : function(minfo){
                     $('#subBtn').removeClass('disClick');
                     if(minfo.flag == '10000'){
                        //alert(minfo.info);
                        window.location.href= minfo.info;
                     } else {
                         tipShow(minfo.msg);
                         return false;
                     }
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    $('#subBtn').removeClass('disClick');
                    alert('数据提交失败，请重新提交！');
                    return;
                }
            });
        }
        $("#subBtn").click(function(){
        	smsFormSub();
        });
        
        //20161212 end
        </script>
