		function eTOc(str){

	   var arr = new Array("!","@","#","\\$","\%","\\^","\&","\\*","\\(","\\)","\\_","\\+","\-","\=","\\{","\\}","\\|","\\[","\\]","\\\\",";","'",":","\"","\<","\>","\\?","\,","\\.","\/");
	   var arr1 = new Array("！","◎","＃","￥","％","……","※","×","（","）","——","＋","－","＝","『","』","§","【","】","÷","：","","；","'","《","》","？","，","\.","、");
	   var _str = str;

	   for(var i=0;i<arr.length;i++){
		 var re = new RegExp(arr[i],"img");
		 _str = _str.replace(re,arr1[i]);
	   }

		return _str;

	}
	function search_check() {
		var title = eTOc(document.getElementById("searchword1").value.replace(/^\s+|\s+$/g,""));
		var searchword1 = ""
		var titleArray = title.split(" ");
		if(title != "")
		{
			for(var i=0; i<titleArray.length;i++)
			{
				if(i==0)
					searchword1 += titleArray[i];
				else
					searchword1 += " and " + titleArray[i];								
			}
		}

		document.searchform.searchword.value = searchword1;

		if (document.searchform.searchword1.value=='' || document.searchform.searchword1.value=='请输入关键字') {
			alert('请输入关键字!');
			return false;
		} else {	
			document.searchform.action();
			return true;
		}
	}
