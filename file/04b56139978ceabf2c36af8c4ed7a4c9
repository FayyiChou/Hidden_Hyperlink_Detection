<script>
function addFavor(board)
{
    if(!confirm("确认将本版加入收藏？"))
    {
        return false;
    }
    Request.sendPOST("/member/favor/add", "board="+board, addFavorDone);
}
function addFavorDone(originalRequest)
{
    var result = eval('('+originalRequest.responseText+')');
    alert(result.info);
}
</script>
