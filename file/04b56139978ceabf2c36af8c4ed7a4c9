										<script language="JavaScript">
<!-- //
var EndTime=1491048287000;
var NowTime = new Date();
var dTime = 1491048287000-NowTime.getTime();
var tt;

function DealNum(num) {
	if(num < 10) {
		return '0'+num;
	} else {
		return num;
	}
		
}

function GetRTime(){
	var trip;
	var NowTime = new Date();
	var nMS = EndTime - NowTime.getTime()-dTime;
	if(window.closed) {
		clearTimeout();
		return false;
	}	
	if(nMS <= 0) {
		trip = '您可能还没有登录，或者可能已经登录超时了，建议您在点击回复前，先复制一下回复的内容，<a href="/login?backUrl=L3RocmVhZC82OC8zMjcvMjAxNzAzMzEvNThkZGUyZDk2NDUxYjJkMTMtMS5odG1s" class="loginlink">点此登录</a>，<a href="https://reg.99.com/NDUser_Register_New.aspx?url=http://bbs.99.com/thread/68/327/20170331/58dde2d96451b2d13-1.html" class="reglink">点此注册</a>';
		clearTimeout(tt);
		document.getElementById("state_trip").innerHTML = trip;
		return false;
	} else {
		var nM=DealNum(Math.floor(nMS/(1000*60)) % 60);
		var nS=DealNum(Math.floor(nMS/1000) % 60);
		trip = '温馨提示：请在<span style="color:red">'+nM+'</span>分<span style="color:red">'+nS+'</span>秒内提交，否则可能丢失您刚刚输入的内容';
	}
	document.getElementById("state_trip").innerHTML = trip;
	tt = setTimeout("GetRTime()",1000);
}
GetRTime();
// -->
</script>
