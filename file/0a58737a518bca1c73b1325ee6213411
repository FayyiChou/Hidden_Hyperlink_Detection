<script src="/Public/Home/js/index.js"></script>
<script type="text/javascript">
//	$(".top").click(function(){
//		 $("#dt").slideDown(500);
//	});
//	$("#dt1").click(function(){
//		 $("#dt").slideUp(500);
//	});
	$(document).ready(function(){	
	var n=$('#new-1-13:visible .p').length;
	$('#new-1-13').css({height:142*n});
	var n1=0;
	var dir=1;
	$('#nnn').click(function(){
		n1+=dir;
		$('#new-1-13:visible').animate({marginTop:-142*n1});
		if(n1==n-5 || n1==0){
			$('#nnn').css('transform','rotate('+(dir==1?180:0)+'deg)');
			$('#nnn').css('-moz-transform','rotate('+(dir==1?180:0)+'deg)');
			$('#nnn').css('-webkit-transform','rotate('+(dir==1?180:0)+'deg)');
			$('#nnn').css('-o-transform','rotate('+(dir==1?180:0)+'deg)');
			$('#nnn').css('-ms-transform','rotate('+(dir==1?180:0)+'deg)');
			dir*=-1;
		}
	});
	});


	$('.video').click(function(){
		$('.shipin').fadeIn(500);
		document.getElementsByTagName('video')[n].play();	
	});
	$('#s1').click(function(){
		$('.shipin').fadeOut(500);
		document.getElementById('v1').pause();
	});


   $(".hua1").mouseover(function(){
   		$("#dt").stop(true,true).slideDown(500);  
    }).mouseout(function(){
    	$("#dt").slideUp(500);	
    });


	
	$(".div3").hide();
	$(".div4").hide();
	$(".aa2").click(function(){
		$(this).addClass("gdbe").siblings().removeClass("gdbe");
		$(".div2").show();
		$(".div3").hide();
		$(".div4").hide();
	});
	$(".aa3").click(function(){
		$(this).addClass("gdbe").siblings().removeClass("gdbe");
		$(".div2").hide();
		$(".div3").show();
		$(".div4").hide();
	})
	$(".aa4").click(function(){
		$(this).addClass("gdbe").siblings().removeClass("gdbe");
		$(".div2").hide();
		$(".div3").hide();
		$(".div4").show();
	})
	
	$("#tc2").click(function(){
		$("#tc").fadeIn(1000);
	});
	$("#tc1").click(function(){
		$("#tc").fadeOut(1000);
	});
	
	
	$("#xw2").hide();
	$("#xw3").hide();
	$(".axw2").click(function(){
		$(this).addClass("m-top-this").siblings().removeClass("m-top-this");
		$("#xw1").hide();
		$("#xw2").show();
		$("#xw3").hide();
	});
	$(".axw3").click(function(){
		$(this).addClass("m-top-this").siblings().removeClass("m-top-this");
		$("#xw1").hide();
		$("#xw2").hide();
		$("#xw3").show();
	});
	$(".axw1").click(function(){
		$(this).addClass("m-top-this").siblings().removeClass("m-top-this");
		$("#xw1").show();
		$("#xw2").hide();
		$("#xw3").hide();
	});
	
	
	$(document).ready(function(){
	var n=$('.new-1-13:visible div').length;
	$('.new-1-13').css({width:300*n});
	var n1=0;
	$('.m5-right').click(function(){
		if(n1<n-5){
			n1++;
			$('.new-1-13:visible').animate({marginLeft:-250*n1});
		}
	});
	$('.m5-left').click(function(){
		if(n1>0){
			n1--;
			$('.new-1-13:visible').animate({marginLeft:-250*n1});
		}
	});
	});
	
