<script type="text/javascript">
function OpenWin (url, w, h) {	
		NewWindow = open(url, "250576", "toolbar=no,location=no,directories=no,scrollbars=yes,resizable=no,menubar=no,status=no,width=" 
		+ w +",height=" + h); 
}
function makeSelect (obj, div) {
	maindiv = document.getElementById(div);
	for(var i=0; i<maindiv.childNodes.length; i++) {  
		var child = maindiv.childNodes[i];
		if (child.nodeName == 'DIV') {
			child.style.background='#ffffff';
		}
	}		
	obj.parentNode.style.background='#cde1fe';
}
</script>


