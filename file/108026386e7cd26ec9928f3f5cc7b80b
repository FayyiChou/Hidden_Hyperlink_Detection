$(document).ready(function() {	
	$("#UserLoginStrTop").load("HdApp/HdBas/HdAjaxGetData.asp?ac=UserLoginStr&ComUrl=Default.asp");
	$("#AdDiv").KinSlideshow();
	
	
	$("#linkPic .jCarouselLite").jCarouselLite({
	visible: 1,
	vertical: true,
	auto: 3000,
    speed: 800
	
	});
	
	
});

function toolschange(toolsid,toolscount){
	for(i=1;i<=toolscount;i++){
		$("#tools"+i).hide();
	}
	$("#tools"+toolsid).show();
}

function tolink(linkid,linkcount){

	$("#link"+linkid).toggle();
	for(i=1;i<=linkcount;i++){
		if(i != linkid){
		$("#link"+i).hide();
		}
	}
}

function PromotionChange(pdivId,pDivCount){
	for(i=1;i<=pDivCount;i++){
		$("#PromotionDiv"+i).hide();
		$("#PromotionMenu"+i).removeClass("PromotionChoose").addClass("PromotionNormal")
	}
	$("#PromotionDiv"+pdivId).show();
	$("#PromotionMenu"+pdivId).removeClass("PromotionNormal").addClass("PromotionChoose")
}



</script>
