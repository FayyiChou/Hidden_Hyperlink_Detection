<script language="javascript">

var nidx = 0;

var mlen = $("#slideshow ul li").length-1;

$("#slideshow .next").click(function() {

		$("#slideshow ul li:eq("+nidx+")").hide(80, function() {

			if ( nidx >= mlen ) {

				nidx = 0;

			} else {

				nidx++;

			}

			$("#slideshow ul li:eq("+nidx+")").show(80);

		});

							  

});

$("#slideshow .previous").click(function() {

		$("#slideshow ul li:eq("+nidx+")").hide(80, function() {

			if ( nidx <= 0 ) {

				nidx = mlen;

			} else {

				nidx--;

			}

			$("#slideshow ul li:eq("+nidx+")").show(80);

		});				  

});

</script>





