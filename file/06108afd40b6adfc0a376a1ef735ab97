<script>

//验证码刷新

function changeValidateImage(){

    document.images["randomCode"].src = "other/validate.php?dumy="+Math.random();

}



function _ajax(act,data,v){

	$.ajax({type:"post",async:false,url:"ajax.php?act=" + act,data:"value=" + data,success:function(msg){

		v = msg;

	}});

	

	return v;

}



function check_form(forminfo){

	if(forminfo.name.value == ""){

		alert("内容不能为空！");

		forminfo.name.focus();

		return false;

	}

	if(forminfo.phone.value == ""){

		alert("内容不能为空！");

		forminfo.phone.focus();

		return false;

	}

	if(forminfo.email.value == ""){

		alert("内容不能为空！");

		forminfo.email.focus();

		return false;

	}

	if(forminfo.content.value == ""){

		alert("内容不能为空！");

		forminfo.content.focus();

		return false;

	}

	if(forminfo.validate0.value==""){

		alert("验证码不能为空！");

		forminfo.validate0.focus();

		return false;

	}else{

		if(_ajax('check_v',forminfo.validate0.value) == '1'){

			alert('验证码错误！');

			forminfo.validate0.focus();

			return false;

		}

	}

	

}

</script>

