				<style type="text/css">
			
				#PCHeaderSlide .SlidePage{
					top:125px;
				}
				

				#imgPreviewWithStyles {
					/*background: #222 url(image/loading.gif) no-repeat center;*/
					background	: #FFF;
					z-index		: 999;
					border		: solid 2px #FFF;
					width		: 200px;
					height		: 65px;
				}
				
			
		</style>
		
		<script type="text/javascript">

			var thumb_flg  = 1; // サムネイル画像フラグ
			var title_flg  = 1; // 説明文フラグ
			var repeat_flg = 0;    // 再生設定
			var page_cnt = 5	//ページ数

			var navi   = "#HeaderSlide_thumb"; // サムネイル画像

			var slide_id  = "#HeaderSlide_image"; // スライド画像
			var txt_id    = "#HeaderSlide_text";  // 説明文
			
			var page	= "a.SlidePage"		//ページ送り
			var next	= "a.page_right";	//ページ送り(右)
			var prev	= "a.page_left";	//ページ送り(左)

			
			var slide_pattern = "fade"; // スライドパターン
			var slide_speed   = "10000";   // スライドスピード(画像切り替え)
			// #1339 2014/06/17 t-ichikawa エフェクトスピード追加
			var effect_speed   = "650";   // スライドスピード(エフェクト)

		
			$(document).ready(function(){


	if($("#PCHeaderSlide").length){

				/*
				$('.bxslider').bxSlider({
					auto: true,
					//adaptiveHeight: true,
					 mode: 'fade',
					pager: false,
					controls:false
					//autoControls: true
				});
				*/
		
		//#888 2013-11-11
		
						
							var background_color	= "#FFFFFF";
								var background_img	= BaseUrl + "files/design/tmplate/15/image/back.jpg";
				var repeat		= "no";
				
				
					var header_pos	= $("#PCHeaderSlide").position();
					var h_top	= header_pos.top;

					//CSS
					$("#PCHeaderSlide").css("background-color",background_color);
					$("#PCHeaderSlide").css("background-image","url(" + background_img + ")");

											//繰り返しなし
						$("#PCHeaderSlide").css("background-position","center -" + h_top + "px");
						$("#PCHeaderSlide").css("background-repeat","no-repeat");
					

				
				
			
				
				
				/***** サムネイル設定 *****/
				
				if(thumb_flg && page_cnt>1)
				{
					$("#HeaderSlide_thumb a.thum_bot").imgPreview({
						containerID: 'imgPreviewWithStyles',
						srcAttr: 'rel',
						distanceFromCursor: {top: 10, left:-110},
						onShow: function(link){
							var $_size = $(link).attr("name").split("_");
							
							var w = ($_size[0]) ? $_size[0] : 0;
							var h = ($_size[1]) ? $_size[1] : 0;
							
							$('img', this).css({"width": w + "px","height": h + "px"});
						}
					});
				}
				
				/***** スライドショー設定 *****/
				$("#PCHeaderSlide").hover(
					function () {
						$(page).show();
					},
					function () {
						$(page).hide();
					}
				);
				
				
				// スライドショー(画像)
				$(slide_id).cycle
				({
					fx		: slide_pattern,
					// #1339 2014/06/17 t-ichikawa エフェクトスピード追加
					speed		: effect_speed,
					timeout		: slide_speed,
					pager		: navi,
					autostop	: repeat_flg,
					next		: next,
					prev		: prev,
					pagerAnchorBuilder : function(idx, slide)
					{
						//IE対策
						$(slide).css({"background-color":"transparent"});
						
						return navi + ' li:eq(' + (idx) + ') ';
					}
				});
				
				// スライドショー(説明文)
				if(title_flg)
				{
					$(".HeaderSlideTextDiv").show();
					
					// 2012-05-29 egawa IE8のcycle:修正
					if (jQuery.support.opacity) {
						var opacity_set = 0.7;
					} else {
						var opacity_set = 0.5;
					}
					
					$(txt_id).cycle
					({
						fx		: 'scrollDown',
						// #1339 2014/06/17 t-ichikawa エフェクトスピード追加
						speed		: effect_speed,
						timeout		: slide_speed,
						pager		: navi,
						autostop	: repeat_flg,
						next		: next,
						prev		: prev,
						// 2012-05-29 egawa IE8のcycle:修正
						opacity		: opacity_set,
						top		: 20,
						sync		: 1,
						pagerAnchorBuilder : function(idx, slide)
						{
							//IE対策
							if($(slide).html() == ""){
								$(slide).css({"background-color":"transparent"});
							}
							
							return navi + ' li:eq(' + (idx) + ') ';
						}
						,
						before		: function(curr, next, opts, fwd){
							//#888
							$_div = $("<div>"+$(this).html()+"</div>").css('visibility','hidden');
							$(this).parent().append($_div);
							//h = $_div.outerHeight();
							h = ($_div.height()) ? $_div.outerHeight() : $_div.height();
							$(this).height(h);
							//$(this).parent().animate({height: h});
							$(this).parent().height(h);

							$_div.remove();


						}
						,
						after		: function(curr, next, opts, fwd){
							//#888
							$_div = $("<div>"+$(this).html()+"</div>").css('visibility','hidden');
							$(this).parent().append($_div);
							//h = $_div.outerHeight();
							h = ($_div.height()) ? $_div.outerHeight() : $_div.height();
							$(this).height(h);
							//$(this).parent().animate({height: h});
							$(this).parent().height(h);

							$_div.remove();


						}
						
					});

					
				}
			
			}
				
			});
		
		</script>




	
	


								
