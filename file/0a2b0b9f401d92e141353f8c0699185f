<script language="javascript">
$(document).ready(function() {
	var strHref = window.location.href; 
	if(document.getElementById("pageNav")!=null){
		var pageNavEle = document.getElementById("pageNav");
		if(pageNavEle.getElementsByTagName("li")!=null&&pageNavEle.getElementsByTagName("li")!=""){
			var liList = pageNavEle.getElementsByTagName("li");
			for(var x=0;x<liList.length;x++){
				var liEle = liList[x];
				var liaEle = liEle.getElementsByTagName("a")[0];
				if(strHref.indexOf(liaEle.getAttribute("href"))!=-1){
					liEle.setAttribute("class","active");
					liaEle.innerHTML=(liaEle.innerHTML+"<span class='sr-only'>(current)</span>");
				}
			}
		}
	}
	$.ajax({
		url : "loginAction!isLogin.action" ,
		type : "POST" ,
		datatype: "JSON",
		success : function(data){
			if(data.isLogin == 1){
				$("#login1").css("display", "none");
				$("#loginInfo").css("display", "block");
				$("#inManager").css("display", "block");
				$("#companyName").html(data.user.companyName + "欢迎登录");
				$("#ip").html("上次登录IP：" + data.ip);
				$("#createTime").html("上次登录时间：" + data.createTime);
				$("#loginTitle").html("登录成功");
			}else if(data.isLogin == 0){
				$("#login1").css("display", "block");
				$("#loginInfo").css("display", "none");
				$("#inManager").css("display", "none");
				$("#loginTitle").html("会员登录");
			}
		},
		error : function(){
		}
	});	
});
var str=window.location.href;
var es=/q=/;
es.exec(str);
var right=RegExp.rightContext;
if(right == '0'){
var tr = document.getElementById("error");
tr.style.display = "block";

}
function checkname(){
     var name = document.getElementById("account").value;
    if((/[\u4e00-\u9fa5]+/).test(name)){         
     $.alert({
		    title: '提示',
		    content: '不能含有汉字！',
		    confirmButton : '确定',
		    columnClass : 'col-md-2 col-md-offset-5 '
		});
     return false;
  }     
  document.form1.submit();
}

function toManager(){
	window.open("/companyInfo!execute.action?forward=success", "_blank");
}

function login(){
	var name = document.getElementById("account").value;
	    if((/[\u4e00-\u9fa5]+/).test(name)){     
	     $.alert({
		    title: '提示',
		    content: '不能含有汉字！',
		    confirmButton : '确定',
		    columnClass : 'col-md-2 col-md-offset-5 '
		});
	     return false;
	  }  
	$.ajax({
		url : "loginAction!login.action" ,
		type : "POST" ,
		datatype: "JSON",
		data : {"account" : $("#account").val(), "password" : $("#password").val()},
		success : function(data){
			if(data.status == 1){
				//如果登录成功，但是首次登陆，需要修改密码
				window.location.href="/companyInfo!execute.action?forward=editPassword&random=" + Math.random();
			}else if(data.status == 2){
				//如果登录成功，并且不是首次登陆，则现在信息div，并打开会员后台窗口
				$("#login1").css("display", "none");
				$("#loginInfo").css("display", "block");
				$("#companyName").html(data.user.companyName + "欢迎登录");
				$("#ip").html("上次登录IP：" + data.ip);
				$("#createTime").html("上次登录时间：" + data.createTime);
				$("#inManager").css("display", "block");
				$("#loginTitle").html("登录成功");
				if(phone==false){
					window.open("/companyInfo!execute.action?forward=success&random=" + Math.random(), "_blank");
				}
			}else if(data.status == 3){
				//用户名或密码错误
				$.alert({
    			    title: '提示',
    			    content: '用户名或密码错误！',
    			    confirmButton : '确定',
    			    columnClass : 'col-md-2 col-md-offset-5 '
    			});
			}
		},
		error : function(){
				$.alert({
    			    title: '提示',
    			    content: '验证异常！',
    			    confirmButton : '确定',
    			    columnClass : 'col-md-2 col-md-offset-5 '
    			});
		}
	});
}
function checkLogin(path){
	$.ajax({
		url : "loginAction!isMember.action" ,
		type : "POST" ,
		datatype: "JSON",
		data : {"account" : $("#account").val(), "password" : $("#password").val()},
		success : function(data){
			if(data.isLogin == 1){
				//如果登录成功
				window.open(path, "_blank");;
			}else if(data.isLogin == 0){
				//未登录
				$.alert({
    			    title: '提示',
    			    content: '该新闻仅会员可浏览，请先登录！',
    			    confirmButton : '确定',
    			    columnClass : 'col-md-3 col-md-offset-5 '
    			});
			}
		},
		error : function(){
		}
	});
}
var isMember;
function checkLogin2(meetingInfoID){
	$.ajax({
		url : "/loginAction!isMember.action" ,
		type : "POST" ,
		datatype: "JSON",
		data : {"account" : $("#account").val(), "password" : $("#password").val()},
		success : function(data){
			if(data.isLogin == 1){
				//如果登录成功
				meetingApply(meetingInfoID);
				isMember = true;
			}else if(data.isLogin == 0){
				//未登录
				$.alert({
    			    title: '提示',
    			    content: '该新闻仅会员可以报名，请先登录！',
    			    confirmButton : '确定',
    			    columnClass : 'col-md-3 col-md-offset-5 '
    			});
			}
		},
		error : function(){
		}
	});
}


</script>

