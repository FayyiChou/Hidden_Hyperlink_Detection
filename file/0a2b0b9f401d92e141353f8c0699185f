   <script language="javascript">
 
$(document).ready(function() {
	 //$("#marquee").marquee({container : '#marquee',containerHeight:270});
		$.ajax({
		url : "loginAction!getListForIndex.action" ,
		type : "POST" ,
		datatype: "JSON",
		success : function(data){
			if(data != null){
				var content = "";
				content = "<marquee behavior=\"scroll\" direction=\"up\" scrollamount=\"2\" style=\"width: 258px; height: 265px;overflow: hidden;\"> ";
				content = content + "<ul class=\"hyml\"> ";			
				var rows = data.rows;
				var x = 1;
				for(var i in rows){
					var row = rows[i];
					var lg = row.logo;
					if(lg == '' || lg == null){
						lg = '/logo/bb483929eb2648c7bcc12636b8558c35.fw.png';
					}
					if(x%2 == 0){
						content = content + "<img width='24px' height='24px' src='" + lg + "' alt='" + row.shortName + "' /><a target='_blank' style='margin-left:8px;' href='" + row.web +"'>" + row.shortName + "</a></li>";
					}else{
						content = content + "<li><div style='padding-left:30px;float:left;width:140px;'><img width='24px' height='24px' src='" + lg + "' alt='" + row.shortName + "' /><a target='_blank' style='margin-left:8px;' href='" + row.web +"'>" + row.shortName + "</a></div>";
					}
					x++;
				}
				content = content + "</ul>";
				content = content + "</marquee>";
				$("#marquee").html(content);
				//$("#marquee").marquee({yScroll: "up"}); 
			}
		},
		error : function(){
		}
	});
});
</script>

