<script>
function meetingApply(meetingInfoID){

	$.ajax({
		url : "/loginAction!validation.action" ,
		type : "POST" ,
		datatype: "JSON",
		data : {"meetingInfoID" : meetingInfoID},
		success : function(data){
			if(data == 2){
				$.alert({
				    title: '提示',
				    content: '已过报名时间！',
				    confirmButton : '确定',
				    columnClass : 'col-md-4 col-md-offset-4 '
				});
			}else if(data == 3){
				$.alert({
				    title: '提示',
				    content: '培训/会议名额已满！',
				    confirmButton : '确定',
				    columnClass : 'col-md-4 col-md-offset-4 '
				});
			}else if(data == 1){
				
				$.ajax({
					url : "/loginAction!getMeetingInfo.action" ,
					type : "POST" ,
					datatype: "JSON",
					data : {"meetingInfoID" : meetingInfoID},
					success : function(data){
						if(data != null){
							if(data.meetingInfo.sfzj != 1){
								$("#sfzjDiv").css("display","none");
							}else{
								$("#sfzjDiv").css("display","block");
							}
							if(data.meetingInfo.sfzs != 1){
								$("#sfzsDiv").css("display" , "none");
							}else{
								$("#sfzsDiv").css("display","block");
							}
							if(data.meetingInfo.sfyc != 1){
								$("#sfycDiv").css("display" , "none");
							}else{
								$("#sfycDiv").css("display","block");
							}
							if(data.meetingInfo.fptt != 1){
								$("#fpttDiv").css("display" , "none");
							}else{
								$("#fpttDiv").css("display" , "block");
							}
							if(data.meetingInfo.zjwt != 1){
								$("#zjwtDiv").css("display" , "none");
							}else{
								$("#zjwtDiv").css("display" , "block");
							}
							if(data.meetingInfo.sfzh != 1){
								$("#sfzhDiv").css("display" , "none");
							}else{
								$("#sfzhDiv").css("display" , "block");
							}
							$("#meetingInfoID").val(meetingInfoID);
							initValue();
							destoryValidator();
							if(data.user != null){
								$("#gsjc").val(data.user.companyName);
								if(data.user.companyCodeA != null){
									$("#gsdm").val(data.user.companyCodeA);
								}else if(data.user.companyCodeB != null){
									$("#gsdm").val(data.user.companyCodeB);
								}
							}
							
							
							if(/AppleWebKit.*Mobile/i.test(navigator.userAgent) || (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent))){ 	
								if(window.location.href.indexOf("?mobile")<0){ 	
									try{ 			
										if(/Android|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent)){ 				// 判断访问环境是 Android|webOS|iPhone|iPod|BlackBerry 则使用移动端报名页
											if(isMember){		
												window.location.href="/wxBindAction!toMemberApply.action?meetingInfoID=" + meetingInfoID;
											}else{
												window.location.href="/wxBindAction!toPublicApply.action?meetingInfoID=" + meetingInfoID;
											}
										}
									}catch(e){
											
									} 	
								} 
							}else{ // 如果以上都不是，则是PC端 
								$("#applyModal").modal("show");
							} 
						}else{
							$.alert({
							    title: '警告',
							    content: '操作失败！',
							    confirmButton : '确定',
							    columnClass : 'col-md-6 col-md-offset-3 '
							});
						}
					},
					error : function(){
						$.alert({
						    title: '警告',
						    content: '操作失败！',
						    confirmButton : '确定',
						    columnClass : 'col-md-6 col-md-offset-3 '
						});
					}
				});
			}
		},
		error : function(){
			$.alert({
			    title: '警告',
			    content: '操作失败！',
			    confirmButton : '确定',
			    columnClass : 'col-md-6 col-md-offset-3 '
			});
		}
	});
}

	function initValue(){
		$("#xm").val("");
		$("#xb").val(0);
		$("#zw").val("");
		$("#dh").val("");
		$("#yx").val("");
		$("#sfyc").val(-1);
		$("#sfzj").val(-1);
		$("#sfzs").val(-1);
		$("#fptt").val("");
		$("#zjwt").val("");
		$("#sfzh").val("");
		$("#applySubmit").removeAttr("disabled");
	}
	
	function destoryValidator(){
		$("#applyForm").data('bootstrapValidator').destroy();

		$('#applyForm').data('bootstrapValidator', null);
		
		initValidator();
	}
	
	$(document).ready(function() {
		initValidator();
	});
	
	function initValidator(){
		$('#applyForm')
		.bootstrapValidator({
				message : 'this value is not valid',
				feedbackIcons : {
					valid : 'glyphicon glyphicon-ok',
					invalid : 'glyphicon glyphicon-remove',
					validating : 'glyphicon glyphicon-refresh'
				},
				fields : {
					zjwt: {
	                    validators:{
	                        regexp: {
	                            regexp:/^[\s|\S]{0,400}$/,
	                            message: '不超过400字符',
	                            
	                        }
	                    }
	                },
					xm : {
						validators : {
							notEmpty : {
								message : '姓名不能为空'
							}
						}
					},
					zw : {
						validators : {
							notEmpty : {
								message : '职务不能为空'
							}
						}
					},
					dh : {
						validators : {
							notEmpty : {
								message : '手机不能为空'
							},
							regexp : {
								regexp:/^(1[35784]\d{9})$/,
								message : '请输入正确的手机号码'
							}
						}
					},
					yx : {
						validators : {
							regexp : {
								regexp : /^[a-zA-Z0-9_\.]+@[a-zA-Z0-9-]+[\.a-zA-Z]+$/,
								message : '请输入正确的邮件地址'
							}
						}
					},
					gsjc : {
						validators : {
							notEmpty : {
								message : '公司简称不能为空'
							}
						}
					},
					sfzh : {
						validators : {
							notEmpty : {
								message : '身份证号不能为空'
							}
						}
					}
				}
		}).on(
			"success.form.bv",
			function(e) {
				e.preventDefault();
				var sfzj = $("#sfzj");
				if($("#sfzjDiv").css("display") != "none" && $(sfzj).val() == -1){
					$.alert({
	    			    title: '提示',
	    			    content: '请选择“是否自驾”！',
	    			    confirmButton : '确定',
	    			    columnClass : 'col-md-4 col-md-offset-4 '
	    			});
					$("#applySubmit").removeAttr("disabled");
					return;
				}
				var sfzs = $("#sfzs");
				if($("#sfzsDiv").css("display") != "none" && $(sfzs).val() == -1){
					$.alert({
	    			    title: '提示',
	    			    content: '请选择“是否住宿”！',
	    			    confirmButton : '确定',
	    			    columnClass : 'col-md-4 col-md-offset-4 '
	    			});
					$("#applySubmit").removeAttr("disabled");
					return;
				}
				var sfyc = $("#sfyc");
				if($("#sfycDiv").css("display") != "none" && $(sfyc).val() == -1){
					$.alert({
	    			    title: '提示',
	    			    content: '请选择“是否用餐”！',
	    			    confirmButton : '确定',
	    			    columnClass : 'col-md-4 col-md-offset-4 '
	    			});
					$("#applySubmit").removeAttr("disabled");
					return;
				}
				$.post(
						"/loginAction!insert.action",
						{
							"xm" : $("#xm").val(),
							"xb" : $("#xb").val(),
							"zw" : $("#zw").val(),
							"dh" : $("#dh").val(),
							"yx" : $("#yx").val(),
							"sfzj" : $("#sfzj").val(),
							"sfzs" : $("#sfzs").val(),
							"sfyc" : $("#sfyc").val(),
							"fptt" : $("#fptt").val(),
							"zjwt" : $("#zjwt").val(),
							"sfzh" : $("#sfzh").val(),
							"gsjc" : $("#gsjc").val(),
							"gsdm" : $("#gsdm").val(),
							"meetingInfoID" : $("#meetingInfoID").val()
						}, function(data) {
							if (data == 1) {
								$.confirm({
								    title: '提示',
								    content: '报名成功！',
								    confirmButton : '继续报名',
								    cancelButton : '关闭',
								    columnClass : 'col-md-4 col-md-offset-4 ',
								    cancel: function(){
								    	
								    	$("#applyModal").modal("hide");
								    }
								});
								initValue();
								destoryValidator();
							}else if(data == 4){
								$.alert({
				    			    title: '提示',
				    			    content: '当前会议该身份证号【' + $("#sfzh").val() + '】已报过名！',
				    			    confirmButton : '确定',
				    			    columnClass : 'col-md-4 col-md-offset-4 '
				    			});
								$("#applySubmit").removeAttr("disabled");
							}else if(data == 2){
								$.alert({
								    title: '提示',
								    content: '已过报名时间！',
								    confirmButton : '确定',
								    columnClass : 'col-md-4 col-md-offset-4 '
								});
								$("#applySubmit").removeAttr("disabled");
							}else if(data == 3){
								$.alert({
								    title: '提示',
								    content: '培训/会议名额已满！',
								    confirmButton : '确定',
								    columnClass : 'col-md-4 col-md-offset-4 '
								});
								$("#applySubmit").removeAttr("disabled");
							}else if(data == 5){
								$.alert({
								    title: '提示',
								    content: '该人员已经报过名，请勿重复报名！',
								    confirmButton : '确定',
								    columnClass : 'col-md-4 col-md-offset-4 '
								});
								$("#applySubmit").removeAttr("disabled");
							}else{
								$.alert({
				    			    title: '警告',
				    			    content: '操作失败！',
				    			    confirmButton : '确定',
				    			    columnClass : 'col-md-4 col-md-offset-4 '
				    			});
							}
						})
			})
	}
</script>
