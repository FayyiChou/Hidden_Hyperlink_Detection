eventlist.push(["November", "9", "Cambodia", "Independence Day" ]);
eventlist.push(["November", "9", "Nepal", "Constitution Day" ]);
eventlist.push(["November", "11", "Poland", "Independence Day" ]);
eventlist.push(["November", "18", "Oman", "Birthday of Sultan Qaboos" ]);
eventlist.push(["November", "18", "Latvia", "Independence Day" ]);
eventlist.push(["November", "22", "Lebanon", "Independence Day" ]);
eventlist.push(["November", "28", "Albania", "Independence Day" ]);

eventlist.push(["December", "1", "Romania", "Great Union Day / Unification Day" ]);
eventlist.push(["December", "2", "United Arab Emirates", "Independence Day" ]);
eventlist.push(["December", "5", "Thailand", "Anniversary of Birthday H.M. King Phumiphon" ]);
eventlist.push(["December", "6", "Finland", "Independence Day" ]);
eventlist.push(["December", "16", "Kazakhstan", "Independence Day" ]);
eventlist.push(["December", "18", "Qatar", "National Day" ]);
eventlist.push(["December", "23", "Japan", "Birthday of Emperor Akihito" ]);
eventlist.push(["December", "24", "Libya", "Independence Day" ]);


document.write("<h3>National Days in " + month[d.getMonth()]+"</h3>");


var m = month[d.getMonth()];
var day = d.getDate();
var i = 0;


for( i=0; i < eventlist.length; i++){
if (m == eventlist[i][0]){
    if(day == eventlist[i][1] )
	{
	document.write("<div style='padding-bottom: 3px; padding-top: 3px; border-bottom: 1px solid #efefef;'><font color='red'><b>"+eventlist[i][0]+"</b>&nbsp;&nbsp;");
     	document.write("<b>"+eventlist[i][1]+"</b>");
    	document.write(" - "+eventlist[i][2]);
	document.write("<BR>"+eventlist[i][3]);
    	document.write("</font></div>");
	}
    else{
   		document.write("<div style='padding-bottom: 3px; padding-top: 3px; border-bottom: 1px solid #efefef;'><b>"+eventlist[i][0]+"</b>&nbsp;&nbsp;");
		document.write("<b>"+eventlist[i][1]+"</b>");
		document.write(" - <font color='teal'>"+ eventlist[i][2]);
		document.write("</font><BR>"+eventlist[i][3]);
  		document.write("</div>");
		}
	}
}
document.write("<div align='right'><A href=\"http://www.embassy.am/national-days\">show all national days</A></div>");
</script>
</div>
