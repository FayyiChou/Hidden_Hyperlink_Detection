function setVersionCookie(){
    var ex = new Date();
    ex.setDate(ex.getDate() + 5);
    document.cookie = 'ocver=old;path=/;domain=.fordiy.com;expires=' + ex.toGMTString();
    //alert(document.cookie.indexOf('ocver=old'));
}

function quickSubmitPhone(obj){
    var phone = $.trim(obj.eva_contact.value);
    if(/[0-9]{11}/.test(phone)){
        var url = 'http://www.fordiy.com/evaluation/submit';
        $.post(
            url,
            {
                'type': 3,
                'phone': phone
            },
            function(d){
                if(d.code == 0){
                    alert('感谢您的提交，我们将尽快与您联系');
                } else {
                    alert(d.msg);
                }
            },
            'json'
        );
    } else {
        alert('请输入有效的手机号码');
    }
    return false;
}
</script> 
