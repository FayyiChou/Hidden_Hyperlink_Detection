	function selAddClass(selector, val){
		$(selector).each(function(){
			sid = $(this).attr('sid');
			if( sid == val ) {
				$(this).addClass('on');
			}
		});
		return sid;
	}
	var catid = selAddClass("#cat a",selInfo[0]);
	var subject = selAddClass("#subject a",selInfo[1]);
	var pinyin = selAddClass("#pinyin a",selInfo[2]);
