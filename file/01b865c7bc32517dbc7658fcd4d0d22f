	$("#sa-comic_show_list").delegate('li.list_item', 'mouseover', function(event) {
		$(this).addClass("hover");
	});
	$("#sa-comic_show_list").delegate('li.list_item', 'mouseout', function(event) {
		$(this).removeClass("hover");
