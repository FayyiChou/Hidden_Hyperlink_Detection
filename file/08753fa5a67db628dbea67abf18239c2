         <script>
       (function(jq){
      var init = function(){                                            
                        var memId = FE.util.getLastMemberId();
                        if(memId){
                            jQuery.ajax('//tpview.1688.com/tp_type.htm', {
                                dataType:'script',
                                data: {
                                    "var": "_cxt_res",
                                    "memberIds": memId
                                }
                            }).done(function (result){
                                if(_cxt_res && _cxt_res.success == "true"){
                                        if(_cxt_res[memId].isTP == "true"){
                                            window.location.href ="//gys.1688.com/help/index.html?spm=a260e.7046529.512872853.6"
                                        }   
                                    }
                                }
                            );
                        }
                    }
      init();
      })(jQuery);
      
      
        jQuery(function(j){
            if(!j.support.placeholder) {//输入框中点击文字时，默认值隐藏；
                    var els = j('input[placeholder]');
                    els.each(function(i, el) {
                    el = j(el);
                    var defValue = el.attr('placeholder'),
                        defColor = el.css('color');
                    el.bind('focus',function() {
                        if(this.value === '' || this.value === defValue) {
                            j(this).css('color', '#333');
                            this.value = '';
                        }
                    });
                    el.bind('blur',function() {
                        if(this.value === '' || this.value === defValue) {
                            j(this).css('color', defColor);
                            this.value = defValue;
                        }
                    });
                    el.triggerHandler('blur');
                });
            }   
           /* j.use('web-alitalk-shunt', function(){
                FE.util.alitalk.shunt(j('a[data-alitalk-shunt]'),{onRemote:function(){}});*/
            });
        })
    </script>
    

