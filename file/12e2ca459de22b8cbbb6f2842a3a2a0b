<script type="text/javascript">

    function checkform() {

        if ($("#name").val() == "") {
            alert("姓名不能为空！");
            return false;
        }
        if ($("#phone").val() == "") {
            alert("电话不能为空！");

            return false;
        }

        if (!$.isNumeric($("#phone").val()) || ($("#phone").val()).length > 11) {//是数字返回为true  不超过11位数返回为true 
            alert('电话格式不正确，请重新输入！');
            return false;
        } 

        if ($("#content").val() == "") {
            alert("留言不能为空！");

            return false;
        }
        return true;
    }
</script>

