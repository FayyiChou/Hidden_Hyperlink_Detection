<script>
    $(function () {
        var $form = $('.smartb_settings');
        var $image_wrap = $(".smartbs_smartb");
        if ($form.length != 0) { // if backend
            $(".smartb_custom_html").draggable({
                containment: ".smartbs_smartb",
                stop: function () {
                    var position = {
                        left: parseInt($(this).css("left")) / ($image_wrap.width() / 100) + "%",
                        top: parseInt($(this).css("top")) / ($image_wrap.height() / 100) + "%"
                    };
                    $(this).css("left", parseInt($(this).css("left")) / ($image_wrap.width() / 100) + "%");
                    $(this).css("top", parseInt($(this).css("top")) / ($image_wrap.height() / 100) + "%");
                    $("[data-image-position-id='" + $(this).data('image-id') + "']").val("left:" + position.left + "; top:" + position.top + ";");
                }
            })
        }
    });
</script>
