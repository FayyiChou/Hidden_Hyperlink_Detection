<script language="JavaScript">
window.setInterval(scroll_to_next, 20000);
var now_idx = 0;
var obj = document.id('scroll_id');
obj.set('tween', {duration: 'long'});
var li_num = 4;

function scroll_to_next()
{
    if (now_idx == li_num)
        now_idx = 0;
    else
        now_idx++;
    
    to   = -(now_idx) * 36;
    obj.tween('top', to);
}
</script>
