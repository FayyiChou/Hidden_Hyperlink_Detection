function showcab(m,n,count){
	var strPic1;
	var strPic2;
	strPic1='url(/web/lzj_new/images/bg1.jpg)';
	strPic2='url(/web/lzj_new/images/bg2.jpg)';
		for(var i=1;i<=count;i++){
		if (i==n){			
			getObject(m+'_'+i).style.background=strPic1;
			getObject('cd_'+m+'_'+i).style.color='#ffffff';
			getObject('cab_'+m+'_'+i).style.display='';
			}
		else {
			getObject(m+'_'+i).style.background=strPic2;
			getObject('cd_'+m+'_'+i).style.color='#233b61';
			getObject('cab_'+m+'_'+i).style.display='none';
			}
	}
}
function getObject(objectId) {
    if(document.getElementById && document.getElementById(objectId)) {
	// W3C DOM
	return document.getElementById(objectId);
    } else if (document.all && document.all(objectId)) {
	// MSIE 4 DOM
	return document.all(objectId);
    } else if (document.layers && document.layers[objectId]) {
	// NN 4 DOM.. note: this won't find nested layers
	return document.layers[objectId];
    } else {
	return false;
    }
} // getObject
//-->
</script>
