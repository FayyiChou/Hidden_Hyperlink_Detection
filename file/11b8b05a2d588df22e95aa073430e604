			$(".sidebar ul li a").click(function(){
				$(".sidebar ul li a").removeClass("hot");
				$(".sub_publicmenu").not($(this).next(".sub_publicmenu")).slideUp("flow");//除去a的下一个div页面其他div变为隐藏。。。
