<script>
$(function(){
    $('#submit-password').on('click', function(){
        var password = $.trim($('#survey_password').val());
        if (!password) {
            $('#survey-page-password').find('small').text('密码不能为空');
            return false;
        }
        $.ajax({
            type:'POST',
            url:'/ajax/common',
            data:{op:'validatePassword',password:password,'sid':63248},
            dataType:'json',
            success:function(data){
                if (data.code == 0) {
                    location.reload();
                } else {
                	$('#survey-page-password').find('small').text(data.content);
                }
            },
        });
    });
});
</script>
    
