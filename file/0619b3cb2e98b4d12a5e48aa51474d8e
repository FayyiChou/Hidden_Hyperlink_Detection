<script>
 	$(function(){
 		$(".search-btn").click(function(){
 			if($(this).hasClass("open")){
 				$(this).removeClass("open").next('.search-box').animate({"width":"0"},400, "easeInCubic");
 			}else{
 				$(this).addClass("open").next('.search-box').animate({"width":"397px"},800, "easeOutCubic");
 			}

 		});

 		$(".scr1000 .lang").hover(function(){
 			$(".lang-list").stop(true,true).animate({"height":82},400)
 		},function(){
 			$(".lang-list").stop(true,true).animate({"height":0},400)
 		});
 		$(".menu-son-li").find("a:first").css({"border-top":0})

 		$(".menu li").hover(function() {
 			$(this).find('.fat').addClass("hover");
 			$(this).find('.menu-son').stop(true,true).slideDown();
 		}, function() {
 			$(this).children('.fat').removeClass("hover");
 			$(this).find('.menu-son').stop(true,true).slideUp();
 		});
 	})
 </script> 

