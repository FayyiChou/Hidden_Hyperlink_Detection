<script>
$(function(){
_PreLoadImg([
"http://www.dali-tech.com.cn/static/web/img/banner01.jpg?v=v2 ",
"http://www.dali-tech.com.cn/static/web/img/banner02.jpg?v=v2 "
],function(){
$(".iban").css("background", "none")
BannerLi(0)
if($(".iban .imglist li").length == 1){
$(".iban .btns").hide()
return
}
var BTime, Bn = 0,
Bset;
$(".iban .btns").css("margin-left", - $(".iban .btns").width() / 2).find("span").eq(0).addClass("cur")
 		 	
 	$(".iban .line-in").stop().animate({width : 100 + '%'}, 8000, "linear", function(){
$(".iban .line-in").css("width", 0)
})

 		BTime = setInterval(function(){
 			Bn ++ ;
 			if(Bn == $(".iban .imglist li").length){
 				Bn = 0
 			}
 			BannerLi(Bn)
 			$(".iban .line-in").css("width", 0).stop().animate({width : 100 + '%'}, 8000, "linear")
}, 8000)

 	var ibanBL = true
 	$(".iban .btns span").click(function(){
 		if(!ibanBL){
 			return
 		}
 		ibanBL = false;
 		if(BTime){
 			clearInterval(BTime)
 		}
 		Bn = $(this).index()
 		BannerLi(Bn)
 		$(".iban .line-in").css("width", 0).stop().animate({width : 100 + '%'}, 8000, "linear")
 		BTime = setInterval(function(){
 			Bn ++ ;
 			if(Bn == $(".iban .imglist li").length ){
 				Bn = 0
 			}
 			BannerLi(Bn)
 			$(".iban .line-in").css("width", 0).stop().animate({width : 100 + '%'}, 8000, "linear")
}, 8000)
setTimeout(function(){
ibanBL = true
}, 1000)
 	})

})
$(".iban .list a, .i-products .list a").hover(function(){
var $obj = $(this).find("span.por")
Left($obj, status, -5, 300)
}, function(){
return
})
$(".iban-box .box").hover(function(){
 		$(this).stop().animate({marginTop : - 10}, 500)
 	}, function(){
 		$(this).stop().animate({marginTop : 0}, 500)
 	})

// INDEX CASES
$(".i-cases li").hover(function(){
var _this = $(this);
_this.find("img").stop(true,true).fadeIn(800).siblings(".sign").addClass("bg").stop().animate({
left : 125,
top  : 25
}, 800).siblings(".tips").stop().animate({
left : 50 + '%',
top  : 89,
marginLeft : - _this.find(".tips").width() / 2
}, 800)
}, function(){
var _this = $(this);
_this.find("img").stop(true,true).fadeOut(800).siblings(".sign").removeClass("bg").stop().animate({
left : 27,
top  : 42
}, 800).siblings(".tips").stop().animate({
left : 111,
top  : 42,
marginLeft : 0
}, 800)
})
$(".i-cases .move").width($(".i-cases ul").length * 999)
if($(".i-cases ul").length > 1){
for (var i = 0; i < $(".i-cases ul").length; i++) {
$(".i-cases .btns").append('<span class="span'+i+' png"></span>');
}
$(".i-cases .btns").css("margin-left", - $(".i-cases .btns").width() / 2).find("span").eq(0).addClass("cur")
$(".i-cases .btns span").click(function(){
var n = $(this).index();
Icases(n)
})
var casesNum = 1,
casesTime = setInterval(function(){
Icases(casesNum)
casesNum ++ ;
if(casesNum == $(".i-cases ul").length){
casesNum = 0
}
}, 5000)
$(".i-cases .w").hover(function(){
if(casesTime){
clearInterval(casesTime);
}
}, function(){
casesTime = setInterval(function(){
Icases(casesNum)
casesNum ++ ;
if(casesNum == $(".i-cases ul").length){
casesNum = 0
}
}, 5000)
})
}
// INDEX PRODUCTS
$(".i-products .list li").hover(function(){
$(this).addClass("hover").find(".bg").hide()
$(this).siblings().find(".bg").stop(true,true).fadeIn(800)
}, function(){
$(this).removeClass("hover")
})
$(".i-products .list").hover(function(){
return
}, function(){
$(this).find(".bg").stop(true,true).fadeOut(800)
})
$(".i-products .next").click(function(){
$(".i-products .list ul").eq(1).find("li").css({
left : 100,
opacity : 0
})
$(".i-products .list").stop().animate({
top : -346,
opacity   : 0
}, 600, function(){
$(".i-products .list").css({
top : 0,
opacity   : 1
}).find("ul").eq(0).appendTo($(".i-products .list"))
Left($(".i-products ul:eq(0) li").eq(0), "true", 100, "next", 400)
})
})
$(".i-products .prev").click(function(){
$(".i-products .list").stop().animate({
top : 346,
opacity   : 0
}, 600, "linear", function(){
$(".i-products .list ul:last").find("li").css({
left : 100,
opacity : 0
})
$(".i-products .list ul:last").prependTo($(".i-products .list"))
$(".i-products .list").css({
top : 0,
opacity   : 1
})
Left($(".i-products ul:eq(0) li").eq(0), "true", 100, "next", 400)
})
})
if($(".i-products .list ul").length > 1){
Hover($(".i-products .btns"), "hover")
$(".i-products .list-out").hover(function(){
$(this).find(".prev").stop(true,true).animate({
marginLeft: -66,
opacity : 1
}, 300)
$(this).find(".next").stop(true,true).animate({
marginRight: -66,
opacity : 1
}, 300)
if(iProList){
clearInterval(iProList);
}
}, function(){
$(this).find(".prev").stop(true,true).animate({
marginLeft: 0,
opacity: 0
}, 300)
$(this).find(".next").stop(true,true).animate({
marginRight: 0,
opacity: 0
}, 300)
})
}else{
var iProList 
}
    // INDEX NEWS
$(".i-news .list.fl li").hover(function(){
$(this).find(".bor").stop().fadeIn(500)
}, function(){
$(this).find(".bor").stop().fadeOut(500)
})
Hover($(".i-news .list.fr li"), "hover")

$(window).scroll(function(){
 		var winT = $(window).scrollTop();
 		if(winT > $(".i-products").offset().top - 300 && $(".i-products li").eq(0).attr("class") != "end"){
$(".i-products li").css({
left : 100,
opacity : 0
}).addClass("end")
Left($(".i-products li").eq(0), "true", 100, "next", 400)
 		}
 		if(winT > $(".i-news").offset().top - 300 && $(".i-news .list.fl li").eq(0).attr("class") != "end"){
 			$(".i-news .list.fl li").css({
left : - 100,
opacity : 0
}).eq(0).addClass("end")
Left($(".i-news .list.fl li:last"), "true", 100, "prev", 400)
Up($(".i-news .list.fr li:first"), status, 100, 400, 2)
 		}
 	})
})
function BannerLi(n){
$(".iban .btns span").eq(n).addClass("cur").siblings().removeClass("cur")
$(".iban .imglist li").eq(n).addClass("active").stop(true, true).fadeIn(1000, "linear").siblings().removeClass("active").stop(true, true).fadeOut(1000)
$(".iban .list li:even").find(".tips").css({
left : - 200,
opacity : 0
}).siblings().css({
right : -200,
opacity : 1
})
$(".iban .list li:odd").find(".tips").css({
right : - 200,
opacity : 0
}).siblings().css({
left : -200,
opacity : 1
})
$(".iban .list li").eq(n).siblings().fadeOut(1000, "linear", function(){
if(n % 2 == 0){
$(".iban .list li").eq(n).show().find(".tips").stop().animate({
left : 0,
opacity : 1
}, 600, "linear").siblings("img").stop().animate({
right : -70,
opacity : 1
}, 600)
}else{
$(".iban .list li").eq(n).show().find(".tips").stop().animate({
right : 0,
opacity : 1
}, 600, "linear").siblings("img").stop().animate({
left : -70,
opacity : 1
}, 600)
}
})
}
function Icases(n){
$(".i-cases .btns span").eq(n).addClass("cur").siblings().removeClass("cur")
$(".i-cases .move").stop().animate({marginLeft : - n * 999}, 800, "linear")
}
