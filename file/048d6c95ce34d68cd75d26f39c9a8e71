        $("#login").click(function () {
            localStorage.web_domain = 'dyej_weblogin';
            layer.open({
                type: 2,
                title: false,
                maxmin: true,
                content: 'http://www.cddyjy.com/WisdomPartyBuildingView/src/modules/loginmodules/djejweblogin/login.html',
                area: ['1200px', '600px'],
                end: function () {
                    loadLoginInfo();
                }
            });
        })
        function loadLoginInfo() {
            if (localStorage.userInfo != "" && localStorage.userInfo != undefined && localStorage.userInfo != "null") {
                $(".login_todyej").hide();
                $("#login_success").show();
                var userInfo = JSON.parse(localStorage.userInfo);
                var nameText = userInfo.UserName;
                if (nameText.length > 6) {
                    nameText = nameText.substr(0, 6) + "…";
                }
                $("#userName").text("欢迎您，" + nameText);
            }
            else {
                $(".login_todyej").show();
                $("#login_success").hide();
            }
        }
        $("#logout").click(function () {
            localStorage.userInfo = '';
            localStorage.clear();
            loadLoginInfo();
        });

        function ImgHide() {
            document.getElementById("gg1").style.display = "none";
        }
        function centerImgHide() {
            document.getElementById("centerImg").style.display = "none";
        }
        
        $(document).ready(function () {
            //  var ad1 = new AdMove("gg1");
            // ad1.Run();
            initUnitsStudyState();
            loadLoginInfo();
            initialNetworkclass();
            addPeriods('6', '1', 'web')
        });
        $(".gcd ul li a").click(function () {
            var href = $(this).attr("href");
            if (href == "http://www.12371.cn/") {
                addPeriods('1', '2', 'web')
            }
            else if (href == "http://dslm.12371.cn/") {
                addPeriods('2', '2', 'web')
            }
            else if (href == "http://www.dygbjy.gov.cn:8008/index.html") {
                addPeriods('3', '2', 'web')
            }
            else if (href == "http://news.12371.cn/2014/07/28/ARTI1406508588803558.shtml") {
                addPeriods('4', '2', 'web')
            }
        })
        $(".tfxf_wl a").click(function () {
            var href = $(this).attr("href");
            if (href == "http://www.scycjy.gov.cn/WebSite/ydy.html") {
                addPeriods('5', '2', 'web')
            }
        })
        function addPeriods(pr_id, op_isMust, op_type) {
