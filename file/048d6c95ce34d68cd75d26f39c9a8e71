        }
        function initialNetworkclass() {
            $.ajax({
                type: "GET",
                url: "http://e.cddyjy.com/WisdomPartyBuildingServices/Static/Cache/Index/networkClass-table.html",
                success: function (data) {
                    $('#networkclass').html(data);
                }
            });

        }

        function initUnitsStudyState() {
            $.ajax({
                type: "GET",
                url: "http://e.cddyjy.com/WisdomPartyBuildingServices/Static/Cache/Index/period-table.html",
                success: function (data) {
                    $("#xstj").html(data)
                }
            });

        }
