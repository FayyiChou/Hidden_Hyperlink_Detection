<script type="text/javascript">
function onSubmit() {
	if(updateLoginPwd()){
		var modulus = "00d7976fbcb4aa0ae773a411f4670083494aa75278d456ac1b21f132fee6257a9958d77e106b38c2e844914d972baf5fb6eef4bf291ce168f4ed127a625d6fac5715c79f2806efad0a570d144b888c0e16cb035a5c557fe0b9754057fb608f08c72e4c95a53505d02ac94606613184f29d092217bbf697853d14a29f598a3ce6eb", exponent = "010001";
	    var key = RSAUtils.getKeyPair(exponent, '', modulus);
	    $("#passwordOne").val(RSAUtils.encryptedString(key, $("#passwordOne").val()));
	    $("#passwordTwo").val(RSAUtils.encryptedString(key, $("#passwordTwo").val()));
	    return true;
	}
	return false;
}

function updateLoginPwd(){
	var myregPsw = /^(?!^[0-9]+$)(?!^[A-z]+$)(?!^[^A-z0-9]+$)^.{6,16}$/;
	var passwordRegex = /^(?![a-zA-Z0-9]+$)(?![^a-zA-Z/D]+$)(?![^0-9/D]+$).{8,12}$/;
	var errorPwdMsg = "数字+字母+特殊字符，8-12位长度";
	var passwordOne = $("#passwordOne").val();
	var passwordTwo = $("#passwordTwo").val();
	$("#passwordOneMsg").removeClass("f12 gray9");
	if(myregPsw != null && myregPsw != ""){
		passwordRegex = myregPsw;
	}
	if(undefined == passwordOne || "" == passwordOne){
		$("#passwordOneMsg").html("密码不能为空");
		$("#passwordOneMsg").addClass("red");
		return false;
	}
	else if(!passwordRegex.test(passwordOne)){
		$("#passwordOneMsg").html(errorPwdMsg);
		$("#passwordOneMsg").addClass("red");
		return false;
	}else{
		$("#passwordOneMsg").html(errorPwdMsg);
		$("#passwordOneMsg").removeClass("red");
		$("#passwordOneMsg").addClass("f12 gray9");
		$("#passwordTwoMsg").removeClass("f12 gray9");
		if(undefined == passwordTwo || "" == passwordTwo){
			$("#passwordTwoMsg").html("请再次输入密码");
			$("#passwordTwoMsg").addClass("red");
			return false;
		}
		else if(passwordTwo != passwordOne){
			$("#passwordTwoMsg").html("两次输入密码不一致");
			$("#passwordTwoMsg").addClass("red");
			return false;
		}
		else {
			$("#passwordTwoMsg").html("两次输入密码不一致");
			$("#passwordTwoMsg").removeClass("red");
			$("#passwordTwoMsg").addClass("f12 gray9");
			return true;
		}
	}
	
	
}
</script>

