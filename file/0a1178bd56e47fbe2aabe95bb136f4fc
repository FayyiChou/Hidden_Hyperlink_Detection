    $(function () {
        $('body .site-content-m .fui-d img').each(function () {
            if (!$(this).attr('alt')) {
                var value = $(this).closest('a').find('span.tit').text();
                $(this).attr('title', value);
                $(this).attr('alt', value);
            }
            $(this).css('width', 'auto').css('height', 'auto');
        });
    });
</script>
<script type="text/javascript">
    $(function(){
        $('ul.site-search-ul li').click(function(){
            $(this).siblings('li').removeClass('current');
            $(this).addClass('current');
            $('#hidden-module-id').text($(this).attr('moduleId'));
        });
        $('.search_keyword').click(function(){
            var search_path1 =  "http://www.goods100.com/api/search.php";
            var moduleid = $('#hidden-module-id').text();
            var kw = $.trim($('#keyword').val());
            var location_href =  "http://www.goods100.com/api/search.php?spread=0&"+"moduleid="+moduleid+'&kw='+kw;
            window.location.href = location_href;
        });
    });
</script>
