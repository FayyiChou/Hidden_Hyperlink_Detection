<script type="text/javascript">
        /*$(document).ready(function() {

         });*/

        /* Добавление в закладки (begin) */
        function docompare(ID) {
            var id = ID;
            if ($("#compare_" + ID).hasClass("go-compare")) {
                $.ajax({
                    type: 'POST',
                    url: "",
                    data: {
                        id: ID,
                        action: "DELETE_FROM_COMPARE_LIST",
                    },
                    success: function (result) {
                        var bigButton = $(result).find('.comp-result-button').html();
                        $('.comp-result-button').html(bigButton);
                    } //удаление товара из сравнения
                });
                $("#compare_" + ID).removeClass("go-compare");

            } else {
                $.ajax({
                    type: 'POST',
                    url: "",
                    data: {
                        id: ID,
                        action: "ADD_TO_COMPARE_LIST",
                    },
                    success: function (result) {
                        var bigButton = $(result).find('.comp-result-button').html();
                        $('.comp-result-button').html(bigButton);
                    } //добавить товар к сравнению
                });
                $("#compare_" + ID).addClass("go-compare");
            }
            return false;
        }
    </script>
