	<script language="javascript">
		function TabOnFocus(focusControlName)
		{
			document.getElementById(focusControlName).className = "current";
			if (focusControlName == "tab1")
			{
				document.getElementById("tab2").className = "";
				document.getElementById("NewTabA").style.display = "";
				document.getElementById("NewTabB").style.display = "none"; 
			}
			else
			{
				document.getElementById("tab1").className = "";
				document.getElementById("NewTabB").style.display = "";
				document.getElementById("NewTabA").style.display = "none"; 
			}
		}
	</script>

