   demoo.scrollTop-=demoo1.offsetHeight
   else{
   demoo.scrollTop++
   }
   }
   var MyMar=setInterval(Marquee,speed2)
   demoo.onmouseover=function() {clearInterval(MyMar)}
   demoo.onmouseout=function() {MyMar=setInterval(Marquee,speed2)}
   </script>
