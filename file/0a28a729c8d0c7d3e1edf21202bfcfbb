<script type="text/javascript">
    $(function () {
	    $(window).bind("resize", rightflyFunction);
	    rightflyFunction();
    });
	
    function rightflyFunction() {
	   // var top = $(document).scrollTop();
//	    $(window).resize(function () { top = document.body.scrollTop || document.documentElement.scrollTop; });	//浏览器窗口调节时触发resize
	    var hei=$(window).height();
		$(window).resize(function(){hei=$(window).height()});
		var top=$(".qh_box").css("top");
		var whei=parseInt(hei);
		var ptop=parseInt(top);
		if(ptop>whei){
			$(".qh_box").css("top",whei-30+"px");
			$(window).bind("scroll", function(){
				var sctop = $(document).scrollTop();
				$(window).resize(function () { sctop = document.body.scrollTop || document.documentElement.scrollTop; });
				var dstop=parseInt(sctop);
				var settop=whei-30+dstop;
				if(settop>830){
					settop=830;
				}
				$(".qh_box").css("top",settop+"px");
			});
		}else{
			$(".qh_box").css("top","830px");
		}
    }
</script>
