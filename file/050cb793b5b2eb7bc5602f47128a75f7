  <script type="text/javascript"> 
$(document).ready(function(){  
$(":submit[id=tijiao]").click(function(check){  
  var val = $("#name").val();  
  if(val==""){  
	alert("姓名不能为空！");  
	$("#name").focus();  
	return false;
  } 
  
  var valtel = $("#tel").val(); 
  if(valtel==""){  
	alert("手机不能为空！");  
	$("#tel").focus();  
	return false;
  } 
  
  var patrn=/((\d{11})|^((\d{7,8})|(\d{4}|\d{3})-(\d{7,8})|(\d{4}|\d{3})-(\d{7,8})-(\d{4}|\d{3}|\d{2}|\d{1})|(\d{7,8})-(\d{4}|\d{3}|\d{2}|\d{1}))$)/; //验证手机和电话
  if (!patrn.exec($("#tel").val())){        
  	alert("手机或电话格式不正确！");  
	$("#tel").focus();  
	return false;      
  }
  
  var valmail = $("#email").val(); 
  if(valmail==""){  
	alert("邮箱不能为空！");  
	$("#email").focus();  
	return false;
  } 
   var search_str = /^[\w\-\.]+@[\w\-\.]+(\.\w+)+$/;
   var email_val = $("#email1").val();
   if(!search_str.test(email_val)){        
   alert("邮箱格式不正确 !");
   $('#email1').focus();
   return false;
   }
   
	 var objs=$("input[name=title]");
	 if ($(objs).val()=="") {
	    alert("\n请您填写留言标题！");
	    $(objs).focus();
	    return (false);
	 }

	 var objs=$("textarea[name=content]");
	 if ($(objs).val()=="") {
	    alert("\n请您填写留言内容！");
	    $(objs).focus();
	    return (false);
	 }
	 
	 

 }); 
});  

</script>

     
     
