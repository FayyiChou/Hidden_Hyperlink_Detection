<script>
$(function(){
	var num=$(".gg_tp").find("td").length;
    for(i=1;i<=num;i++){
		if(i==1){  //根据图片数量加入按钮
			$(".gg_con ul").append("<a href=\"javascript:void();\" class=\"over\" >1</a>"); //第一个按钮默认
		}else{
	      $(".gg_con ul").append("<a href=\"javascript:void();\">"+i+"</a>"); 
		}
	}
	
	var over_num=0;
	
	
	$(".gg_con a").click(function(){
		$(".over").removeClass("over");
		 $(".gg_tp table").stop();
		$(".gg_con").find("img[src*='dian_over.png']").attr("src","images/dian_out.png"); //改变之前的按钮图片		
		over_num=($(this).index());  //判断是第几个按钮
		$(this).find("img").attr("src","images/dian_over.png"); //改变当前的按钮图片
		$(".gg_tp table").animate({"margin-left":"-"+(100*over_num)+"%"});	//根据第几个按钮滚动图片到相应的图片位置
		$(this).addClass("over");
		$(".tp_wz ul").css("display","none");
		var leftnum=$(".tp_wz ul:eq("+over_num+")").css("margin-left");		
		$(".tp_wz ul:eq("+over_num+")").delay(500).css({"margin-top":"-40px","display":""}).animate({"margin-top":"20px"});
		
		//.css({"margin-left":"0px","display":""}).animate({marginLeft:leftnum});
	    
	});
});
</script>

