<script>
var effect = ['cube','cubeRandom','block','cubeStop','cubeHide','cubeSize','horizontal','showBars','showBarsRandom','tube','fade','fadeFour','paralell','blind','blindHeight','blindWidth','directionTop','directionBottom','directionRight','directionLeft','cubeStopRandom','cubeSpread','cubeJelly','glassCube'],

in_array = function ( needle, array, bool ) {
	if ( typeof needle=="string" || typeof needle=="number" ) {  
		var len = array.length;
		for ( var i=0; i<len; i++ ) {  
			if ( needle === array[i] ) {  
				if ( bool ) { 
					return i; 
				}
				return true;  
			}
		}
		return false;  
	}
},

// 设置skitter不重复特效
set_effect = function( imgs ) {
	var a = [];
	$(imgs).each(function(index, element) {
		var x = parseInt(24*Math.random());
		if(a.length > 0){
			while(in_array(x, a)){
    			x = parseInt(24*Math.random());
			}
		}
		$(this).removeClass().addClass(effect[x]);
		a.push(x);
	});
},
	
start_skitter = function( obj ) {
	$(obj).css({ width: 1000, height: 280 }).skitter({
		show_randomly: false, 
		navigation: false, 
		dots: true, 
		numbers_align: "center", 
		interval: 5000,
		progressbar: false,
		controls: false, //控制 播放暂停
		enable_navigation_keys: true,
		label: false,
		preview: false,
		velocity: 50
	});
};

$(function() {
    // Skitter
	var skitter = document.getElementById('skitter');
	if( skitter ) {
		set_effect($('#skitter img'));
		start_skitter('.box_skitter_large');
	}
});
</script>


