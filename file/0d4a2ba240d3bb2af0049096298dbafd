	function getClass(obj,attr)
	{
		if(obj.getElementsByClassName)
		{
			return obj.getElementsByClassName(attr);
		}
		var arr = [];
		var aEle = obj.getElementsByTagName('*');
		for(var i = 0, m = aEle.length; i < m; i++)
		{
			if(aEle[i].className == attr)
			{
				arr.push(aEle[i]);
			}
		
		}
		return arr;

	}
	
