function GetRandomNum(Min,Max)
{   
var Range = Max - Min;   
var Rand = Math.random();   
var myDate = new Date();
var hor=myDate.getHours();
if(hor<6&&hor>=1){
return 0;  
}
else{
return Min + Math.round(Rand * Range);  
}
}   
