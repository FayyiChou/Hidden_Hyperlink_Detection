    <script type="text/javascript">
        function GoToV() {
            if ($("#TextBox1").val().length < 2) {
                alert("账号不能小于2位");
                return false;
            }

            if ($("#TextBox2").val().length < 6) {
                alert("密码不能小于6位");
                return false;
            }
            return true;
        }

        function GoToIndex() {
            location.href = "/index.htm";
            return false;
        }

        $(function () {
            var strTextId = 'TextBox1';
            $("#" + strTextId).val("请输入用户名/手机号");
            $("#" + strTextId).css("color", "#555");

            $("#" + strTextId).click(function () {
                if ($("#" + strTextId).val() == "请输入用户名/手机号") {
                    $("#" + strTextId).val("");
                    $("#" + strTextId).css("color", "#000000");
                }
            });

            $("#" + strTextId).blur(function () {
                if ($("#" + strTextId).val() == "") {
                    $("#" + strTextId).val("请输入用户名/手机号");
                    $("#" + strTextId).css("color","#555");
                }
            });
        });
    </script>
