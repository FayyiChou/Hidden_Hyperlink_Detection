        $(function () {
            //默认礼品
            var firstGiftObj = $('#roll li').first();
            firstGiftObj.addClass("cur");
            var price = firstGiftObj.find("em").text();
            $('#stotal').text(price);

            //点击礼品
            $(".songli_list>li").live('click', function () {
                $(this).addClass("cur").siblings().removeAttr("class");
                var cur_money = $(this).find("em").html();
                $("#stotal").html(parseInt(cur_money));
            });

            //点击点赞
            $(".dianzan").click(function () {
                var obj = $(this);
                var blogId = obj.attr('blogId');
                $.getJSON("/Blog/SetGood", { "blogId": blogId }, function (json) {
                    if (json.isSuccess) {
                        if (json.type == 'decr') { //取消点赞
                            var cur_val = obj.find("span").html();
                            if (cur_val > 0) {
                                obj.find("span").html(parseInt(cur_val) - 1);
                            }
                            obj.removeClass("dianzan_end");
                        } else if (json.type == 'incr') { //点赞
                            obj.addClass("dianzan_end");
                            var cur_val = obj.find("span").html();
                            obj.find("span").html(parseInt(cur_val) + 1);
                        }
                    }
                    else {
                        layer.msg(json.type);
                        if (json.type == '未登录无法点赞') {
                            location.href = json.skiplink;
                        }
                    }
                });
            });

            //点击赠送
            $('.buyBtn').click(function () {
                var needMoney = $('#stotal').text();;
                var seq = 166;
                var giftId = $('.cur').attr('giftId');
                $('#giftId').val(giftId);

                $.ajaxSettings.async = false;
                $.getJSON("/Blog/BuyGift", { "needMoney": needMoney,"seq":seq,"giftId":giftId }, function (json) {
                    if (json.isSuccess) {
                        $('#sign').val(json.data);
                        $("#payForm").submit();
                    }
                    else {
                        if (json.data=='notLogin') { //未登录
                            location.href=json.skiplink;
                        }
                        else if(json.data=='moneyLack') { //余额不足
                            layer.msg('您的余额不足，请立即充值！');
                        }
                        else
                        {
                            layer.msg('赠送失败');
                        }
                    }
                });
                $.ajaxSettings.async = true;

            });


        });
    </script>


