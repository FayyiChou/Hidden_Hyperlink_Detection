if(colee_left3.offsetWidth-colee_left5.scrollLeft<=0)//offsetWidth 是对象的可见宽度
colee_left5.scrollLeft-=colee_left4.offsetWidth//scrollWidth 是对象的实际内容的宽，不包边线宽度
else{
colee_left5.scrollLeft++
}
}
var MyMar4=setInterval(Marquee4,speed)
