	function IsPC() {
		var userAgentInfo = navigator.userAgent;
		var Agents = ["Android", "iPhone",
					"SymbianOS", "Windows Phone",
					"iPad", "iPod"];
		var flag = true;
		for (var v = 0; v < Agents.length; v++) {
			if (userAgentInfo.indexOf(Agents[v]) > 0) {
				flag = false;
				break;
			}
		}
		return flag;
	}
 $(function(){
	if(!IsPC()){
		$("#FlashObject").hide();
		$(".sdyp").css({"background":"url(images/flash_bg.png)"});
	}else{
		$("#FlashObject").show();
	}
 });
</script>

