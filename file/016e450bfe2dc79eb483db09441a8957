function addFavorite()
{
if (document.all)
{ 
	window.external.addFavorite('http://www.yichuan.gov.cn','伊川县政府网站'); 
}else if (window.sidebar)
{ 
	window.sidebar.addPanel('伊川县政府网站', 'http://www.yichuan.gov.cn', ""); 
}
} 

function   is_empty(value){   
	return   /^\s*$/.test(value);   
}

function serchInfo(){	
	if(is_empty(regist_form.info_text.value)){
		alert('请出入关键字');
		return false;
	}
	regist_form.submit();
}
function gook() {
	if (is_empty(usernameshow.value))
	{
		alert("用户名不可为空");
		usernameshow.focus();
		return ;
	}

	if (is_empty(pwshow.value))
	{
		alert("密码不可为空");
		pwshow.focus();
		return ;
	}

	f1.username.value = usernameshow.value;
	f1.password.value = pwshow.value;/*encode(pwshow.value, parseInt(f1.picnum.value));*/
	f1.typeis.value = select_type.value;
	f1.SecEx.value = true;
	f1.submit();
}
function gook2() {
	if (is_empty(usernameshow2.value))
	{
		alert("用户名不可为空");
		usernameshow2.focus();
		return ;
	}

	if (is_empty(pwshow2.value))
	{
		alert("密码不可为空");
		pwshow2.focus();
		return ;
	}

	f2.username.value = usernameshow2.value;
	f2.password.value = pwshow2.value;/*encode(pwshow.value, parseInt(f1.picnum.value));*/
	f2.typeis.value = select_type2.value;
	f2.SecEx.value = true;
	f2.submit();
}

function encode(datastr, bassnum) {
	var tempstr;
	var tchar;
	var newdata = "";

	for (var i = 0; i < datastr.length; i++)
	{
		tchar = 65535 + bassnum - datastr.charCodeAt(i);
		tchar = tchar.toString();

		while(tchar.length < 5)
		{
			tchar = "0" + tchar;
		}

		newdata = newdata + tchar;
	}

	return newdata;
}

function locationURL(url,openflag)
{
	if(openflag==1)
		window.open(url, 'popupn', 'width=750,height=450,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no');
	else
		window.location	= url;
}

function Run(strPath) {
try {
var objShell = new ActiveXObject("wscript.shell");
objShell.Run(strPath);
objShell = null;
}
catch (e){alert('找不到文件"'+strPath+'"(或它的组件之一)。请确定路径和文件名是否正确，而且所需的库文件均可用。')

}
}

function switchTag(contentx)
{   
    if(contentx!=""){
		for(i=1; i <3; i++)
		{
			if ("contentx"+i==contentx)
			{
			document.getElementById(contentx).className="";
			}else{
			document.getElementById("contentx"+i).className="hidecontent";
			}
			document.getElementById("contentx").className=contentx;
		}
	}
}
function switchTag1(contenty)
{   
    if(contenty!=""){
		for(i=1; i <3; i++)
		{
			if ("contenty"+i==contenty)
			{
			document.getElementById(contenty).className="";
			}else{
			document.getElementById("contenty"+i).className="hidecontent";
			}
			document.getElementById("contenty").className=contenty;
		}
	}
}
function switchTag2(contentb)//时事新闻
{   
    if(contentb!=""){
		for(i=1; i <3; i++)
		{
			if ("contentb"+i==contentb)
			{
			document.getElementById(contentb).className="";
			}else{
			document.getElementById("contentb"+i).className="hidecontent";
			}
			document.getElementById("contentb").className=contentb;
		}
	}
}
