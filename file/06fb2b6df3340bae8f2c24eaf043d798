<script type="text/javascript">
function pwdBlur(keyword) {
	if(keyword == 'login'){
		if ($('#password').attr('value') == '') {
	        $('#password').hide();
	        $('#fakepassword').show();
	    }
	}
}
function pwdFocus(keyword) {
	if(keyword == 'login'){
		$('#fakepassword').hide();
		$('#password').show();
		$('#password').focus();
	}
}
function emptycheck(){
	if(document.getElementById("username").value==''){
		alert("请输入您的用户名");
		return false;
	}
	if(document.getElementById("password").value==''){
		alert("请输入您的密码");
		return false;
	}
	return true;
}
function emptysharecheck(){
	if(document.getElementById("sharenumber").value==''){
		alert("请输入您要查询的分享号码");
		return false;
	}
	return true;
}

</script>
