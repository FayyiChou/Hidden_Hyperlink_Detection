	<script>
		
	(function ($) {
	 //check if the browser width is less than or equal to the large dimension of an iPad
	 if ($(window).width() > 1024) {
		 $(window).load(function(){
				$("body").mCustomScrollbar({
					scrollButtons:{
						enable:true,
					},
					mouseWheelPixels: 500,
					callbacks:{
				        whileScrolling:function(){
							$('#top_work').fadeIn();
				        },
						onTotalScrollBack:function(){
							$('#top_work').hide();
			        	}
				    }
				});
				$("a[rel~='_scrollTo']").click(function(e){
					e.preventDefault();
					$("body").mCustomScrollbar("scrollTo",$(this).attr("href"));
					
				});
				if(typeof(document.location.hash) != 'undefined' && document.location.hash != "") $("body").mCustomScrollbar("scrollTo", document.location.hash);

				
			});
	        }
	    })(jQuery);
		
	</script>


<script>
$(document).ready(function(){

	$("#top_work").hide();
	
	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 100) {
				$('#top_work').fadeOut();
			} else {
				$('#top_work').fadeIn();
			}
		});

	});

});
</script>

