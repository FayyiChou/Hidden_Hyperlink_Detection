<script type="text/javascript">
function userBehaviorTrack2(obj,url,content){
	try{
		if(typeof(sendUserBeHaviorData)=="function"){
			var fillContent="";
			if(url!=null&&url!=""&&url!="undefined"){
				fillContent=url;
			}else if(obj!=null&&obj!=""){
				fillContent=$(obj).attr("href");
			}
			var platformStr = $("meta[name=track_pageId]").attr("content");
			var paramData='platformStr='+platformStr+'&dataType=00&extraction=00'+'&fieldName='+content+'&fillContent='+fillContent;
			sendUserBeHaviorData(paramData);
		}
	}catch(e){
		
	}
	if(url!=null&&url!=""){
		window.open(url);
	}
		
} 
</script>
