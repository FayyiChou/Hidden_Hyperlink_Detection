window.onload = function(){
		var accountName = decodeURIComponent($("#accountName").val());
		$("#accountName").val(accountName);
}
//会员中心问题和建议   modify by wangchenguang  2015-2-5  end -->

function checkPwd(){
	var pwd = $("#accountPwd").val();
	if(pwd.length==0){
 		$("#accountPwd").next("span").removeClass("hidden");
	}else{
		$("#accountPwd").next("span").addClass("hidden");
		$("#mobileOrMailOrID").hide();
	}
}
// $(function(){
// 	$("#mobileOrMailOrID").html("");
// })

//判断输入的用户名属于哪种类型（手机号/邮箱/身份证），根据类型不同，提示不同信息 by fukun-wb  start
function testName(){
	var username = $("#accountName").val();
	if((username.indexOf("***")>-1)&&($("#realName").val()!=null)){
		$("#mobileOrMailOrID").hide();
	}
	if((username.indexOf("***")>-1)&&($("#realName").val()==null)){
		$("#mobileOrMailOrID").show();
		$("#mobileOrMailOrID").html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>请输入正确的手机号码或邮箱或身份证！</span>");
// 		$("#accountPwd").val("");
	}
	if(username.length==0){
		$("#mobileOrMailOrID").hide();
 		//$("#mobileOrMailOrID").html("");
 		$("#accountName").next("span").removeClass("hidden");
 		$("#accountName").removeClass("input_error");
	}
	else{
		$("#mobileOrMailOrID").hide();
// 		$("#mobileOrMailOrID").html("");
	
	
	}	
}

		function testTeamName(){
			var teamUserName = $("#accountNameForTeam").val();
			if(teamUserName.length==0){
				$("#teamerror").hide();
			}
			$.ajax({
				url:"/auth/login_testTeamName.action",
				type:"post",
				dataType:"json",
				data:{teamUserName:$("#accountNameForTeam").val()},
				success: function(data){
					if(data!=null){
					var msg = data.NO;
					if(msg=="team"){
		         		$("#accountNameForTeam").focus();
		         		$("#teamerror").show();
		         		$("#teamerror").html("<span class='inp_error' style='top:0px;left:0px;'>账号不存在</span>");
// 		         		$("#accountPwdForTeam").val("");
					}
					}
			}
			})
		}
//判断输入的用户名属于哪种类型（手机号/邮箱/身份证），根据类型不同，提示不同信息 by fukun-wb  end 
