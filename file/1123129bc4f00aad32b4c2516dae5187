<script>
function filedSaveBehaviorShouXian(startDate,fieldName,fillContent){
	if(fillContent==""||fieldName==""||startDate==null)return;
	fieldName = fieldName.trim();
	fillContent = fillContent.trim();
	var _platformStr = $("meta[name='track_pageId']").attr('content');
	if(_platformStr==undefined||_platformStr==""){
		return;
	}
	var filedParamData = "platformStr="+_platformStr+"&dataType=00";
	var fillTime = getAccessTime();
	var lengthOfStayField = getLengthOfStayHere(startDate);
	filedParamData+="&fieldName="+fieldName+"&fillContent="+fillContent+"&fillTime="+fillTime+"&lengthOfStayField="+lengthOfStayField+"&extraction=00";
	sendUserBeHaviorData(filedParamData);
	filedParamData = "";
}
/**
 * Ã¥ÂÂÃ¥ÂÂÃ¥ÂÂ°Ã¦ÂÂÃ¤ÂºÂ¤Ã¦ÂÂ°Ã¦ÂÂ®(Ã¨Â¡ÂÃ¤Â¸ÂºÃ¦ÂÂ¶Ã©ÂÂ)
 *
 * @param paramData	Ã¥ÂÂÃ¦ÂÂ°
 *
 */
function sendUserBeHaviorData(paramData){

	var params = "referer=" + referer + "&clientProvinceName=" + clientProvinceName + "&clientCityName=" + clientCityName + "&clientOSName=" + clientOSName;
	if(paramData != undefined && "" != paramData){
		params = paramData + "&" + params;
	}
	//alert(params);
	//return;
	$.ajax({
	    url: "/userBehaviorRecord_sendUserBeHaviorData.action",
	    type: "post",
	    dataType: "json",
	    data: params,
	    success: function(data) {}
	});
}	

/**
 * Ã¥ÂÂÃ¥ÂÂÃ¥ÂÂ°Ã¦ÂÂÃ¤ÂºÂ¤Ã¦ÂÂ°Ã¦ÂÂ®(Ã¤ÂºÂÃ¥ÂÂ¨/Ã¨Â´Â­Ã¤Â¹Â°Ã¦ÂÂÃ¥ÂÂÃ¦ÂÂ°Ã¦ÂÂ®)
 *
 * @param paramData	Ã¥ÂÂÃ¦ÂÂ°
 *
 */
function sendUserInteractionData(paramData){
	var params = "referer=" + referer + "&tableFlag=I"+"&clientProvinceName=" + clientProvinceName + "&clientCityName=" + clientCityName + "&clientOSName=" + clientOSName + "&reqUrl=" + location.href;
	if(paramData != undefined && "" != paramData){
		params = paramData + "&" + params;
	}
	//alert("paySuccess-------"+params);
	//return;
	$.ajax({
	    url: "/userBehaviorRecord_sendUserBeHaviorData.action",
	    type: "post",
	    dataType: "json",
	    data: params,
	    success: function(data) {
	    }
	});
}



$(function(){
    var metaValue = $("meta[name=track_pageId]");   
    var pfs =metaValue.attr("content");
    if(pfs==undefined||""== pfs) return;
	 var dt ="00";
	 var para =  "platformStr=" + pfs + "&dataType=" + dt;
	 sendUserBeHaviorData(para)
    
});
</script>

	
<script>
String.prototype.trim=function(){return this.replace(/(^\s*)|(\s*$)/g,"");}
var startDate=null;
$(function(){
   $(".inputRecord").focus(function(){
		startDate = new Date();
	});
   $(".inputRecord").blur(function(){
      var fieldContent = $(this).val();
      var fieldName = $(this).attr("placeholder");
      //console.info(fieldContent);
      //console.info(fieldName);
      filedSaveBehavior(startDate,fieldName,fieldContent);
   });
   
   $(".inputRecord2").focus(function(){
		startDate = new Date();
	});
  $(".inputRecord2").blur(function(){
     var fieldContent = $(this).val();
     var fieldName = $(this).attr("fieldName");
     var fieldType = $(this).attr("type");
     if(fieldType=="password"){
    	 fieldContent="******";
     }
     //console.info(fieldContent);
     //console.info(fieldName);
     filedSaveBehavior(startDate,fieldName,fieldContent);
  });
  
   
});

//20170106_06 单点登录系统操作轨迹补全 zhangyuan-wb 2016-02-26 start
function userBehaviorTrack_auth(obj,url,content){
    var _platformStr = $("meta[name='track_pageId']").attr('content');
 	if(_platformStr == undefined || _platformStr == null || _platformStr == ""){
		return;
   	} 
    var dataType ="00";
    var paramData = 'platformStr='+_platformStr+'&dataType='+dataType+'&extraction=00';
    var fieldName=content;
    var fillContent="";
    if(url!=null&&url!=""&&url!="undefined"){
    	fillContent=url;
    }else if(obj!=null&&obj!=""){
    	fillContent=$(obj).attr("href");
    }
    if(fillContent==null||fillContent=="undefined"){
    	fillContent="";
    }
    paramData+="&fieldName="+fieldName+"&fillContent="+fillContent;
    sendUserBeHaviorData(paramData);
    if(url!=null&&url!="undefined"&&url!=""){
    	 window.location.href=url;
    }
}

function userBehaviorTrackResult(url,content,result){
    var _platformStr = $("meta[name='track_pageId']").attr('content');
 	if(_platformStr == undefined || _platformStr == null || _platformStr == ""){
		return;
   	} 
   	var fieldName=content;
   	var fillContent="";
    var dataType ="00";
    var paramData = 'platformStr='+_platformStr+'&dataType='+dataType+'&extraction=00&operationResult='+result+"&fieldName="+fieldName+"&fillContent="+fillContent;
    sendUserBeHaviorData(paramData);
    if(url!=null&&url!="undefined"&&url!=""){
    	 window.location.href=url;
    }
}
// 20170106_06 单点登录系统操作轨迹补全 zhangyuan-wb 2016-02-26 end 
function filedSaveBehavior(startDate,fieldName,fillContent){
	if(fillContent==""||fieldName==""||startDate==null)return;
	fieldName = fieldName.trim();
	fillContent = fillContent.trim();
	var _platformStr = $("meta[name='track_pageId']").attr('content');
	if(_platformStr==undefined||_platformStr==""){
		return;
	}
	var filedParamData = "platformStr="+_platformStr+"&dataType=00";
	var fillTime = getAccessTime();
	var lengthOfStayField = getLengthOfStayHere(startDate);
	filedParamData+="&fieldName="+fieldName+"&fillContent="+fillContent+"&fillTime="+fillTime+"&lengthOfStayField="+lengthOfStayField+"&extraction=00";
	sendUserBeHaviorData(filedParamData);
	filedParamData = "";
}

