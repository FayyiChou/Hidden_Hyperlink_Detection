<script type="text/javascript">
/**
 * 向后台提交数据
 *
 * @param platform  平台
 * @param weixinOpenId 微信ID
 * @param fromUrl 访前链接地址
 * @param operation 操作
 * @param loginType 登录类型
 *
 */

function sendStatisticData(operation,status,description,accountname)
{
	var urll=window.location.href;
	fromUrl=urll.split("redirectUrl=")[1];
	platform="pc";
	loginType="01";
    $.ajax({
        url: "/auth/loginStatistic_sendStatisticData.action",
        type: "post",
        dataType: "json",
        data:"platform=" + platform + "&fromUrl=" + fromUrl+ "&operation=" + operation+ "&loginType=" + loginType+ "&status=" + status+ "&description=" + description+ "&accountName=" + accountname,
        success: function(data) {}
    });
}

function forgetpwd(){
	 userBehaviorTrack_auth('','','点击找回密码');
	sendStatisticData("03","","","");
	window.open("http://auth.sinosig.com/resetPwd.jsp");
}
var hasCheckVerifyCode = false;
var nameSign = true;	
//提交登录信息
function agentLogin() {
	userBehaviorTrack_auth('','','个人用户-登录');
	if (validate())
	{
		mask();
		var QRCode = 'null';
		if(QRCode && QRCode != 'null'){
			var iWidth=350; //弹出窗口的宽度;
			var iHeight=250; //弹出窗口的高度;
			var iTop = (window.screen.availHeight-30-iHeight)/2; //获得窗口的垂直位置;
			var iLeft = (window.screen.availWidth-10-iWidth)/2; //获得窗口的水平位置;
			var winQRCode = window.open(QRCode,'','height='+iHeight+',,innerHeight='+iHeight+',width='+iWidth+',innerWidth='+iWidth+',top='+iTop+',left='+iLeft+',toolbar=no,menubar=no,scrollbars=auto,resizeable=no,location=no,status=no'); 
			winQRCode.document.write("<TITLE>扫描二维码关注官微</TITLE>");
			winQRCode.document.write("<div style=\"text-align:center; \"><img src=\""+QRCode+"\" style=\"padding-top:50px;padding-bottom:50px;\" \/><\/div>");
			winQRCode.document.close();
			$("#agentLoginForm").submit();
		}else{
			$("#agentLoginForm").submit();
		}
	}
}


function teamLogin() {
	userBehaviorTrack_auth('','','团体用户-登录');
	if (validateTeam())
	{
		mask();
		$("#agentLoginForm").submit();
	}
}



//提交前的验证
function validate()
{
	//验证用户名
	var accountName=$("#accountName");
	var accountNameVal=$.trim(accountName.val());
	if (accountNameVal=="" || !nameSign)
	{
		$("#checkNull").hide();
		$("#mobileOrMailOrID").show();
		$("#mobileOrMailOrID").html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>请输入手机号码或邮箱或身份证！</span>");
		accountName.focus();
		userBehaviorTrackResult('','个人用户-登录','请输入手机号码或邮箱或身份证！');
		return false;
	}	
	//验证密码
	var accountPwd=$("#accountPwd");
	var accountPwdVal=$.trim(accountPwd.val());
	if (accountPwdVal=="")
	{
		$("#checkNull").hide();
		//$("#checkNull").html();
		$("#mobileOrMailOrID").show();
		$("#mobileOrMailOrID").html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>请输入密码</span>");
		accountPwd.focus();
		userBehaviorTrackResult('','个人用户-登录','请输入密码');
		return false;
		
	}
	if (accountPwdVal.length<6)
	{
		$("#checkNull").hide();
		//$("#checkNull").html();
		$("#mobileOrMailOrID").show();
		$("#mobileOrMailOrID").html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>用户名或密码错误，请重新输入!</span>");
		accountPwd.select();
		userBehaviorTrackResult('','个人用户-登录','请输入6～20位的密码!');
		return false;
	}
	if (!hasCheckVerifyCode) {
		$("#checkNull").hide();
		//$("#checkNull").html();
		$("#mobileOrMailOrID").show();
		$("#mobileOrMailOrID").html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>请拖动滑块进行验证</span>");
		userBehaviorTrackResult('','个人用户-登录','请拖动滑块进行验证');
		return false;
	}
	
	return true;
}

function validateTeam()
{
	//验证用户名
	var accountName=$("#accountNameForTeam");
	var accountNameVal=$.trim(accountName.val());
	if (accountNameVal=="" || !nameSign)
	{
		$('#teamerror').show();
		$('#teamerror').html("<span class='inp_error' style='top:0px;left:0px;'>请输入团体用户名</span>");
		userBehaviorTrackResult('','团体用户-登录','请输入团体用户名');
		accountName.focus();
		return false;
	}else{
		var objVal=accountNameVal;
		if(objVal.length>=4 && objVal.length<=20){
			var rex="/^[a-zA-Z]{1}([a-zA-Z0-9]){1,}$/";
		    if(rex.match(objVal)){
		    	//$('#teamerror').html("<span class='inp_error'>用户名错误，用户名为4-20位字母或数字与字母的组合</span>");
		    	//accountName.select();
		    	return false;
		    }else {
				$('#teamerror').hide();
			}
		}else{
			//$('#teamerror').html("<span class='inp_error'>用户名错误，用户名为4-20位字母或数字与字母的组合</span>");
			//accountName.select();
	    	return false;
		}
	}	
	//验证密码
	var accountPwd=$("#accountPwdForTeam");
	var accountPwdVal=$.trim(accountPwd.val());
	if (accountPwdVal=="")
	{
		$('#teamerror').show();
		$('#teamerror').html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>请输入密码</span>");
		accountPwd.focus();
		userBehaviorTrackResult('','团体用户-登录','请输入密码');
		return false;
	}
	if (accountPwdVal.length<6)
	{
		$('#teamerror').show();
		$('#teamerror').html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>用户名或密码错误，请重新输入</span>");
		accountPwd.select();
		userBehaviorTrackResult('','团体用户-登录','请输入6～20位的密码');
		return false;
	}
	
	//验证验证码
	var verifycode=$("#verifycodeForTeam");
	if ($.trim(verifycode.val())=="")
	{
		$('#teamerror').show();
		$('#teamerror').html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>请输入验证码</span>");
		verifycode.focus();
		userBehaviorTrackResult('','团体用户-登录','请输入验证码');
		return false;
	}
	return true;
}

/**
 * 校验验证码
 * @date 2014年5月15日 
 */
var verifycodeArr = new Array();
var isCheck = false;
function checkVerifyCode(){
	var verifycode = $("#verifycode");
	var trimVerifycode = $.trim(verifycode.val());
	if(trimVerifycode == ""){
		verifycode.removeClass("verifycode_err").removeClass("verifycode_cor");
		return;
	}
	if(trimVerifycode != "" && verifycodeArr[0] != trimVerifycode){
		$.ajax({ 
			url: "/auth/login_checkVerifyCode.action", 
			type: 'post',
			async: false,
			data: {verifycode: trimVerifycode},
			dataType: 'json',
			success: function(data){
				isCheck = data.check;
				if(data.check){
					verifycode.removeClass("verifycode_err").addClass("verifycode_cor");
				}else{
					verifycode.removeClass("verifycode_cor").addClass("verifycode_err");
				}
				// 弹出最后一个元素
				if(verifycodeArr.length > 0){
					verifycodeArr.pop();
				}
				// 验证后添加到数组中
				verifycodeArr.push(trimVerifycode);
			}
		});
	}
}

var verifycodeArrForTeam = new Array();
var isCheckForTeam = false;
function checkVerifyCodeForTeam(){
	var verifycode = $("#verifycodeForTeam");
	var trimVerifycode = $.trim(verifycode.val());
	if(trimVerifycode != ""){
		$.ajax({ 
			url: "/auth/login_checkVerifyCode.action", 
			type: 'post',
			async: false,
			data: {verifycode: trimVerifycode},
			dataType: 'json',
			success: function(data){
				isCheckForTeam = data.check;
				if(verifycodeArrForTeam.length > 0){
					verifycodeArrForTeam.pop();
				}
				verifycodeArrForTeam.push(trimVerifycode);
			}
		});
	}
}

function checkOnAccountPwdChanged(){
    obj=$(this);
	var objVal=$.trim(obj.val());
	if(objVal==null||objVal=="")
	{
	    $("#tips_1").removeClass("tip-hide");
		return false;
	
	}else {
	    changeCssHide_1();
	 }
}
function checkOnVerifycodeChanged(){
    obj=$(this);
	var objVal=$.trim(obj.val());
	if(objVal==null||objVal=="")
	{
	    $("#tips_2").removeClass("tip-hide");
		return false;
	
	}else {
	    changeCssHide_2();
	 }
}
var xiaLa ;

function checkOnValChanged()
{   
	
	obj=$(this);
	var objVal=$.trim(obj.val());
	var itmId=this.id;
	
	if(objVal==null||objVal=="")
	{
	    $("#tips").removeClass("tip-hide");
	  //  setMarkAndMessage(obj,null,"");
		//obj.select();
		return false;
	
	}else {
	    changeCssHide();
		if (isEmail(objVal)|| checkMobile(objVal))
		{
		}else {
			if (checkIdcard(objVal)){
				nameSign = false;				
				$("#accountList").empty();
				$("#accountName").attr("name", "sso_userName_show");
				$("#accountName").attr("disabled", "disabled");
				$.ajax({
					url:"/auth/login_getAccountsByCharacternum.action",
					type:"post",
					dataType:"json",
					data:"characternum="+objVal,
					success: function(msg){
						if(msg.accountList.length == 0){
							var tipTextNode = $("<span class='inp_error' style='top:0px;left:0px;width:262px;'>该身份证信息未被使用过，请先注册阳光会员，绑定身份证信息后再进行登录</span>");
							$("#accountList").append(tipTextNode);
							$("#accountName").attr("name", "sso_userName");
							$("#accountName").attr("disabled", "");
// 							$("#accountPwd").val("");
							nameSign = true;
							return false;
						}
						if(msg.accountList.length != 0){
						$("#accountPwd").attr("disabled", "disabled");
						var tipTextNode = $("<li class='xUanZe' style='background: #eee'>请选择常用登录方式：</li>");
						$("#accountList").append(tipTextNode);
						$(".user_sel").removeClass("hidden");
						$(".history_list").removeClass("hidden");
						$("#accountName").removeClass("input_error");
						//$("#accountName").attr("readonly","readonly");
						xiaLa = true;
						//显示账号列表
						for(var i=0;i<msg.accountList.length;i++)
						{
							if(msg.accountList[i].type == "01"){
								
								//mail
								var mail = msg.accountList[i].accountName;
								var mailSplit = mail.split("@");
								var mailOne = mailSplit[0];
								if(mailOne.length>=5){
									mail = mailOne.substr(0,3)+"***"+mailOne.substr(mailOne.length-2)+mail.substr(mailOne.length);
								}
								if(mailOne.length==4){
									mail = mailOne.substr(0,1)+"**"+mailOne.substr(mailOne.length-1)+mail.substr(mailOne.length);
								}
								if(mailOne.length<=3){
									mail = mailOne.substr(0,1)+"*"+mailOne.substr(mailOne.length-1)+mail.substr(mailOne.length);
								}
								var mailNode = $("<li class='historyList' id='forHidden"+i+"' onclick='hiddenLi()' realV='"+msg.accountList[i].accountName+"'>"+mail+"</li>");
								mailNode.click(function(){			
									$("#accountName").attr("value", $(this).html());
									//$("#accountName").attr("readonly","readonly");
									$("#realName").attr("value", $(this).attr("realV"));
									nameSign = true;
									$("#accountPwd").attr("disabled", "");
								});
								$("#accountList").append(mailNode);
								$(".historyList").hover(function() {
									$(this).addClass("his_bg");
								}, function() {
									$(this).removeClass("his_bg");
								});
							}
							if(msg.accountList[i].type == "02"){
								$("#accountPwd").blur();
								//phone
								var inputNode = $("<li class='historyList' id='forHidden"+i+"' onclick='hiddenLi()' realV='"+msg.accountList[i].accountName+"'>"+msg.accountList[i].accountName.substr(0, 3) + "***" + msg.accountList[i].accountName.substr(7, 4)+"</li>");
								inputNode.click(function(){
									$("#accountName").attr("value", $(this).attr("realV").substr(0, 3) + "***" + $(this).attr("realV").substr(7, 4));
									//$("#accountName").attr("readonly","readonly");
									$("#realName").attr("value", $(this).attr("realV"));
									nameSign = true;
									$("#accountPwd").attr("disabled", "");
								});
								$("#accountList").append(inputNode);
								$(".historyList").hover(function() {
									$(this).addClass("his_bg");
								}, function() {
									$(this).removeClass("his_bg");
								});
							}
						}
						}
					}
					 	});	
						}else{
							obj.select();
							return false;
						}
					}
				}
}


	function checkOnValChangedForTeam()
	{   
		obj=$(this);
		var objVal=$.trim(obj.val());
		var itmId=this.id;
		
		if(objVal==null||objVal=="")
		{
			return false;
		
		}else {
		if(objVal.length>=4 && objVal.length<=20){
			var rex="/^[a-zA-Z]{1}([a-zA-Z0-9]){1,}$/";
		    if(rex.match(objVal)){
		    	//$('#teamerror').html("<span class='inp_error'>用户名错误，用户名为4-20位字母或数字与字母的组合</span>");
		    	obj.select();
		    	return;
		    }else {
				$('#teamerror').text('');
			}
			}
		}
		
	}

function active()
{
    var nameValue = $.trim($("#accountName").val());
    var type ;
    if(isEmail(nameValue))
    {
        $("#type").val("01");
    }else if(checkMobile(nameValue))
    {
         $("#type").val("02");
    }
    var loginForm=$("#agentLoginForm");
	loginForm.attr("action","http://auth.sinosig.com/auth/regist_deliver.action");
	loginForm.submit();
}

