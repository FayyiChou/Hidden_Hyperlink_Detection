//登陆时，如果参数有用户名密码，赋值
$(function(){
	var accountNameTM = 'null';
	if(accountNameTM && accountNameTM != 'null'){
		$("#accountName").val(accountNameTM);
		kdownName();
	}
	var accountPwdTM = 'null';
	if(accountPwdTM && accountPwdTM != 'null'){
		$("#accountPwd").val(accountPwdTM);
		kdownPwd();
		checkPwd();
	}
	
	$("#checkboxForTeam").click(function(){
		if($(this).is(':checked')){
			userBehaviorTrack_auth('','','团体用户-记住用户名');
		}else{
			userBehaviorTrack_auth('','','团体用户-不记住用户名');
		}
	})
	
	$("#checkbox").click(function(){
		if($(this).is(':checked')){
			userBehaviorTrack_auth('','','个人用户-记住用户名');
		}else{
			userBehaviorTrack_auth('','','个人用户-不记住用户名');
		}
	})
	
})
