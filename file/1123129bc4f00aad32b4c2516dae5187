		function kdownName(){
			$("#accountName").next("span").addClass("hidden");
		}
		function kdownPwd(){
			$("#accountPwd").next("span").addClass("hidden");
		}
		
			$(function() {
				$(".info_menu").find("li").each(function(index, obj) {
					$(obj).bind("click", function() {
						$(".info_menu li").removeClass("active").eq(index).addClass("active");
						$(".info_frame").addClass("hidden").eq(index).removeClass("hidden");
					});
				});
				$(".cover").bind("click", function() {
					$(this).siblings("input").focus();
				});
				
				$("#showOrHide").click(function(){
					$(".history_list").toggleClass("hidden");
					$("#accountPwd").attr("disabled", "");
				})
				
				//错误提示信息div隐藏
				$("#mobileOrMailOrID").hide();
				
				//记住账户
				$(".cus_check").click(function() {
					$(this).toggleClass("cus_on");
				});
				
				//登录方式
				$(".mode").hover(function() {
					$(this).addClass("mode_on");
				}, function() {
					$(this).removeClass("mode_on");
				});
				
				//点击清除按钮
				$(".clear_input").click(function(){
					userBehaviorTrack_auth('','','清除输入框');
					$(this).siblings("input.inp_all").val("").focus();
				});
				
				$("input").keyup(function(){
					if($(this).val()==null || $(this).val()==''){
				$(this).siblings("div.clear_input").css("display","none");
					}else{
				$(this).siblings("div.clear_input").css("display","block");
					}
				$(this).parent().parent().siblings().find("div.clear_input").css("display","none");
					});
				})

			
				function hiddenLi(){
					$(".xUanZe").addClass("hidden");
					$(".history_list").addClass("hidden");
				}
			
			
		</script>
		
		
		<script type="text/javascript">
		
			//20170223_07 注册登录功能优化-登录 lihuijin 2017年2月24日18:26:02 start
			function usernameKeyUp(){
				var value = $('#accountName').val();
				value=value.replace(/[^\a-\z\A-\Z0-9\@\_\.\*\-]/g,'');
				value=value.replace(/(\-)+/g, '-');
				value=value.replace(/(\_)+/g, '_');
				$('#accountName').val(value);
			}
			function usernamePaste(){
				var value = $('#accountName').val();
				value=value.replace(/[^\a-\z\A-\Z0-9\@\_\.\*\-]/g,'');
				value=value.replace(/(\-)+/g, '-');
				value=value.replace(/(\_)+/g, '_');
				$('#accountName').val(value);
			}
			//20170223_07 注册登录功能优化-登录 lihuijin 2017年2月24日18:26:02 end
		</script>
	
