function selectTabIndex(){
	var indexOf=0;
	$(".info_menu li").each(function(index,obj){
		var hasClass=$(obj).hasClass("active");
		if(hasClass==true){
			indexOf=index;
			return false;
		}
    });
	return indexOf;
}

$(function(){
	$(".info_menu li").each(function(index,obj){
        $(obj).bind("click",function(){
            $(obj).addClass("active").siblings("li").removeClass("active");
            $(".info_frame").addClass("hidden");
            $(".info_frame").eq(index).removeClass("hidden");
            if(index==0){
            	userBehaviorTrack_auth('','','个人用户');
            	$('#verifycodeImgButton').trigger('click');
            	$('#tabIndex').val('0');
            	window.location.href='http://auth.sinosig.com/login.html?redirectUrl=http://home.sinosig.com/pc/home/home!index.action';
            }
            if(index==1){
            	userBehaviorTrack_auth('','','团体用户');
            	$('#verifycodeImgButtonForTeam').trigger('click');
            	$('#tabIndex').val('1');
            	window.location.href='http://auth.sinosig.com/login.html?tabIndex=1&redirectUrl=http://grp.sinosig.com/pc/home/team_home!index.action'; 
            }
        });
    });
	var tabIndex="";
	var paramTabIndex="";
	if(paramTabIndex=='1'){
		tabIndex="1";
	}
	if(tabIndex=="1"){
		$(".info_menu li").each(function(index,obj){
			if(index==1){
				$(obj).addClass("active").siblings("li").removeClass("active");
	            $(".info_frame").addClass("hidden");
	            $(".info_frame").eq(index).removeClass("hidden");
	            $('#tabIndex').val('1');
            	//$('#verifycodeImgButtonForTeam').trigger('click');
            }
		});
	}
	
	$("#teamerror").hide();
	
	
	
	//登录回车事件
	$("input").each(function(){
		$(this).bind("keypress",function(e){
			var keycode=e.keyCode?e.keyCode:e.which;
			if (keycode==13) {
				var indexOf = selectTabIndex();
				if(indexOf==0){
					agentLogin();
				}
				if(indexOf==1){
					teamLogin();
				}
			}
		});
		
	});
	
	//页面加载时，集点移到第一个输入框，即用户名输入框
	var indexOf = selectTabIndex();
	if(indexOf==0){
		var focusObj=$("#accountName");
		if (focusObj.val()=="") 
		{
		    focusObj.focus();
		    changeCss();
		}else{
			changeCssHide();
		}
	}
	
	if(indexOf==1){
		var focusObj=$("#accountNameForTeam");
		if (focusObj.val()=="") 
		{
		    focusObj.focus();
		}
	}
	
	
	//集点移开事件挷定
	$("#accountName").bind("blur",checkOnValChanged);
	$("#accountNameForTeam").bind("blur",checkOnValChangedForTeam);
	$("#accountNameForTeam").bind("keypress",function(){$('#teamerror').text('');});
	$("#accountPwd").bind("blur",checkOnAccountPwdChanged);
	//$("#verifycode").bind("blur",checkOnVerifycodeChanged);
	$("#accountName").bind("focus",changeCss);
	$("#accountName").bind("keypress",changeCssHide);
	$("#accountPwd").bind("focus",changeCss_1);
	$("#accountPwd").bind("keypress",changeCssHide_1);
	$("#accountPwdForTeam").bind("keypress",function(){$('#teamerror').text('');});
	//$("#verifycode").bind("focus",changeCss_2);
	//$("#verifycode").bind("keypress",changeCssHide_2);
	$("#verifycodeForTeam").bind("keypress",function(){$(this).parent().addClass('tip-hide');$('#teamerror').text('');});
	
	
	$(".dia_c").click(function(){
        $(this).siblings('.chuqu').focus();
    });

    $(".chuqu").each(function(){
        var thisVal = $(this).val();
        if (thisVal != "") {
            $(this).siblings('.dia_c').hide();
        } else {
            $(this).siblings(".dia_c").show();
        }
        $(this).keyup(function () {
            var val = $(this).val();
            $(this).siblings(".dia_c").hide();
        }).blur(function () {
            var val = $(this).val();
            if (val != "") {
                $(this).siblings(".dia_c").hide();
            } else {
                $(this).siblings(".dia_c").show();
            }
        })
    });
    
    
	$("#tips").bind("click",function(){
	 $("#accountName").focus();
	});
	$("#tips_1").bind("click",function(){
	 $("#accountPwd").focus();
	});
	
	$("#verifycodeImgButtonForTeam").click(function(){
		userBehaviorTrack_auth('','','刷新验证码');
		 changeValidateKey(document.getElementById("verifycodeImgForTeam"));
	});
	 var ret = "";
	 if(ret!=null&&ret!=""&&ret!="undefined"){
		 userBehaviorTrackResult("","登录",ret);
	 }
	 if(indexOf==0){
		 	if(ret.length>20){
		 		$("#mobileOrMailOrID").hide();
				  $("#checkNull").html("<span class='inp_error' style='top:5px;left:33px;width:262px;'>"+ret+"</span>");
				  $("#accountPwd").val("");
// 				  if($("#checkMessage .the_login_yc_back").length<=0)$("#checkMessage").addClass("the_login_yc_back");
// 				  $("#checkMessage").show();
			 }else if(ret.length>0){
				 $("#mobileOrMailOrID").hide();
				  $("#checkNull").html("<span class='inp_error'>"+ret+"</span>");
				  $("#accountPwd").val("");
			 }
	 }
	 if(indexOf==1){
		 if(ret.length>0){
				$('#teamerror').show();
				$('#teamerror').html("<span class='inp_error' style='top:0px;left:0px;width:262px;'>"+ret+"</span>");
		 }
	 }
	 
	var nc = new noCaptcha();
	nc.init({
		renderTo:'dom_id',//渲染到该DOM ID指定的Div位置
		appkey : '1945' , // 应用标识
		token : '505ed27d832795dd5933f643384d588c',
		customWidth:310,
		callback: function (data) {// 校验成功回调
			userBehaviorTrack_auth('','','滑块通过');
			$("#csessionId").val(data.csessionid);
			$("#csig").val(data.sig);
			hasCheckVerifyCode = true;
		},
		error : function(e){//失败回调（网络超时类型失败）
			console.error(e);
		}
	});
	nc.show();
	
});
