		<script type="text/javascript">
		//登陆时，若用户名栏有值，则去掉提示信息
		$(function(){
			
			//输入框提示
			$(".inp_all").bind("keyup blur paste", function() {
				var s = $(this).val();
				if (s != "") {
					$(this).next("span").addClass("hidden");
				} else {
					$(this).next("span").removeClass("hidden");
				}
			});
			
			var a = $("#accountName").val();
			if(a.length!=0){
				$("#accountName").next("span").addClass("hidden");
			}else{
				$("#accountName").next("span").removeClass("hidden");
			}
		})
		</script>
		
		<script type="text/javascript">
		$(function(){
			var bPwd = $("#accountPwd").val();
			if(bPwd.length!=0){
// 				alert("111");
				$("#accountPwd").next("span").addClass("hidden");
			}else{
// 				alert("000");
				$("#accountPwd").next("span").removeClass("hidden");
			}
		})
		</script>					
		
		
