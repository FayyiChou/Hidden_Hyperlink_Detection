    <script language="javascript" type="text/javascript">
     var userid=0;
     function getUserid()
     {
	    $.ajax({url: 'http://login.interface.baomihua.com/userapi.asmx/GetCurrentLoginUserInfo',
			    data: 'flag=json',
			    dataType: 'jsonp',
			    jsonp: 'jsoncallback',
			    success: function(data) {
			    if (typeof data !== 'undefined' && typeof data.UserInfoList !== 'undefined') {
					    userid = data.UserInfoList[0].UserId;
				    } else {
					    userid = 0;
				    }
                    }
		    });
      }

      window.onload=function(){ //页面初始化开始

        getUserid();//先获取当前用户id
        getDZCount();//获取点赞数 
        getCommentCount();//获取评论数
//        IsDianzan();//是否已经点赞 
//        IsShouCang();//是否已经收藏
        setTimeout("IsDianzan()","100");//是否已经点赞  间隔0.1秒、等css加载好再执行
        setTimeout("IsShouCang()","100");//是否已经收藏 间隔0.1秒、等css加载好再执行

} //页面初始化结束

