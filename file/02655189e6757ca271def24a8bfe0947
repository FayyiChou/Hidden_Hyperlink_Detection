        function maxWHT(img) {
            var vWidth = img.width;
            var vHeight = img.height;
            if (vWidth > vHeight) {
                img.width = 100;
                img.height = vHeight / vWidth * 100;
                img.style.marginTop = (100 - img.height) / 2;
            } else if (vWidth < vHeight) {
                img.height = 100;
                img.width = vWidth / vHeight * 100;
                img.style.marginTop = 0;
            } else {
                img.width = 100;
                img.height = 100;
                img.style.marginTop = 0;
            }
        }
        function setSel(obj) {
            if (obj.id != "LI2") {
                obj.className = "sel";
