                    function buscador(event){
                        var textCount = parseInt(document.getElementById('seek_text').value.length);
                        if(event)
                        {
                            if(textCount >= 3 && event.keyCode == 13)
                                document.getElementById("seek_form").submit();
                            else
                                return false;
                        }
                        else
                        {
                            if(textCount >= 3) document.getElementById("seek_form").submit();
                        }
