<div id="rightArrow" style="left: 150px; background-position: 0px 0px;"><a href="javascript:;" title="在线客户"></a></div>

<script type="text/javascript">
var flag=1;
$('#rightArrow').click(function(){
	if(flag==1){
		$("#floatDivBoxs").animate({left: '-150px'},300);
		$(this).animate({left: '-5px'},300);
		$(this).css('background-position','-0px 0');
		flag=0;
	}else{
		$("#floatDivBoxs").animate({left: '0'},300);
		$(this).animate({left: '150px'},300);
		$(this).css('background-position','0px 0');
		flag=1;
	}
});
</script>

