    $(function () {
        setTimeout(function () {
            $('.banbox .item0 .rollimg').animate({ top: 75, opacity: 1 }, {
                duration: 400, easing: 'easeOutBack', complete: function () {
                    $('.banbox .item0 h3').animate({ left: 711, opacity: 1 }, {
                        duration: 600, easing: 'easeInOutCirc', complete: function () {
                        }
                    });
                }
            });
        }, 400);

        $('.banbox').switchable({
            triggers: '&bull;',
            triggerType: 'click',
            duration: 2,
            effect: 'fade',
            autoplay: true,
            interval: 5
        });

        if ($('.caseWrap .case li').length > 5) {
            $('.caseWrap .case').switchable({
                triggers: '&bull;',
                panels: 'li',
                easing: 'ease-in-out',
                effect: 'scrollLeft',
                steps: 2,
                visible: 5, // important
                end2end: true,
                autoplay: true
            });
        }
    });
