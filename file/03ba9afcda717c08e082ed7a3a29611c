}
function lphyquotepost(pid){
	var postconetent=document.getElementById('content_'+pid).innerText;
	if(postconetent.length>50){
		postconetent=postconetent.substr(0,50)+'...';
	}
	document.getElementById('message').innerHTML='[quote]原帖由 [i]'+document.getElementById('author_'+pid).innerText+'[/i] 于 '+document.getElementById('postime_'+pid).innerText+' 发表\n'+postconetent+'[/quote]';
	window.location.hash="replyform";
}
