<script type="text/javascript">

 $(window).load(function() {
	var this_url = location.href;
	
		var show = true;
		var mouse_move = true;
		
		function timerIncrement() {
			mouse_move = false;
		}
		
		var idleInterval = setInterval(timerIncrement, 1000*60*2);

		$(document).mousemove(function(e){
			mouse_move = true;
			clearInterval(idleInterval);
			idleInterval = setInterval(timerIncrement, 1000*60*2);
		});
		
		var myTimer = setInterval(show_idle, 1000*60*2);
		
		function show_idle()
		{	
			//console.log(show);

			if(show && mouse_move==false){
				show = false;
				
				$.fancybox.open('<div style="width:980px;height:720px;"><iframe src="http://event.babyhome.com.tw/idle_status/?from=www" width="100%"; height="100%; overflow: hidden;"></iframe></div>',{
					onUpdate : function(){
						$('.fancybox-overlay').css({'z-index' : '9998'});
						$('.fancybox-inner').css({'width':'980px', 'height' : '720px','overflow':'hidden','z-index' : '9999'});
						if(_gaq){
							_gaq.push(['_trackEvent', 'idle_track', 'open', 'www']);
						}
					},
					afterClose : function(){
						show = true;
						$('.fancybox-overlay').hide();
						if(_gaq){
							_gaq.push(['_trackEvent', 'idle_track', 'close', 'www']);
						}
					}
				});
			}
			//console.log(sec);
			//console.log(show);
		}
});
</script>
