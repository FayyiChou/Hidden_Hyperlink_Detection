    <script language="javascript">
      var indexSearchForm = document.getElementById ('indexSearchForm');
      function switchSearchTag (o) {
        var f = indexSearchForm;
        var a = f.getElementsByTagName ('a');
        for (i=0; i<a.length; i++) {
          if (a[i].className == 'tag')
            a[i].parentNode.className = a[i].parentNode.className.replace ('tag_on', 'tag_off');
        }
        o.parentNode.className = o.parentNode.className.replace ('tag_off', 'tag_on');
        f.action = o.rel;
        return false;
      }
    </script>
