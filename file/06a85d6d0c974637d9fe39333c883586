document.onkeydown=function(e){
	e=window.event||e;
	if (e.keyCode==37){
		turnPerPage();
	 }else if(e.keyCode==39){
	 	turnNextPage();
	 }else if (e.keyCode==123){
		 e.keyCode = 0;
          return false;
	 }
} 

