	<script>
	var SecCfg=momo.admin.common.SecCfg;
	var EmotionMap=momo.admin.common.EmotionMap;
	function _filterEmotion(str){
    	var mos=str.match(/.*?\[(.*?)\].*?/g);
    	for (var i in mos){
    		var index=mos[i].match(/.*?\[(.*?)\].*?/)[1];    		
    		if(null!=EmotionMap[index]){
    			var png=EmotionMap[index].replace('.png','@2x.png');
    			str=str.replace('['+index+']','<img class="smalldrop" style="border:none;" src="/static/a2/img/xiaoshuidi/'+png+'"/>');    			
    		}    		
    	}    	
    	return str;	
    }
    $(function(){
        var str=$('.user-describe').html();
        $('.user-describe').html(_filterEmotion(str));
    });
	</script>



