<div align="center" onmouseout="interval = setInterval

('changePos()', delay)" onmouseover="clearInterval(interval)" style="position: absolute;z-index:999999; /*visibility: visible; */width: 217px; height: 176px; top: 0px; left: 0px;" id="img">
<table style="border:0px;">
   <tbody><tr>
       <td width="217" valign="middle" align="center" height="176" style="border:0px;background-image: url('/./images/20161101.jpg')" id="info_tab"  onclick="window.open('http://www.dltwhj.com/index.php?option=com_content&view=article&id=590:2015-07-01-02-31-25&catid=35:dropdown&Itemid=154/')">
           
                 <a href="http://www.dltwhj.com/index.php?option=com_content&view=article&id=590:2015-07-01-02-31-25&catid=35:dropdown&Itemid=154/"></a>
           
       </td>
