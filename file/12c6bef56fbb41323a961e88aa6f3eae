	<link rel="stylesheet" href="http://www.google.com/cse/style/look/default.css" type="text/css" />
	<script type="text/javascript">
	(function(config)
	{
		var parseQueryFromUrl = function()
		{
			var queryParamName = "q";
			var search = window.location.search.substr(1);
			var parts  = search.split('&');
			for (var i = 0; i < parts.length; i++)
			{
				var keyvaluepair = parts[i].split('=');
				if (decodeURIComponent(keyvaluepair[0]) == queryParamName)
				{
					return decodeURIComponent(keyvaluepair[1].replace(/\+/g, ' '));
				}
			}
			return '';
		}
		
		google.load('search', '1', {language : 'ru'});
		
		// head search form
		google.setOnLoadCallback(function()
		{
			var customSearchControl = new google.search.CustomSearchControl(config.uid);
			customSearchControl.setResultSetSize(google.search.Search.FILTERED_CSE_RESULTSET);
			var options = new google.search.DrawOptions();
			options.enableSearchboxOnly("http://www.gocruise.ru/search");
			customSearchControl.draw(config.headFormId, options);
		}, true);

		// results page search form
		google.setOnLoadCallback(function()
		{
			var queryFromUrl        = parseQueryFromUrl();
			var customSearchControl = new google.search.CustomSearchControl(config.uid);
			customSearchControl.setResultSetSize(google.search.Search.FILTERED_CSE_RESULTSET);
			customSearchControl.draw(config.resultsFormId);
			if (queryFromUrl)
			{
				customSearchControl.execute(queryFromUrl);
			}
		}, true);
	})({"headFormId":"search_field","resultsFormId":"search-results-form","uid":"013704922750424628817:6ltzw6nsjo4"});
	</script>
	
