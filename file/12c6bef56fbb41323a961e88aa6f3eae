									<script type="text/javascript">
$j.get('http://www.pac.ru/gocruise_notice.php', function(resp)
{
    if(!resp || !resp.length)
    {
        return;
    }
    var tpl = '<div class="important-notice"><h3>{{title}}</h3><div class="important-notice-content">{{content}}</div></div>';
    var html = '', i=0, l=resp.length;
    for(; i<l; i++)
    {
        html += tpl.replace('{{title}}', resp[i].title).replace('{{content}}', resp[i].content);
    }
    $j('.gocruise_notice.hidden .content_middle_bottom').html(html);
    $j('.gocruise_notice.hidden').show();
},'jsonp');
</script>								</div>
