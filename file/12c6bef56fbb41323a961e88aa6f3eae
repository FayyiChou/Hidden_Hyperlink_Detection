<script type="text/javascript">
if (!Function.prototype.bind)
{
	Function.prototype.bind = function (oThis)
	{
		if (typeof this !== "function")
		{
			// closest thing possible to the ECMAScript 5 internal IsCallable function
			throw new TypeError("Function.prototype.bind - what is trying to be bound is not callable");
		}

		var fSlice  = Array.prototype.slice,
			aArgs   = fSlice.call(arguments, 1),
			fToBind = this,
			fNOP    = function(){},
			fBound  = function()
			{
				return fToBind.apply(
					this instanceof fNOP ? this : oThis || window,
					aArgs.concat(fSlice.call(arguments))
				);
			};

		fNOP.prototype   = this.prototype;
		fBound.prototype = new fNOP();

		return fBound;
	};
}
window.Gagarin = function()
{
	this.id      = ++window.Gagarin.prototype.id;
	this.letters = [70, 85, 70, 72, 66, 89];
	$j(document).bind('keyup.gagarin'+this.id, this.checkLetter.bind(this));
	this.setTimeout();
};
window.Gagarin.letter    = 85;
window.Gagarin.prototype = {
	id : 0,

	checkLetter : function(e)
	{
		if(e && e.which != this.letters.shift())
		{
			this.destruct();
			return;
		}
		if(this.letters.length == 0)
		{
			this.unbind();
			this.clearTimeout();
			this.gogogo();
			return;
		}
		this.setTimeout();
	},

	setTimeout : function()
	{
		this.clearTimeout();
		this.timeout = setTimeout(this.destruct.bind(this), 3000);
	},

	clearTimeout : function()
	{
		if(!this.timeout)
		{
			return;
		}
		window.clearTimeout(this.timeout);
		this.timeout = undefined;
	},

	unbind : function()
	{
		$j(document).unbind('.gagarin'+this.id);
	},

	destruct : function()
	{
		this.unbind();
	},

	random : function(min, max, isInt)
	{
		if(isInt)
		{
			return Math.floor(Math.random() * (max - min + 1)) + min;
		}
		return Math.random() * (max - min) + min;
	},

	gogogo : function()
	{
		$j('BODY').append('<div style="height:500px;" id="gagarin-spacer"></div>');
		this.fade = $j('<div id="gagarinFade" style="position:absolute;top:0;left:0;background:#000;z-index:999999;"></div>');
		this.fade
			.css({
				opacity : 0,
				height  : $j(document).height(),
				width   : $j(window).width()
			})
			.appendTo('BODY')
			.animate({opacity:'0.9'}, 2000, this.startTheRocket.bind(this));

		var stars = Math.ceil($j(window).height() * $j(window).width() / 75 / 75);
		var xMin  = 0,
		    xMax  = $j(window).width(),
		    yMin  = $j(document).scrollTop(),
		    yMax  = yMin + $j(window).height();
		var x, y, radius, style, opacity;

		while(stars--)
		{
			radius  = this.random(1, 2, true);
			x       = this.random(xMin, xMax-radius, true);
			y       = this.random(yMin, yMax-radius, true);
			opacity = this.random(0.5, 1);
			style   = 'top:'+y+'px;left:'+x+'px;width:'+radius+'px;height:'+radius+'px;opacity:'+opacity+';filter: alpha(opacity='+opacity*100+');';
			$j('BODY').append('<div class="gagarin-stars" style="'+style+'"></div>');
		}
	},

	startTheRocket : function()
	{
		var top    = $j(document).scrollTop()+$j(window).height();
		var left   = $j(window).width()/2 - 200;
		var style  = 'top:'+top+'px;left:'+left+'px;';
		var rocket = $j('<div id="gagarinRocket" style="'+style+'"><div><div><div id="gagarinFire" class="k1"></div></div></div></div>');
		$j('BODY').append(rocket);
		$j('BODY').append('<div id="gagarin-poehali" style="top:'+(top-400+50)+'px;left:'+(left+115)+'px;"></div>');
		var interval = window.setInterval(this.animateFire.bind(this), 70);
		rocket.animate({top:'-=400'}, 1000, function()
		{
			window.setTimeout(
				function()
				{
					$j('#gagarin-poehali').animate({opacity:1}, 500);
				},
				1000
			);
			window.setTimeout(
				function()
				{
					$j('#gagarin-poehali').animate({opacity:0}, 500);
				},
				3500
			);
			window.setTimeout(
				function()
				{
					$j('#gagarinRocket').animate({top:'-='+$j(window).height()}, 1500);
				},
				4000
			);
			window.setTimeout(
				function()
				{
					window.clearInterval(interval);
					$j('#gagarinFade').animate({opacity:0}, 1500, function(){
						$j('.gagarin-stars').remove();
						$j('#gagarinFade').remove();
						$j('#gagarinRocket').remove();
						$j('#gagarin-poehali').remove();
						$j('#gagarin-spacer').remove();
					});
				},
				4500
			);
		})
	},

	animateFire : function()
	{
		var fire = $j('#gagarinFire');
		var k    = parseInt(fire.attr('class').replace('k', ''), 10)+1;
		if(k > 3) k = 1;
		fire.attr('class', 'k'+k);
	}
};


$j(document).keyup(function(e)
{
	if(e.which == window.Gagarin.letter)
	{
		new window.Gagarin();
	}
});
</script>
