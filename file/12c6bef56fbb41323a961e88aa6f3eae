	<script language="javascript" type="text/javascript">
		$j(document).ready(function(){
			
			moove_footer();

			$j('#top_navigation LI LI:first-child').addClass('first_menu_item');
			$j('#top_navigation LI LI:last-child').addClass('last_menu_item');
			$j('#top_navigation .menu > LI').hover(
				function () {
					var width = 0;
					$j(this)
						.find('UL').show()
						.find('LI').each(function(){
							if( this.clientWidth>width ) {
								width = this.clientWidth;
							}
						})
						.css('width',width+'px');
				}, 
				function () {
					$j(this).find('ul').hide();
				}
			);
		      

			$j('#company_navigation .cn_has_childs').mouseover(function(){

				$j('#cn_list').remove();

				
				var $this  = $j(this);
				var list   = $this.find('UL').clone();
				var offset = $this.offset();
				var menuTimer, long_menuTimer, last_check=0;

				remove_menu = function( x, y )
				{
					var cn_list = $j('#cn_list');
					var ofst = cn_list.offset();
					if(!ofst)return;
					var size = {w:cn_list.width(),h:cn_list.height()};
					
			    	clearTimeout(menuTimer);
			    	clearTimeout(long_menuTimer);
					
			    	
					if( x<ofst.left || y<ofst.top || x>ofst.left+size.w+45 || y>ofst.top+size.h+15 )
					{
						$j("BODY").unbind('mousemove');
						$j('#cn_active').removeAttr('id');
						cn_list.fadeOut('fast', function(){cn_list.remove();});
						return;
					}
					
			    	return;
				};

				$this.attr('id', 'cn_active');
			    $j("BODY").mousemove(function(e){
			    	menuTimer = setTimeout('remove_menu('+e.pageX+','+e.pageY+')', 100);
				});

				list
					.prepend('<li class="cn_fst_li"></li>')
					.find('.cn_fst_li').prepend( $this.find('DT A').clone() )
					.end()
					.css({
						position:'absolute',
						top:offset.top,
						left:offset.left-1,
						zIndex:10,
						display:'none'
					})
					.attr('id', 'cn_list')
					.prependTo('BODY')
					.bgiframe()
					
				list.fadeIn('fast');

				
			});

			$j('.companies_buttons A').hover(
				function () {
					$j(this)
						.find('.hover_magic')
						.stop()
						.animate({'opacity':1}, 'slow');
				}, 
				function () {
					$j(this)
						.find('.hover_magic')
						.stop()
						.animate({'opacity':0}, 'slow');
				}
			);

			$j('.companies_buttons A').attr('title','');

			$j('.fancybox').fancybox();
			
		});
		function moove_footer() {
			var difference = $j(document).height() - ($j(".footer_wraper").height() + $j(".footer_wraper").offset().top);
			if( difference>0 ) {
			    $j(".footer_wraper").css('top', difference);
			}
		}
		function _a(str){
			return;			alert(str);
		}
	</script>
	
	
		
