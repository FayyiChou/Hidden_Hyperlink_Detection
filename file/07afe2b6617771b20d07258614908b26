    function inquiryNow(name) {
        if (name == null) {
            name = "hs_basket_product";
        }
        if (document.inquiryForm && !Util.isChecked(name) && 0 <= 0) {
            alert("请至少选择一项");
            return;
        }
        var form = document.inquiryForm ? document.inquiryForm: document.inquiryForm2;
        form.action = "/inquiry.html";
        form.submit();
    }
