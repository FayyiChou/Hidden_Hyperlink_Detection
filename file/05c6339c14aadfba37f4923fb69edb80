                                    var articleid = "124550";
                                    var setvaluereadonly = function (s) {
                                        jQuery('#xingji').html("");
                                        jQuery(".votenumber em").html(Math.floor(new Number(s.star) * 10) / 10);
                                        jQuery(".canyu em").html(s.count);
                                        jQuery('#xingji').rater({ max: 10, value: s.star, enabled: false });
                                        jQuery("#xingji").click(function () { alert("您已经参与过了"); });
                                    }
                                    var setvalue = function (s) {
                                        jQuery('#xingji').html("");
                                        jQuery(".votenumber em").html(Math.floor(new Number(s.star) * 10) / 10);
                                        jQuery(".canyu em").html(s.count);
                                        jQuery('#xingji').rater({ max: 10, value: s.star, after_click: function (ret) {
                                            alert("感谢您的参与");
                                            DOTA_VC.SaveVideoCookie(articleid);
                                            Star.click(articleid, ret.value, setvaluereadonly);
                                        }
                                        });
                                    };
                                    if (DOTA_VC.CheckVideoCookie(articleid)) {
                                        Star.get(articleid, setvaluereadonly);
                                    } else {
                                        Star.get(articleid, setvalue);
                                    }
                                </script>
                            </div>
                            <!-- 将此标记放在您希望显示like按钮的位置 -->
