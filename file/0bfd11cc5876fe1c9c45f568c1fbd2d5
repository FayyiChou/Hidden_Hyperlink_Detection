<script type="text/javascript">
    $('#sub_search').click(function() {
        var val = $.trim($('#sub_tag').val());
        if( val == '') {
            alert('搜索内容不能为空');
            return false;
        }
        $('#sub_form').submit();
    });

    $('.cls_search_tag').click(function() {
        $('#sub_tag').css('color', '#fff');
        $('#sub_tag').val($(this).html());
        $('#sub_form').submit();
        return false;
    });
</script>

<script type="text/javascript">
    $('#sub_search_2').click(function() {
        var val = $.trim($('#sub_tag_2').val());
        if( val == '') {
            alert('搜索内容不能为空');
            return false;
        }
        $('#sub_form_2').submit();
    });

    $('.cls_search_tag_2').click(function() {
        $('#sub_tag_2').css('color', '#fff');
        $('#sub_tag_2').val($(this).html());
        $('#sub_form_2').submit();
        return false;
    });
</script>

<script type="text/javascript">
    $(function(){
        $(".main-new-li li").hover(function () {
            var h_src=$(this).find("img").attr("data-src");
            var n_src=$(this).find("img").attr("src");
            $(this).find("img").attr("src",h_src);
            $(this).find("img").attr("data-src",n_src);
        },function(){
            var n_src=$(this).find("img").attr("data-src");
            var h_src=$(this).find("img").attr("src");
            $(this).find("img").attr("src",n_src);
            $(this).find("img").attr("data-src",h_src);
        });
    });
</script>
