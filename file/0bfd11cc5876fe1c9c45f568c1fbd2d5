<script type="text/javascript">
	$(function(){
		var swiper = new Swiper('.swiper-container2');
		var ww = window.innerWidth ? window.innerWidth : document.body.clientWidth;
		// banner轮播
		var swiper = new Swiper('.swiper-container', {
			pagination: '.swiper-pagination',
			autoplay : 3000,
			paginationClickable: true,
			autoplayDisableOnInteraction:false,
			loop:true,
			calculateHeight:true,
		});

		successSwiper('.item_b_c',$('.item1_dir_left'),$(".item1_dir_right"),3,5000);
		function successSwiper(container,dir1,dir2,num,speed){
			//container:轮播图的容器div；dir1:方向箭头左，若值为null,则没有箭头左事件；dir2:方向箭头右，若值为null,则没有箭头右事件；num:大屏幕显示的图片张数；speed:轮播速度；
			var ww = window.innerWidth ? window.innerWidth : document.body.clientWidth;
			if (ww>768) {
				var swiper1 = new Swiper(container, {
					autoplay : speed,
					wrapperClass : 'item1-b-ul',
					slideClass : 'item1-b-li',
					autoplayDisableOnInteraction:false,
					loop:true,
					resizeReInit : true,
					slidesPerView:num,
					paginationAsRange:false,
					calculateHeight:true,
				});

			}else if (ww>320) {
				var swiper1 = new Swiper(container, {
					autoplay : speed,
					wrapperClass : 'item1-b-ul',
					slideClass : 'item1-b-li',
					autoplayDisableOnInteraction:false,
					loop:true,
					resizeReInit : true,
					slidesPerView:2,
					paginationAsRange:false,
					calculateHeight:true,
				});
			}else{
				var swiper1 = new Swiper(container, {
					autoplay : speed,
					wrapperClass : 'item1-b-ul',
					slideClass : 'item1-b-li',
					autoplayDisableOnInteraction:false,
					loop:true,
					resizeReInit : true,
					paginationAsRange:false,
					calculateHeight:true,
				});
			};
			if (dir1 != null && dir2 != null) {
				dir1.on('click', function(e){
					e.preventDefault()
					swiper1.swipePrev()
				});
				dir2.on('click', function(e){
					e.preventDefault()
					swiper1.swipeNext()
				});
			};
		}

		successSwiper2('.swiper-container2',$('.button-prev'),$(".button-next"),3,5000);
		function successSwiper2(container,dir1,dir2,num,speed){
			//container:轮播图的容器div；dir1:方向箭头左，若值为null,则没有箭头左事件；dir2:方向箭头右，若值为null,则没有箭头右事件；num:大屏幕显示的图片张数；speed:轮播速度；
			var ww = window.innerWidth ? window.innerWidth : document.body.clientWidth;
			var swiper1 = new Swiper(container, {
				autoplay : speed,
				wrapperClass : 'swiper-wrapper',
				slideClass : 'swiper-slide',
//					loop:true,
				resizeReInit : true,
				paginationAsRange:false,
				calculateHeight:true,
			});
			if (dir1 != null && dir2 != null) {
				dir1.on('click', function(e){
					e.preventDefault()
					swiper1.swipePrev()
				});
				dir2.on('click', function(e){
					e.preventDefault()
					swiper1.swipeNext()
				});
			};
		}
		$(window).resize(function(){
			swiper.reInit();
			successSwiper('.item_b_c',$('.item1_dir_left'),$(".item1_dir_right"),3,5000);
			successSwiper2('.swiper-container2',$('.button-prev'),$(".button-next"),3,5000);
		});

		//	品牌介绍
		$(".item1 .item1-b .col-xs-6").on("click",function(){
			var index = $(this).attr("data-type");
			$(this).find(".imgBox").css({"backgroundColor": "#0168B7"}).find(".dialog").removeClass("dialog_div");
			$(this).siblings().find(".imgBox").css({"backgroundColor": "#EEEEEE"}).find(".dialog").addClass("dialog_div");
			$(this).parent().parent().find(".select_btn span").eq(index).addClass("select_span").siblings().removeClass("select_span");
		});
		$(".item1 .select_btn span").on("click",function(){
			var index=$(this).attr("data-type");
			$(this).addClass("select_span").siblings().removeClass("select_span");
			$(this).parent().parent().find(".item1-b .col-xs-6").eq(index).find(".imgBox").css({"backgroundColor": "#0168B7"}).find(".dialog").removeClass("dialog_div").parent().parent().siblings().find(".imgBox").css({"backgroundColor": "#EEEEEE"}).find(".dialog").addClass("dialog_div");
		});

		//	招商加盟商支持
		$(".item3 .item3-b li").hover(function(){
			var objImg=$(this).find(".div_box").find("img");
			var dataSrc=objImg.attr("data-src");
			objImg.attr("src",dataSrc);
			$(this).find(".div_box").addClass("selectDivBox").find(".div_title").addClass("select_DivTitle");
			$(this).siblings().find(".div_box").removeClass("selectDivBox").find(".div_title").removeClass("select_DivTitle");
		}, function () {
			var objImg=$(this).find(".div_box").find("img");
			var dataSrc=objImg.attr("data-srcOld");
			objImg.attr("src",dataSrc);

			$(this).find(".div_box").removeClass("selectDivBox").find(".div_title").removeClass("select_DivTitle");
		});

		//	成功案例
		$(".item5 .item1-b li").hover(function(){
			$(this).find(".ell1").hide();
			$(this).find(".dialog_none").show().addClass("dialog_item5").parent().parent().siblings().find(".dialog_none").hide().removeClass("dialog_item5");
		},function () {
			$(this).find(".ell1").show();
			$(this).find(".dialog_none").hide().removeClass("dialog_item5");
		});

		$(".span_btn").hover(function(){
            if($(this).hasClass("spanBtn1")){
                $(this).removeClass("spanBtn1").addClass("spanBtn1_1");
            }else if($(this).hasClass("spanBtn2")){
                $(this).removeClass("spanBtn2").addClass("spanBtn2_2");
            }
        },function(){
            if($(this).hasClass("spanBtn1_1")){
                $(this).removeClass("spanBtn1_1").addClass("spanBtn1");
            }else if($(this).hasClass("spanBtn2_2")){
                $(this).removeClass("spanBtn2_2").addClass("spanBtn2");
            }
        });

        $(".move_btn").hover(function(){
            $(this).css({"background-color":"#FF7A36"});
        },function(){
            $(this).css({"background-color":"#0068B7"});
        });

		// 新闻资讯
		$(".item6 .news-b li a").hover(function(){
			$(this).css({"backgroundColor":"#0168B7"}).find(".mon").removeClass("mon_day").addClass("mon_day_select").parent().parent().find(".news-b-info").find("h1").css({"color":"#fff"}).parent().find("p").css({"color":"#fff"});
		},function(){
			$(this).css({"backgroundColor":"#fff"}).find(".mon").removeClass("mon_day_select").addClass("mon_day").parent().parent().find(".news-b-info").find("h1").css({"color":"#333"}).parent().find("p").css({"color":"#666"});
		});
	});
</script>
<script>
	function leave_msg() {
		var nicknamex = $.trim($("#name").val());
		var phonex = $("#phone").val();
		var titlex = $("#title").val();
		var bookcontentx =  $.trim($("#msg").val());
		var codex = $("#valid").val();


		if(nicknamex == '') {
			$('#Notice').html('联系人不能为空!');
			$("#name").css("border-color","red");
			setTimeout(function() {
				$("#name").css("border-color","#ddd");
			},2000);
			return false;
		} else {
			$('#Notice').html('');
		}

		if(phonex == '') {
			$('#Notice').html('联系电话不能为空!');
			$("#phone").css("border-color","red");
			setTimeout(function() {
				$("#phone").css("border-color","#ddd");
			},2000);
			return false;
		} else {
			var myReg = /^([0-9]{11})?$/;
			if(!myReg.test(phonex)) {
				$('#Notice').html('联系电话格式不正确!');
				$("#phone").css("border-color","red");
				setTimeout(function() {
					$("#phone").css("border-color","#ddd");
				},2000);
				return false;
			} else {
				$('#Notice').html('');
			}
		}
		if(titlex == ''){
			$('#Notice').html('留言标题不能为空');
			$("#title").css("border-color","red");
			setTimeout(function() {
				$("#title").css("border-color","#ddd");
			},2000);
			return false;
		}
		if(bookcontentx == '') {
			$('#Notice').html('留言内容不能为空!');
			$("#msg").css("border-color","red");
			setTimeout(function() {
				$("#msg").css("border-color","#ddd");
			},2000);
			return false;
		} else {
			$('#Notice').html('');
		}
		if(codex == '') {
			$('#Notice').html('验证码不能为空!');
			$("#codex").css("border-color","red");
			setTimeout(function() {
				$("#codex").css("border-color","#ddd");
			},2000);
			return false;
		} else {
			$('#Notice').html('');
			$.ajax({
				url:'/msg/check_code',
				type:'post',
				dataType:'json',
				data:{
					code:codex
				},
				success:function(msg){
					if(msg == '1'){
						$("#form_msg").submit();
						return false;
					}else if(msg == '2'){
						$('#Notice').html('验证码不正确!');
						return false;
					}
				}
			});
		}

		return false;
	}
</script>