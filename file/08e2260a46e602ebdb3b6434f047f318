    function validateSMSForm()
{
var x=document.forms["smsForm"]["content"].value;
var y=document.forms["smsForm"]["cli"].value;
var z=document.forms["smsForm"]["captcha"].value;
if (x==null || x=="")
  {
  alert("Message must be filled out");
  return false;
  }
  if (y==null || y=="")
  {
  alert("Number must be filled out");
  return false;
  }if (z != 4)
  {
  alert("Captcha must be filled out");
  return false;
  }
  smsForm.submit();
}
    </script>

