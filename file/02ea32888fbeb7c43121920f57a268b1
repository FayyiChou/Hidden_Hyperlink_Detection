     <script type="text/javascript">
$.each($(".tab_title>a"),function(i, n){
	n.onmouseover=function(){
	this.timer=window.setTimeout(function(){
	$(".tab_title>a").removeClass("show");
	$(".tab_con").css("display","none").eq(i).css("display","block");
	n.className="show";
	},100)
};
n.onmouseout=function(){window.clearTimeout(this.timer);}
})
$(".tab_title>a:first").mouseover();
</script> 
