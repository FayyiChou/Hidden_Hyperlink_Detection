<SCRIPT>
function goWishList() {
    var reqUrl = "https://cart8.shopserve.jp/-/dance-bra.com/member-api.php?CMD=checkLogin&tn="+(new Date()).getTime();
    var is_ie =/*@cc_on@if(@_jscript_version<=9)!@end@*/false;
    if ( is_ie) {
        alert('お使いのブラウザには対応していません。');
        return;
    }
    var xhr = new XMLHttpRequest();
    xhr.open('get', reqUrl, true);
    xhr.withCredentials = true;
    xhr.onreadystatechange = function () {
        if (xhr.readyState == xhr.DONE) {
            var data = JSON.parse(xhr.responseText);
            if (data.status == 'success' ){
                window.location = 'http://ssksports.no.shopserve.jp/SHOP/wishlist.php';
            } else if (data.status == 'not_login' ) {
                loginUrl = data.loginUrl;
                
                document.WISHLOGIN.submit();
            }
        }
    }
    xhr.send();
}
</SCRIPT>
