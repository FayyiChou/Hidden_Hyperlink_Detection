江西省南昌市站前路176号省地矿局办公大楼6楼613室驻省地矿局纪检组</p>
</div></div>
  </div>
</div>
<script type="text/javascript">
function wechat_qrcode(type) {
	if(type){
		return
	}
	var qrleft = parseInt(jquery('.module.center.footer').css('width')) + parseInt(jquery('.module.center.footer').offset().left) + 10;
	jquery('#oddpiFloatAD').css('display','block');
	if(qrleft + jquery('#oddpiFloatAD')[0].clientWidth > document.documentElement.clientWidth) {
		jquery('#oddpiFloatAD').css('float','right');
		jquery('#oddpiFloatAD').css('left','auto');
		jquery('#oddpiFloatAD').css('right',0);
	} else {
		jquery('#oddpiFloatAD').css('float','left');
		jquery('#oddpiFloatAD').css('left',(qrleft) + 'px');
		jquery('#oddpiFloatAD').css('right','auto');
	}
}

jquery(document).ready(function(e) {
  jquery('#floatClose').click(function(e) {
		jquery('#oddpiFloatAD').hide(500);
	});

});

jquery(window).resize(function(e){
	if(jquery('#oddpiFloatAD').css('display') == 'none'){
		wechat_qrcode(1);
	}else{
		wechat_qrcode(0);
	}
});
jquery(window).scroll(function(e) {
  wechat_qrcode(1);
});
jquery(window).load(function(e) {
  wechat_qrcode(0);
});
</script>


