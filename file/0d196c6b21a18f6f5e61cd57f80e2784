<script type="text/javascript">
//var jquery=jQuery.noConflict();
jquery(document).ready(function(){
	//tab auto
	jquery("a[_for]").mouseover(function(){
		//alert((jquery(this).attr("_for")).substring(0,3));
		switch((jquery(this).attr("_for")).substring(0,4)){
			case "blue":
				jquery(this).parents().children("a[_for]").removeClass("thisclass").parents().children("dd").hide();
				jquery(this).addClass("thisclass").blur();
				break;
			case "gree":
				jquery(this).parents().children("a[_for]").removeClass("thisclass_green").parents().children("dd").hide();
				jquery(this).addClass("thisclass_green").blur();
				break;
			case "brow":
				jquery(this).parents().children("a[_for]").removeClass("thisclass_brown").parents().children("dd").hide();
				jquery(this).addClass("thisclass_brown").blur();
				break;
			default:
				jquery(this).parents().children("a[_for]").removeClass("thisclass").parents().children("dd").hide();
				jquery(this).addClass("thisclass").blur();
				break;
		}
		//jquery(this).parents().children("a[_for]").removeClass("thisclass").parents().children("dd").hide();
		//jquery(this).addClass("thisclass").blur();
		
		jquery("#"+jquery(this).attr("_for")).show();
	});
	//jquery("a[_for=ID]").mouseover();
	jquery("a[_for=blue_1_1]").mouseover();
	jquery("a[_for=blue_2_1]").mouseover();
        jquery("a[_for=green_1_1]").mouseover();
	jquery("a[_for=green_2_1]").mouseover();
	jquery("a[_for=brown_1_1]").mouseover();
	jquery("a[_for=brown_2_1]").mouseover();
	
	//scroll topic
	var scroll_topic_1= new ifeng_Scroll("topiclist_1","topiclist_1_up","topiclist_1_down");
	scroll_topic_1.IsAutoScroll=true;
	scroll_topic_1.Speed=120; 
	scroll_topic_1.IsSmoothScroll= false;
	scroll_topic_1.PauseTime = 0;
	scroll_topic_1.Direction = "N"; 
	scroll_topic_1.Step =2;
	scroll_topic_1.ControllerType = "click";
	scroll_topic_1.BackCall = null;
	scroll_topic_1.Init();
	scroll_topic_1.Start();
	
	var scroll_topic_2= new ifeng_Scroll("topiclist_2","topiclist_2_up","topiclist_2_down");
	scroll_topic_2.IsAutoScroll=true;
	scroll_topic_2.Speed=120; 
	scroll_topic_2.IsSmoothScroll= false;
	scroll_topic_2.PauseTime = 0;
	scroll_topic_2.Direction = "N"; 
	scroll_topic_2.Step =2;
	scroll_topic_2.ControllerType = "click";
	scroll_topic_2.BackCall = null;
	scroll_topic_2.Init();
	scroll_topic_2.Start();
	
	//pixviewer 焦点图片
	var pixviewer_02 = new Pixviewer("Pixviewer_2","BigPic_2","Number_2", "NumberBG_2", 311, 217);
	pixviewer_02.TimeOut = 5000;
	pixviewer_02.TitleID = "Title_2";
	pixviewer_02.titleHeight = 29;
	pixviewer_02.htmlData = document.getElementById('focusData_01').getElementsByTagName('dl');
	for(var i=0;i<pixviewer_02.htmlData.length;i++){
		var jsonData ={
			title : pixviewer_02.htmlData[i].getElementsByTagName('a')[0].innerHTML,
			url : pixviewer_02.htmlData[i].getElementsByTagName('a')[0].href,
			pic : pixviewer_02.htmlData[i].getElementsByTagName('dd')[0].innerText || pixviewer_02.htmlData[i].getElementsByTagName('dd')[0].textContent
		};
		pixviewer_02.Add(jsonData);
	};
	pixviewer_02.begin();
});
//去除 html 代码
function delHtmlTag(str){
	str=str.replace(" ","");
	str=str.replace("&nbsp;","");
	str=str.replace(/&nbsp;/g,'').replace(/ /g,'');
	str=str.replace(/(^\s*)|(\s*$)/g,"")
	str=str.replace(/<[^>]+>/g,"");//去掉所有的html标记
	return str;
}
