<link rel="stylesheet" href="css/css/style.css" />
<script type="text/javascript" src="js/jquery.aw-showcase.js"></script>
<script type="text/javascript" src="js/jquery.timers-1.1.2.js"></script>
<script type="text/javascript">
$(window).ready(function()
{
	$("#showcase").awShowcase(
	{
		content_width:			980,
		fit_to_parent:			false,
		auto:					true,
		interval:				10000,
		continuous:				true,
		loading:				true,
		tooltip_width:			200,
		tooltip_icon_width:		32,
		tooltip_icon_height:	32,
		tooltip_offsetx:		18,
		tooltip_offsety:		0,
		arrows:					false,
		buttons:				false,
		btn_numbers:			false,
		keybord_keys:			true,
		mousetrace:				false, /* Trace x and y coordinates for the mouse */
		pauseonover:			true,
		stoponclick:			true,
		transition:				'fade', /* hslide/vslide/fade */
		transition_speed:		1000,
		transition_delay:		600,
		show_caption:			'onhover', /* onload/onhover/show */
		thumbnails:				true,
		thumbnails_position:	'outside-last', /* outside-last/outside-first/inside-last/inside-first */
		thumbnails_direction:	'horizontal', /* vertical/horizontal */
		thumbnails_slidex:		8, /* 0 = auto / 1 = slide one thumbnail / 2 = slide two thumbnails / etc. */
		dynamic_height:			true, /* For dynamic height to work in webkit you need to set the width and height of images in the source. Usually works to only set the dimension of the first slide in the showcase. */
		speed_change:			false, /* Set to true to prevent users from swithing more then one slide at once. */
		viewline:				false /* If set to true content_width, thumbnails, transition and dynamic_height will be disabled. As for dynamic height you need to set the width and height of images in the source. */
	});
	$('.showcase').bind('mouseenter',function(){
		$('.showcase-thumbnail').css({
		'display':'inline'});
		$('.showcase-thumbnail-container').css({
		'display':'inline'});
		$('.showcase-thumbnail-restriction').css({
		'width':'905px'});
		$('.showcase-thumbnail-wrapper showcase-thumbnail-wrapper-horizontal').wrapInner('<marquee></marquee>');
		$('.showcase').bind('mouseleave',function(){
			$('.showcase-thumbnail').css({
			'display':'none'});
			$('.showcase-thumbnail-container').css({
			'display':'none'});
		});
	});
	
});

</script>
