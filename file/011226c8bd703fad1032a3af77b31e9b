<script>
function displaySubMenu(i){
var subMenu = document.getElementById("zz"+i);
var zwm = document.getElementById("zwmid"+i);
subMenu.style.display = "block";
zwm.style.backgroundPosition="0px -22px";
}
function hideSubMenu(i){
var subMenu = document.getElementById("zz"+i);
var zwm = document.getElementById("zwmid"+i);
subMenu.style.display = "none";
zwm.style.backgroundPosition="0px 0px";
}
function displayli(li,a,b){
var c=a.toString()+b.toString();
li.style.backgroundColor="#9b9cc6";
}
function hideli(li,a,b){
var c=a.toString()+b.toString();
li.style.backgroundColor="#ffffff";
}
function html1(div,j){
var px2change=document.getElementById("px1");
px2change.innerHTML= div.innerHTML;
}
</script>
