<script>
	//banner滚动
	function play_image_list(elementID,effect,play_time){
        var _div=$("."+elementID+" ul");
        var temp_time;
		_div.width(_div.children("li").length*(_div.children("li").eq(0).width()+19));
        if(_div.children("li").length<=4){return false;}
        //play_img("left");
        if(effect==="continue"){play_time=30;_div.css("margin-left","0px");}
        temp_time=setTimeout(function(){play_img("left")},play_time);
        _div.parent().hover(
                   function(){clearTimeout(temp_time)},
                   function(){temp_time=setTimeout(function(){play_img("left")},play_time);}
                   )
    
        function play_img(type){
            if(temp_time){clearTimeout(temp_time);}
            var div_f=$("."+elementID+" ul>li:first"),div_l=$("."+elementID+" ul>li:last"),div_width=div_f.width();
            //alert(div_width);
        if(effect==="continue"){
                play_time=40;
                var num=(_div[0].style.marginLeft=="0px")?parseInt(_div[0].style.marginLeft):parseInt(_div[0].style.marginLeft.substring(1));
                num++;
                if(num<=div_width+20){_div.css("margin-left",-num+"px");}
                else{_div.append(div_f);_div.css("margin-left","0px");}
                    }
            temp_time=setTimeout(function(){play_img("left")},play_time);return false;
            }
    }
	
	play_image_list("rollbox","continue",4000);	

</script>
