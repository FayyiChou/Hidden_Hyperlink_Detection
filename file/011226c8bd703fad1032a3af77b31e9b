			$(".r1_bx2brt li").removeClass("zjn"); 
            $(".r1_bx2brt li").eq(0).addClass("zjn");
			} else if (Zj=="星期二"){
			$(".r1_bx2brt li").removeClass("zjn"); 
            $(".r1_bx2brt li").eq(1).addClass("zjn");
			} else if (Zj=="星期三"){
			$(".r1_bx2brt li").removeClass("zjn"); 
