<script type="text/javascript">
    //<![CDATA[
    var tips; var theTop = 200/*这是默认高度,越大越往下*/; var old = theTop;
    function initFloatTips() {
        tips = document.getElementById("xixi");
        moveTips();
    };
    function moveTips() {
        var tt = 200;
        if (window.innerHeight) {
            pos = window.pageYOffset
        }
        else if (document.documentElement && document.documentElement.scrollTop) {
            pos = document.documentElement.scrollTop
        }
        else if (document.body) {
            pos = document.body.scrollTop;
        }

        pos = pos - tips.offsetTop + theTop;
        pos = tips.offsetTop + pos / 10;


        if (pos < theTop) pos = theTop;
        if (pos != old) {
            tips.style.top = pos + "px";
            tt = 10;
            //alert(tips.style.top);
        }

        old = pos;
        setTimeout(moveTips, tt);
    }
    function close1() {
        abc = document.getElementById("xixi");
        abc.style.display = 'none';
    };
    //!]]>
    initFloatTips();




    function hideMsgBox(theEvent) { //theEvent用来传入事件，Firefox的方式
        if (theEvent) {
            var browser = navigator.userAgent; //取得浏览器属性
            if (browser.indexOf("Firefox") > 0) { //如果是Firefox
                if (document.getElementById('QQFloat').contains(theEvent.relatedTarget)) { //如果是子元素
                    return; //结束函式

                }

            }

            if (browser.indexOf("MSIE") > 0) { //如果是IE

                if (document.getElementById('QQFloat').contains(event.toElement)) { //如果是子元素

                    return; //结束函式

                }

            }

        }

    }
        </script>
<script language="javascript" type="text/javascript">
    lastScrollY = 0;

    var InterTime = 1;
    var maxWidth = -1;
    var minWidth = 0;
    var numInter = 8;

    var BigInter;
    var SmallInter;

    var o = document.getElementById("xixi");
    var i = parseInt(o.style.right);
    function Big() {
        if (parseInt(o.style.right) < maxWidth) {
            i = parseInt(o.style.right);
            i += numInter;
            o.style.right = i + "px";
            if (i == maxWidth)
                clearInterval(BigInter);
        }
    }
    function toBig() {
        document.getElementById("tab1").style.display = "block";
        clearInterval(SmallInter);
        clearInterval(BigInter);
        BigInter = setInterval(Big, InterTime);
    }
    function Small() {
        if (parseInt(o.style.right) > minWidth) {
            i = parseInt(o.style.right);
            i -= numInter;
            o.style.right = i + "px";

            if (i == minWidth)
                clearInterval(SmallInter);
        }
    }
    function toSmall() {
        document.getElementById("tab1").style.display = "none";
        clearInterval(SmallInter);
        clearInterval(BigInter);
        SmallInter = setInterval(Small, InterTime);

    }

        </script>
   

</body>
