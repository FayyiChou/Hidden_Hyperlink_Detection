        <script type="text/javascript" language="JavaScript">
            var nav1 = document.getElementById("navii");
            var links1 = nav1.getElementsByTagName("li");
            var lilen1 = nav1.getElementsByTagName("a");

            var currenturl1 = document.location.href;
            if (currenturl1.indexOf("newscon.aspx") != -1) {
                currenturl1 = currenturl1.replace("newscon.aspx", "news.aspx");

            }
            if (currenturl1.indexOf("zixuncon.aspx") != -1) {
                currenturl1 = currenturl1.replace("zixuncon.aspx", "zixun.aspx");

            }
            if (currenturl1.indexOf("fwcon.aspx") != -1) {
                currenturl1 = currenturl1.replace("fwcon.aspx", "fw.aspx");

            }
            if (currenturl1.indexOf("message.aspx") != -1) {
                currenturl1 = currenturl1.replace("message.aspx", "joinUs.aspx");

            }
            if (currenturl1.indexOf("youhui.aspx") != -1) {
                currenturl1 = currenturl1.replace("youhui.aspx", "zixun.aspx");

            }

            var last1 = 0;
            for (var i = 0; i < links1.length; i++) {
                var linkurl1 = lilen1[i].getAttribute("href");

                if (currenturl1.indexOf(linkurl1) != -1) {
                    last1 = i;
                }
            }
          
          //  links1[last1].className = "imp";
            lilen1[last1].className = "drop";
            lilen1[last1].style.color="rgb(255,255,255)";
            </script>
