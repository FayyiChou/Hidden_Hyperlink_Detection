			<script type="text/javascript"> 				    
				$(function() {                
					$(".acav li a").click(function() {            
						var curList = $("#.acav li a.current").attr("rel");
						var $newList = $(this);            
						$(".acav li a").removeClass("current");
						$newList.addClass("current");                    
						var listID = $newList.attr("rel");                        
						if (listID != curList) {                            
							$("#"+curList).fadeOut(300, function() {
								$("#"+listID).fadeIn();    
							});
						} 
						return false;
					});
				});
			</script>  
