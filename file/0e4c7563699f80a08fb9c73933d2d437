function replaceall(str,oldstr,newstr){
	  return str.replace(new RegExp(oldstr,"gm"),newstr);
	}
	function e(e, t, n, i, o, a) {
	    var c = new Date;
	    c.setTime(c.getTime()),
	    document.cookie = e + "=" + escape(t) + (n ? ";expires=" + n.toGMTString() : "") + (i ? ";path=" + i: "") + (o ? ";domain=" + o: "") + (a ? ";secure": "")
	}
	function t(t, n, i, o, a) {
		console.log(n);
	    if(n.indexOf("news")>-1)
		{
	    	n=replaceall(n,"news.","");
	    	
	    	if(n.indexOf("news")>-1){
	    		n="bj";
	    	}
		
		}
		
	    var c = new Date;
	    c.setTime(c.getTime() + 24 * i * 3600 * 1e3),
	    e(t, n, c, o, a)
	}
	//location.hostname.substring(0, location.hostname.length - 9)
	//var domain="news.fang.com";
	var domain=location.hostname;
	console.log(domain);
	if(domain.indexOf("/news")>-1)
	{
	}
	else
