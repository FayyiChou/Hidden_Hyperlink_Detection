<script type="text/javascript">
function enterSubmit(){
	//按下Enter键
	if(window.event.keyCode==13){
		//alert("Hello!Hello!");
		searchSubmit();
	}
}


function searchSubmit(){
	var so_city = '宁波';
	var so_cityEn = 'nb';
	var q = jQuery('#q').val();
	var keyword = encodeURI(encodeURI(q));
	var keywordtwo = encodeURI(q);
