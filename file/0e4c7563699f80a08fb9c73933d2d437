<script type="text/javascript">
	function loadScript(B,D){var A=document.createElement("script"),C=document.documentElement.firstChild;A.type="text/javascript";if(A.readyState){A.onreadystatechange=function(){if(A.readyState=="loaded"||A.readyState=="complete"){A.onreadystatechange=null;D()}}}else{A.onload=function(){D()}}A.src=B;C.insertBefore(A,C.firstChild)};
</script>
<script type="text/javascript">
var city = "北京";
var $s_=function(id) {
   return document.getElementById(id);
}
var myXmlHttpRequest;
//创建ajax引擎                              
function getXmlHttpObject(){	
	var xmlHttpRequest;
	//不同的浏览器获取对象xmlhttprequest 对象方法不一样
	if(window.ActiveXObject){		
		xmlHttpRequest=new ActiveXObject("Microsoft.XMLHTTP");		
	}else{
		xmlHttpRequest=new XMLHttpRequest();
	}
		return xmlHttpRequest;
}
function doSearchSubmitBJ(t){
    var nowKeywValue = $s_("searchinput_"+t).value;
	$s_("searchinput_"+t).value = nowKeywValue;
	if(nowKeywValue!="请输入关键字（楼盘名/地名/开发商等）" && nowKeywValue!=''){
		myXmlHttpRequest=getXmlHttpObject();
		if(myXmlHttpRequest){	
		//创建ajax引擎成功
		var url="/house/ajaxrequest/search_keyword_submit.php";
		var data = 'city='+city+"&keyword="+nowKeywValue;
		myXmlHttpRequest.open("post",url,false);
		myXmlHttpRequest.setRequestHeader("Content-Type","application/x-www-form-urlencoded;charset=utf-8");
		myXmlHttpRequest.onreadystatechange=function chuli(){
			//接收数据json 
			if(myXmlHttpRequest.readyState==4){
				if(myXmlHttpRequest.status==200){
					var result= myXmlHttpRequest.responseText;
					var arr  = result.split("^");
					if(arr[0]==100){
						 window.open(arr[1]);
						 return false;				
					}else{
					    $s_('strNamekeyword_'+t).value=nowKeywValue;
						$s_("form_newhouse_"+t).submit();						
					}
				}	
			 }
		}
		myXmlHttpRequest.send(data);
        return false;		
		}	
	}else{
	     $s_('strNamekeyword_'+t).value='';
		 $s_("form_newhouse_"+t).submit();		
	}
	 return true;
}
</script>
<script language="vbscript"> 
Function URLDecode(input) 
Dim str,code,a(3),b(1) 
str="" 
code=input 
code=Replace(code,"+"," ") 
Const hexstr = "0123456789ABCDEF" 
While len(code)>0 
If InStr(code,"%")>0 Then 
str = str & Mid(code,1,InStr(code,"%")-1) 
code = Mid(code,InStr(code,"%")) 
a(0) = UCase(Mid(code,2,1)) 
a(1) = UCase(Mid(code,3,1)) 
If a(1)<>"" And InStr(hexstr,a(0))>0 And InStr(hexstr,a(1))>0 Then 
b(0) = Eval("&H" & a(0) & a(1)) 
If b(0)>127 And Mid(code,4,1)="%" And len(code)>=6 Then 
a(2) = UCase(Mid(code,5,1)) 
a(3) = UCase(Mid(code,6,1)) 
If a(3)<>"" And InStr(hexstr,a(2))>0 And InStr(hexstr,a(3))>0 Then 
b(1) = Eval("&H" & a(2) & a(3)) 
str = str & chr(b(0)*256+b(1)) 
code = Mid(code,7) 
Else 
str = str & "%" 
code = Mid(code,2) 
End If 
Else 
str = str & chr(b(0)) 
code = Mid(code,4) 
End If 
Else 
str = str & "%" 
code = Mid(code,2) 
End If 
Else 
str = str & code 
code = "" 
End If 
Wend 
URLDecode = str 
End Function 
</script>
<script language="javascript"> 

function decodeURI(code){ 
	return URLDecode(code); 
}
</script>
<script language="javascript"> 
    function jsonToarray(dat){
        return eval('(' + dat + ')')
    }
    var backurl = urlencode(location.href);

    function doHeadloginuser()
    {
        var user_data = this.responseText;//获取cookie
        if(user_data){
            var obj = jsonToarray( user_data );
            if(obj.uname && obj.uid)
            {
                document.getElementById('sfHeadUsername').innerHTML = obj.uname ;
                document.getElementById('sfHeadLogout').href = "http://passport.fang.com/logout.aspx?backurl="+backurl;
                var tdate = new Date();
    			tdate.setHours(tdate.getHours() + 80);
            }
        }
    }


    function getCookie(name) { 
		//document.domain='fang.com';
      	var start = document.cookie.indexOf(name+"=");
       	var len = start+name.length+1;
       	if ((!start) && (name != document.cookie.substring(0,name.length))) return null;
       	if (start == -1) return null;
      	var end = document.cookie.indexOf(";",len);
       	if (end == -1) end = document.cookie.length;
       	return document.cookie.substring(len,end);
    }  
    var cookie_passport = getCookie('passport');
    var cookie_new_loginid = getCookie('new_loginid');
    var cookie_new_loginname = getCookie('new_loginname');
    var cookie_new_sfut = getCookie('sfut');
    var isLogin = false; 
    var showLoginName = ""; 
    var showLoginId = "";
    var ppUsername = cookie_new_loginname;
    var ppUserid   = cookie_new_loginid;
    
   if( cookie_new_sfut)
    {
        showLoginName = cookie_new_loginname;  
        showLoginId = cookie_new_loginid;
        //isLogin = true;
        jQuery.ajax({
            type:'get',
            dataType:'jsonp',
          //  url:"http://manage.news.fang.com/ajax_new.jsp",//改成新的jsp-----ajax_new.jsp
          url:"http://zhuanti.news.fang.com/ajax.do",
            data:{},
            complete:function(){},
            error:function(){},
            success:function(msgRes){
			var result = msgRes.feed.index;
			 if(result==100)
			  {
				 isLogin = true;
				     if(isLogin)
					{
				    	showLoginName=msgRes.feed.username; //返回的username
					    showLoginId= msgRes.feed.userid; //返回的userid
						document.getElementById('topHeadNotLogin').style.display = "none";
						document.getElementById('topHeadLogin').style.display = "block";
						document.getElementById('sfHeadLogout').href = "http://passport.fang.com/logout.aspx?backurl="+backurl;//设置logout
						var nameStrTemp = showLoginName ;
						//document.getElementById('sfHeadUsername').innerHTML =  decodeURI(nameStrTemp);
						document.getElementById('sfHeadUsername').innerHTML =  nameStrTemp;
						   document.cookie =  "new_loginname=" + showLoginName;
						   document.cookie =  "new_loginid=" + showLoginId;
					}
					else
					{
						document.getElementById('topHeadNotLogin').style.display = "block";
						document.getElementById('topHeadLogin').style.display = "none";
					}
			  }
            }

        });

    }
    else if(ppUsername && ppUserid)
    {
        showLoginName = ppUsername;  showLoginId = ppUserid;
        //isLogin = true;
         jQuery.ajax({
            type:'get',
            dataType:'jsonp',
          //  url:"http://manage.news.fang.com/ajax_new.jsp",//改成新的jsp-----ajax_new.jsp
          url:"http://zhuanti.news.fang.com/ajax.do", 
          data:{},
            complete:function(){},
            error:function(){},
            success:function(msgRes){
			var result = msgRes.feed.index;
			 if(100==100)
			  {
				 
				 isLogin = true;
				     if(isLogin)
					{
				    	//showLoginName=msgRes.feed.username; //返回的username
					   // showLoginId= msgRes.feed.userid; //返回的userid
						document.getElementById('topHeadNotLogin').style.display = "none";
						document.getElementById('topHeadLogin').style.display = "block";
						document.getElementById('sfHeadLogout').href = "http://passport.fang.com/logout.aspx?backurl="+backurl;//设置logout
						var nameStrTemp = showLoginName ;
						//document.getElementById('sfHeadUsername').innerHTML =  decodeURI(nameStrTemp);
						document.getElementById('sfHeadUsername').innerHTML =  nameStrTemp;
					}
					else
					{
						document.getElementById('topHeadNotLogin').style.display = "block";
						document.getElementById('topHeadLogin').style.display = "none";
					}
			  }
            }

        }); 
    }
    if(isLogin)
    {
        document.getElementById('topHeadNotLogin').style.display = "none";
        document.getElementById('topHeadLogin').style.display = "block";
        document.getElementById('sfHeadLogout').href = "http://passport.fang.com/logout.aspx?backurl="+backurl;//设置logout

        var nameStrTemp = showLoginName ;
        
        //document.getElementById('sfHeadUsername').innerHTML =  decodeURI(nameStrTemp);
		document.getElementById('sfHeadUsername').innerHTML =  nameStrTemp;

        Xml.Request("http://www.fang.com/house/esf/getusernewhouse.php", null, doHeadloginuser);
    }
    else
    {
        document.getElementById('topHeadNotLogin').style.display = "block";
        document.getElementById('topHeadLogin').style.display = "none";
       /// document.getElementById('shHeadBackurl').value = backurl;
    }
    function getParamValueByPassport(passport, paramKey)
    {
        var passportArr = passport.split("&");
        for(var i=0; i<passportArr.length; i++)
        {
            if(passportArr[i].indexOf(paramKey+"=") != -1)
            {
                return passportArr[i].replace(paramKey+"=", "");
            }
        }
    }
    function urlencode(str) 
    {
        str = (str + '').toString();
        return encodeURIComponent(str).replace(/!/g, '%21').replace(/'/g, '%27').replace(/\(/g, '%28').
        replace(/\)/g, '%29').replace(/\*/g, '%2A').replace(/%20/g, '+');
    }

    
