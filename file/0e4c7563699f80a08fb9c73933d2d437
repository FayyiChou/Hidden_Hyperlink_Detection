        	title:{
        	       text:false//隐藏y坐标题'元/㎡
        	   },
            labels: {
                formatter: function () {
                    return this.value;
                }
            }
        },
        tooltip: {
            crosshairs: true,
            shared: true
        },
        series: [{   
        	name: '宁波',
        	showInLegend: false,//图例隐藏
        	text:false,
            data: []
        }],
        credits: {
            enabled:false
  }
    });
});
		</script>
