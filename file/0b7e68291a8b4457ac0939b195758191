<script type="text/javascript">
	$(".zfnav1 ul li a").mouseover(function(){
		$(this).parent("li").find("a").css({"display":"none"})
		$(this).parent("li").find(".a1").css({"display":"block"})
	})
	$(".zfnav1 ul li a").mouseleave(function(){
		$(this).parent("li").find(".a1").css({"display":"none"})
		$(this).parent("li").find("a").css({"display":"block"})
	})

</script>

