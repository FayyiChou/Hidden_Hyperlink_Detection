<script>
var cookielist=document.cookie;
		var cookiearray=cookielist.split(";");
		var expires_data=new Date();
		expires_data.setTime(expires_data.getTime()+3*24*60*60*1000);
		
		for(var i=0;i<cookiearray.length;i++)
		{
			var cookieName=cookiearray[i].split("=")[0];
			var cookieValue=cookiearray[i].split("=")[1];
			
			var cookie_str=cookieName+"="+cookieValue+";path=/;domain=.gysjyw.gov.cn;expires="+expires_data.toGMTString();
			document.cookie=cookie_str;
		}
</script>


