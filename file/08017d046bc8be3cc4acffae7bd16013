	//*调查问卷提交 add by Lynn 2014-11-21 10:48:42
	$("#J-submit").click(function(){
		var work =$('input[name="work"]');
		var correct =$('input[name="correct"]');
		
		if((work[0].checked == false ) && work[1].checked == false ){
				alert("请您选择问题一的答案！");	
		}else if((correct[0].checked == false ) && correct[1].checked == false ){
				alert("请您选择问题二的答案！");	
		}else {
				alert("感谢您的参与！");	
				$("#myform").css('display','none');		
				$("#mydiv").css('display','');
		}
	});	
	
	//* 飘窗
	$("#img").imgFloat({speed:15,xPos:10,yPos:10});  
	 $(".reset").click(function(){
		 $("#img").css("display","none");
	});
	
});
</script>
