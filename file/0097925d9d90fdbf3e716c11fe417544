            event_stack_0.push(current_key + '-' + key);
            return;
          }
          var direction = 'right';
          if (bwg_current_key_0 > key) {
            var direction = 'left';
          }
          else if (bwg_current_key_0 == key) {
            return;
          }
          jQuery(".bwg_slideshow_watermark_0").css({display: 'none'});
