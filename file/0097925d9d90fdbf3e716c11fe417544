          jQuery(".bwg_slideshow_description_text_0").css({display: 'none'});
          /* Set active thumbnail position.*/
          bwg_current_filmstrip_pos_0 = key * (jQuery(".bwg_slideshow_filmstrip_thumbnail_0").width() + 2 + 2 * 1);
