<script type="text/javascript">
        function SearchKeyword() {
            var input = document.getElementById("search1016");
            var a = document.getElementById("aSearch");
            var val = input.value;
            if ((val != "") && (val != "请输入关键字")) {
                 a.href ="http://218.246.85.7/tabid/39/searchmid/3505/searchtid/1084/Default.aspx?keywords="+ escape(val);
                return true;               
            }else{  
                alert("请输入关键字！");
                return false;
            }
        }
        function enterInput(evt) {
            var evt = evt ? evt : (window.event ? window.event : null); //兼容IE和FF
            if (evt.keyCode == 13) {
                document.getElementById("aSearch").click();
            }
        }
    </script>
