function setCookie1(name,value) 
{ 
    var date = new Date(); 
    date.setTime(date.getTime() + 24 * 3600 * 1000); 
    document.cookie = name + "="+ escape (value) + ";expires=" + date.toGMTString() + "; path=/; domain=.3155.com";
} 
//读取cookies 
function getCookie1(name) 
{ 
    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
 
    if(arr=document.cookie.match(reg))
 
        return unescape(arr[2]); 
    else 
        return null; 
} 
var refurl=window.location.href;
var fromUrl=document.referrer;
if(refurl.indexOf("?fr=sogou")>-1 && refurl.indexOf("http://www.3155.com")>-1)
{ 
  var url=refurl;
  var myydj=getCookie1("ydji");
  if(myydj=="mysogou")
   {
	  setCookie1("ydji","my_2_sogou");
	  url=url.replace("http://www.3155.com","http://sogou.3155.com");
	  window.location.href=url;
   }
    else if(myydj=="my_2_sogou" && refurl.indexOf("http://www.3155.com")>-1)
    {
    	setCookie1("ydji","my_3_sogou");
    	url=url.replace("http://www.3155.com","http://sg.3155.com");
    	window.location.href=url;
    }
	//else if(myydj=="my_3_sogou" && refurl.indexOf("http://www.3155.com")>-1)
