            if(json == 1)
            {
                window.location.href="http://wangzhengqing.haodf.com/payment/newintro?&rand=MTM3MzU0NDUxMw==";
            }
            else
            {
                var msg = "冬日送暖活动已结束，感谢您的支持，好大夫在线一直为了“提高医疗效率，跨越医患鸿沟”而努力！";
                var mask = $('<div class="g-mask"></div>').hide(),
                    bubble = $('<div class="g-bubble"></div>').hide();
                if(!$(".g-mask").get(0)){
                    $("body").append(mask, bubble);
                }
                mask = $(".g-mask");
                bubble = $(".g-bubble").html(msg);
                if(mask.data("isProcessing")){
                    return;
                }
                mask.data("isProcessing", true).fadeIn(100, function(){ bubble.fadeIn(100); })
                    .fadeOut(1000, function(){ bubble.fadeOut(5000,function(){mask.data("isProcessing",false);}); });

                var timer = setTimeout(function(){
                    location.reload();
                },1000);
            }
        })
    });
});
</script>
