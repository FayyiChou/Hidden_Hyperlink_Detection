		var title = $(".banner dd ul li a img").eq(i).attr("title");
		var summary = $(".banner dd ul li a img").eq(i).attr("alt");
		var href = $(".banner dd ul li>a").eq(i).attr("href");
		$(".banner dd ul li").eq(i).append("<div><h2></h2><h3></h3><p><a target='_blank'>立刻参与&nbsp;&gt;&gt;</a></p></div>");
		$(".banner dd ul li div h2").eq(i).text(title);
		$(".banner dd ul li div h3").eq(i).text(summary);
		$(".banner dd ul li div p a").eq(i).attr("href", href);
	};
	$(".banner dd ul li").soChange({
		thumbObj: '.banner dt span',
		slideTime: 0
	});
	$(".ad span:contains('广告洽谈')").remove();
	(function() {
		var ad_num = $(".ad0 span").length;
		var ad_nowNum = Math.floor(Math.random() * ad_num);
		$(".ad0").prepend($(".ad0 span").eq(ad_nowNum));
		$(".ad0 span").soChange();
	})();
	(function() {
		var ad_num = $(".ad1 span").length;
		var ad_nowNum = Math.floor(Math.random() * ad_num);
		$(".ad1").prepend($(".ad1 span").eq(ad_nowNum));
		$(".ad1 span").soChange();
	})();
	(function() {
		var ad_num = $(".ad2 span").length;
		var ad_nowNum = Math.floor(Math.random() * ad_num);
		$(".ad2").prepend($(".ad2 span").eq(ad_nowNum));
		$(".ad2 span").soChange();
	})();
	(function() {
		var scrollBar = function(elem) {
			var vbottom = $(".footer").outerHeight();
			var bar = $(elem);
			menuYloc = null;
			var menuYloc0 = parseFloat($(bar).css("top"), 10);
			menuYloc = menuYloc0;
			$(bar).css({
				top: menuYloc,
				bottom: ""
			});
			var vheight = $(document).height() - $(bar).height() - vbottom;
			$(window).scroll(function() {
				offset1 = menuYloc + $(document).scrollTop();
				if (offset1 < vheight) {
					offset = menuYloc + $(document).scrollTop() + "px";
					$(bar).animate({
						top: offset
					}, {
						duration: 500,
						queue: false
					});
				}
			});
		};
		scrollBar(".fixed");
	})();
});
</script>
