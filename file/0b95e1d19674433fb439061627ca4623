<script type="text/javascript">
    expdate = new Date();
    var expireDay = '1';
    FixCookieDate(expdate);
    var cookieVal = '';
    //expdate.setTime (expdate.getTime() + (31536000));
    // alert("expireDay---found "+expireDay);
    expdate.setTime(expdate.getTime() + (expireDay * 24 * 60 * 60 * 1000));
    var campaignName_old = GetCookie('campaignName');
    if (campaignName_old != null && campaignName_old != "") {
        //alert('old campaignName found-->  '+campaignName_old+'!');

    }
    if (cookieVal != null && cookieVal != "") {
        //alert('old campaignName not found-->  '+campaignName_old+'!');
        //new cookies found set new value

        SetCookie("campaignName", cookieVal, expdate, '/');

    }
</script>

