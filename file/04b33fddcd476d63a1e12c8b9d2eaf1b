$(function(){	
$(window).scroll(function() {		
if($(window).scrollTop() >= 800){
$('.backtotop').fadeIn(300); 
}else{    
$('.backtotop').fadeOut(300);    
}  
});
$('.backtotop').click(function(){$('html,body').animate({scrollTop: '0px'}, 800);});	
});
</script> 
