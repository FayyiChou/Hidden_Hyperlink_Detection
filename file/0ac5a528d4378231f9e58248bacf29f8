    try {
        if( RollingArray && RollingArray.length != 0  ) {
            ghtCommentOptions.rolling = ghtCommentOptions.rolling.concat(RollingArray);
        }
    } catch(e){

    }

    function convertCurrency(sText) {
        var sText = sText+"";
        if(isNaN(parseFloat(sText,10))){
            return sText;
        }
        var nDot = 0,
                sReturn = "",
                nDotPosition = sText.indexOf("."),
                nLastPosition = sText.length;

        if (nDotPosition > -1) {
            nLastPosition = nDotPosition - 1;
        }

        //세자리마다 ,찍어 통화형식으로 만들기
        for (var i = sText.length; i >= 0; i--) {
            var sChar = sText.charAt(i);
            if (i > nLastPosition) {
                sReturn = sChar + sReturn;
                continue;
            }
            if (/[0-9]/.test(sChar)) {
                if (nDot >= 3) {
                    sReturn = ',' + sReturn;
                    nDot = 0;
                }
                nDot++;
                sReturn = sChar + sReturn;
            }
        }
        return sReturn;
    }

</script>






    
    
    


