    var nsc = 'tvcast.end';
    var ghtEnv = {
        sPageName : "clipEnd"
        , nClipNo : 1551021
        , sPartnerNo : '480'
        , sStationNo : '524'
        , sVideoId : '77AC7F23CDA921C338A99FBAC37C07F8CBB8'
        , sSearchKeyword : ''
        , sChannelId : 'golfdigestv'
        , sDisplayChannelId: 'golfdigestv'
        , sChannelName : '골프다이제스트TV'
        , sClipNo : '1551021'
        , sCategoryCodeAsString : 'SPORT'
        , nLikeItCount : 71
        , sLikeItDomain : 'http://common.like.naver.com'
        , bIsLogin : false
        , isPlayableAuth : 'notAdult'
        , sAccessKey : ''
        , nPlaylistNo : 17624
        , sPlaylistType : 'GENERAL'
        , nNextClipNo : '1536348'
        , nNextPlaylistNo : '17624'
        , sContinuousPlayCookieKey : 'NTC_ContPL'
        , sClipboardSwfUrl : '/resources/swf/clipboard.swf'
        , sCurrentClipUrl : 'http://tv.naver.com/v/1551021/list/17624'
        , sNextClipUrl : 'http://tv.naver.com/v/1536348/list/17624'
        , bIsDescriptionAvailable : true
        , bIsDescriptionOpened : false
        , bCommentOpen : false
        , bIsSmr : false
        , htComment2Option : {"domain":"http://static.cbox.naver.net","apiDomain":"http://apis.naver.com/commentBox/cbox","login":false,"ticket":"channel01","templateId":"default","objectId":"1551021","language":"ko","country":"KR","pageType":"default","pageSize":20,"indexSize":5,"replyPageSize":5,"cssId":"tvcast","cssUrl":"http://tv.naver.com/resources/release/css/comment/pc_20170323051955.css","id":"cbox_module","likeItId":"1551021","help":"down","groupId":"golfdigestv"}
        , bIsComment2 : true
        , apiDomain : "http://api.tv.naver.com"
        , logApiDomain : "http://logapi.tvcast.naver.com"
        , nstoreFreeMovie : "false"
    };

    var channelPPL = {};

    window.attachEvent ? window.attachEvent('onload', loadHandler) : window.addEventListener('load', loadHandler, false);

    function loadHandler() {
        var aLoadJs = new Array();

        aLoadJs.push('http://img.tvcast.naver.net/pc/js/jindo/jindo.component.1_13_0.20140930.min.js');
        aLoadJs.push('/resources/release/js/lcslog_20170323051955.js');
        aLoadJs.push('/resources/release/js/tvcast.Namespace_20170323051955.js');
        aLoadJs.push('/resources/release/js/tvcast.pc.Gnb_20170323051955.js');

        
        
        aLoadJs.push('/resources/release/js/clipEnd/tvcast.pc.ClipEnd.all_20170323051955.js');
        
        
        
        jindo.LazyLoading.load(aLoadJs, function() {
            tvcast.pc.Gnb = new Gnb();
            tvcast.pc.ClipEnd = new ClipEnd();
            tvcast.pc.LikeIt = new LikeIt(ghtEnv);
            tvcast.pc.Comment = new Comment();
            tvcast.pc.RecommendStats = new tvcast.RecommendStats({
                apiDomain : 'http://logapi.tvcast.naver.com'
                , clipNo : '1551021'
                , device : 'PC'
                , brandId : ''});
            tvcast.pc.Subscription = new Subscription();
            tvcast.pc.CreatorSubscription = new CreatorSubscription();
            tvcast.pc.pplCommerce.init(channelPPL);
        });

        aLoadJs = null;
    }
</script>





