<script>
	  


function pmsg(){
	
	if($("#name").val()==''){
		alert('姓名不能为空！');
		return;
		}

	if($("#email").val()==''){
		alert('邮箱地址不能为空！');
		return;
		}
	if($("#tel").val()==''){
		alert('联系方式不能为空！');
		return;
		}
	
	if($("#text").val()==''){
		alert('留言内容不能为空！');
		return;
		}
	
	
$.ajax({
 dataType: "json",
 data:{'text':$('#text').val(),'tel':$('#tel').val(),'name':$('#name').val(),'email':$('#email').val()},
url: "/Index/Message/runAdd.html",
   success: function (data) {

	   	if(data.msg=='2'){ alert('验证码错误！');return;}
		if(data.msg=='1'){alert('留言成功'); return;}
		if(data.msg=='0'){alert('留言失败'); return;}
      
         },
    error: function (XMLHttpRequest, textStatus, errorThrown) {
           alert("error!");
		  // $('#verify').click();
                                        } 
});
	}


</script>

