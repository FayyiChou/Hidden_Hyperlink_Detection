    <script type="text/javascript">
    var a = document.getElementsByClassName('swiper-slide');
    var arr = Array();
    for(i=0; i<a.length;i++){
      var img =  a[i].getElementsByTagName("img");
      arr[i] = img[0].getAttribute("data-src");
    }

    console.log(arr);
    
    //轮播初始化
    var swiper = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        prevButton:'.swiper-button-prev',
        nextButton:'.swiper-button-next',
        autoplay:false,
        paginationClickable: true,
        lazyLoading : true,
        lazyLoadingInPrevNext : true,
        arr:arr,
        paginationBulletRender: function (index, className) {
            if(this.arr[index].indexOf('pics.dangbei.net') > 0 ){
               strr = '<span class="' + className + '"><img src="'+this.arr[index].replace("!0", "")+'!h60"></span>';
            }else{
               strr = '<span class="' + className + '"><img src="'+this.arr[index]+'"></span>';
            }
            return strr;
        }
    });
//导航栏滚动条控制
    var pagination = document.getElementById('swiper-pagination');
    var pagination_pre = document.getElementById('swiper-pagination-pre');
    var pagination_next = document.getElementById('swiper-pagination-next')
    pagination_next.addEventListener('click',function(){
        var num = pagination.scrollLeft;
        pagination.scrollLeft = num + 1000;
    },true)
    pagination_pre.addEventListener('click',function(){
        var num = pagination.scrollLeft;
        pagination.scrollLeft = num - 1000;
    },true)

    function gp(){
        var a = document.getElementsByClassName('swiper-slide');
        for (i=0;i<a.length;i++ ) {
          if(a[i].className == 'swiper-slide swiper-slide-active')
            document.getElementsByClassName('red')[0].innerHTML = i+1;
        }
    }
    setInterval ('gp()',200);
</script>

