<script>
    $(function() {
        $('#close-ad').click(function () {
            $(this).parent().remove();
        });
    });
    $(function () {
        $("body").delegate('.layui-layer-shade', 'click', function () {
            layer.closeAll();
        });
        $('#keywords').bind({
            focus: function () {
                $(this).addClass('color-3');
            },
            blur: function () {
                $(this).removeClass('color-3');
            },
            keypress: function (event) {
                if (event.keyCode == "13") {
                    if ($("#keywords").val() != '') {
                        //sSearch();
                        window.location.href = "/search/" + $("#keywords").val();
                    } else {
                        $("#keywords").attr("placeholder", "搜索节目、明星、频道");
                    }
                }
            }
        });
        $(".search-top-btn").click(function () {
            if ($("#keywords").val() != '') {
                setTimeout("window.location.href = '/search/" + $("#keywords").val() + "/'", 100);
            } else {
                $("#keywords").attr("placeholder", "请输入关键字");
            }
        });
    });

</script>

<script>
	$(function(){
		$('.episodes-tab').click(function(){
			$('.episodes-dd').hide()
			$('.'+$(this).attr('data-id')).show()
			$('.episodes-tab').parent().removeClass('active');
			$(this).parent().addClass('active');
		});
		$('.episodes-more-btn').click(function(){
			$('.episodes-dd').hide()
			$('.episodes-0').show()
			$('a[data-id="episodes-0"]').parent().show();
			$('a[data-id="episodes-0"]').parent().addClass('active');
		});
	});
</script></body></html>