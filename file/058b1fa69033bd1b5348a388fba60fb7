<script>
$(function(){
    var gtoup_more_time;
    $('.gtoup-more').hover(function(){
        clearTimeout(gtoup_more_time);
        $('.group-tc-box').show();
    },function(){
        gtoup_more_time = setTimeout(function () {
            $('.group-tc-box').hide();
        }, 500)
    })
    $('.group-tc-box').hover(function(){
        clearTimeout(gtoup_more_time);
        $('.group-tc-box').show();
    },function(){
        gtoup_more_time = setTimeout(function () {
            $('.group-tc-box').hide();
        }, 500)
    })
});
</script>
