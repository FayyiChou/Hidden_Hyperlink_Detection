        var _order_timeout;
        $(".header-btn").hover(
            function () {
                clearTimeout(_order_timeout);
                $(".header-list").show();
            },
            function () {
                _order_timeout = setTimeout(function () {
                    $('.header-list').hide();
                }, 500);
            }
        );
