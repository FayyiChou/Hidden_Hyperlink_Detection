<script>
$(function(){
    var cache = {};
    $( "#loginname" ).autocomplete({
      minLength: 1,
      source: function( request, response ) {
        var term = request.term;
        if ( term in cache ) {
          response( cache[ term ] );
          return;
        }

        $.getJSON("http://nba.weibo.com/push/get_mail", request, function( data, status, xhr ) {
          cache[ term ] = data;
          response( data );
        });
      }
    });
})
    
</script>
