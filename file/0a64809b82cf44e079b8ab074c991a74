<script type="text/javascript" >
goto_mobile();//转跳到手机版

$(function() {

function aboxinit(){
try {  
  $('body').append('<div id="adbigbox" style="position:absolute;z-index:1000; text-align:center; width:100%; display:none;" ></div>')
  $('.a-box').each(function() {	
	var p=$(this),pb=$('#adbigbox'); 
	pb.unbind().bind('mouseleave',function() {pb.fadeOut(300)})
		p.find("a[bigimg]").mousemove(function() {var bigimg=$(this).attr('bigimg');  if(!bigimg||bigimg.indexOf('nopic')>0)return; pb.css({ top:($(this).offset().top-1),left:0}).fadeIn(300).html('<a target="_blank" href="'+$(this).attr('href')+'"><img src="'+bigimg+'"/></a>')});
	
	
  if(p.find('li').length<2)return;
	function abox_autoPlay(){
		p.find('li:last').stop().animate({opacity: 'hide'},500,function(){ $(this).show().insertBefore(p.find('li:first'));p.mouseleave(); } );
	} 
	p.hover(function(){ clearInterval(p.data('abtime'));
	
	},function(){				
		var time=parseInt(p.find('li:last').attr('interval'))||3;
		clearInterval(p.data('abtime'));p.data('abtime',setInterval(abox_autoPlay,time*1000));}).mouseleave();
 })
} 
catch (e) {
}
}

aboxinit()
});

</script>

