<script type="text/javascript">
	$(function(){
		$('.head-sitemap').attr('href', APP_URL + 'map.php');
		$('.head-rss').attr('href', APP_URL + 'rss.php');
		$('.head-login').attr('href', APP_URL + '?app=member&controller=index&action=login');
		$('.head-register').attr('href', 'http://bbs.xtol.cn/member.php?mod=register');
		$('.head-member').attr('href', APP_URL + '?app=contribution&controller=panel&action=index');
		$('.head-logout').attr('href', APP_URL + '?app=member&controller=index&action=logout');
		if ($.cookie(COOKIE_PRE+'auth')) {
			$('.login-after').show();
			var username = $.cookie(COOKIE_PRE+'username');
			if(!username) username = $.cookie(COOKIE_PRE+'rememberusername');
			$('.login-after span').html(username);
		} else {
			$('.login-before').show();
		}

	});
	function setHomePage()
	{
		var url = WWW_URL;
		if(!url) {
			url=document.URL;
		}
		if (document.all) {
			document.body.style.behavior = 'url(#default#homepage)';
			document.body.setHomePage(url);
		}
	}
	function addFavorite(title)
	{
		var url = WWW_URL;
		if(!(url&&title)) {
			url=document.URL;
			title=document.title;
		}
		if (document.all) {
			window.external.addFavorite(url,title);
		} else if (window.sidebar) {
			window.sidebar.addPanel(title, url, "");
		}
	}
</script>

