            var s = $('.hot_right ul .li_dy').size();
            for (var i = 0; i < s ; i++) {
                $('.hot_right ul .li_dy').eq(2 * i - 1).css('margin-right', '0');
            }
            $('.product').each(function () {
                $(this).find('ul .li_dy:last').css('margin-right', '0');
            });

            $('.hot_right ul .li_dy').hover(function () {
                $(this).css('border', '1px solid #fff');
                $(this).find('.dl_one').hide();
                $(this).find('.Mask').stop(true, true).show();
            }, function () {
                $(this).css('border', '1px solid #cbccc6');
                $(this).find('.dl_one').show();
                $(this).find('.umulate').show();
                $(this).find('.Mask').hide();
            });

            $('.cakes .cakes_hot .hot_left').hover(function () {
