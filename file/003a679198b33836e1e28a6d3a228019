	 <script type="text/javascript">
	 	$(document).ready(function(){
	 		var random =  Math.floor(Math.random() * (8- 1) + 1);
	 		$("#randomPic").html('<img src="https://css.taoguba.com.cn/images/shuoLogin/shuoLogin_'+random+'.jpg">').show("slow");
	 	});
		function tosub(){
		
			//if(!checkUserName()){return false;}
			//if(!checkPwd()){return false;}
			//if(!checkCode()){return false;}
			String.prototype.trim = function(){
				return this.replace(/(^\s*)|(\s*$)/g, "");
			}
			if(!checkLowPwd1(document.getElementById("pwd").value,document.getElementById("userName").value)){
				showAlert("密码错误！");
				return;
			}
			if(document.getElementById("checkCode") && document.getElementById("checkCode").value==""){
				showAlert("请输入验证码");
				return;
			}
			if(document.getElementById("pwd").value.length<6){
				document.getElementById("pwdlevel").value="N";
			}
			var f = document.forms["userFrm"];	
			f.submit();
			return;
		}
		function checkLowPwd1(pwd,userName){
			if(pwd=="password" || pwd=="qwert" || pwd=="QWERT" || pwd==userName){
				document.getElementById("pwdlevel").value="N";
				//showAlert(userName+"兄好，您的密码过于简单,请您登录后修改密码。");
				return true;
			}
			var lavel = checkSimplePwd(pwd);
			if(lavel==2){
				return false;
			}else if(lavel==1){
				document.getElementById("pwdlevel").value="N";
				//showAlert(userName+"兄好，您的密码过于简单,请您登录后修改密码。");
				return true;
			}
			return true;
			
		}
		function checkCode(){
			var object=document.getElementById('checkCode');
			if(isNaN(object.value)){
				showAlert("验证码错误!");
				return false;
			}else if(object.value.length!=4){
				showAlert("验证码错误!");
				return false;
			}else{
				return true;
			}
		}
		function checkPwd(){
		//	var object=document.getElementById('pwd');
			if(document.getElementById('pwd').value==""){
				showAlert("密码不能为空");
				return false;
			}else{return true;}
		}
		function checkUserName(){
			var object=document.getElementById('userName');
			if(object.value==""){
				showAlert("用户名不能为空");
				return false;
			}
			object.value = object.value.trim();
			var c = 0;
			var count = 0;
			var value = escape(object.value);
			for(i = 0; i < value.length; i ++){
				c = value.indexOf("%u", c);
				if (c > -1){
					c++;
					count ++;
					i = c;
				}else{
					break;
				}
			}
			if (count > 6){showAlert("最多只能有6个汉字");object.value = "";object.focus();return false;}
			if (count + object.value.length > 24){showAlert("用户名超过规定长度.");object.value = "";object.focus();return false;}
			return true;
		}
		function changeVcode(){
			var imgsObj = document.getElementById("codeImage");
			imgsObj.src = "getCode?ref="+Math.random();
			return false;
		}
	</script>
	
