<a class="fixed-right-bottom" href="http://zhuangxiu.ganji.com/zhaobiao/" gjalog="/zhuangxiu_star/trace@tab=service_list_right@city=162@atype=click" target="_blank"><em class="closebtn" href="#"></em></a>
<script type="text/javascript">
GJ.use(['jquery','js/util/storage/storage2.js'], function ($,Storage) {
    function GetDateStr(AddDayCount)
    {
        var dd = new Date();
        dd.setDate(dd.getDate()+AddDayCount);//获取AddDayCount天后的日期
        var y = dd.getFullYear();
        var m = dd.getMonth()+1;//获取当前月份的日期
        var d = dd.getDate();
        return y+"/"+m+"/"+d;
    }

    var serviceBottomRightStorage = new Storage('gj_service_store');//add by liuxiaopeng FUWU-4444

    var showBottomRightCookie = 'bottom_right';
    var localKey = 'local_bottom_right';
    var currentUrl = 'zhaoshangjiameng';
    var point24 = new Date(GetDateStr(1)).getTime();
    point24 = point24/1000;

    var cache_data = null;
    try {
        //cache_data = GJ.storage.get(localKey);
        cache_data = serviceBottomRightStorage.get(localKey);
    } catch (ex) {
        cache_data = null;
    }


    if(cache_data != null){
        cache_data = GJ.jsonDecode(cache_data);
    }
    $(".fixed-right-bottom .closebtn").click(function(){
        $(this).parent().hide();
        //GJ.setCookie(showBottomRightCookie, 1, 86400);
        serviceBottomRightStorage.set(showBottomRightCookie, 1, new Date().getTime()+86400*1000);
        if(cache_data == null){
            cache_data = {};
        }
        cache_data[currentUrl] = point24;
        //GJ.storage.set(localKey, GJ.jsonEncode(cache_data));
        serviceBottomRightStorage.set(localKey, GJ.jsonEncode(cache_data));
        return false;
    });
    var isShowBottomRightTender = function(){
        if(serviceBottomRightStorage.get(showBottomRightCookie) == null) {//用户没有手动点击关闭
                ShowBottomRightTender(currentUrl, cache_data, point24);
        }
    }
    var ShowBottomRightTender = function(currentUrl, localData, pointHour24){
        var showImageObj = $(".fixed-right-bottom");
        var timestamp = Date.parse(new Date());
        timestamp = timestamp/1000;

        if(localData == null){
            showImageObj.show();
            localData = {};
            localData[ currentUrl ] = pointHour24;
        }else{
            if(localData[currentUrl]){//设置过
                if(timestamp > localData[currentUrl]){
                    showImageObj.show();
                    localData[currentUrl] = pointHour24;
                }
            }else{
                showImageObj.show();
                localData[currentUrl] = pointHour24;
            }
        }

        if(showImageObj.css("display") == 'block' || showImageObj.css("display") == '' || showImageObj.css("display") == 'inline'){
            setTimeout(function(){
                showImageObj.hide();

                //GJ.storage.set(localKey, GJ.jsonEncode(localData));
                serviceBottomRightStorage.set(localKey, GJ.jsonEncode(localData));
            }, 10000);
        }
    }

    isShowBottomRightTender();
})
</script>

