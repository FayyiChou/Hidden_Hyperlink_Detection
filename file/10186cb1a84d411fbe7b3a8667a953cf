<script type="text/javascript">
    GJ.use('jquery', function(){
        $('.h-search').hide();
        $('#ad_containter_couplet').hide();

        $(".close-btn").click(function(){
            $(".fixed-mobile").hide();
            return false;
        });

        $(".company").mouseover(function(){
            var str = $(this).attr('id');
            var id = str.split('_')[1];

            $("#div_"+id).addClass('active');

        });

        $(".company").mouseout(function(){
            var str = $(this).attr('id');
            var id = str.split('_')[1];
            $("#div_"+id).removeClass('active');
        });



    });
</script>


