<script type="text/javascript">
    var tab=function(id,titleClass,contentClass){
        var obj=document.getElementById(id);
        var tabTitle = obj.querySelectorAll(titleClass);
        var tabContent=obj.querySelectorAll(contentClass);
        for(var i=0;i<tabTitle.length;i++){
            var j=i;
            (function(j){
                tabTitle[j].addEventListener('click',function(){
                    for(var i=0;i<tabTitle.length;i++){
                        tabTitle[i].className="";
                        tabContent[i].style.display="none";
                    }
                    this.className='now';
                    tabContent[j].style.display='block';
                })
            })(i)
        }
    }

    //tab('list_tab','.tab_title span','.tab_content');
    tab('pro_list_tab','.pro_tab_title span','.pro_tab_content');

</script>
<script type="text/javascript">
    $(function(){
        getList(3)
        $('#list_tab .tab_title span').on('click', function(){
            $('#list_tab .tab_title span').removeClass('now');
            $(this).addClass('now');

            $('#list_tab .tab_content').html('加载中……');
            var zone = $(this).data('zone');
            getList(zone);
        })
        function getList(zone){
            $.ajax({
                type: 'get',
                url: 'http://topic.17k.com/liansai2016/getBookVoteRank.action?zone='+zone+'&pageSize=5&jsonp=callback',
                dataType: 'jsonp',
                jsonpCallback:'callback',
                success: function(list){
                    var html = '';
                    if(list.totalCount > 0){
                        for(var i=0; i < list.data.length; i++){
                            html += '<div class="list">';
                            html += '<img src="http://img.17k.com/subject/wwls2016/skin/h5_list_icon'+(i+1)+'.jpg" />';
                            html += '<a href="http://h5.17k.com/book/'+list.data[i].bookId+'.html">['+list.data[i].categoryName+']</a>';
                            html += '<a href="http://h5.17k.com/book/'+list.data[i].bookId+'.html">'+list.data[i].bookName+'</a>';
                            html += '<span>'+list.data[i].vote+'</span>';
                            html += '</div>';
                        }
                    }else{
                        html += '暂无榜单';
                    }

                    $('#list_tab .tab_content').html(html);

                }
            })
        }

    });
</script>
