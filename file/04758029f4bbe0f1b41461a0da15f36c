			<script language="JavaScript">
			var bookid='66234';
			var bookname='重生之军营小媳妇';
			var prevdata='';
			function sendnewreview()
			{
				var reviewuser='192.187.101.58';
				var reviewdata=$('#reviewdata').val();
				if (reviewdata=='')
				{
					return;
				}
				if (reviewdata.length<10)
				{
					alert('你评论的文字太少了');
					return;
				}

				if (reviewdata.length>100)
				{
					alert('你评论的文字太多了');
					return;
				}
				if (prevdata==reviewdata)
				{
					alert('你不能发表重复评论');
					return;
				}
				prevdata=reviewdata;


				$.get("/ajaxcall.php?action=newreview&bookid="+bookid+"&bookname="+escape(bookname)+"&data="+escape(reviewdata), function(result){
					
					if(result=='fail01')
					{
						alert('你不能评论的内容过少或者内容含有过多无效字符');
					}
					else if(result=='fail02')
					{
						alert('你不能连续发表评论,请过几分钟再试');
					}
					else if(result=='OK')
					{
						var reviewhtml="<p class='reviewuser'>书友评论了《<a href='/haokan/"+bookid+".html'>"+bookname+"</a>》：</p><p class='reviewrow'>"+reviewdata+"</p>";
						$('#reviewlist').append(reviewhtml);
						alert('内容已经提交,审核通过后将自动发布');
					}

				});
			}
			</script>

