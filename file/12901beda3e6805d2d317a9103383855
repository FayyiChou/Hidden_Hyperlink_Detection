<script>
function change_type(s_type){
  $("#search_type").val(s_type);
}
$('#search_content').bind('keypress', function (event) {
            if (event.keyCode == "13") {
               search_jump();
            }
});
function search_jump(){
  var search_type = $("#search_type").val();
  var search_content = $.trim($("#search_content").val());

  	var re_script=/<script.*>.*script>/;
	var re_iframe=/<iframe.*>.*iframe>/;
	var re_style=/<style.*>.*style>/;
	if(re_script.test(search_content) || re_iframe.test(search_content)||re_style.test(search_content)){
		art.tips({content:'您当前输入的内容存在风险，请重新输入',type:'0'});
		$("#search_content").val('')
		return false;
	}
  var url = encodeURIComponent("http://yun.jzjyyun.cn/index.php?r=center/person/address/friendship&keyword="+search_content);
 // if(search_content!=''){
    if(search_type=='1'){
      location.href = "http://res.jzjyyun.cn/searchCont.html?title="+search_content;
    }else if(search_type=='2'){
      location.href = "http://apps.jzjyyun.cn/sme-portal/app/list.html?appName="+search_content;
    }else if(search_type=='3'){
                  location.href = "http://yun.jzjyyun.cn/index.php?r=portal/user/login&service="+url;
              }else if(search_type=='4'){
		  location.href ="http://yun.jzjyyun.cn/index.php?r=studio/searchcourse/course&search="+search_content;
		  
	  }else if(search_type=='5'){
		  
		  location.href ="http://yun.jzjyyun.cn/index.php?r=studio/square/search&type=studio&key_words="+search_content;
	  }else if(search_type=='6'){
		  location.href ="http://yun.jzjyyun.cn/index.php?r=portal/searchschool/index&key_word="+search_content;
	  }
      
   // }
  }
  
/* function login_tips(){
  var search_type = $("#search_type").val();
  var search_content = $.trim($("#search_content").val());
  var url = encodeURIComponent("http://yun.jzjyyun.cn/index.php?r=center/person/address/friendship&keyword="+search_content);
  
  if(search_type=='3'){
      //  if(search_content==''){
    location.href = "http://yun.jzjyyun.cn/index.php?r=portal/user/login&service="+url;//}
        
    
  }
} */


</script>
