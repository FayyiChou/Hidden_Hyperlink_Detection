            if (strCommentId == "minicontent") {                
                if (ueContent.hasContents() == false) {
                    art.dialog({ title: "系统提示", content: "回复内容不能为空" }).time(2);
                    return false;
                }
                else if (ueContent.getContentTxt().length < 10) {
                    art.dialog({ title: "系统提示", content: "回复内容不能少于10字" }).time(2);
                    return false;
                }
                strContent = ueContent.getContent().replace(/&/g, "[]");
                //$("#" + strCommentId).val(ueContent.getContent().replace(/&/g, "[]"));
            }
            else {
                if ($("#" + strCommentId).val().replace(/ /g, "") == "") {
                    art.dialog({ title: "系统提示", content: "回复内容不能为空" }).time(2);
                    return false;
                }
                else if ($("#" + strCommentId).val().replace(/ /g, "").length < 10) {
                    art.dialog({ title: "系统提示", content: "回复内容不能少于10字" }).time(2);
                    return false;
                }
                strContent = "<p>" + $("#" + strCommentId).val() + "</p>";
            }

            $.ajax({
                url: "/control/UserControl.ashx?action=CommentAdd",
                data: "b_id=" + strB_Id + "&p_id=" + strP_Id + "&content=" + strContent,
                type: "post",
                async: false,
                dataType: "json",
                beforeSend: function () {
                    art.dialog({ id: 'dlgWait', title: '系统提示', content: '正在处理中，请等待。。。', icon: 'info' });
                    return true;
                },
                success: function (objJSON) {
                    art.dialog.list['dlgWait'].close();
                    //var objJSON = eval("(" + data + ")");  // change the JSON string to javascript object
                    if (objJSON.success) {
                        art.dialog({
                            title: "系统提示", content: objJSON.msg,
                            close: function () {
                                window.location.reload();
                                $("#ulComment").append(objJSON.id);
                                if (strCommentId == "minicontent") {
                                    ueContent.setContent("");
                                }
                                else {
                                    $("#" + strCommentId).val("");
                                }
                            }
                        }).time(2);
                    }
                    else {
                        art.dialog({
                            title: "系统提示", content: objJSON.msg
                        }).time(3);
                    }
                }
            });
        }

        function followPostAdd(strP_Id) {
            $.ajax({
                url: "/control/UserControl.ashx?action=FollowPostAdd",
                data: "p_id=" + strP_Id,
                type: "post",
                async: false,
                dataType: "json",
                beforeSend: function () {
                    art.dialog({ id: 'dlgWait', title: '系统提示', content: '正在处理中，请等待。。。', icon: 'info' });
                    return true;
                },
                success: function (objJSON) {
                    art.dialog.list['dlgWait'].close();
                    //var objJSON = eval("(" + data + ")");  // change the JSON string to javascript object
                    if (objJSON.success) {
                        art.dialog({
                            title: "系统提示", content: objJSON.msg,
                            close: function () {
                                //window.location.reload();
                                //window.location.href = "/login.aspx";                                
                            }
                        }).time(2);
                    }
                    else {
                        if (objJSON.code == 1000) {
                            window.location.href = "http://www.dyonr.com/login.aspx?ReturnUrl=" + "http://bbs.dyonr.com" + window.location.pathname + window.location.search;
                        }
                        else {
                            art.dialog({
                                title: "系统提示", content: objJSON.msg
                            }).time(3);
                        }
                    }
                }
            });
        }


        function deleteComment(strB_Id, strC_Id, strU_Id) {
            art.dialog.confirm("确定删除该评论吗？", function () {
                $.ajax({
                    url: "/control/UserControl.ashx?action=DeleteComment",
                    data: "b_id=" + strB_Id + "&c_id=" + strC_Id + "&u_id=" + strU_Id,
                    type: "post",
                    async: false,
                    dataType: "json",
                    beforeSend: function () {
                        art.dialog({ id: 'dlgWait', title: '系统提示', content: '正在处理中，请等待。。。', icon: 'info' });
                        return true;
                    },
                    success: function (objJSON) {
                        art.dialog.list['dlgWait'].close();
                        if (objJSON.success) {
                            $("#liComment_" + strC_Id).remove();
                            art.dialog({
                                title: "系统提示", content: objJSON.msg,
                                close: function () {
                                }
                            }).time(2);
                        }
                        else {
                            art.dialog({
                                title: "系统提示", content: objJSON.msg
                            }).time(3);
                        }
                    }
                });
            });
        }

        function followBoard(strId) {
            $.ajax({
                url: "/control/UserControl.ashx?action=FollowBoard",
                data: "b_id=" + strId,
                type: "post",
                async: false,
                dataType: "json",
                beforeSend: function () {
                    art.dialog({ id: 'dlgWait', title: '系统提示', content: '正在处理中，请等待。。。', icon: 'info' });
                    return true;
                },
                success: function (objJSON) {
                    art.dialog.list['dlgWait'].close();
                    //var objJSON = eval("(" + data + ")");  // change the JSON string to javascript object
                    if (objJSON.success) {
                        art.dialog({
                            title: "系统提示", content: objJSON.msg,
                            close: function () {
                                window.location.reload();
                                //window.location.href = "/login.aspx";
                            }
                        }).time(2);
                    }
                    else {
                        if (objJSON.code == 1000) {
                            window.location.href = "http://www.dyonr.com/login.aspx?ReturnUrl=" + "http://bbs.dyonr.com" + window.location.pathname + window.location.search;
                        }
                        else {
                            art.dialog({
                                title: "系统提示", content: objJSON.msg
                            }).time(3);
                        }
                    }
                }
            });
        }
