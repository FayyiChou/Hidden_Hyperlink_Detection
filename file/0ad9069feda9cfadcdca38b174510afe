<script>
    $(function(){
        $("#loginBtn").click(function(e){
        	  if($("#password").val()==""||$("#userName").val()==""){
        		    $("#font_cwxx").html("用户名或密码不能为空");
        	    	$("#span_login").hide();
        	    	$("#font_cwxx").show();
        	        return false;
        	  }
        	  $("#loginBtn").val("正在登录...");
        	  $.ajax({
        			url:  '/dologin',
        			type: 'POST',
        			data:$("#glodon_login_normal").serialize(),
        			success: function(data) {
        				if(data.Msg=="loginFalse"){
        			    	$("#font_cwxx").html("用户名或密码错误");
        			    	$("#span_login").hide();
        			    	$("#font_cwxx").show();
        			    	$("#loginBtn").val("立即登录");
        			    }else if(data.Msg=="locked"){
        			    	$("#font_cwxx").html("登录太频繁啦,休息一下!");
        			    	$("#span_login").hide();
        			    	$("#font_cwxx").show();
        			    	$("#loginBtn").val("立即登录");
        			    }else if(data.Msg="success"){
        			    	window.frames["loginIframeRedirect"].location.href = "https://account.glodon.com/tokenLogin?token="+data.login_token + "&target_url="+"http://xunjia.gldjc.com/index";
        			        window.location.href="http://xunjia.gldjc.com/index";
        			    }else{
        			    	$("#font_cwxx").html("网络异常!");
        			    	$("#span_login").hide();
        			    	$("#font_cwxx").show();
        			    	$("#loginBtn").val("立即登录");
        			    }
        			}
        	  });
        });
        
    	$('#password').keydown(function(e){
    		if(e.keyCode==13){
    			$("#loginBtn").click();
    		}
        });
    });
    function applyForm(){
        window.location.href = '/reg/signUpBudgeteer';
    }
    function loginByThirdParty(value){
    	var hostUrl="http://www.gldjc.com";
    	if(value=="1"){
    		window.location.href = "https://account.glodon.com/3rd/login?source=qq&service_key=E6xtqZdaM3D0no2jG9Q6p5yisfh4WmJJ&callback_url="+hostUrl
    	}else if(value=="2"){
    		window.location.href = "https://account.glodon.com/3rd/login?source=sina&service_key=E6xtqZdaM3D0no2jG9Q6p5yisfh4WmJJ&callback_url="+hostUrl
    	}else if(value=="3"){
    		window.location.href = "https://account.glodon.com/3rd/login?source=wx&service_key=E6xtqZdaM3D0no2jG9Q6p5yisfh4WmJJ&callback_url="+hostUrl
    	}
    }
    
