//日期下拉框选择
function selDateDdl()
{
    if($("#ddlStartDate").val()!=null)
        startDate=$("#ddlStartDate").val();
    if($("#ddlEndDate").val()!=null)
        endDate=$("#ddlEndDate").val();
    if(startDate>endDate)
    {
         alert('期末时间需晚于期初时间');
         return;
    }
    getDataUrl("HongGuanLine","/area/data/DiQuZhiBiao.ashx?startDate="+startDate+"&endDate="+endDate+"&codelist="+indexList+"&citycode=360000","update")
      $("#iframe_datalist").attr("src","/area/dqzb_iframeInner.aspx?page=1&codeList="+mainCode+"&citycode=360000&startdate="+firstDate+"&endDate="+endDate);
    	
}
