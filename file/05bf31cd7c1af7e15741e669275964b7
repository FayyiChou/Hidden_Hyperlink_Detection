    <script type="text/javascript">
    $.ajax({ 
		async:false, 
		url: 'http://brand.egou.com/api/rand_items.htm?num=5',
		type: "GET", 
		dataType: 'jsonp', 
		timeout: 5000, 
		success: function (json) {
		    var ads = "";
			var array = new Array();
			var data = json;//
			
			for(var i=0;i<data.length;i++){
				array.push('<li><a target="_blank" href="http://union.egou.com/to?site=17&page=bjdetail&pos=detailp');
				array.push(i+1);
				array.push('&uniq=&fuid=&ext=&url=');
				array.push(data[i].link);
			    array.push('"></a><div class="pic"><img src="');
			    if(data[i].itemLogo!=null&&data[i].itemLogo!=""){
			    array.push(data[i].itemLogo);
			    }else if(data[i].productImg!=null&&data[i].productImg!=""){
			    array.push(data[i].productImg);
			    }
			    array.push('"></div><div class="title"><span class="red">');
			    array.push(data[i].rebate);
			    array.push('折');
			    if(data[i].editorRecommend!=null&&data[i].editorRecommend!=""){
			     array.push('/');
			     array.push(data[i].editorRecommend);
			    }
			    array.push('</span>');
			    array.push(data[i].itemName);
			    array.push('</div><div class="money_bg">￥<span class="money">');
			    array.push(data[i].discountPrice.toFixed(2));
			    array.push('</span><del>￥');
			    array.push(data[i].reservePrice.toFixed(1));
			    array.push('</del></div><div class="f_money"><span class="fuhao">-</span><span class="f_num">');
			    array.push(data[i].backMoney.toFixed(2));
			    array.push('</span><span class="f_word">购买后返');
			    array.push(data[i].backRebate);
			    array.push('%</span></div></li>');
			}
			if(array.length > 0){
				ads = array.join('');
				$("#adv_cjf_list").html(ads);
				$(".adv_cjf_main").show();
			}else{
				$(".adv_cjf_main").hide();
			}
		}
	});
    </script>
