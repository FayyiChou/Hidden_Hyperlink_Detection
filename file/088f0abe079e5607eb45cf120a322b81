		}

        window.onbeforeunload = onbeforeunload_handler;   
        window.onunload = onunload_handler;   
        var DispClose = true;
        function onbeforeunload_handler(){ 
            if(DispClose){
              var change = getFlash("Main").getPhotoChange();
               if(change)
                {
                    return "您辛苦编辑的照片还未保存呢！";
                }
                else
                {
                    return void (0);
                }
                DispClose=false;
            }
        }         
        function onunload_handler(){   
            if(DispClose){
                var change = getFlash("Main").getPhotoChange();
               if(change)
                {
                    return "您辛苦编辑的照片还未保存呢！";
                }
                else
                {
                    return void (0);
                }
                DispClose=false;
            }
        } 
        var xmlHttp;
        function S_xmlhttprequest() {
            if(window.ActiveXObject) {
                xmlHttp = new ActiveXObject('Microsoft.XMLHTTP');
            } else if(window.XMLHttpRequest) {
                xmlHttp = new XMLHttpRequest();
            }
        }
        function submitdata(data){
            S_xmlhttprequest();	
            xmlHttp.open("GET","data.php?content="+data,true)
            xmlHttp.send(null);
            return true;
        }
        function closeWindow(url)
        {
			parent.location.href=url;	
        }
        function showBaiduSignDialog(url)
        {
            var ifr=document.createElement('iframe');
            document.body.appendChild(ifr);
            ifr.src=url;
            ifr.id='baiduLoginIfr';
            ifr.width='504';
            ifr.height='360';
            ifr.scrolling='no';
            ifr.style.border='none';
            ifr.style.background='#fff url(http://xiuxiu.web.meitu.com/baidu/images/loading.gif) center center no-repeat';
            ifr.style.overflow='hidden';
            ifr.style.position='absolute';
            ifr.style.top='50%';
            ifr.style.left='50%';
            ifr.style.margin='-165px 0 0 -250px';
            ifr.setAttribute("frameborder", "0", 0);// 最后一个0 表示是否区别大小写  ie下不支持 直接ifr.frameBorder='0'
            ifr.setAttribute("scrolling", "no", 0);
            ifr.onload=function(){
                this.style.background='#fff';
            }
        }
