<script>
	$(function () {
		//活动板块弹窗询价开始
		$("#hdnew ul li").click(function(){
			var hd_sonname = $(this).find('h5').html();
			$(".y_xjMask_txt").html(hd_sonname);
			$("#UserMessage2").val(hd_sonname);
			$("#y_txxx").show();
		})

		$("#tjcgbutton2").click(function(){
			$("#txxx2").hide();
			location.reload();
		})
		$("#ljbm2").click(function(){
			var tjcode = '587307ab39cad054251ee3f1';
			xunjia2(tjcode);
		})

		function xunjia2(tjcode) {
			var object = {}
			object.UserMessage = $('#UserMessage2').val();
			object.UserName = $("#txxx_name2").val();
			object.UserPhone = $("#txxx_phone2").val();
			object.source_type = 9; //活动询价

			if ($("#is_car_loan").is(":checked")) {
				object.is_loan = 1;
			}
			if ($("#is_ershouche").is(":checked")) {
				object.is_ershouche = 1;
			}
			if ($("#is_import").is(":checked")) {
				object.is_import = 1;
			}
			if ($("#is_newcar").is(":checked")) {
				object.is_newcar = 1;
			}

			if(!(/[\u4E00-\u9FA5]{1,5}(?:·[\u4E00-\u9FA5]{1,5})*/.test(object.UserName))) {
				$(".error p").show();
				$(".error p").html("姓名请填写1-5个汉字");
				return false;
			}

			if(!(/^1[3|4|5|7|8]\d{9}$/.test(object.UserPhone))){
				$(".error p").show();
				$(".error p").html("手机号码格式有误，请重填");
				return false;
			}

			$.ajax({
				type: "POST",
				url: "/qiche/ajax/orderPrice",
				data: object,
				dataType:'jsonp',
				jsonp: "callback_price",
				success: function(res){
					if(res.code == 0){
						$ANA.event({
							id: tjcode,
							sid: res.id
						});
						$('#writeForm11').hide();
						$('#writeForm22').show();
					}
					else if(res.code == 1){
						alert('您已经提交过询价信息了!');
					}
				}
			})
		}
		//活动板块弹窗询价结束

		//C1、C5 广告位
		/*$("#detailshow_1").click(function(){
			$("#tjcode").val('585ce57339cad024ff256f91');
			$("#txxx").show();
		})
		$("#detailshow_5").click(function(){
			$("#tjcode").val('585d187439cad0218c7b9cf1');
			$("#txxx").show();
		})*/
		$("#tjcgbutton").click(function(){
			$("#txxx").hide();
			location.reload();
		})
		$("#ljbm").click(function(){
			var tjcode = $("#tjcode").val();
			xunjia(tjcode);
		})


		function xunjia(tjcode) {
			var object = {}
			object.UserName = $("#txxx_name").val();
			object.UserPhone = $("#txxx_phone").val();
			object.dropCar = $("#txxx_car").val();
			object.dropType = 4232;
			object.source_type = 7;

			//console.log(object);
			if ($("#is_car_loan").is(":checked")) {
				object.is_loan = 1;
			}
			if ($("#is_ershouche").is(":checked")) {
				object.is_ershouche = 1;
			}
			if ($("#is_import").is(":checked")) {
				object.is_import = 1;
			}
			if ($("#is_newcar").is(":checked")) {
				object.is_newcar = 1;
			}

			if(!(/[\u4E00-\u9FA5]{1,5}(?:·[\u4E00-\u9FA5]{1,5})*/.test(object.UserName))) {
				$(".error p").show();
				$(".error p").html("姓名请填写1-5个汉字");
				return false;
			}

			if(!(/^1[3|4|5|7|8]\d{9}$/.test(object.UserPhone))){
				$(".error p").show();
				$(".error p").html("手机号码格式有误，请重填");
				return false;
			}

			$.ajax({
				type: "POST",
				url: "/qiche/ajax/orderPrice",
				data: object,
				dataType:'jsonp',
				jsonp: "callback_price",
				success: function(res){
					if(res.code == 0){
						$ANA.event({
							id: tjcode,
							sid: res.id
						});
						$('#writeForm1').hide();
						$('#writeForm2').show();
					}
					else if(res.code == 1){
						alert('您已经提交过询价信息了!');
					}
				}
			})
		}

		$(".queding").click(function(){
			var catid=121;
			var module_id = $(this).attr('data-module_id');
			var shield_time=$(this).parents(".hideOne"+module_id).find("input[type='radio']:checked").val();
			var brandTypeId = autoBrandTypeId;
			var url = 'http://news.k618.cn/qiche/2017-03-25/1929701.html';
			$.ajax({
				url: url_pre+'/ajax/setNotLike',
				type: 'POST',
				data: {catid:catid,module_id: module_id,brandTypeId:brandTypeId,url:url,shield_time:shield_time*86400},
				dataType:'jsonp',
				success: function (rsp) {
					if(rsp.status == 1){
						$(".hideOne"+module_id).hide();
					}
				},
				error:function(rsp){
				}
			});
		});

	});

</script>

<script>
	countClick(2,4232);
	countClick_art(1929701);

	window._bd_share_config = {
		common : {
			bdText : ''
		},
		share : [{
			"bdSize" : 16
		}]
	}
	with(document)0[(getElementsByTagName('head')[0]||body).appendChild(createElement('script')).src='http://bdimg.share.baidu.com/static/api/js/share.js?v=89860590.js?cdnversion='+~(-new Date()/36e5)];

	$("#submit").bind("click", function(){
		var object = {}
		object.UserName = $("#zixun_name").val();
		object.UserPhone = $("#zixun_phone").val();
		object.UserSex = $(".zixun_sex:checked").val();
		object.dropType = 4232;
		object.dropCar = $("#zixun_car").val();
		object.province = $("#zixun_province").val();
		object.dropCity = $("#zixun_city").val();
		object.dropBrand = 1;
		object.source_type = 6;

		if(!(/[\u4E00-\u9FA5]{1,5}(?:·[\u4E00-\u9FA5]{1,5})*/.test(object.UserName))) {
			alert("姓名有误，请重填");
			return false;
		}

		if(!(/^1[3|4|5|7|8]\d{9}$/.test(object.UserPhone))){
			alert("手机号码有误，请重填");
			return false;
		}
		
       if ($("#zixun_province").val()==''){
            alert("请选择省份");
            return false;        	
        } else if ($("#zixun_city").val()=='') {
        	alert("请选择城市");
            return false;
        }			

		$.ajax({
			type: "POST",
			url: "/qiche/ajax/orderPrice",
			data: object,
			dataType:'jsonp',
			jsonp: "callback_price",
			success: function(res){
				if(res.code == 0){
					$ANA.event({
						id: '5851224d39cad07e1026bcc1',
						sid: res.id
					});
					alert(res.msg);
				}
				else if(res.code == 1){
					alert('您已经提交过询价信息了!');
				}
			}
		})
	});

	var city_id = '320100';
	function setCity(){
		var provinceId = $("#zixun_province").val();
		var url = "/qiche/jdata/getCitys";
		$.getJSON(url+'?callback_getcitys=?',{'provinceId':provinceId}, function (res){
			var html = '<option value="">请选择</option>';
			$.each(res, function(i, item){
				html += '<option value="'+item.id+'" >'+item.name+'</option>';
			})
			$("#zixun_city").html(html);
		});

	}
	setCity();
	$("#zixun_province").change(function(){setCity();})

	/**
	 * 正文首图询价城市切换 及 询价提交
	 */
	$(".giftBtn").bind("click", function(){
		var object = {}
		object.UserName = $("#pic_zixun_name").val();
		object.UserPhone = $("#pic_zixun_phone").val();
		object.dropType = 4232;
		object.dropCar = $("#pic_zixun_car").val();
		object.province = $("#pic_zixun_province").val();
		object.dropCity = $("#pic_zixun_city").val();
		object.dropBrand = 1;
		object.source_type = 6;

		if(!(/[\u4E00-\u9FA5]{1,5}(?:·[\u4E00-\u9FA5]{1,5})*/.test(object.UserName))) {
			alert("姓名有误，请重填");
			return false;
		}

		if(!(/^1[3|4|5|7|8]\d{9}$/.test(object.UserPhone))){
			alert("手机号码有误，请重填");
			return false;
		}
		
       if ($("#pic_zixun_province").val()==''){
            alert("请选择省份");
            return false;        	
        } else if ($("#pic_zixun_city").val()=='') {
        	alert("请选择城市");
            return false;
        }		
		
		

		$.ajax({
			type: "POST",
			url: "/qiche/ajax/orderPrice",
			data: object,
			dataType:'jsonp',
			jsonp: "callback_price",
			success: function(res){
				if(res.code == 0){
					$ANA.event({
						id: '586cb22739cad064e14c6911',
						sid: res.id
					});
					alert(res.msg);
				}
				else if(res.code == 1){
					alert('您已经提交过询价信息了!');
				}
			}
		})
	});

	function picSetCity(){
		var provinceId = $("#pic_zixun_province").val();
		var url = "/qiche/jdata/getCitys";
		$.getJSON(url+'?callback_getcitys=?',{'provinceId':provinceId}, function (res){
			var html = '<option value="">请选择</option>';
			$.each(res, function(i, item){
				html += '<option value="'+item.id+'" >'+item.name+'</option>';
			})
			$("#pic_zixun_city").html(html);
		});

	}
	picSetCity();
	$("#pic_zixun_province").change(function(){picSetCity();})

	/**
	 * 活动版块询价城市切换 及 询价提交
	 */
	$(".xprice").bind("click", function(){
		var object = {}
		object.UserName = $("#huodong_name").val();
		object.UserPhone = $("#huodong_phone").val();
		object.dropType = 4232;
		object.dropCar = $("#huodong_car").val();
		object.province = $("#huodong_province").val();
		object.dropCity = $("#huodong_city").val();
		object.dropBrand = 1;
		object.source_type = 6;

		if(!(/[\u4E00-\u9FA5]{1,5}(?:·[\u4E00-\u9FA5]{1,5})*/.test(object.UserName))) {
			alert("姓名有误，请重填");
			return false;
		}

		if(!(/^1[3|4|5|7|8]\d{9}$/.test(object.UserPhone))){
			alert("手机号码有误，请重填");
			return false;
		}
		
       if ($("#huodong_province").val()==''){
            alert("请选择省份");
            return false;        	
        } else if ($("#huodong_city").val()=='') {
        	alert("请选择城市");
            return false;
        }		
		

		$.ajax({
			type: "POST",
			url: "/qiche/ajax/orderPrice",
			data: object,
			dataType:'jsonp',
			jsonp: "callback_price",
			success: function(res){
				if(res.code == 0){
					$ANA.event({
						id: '587334ab39cad021e709c3b1',
						sid: res.id
					});
					alert(res.msg);
				}
				else if(res.code == 1){
					alert('您已经提交过询价信息了!');
				}
			}
		})
	});

	function huodongSetCity(){
		var provinceId = $("#huodong_province").val();
		var url = "/qiche/jdata/getCitys";
		$.getJSON(url+'?callback_getcitys=?',{'provinceId':provinceId}, function (res){
			var html = '';
			$.each(res, function(i, item){
				html += '<option value="'+item.id+'" '+(item.id==city_id?'selected=selected':'')+'>'+item.name+'</option>';
			})
			$("#huodong_city").html(html);
		});

	}
	huodongSetCity();
	$("#huodong_province").change(function(){huodongSetCity();})
</script>
<script type="text/javascript">
	function setCarTypes(brand_id){
		var url="/qiche/hduibi/updateBInfos";
		$.getJSON(url+'?callback_chexi=?',{'BRAND_ID':brand_id}, function (res){
			$("#bTypeInfo").html(res);
		});
	}

	function setCarBrand() {
		var url="/qiche/hduibi/getCarBrands";
		$.getJSON(url+'?callback_pinpai=?',{}, function (res){
			$("#brand").html(res);
		});
	}

	setCarBrand();

	$("#brand").change(function(){
		var brand_id = $(this).val();
		setCarTypes(brand_id);
	})
</script>

<script type="text/javascript">
	//车型对比
	var ava_vm_news = {
		baseurl:"/qiche",
		duibiUrl:'/qiche/duibi/chexing/',
		//车系名称
		typeName:'途昂',
		//选中车型
		cxArr:[],
		//车系id
		currTypeid:'4232',
		//当前车型id
		currCarid:'28248',
		returnCars:[],
		requestParams:{},
		//选择车型
		shoseCarid:function(othis){
			if($(othis).prop('checked')){
				ava_vm_news.cxArr.push($(othis).val());
			}else{
				ava_vm_news.cxArr.pop($(othis).val());
			}
			//console.log(ava_vm_news.cxArr);
		},
		//对比 reload 到对比页
		jumpDuibi:function(){
			if(!ava_vm_news.currCarid){
				return false;
			}
			if(ava_vm_news.cxArr.length == 0){
				alert('没选中对比车型或无对比车型可选!');
				return false;
			}
			var carids = ava_vm_news.cxArr.join(',');
			var params = ava_vm_news.currCarid+','+carids;
			//console.log(params);
			//location = ava_vm_news.duibiUrl+'carids='+params;
			window.open(ava_vm_news.duibiUrl+'carids='+params);
		},
		_setParams:function(){
			ava_vm_news.requestParams.typeId = ava_vm_news.currTypeid;
			ava_vm_news.requestParams.carId = ava_vm_news.currCarid;
		},
		//随机更换对比车型
		changeRandom:function () {
			//判断参数
			if(!ava_vm_news.currTypeid || !ava_vm_news.currCarid){
				return false;
			}
			var url = ava_vm_news.baseurl+'/ajax/getRandCars';
			//设置请求参数
			ava_vm_news._setParams();
			$.ajax({
				url:url,
				type:'GET',
				dataType:'jsonp',
				jsonp:'callback',
				data:ava_vm_news.requestParams,
				success:function(r){
					//console.log(r);
					$('#ava_duibi_list').find('input[type="checkbox"]').prop('checked',false);
					ava_vm_news.cxArr = [];
					ava_vm_news.returnCars = r;
					//拼接html
					ava_vm_news._makeHtml();
				},
				error:function(xhr,errstatus,errtext){
					alert(errstatus+':'+errtext);
				}
			});
		},
		_makeHtml:function () {
			if(ava_vm_news.returnCars.length == 0){
				return false;
			}
			var cars = ava_vm_news.returnCars;
			var html = '';
			for(var i=0;i<cars.length;i++){
				html +='<div class="cf">' +
				'<label class="checkbox-inline"><input onclick="ava_vm_news.shoseCarid(this);" type="checkbox" id="" value="'+cars[i].car_id+'">'+ava_vm_news.typeName+' '+cars[i].car_name+'</label>' +
				'</div>';
			}
			//console.log(html);
			$('#ava_duibi_list').html(html);

		}
	};
</script>

<!-- 倒计时js -->
<script type="text/javascript">
	var times = new Date();
	var timesout = new Date("2017/01/12 23:59:59");
	var timesg = times.getTime();
	var timeoutg = timesout.getTime();
	var timescha = parseFloat(timeoutg) - parseFloat(timesg);


	if (timescha > 0) {
		$('#hdgg').show();
		var days = Math.floor(timescha / 1000 / 60 / 60 / 24);
		var timesmins = timescha % (24 * 3600 * 1000);
		var timeshouer = Math.floor(timesmins / (3600 * 1000));
		var timesmin = Math.floor(timesmins % (3600 * 1000) / (60 * 1000));
		$('.time-day').text(days);
		$('.time-hour').text(timeshouer);
		$('.time-min').text(timesmin);
	}
</script>
<script>
	//下次版本上导航定位时删除此代码
	$(window).load(function(){
		var showEwm = $('#showEwmbg').offset().top;
		// var showEwmWidth = $("#showEwmbg").width();
		var showheight = $("#tunId").height();
		var showLeft = $('.col-showLeft').height();
		var showLeftoffset = $('.col-showLeft').offset().top-70;
		// alert(showLeft);
		var showRight = $('.col-showRight').height();

		var stickyMenu02 = function(){
			var scrollTop = $(document).scrollTop();
			var scrollheight = $(document).height();
			// var bottomHeight=$('#friendLink').height()+$('#footer').height()+75;
			// var elseHeight= scrollheight-bottomHeight;
			if (showLeft < showRight){
				$('#tunId').css({ 'position': 'relative', 'top':0})
			};
			if (scrollTop > showEwm&&showLeft > showRight) {
				$('#tunId').css({'position': 'fixed', 'top':-10});

				if(scrollTop + showheight >showLeftoffset+showLeft){
					$('#tunId').css('top',showLeftoffset+showLeft-scrollTop-showheight+'px');
				};

			} else {
				$('#tunId').css({ 'position': 'relative', 'top':0})
			};
		};
		$(window).scroll(function(){stickyMenu02();});
	});

</script>

