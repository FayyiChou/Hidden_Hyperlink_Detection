<script>
    function applyRealName()
    {

        $("#foot_retmsg").text("");

        if( $("#foot_realName").attr("value") == '' )
        {
            $("#foot_retmsg").show();
            $("#foot_retmsg").text("请输入姓名");
            $("#validdiv").show();
            return false;
        }

        if ( $("#foot_certNo").attr("value") == '' )
        {
            $("#foot_retmsg").show();
            $("#foot_retmsg").text("请输入证件号");
            $("#validdiv").show();
            return false;
        }


        if( $("#foot_moblie").attr("value") == '' )
        {
            $("#foot_retmsg").show();
            $("#foot_retmsg").text("请输入手机号码");
            $("#validdiv").show();
            return false;
        }

        if( $("#validdiv").is(':visible') && $("#foot_txtValidCode").attr("value")== '')
        {
            $("#foot_retmsg").show();
            $("#foot_retmsg").text("请输入验证码");
            $("#validdiv").show();
            return false;
        }

        var certNo = $("#foot_certNo").attr("value");
        var realName = $("#foot_realName").attr("value");
        var mobile =$("#foot_moblie").attr("value");
        var txtValidCode=$("#foot_txtValidCode").attr("value");
        var verify_orderNo = $("#verify_orderNo").attr("value");
        var verify_customerNo = $("#verify_customerNo").attr("value");
        var para={"certNo" : certNo, "realName" : realName, "mobile":mobile, "txtValidCode":txtValidCode,"validCodeKey":"realNameAuthCode","orderNo":verify_orderNo,"customerNo":verify_customerNo};
//    $("#foot_realNameBnt").attr("value","提交中。。。");
//    $("#foot_realNameBnt").attr("disabled","disabled");
//    $("#foot_realNameBnt").attr("class","button-style-03-disable");
        ajaxRequest(wrapUrl("trueNameCheck.htm"), afterApplyRealName, para,'POST', true);
    }

    function afterApplyRealName(result){
        try
        {
            var resultobj = jQuery.parseJSON(result);
            if("true"==resultobj.result){
                // $("#foot_formarea").hide();
                $("#foot_retmsg").hide();
                // $("#closediv").show();
                $("#realNameInputDiv").hide();

                $("#needInputRealName").attr("value", false);
                $('#bankSubmit').removeAttr('disabled');
                $('#bankSubmit').attr('class',"button-style-01");

                $("#validCodeDiv").show();

            }else{
                $("#foot_retmsg").show();
                $("#foot_retmsg").text(resultobj.errorMsg);
                $("#foot_realNameBnt").removeAttr("disabled");
                $("#foot_realNameBnt").attr("class","button-style-03");
                $("#validdiv").show();
                $("#foot_realNameBnt").attr("value","立即认证");
            }
        }catch(err)
        {
            $("#foot_retmsg").show();
            $("#foot_retmsg").text("系统异常");
            $("#foot_realNameBnt").removeAttr("disabled");
            $("#foot_realNameBnt").attr("class","button-style-03");
            $("#validdiv").show();
            $("#foot_realNameBnt").attr("value","立即认证");
        }
    }
</script>
  </div>
</div>


