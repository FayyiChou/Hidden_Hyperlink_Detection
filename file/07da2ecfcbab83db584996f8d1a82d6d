<script>
	
	function dianji(){
		var aid=$('input[name=aid]').val();
		$.ajax({
			url:'/index/index/dianjishu',
			data:{'aid':aid},
			type:'get',
			success:function(data){
			
			}
		})
		
		
	}
	function show(){
		$('#queren').attr('class','');
	}

	function fanhui()
	{
		$('#queren').attr('class','hide');
	}
	
	var zstj=$('input[name=zstj]').val();
	if(zstj=='强制住宿'){
		$('.buy-style ul li').first().addClass('on');
		$('input[name=zsf]').val('zs');
	}else if(zstj=='强制不住宿'){
		$('.buy-style ul li').last().addClass('on');
		$('input[name=zsf]').val('bzs');
	}else if(zstj=='满'){
		$('.xuefei').html($('input[name=zsxf]'));
	}else if(zstj=='否'){
		$('.buy-style ul li').first().addClass('on');
		$('input[name=zsf]').val('zs');
	}else if(zstj=='是'){
		$('.buy-style ul li').first().addClass('on');
		$('input[name=zsf]').val('zs');
		$('.buy-style ul li').on('click',function(){
			if($(this).html()=='不住宿'){
				$('input[name=zsf]').val('bzs');
				$('.xuefei').html($('input[name=xf]').val()-$('input[name=zs]').val());
			}else{
				$('.xuefei').html($('input[name=xf]').val());
				$('input[name=zsf]').val('zs');
			}
			$(this).addClass('on').siblings().removeClass('on');
		})
	}
	
	function queren(){
		$('input[name=jq]').val($('.xuefei').html());
	}
</script>
</body></html>