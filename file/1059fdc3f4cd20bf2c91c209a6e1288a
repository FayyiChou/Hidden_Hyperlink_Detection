<script>
jQuery( '.rightreturn ul li' ).hover(function(){
jQuery( this ).addClass( 'huaguo').siblings().removeClass('huaguo');
if(jQuery(this).attr('id') != 'hotbk'){
jQuery( '.rightreturn ul li.hotbk .textnoted' ).css('display','none');
}else{
jQuery( '.rightreturn ul li.hotbk .textnoted' ).css('display','block');
}
},
function(){
jQuery( '.rightreturn ul li.hotbk .textnoted' ).css('display','none');
jQuery( this ).removeClass('huaguo');
});
</script>
