<script type="text/javascript">
(function() {
// 针对 player.letvcdn.com  *.letv.com 的flash进行特殊处理
jQuery("embed").each(function() {
var src = jQuery(this).attr("src");
var srcInfo = src.split("/");
var srcDomain = srcInfo && srcInfo.length > 3 ? srcInfo[2] : "";
var regex = /^[a-zA-Z0-9\.\-]*?\.letv\.com$/;
if (srcDomain == "player.letvcdn.com" || regex.test(srcDomain)) {
jQuery(this).attr({
allownetworking : "all",
allowscriptaccess : "always",
src : src + "&r=" + Math.random()
});
var parentObj = jQuery(this).parent();
var html = parentObj.html();
parentObj.html(html);
}
});

})();
</script>

<script type="text/javascript">
(function() {
//导航鼠标切换效果
var url = window.location.href;
jQuery(".navlist li").removeClass("a");
var new_url = url.replace(/[http:\/\/[bbs.letv.cn|bbs.letv.com]+\/]??/,"");
if(new_url=="") new_url = "/";
jQuery(".navlist li a").each(function(index){
var href = jQuery(this).attr("href");
var new_href = href.replace(/[http:\/\/[bbs.letv.cn|bbs.letv.com]+\/|\/]??/,"");
if(new_href=="") new_href = "/";
if(new_href==new_url) {
jQuery(".navlist li").eq(index).addClass("a");
}

});
})();
</script>

