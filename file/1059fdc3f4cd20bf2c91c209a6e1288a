<script type="text/javascript">
//赞
jQuery(function($) {
$(".zan").on('click',function(){
var tid = $(this).attr("data-tid");
var formhash = "5a9a597c";
var thisObj = this;
var postdata = {
"inajax" : 1,
"ajaxdata" : "json",
"formhash":formhash,
"tid" : tid,
"praisesubmit" : "yes"
};
jQuery.ajax({
type : "POST",
url : "forum.php?mod=praise",
data : postdata,
dataType : "json",
success : function(result) {
if (result.message == "ok") {
var iconObj = $(thisObj).children('.icon');
var textObj = $(thisObj).children('.times');
var count = $(thisObj).attr("data-count");
if(result.data.oper=="add") {
iconObj.addClass("done");
count++;
} else {
iconObj.removeClass("done");
count--;
}
textObj.text("赞 (" + count + ")");
$(thisObj).attr("data-count", count);
} else if (result.message.indexOf("登录") > -1) {
location.href = 'member.php?mod=logging&action=login';
} else {
alert(result.message);

}
}
});

});
});
</script><div style="clear:both;"></div><div class="footer">

