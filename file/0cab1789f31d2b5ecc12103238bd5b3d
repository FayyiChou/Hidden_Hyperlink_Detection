  <script type="text/javascript">
    //<![CDATA[
    if (typeof vsSliders == 'undefined')
      var vsSliders = [];

    (function($, window, undefined) {
      var arrSliderOptions =
      {
        
        
                jumpSelector: '#vsSlider_promo_unit_home_promo_1_thumbs',
                                delay: 5000,
                height: 446,
        width: 745,
        onSlideStart: function(){if(typeof removeThumbActive == 'function') removeThumbActive()},
        onSlideStop: function(){if(typeof addThumbActive == 'function') addThumbActive()}
              };

      function init_slider()
      {
        if (typeof $('body').data('sliderOptions') == 'undefined')
          var arrSliderCustomOptions = {};
        else
          var arrSliderCustomOptions = $('body').data('sliderOptions');
        vsSliders.push( $("#vsSlider_promo_unit_home_promo_1").find('.vsSlider_slide').removeClass('slider_pre_load').parent().vsSlider($.extend({}, arrSliderOptions, arrSliderCustomOptions)) );
      }

      $(function()
      {
        if(typeof(init_slider) == 'function')
          init_slider();
      });
    })(jQuery, window);
    //]]>
  </script>


