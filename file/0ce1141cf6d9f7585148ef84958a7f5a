	function get( name ) {
            var c = document.cookie;
            if ( !c.length ) {
                return '';
            }
            var tp = c.split( '; ' );
            for ( var i = tp.length - 1; i >= 0; i-- ) {
                var tm = tp[ i ].split( '=' );
                if ( tm.length > 1 && tm[ 0 ] == name && tm[ 1 ] ) {
                    return ( $.trim( tm[ 1 ] ) );
                }
            }
            return '';
        }

$(document).ready(function(){
	if(MS.cookie.get('countMap')!=undefined&&MS.cookie.get('countMap')!=''){
		var countMap= $.parseJSON(decodeURIComponent(MS.cookie.get('countMap')));
		if(countMap != undefined)
		{	
			MS.cookie.set('countMap', '',-1,"msyidai.com" );
			layer.alert(showNumMsg(countMap),function(index){
	    					layer.close(index);
	    				});
	  	}
	}
	if(MS.cookie.get('newUserCoupon')!=undefined&&MS.cookie.get('newUserCoupon')!=''){
		var newUserCoupon= $.parseJSON(decodeURIComponent(get('newUserCoupon')));
		if(newUserCoupon != undefined)
		{
			var info  = getInfo(newUserCoupon.coupMap,newUserCoupon.coupThiMap);
			if(info!=''){
				MS.cookie.set('newUserCoupon', '',-1,"msyidai.com" );
				layer.alert(info,function(index){
	    					layer.close(index);
	    				});
			}
	    	
	 	 }
	}
});

