<script>



var islogin = 1;

window.onload = function(){
	if(islogin==0){
		var cookieObj = {
		        get : function( name ) {
		            var c = document.cookie;
		            if ( !c.length ) {
		                return '';
		            }
		            var tp = c.split( '; ' );
		            for ( var i = tp.length - 1; i >= 0; i-- ) {
		                var tm = tp[ i ].split( '=' );
		                if ( tm.length > 1 && tm[ 0 ] == name && tm[ 1 ] ) {
		                    return unescape( $.trim( tm[ 1 ] ) );
		                }
		            }
		            return '';
		        },
		        set : function( name, value, day, domain, usehost ) {
		            day = day || 365, domain = domain || '.msyidai.com', usehost = usehost || 0;
		            var expires = new Date();
		            expires.setTime( (new Date()).getTime() + 3600 * 24 * 1000 * day );
		            document.cookie = name + "=" + escape( value ) + "; path=/; " + (usehost ? 'host' : 'domain') + "=" + domain + (day == -1 ? '' : ";expires=" + expires.toGMTString());
		        },
		        del : function( name ) {
		            this.set( name, '', -365, null, 0 );
		            this.set( name, '', -365, document.location.host, 1 );
		        }
		    };

		var __guideCookie = cookieObj.get( '__guide' );
		if( !__guideCookie || __guideCookie != 1 ){
			//slingWindow();
		}else{
			var sl = getQueryString('sl');
			if(sl==1){
				//slingWindow();
			}
		}

		//吊起浮窗
		function slingWindow(){
			var __info = $('#__info'), __shadow = $('#__shadow'), __jsWrap = $('#js_wrap');
			var tpl = [
				'<div class="wrap_sling" id="js_wrap">',
					'<a href="javascript:;" id="js_cosBtn"></a>',
					'<div class="tit"><i></i>金币投资抵现金，惊喜不断！</div>',
					'<div class="text">民生易贷回馈新老用户，投资人注册、投资、邀请好友等均可获得金币奖励。您在民生易贷投资时可使用金币抵扣现金，1金币抵扣1元人民币。</div>',
					'<a class="see-more" href="http://www.msyidai.com//toNoviceGuidePage?act=2&tag=6">查看更多活动规则&gt;&gt;</a>',
				'</div>'
			].join('');
			//判断dom节点是否存在
			function isTrue(ele, idN){
				if( !ele.length ){
					var html = ( idN == '__info' ) ? tpl : '';
					ele = $('<div id="'+ idN +'">'+html+'</div>');
					ele.appendTo( 'body' );
					cookieObj.set( '__guide', 1 );
				}else{
					ele.show();
				}
			}
			//移除dom节点
			function removeFun(){
				$('#__info').remove();
				$('#__shadow').remove();
			}
			isTrue( __info, '__info' );
			isTrue( __shadow, '__shadow' );
			//点击关闭按钮
			$('#js_cosBtn').length && $('#js_cosBtn').off( 'click', removeFun ).on( 'click', removeFun );
		}
		}
}
