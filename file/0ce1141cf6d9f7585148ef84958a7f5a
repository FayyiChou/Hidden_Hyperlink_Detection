$(function(){
	$(".sn-menu").hover(function(){
		$(".pop").show();
	},function(){
		$(".pop").hide();
	});
	
//	$('img').each(function(){
//		if($(this).attr("ydSrc")){
//			$(this).attr("src",YD.stateSrc+$(this).attr("ydSrc")+"?v="+YD.version);
//		}
//	});
//	$('a').each(function(){
//		if($(this).attr("ydhref")){
//			$(this).attr("href",YD.appSrc+"/"+$(this).attr("ydhref"));
//		}
//	});
	var qq1 = "1915644236";
	var qq2 = "1494774867";
	var onlineClass='qqtalk';
	var notOnlineClass='qqtalk1';
	var a1 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq1+'&site=qq&menu=yes" onclick="to_hmt()"  target="_blank" class="'+onlineClass+'"></a>';
	var a2 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq2+'&site=qq&menu=yes" onclick="to_hmt()"  target="_blank" class="'+onlineClass+'"></a>';
	$('#qqtalk_div').html(a1+a2);
	var isAllonline = $.cookies.get( 'isAllonline' );
	if(isAllonline != 1){
		$.ajax({
			url:address+"getQQOnline",
			type:'post',
			data:{'qqs':qq1+':'+qq2+':'},
			success:function(data){
				if(data!=null && data.re == 0){
					var a11 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq1+'&site=qq&menu=yes" onclick="to_hmt()"  target="_blank" class="'+onlineClass+'"></a>';
					var a22 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq2+'&site=qq&menu=yes" onclick="to_hmt()" target="_blank" class="'+onlineClass+'"></a>';
					var boo = false; 
					if(data[qq1]==1){
						a11 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq1+'&site=qq&menu=yes" onclick="to_hmt()"  target="_blank" class="'+onlineClass+'"></a>';
						boo = true;
					}else{
						a11 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq1+'&site=qq&menu=yes" onclick="to_hmt()" target="_blank" class="'+notOnlineClass+'"></a>';
					}
					var boo1 = false;
					if(data[qq2]==1){
						a22 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq2+'&site=qq&menu=yes" onclick="to_hmt()"  target="_blank" class="'+onlineClass+'"></a>';
						boo1 = true;
					}else{
						a22 = '<a href="http://wpa.qq.com/msgrd?v=3&uin='+qq2+'&site=qq&menu=yes" onclick="to_hmt()" target="_blank" class="'+notOnlineClass+'"></a>';
					}
					if(boo && boo1){
						$.cookies.set( 'isAllonline', '1' );
					}
					$('#qqtalk_div').html(a11+a22);
				}
		    },error:function(XMLHttpRequest, textStatus, errorThrown){
		    	//console.log("通信失败！");
		    } 
	    });
	}
});
