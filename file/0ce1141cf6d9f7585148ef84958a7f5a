<script>
	var isloginforsso=false;
	var nickName="";
	var loginEvn="online"
	setTimeout('checkIsLogin()',1000); 
	function checkIsLogin(){
		if(loginEvn=='dev'){return;}
		$.ajax({ 
			type:"POST",
			url:"/getUserStatus",
			dataType:"JSON",
			isRefToken:false,
			success: function(res) {
				var login = res.sucess;
				var b=login==isloginforsso;
				if(!b){
					 location.reload();
				}else{
					if(isloginforsso){
						$.ajax({
							type:"POST",
							url:"/getNickName",
							dataType:"JSON",
							isRefToken:false,
							success: function(res) {
								if(nickName!=""&&res.nickName!=nickName){
									 location.reload();
								}
							}
						});
					}
				}
			}
		});
		
	}
    function chooseRole(){
        if($("#registerRole").css("display")=="block"){
            $("#registerRole").hide();
        }else if($("#registerRole").css("display")=="none"){
            $("#registerRole").show();
        }
    }
    function closeChoose(){
        $('#registerRole').hide();
    }
    $(function() {
        $("#registerRole").mouseleave(function () {
            $(this).hide("fast");
        });
        // 设置head链接聚焦问题
        var flag = '0';
        var arrays = $(".h_nav li");
        for(var i=0; i<arrays.length; i++) {
            if(i == flag*1) {
                $(arrays[i]).attr("class","current");
                break;
            }
        }
        if('' !=''){
        	getMsgCount();
            setInterval("getMsgCount()", 60000);
        }
    });
	function getMsgCount(){
		$.ajax({
			type:"POST",
			url:"http://www.msyidai.com/updateMsgNotRCAndReturnAjax",
			dataType:"JSON",
			isRefToken:false,
			success: function(res) {
				var count = res.msgNotRCCount;
				if (count != null && count != "" && count != "0") {
					document.cookie="msgCount="+count; 
					$("#span_count").html(count).show();
				}else{
					$("#span_count").hide();
				}
			}
		});
	}
</script>
