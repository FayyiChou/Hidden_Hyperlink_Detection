    function ask() {

        var topic = $("#__Topic").val();
        if (notlogin()) {
            showLoginBox(undefined, '/question/gimq/ask?__Topic=' + topic);
            return;
        }
        $('#SearchQuestionForm')[0].action = '/question/gimq/ask'
        $('#SearchQuestionForm').submit();
    }

