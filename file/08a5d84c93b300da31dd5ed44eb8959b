    <script type="text/javascript">
      gUserID = '';
      $(function(){
          $(".divImg img,.formatRemark2 img").each(function() {
            if ($(this).width() > 650)
                $(this).width("650px");
            });
      });
         //解决由于页面加载flash，导致点击锚点改变页面标签title的值的bug
        var originalTitle = document.title.split("#")[0];
        if (window.document.attachEvent) {     //在ie浏览器情况下添加事件
            window.document.attachEvent('onpropertychange', function (evt) {
                // document.getElementById("answer_editor").attachEvent('onpropertychange', function (evt) {
                evt = evt || window.event;
                if(evt.propertyName === 'title' && document.title !== originalTitle) {
                    setTimeout(function () {
                        document.title = originalTitle;
                    }, 1);
                }
            })
        } else if (window.document.addEventListener) {    //在其他浏览器情况下添加事件
            window.document.addEventListener('onpropertychange', function (evt) {
                // document.getElementById("answer_editor").attachEvent('onpropertychange', function (evt) {
                evt = evt || window.event;
                if(evt.propertyName === 'title' && document.title !== originalTitle) {
                    setTimeout(function () {
                        document.title = originalTitle;
                    }, 1);
                }
            })
        }
        //结束
    </script>
    <script>
    var curtoday = new Date();
    $(document).ready(function(){
        if( gUserID =="undefined" || typeof(gUserID) =="undefined" || gUserID < 0 || !gUserID)
        {
            $(".dayimaopao").hide();
        }
        else
        {
            $.ajax({
                type: "POST",
                url: "/question/giml/get_adopt_count",
                cache:false,
//                data:{
//                  count:,
//                  op_date: curtoday.getTime()
//                },
                success: backCallget_adopt_count
            });
        }
    });
    function backCallget_adopt_count(msg)
    {
        var adopt_info;
        if(typeof(msg.count) !="undefined")
        {
            adopt_info = msg;
        }
        else
        {
            adopt_info = eval('(' + msg + ')');
        }
        var relate_count = adopt_info.max_count-adopt_info.count;
        var relate_html = "";
        if(relate_count ==0)
        {
            relate_html = "你被采纳的问题数排在第一！"
        }
        else
        {
            relate_html = "距离第一名还有<span style=\"color:red;\">" + relate_count + "</span>个问题！";
        }
        var bubble_content_a = "<a href='http://new.fwxgx.com/users/my_task' target='_blank'>点击查看</a>";
        if(adopt_info.count<=0)
        {
            $("#bubble_content").html("每天被采纳<em> " + adopt_info.left_count + " </em>个问题，额外赠送<em> " + adopt_info.point + " </em>贡献分," + bubble_content_a + "！");
            $("#bubble_content").show();
            $("#paiming").html(relate_html)
            $("#paiming").show();
        }
        else if(adopt_info.count<50)
        {
            $("#bubble_content").html("今天再有<em> " + adopt_info.left_count + " </em>个问题被采纳，奖励贡献分<em> " + adopt_info.point + " </em>分," + bubble_content_a + "！");
            $("#bubble_content").show();
            $("#paiming").html(relate_html)
            $("#paiming").show();
        }

        if(adopt_info.count >= 50)
        {
            $("#bubble_content").html("今天的日常答疑任务都已经完成！" + bubble_content_a);
            $("#bubble_content").show();
            $("#paiming").html(relate_html)
            $("#paiming").show();
        }
        else
        {
            $(".dayimaopao").show();
            $(".dayimaopao_tip").click(function(){
                if ($(".dayimaopao_content").is(":visible")) {
                    $(".dayimaopao_content").hide();
                } else{
                    $(".dayimaopao_content").show();
                };
            });
            var cur_date = curtoday.getFullYear() + "" + curtoday.getMonth() + "" + curtoday.getDate();
            if(GetCookie('bubble_show_flag')==cur_date)
            {
                $(".dayimaopao_content").hide();
            }
            else
            {
                $(".dayimaopao_content").show();
            }
            $(".dayimaopao i").click(function(){
                $(".dayimaopao_content").hide();
                SetCookie('bubble_show_flag', cur_date,1,'/question');
            });
        }
    }
</script>
