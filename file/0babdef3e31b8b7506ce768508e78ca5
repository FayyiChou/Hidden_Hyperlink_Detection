　function Marquee(){ 
　　if(demo.scrollLeft>=demo1.scrollWidth){ 
　　　demo.scrollLeft=0 
　　}else{ 
　　　demo.scrollLeft++ 
　　}
　} 
　var MyMar=setInterval(Marquee,speed) 
