	<script>
	//var apiUrl = 'http://api.mxtoolbox.com/api/v1/lookup/blacklist/';
//	var apiUrl = 'http://api.mxtoolbox.com/api/v1/lookup/blacklist/example.com?port=80';
//	var apiKey = 'fb458259-b745-4fa2-82fc-e0bb92fbab31';
	　function search_blacklist(){
    	var apiUrl = '/rbl/query.php?ip=';
			var html='';
			var __keys=$('#keys').val();
			$('#serstr').html(__keys);
			$('#s_content').html('');
			$('#s_list').html('');
			$('#imgWait3').show();
			apiUrl=apiUrl+__keys;
			$.ajax({ 
			url: apiUrl, 
			type: 'GET', 
//			dataType: 'json',
			dataType: 'text',  
			success: function (result) { 
				$('#imgWait3').hide();
        $('#s_content').html(result); 
				//Show Formatted JSON 
				//$('#s_list').html(JSON.stringify(result, null, 4)); 
//				$('#s_content').html('<p>checking <b>'+$('#keys').val()+'</b> which resolves to <b>'+result.CommandArgument+'</b> against <b>'+result.Failed.length+'</b> kown blacklist...<br>Listed <b>0</b> times width <b>3</b> timeouts </p><div class="org-circle"><span class="text">MxRep</span><div class="font22">100</div></div>');
				if(result.Failed.length>0){
				$(result.Failed).each(function(i, n) {
								html+='<tr><td><i class="checkGreen"></i> ok</td><td>'+n.Name+'</td><td>&nbsp;</td><td>'+n.BlacklistTTL+'</td><td>'+n.BlacklistResponseTime+'</td><td></td></tr>';
							});
				}else{
					html+='<tr><td colspan="6">暂无数据</td></tr>';
				}
				$('#s_list').html(html);
				}, 
				error: function (xhr, status, error) { 
					$('#s_content').html(status + ' - ' + error); 
				/*beforeSend: function (xhr) { 
					if (apiKey) { 
						xhr.setRequestHeader('Authorization', apiKey); 
					} 
					}*/
				} 
			});
	};
