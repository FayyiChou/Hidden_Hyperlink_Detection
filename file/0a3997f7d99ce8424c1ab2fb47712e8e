$('#form_btn').on('click',function(){
			var _domain=$('#tryout input[name=domain]').val();
			var _email=$('#tryout input[name=email]').val();
			var _linkman=$('#tryout input[name=linkman]').val();
			var _ip=$('#tryout input[name=ip]').val();
			if(_domain=='域名' || _linkman=='联系人' || _email=='企业邮箱' || _domain=='邮件服务器IP'){
				alert('请填写必填的信息，O(∩_∩)O谢谢');
				return;
			}
			$.ajax({
				 type:"POST",
				 url:'/ajax/tryout',
				 data:'domain='+_domain+'&email='+_email+'&linkman='+_linkman+'&ip='+_ip,
				 success:function(data){
						if(data==1){
							alert('感谢您提交信息，现在我们将为您的IP提供黑名单监测服务，并及时发送IP监控邮件到您的邮箱里。谢谢支持！');
							$('#tryout input[name=domain]').val('');
							$('#tryout input[name=email]').val('');
							$('#tryout input[name=linkman]').val('');
							$('#tryout input[name=ip]').val('');
						}else if(data==-1){
							alert('请填写必填的信息，O(∩_∩)O谢谢');
						}else{
							alert('咨询提交失败！请稍后尝试.');
						}
				 },
				timeout:5000,
				error:function(){
					alert('请求超时，请重试！');
				}
		 });	
	});
	</script>


  </body>
</html>