<script type="text/javascript">
$(function(){
$('.wrap_video_list .list_video:last').css({border:0,'padding-bottom':0});
$('.product_category table:last').css({border:0});

$('.topic a').mouseover(function(){
var pid=$(this).parent().attr('id');
if(typeof(pid)=="undefined") return false;
var name =pid.split('_')[1];
var j = $(this).parent().find('a').length;
if(j==0) return false;
for(i=0;i<j;i++) {
$('#'+name+'_'+(i+1)).hide();
}
$(this).parent().find('a').removeClass('on');
$(this).addClass('on');
var nowid = $(this).parent().find('a').index($(this));
$('#'+name+'_'+(nowid+1)).show();
})

/*$('#example2').bxCarousel({
display_num: 3, 
move: 1, 
auto: true, 
controls: false,
margin: 10,
auto_hover: true
});*/
})
</script>
