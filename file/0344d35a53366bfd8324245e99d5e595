<script language="JavaScript" type="text/javascript">
        var totalTime = 10;
        var newLocation = "http://www.tsyzh.com/";
        function ShowTime()
        {
            var obj = document.getElementById("ShowTimeDiv");
            if ( totalTime > 0 )
            {
                obj.innerHTML = totalTime;
                totalTime --;
            }
            else
            {
                location.href= newLocation;
            }
        }
        setInterval("ShowTime()",1000);
    </script>

