$(".layer1").hover
(
  function()
  {
    if($(this).find(".layer2 li").length > 0)
    {
      $(this).find(".layer2").stop().show();

    }
    $(this).addClass("change");
  },
  function()
  {
    $(this).find(".layer2").stop().hide();
    $(this).removeClass("change");
  }
);
