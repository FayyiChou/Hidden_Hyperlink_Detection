<script type="text/javascript" language="javascript">
    function follow(){
        $('#blog_tc,#divTipsPop').css('position', 'absolute');
		$(window).scroll(function(){
		    var top = (document.documentElement.scrollTop+document.documentElement.clientHeight-$("#blog_tc")[0].offsetHeight);
		    var left = (document.documentElement.scrollLeft+document.documentElement.clientWidth-$("#blog_tc")[0].offsetWidth);
			$('#blog_tc').css( 'top' , top+"px");
			$('#blog_tc').css('left', left+"px");

			$('#divTipsPop').css('top', (top - 100) + "px");
		});
    }

    $(document).ready(function () {
        //只有ie6需要js动态改变高度
        $("#divTipsPop").css("bottom", "222px");
        
        if ($("#haoetv").css("display") == "block") {
            $("#blog_tc").css("bottom", "190px");
        }
        if ($.browser.msie && $.browser.version == 6)
            follow();
    });

</script>


