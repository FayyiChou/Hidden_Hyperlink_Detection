	<script type="text/javascript">
    $(document).ready(function () {
        var btnCol = $(".searchBtn a");
        if (btnCol.attr("href") != undefined) {
            btnCol.attr("href", "javascript:onFullTextSearchLinkClick();");
        }
    });
    function onFullTextSearchLinkClick() { 
        var searchTextInput = document.getElementById("ess_ctr126388_ListC_Info_ctl00_KEYWORDS");
        if (searchTextInput.value != "" && searchTextInput.value != "请输入搜索关键字")
            window.open("http://search.customs.gov.cn/dig/search.action?ty=&dr=true&w=true&f=&rp=szns&sr=score+desc&lmq=false&q=" + encodeURI(searchTextInput.value));
        else
            window.open("http://search.customs.gov.cn/dig/search.action?ty=&dr=true&w=true&f=&rp=szns&sr=score+desc&lmq=false&q=");
    } 

    document.onkeydown = function (e) {
        e = e ? e : window.event;
        if (e.keyCode == 13) {
            onFullTextSearchLinkClick();
            return;
        }
    }

</script>
