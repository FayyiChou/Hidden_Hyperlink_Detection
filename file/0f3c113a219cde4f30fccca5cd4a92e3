jQuery(function(){
    var url=location.href;
    if(url.indexOf("id=")>-1){    
         var param=url.substring(url.indexOf("id=")+3);
         var paramNum=param.substring(param.indexOf("NTit")+4,param.indexOf("_con"));
         var obj=jQuery("#"+param);
         jQuery(".con_box").css("display","none");
         obj.css("display","block");
         jQuery("#NTit01").parent().addClass("navTitle"+paramNum);
          jQuery("#bgDiv").hide();  jQuery("#msgDiv").hide();
    }
});
