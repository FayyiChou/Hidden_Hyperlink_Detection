<!-- Start_Module_139577 --><table id="ess_ctr139577_ModuleContent" class="ModCInfoC" cellpadding="0" cellspacing="0" border="0" width="100%"><tbody><tr><td>
	<script type="text/javascript">
    jQuery.noConflict();
    jQuery(function () {
        var timeId;
        jQuery(".hg_r ul").mouseover(function () {
            var index = jQuery(this).index();
            jQuery(this).addClass("on").siblings().removeClass("on");
            jQuery(".hg_l ul").eq(index).fadeIn("slow").siblings().hide();
            clearTimeout(timeId);
            timeId = setTimeout("fun(" + index + ")", 5000);
        })
        timeId = setTimeout("fun('0')", 5000);
        for (var i = 4; i < 8; i++) {
            jQuery(".hg_r ul").eq(i).hide()
        }
    })
    function fun(index) {
        index = index == 7 ? 0 : index + 1;
        if (index == 0) {
            for (var i = 0; i < 4; i++) {
                jQuery(".hg_r ul").eq(i).slideDown("slow");
            }
            for (var i = 4; i < 8; i++) {
                jQuery(".hg_r ul").eq(i).slideUp("slow");
            }
        }
        if (index == 4) {
            for (var i = 0; i < 4; i++) {
                jQuery(".hg_r ul").eq(i).slideUp("slow");
            }
            for (var i = 4; i < 8; i++) {
                jQuery(".hg_r ul").eq(i).slideDown("slow");
            }
        }


        jQuery(".hg_r ul").eq(index).mouseover();
    }
    </script>
