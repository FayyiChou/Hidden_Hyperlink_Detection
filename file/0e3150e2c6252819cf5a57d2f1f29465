<script language="javascript" type="text/javascript">
var time_1 = 500; 
var h_1 = 0; 
function init_sh(){
time_1 = 500;
h_1 = 0;
}
function init_hi(){
time_1 = 500;
h_1 = 100;
}
function show_lo(tc_1){ 
time_1--; 
h_1 = h_1+20; 
if(h_1>100){
document.getElementById('navi_sh_img').style.display = '';
document.getElementById(tc_1).style.height = "100px"; 
return; 
} 
document.getElementById(tc_1).style.display = ''; 
document.getElementById(tc_1).style.height = h_1+"px"; 
setTimeout("show_lo('"+tc_1+"')",2); 
}
function hide_lo(tc_1) { 
time_1--; 
h_1 = h_1-20;
if(h_1<0)
{
document.getElementById('navi_sh_img').style.display = '';
document.getElementById(tc_1).style.display = "none"; 
document.getElementById(tc_1).style.height = "0px"; 
return; 
} 
document.getElementById(tc_1).style.display = ''; 
document.getElementById(tc_1).style.height = h_1+"px"; 
setTimeout("hide_lo('"+tc_1+"')",2); 
}
setTimeout(function(){
if(document.getElementById('navi_sh_img').style.display == ''){
init_hi();
hide_lo('navi_to');
}
}, 99999999);
</script>
