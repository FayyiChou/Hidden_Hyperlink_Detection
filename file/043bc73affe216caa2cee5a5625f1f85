//获取Flash对象
function get_flash_obj(movieName){
	if(window.document[movieName]){return window.document[movieName];}
	if(navigator.appName.indexOf("Microsoft Internet")==-1){if (document.embeds && document.embeds[movieName]){return document.embeds[movieName];}}else{return document.getElementById(movieName);}
}

//同系列游戏滚动处理
function roll_space_init(){
	if($("#game_related_game .roll_box a").length<8){return false;}
	var remainder=$("#game_related_game .roll_box a").length%6;
	if(remainder>0){$("#game_related_game .roll a:lt("+(6-remainder)+")").clone().css("visibility","hidden").insertAfter("#game_related_game .roll a:last");}
	$("#game_related_game .roll_box").width(630);
	$("#game_related_game .go_left,#game_related_game .go_right").show();
	$("#game_related_game .go_left div,#game_related_game .go_right div").hover(function(){
		$(this).addClass("active");
	},function(){
		$(this).removeClass("active");
	});
	$("#game_related_game .go_left div").click(function(){
		$("#game_related_game .roll a:gt("+($("#game_related_game .roll_box a").length-7)+")").insertBefore("#game_related_game .roll a:first");
		$("#game_related_game .roll").css("left","-630px").stop(true,true).animate({"left":"0"},700);
		return false;
	});

	$("#game_related_game .go_right div").click(function(){
		$("#game_related_game .roll").stop(true,true).animate({"left":"-630px"},700,function(){
			$("#game_related_game .roll a:lt(6)").insertAfter("#game_related_game .roll a:last");
			$("#game_related_game .roll").css("left","0");
		});
		return false;
	});
}

//关灯处理函数
function light_switch(){
	var s_obj=$("#light_switch");
	var b_obj=$("#game_play_box .middle");
	if($("#full_screen_black_wall:visible").length){
		$("#full_screen_black_wall").hide();
		s_obj.removeClass("active").removeClass("high_level").find("strong").html("关灯");
		$("html").css({"overflow":"auto"});
		$("html").css({"margin-right":"auto"});
		$(window).unbind("keydown");
	}else{
		sr_left=s_obj.offset().left-b_obj.offset().left;
		sr_top=s_obj.offset().top-b_obj.offset().top;
		var now_width=document.documentElement.clientWidth;
		if(navigator.appName==="Microsoft Internet Explorer"){$("html").css({"overflow":"hidden"})};
		$("html").css({"margin-right":document.documentElement.clientWidth-now_width});
		$("#full_screen_black_wall").show().css({"filter":"alpha(opacity=90)","opacity":"0.9","width":document.documentElement.clientWidth>document.documentElement.scrollWidth?document.documentElement.clientWidth:document.documentElement.scrollWidth,"height":document.documentElement.clientHeight>document.documentElement.scrollHeight?document.documentElement.clientHeight:document.documentElement.scrollHeight});
		s_obj.addClass("active").addClass("high_level").css({"left":Math.round(b_obj.offset().left+sr_left),"top":Math.round(b_obj.offset().top+sr_top)}).find("strong").html("开灯");
		$(window).keydown(function(e){
			if(e.keyCode==27){
				light_switch();
			}
		});
		$(window).one("resize",first_reset_light_off);
	}
}

function first_reset_light_off(){
	window.setTimeout(second_reset_light_off,0);
}
function second_reset_light_off(){
	var s_obj=$("#light_switch");
	var b_obj=$("#game_play_box .middle");
	if($("#full_screen_black_wall:visible").length){
		$("#full_screen_black_wall").hide().css({"width":document.documentElement.clientWidth>document.documentElement.scrollWidth?document.documentElement.clientWidth:document.documentElement.scrollWidth,"height":document.documentElement.clientHeight>document.documentElement.scrollHeight?document.documentElement.clientHeight:document.documentElement.scrollHeight}).show();
		s_obj.css({"left":Math.round(b_obj.offset().left+sr_left),"top":Math.round(b_obj.offset().top+sr_top)});
		$(window).one("resize",first_reset_light_off);
	}
}

//评分层hover判断
function score_layer_hide(e,obj1,obj2){
	if((e.pageX<obj1.offset().left||e.pageX>=(obj1.offset().left+obj1.outerWidth())||e.pageY<obj1.offset().top||e.pageY>=(obj1.offset().top+obj1.outerHeight()))&&(e.pageX<obj2.offset().left||e.pageX>=(obj2.offset().left+obj2.outerWidth())||e.pageY<obj2.offset().top||e.pageY>=(obj2.offset().top+obj2.outerHeight()))){
		obj1.removeClass("score_active");
		obj2.hide();
		$("#game_play_box .bottom").removeClass("bottom_relative");
	}
}

//评分层初始化函数
function score_layer_init(obj1,obj2){
	obj1.hover(function(e){
		obj1.addClass("score_active");
		$("#game_play_box .bottom").addClass("bottom_relative");
		obj2.show().unbind().hover(function(e){
		},function(e){
			score_layer_hide(e,obj1,obj2);
		});
	},function(e){
		score_layer_hide(e,obj1,obj2);
	});
}

function flash_cover_init(){
	if($("#flash_cover_box").length){
		$("#flash_cover_box .cover_bar").css("visibility","visible");
		$("#flash_cover_box .cover_bar span").html(cover_play_time);
		cover_play_timer=window.setInterval(flash_cover_show,1000);
	}
}

//flash广告遮盖显示及完成处理
function flash_cover_show(){
	if(cover_play_time>1){
		$("#flash_cover_box .cover_bar span").html(--cover_play_time);
	}else{
		window.clearInterval(cover_play_timer);
		cover_play_ok=true;
		$("#flash_cover_box").remove();
		if(flash_load_ok){$("#game_play_box .bottom").show();}
	}
}

//flash加载进度显示及完成后处理
function flash_loading_show(){
	var s=get_flash_obj("flash_loading_cover_obj");
	var p=get_flash_obj("flash_play_obj").PercentLoaded();
	try{
		s.SetPos(Math.abs(p));
		$("#flash_play_box .progress_bar").remove();
	}catch(e){
		if($("#flash_play_box .progress_bar").length){
			$("#flash_play_box .progress_bar").html(Math.abs(p)+"%");
		}else{
			if(cover_play_ok){
				$("<div class=\"progress_bar\">"+Math.abs(p)+"%</div>").appendTo($("#flash_play_box"));
				$("#flash_play_box .progress_bar").css({"left":$("#flash_play_box").outerWidth()/2-50,"top":$("#flash_play_box").outerHeight()/2+42}).show();
			}
		}
	};
	if(p==100 || p<0){
		flash_load_ok=true;
		window.clearInterval(game_loading_timer);
		window.setTimeout(function(){
			if($("#flash_play_box .progress_bar").length){$("#flash_play_box .progress_bar").remove()}
			$("#flash_loading_cover_obj").remove();
			$("#game_play_box .middle .alarm").remove();
			$("#game_play_box .middle .remind").removeClass("remind_hide");
			game_recommend_box_reset();
			if(cover_play_ok){$("#game_play_box .bottom").show();}
		},1000);
	}
}

//进度汇报函数
function progress_report(){
	window.setTimeout(function(){
		progress_scan(get_flash_obj("flash_play_obj").PercentLoaded());
	},60000);
}

//进度检查处理函数
function progress_scan(number){
	if(!flash_load_ok){
		if(number==0){
			$.getScript("http://track.doyo.cn/flash_loading_statistics?gid="+game_id+"&type=0");
		}else if(flash_file_size<1048576||number<104857600/flash_file_size){
			$.getScript("http://track.doyo.cn/flash_loading_statistics?gid="+game_id+"&type=1");
		}
	}
}

//flash播放框初始化
function flash_play_box_init(){
	game_recommend_box_init();
	if(flash_show_width>=910){
		flash_show_height=Math.round(flash_show_height*910/flash_show_width);
		flash_show_width=910;
		$("#game_play_box .bottom a.amplification").click();
		zoom_level=0;
	}else if(flash_show_width<=300){
		flash_show_height=Math.round(flash_show_height*300/flash_show_width);
		flash_show_width=300;
		$("#game_play_box .bottom a.narrow").click();
		zoom_level=0;
	}
	$("#flash_play_box_space").show();
	$("#flash_play_box_space,#flash_play_box,#flash_loading_cover_obj,#flash_loading_cover_obj embed").css({"width":flash_show_width,"height":flash_show_height});
	$("#flash_play_box").addClass("high_level").css({"top":Math.round($("#flash_play_box_space").offset().top),"left":Math.round($("#flash_play_box_space").offset().left)});
	game_recommend_box_reset();
	if(need_iframe){
		$("#flash_play_iframe").css({"width":flash_show_width,"height":flash_show_height});
		document.getElementById("flash_play_iframe").contentWindow.flash_change_size(flash_show_width,flash_show_height);
	}else{
		$("#flash_play_obj,#flash_play_obj embed").css({"width":flash_show_width,"height":flash_show_height});
		game_loading_timer=window.setInterval(flash_loading_show,300);
	};
	$(window).one("resize",first_reset_flash_play_box);
}

//右侧推荐初始化
function game_recommend_box_init(){
	$("#game_recommend_box .title .label").noker_labelChange({s:$("#game_recommend_box .content .list"),a:"mouseover",c:"label_active",b:0});
	$("#game_recommend_box .title .label").hover(function(){
		game_recommend_list_lazyimg();
	},function(){});

	$("#game_recommend_box .content .explain_list").html($("#game_explain .content").html());
	$("#game_recommend_box .content .hot_list .control .point").hover(function(){
		if(!$(this).hasClass("point_active")){
			$(this).addClass("point_active").siblings(".point").removeClass("point_active");
			$("#game_recommend_box .content .hot_list .game_list").eq($("#game_recommend_box .content .hot_list .control .point").index(this)).show().siblings(".game_list").hide();
			game_recommend_list_lazyimg();
		}
	},function(){});
	$("#game_recommend_box .content .hot_list .control .go_left").click(function(){
		var obj=$("#game_recommend_box .content .hot_list .game_list")
		var i=obj.index($("#game_recommend_box .content .hot_list .game_list:visible"));
		var l=obj.length;
		if(i==0){i=l-1;}else{i--;}
		obj.eq(i).show().siblings(".game_list").hide();
		$("#game_recommend_box .content .hot_list .control .point").removeClass("point_active").eq(i).addClass("point_active");
		game_recommend_list_lazyimg();
		return false;
	});
	$("#game_recommend_box .content .hot_list .control .go_right").click(function(){
		var obj=$("#game_recommend_box .content .hot_list .game_list")
		var i=obj.index($("#game_recommend_box .content .hot_list .game_list:visible"));
		var l=obj.length;
		if(i==(l-1)){i=0;}else{i++;}
		obj.eq(i).show().siblings(".game_list").hide();
		$("#game_recommend_box .content .hot_list .control .point").removeClass("point_active").eq(i).addClass("point_active");
		game_recommend_list_lazyimg();
		return false;
	});
	$("#game_recommend_box .content .related_list .control .point").hover(function(){
		if(!$(this).hasClass("point_active")){
			$(this).addClass("point_active").siblings(".point").removeClass("point_active");
			$("#game_recommend_box .content .related_list .game_list").eq($("#game_recommend_box .content .related_list .control .point").index(this)).show().siblings(".game_list").hide();
			game_recommend_list_lazyimg();
		}
	},function(){});
	$("#game_recommend_box .content .related_list .control .go_left").click(function(){
		var obj=$("#game_recommend_box .content .related_list .game_list")
		var i=obj.index($("#game_recommend_box .content .related_list .game_list:visible"));
		var l=obj.length;
		if(i==0){i=l-1;}else{i--;}
		obj.eq(i).show().siblings(".game_list").hide();
		$("#game_recommend_box .content .related_list .control .point").removeClass("point_active").eq(i).addClass("point_active");
		game_recommend_list_lazyimg();
		return false;
	});
	$("#game_recommend_box .content .related_list .control .go_right").click(function(){
		var obj=$("#game_recommend_box .content .related_list .game_list")
		var i=obj.index($("#game_recommend_box .content .related_list .game_list:visible"));
		var l=obj.length;
		if(i==(l-1)){i=0;}else{i++;}
		obj.eq(i).show().siblings(".game_list").hide();
		$("#game_recommend_box .content .related_list .control .point").removeClass("point_active").eq(i).addClass("point_active");
		game_recommend_list_lazyimg();
		return false;
	});
}

//右侧推荐重置
function game_recommend_box_reset(num){
	var w=$("#flash_play_box_space").width();
	var h=$("#flash_play_box_space").height();
	var wn=0;
	var hn=0;
	if(w<=580){
		$("#game_recommend_box").show().width(320);
		$("#game_recommend_box .title").removeClass("two").addClass("three");
		$("#game_recommend_box .content").removeClass("two").addClass("three");
		$("#game_recommend_box .title .explain").show();
		wn=3;
	}else if(w<=670){
		$("#game_recommend_box").show().width(230);
		$("#game_recommend_box .title").removeClass("three").addClass("two");
		$("#game_recommend_box .content").removeClass("three").addClass("two");
		$("#game_recommend_box .title .explain").hide();
		wn=2;
	}else{
		$("#game_recommend_box").hide();
		$("#game_recommend_box .title").removeClass("two").removeClass("three");
		$("#game_recommend_box .content").removeClass("two").removeClass("three");
		wn=0;
	}

	if($("#game_play_box .middle .alarm").length){
		if(h<442){
			$("#game_recommend_box").height(500);
		}else{
			$("#game_recommend_box").height(h+58);
		}
	}else{
		if(h<462){
			$("#game_recommend_box").height(500);
			var pt=Math.round((462-h)/2);
			var pb=462-pt-h;
			$("#game_play_area").css({"padding-top":pt,"padding-bottom":pb});
		}else{
			$("#game_recommend_box").height(h+38);
			$("#game_play_area").height("auto");
			$("#game_play_area").css({"padding-top":0,"padding-bottom":0});
		}
		$("#flash_play_box").css({"top":Math.round($("#flash_play_box_space").offset().top),"left":Math.round($("#flash_play_box_space").offset().left)});
	}

	hn=Math.floor(($("#game_recommend_box").height()-77)/105);
	$("#game_recommend_box .content").height($("#game_recommend_box").height()-33);
	$("#game_recommend_box .content .list .game_list").height(hn*105);
	$("#game_recommend_box .content .explain_list").height($("#game_recommend_box").height()-63);
	game_recommend_list_init(wn,hn);
}

//右侧推荐列表初始化
function game_recommend_list_init(wn,hn){
	var c=0;
	var s="";
	if(wn!=0&&(wn!=recommend_box_wn||hn!=recommend_box_hn)){
		$("#game_recommend_box .content .list .game_list a").remove();
		$("#game_recommend_box .content .hot_list .game_list").each(function(i){
			for(c;c<wn*hn*(i+1);c++){
				if(c<56){
					if(flash_game_hot[c][3]==1){
						s+="<a title=\""+flash_game_hot[c][0]+"\" target=\"_blank\" href=\""+flash_game_hot[c][2]+"\"><img src=\""+flash_game_hot[c][1]+"\">"+flash_game_hot[c][0]+"</a>"
					}else{
						s+="<a title=\""+flash_game_hot[c][0]+"\" target=\"_blank\" href=\""+flash_game_hot[c][2]+"\"><img osrc=\""+flash_game_hot[c][1]+"\" src=\"http://s.doyo.cn/s.gif\">"+flash_game_hot[c][0]+"</a>"
					}
				}
			}
			$(s).appendTo($(this));
			s="";
		});
		c=0;
		$("#game_recommend_box .content .related_list .game_list").each(function(i){
			for(c;c<wn*hn*(i+1);c++){
				if(c<56){
					if(flash_game_related[c][3]==1){
						s+="<a title=\""+flash_game_related[c][0]+"\" target=\"_blank\" href=\""+flash_game_related[c][2]+"\"><img src=\""+flash_game_related[c][1]+"\">"+flash_game_related[c][0]+"</a>"
					}else{
						s+="<a title=\""+flash_game_related[c][0]+"\" target=\"_blank\" href=\""+flash_game_related[c][2]+"\"><img osrc=\""+flash_game_related[c][1]+"\" src=\"http://s.doyo.cn/s.gif\">"+flash_game_related[c][0]+"</a>"
					}
				}
			}
			$(s).appendTo($(this));
			s="";
		});
		c=0;
	}
	recommend_box_wn=wn;
	recommend_box_hn=hn;
	game_recommend_list_lazyimg();
}

//右侧推荐列表图片后载
function game_recommend_list_lazyimg(){
	var img=$("#game_recommend_box .content .list .game_list:visible img[osrc]");
	var index=null;
	var array_obj=null;
	if(img.length){
		if(img.eq(0).closest(".list").hasClass("hot_list")){
			array_obj=flash_game_hot;
		}else if(img.eq(0).closest(".list").hasClass("related_list")){
			array_obj=flash_game_related;
		}
		img.each(function(i){
			$(this).attr("src",$(this).attr("osrc"));
			$(this).removeAttr("osrc");
			index=$(".game_list img",$(this).closest(".list")).index(this);
			array_obj[index][3]=1;
		});
	}
}

function first_reset_flash_play_box(){
	window.setTimeout(second_reset_flash_play_box,0);
}
function second_reset_flash_play_box(){
	if(!flash_full_screen){
		$("#flash_play_box").css({"top":Math.round($("#flash_play_box_space").offset().top),"left":Math.round($("#flash_play_box_space").offset().left)});
	}
	$(window).one("resize",first_reset_flash_play_box);
}

//解决IE浏览器游戏时上下键导致页面滚动的问题
function flash_ie_roll_fix(){
	if(navigator.appName==="Microsoft Internet Explorer"){
		$("#game_related_game .go_left div,#game_related_game .go_right div,#game_play_box .bottom a.refresh,#game_play_box .bottom a.amplification,#game_play_box .bottom a.narrow,#game_play_box .bottom a.full_screen,#light_switch,#game_play_box .score").click(function(){
			$(document).click();
		});
		if(_noker_getUserBrowserInfo().v=="6"){
			$("body").width(document.documentElement.clientWidth);
			$(window).resize(function() {
				$("body").width(document.documentElement.clientWidth);
			});
		}
		get_flash_obj("flash_play_obj").onfocus=function(){
			if(!$("#full_screen_black_wall:visible").length){
				var now_width=document.documentElement.clientWidth;
				$("html").css({"overflow":"hidden"});
				$("html").css({"margin-right":document.documentElement.clientWidth-now_width});
				$(document).one("click",function(){
					$("html").css({"overflow":"auto"});
					$("html").css({"margin-right":"auto"});
				}).one('mousewheel',function(){
					$(document).click();
					$("#game_play_box .score").focus().blur();
				});
			}
		}
	}
}

//全屏功能初始化
function flash_full_screen_init(){
	flash_full_screen=true;
	$(document).scrollTop(0);
	if(navigator.appName==="Microsoft Internet Explorer"){$("html").css({"overflow":"hidden"})};
	$("#full_screen_black_wall").show().css({filter:"alpha(opacity=100)","opacity":"1","width":document.documentElement.clientWidth>document.documentElement.scrollWidth?document.documentElement.clientWidth:document.documentElement.scrollWidth,"height":document.documentElement.clientHeight>document.documentElement.scrollHeight?document.documentElement.clientHeight:document.documentElement.scrollHeight});
	var wf=document.documentElement.clientWidth;var hf=document.documentElement.clientHeight;var pf=flash_show_width/flash_show_height;var ps=(wf-90)/hf;var w=0;var h=0;var l=0;var t=0;
	if(pf>ps){w=wf-90;h=w/pf;l=45;t=(hf-h)/2;}else{w=hf*pf;h=hf;l=(wf-90-w)/2+45;t=0;}
	$("#flash_play_box").addClass("no_border").css({"left":Math.round(l),"top":Math.round(t)})
	$("#flash_play_box").css({"width":Math.round(w),"height":Math.round(h)});
	if(need_iframe){
		$("#flash_play_iframe").css({"width":Math.round(w),"height":Math.round(h)});
		document.getElementById("flash_play_iframe").contentWindow.flash_change_size(Math.round(w),Math.round(h));
	}else{
		$("#flash_play_obj,#flash_play_obj embed").css({"width":Math.round(w),"height":Math.round(h)});
	}
	$("#full_screen_close_button").show().css({"left":document.documentElement.clientWidth-40,"top":"3px"}).one("click",function(){
		flash_full_screen=false;
		$(this).hide();
		$("#full_screen_black_wall").hide();
		$("html").css({"overflow":"auto"});
		$("#flash_play_box").css({"width":$("#flash_play_box_space").width(),"height":$("#flash_play_box_space").height()});
		if(need_iframe){
			$("#flash_play_iframe").css({"width":$("#flash_play_box_space").width(),"height":$("#flash_play_box_space").height()});
			document.getElementById("flash_play_iframe").contentWindow.flash_change_size($("#flash_play_box_space").width(),$("#flash_play_box_space").height());
		}else{
			$("#flash_play_obj,#flash_play_obj embed").css({"width":$("#flash_play_box_space").width(),"height":$("#flash_play_box_space").height()});
		}
		$("#flash_play_box").removeClass("no_border").css({"top":Math.round($("#flash_play_box_space").offset().top),"left":Math.round($("#flash_play_box_space").offset().left)});
		$(window).unbind("keydown");
		return false;
	});
	$(window).keydown(function(e){
		if(e.keyCode==27){
			$("#full_screen_close_button").click();
		}
	});
	$(window).one("resize",first_reset_flash_full_screen);
}

function first_reset_flash_full_screen(){
	window.setTimeout(second_reset_flash_full_screen,0);
}
function second_reset_flash_full_screen(){
	if(flash_full_screen){
		$("#full_screen_black_wall").css({"width":document.documentElement.clientWidth>document.documentElement.scrollWidth?document.documentElement.clientWidth:document.documentElement.scrollWidth,"height":document.documentElement.clientHeight>document.documentElement.scrollHeight?document.documentElement.clientHeight:document.documentElement.scrollHeight});
		var wf=document.documentElement.clientWidth;var hf=document.documentElement.clientHeight;var pf=flash_show_width/flash_show_height;var ps=(wf-90)/hf;var w=0;var h=0;var l=0;var t=0;
		if(pf>ps){w=wf-90;h=w/pf;l=45;t=(hf-h)/2;}else{w=hf*pf;h=hf;l=(wf-90-w)/2+45;t=0;}
		$("#flash_play_box").css({"left":Math.round(l),"top":Math.round(t)});
		$("#flash_play_box").css({"width":Math.round(w),"height":Math.round(h)});
		if(need_iframe){
			$("#flash_play_iframe").css({"width":Math.round(w),"height":Math.round(h)});
			document.getElementById("flash_play_iframe").contentWindow.flash_change_size(Math.round(w),Math.round(h));
		}else{
			$("#flash_play_obj,#flash_play_obj embed").css({"width":Math.round(w),"height":Math.round(h)});
		}
		$("#full_screen_close_button").css({"left":document.documentElement.clientWidth-40});
	}
	$(window).one("resize",first_reset_flash_full_screen);
}

//flash播放框缩放处理
function flash_zoom_process(){
	var z_level=Math.abs(zoom_level);
	var coefficient=0;
	if(zoom_level==0){
		coefficient=1;
		if(flash_show_width==910){$("#game_play_box .bottom a.amplification").click();zoom_level--;}//初始尺寸即为最大宽度的特殊处理
		if(flash_show_width==300){$("#game_play_box .bottom a.narrow").click();zoom_level++;}//初始尺寸即为最小宽度的特殊处理
	}else if(zoom_level>0){
		if(flash_show_width*Math.pow(1.2,z_level)>=910){
			$("#game_play_box .bottom a.amplification").hide();
			$("#game_play_box .bottom span.amplification_disable").css("display","inline-block");
			coefficient=910/flash_show_width;
		}else{
			coefficient=Math.pow(1.2,z_level);
		}
	}else if(zoom_level<0){
		if(flash_show_width*Math.pow(0.8,z_level)<=300){
			$("#game_play_box .bottom a.narrow").hide();
			$("#game_play_box .bottom span.narrow_disable").css("display","inline-block");
			coefficient=300/flash_show_width;
		}else{
			coefficient=Math.pow(0.8,z_level);
		}
	}else{}
	$("#game_recommend_box").hide();
	if(flash_show_width*coefficient<=580){
		$("#game_play_area").width(622);
	}else if(flash_show_width*coefficient<=670){
		$("#game_play_area").width(712);
	}else{
		$("#game_play_area").width("auto");
	}
	if(flash_show_height*coefficient<462){
		var pt=Math.round((462-flash_show_height*coefficient)/2);
		var pb=462-pt-flash_show_height*coefficient;
		$("#game_play_area").css({"padding-top":pt,"padding-bottom":pb});
	}else{
		$("#game_play_area").height("auto");
		$("#game_play_area").css({"padding-top":0,"padding-bottom":0});
	}
	$("#flash_play_box_space,#flash_play_box").css({"width":Math.round(flash_show_width*coefficient),"height":Math.round(flash_show_height*coefficient)});

	if(need_iframe){
		$("#flash_play_iframe").css({"width":Math.round(flash_show_width*coefficient),"height":Math.round(flash_show_height*coefficient)});
		document.getElementById("flash_play_iframe").contentWindow.flash_change_size(Math.round(flash_show_width*coefficient),Math.round(flash_show_height*coefficient));
	}else{
		$("#flash_play_obj,#flash_play_obj embed").css({"width":Math.round(flash_show_width*coefficient),"height":Math.round(flash_show_height*coefficient)});
	}
	$("#flash_play_box").css({"top":Math.round($("#flash_play_box_space").offset().top),"left":Math.round($("#flash_play_box_space").offset().left)});
	game_recommend_box_reset();
};

function my_play_list_init(){
if(!getCookie("flash_recently_played_list")){return false;};
$.get("/flash/get_game_info",{"game_list":getCookie("flash_recently_played_list").split(",")},function(data){
	var game_info_temp="";
	$.each(data.game_info,function(i,n){
		if(i<4){game_info_temp+="<a href=\"/flash/game/"+n[0]+"\" target=\"_blank\" title=\""+n[1]+"\"><img src=\""+n[2]+"\">"+n[1]+"</a>";}
	});
	$("#my_play_list").show().find(".list").html(game_info_temp);
},"json");

};


