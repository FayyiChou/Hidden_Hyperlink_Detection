<script type="text/javascript">

//搜索自动完成变量
var search_temp_str="";
var search_temp_timer=null;

//自动完成探测及处理
function auto_complete_check(word){
	if(word.length){
		$.get("/flash/autocomplete?q="+encodeURI(word),function(data){
			if(data.gameinfo.length){
				var temp_str="";
				$.each(data.gameinfo,function(i,n){
					temp_str+="<a href=\""+n[5]+"\"><img src=\""+n[4]+"\">"+n[0]+"<br /><span>"+n[1]+" - "+n[2]+" &nbsp; "+n[3]+"</span></a>";
				});
				$("#web_header_s .header_search_auto_box").html(temp_str).show().find("a").unbind().hover(function(){
					$(this).addClass("hover");
				},function(){
					$(this).removeClass("hover");
				});
			}else{
				$("#web_header_s .header_search_auto_box").hide();
			}
		},"json");
	}else{
		$("#web_header_s .header_search_auto_box").hide();
	}
};

//搜索字符串检查
function check_search_key(){
	if($("#web_header_s .header_search_key").val().length>0){
		return true;
	}else{
		alert("请输入搜索关键字！");
		$("#web_header_s .header_search_key").focus();
		return false;
	};
};

//前往搜索页
function go_search_page(){
	location="/flash/search/"+encodeURI($("#web_header_s .header_search_key").val());
}

jQuery(function($){

//搜索回车提交
$("#web_header_s .header_search_key").keypress(function(e){
	if(e.keyCode==13){
		if($("#web_header_s .header_search_auto_box:visible a.active").length){
			location.href=$("#web_header_s .header_search_auto_box:visible a.active").attr("href");
		}else{
			if(check_search_key()){
				go_search_page();
			}
		}
	};
});

//搜索按钮提交及hover效果
$("#web_header_s .header_search_button").click(function(){
	if(check_search_key()){go_search_page();}
}).hover(function(){
	$(this).addClass("header_search_button_hover");
},function(){
	$(this).removeClass("header_search_button_hover");
});

//热门搜索关键词读取
$("#web_header_s .header_search_hot_key").load("/flash/topsearch?r="+document.referrer.toLowerCase());

//自动完成功能
$("#web_header_s .header_search_key").keyup(function(e){
	if(e.keyCode!=38&&e.keyCode!=40&&e.keyCode!=13){
		$("#web_header_s .header_search_bar").addClass("header_search_bar_high_level");
		auto_complete_check($("#web_header_s .header_search_key").val());
	}else{
		if($("#web_header_s .header_search_auto_box:visible").length){
			if(e.keyCode==38){
				if($("#web_header_s .header_search_auto_box a.active").length){
					if($("#web_header_s .header_search_auto_box a.active").index()==0){
						$("#web_header_s .header_search_auto_box a.active").removeClass("active");
						$("#web_header_s .header_search_auto_box a:last").addClass("active");
					}else{
						$("#web_header_s .header_search_auto_box a.active").removeClass("active").prev().addClass("active");
					};
				}else{
					$("#web_header_s .header_search_auto_box a:last").addClass("active");
				};
			}else if(e.keyCode==40){
				if($("#web_header_s .header_search_auto_box a.active").length){
					if($("#web_header_s .header_search_auto_box a.active").index()==$("#web_header_s .header_search_auto_box a").length-1){
						$("#web_header_s .header_search_auto_box a.active").removeClass("active");
						$("#web_header_s .header_search_auto_box a:first").addClass("active");
					}else{
						$("#web_header_s .header_search_auto_box a.active").removeClass("active").next().addClass("active");
					};
				}else{
					$("#web_header_s .header_search_auto_box a:first").addClass("active");
				};
			};
		};
	};
}).blur(function(){
	window.setTimeout(function(){$("#web_header_s .header_search_auto_box").hide();$("#web_header_s .header_search_bar").removeClass("header_search_bar_high_level");},300);
});

});
</script>

