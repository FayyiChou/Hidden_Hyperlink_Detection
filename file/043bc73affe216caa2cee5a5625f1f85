//加入最近玩过（仅用于Flash首页显示，web独有）
if(getCookie("flash_recently_played_list")){
	var flash_recently_played_list=getCookie("flash_recently_played_list").split(",");
	flash_recently_played_list=$.grep(flash_recently_played_list,function(n,i){
		return n!=game_id&&i<4;
	});
	if(flash_recently_played_list.join(",").length){
		createCookie("flash_recently_played_list",game_id+","+flash_recently_played_list,expTime(365*24*60*60*1000),"/",".doyo.cn");
	}else{
		createCookie("flash_recently_played_list",game_id,expTime(365*24*60*60*1000),"/",".doyo.cn");
	}
}else{
	createCookie("flash_recently_played_list",game_id,expTime(365*24*60*60*1000),"/",".doyo.cn");
}

