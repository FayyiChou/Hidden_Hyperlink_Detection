<script type="text/javascript">
var imgArr = ["http:\/\/img0.kfzimg.com\/pmgs\/pre_show_pic\/6\/1558\/1201\/1612.jpg"];</script>
<script type="text/javascript">
	var imgArray = (typeof imgArr == 'undefined' || !imgArr)?[]:imgArr;
	var $box = $('.show_img_center ul'),href = $box.find('li a').eq(0).attr('href');
	var html = '';
	imgArray.length && $.each(imgArray,function(i,o){
		html += '<li><a href="'+ href +'" target="_blank"><img src="'+o+'"/></a></li>';
	});
	$box.html(html);
	setTimeout(function(){
		KFZ.ui.resizeImage('.show_img_box img',{
			maxWidth:600,
			maxHeight:360,
			isSetPosition:1,
			errSrc:'/img/none.jpg'
		});
		var imgIndex = 0;
		var len = imgArray.length;
		var timer = '';
		$('.show_img_box')
		.off('click','.left_ico').on('click','.left_ico',function(){
			if(imgIndex > 0){
				imgIndex --;
				$('.show_img_center li').hide().eq(imgIndex).stop().fadeIn('fast');
				$('.point_box a').removeClass('now').eq(imgIndex).addClass('now');
				if(timer)clearInterval(timer);
				timerFunction();
			}
		})
		.off('click','.right_ico').on('click','.right_ico',function(){
			if(imgIndex < len-1){
				imgIndex ++;
				$('.show_img_center li').hide().eq(imgIndex).stop().fadeIn('fast');
				$('.point_box a').removeClass('now').eq(imgIndex).addClass('now');
				if(timer)clearInterval(timer);
				timerFunction();
			}
		})
		.off('click','.point_box a').on('click','.point_box a',function(){
			imgIndex = $('.point_box a').index($(this));
			$('.show_img_center li').hide().eq(imgIndex).stop().fadeIn('fast');
			$('.point_box a').removeClass('now').eq(imgIndex).addClass('now');
			if(timer)clearInterval(timer);
			imgArray.length > 1 && timerFunction();
		});
		var timerFunction = function(){
			timer = setInterval(function(){
				if(imgIndex == len - 1){
					imgIndex = 0;
				}else{
					imgIndex ++;
				}
				$('.show_img_center li').hide().eq(imgIndex).stop().fadeIn('fast');
				$('.point_box a').removeClass('now').eq(imgIndex).addClass('now');
			},5000);
		};
		imgArray.length > 1 && timerFunction();
	});
</script>
