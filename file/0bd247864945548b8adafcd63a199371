linkarr = new Array();
picarr = new Array();
textarr = new Array()

var files = "";
var links = "";
var texts = "";			

			
linkarr[1] = "http://www.cnjiayu.com.cn/html/2017/jyxw_0217/1537690.html";
picarr[1]  = "http://www.cnjiayu.com.cn/uploadfile/2017/0217/20170217102809407.jpg";
