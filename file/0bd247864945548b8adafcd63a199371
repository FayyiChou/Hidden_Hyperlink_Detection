picarr[3]  = "http://www.cnjiayu.com.cn/uploadfile/2017/0215/20170215035938368.jpg";
textarr[3] = "我县34名贫困&ldquo;两癌&rdquo;妇女获";
linkarr[4] = "http://www.cnjiayu.com.cn/html/2017/jyxw_0215/1537515.html";
picarr[4]  = "http://www.cnjiayu.com.cn/uploadfile/2017/0215/20170215032114267.jpg";
textarr[4] = "县纪委十四届二次全体（扩大";
linkarr[5] = "http://www.cnjiayu.com.cn/html/2017/jyxw_0215/1537512.html";
picarr[5]  = "http://www.cnjiayu.com.cn/uploadfile/2017/0215/20170215031246960.jpg";
textarr[5] = "嘉鱼21个项目集中开工总投资";

for(i=1;i<picarr.length;i++){
if(files=="") files = picarr[i];
else files += "|"+picarr[i];
}
for(i=1;i<linkarr.length;i++){
if(links=="") links = linkarr[i];
else links += "|"+linkarr[i];
}
for(i=1;i<textarr.length;i++){
if(texts=="") texts = textarr[i];
else texts += "|"+textarr[i];
}

var pw =332; //宽
var ph = 250;  //高
var sizes = 14;
var Times = 4000;
var umcolor = 0xFFFFFF;
var btnbg =0xFF7E00;
var txtcolor =0xFFFFFF;
var txtoutcolor = 0x000000;
var flash = new SWFObject('statics/images/jywg/sy_player.swf', 'mymovie', pw, ph, '7', '');
flash.addParam('allowFullScreen', 'true');
flash.addParam('allowScriptAccess', 'always');
flash.addParam('quality', 'high');
flash.addParam('wmode', 'Transparent');
flash.addVariable('pw', pw);
flash.addVariable('ph', ph);
flash.addVariable('sizes', sizes);
flash.addVariable('umcolor', umcolor);
flash.addVariable('btnbg', btnbg);
flash.addVariable('txtcolor', txtcolor);
flash.addVariable('txtoutcolor', txtoutcolor);
flash.addVariable('urls', links);
flash.addVariable('Times', Times);
flash.addVariable('titles', texts);
flash.addVariable('imgs', files);
flash.write('dplayer2');</script>
