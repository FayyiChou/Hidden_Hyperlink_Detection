<script type="text/javascript">


var speed=30;
var marquee_product2MmCsYg = document.getElementById("marquee_product2MmCsYg");
var marquee_product1MmCsYg = document.getElementById("marquee_product1MmCsYg");
var marquee_demoMmCsYg = document.getElementById("marquee_demoMmCsYg");

var showwidthMmCsYg;
var loopwidthMmCsYg;
var iMmCsYg;
var mMmCsYg,nMmCsYg;

showwidthMmCsYg=marquee_demoMmCsYg.offsetWidth*1;
loopwidthMmCsYg=marquee_product1MmCsYg.offsetWidth*1;

if(loopwidthMmCsYg<=1){loopwidthMmCsYg=1;}

mMmCsYg=Math.ceil(showwidthMmCsYg/loopwidthMmCsYg);

for(nMmCsYg=0;nMmCsYg<mMmCsYg;nMmCsYg++){
marquee_product2MmCsYg.innerHTML+=marquee_product1MmCsYg.innerHTML;
}

marquee_demoMmCsYg.scrollLeft=0;
iMmCsYg=0;

function MarqueeMmCsYg(){

if(iMmCsYg>=loopwidthMmCsYg){
 iMmCsYg=0;
 }else{
 iMmCsYg++;
}
marquee_demoMmCsYg.scrollLeft=iMmCsYg;

}
var MyMarMmCsYg=setInterval(MarqueeMmCsYg,speed);
marquee_demoMmCsYg.onmouseover=function(){clearInterval(MyMarMmCsYg);}
marquee_demoMmCsYg.onmouseout=function(){MyMarMmCsYg=setInterval(MarqueeMmCsYg,speed);}

</script>


