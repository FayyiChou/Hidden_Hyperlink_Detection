<script type="text/javascript">
    $(document).ready(function () {
        $(".er").each(function () {
            if ($(this).width() - $(this).parent().width() > 0) {
                $(this).css({ "left": "-" + ($(this).width() - $(this).parent().width()) / 2 + "px" });
            } else {
                $(this).css({ "left": ($(this).parent().width() - $(this).width()) / 2 + "px" });
            }
        });
    });
</script>
