<script type="text/javascript">
_jsonpSuggest = function(e){//头部的检索条
	var result = []; 
	if(unescape(e.q)==jQuery("#sw").val())
	jQuery(unescape(e.values).split("|")).each(function(){ result.push(this); })
	window.sugresult = result.join("\r\n");
};
jQuery("#sw").ready(function(){jQuery("#sw").suggest('http://relt1.duxiu.com/swjson.jsp?jsoncallback=_jsonpSuggest',{});});
_jsonpSuggest = function(e){//检索框的检索框
	var result = []; 
	if(unescape(e.q)==jQuery(".zt_search #sw").val())
	jQuery(unescape(e.values).split("|")).each(function(){ result.push(this); })
	window.sugresult = result.join("\r\n");
};
jQuery(".zt_search #sw").ready(function(){jQuery(".zt_search #sw").suggest('http://relt1.duxiu.com/swjson.jsp?jsoncallback=_jsonpSuggest',{});});
</script>
