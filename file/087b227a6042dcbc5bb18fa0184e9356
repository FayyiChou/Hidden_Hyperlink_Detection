<script>
doCheckLogin();
$(function(){
	var ct=null;
	$('.zt_user,.test11').hover(function(){
		clearTimeout(ct);
		ct=setTimeout(function(){$('.zt_u_bg').slideDown(100);},100);
		$('.zt_u_name').addClass('zt_u_x');
	},function(){
		clearTimeout(ct);
		ct=setTimeout(function(){$('.zt_u_bg').slideUp(100);},100);
		$('.zt_u_name').removeClass('zt_u_x');
	});
	$(".Wraptubiao dl dt a").hover(function(){
		$(this).children().eq(0).hide();
		$(this).children().eq(1).show();
		},function(){
		$(this).children().eq(0).show();	
		$(this).children().eq(1).hide();	
	})
	$(".Jansuo").click(function(){
		if($(this).siblings("#sw").val()=='' ){
			alert("请输入检索词");
		}else{
			$("#dosearch").html("");
			$(this).parents("#hssform").find("input").each(function(){
				if ($(this).val() != "") {
					$("<input type=\"hidden\" name=\""+$(this).attr("name")+"\" value=\""+htmlspecialchars($(this).val())+"\" />").appendTo("#dosearch");
				}
			});
			$("#dosearch").submit();
		}
	});
});
setTimeout(function(){
		var stat = document.createElement('script'); stat.type = 'text/javascript';
	    stat.src = "http://counter.zhizhen.com/api/statistics/log?clientid=1&groupID=0&msign_dsr=1491056282029&userid_dsr=21498&AID_dsr=7209&searchtime=";
	    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(stat, s);
}, 100);
function enterIn(e){
  var evt=e?e:(window.event?window.event:null);//兼容IE和FF
  var evtvalue = evt.keyCode||evt.which||evt.charCode;
  if (evtvalue==13){
	  var chls = document.getElementById("strchannel").value+document.getElementById("cxfindright").value+document.getElementById("specialchannel").value;
	  var mags = document.getElementById("mags").value;
	  if(mags == null || mags == ''){
		  goSearchfirst(chls);
	  }else{
		  goSearchfirst();
	  }
	  return ;
	} else {
		return false;
	}
}
</script>
