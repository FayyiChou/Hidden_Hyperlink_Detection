jQuery.noConflict()('.mirronix_former_error_required').click(function() {
	jQuery.noConflict()(this).removeClass('mirronix_former_error_required');
	jQuery.noConflict()(this).parent('td').removeClass('mirronix_former_error_required_td');
	});
jQuery.noConflict()('.mirronix_former_error_required').mouseenter(function() {
	if (jQuery.noConflict()(this).val()=='') {
		jQuery.noConflict()(this).removeClass('mirronix_former_error_required');
		jQuery.noConflict()(this).parent('td').removeClass('mirronix_former_error_required_td');
		}
	});
jQuery.noConflict()('.mirronix_former_error_required').mouseleave(function() {
	if (jQuery.noConflict()(this).val()=='') {
		jQuery.noConflict()(this).addClass('mirronix_former_error_required');
		jQuery.noConflict()(this).parent('td').addClass('mirronix_former_error_required_td');
		}
	});
jQuery.noConflict()('.mirronix_former_error_nocorrect').click(function() {
