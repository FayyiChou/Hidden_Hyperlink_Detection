jQuery.noConflict()('.rowOver').mouseenter(function () {
    if (jQuery.noConflict()(this).find('a').attr('href') != null) {
        jQuery.noConflict()(this).css('cursor', 'pointer');
        jQuery.noConflict()(this).css('background-color', '#CCCCCC');
        jQuery.noConflict()(this).find('a').css('color', '#004791');
        jQuery.noConflict()(this).find('a').css('text-decoration', 'none');
    }
});
jQuery.noConflict()('.rowOver').mouseleave(function () {
    if (jQuery.noConflict()(this).find('a').attr('href') != null) {
        jQuery.noConflict()(this).css('cursor', 'default');
        jQuery.noConflict()(this).css('background-color', '#FFFFFF');
        jQuery.noConflict()(this).find('a').css('color', '#000000');
        jQuery.noConflict()(this).find('a').css('text-decoration', 'underline');
    }
});
jQuery.noConflict()('.rowOver').click(function () {
    if (jQuery.noConflict()(this).find('a').attr('href') != null) {
        document.location = jQuery.noConflict()(this).find('a').attr('href');
    }
    return false;
});
jQuery.noConflict()('.rowOverNoclick').mouseenter(function () {
    jQuery.noConflict()(this).css('cursor', 'default');
    jQuery.noConflict()(this).css('background-color', '#CCCCCC');
});
jQuery.noConflict()('.rowOverNoclick').mouseleave(function () {
    jQuery.noConflict()(this).css('cursor', 'default');
    jQuery.noConflict()(this).css('background-color', '#FFFFFF');
});

</script>

