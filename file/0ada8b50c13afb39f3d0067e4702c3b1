<script type="text/javascript">
	window.onload=function(){  
	  window.onresize = function(){
		  $(".floatApp").css({"right":992+($(window).width()-998)/2+2});
		  //$(".floatApp").css({"width":($(window).width())/2+2});
		  $("#downloanPic").attr("width",$(window).width()/8-20);
		 //alert($(window).width());
	  };  
	}  
	$("body").attr("onResize","ReSet()");
	function ReSet(){
		$(".floatApp").css({"right":998+($(window).width()-998)/2+2});
		$("#downloanPic").attr("width",$(window).width()/8-20);
	}
	function floatAppClose(){
		$(".floatApp").hide();
	}
	$(".floatNews").mouseleave(function(){
		//TfloatNews = setInterval("floatNewsliMove()",1500);
	});
	ReSet();
</script>
