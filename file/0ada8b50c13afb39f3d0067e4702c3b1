<script type="text/javascript">
function doKeyDownWithEnter(){
    if(event.keyCode==13) 
        { 
         doSiteSearch('0');
        }
}
function doSiteSearch(siteCatalogId)
{
    var webrootWithBackslash = '/web';
    var url = webrootWithBackslash + "/dynamic/site/siteArticleSearchAction!searchArticle.action";
    
    if(!siteCatalogId){
        alert("js error：parameter(siteCatalogId) is undefined in this function 'doSiteSearch'");
        return false;
    }
    
    var siteKeyWords = document.getElementById('siteKeyWords').value;
    
    if(siteKeyWords == null || (siteKeyWords != null && siteKeyWords.Trim() == "")){
        alert("请输入关键字！");
        return false;
    }
    
    if(siteCatalogId)
        url = url + "?siteCatalogId=" + siteCatalogId;
    if(siteKeyWords != null && siteKeyWords != "")
        url = url + "&isTitle=0&siteKeyWords=" + encodeURIComponent(encodeURI(siteKeyWords));
    
    window.open(url);
    return false;
    
}
</script>
