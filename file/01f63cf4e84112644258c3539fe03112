<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head>
    <script src="//hm.baidu.com/hm.js?e21fe362fe7ca664eb5fd89ed45ed835"></script><script type="text/javascript" async="" charset="utf-8" src="http://dl.ntalker.com/js/xn6/ntkfstat.js?siteid=kf_9887&amp;XNform=t2d2"></script><script>
    //根据navigator.userAgent判断使用设备
	function getDeviceType() {
        var userAgent = navigator.userAgent.toLowerCase();
        var return_str = '';
        var android = userAgent.indexOf('android') > -1;
        var iphone = userAgent.indexOf('iphone') > -1;
        var ipad = userAgent.indexOf('ipad') > -1;
        var ipod = userAgent.indexOf('ipod') > -1;
        var windows_phone = userAgent.indexOf('windows phone') > -1;
        var blackberry = userAgent.indexOf('blackberry') > -1;
        var symbia = userAgent.indexOf('symbia') > -1;
        var nokiabrower = userAgent.indexOf('nokiabrower') > -1;
        if (iphone || windows_phone || blackberry || symbia || nokiabrower || ipod){
            return_str = 'phone';
        }else if (ipad){
            return_str = 'ipad';
        }else if (android){
            return_str = 'android';
        }else {
            return_str = 'ipad';
        }
        return return_str;
	}   
    //获取跳转地址
    function getLocationUrl(){
        var return_arr = {
            //输入的域名
            hostname: window.location.hostname,
            //系统分配的pc端域名
            padHost: '',
            //系统分配的wap端域名
            phoneHost: '',
            //系统分配的wap端主机名
            phoneUrl: 'http://m.lining.com',
            //系统分配的pc端主机名
            padUrl: 'http://store.lining.com'
        };
        
        return_arr.padHost = return_arr.padUrl.substring(7);
        return_arr.phoneHost = return_arr.phoneUrl.substring(7);

        var loacationUrl = window.location+'';
        var pattpad = new RegExp('http://store.lining.com');
        var pattphone = new RegExp('http://m.lining.com');
        if (pattpad.test(loacationUrl)){
            return_arr.padUrl = loacationUrl;
            return_arr.phoneUrl = loacationUrl.replace(return_arr.padHost, return_arr.phoneHost);
            return_arr.phoneUrl = return_arr.phoneUrl.replace(/shop/, 'wap');
        }
        if (pattphone.test(loacationUrl)){
            return_arr.phoneUrl = loacationUrl;
            return_arr.padUrl = loacationUrl.replace(return_arr.phoneHost, return_arr.padHost);
            return_arr.padUrl = return_arr.padUrl.replace(/wap/, 'shop');
        }
        return return_arr;
    }
    
    var urlParam = getLocationUrl();
    var deviceType = getDeviceType();
        
    if (deviceType == 'phone'){
        if (urlParam.hostname != urlParam.phoneHost){
            self.location = urlParam.phoneUrl;
        }
	}else if(deviceType == 'ipad'){
        if (urlParam.hostname != urlParam.padHost){
            self.location = urlParam.padUrl;
        }
	}
</script>
