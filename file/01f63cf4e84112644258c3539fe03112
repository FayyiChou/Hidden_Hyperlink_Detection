<!--到货通知 start-->
	<script language="javascript">
		function guanbi(){
			$("#datecheck").hide();
	        $("#mask").addClass("hide");
		}
	
	    function tijiao(){
	        var mobile = $('#mobile').val();
	        var email = $('#email').val();
	        var product_code = $('#goods_product_code').val();
	        var msg = '';
	
	        if(email == ''){
	            msg += '请输入邮箱地址<br/>';
	        }
	        if(product_code == ''){
	            msg += '请选择您的商品尺码<br/>';
	        }
	
	        if(msg != ''){
	            comm_alert(msg);
	        }else{
	            xajax_do_req_goods([mobile,email,product_code], 11);
	        }
	    }
	
	    function tijiao_call(id){
	        guanbi();
	        switch (id){
	            case 1:
	                comm_alert('保存成功，商品到货后将立即通知您',3);
	                break;
	
	            case 2:
	                comm_alert('此商品已经保存过了');
	                break;
	
	        }
	        //history.go(0);
	    }
	</script>
<!--到货通知 end-->

<!--左侧搜索列表-->
	<script language="javascript">
	
		//左侧菜单
		var WEBHOSTNAME = 'http://store.lining.com';//网站域名
	
		function setScrollTop(id){
			var images = $('#PD_desc_picture').find('img');
			images_orsrc = images.attr('orginalsrc');
			images_src = images.attr('src');
			if (images_orsrc != images_src){
				images.attr('src',images_orsrc);
				images.load(function(){
					$("html,body").animate({scrollTop:$("#"+id).offset().top-90});
				});
			}else {
				$("html,body").animate({scrollTop:$("#"+id).offset().top-90});
			}
		}
	</script>
<!--左侧搜索列表-->

<!--放大镜开始-->
	<script type="text/javascript" src="http://cdn.lining.com/shop/lining/theme/xth2/js/jquery.imagezoom.min.js?v=2016122211"></script>
	<script type="text/javascript">
	    $(document).ready(function(){
	        $(".jqzoom").imagezoom();
	        $("#thumblist li a").mouseover(function () {
				//商品鼠标滑上，放大镜操作显示 科技正文展示隐藏 科技icon变成浅色
				$('.jqzoom').show();
				$('.tecContent').hide();
				$(".tecLeftIcon").removeClass("tecIconBlock");
				//结束
	            $(this).parents("li").addClass("tb-selected").siblings().removeClass("tb-selected");
	            $(".jqzoom").attr('src',$(this).find("img").attr("mid"));
	            $(".jqzoom").attr('rel',$(this).find("img").attr("big"));
	        });

			$(".tecLeftIcon").mouseover(function () {
				//商品鼠标滑上，放大镜操作隐藏 科技正文展示显示
				$('.jqzoom').hide();
				$('.tecContent').show();
				var index = $(this).index();
				$(this).addClass("tecIconBlock").siblings().removeClass("tecIconBlock");
				$('.tecDisplay:eq('+index+')').addClass("tecBlock").siblings().removeClass("tecBlock");
			});

			$('.tecShow_Nav').mouseover(function () {
				//科技详情鼠标点击展示
				var index = $(this).index();
				$(this).addClass("tecShow_Nav_choosed").siblings().removeClass("tecShow_Nav_choosed");
				$('.tecShow_desc:eq('+index+')').addClass("tecShow_desc_show").siblings().removeClass("tecShow_desc_show");
			});
	    });

	    //加载随意搭
	    var package_flg = '';
	    var package_spu = '';
	</script>
<!--放大镜结束 -->
<!-- 商品评论 -->
<script>
$(function(){
	//选择评价标签
	$('.comments_tag_area .cta_tag_list li').live('click', function(){
		if ($(this).hasClass('selected')) {
			$(this).removeClass('selected').addClass('unselected');
		} else {
			$(this).removeClass('unselected').addClass('selected')
		           .siblings('li').removeClass('selected')
		           .addClass('unselected');
		}
		
		loading.comments(1);
	})
	
	//选择评论形式
	$('.comments_condition_area li').live('click', function(){
		if (!$(this).is('.selected')) {
			$(this).removeClass('unselected').addClass('selected')
		           .siblings('li').removeClass('selected').addClass('unselected');

			loading.comments(1);
		}
		
	});
	
	//展示评论图片
	$('.comments_matter_area .ctl_showimg li').live('click', function(){
		if ($(this).is('.ctl_current')) {
			$(this).removeClass('ctl_current');
			$(this).closest('.ctl_showimg').next('.ctl_showimg_big').hide()
                   .find('.ctl_photo_viewer').html('');
		} else {
			$(this).addClass('ctl_current').siblings('li').removeClass('ctl_current');
			var src = $(this).find('img').attr('src'),
			img = '<img src=' + src + ' width="230px" height="230px">';
			var l = $(this).closest('.ctl_showimg').next('.ctl_showimg_big')
			               .animate({width:'show',height:'show',opacity:'show'},'fast')
			               .find('.ctl_photo_viewer').html(img);
		}
	});
	
	$('.comments_matter_area .ctl_photo_viewer').live('click', function(){
		$(this).html('').closest('.ctl_showimg_big')
		       .hide().siblings('.ctl_showimg')
		       .find('li').removeClass('ctl_current');
	});
	
});

//全部分类
$('.goodsList-new-topTitle').click(function(){
    window.open('/shop/goodsCate-0-0-0-0-0-0-min,max-0.html','_blank');
});
$('.goodsList-new-categoryTitle').click(function(){
    var url = $(this).attr('cateurl');
    if (url != ''){
        window.open(url,'_blank');
    }
});	
</script>
