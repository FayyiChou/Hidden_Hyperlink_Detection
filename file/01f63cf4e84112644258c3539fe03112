<!-- omniture-->
			<script language="JavaScript" type="text/javascript">
		var proName11	= "【2017新品】李宁超轻十四代男子专业跑反光一体织减震支撑耐磨防滑轻质跑鞋";
		var proPartNum	= "ARBM019-4";
		//添加购物车时执行此函数
		function addOmniture(product_code){
			var s=s_gi('lining-prd');
			s.linkTrackVars='products,events';
			s.linkTrackEvents='scAdd';
			s.products=product_code;
			s.events='scAdd';
			s.tl(this,'o','添加购物车');
		}
	
		s.pageName="产品:"+proName11+":"+proPartNum;
		s.prop4="产品详情页";
		s.prop9="非登录用户";
		var gust="";
		if(customer.cust_id && (gust=="1")){
			s.eVar21=customer.cust_id;
		}
		s.events="prodView,event3";
		s.products=goods.category +";"+ proPartNum;
		s.eVar24=goods.gender;
		s.eVar25=goods.category;
		s.eVar26= goods.price;
		s.eVar27= goods.rate;
		s.channel="产品详情页";
	
		var s_code=s.t();if(s_code)document.write(s_code)//--></script>
		<script language="JavaScript" type="text/javascript"><!--
		if(navigator.appVersion.indexOf('MSIE')>=0)document.write(unescape('%3C')+'\!-'+'-')//-->
		</script><noscript>&lt;img src="http://lining.122.2o7.net/b/ss/lining-prd/1/H.23.3--NS/0"
		height="1" width="1" border="0" alt="" /&gt;</noscript>
		
