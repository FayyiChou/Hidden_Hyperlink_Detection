<script>
    // 获取设备类别的方法
    function getDeviceState() {
        //创建状态指示元素
        var indicator = document.createElement('div');
        indicator.className = 'state-indicator';
        document.body.appendChild(indicator);
        return parseInt(window.getComputedStyle(indicator).getPropertyValue('z-index'), 10);
    }
    if (deviceType == 'android'){
        var deviceStutas = getDeviceState();
        if (Number(deviceStutas) == 4){
            if (urlParam.hostname != urlParam.phoneHost){
                self.location = urlParam.phoneUrl;
            }
        }else if (Number(deviceStutas) < 3 && window.devicePixelRatio >= 2){
            if (urlParam.hostname != urlParam.phoneHost){
                self.location = urlParam.phoneUrl;
            }
        }else if (urlParam.hostname != urlParam.padHost) {
            self.location = urlParam.padUrl;
        }
    }
</script>
