	function showTot(){
	
		if ($("#comment_tck").css("display") == "none") {
	           $("#comment_tck").show();
	           $("#pop_mask").removeClass('hide');
	           $("#pop_mask").css('height','6888px');
	        } else {
	           $("#comment_tck").hide();
	           $("#pop_mask").addClass('hide');
	        }
	}
	
	function close_box(){
		$("#comment_tck").hide();
	    $("#pop_mask").addClass('hide');
	}
