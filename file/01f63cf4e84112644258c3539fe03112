<script type="text/javascript">
	function serviceShow(){
		$("#buttonShow").removeClass("hide");
		$("#mask").removeClass("hide");
		$(".instructions").show();
	}
	function showClose(){
		$("#buttonShow").addClass("hide");
		$("#mask").addClass("hide");
		$(".instructions").hide();
	}
</script>

<!--浏览记录图片切换-->
	<script type="text/javascript">
	function changeSlave1(obj){
		$(obj).siblings().removeClass('choiceSlave');
		$(obj).addClass('choiceSlave');
		var goodsName = $(obj).attr('goodsname');//商品名称
		var url = $(obj).attr('url');//商品详情地址
		var picUrl = $(obj).attr('picurl');//图片路径
		var sku = $(obj).find('div').attr('sku');//商品sku
		var price = $(obj).find('div').html();//商品价格
		$(obj).parent().prev().attr('href',url);
		$(obj).parent().prev().find('img').attr('src',picUrl);//替代主图
		$(obj).parent().parent().parent().find('.price').attr('sku',sku).html(price);
		$(obj).parent().parent().parent().find('.hgoodsName').html(goodsName);
	}
	</script>
<!--浏览记录图片切换-->

<!--你可能喜欢 start-->
	<script type="text/javascript">
		$(function(){
			var page = 1; //当前第几页
			var i = 3; //图片个数
			var $content = $("#f_litimg");//寻找到内容展示区
			var $content_w = $("#g_fushil").height();//内容展示区外围div宽度
			var $len = $("#f_litimg ul li").length;//拿到li的个数
			var page_count = Math.ceil($len/i);  //只要不是整数，就往大的方向取最小的整数
			$("#fu_top").click(function(){
	
				if( !$content.is(":animated") ){
					if( page < page_count ){
						$content.animate({top:'-='+$content_w},"normal");//改变left值，每次换一个版
						page++;
					}
	
				}
			});
	
			$("#fu_down").click(function(){
				if( !$content.is(":animated") ){    //判断"视频内容展示区域"是否正在处于动画
					if( page >= page_count ){
						$content.animate({ top : '+='+$content_w }, "slow");//改变left值，每次换一个版
						page--;
					}
				}
			})
	
	
		});
	</script>
<!--你可能喜欢-->

