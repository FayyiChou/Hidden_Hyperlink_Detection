<script type="text/javascript" src="http://cdn.e-lining.com/shop/lining/theme/xth2/comm_js/jquery.js?v=20120627"></script> <!--抓价格--> <script>
    $(document).ready(function(e) {
        var skuStr = '';
        $('.sku').each(function(k, v) {
            var sku = $(this).attr('sku');
            skuStr += sku + ',';
        });

        if (skuStr != '') {
            var l = skuStr.length;
            skuStr = skuStr.substring(0, (l - 1));
            $.ajax({
                type: 'POST',
                url: '/shop/clothes_preview/indexLoadPrice.html',
                dataType: 'JSON',
                async: false,
                data: {
                    sku: skuStr,
                    type: 1},
                success: function( o ) {
                    if (o.code == 200) {
                        $.each(o.data, function(k, v)
                        {
                            var sku = v.sku;
                            var picURL = v.mainPic;
                            var goodname = (v.goodsName).replace(/&quot;/g,'"');
                            var goodUrl = v.goodsLink;
                            // Setup values in html page
                            $('#' + sku + '-img').attr('src', picURL);
                            $('a[lsku="' + sku +'"]').attr('href', "http://store.lining.com"+goodUrl);
                            $('area[lsku="' + sku +'"]').attr('href', "http://store.lining.com"+goodUrl);
                            $('span[sku="' + sku + '"]').html(Math.round(v.price));
                            $('span[ysku="' + sku + '"]').html(Math.round(v.marketPrice));
                            $('div[gsku="'+sku+'"]').text(goodname);
                            $('a[gsku="'+sku+'"]').text(goodname);
                            //折扣
                            var num  =v.price/v.marketPrice*10;
                            num = Math.floor(num*10+0.50000000001);
                            num = num/10;
                            if(num==10){
                                num="新";
                                $('div[zsku="' + sku + '"]').html(num);
                            }else if(num*10%10==0){
                                $('div[zsku="' + sku + '"]').html(num+'.0折');
                            }
                            else{$('div[zsku="' + sku + '"]').html(num+'折');}
                        })
                    }
                }
            });
        }
    });
</script> <script>
    $(document).ready(function(){
        for(i=1;i<=20;i++){
            var text = document.getElementById('txt'+i);
            if (text ){
                var str = text.innerHTML;
                textLeng = 13;
                if(str.length > textLeng ){
                    text .innerHTML = str.substring(0,textLeng )+"...";
                } }
        }
    });
</script>&nbsp;&nbsp;&nbsp;
