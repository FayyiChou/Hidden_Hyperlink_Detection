<script type="text/javascript">
seajs.use(['jquery','login','user'],function($,login,user){
        //微信赠品活动
        //20160727 下掉微信赠品功能，需要使用把以下注释代码恢复即可
        $.ajax({
            url: "http://api.ddp.vip.pptv.com/channel/detail?channelid=25180921&format=jsonp&cb=?",
            type: "GET",
            dataType: "jsonp",
            async: true,
            success: function(data) {
                if (data.channel.shareStatus == 0) { //0：可赠 1：不可赠
                    $(".wechat_video").show().hover(function(){
                        getweChatCode();  
                    });
                }else{
                    $(".wechat_video").hide();
                }
            }
        });

        $(window).scroll(function(){    
            wechat_video();
        });

        wechat_video();

        function wechat_video(){
            scroH = $(this).scrollTop();
            if(scroH>=300){
                $(".wechat_video").removeClass("wechat_video_top"); //微信赠片
                $(".viewPhonescan").removeClass("viewPhonescan-top"); //手机扫码看
            }else{
                $(".wechat_video").addClass("wechat_video_top");
                $(".viewPhonescan").addClass("viewPhonescan-top");
            }
        }

        function getweChatCode(){
            var codeurl = "http://api.ddp.vip.pptv.com/shareTicket/qrcode?channelId=25180921&username="+getcookie()+"&token="+gettoken();
            if(vipType==0){
                $(".wechat_video_show wechat_video_vip").show();
                $(".wechat_video_show wechat_video_novip").hide();
                $("#img1").attr("src",codeurl);
                $("#img2").attr("src",codeurl);
                $(".ui-wechat_video").toggleClass("ui-wechat_video_cur");
                $(".wechat_video_vip").toggle();                 
            }else{
                $(".wechat_video_show wechat_video_vip").hide();
                $(".wechat_video_show wechat_video_novip").show();
                $("#img1").attr("src",codeurl);
                $("#img2").attr("src",codeurl);
                $(".ui-wechat_video").toggleClass("ui-wechat_video_cur");
                $(".wechat_video_novip").toggle();
            }
        }

         //获取手机扫码看二维码
        $(".viewPhone").hover(function(){               
            var mebileLookcodeurl = "http://aplus.pptv.com/qrcode/?value=http://v.pptv.com/show/ibQHkY8sxod9CwAw.html";
            $("#mobileLook-qrcode").attr("src",mebileLookcodeurl);
            $("#mobileLook-filmname").html("南极大冒险");       
        });
       
});
</script>

