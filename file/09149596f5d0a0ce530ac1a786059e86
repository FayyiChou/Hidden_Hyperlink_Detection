    <!-- 评论组件-js -->
    <!--这一段js要紧接webcfg的后面-->
<script>
    var param = {
        afterCommentLoaded: function(data){},   //评论加载完成后执行
        immediateLoad: true    //是否立即加载评论，true\false，立即加载\延迟加载
    };
    seajs.use(['app/comment/commentv3'],function(comment){
        comment.init(param);
    });
</script>
    <!-- 播放器各种事件监听及处理-js -->
    
<script type="text/javascript">
seajs.use(['jquery','login','user','util/cookie','pub/user-data','util/json','util/history'],function($,login,user,cookie,userData, JSON, history){
        var count = 0;
        var marTop;
        window.gjcnum='';
        
        if($("#body").hasClass("grid-1390")){
                    marTop = -645;
                }else if($("#body").hasClass("grid-1210")){
                    marTop = -545;
                }else if($("#body").hasClass("grid-990")){
                    marTop = -425;
        }
        $(window).resize(function(){
                if($("#body").hasClass("grid-1390")){
                    marTop = -645;
                }else if($("#body").hasClass("grid-1210")){
                    marTop = -545;
                }else if($("#body").hasClass("grid-990")){
                    marTop = -425;
                }
            playW = $(".play_wapper").width();
            if(window.gjcnum==0){
            $(".playR").css({"margin-top":marTop});
            }
        });

        window.callPlayer = function(){
            webcfg.player.createConfig.playerbox = document.getElementById('pptv_playpage_box');
            window.PLAYER1 = new PPTVPlayer(webcfg.player.createConfig);
            count ++;
            if(window.PLAYER1){
                     var player = window.PLAYER1;
                     var box = $('#pptv_playpage_box');
                     isWide = true;

                     var boxclass = box.attr("className");
                     console.log(boxclass);
                     if(!boxclass)   boxclass ="";

                     if(boxclass.indexOf('playbox_w660')>-1){
                         isWide = false;
                     }

                     player.onModeChanged.add(function(arg){
                          if(arg==6){
                                box.attr('className','playbox_w660');
                                isWide = false;
                           }else if(arg==5){
                                box.attr('className','playbox_w960');
                                isWide = true;
                           }
                     }); 

                     var _hasCallStop = false;
                     player.onError.add(function(arg){
                          var code = parseInt(arg.code); 
                          if(code==3004||code==3005){
                                 _hasCallStop = true;
                          }
                     });

                     //播放下一集
                     player.onPlayStateChanged.add(function(arg){
                             if(arg=='8' && !_hasCallStop){                        
                                 if(webcfg.next_url){
                                      window.location = webcfg.next_url;
                                  }else {         
                                                                   
                                  }
                             }else if(arg=='7'){
                                  _hasCallStop = true;
                             }else{
                                  _hasCallStop = false;
                             }
                     });

                     function _call3008(action){
                          location.href = location.href;
                     }

                     //剧场模式
                     player.onRegister('theatre', function(data) {
                          var mode= data['body']['data']['mode'];
                          window.gjcnum=mode;//作为全局变量
                          playForTheatre(mode);              
                     });

                     function playForTheatre(jcnum){
                         if(jcnum==1){
                            $(".playR").animate({"margin-top":0});
                            $(".play_info").css({"right":0});
                            $(".packleft").fadeOut();
                            $(".wp-grid").addClass("wp-grid-curr");		                  
                         }else if(jcnum==0){
                            $(".playR").animate({"margin-top":marTop});
                            $(".packleft").fadeIn();
                            $(".wp-grid").removeClass("wp-grid-curr");
                            if(now==1){
                                $(".play_info").css({"right":-310});
                            }		              
                         }
                         navH = $(".wapper5").offset().top;
                     };

                    //免费试看
                    player.onError.add(function(arg){
                       if(arg.code==3006){
                            //提示付费                        
                            box.css({"height":"0","width":"0"});   
                            $("#buy_box").show();
                       }else if(arg.code==3008){                        
                            JoinVip.showJoinInfo(null,$('#pptv_playpage_box').position().y);
                       }else if(arg.code == 2001 || arg.code == 3001){
                            if(count <=3){
                                 callPlayer();
                            }
                       }
                    });

                    //蓝光加速
                    var urls = {
                          speedUp: 'http://viptv.pptv.com/vipprivilege/spdup'
                    };
                    window.playerVipSource = -1; //播放器内跳广告类型标识 跳广告 蓝光 加速
                    player && player.onVipValidate && player.onVipValidate.add(function(s) {
                        playerVipSource = s;    //vip登录来源,全局变量，用于setuserinfo
                        var logimg = new Image();
                        window['_temp_img_' + +new Date()] = logimg;
                        logimg.src = 'http://pay.vip.pptv.com/paylogin?web_ad=tggg&' + (+new Date()); //统计请求
                        if (window.pluginFlyobject) { //如果有小窗飞出，关掉,仅针对32IE浏览器内核
                            try {
                                  pluginFlyobject.CloseFlyOutWnd();
                            } catch (e) {}
                        }
                        if (s === '2') {
                            JoinVip.changeFixframe('270');
                            JoinVip.showJoinInfo(urls.speedUp);
                            return;
                        }         
                        if (s === "5") { //播放器内置登录
                            login.check();
                            return;
                        }
                        JoinVip.showJoinInfo(null);
                    });

                    //云同步
                    playPos = getQueryString(window.location.href, 'rcc_starttime') || 0;

                    var channel_pid = "";
                    if(webcfg.pid == 0){
                         channel_pid = webcfg.id;
                    }else{
                         channel_pid = webcfg.pid;
                    }
                    window.writeVideoInfo = function(pos) {
                         var videotype = 3;
                         var localHistory = history.PlayHistory; //本地记录
                         var HistoryObject = {
                               Id: webcfg.channel_pid || webcfg.id, //合集ID，分集ID
                               SubId: webcfg.id, //必须  分集ID
                               Pos: pos || 0
                         };

                        var localHistoryData = {
                             Id: webcfg.channel_pid || webcfg.id,
                             SubId: webcfg.id,
                             Name: webcfg.title,
                             Pos: pos || 0,
                             Duration: webcfg.duration,
                             link: webcfg.id_encode ? webcfg.id_encode : parseIdCode(location.href),
                             VideoType: videotype, //视频类型videoType和上次观看时间_mt
                             _mt: "" + (+new Date())
                        };

                        if (!login.isLogined()) {
                             //本地历史记录
                             yunTime = 60 * 1000;
                             if (localHistory) {
                                 localHistoryData._mt = "" + (+new Date());
                                 localHistory.add(localHistoryData);
                             }
                        } else {
                             //云同步...
                             yunTime = 3 * 60 * 1000;
                             var PlayHistory = userData.PlayHistory; //需要重新写代码
                             if (webcfg.channel_pid === 0 && webcfg.id === 0) {
                                 log('数据错误，PID 且 ID 不能为0，不执行云同步！');
                                 return;
                             }

                             PlayHistory.go({
                                 type: 'Recent',
                                 cid: webcfg.channel_pid || webcfg.id,
                                 parms: {
                                     from: 'web',
                                     _method: 'post',
                                     _json: decodeURIComponent(JSON.stringify(HistoryObject)),
                                     tk: cookie.get('ppToken') || ''
                                 }
                             }, function(d) {
                                 log('云同步成功...', d.errMsg, d);
                             });
                        }
	          };
			
                  function parseIdCode(url) {
                       var m = url.match(/.*\/(.*?)\.html/);
                       return m ? m[1] : null;
                  }

                  function getQueryString(str, name) {
                       var reg = new RegExp("(^|\\?|&)" + name + "=([^&]*)(\\s|&|$)", "i");
                       if (reg.test(str)) return unescape(RegExp.$2.replace(/\+/g, " "));
                  }

                  player.onProgressChanged.add(function(args) {
                       playPos = args.timeloaded || '0';
                  });

                  writeVideoInfo(playPos);
                  setInterval(function() {
                         writeVideoInfo(playPos); //云同步历史记录
                  }, yunTime);

                  window.onbeforeunload = function() {
                      try {
                            // 添加播放记录
                           writeVideoInfo(playPos);                      
                      } catch (e) {}
                  };
             }
        }

         callPlayer();
         favoriteOperator("check");

         user.loginEvents.add(function(){
              $("#buy_box").hide();
              $("#pptv_playpage_box").show();
              $("#pptv_playpage_box").css({"height":"100%","width":"100%"}); 
              callPlayer();
              buyInfoLayer();
              favoriteOperator("check");
              var payurl = "http://pay.vip.pptv.com/ddp_pay_saoma?aid=vod_saoma&username="+getcookie();
              var codeurl = "http://api.passport.pptv.com/getQrcodeByUrl.do?url="+encodeURIComponent(payurl)+"&username="+getcookie()+"&token="+gettoken();	
              $("#saoma_code_login").attr("src",codeurl);
              $("#saoma_login").show();
              $("#saoma_nologin").hide();
         });  

         user.logoutEvents.add(function(){ 
              $("#buy_box").hide();
              $("#pptv_playpage_box").show();
              $("#pptv_playpage_box").css({"height":"100%","width":"100%"}); 
              callPlayer();
              buyInfoLayer();
              favoriteOperator("check");
              $("#saoma_nologin").show();
              $("#saoma_login").hide();
              vipType=0;
         });       
});
</script>
    <!-- 播放器小窗模式-js -->
    <script type="text/javascript">
seajs.use(['jquery'],function($){       			
        //滚动播放器显示小屏，可拖动
        var navH = $(".wapper5").offset().top;  //视频下面div距上边框的间隔
        var playW = $(".play_wapper").width();  //页面主题内容宽度
        var h = $(window).height();             //窗口高度
        var w = $(window).width();              //窗口宽度
        var _h = 170;                           //缩小播放器高度
        var _w = 300;                           //缩小播放器宽度
        var scroH;                              //页面滚动高度
        var Vbox = $("#pptv_playpage");         //播放器窗口
        var Vshade = $(".playmask");            //播放器遮罩
        var move = false;                       //是否移动过小窗口
        var Small = false;                      //是否显示小窗口的hover
        //播放器窗口初始化样式
        Vbox.css({"width":"100%","height":"100%","position":"static","top":0,"left":0,"margin":"0","padding":"0","background":"#000","z-index":"20","cursor":"Default"});
        //页面滚动触发事件
        $(window).scroll(function(){
            scrollwin();
        });
        //窗口改变触发事件
        $(window).resize(function(){
            //初始化
            navH = $(".wapper5").offset().top;
            playW = $(".play_wapper").width();
            h = $(window).height();
            w = $(window).width();
            move = false;
            Small = false;
            scrollwin();
        });
        //滚动事件
        function scrollwin(){
            scroH = $(this).scrollTop();
            if(scroH>=navH){
                if(move){
                    Vbox.css({"position":"fixed","top":y,"left":x,"width":_w,"height":_h,"z-index":"99999","border":"solid 9px #363636","cursor":"move"});
                }else{
                    Vbox.css({"position":"fixed","top":h-_h-18,"left":w/2+playW/2-301,"width":_w,"height":_h,"z-index":"99999","border":"solid 9px #363636","cursor":"move"});
                }
                Small = true;
                movevideo();
            }else if(scroH<navH){
                Small = false;
                Vshade.hide();
                Vbox.css({"position":"static","top":0,"left":0,"width":"100%","height":"100%","margin":"0","padding":"0","border":"0","cursor":"Default"});
            }
        }
        //移动事件
        function movevideo(){
            var _move = false;//移动标记
            Vbox.hover(function(e){
                if(Small){Vshade.show();}
            },function(e){
                if(Small){Vshade.hide();}
            });	
            Vbox.mousedown(function(e){
                if(Small){
                    _move = true;
                    move = true;
                    _x=e.pageX-parseInt(Vbox.css("left"));
                    _y=e.pageY-parseInt(Vbox.css("top"));
                }
            });
            $(document).mousemove(function(e){
                if(_move){
                    x=e.pageX-_x;//移动时根据鼠标位置计算控件左上角的绝对位置
                    y=e.pageY-_y;
                    if(x<=0){x=0;}
                    if(y<=0){y=0;}
                    if(x>=w-_w){x=w-_w;}
                    if(y>=h-_h){y=h-_h;}
                    Vbox.css({top:y,left:x});
                }
            }).mouseup(function(){
                if(Small){
                    _move=false;
                }
            });
        }           			
});
</script>
