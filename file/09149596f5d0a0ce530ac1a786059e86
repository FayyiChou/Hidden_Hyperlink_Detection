    <!-- 用户登陆等功能公共碎片-js-->
    <script type="text/javascript">            
seajs.use(['jquery','login','user'],function($,login,user){

window.getcookie=function(){
        
         var domain="pptv.com"; 
         var PPName="";
         var cookie = document.cookie;
         var arrycookie = cookie.split(";"); 
         for(var i=0; i<arrycookie.length; i++){    
                 try
                 {
                     var arrcookie = arrycookie[i].split("="); 
                     var cookieName = arrcookie[0].replace(/(^\s*)|(\s*$)/g,"");
                 }
                 catch(err)
                 {
                     var cookieName = "0";
                 }

                 if(cookieName=="PPName"){
                     PPName = arrcookie[1].split("$")[0];

                 } 
         }
              PPName =PPName.split("%24")[0];
         return PPName;
   }

window.gettoken=function(){
     
    
  var domain="pptv.com"; 
  var cookie = document.cookie;
  var arrycookie = cookie.split(";");
  var Token="";
  for(var i=0; i<arrycookie.length; i++){    
        try
        {
            var arrcookie = arrycookie[i].split("="); 
            var cookieName = arrcookie[0].replace(/(^\s*)|(\s*$)/g,"");
            
        }
        catch(err)
        {
            var cookieName = "0";
        }
        if(cookieName=="ppToken"){
            Token = arrcookie[1];
        } 
    }
        return Token;
}         

window.ajaxGet=function(url,async,datatype,callback){
         if(!url) return;
		 $.ajax({
              url:url,
		      type:"GET",
			  async:async,
			  dataType:datatype,
			  success:function(data){
			        callback(data);
			  }
		 });	
}

window.checklogin = function(word){
            
		if(!user.isLogined) {	
			if(typeof(word) != "undefined" && word != ""){
				login.check(function() {}, {
					type: "login",
					from: "collect",
					tip: encodeURIComponent(word)
				});
			}else{
				login.init();  
			}
			return false;
			//user.loginEvents.add(afterlogin);  
		}else return true;
        }

window.loginstatus=function(){
          
		if(!user.isLogined){
                      return false;		
		}else{
                      return true;
                }
        }

//观影券订单id
window.genOutOrderId=function(){
        var timestamp = new Date().getTime();      
        return timestamp+Math.random(100);
}

$("#scoremark .star").click(function(){
      if(checklogin()){
          $("#score").show();
          $("#pptv_playpage_box").css({"height":"0","width":"0"});      
      }
});
    
window.share=function(num){
    var wbpic="http://img24.pplive.cn/2008/05/30/10413924514_230X306.jpg";
    var sharetext="南极大冒险";
    var sharetitle="";
    var shareurl=window.location.href;
    window.openurl;
   if(num==1){
       window.openurl="http://share.v.t.qq.com/index.php?c=share&a=index&title="+encodeURIComponent(sharetext)+"&url="+encodeURIComponent(shareurl)+"&pic="+encodeURIComponent(wbpic)+"&appkey=dcba10cb2d574a48a16f24c9b6af610c";}
   if(num==2){
       window.openurl="http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url="+encodeURIComponent(shareurl)+"&title="+encodeURIComponent(sharetitle)+"&pics="+encodeURIComponent(wbpic)+"&summary=" + encodeURIComponent(sharetext);}
   if(num==3){
       window.openurl="http://v.t.sina.com.cn/share/share.php?c=spr_web_bd_pplive_weibo"+"&url=" +encodeURIComponent(shareurl)+ "&title=" + encodeURIComponent(sharetext)+ "&source=PPLive网络电视"+ "&sourceUrl=" +encodeURIComponent(shareurl)+ "&content=utf-8"+ "&appkey=1938876518"+"&pic="+encodeURIComponent(wbpic);}
   if(num==4){
       window.openurl="http://widget.renren.com/dialog/share?resourceUrl="+encodeURIComponent(shareurl)+"&title="+encodeURIComponent(sharetitle)+"&image="+encodeURIComponent(wbpic)+"&description="+encodeURIComponent(sharetext);}        
   if(num==5){
       window.openurl="http://www.douban.com/share/service?title="+encodeURIComponent(sharetitle)+"&href="+encodeURIComponent(shareurl)+"&image="+encodeURIComponent(wbpic)+"&url="+encodeURIComponent(shareurl);
    } 
   if(num==6){
       window.openurl="http://tieba.baidu.com/f/commit/share/openShareApi?url="+encodeURIComponent(shareurl)+"&title="+encodeURIComponent(sharetitle)+"&desc="+encodeURIComponent(sharetext)+"&pic="+encodeURIComponent(wbpic);
    }
        window.open(openurl);                   

} 

window.toDesktop = function(sUrl,sName){ 
       try 
       { 
         var WshShell = new ActiveXObject("WScript.Shell"); 
         var oUrlLink = WshShell.CreateShortcut(WshShell.SpecialFolders("Desktop") + "\\" + sName + ".url"); 
         oUrlLink.TargetPath = sUrl; 
         oUrlLink.Save(); 
       } 
       catch(e) 
       { 
          alert("当前浏览器不允许该操作！"); 
       } 
}

window.favoriteOperator = function(method){
	if(loginstatus()){
		$.ajax({
            url: "http://vip.pptv.com/show/favorite/?channelid=25180921&method="+method+"&username="+getcookie()+"&token="+gettoken(),
            type: "GET",
            async: true,
            success: function(data) {
				$(".collect a").remove();
				$(".collect").prepend(data);
				if($(".collect a").is(".currer")){					
					$(".collect a").click(function(){
						favoriteOperator("del");
					});
				}else{
					$(".collect a").click(function(){
						favoriteOperator("add");
					});
				}
            }
        });
    }else{
		$(".collect a").remove();
		$(".collect").prepend('<a class="" href="javascript:void(0);" onclick=checklogin("亲，需要登录后才能收藏此节目哦");>收藏</a>');
	}	                           
}
});
</script>
