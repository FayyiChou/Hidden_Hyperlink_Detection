	$("#tab1").hide();
	$("#tab2").show();    
}else{  
	$(".play_mune li").click(function(){
		$(".play_info .tabs_info").hide();
		$(".play_mune li").attr("class","");
		$(this).attr("class","mcur"); 
		$("#"+$(this).find("a").attr("data-herf")).show();
		return false;
	});    
}

//高亮选中样式后定位
if(vt==21 || vt==22) {
	var nowTop = $(".now").position().top;
	var videoH = $(".videolist_container").height();
	var jcH = $(".jscroll-c").height();
	if(jcH-nowTop<=videoH){
		$(".jscroll-c").animate({"top":videoH-jcH});	
	}else{
		$(".jscroll-c").animate({"top":-nowTop});
	}
}

$("#no_resource_close").click(function(){
      $("#no_resource").hide();
      $("#pptv_playpage_box").css({"height":"100%","width":"100%"});                             
});

