<!-- 播放页页面自适应宽度功能JS(必须紧挨着body)-->
<script>
//初始化状态显示样式判断
var winWidth = document.body.clientWidth;
if (winWidth <= 1230) {
	body.className = "grid-990";
} else if (winWidth <= 1410) {
	body.className = "grid-1210";
} else if (winWidth > 1410) {
	body.className = "grid-1390";
} else {
	alert("do not know!");
}

window.onresize=function(){
var winWidth = document.body.clientWidth;
if(winWidth <=1230){
    body.className="grid-990";	   
}else if(winWidth<= 1410){
    body.className="grid-1210";
}else if(winWidth>1410){
    body.className="grid-1390";
}else {
    alert("do not know!");
}
}
</script>
