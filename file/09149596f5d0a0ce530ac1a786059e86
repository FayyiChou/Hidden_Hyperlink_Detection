$(".vip_novbox_blackbg").hide();
$(".lefttxt li").eq(1).addClass("cur");

//菜单hover效果
$(".lefttxt li").mouseenter(function(){
	$(this).addClass("cur").siblings("li").removeClass("cur");
	$(this).find(".submune").show();
	if($(this).index()==1){
		$(".vip_novbox_blackbg").show();
	}
	var x = $(this).find(".submune a").size();
	var w = $(this).find(".submune a").outerWidth(true);
	$(this).find(".submune").width(w*x);
});

$(".lefttxt li").mouseleave(function(){
	$(".lefttxt li").removeClass("cur").eq(1).addClass("cur");
	$(this).find(".submune").hide();
	$(".vip_novbox_blackbg").hide();
});

//如果父id和channelid一样，那就高亮第一集
