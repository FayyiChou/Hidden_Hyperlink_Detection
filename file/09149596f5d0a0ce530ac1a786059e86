<script type="text/javascript">
      function load(url, urlhash, callback, scope) {
            var head = document.getElementsByTagName("head")[0];
            var ct = document.createElement('script');
            ct.type = 'text/javascript';
            //组合查询字符
            var a = [];
            for (var p in urlhash) {
                a.push([p, '=', urlhash[p]].join(''));
            }
            //动态创建回调函数
            load.maxix++;
            var cb = "cb_" + load.maxix;
            load.cbs[cb] = function() {
                callback.apply(scope, arguments);
            }
            ct.src = url + 'cb=load.cbs.cb_' + load.maxix + (a.length > 0 ? '&' + a.join('&') : '');
            head.appendChild(ct);
        }
        load.maxix = 0;
        load.cbs = {};

    var vipType = 0;
    var vipPrice = 0;
    seajs.use(['jquery', 'login', 'user'], function($, login, user) {

