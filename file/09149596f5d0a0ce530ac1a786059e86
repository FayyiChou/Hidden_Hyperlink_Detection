    <!--下拉菜单js-->
    <!--VIP导航公用JS-->
    <script>
	var vipnum=1 ;//当前一级导航指定ID选中样式
	var vipsubnum;//当前二级导航指定ID选中样式，不需要的话不用赋值
    </script>
    <script>
//公用JS
seajs.use(["jquery"],function($){
    //当前一级导航选中样式
    $(".lefttxt li").eq(vipnum).addClass("cur");

    //获取子菜单宽度
    $(".submune").each(function(index, element) {
        var x = $(this).find("a").size();
        var w = $(this).find("a").outerWidth(true);
        $(this).width(w*x);
    });
    //菜单hover效果
    $(".lefttxt li").mouseenter(function(){
        $(this).addClass("cur").siblings("li").removeClass("cur");
        $(".submune,.vip_novbox_blackbg").hide();
        if($(this).index()==1){ //只有会员影院下面有下拉菜单
            $(this).find(".submune").show();
            $(".vip_novbox_blackbg").show();
        }
    });
    $(".lefttxt li").mouseleave(function(){
        $(".lefttxt li").removeClass("cur").eq(vipnum).addClass("cur");
        $(".submune,.vip_novbox_blackbg").hide();
    });
    //当前子菜单选中样式
    $(".lefttxt li").eq(vipnum).find(".submune").find("a").eq(vipsubnum).addClass("subcur"); 
});
//子菜单始终显示函数
window.alwaysshow =function(vipnum){
    seajs.use(["jquery"],function($){
        $(".lefttxt li").mouseleave(function(){
            $(".lefttxt li").removeClass("cur").eq(vipnum).addClass("cur").find(".submune").show();
            $(".vip_novbox_blackbg").show();
        });
        $(".lefttxt li").eq(vipnum).find(".submune").show(); //解除导航菜单hover效果,子菜单始终显示
        $(".vip_novbox_blackbg").show(); //下拉菜单背景显示
    });
}
</script>
