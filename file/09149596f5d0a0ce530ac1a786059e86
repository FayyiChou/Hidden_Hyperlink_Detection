buyInfoLayer();


        function genOutOrderId() {
            var timestamp = new Date().getTime();
            return timestamp + Math.random(1000);
        }

        //观影券兑换影片
        window.exchange = function(channelid) {
            if (!checklogin()) return;
            $.getJSON("http://ddp.vip.pptv.com/ticket_exchange/?username=" + getcookie() + "&contenttype=0&contentid=" + channelid + "&token=" + gettoken() + "&outorderid=" + genOutOrderId() + "&format=jsonp&cb=?", function(data) {
                if (data["errorCode"] == "0") {
                    $("#useTicketLayer").show();
                } else if (data["errorCode"] == "117") {
                    //alert("已经兑换过");			
                }
                $("#use_ticket").hide();
                $("#ticketLayer").hide();
                $("#buy_box").hide();
                $("#pptv_playpage_box").css({
                    "height": "100%",
                    "width": "100%"
                });
                callPlayer();
                setTimeout(buyInfoLayer, 800);
            });
        }

        $("#use_ticket_close,#use_ticket_confirm").click(function() {
            $("#useTicketLayer").hide();
            $("#pptv_playpage_box").css({
                "height": "100%",
                "width": "100%"
            });
        });

        //单片购买
        window.payfilm = function(buyurl) {
            $.ajax({
                type: "GET",
                dataType: "jsonp",
                url: "http://api.ddp.vip.pptv.com/buyed/isvalid?username=" + getcookie() + "&channelid=25180921&v=2.0&format=jsonp&cb=?",
                success: function(data) {
                    if (data.errorCode == 0) {
                        if (data.buyedExpired == false && data.userBuyed == true) {
                            alert("您已经购买过该影片，无需再次购买！");
                        } else {
                            window.open(buyurl, "_blank");
                            //window.location.href= buyurl; 
                        }
                    }
                }
            });
        }

        window.showticketLayer = function() {
            $("#pptv_playpage_box").css({
                "height": "0",
                "width": "0"
            });
            $('#ticketLayer').show();
        }

        $(".btn_use").click(function() {
            exchange("25180921");
        });

        $(".mainclose,.btn_cancel").click(function() {
            $(".bombBox").hide();
            $("#ticketLayer").hide();
            $("#pptv_playpage_box").css({
                "height": "100%",
                "width": "100%"
            });
        });

        //根据登陆状态显示不同的二维码，未登录就显示固定的二维码
        if(!user.isLogined) {
              $("#saoma_nologin").show();
              $("#saoma_login").hide();
        }else{
              var payurl = "http://pay.vip.pptv.com/ddp_pay_saoma?aid=vod_saoma&username="+getcookie();
              var codeurl = "http://api.passport.pptv.com/getQrcodeByUrl.do?url="+encodeURIComponent(payurl)+"&username="+getcookie()+"&token="+gettoken();		
              $("#saoma_code_login").attr("src",codeurl);
              $("#saoma_login").show();
              $("#saoma_nologin").hide();
        }

    });
</script>
    <!-- 播放器右侧栏收起打开-js，该碎片要放在购买信息tab页-js碎片的下面，否则会有问题  -->
    <script type="text/javascript">
//视频上展开和收起左侧区域按钮效果
define('flexible',function(require, exports, module) {
var $ = require('jquery');
$.fn.flex = function(options){
	var defaults = {
		playmode:2  //展示模式，1：剧场模式 2：正常模式
	};
	var options = $.extend(defaults,options);
	
	this.each(function(){
		var $body = $(this),
			$width = $body.width(),
			$videobox = $body.find(".play_video"),//左侧视频div
			$playinfo = $body.find(".play_info"),//右侧购买信息div
			$packbtn = $body.find(".packleft"),//收起的按钮
			$videoW = $videobox.width(),
			$palyW = 0;
        if($("#body").hasClass("grid-1390")){
            $palyW = 1390;
        }else if($("#body").hasClass("grid-1210")){
            $palyW = 1210;
        }else if($("#body").hasClass("grid-990")){
            $palyW = 990;
        }	
		$(window).resize(function(){
			if($("#body").hasClass("grid-1390")){
			$palyW = 1390;
		}else if($("#body").hasClass("grid-1210")){
			$palyW = 1210;
		}else if($("#body").hasClass("grid-990")){
			$palyW = 990;
		}	
		playwidthResize();
			function playwidthResize(){
				$videoW = $palyW-310;
				if(now==1){
					$videobox.css({"width":$palyW});
				}else if(now==0){
					$videobox.css({"width":$palyW-310});				
				}	
			};
		});
		//console.log(options.playmode);
		if(options.playmode==1){
			$packbtn.find("p").html("展<br/>开");			
			$packbtn.css({"left":"291px"},500);
			$playinfo.css({"right":"310px"},500);
			$videobox.css({"width":$palyW});
			now = 1;
		}else{
			$packbtn.find("p").html("收<br/>起");
			$packbtn.css({"left":"-19px"},500);
			$playinfo.css({"right":"0px"},500);
			$videobox.css({"width":$videoW});
			now = 0;
		}
		$packbtn.click(function(){
			if(now==0){
				$(this).find("p").html("展<br/>开");			
				$packbtn.css({"left":"291px"},500);
				$playinfo.css({"right":"310px"},500);
				$videobox.css({"width":$palyW});
				now = 1;
			}else if(now==1){
				$(this).find("p").html("收<br/>起");
				$packbtn.css({"left":"-19px"},500);
				$playinfo.css({"right":"0px"},500);
				$videobox.css({"width":$videoW});
				
				now = 0;
			}
		});	
	});
}
});
</script>

<script>
seajs.use(['jquery','flexible'],function($,flexible){
//视频详情展开和收起按钮
videoinfo($("#tool01"));
function videoinfo(videoinfo){
	var btnX=0;
	videoinfo.find(".btn_expland").click(function(){
		if(btnX==0){
			$(this).html("收起<i></i>").removeClass("show").addClass("hide");
			videoinfo.find(".intro-content").removeClass("intro-short");
			videoinfo.find(".shortinfo").hide();			
			btnX = 1;
		}else if(btnX==1){
			$(this).html("展开<i></i>").removeClass("hide").addClass("show");
			videoinfo.find(".intro-content").addClass("intro-short");
			videoinfo.find(".shortinfo").show();
			btnX = 0;
		}
	});
}

//推荐视频
photoscroll();
function photoscroll(){
	var $activityPicbox = $(".suggesPicbox"),
		$activityPic = $(".suggesPic"),
		$activityul = $(".suggesPic ul.part01"),
		$activityli = $(".suggesPic li"),
		$left = $(".sugges_Lbtn"),
		$right = $(".sugges_Rbtn"),
		$liwidth = $activityli.outerWidth(true),
		$lisize = $activityli.size(),
		$htmlall = $activityul.html(),
		lastTime = 0,
		oSpeed = 300;
		
	//复制
	$activityPic.append("<ul class='part02'></ul>");
	var $activityul2 = $(".suggesPic ul.part02");
	$activityul2.html($htmlall).css({"width":$liwidth*$lisize});
	$activityul.css({"width":$liwidth*$lisize});
	$activityPic.css({"width":$liwidth*$lisize*2});
	
	$(window).resize(function(){
		$activityPic.css({"margin-left":0});
	});
	
	//点击滚动事件
	$left.live("click",function(){
		var nowTime = new Date().getTime();
		if(nowTime - lastTime < oSpeed) return;
		else{
			if($activityul.position().left==0){
				$activityPicbox.scrollLeft($activityul2.position().left);
				var left = $activityPicbox.scrollLeft();
				left=left-$liwidth;
				$activityPicbox.animate({"scrollLeft":left},oSpeed);			
			}else{
				var left = $activityPicbox.scrollLeft();
				left=left-$liwidth;
				$activityPicbox.animate({"scrollLeft":left},oSpeed);
			}
			lastTime = nowTime;
		}
	});	
	$right.live("click",function(){
		var nowTime = new Date().getTime();
		if(nowTime-lastTime < oSpeed) return;
		else{
			if($activityul2.position().left<=0){
				$activityPicbox.scrollLeft($activityul2.position().left);
				var left = $activityPicbox.scrollLeft();
				left=left+$liwidth;
				$activityPicbox.stop(true).animate({"scrollLeft":left},oSpeed);			
			}else{
				var left = $activityPicbox.scrollLeft();
				left=left+$liwidth;
				$activityPicbox.stop(true).animate({"scrollLeft":left},oSpeed);
			}
			lastTime = nowTime;
		}
	});		
}

//星星评分
starScore($(".scoremark"));
function starScore(star){
	star.find(".star ul li a").mouseenter(function(){
		var txt = $(this).attr("data-name");
		var x = $(this).parent("li").index();
		star.find(".tips").html(txt).css("left",-10+x*24).show();
	});
	star.find(".star ul li a").mouseleave(function(){
		star.find(".tips").html("").css("left",0).hide();
	});	
}

//切换视频详情和分享的js
tabsJs($(".detial_video"),'hover');
function tabsJs(tabs,x){
	tabs.find("#tabs li").on(x,function(){
		tabs.find(".tabs_info").hide();
		tabs.find("#tabs li").attr("class","");
		$(this).attr("class","mcur"); 
		var idx = $(this).index();
		tabs.find(".tabs_info").eq(idx).show();
		return false;
	});	
}

});
</script>    <script>
seajs.use(['jquery', 'login', 'user'], function($, login, user) {
	var channelid = "25180921";
    var username = user.info.UserName, //user模块中获取用户名
        token = user.info.token;       //user模块中获取token
	window.qrcodepay = function(){
        $.getJSON("http://api.ddp.vip.pptv.com/qrcodepay/create?username="+username+"&token="+token+"&from=dippay&contenttype=0&contentid="+channelid+"&sectionid=0&fromchannel=%26plt%3Dweb&format=jsonp&callback=?",function(data) {
        	if (data.errorcode== 0) { 
	        	window.qrcode = data.qrcode;
	        	console.log(window.qrcode);
	            var codeurl = "http://api.ddp.vip.pptv.com/qrcodepay/getimage?username="+username+"&token="+token+"&qrcode="+qrcode+"&height=240&width=240";
	            console.log(codeurl);
	            $("#qrcodepaypic").attr("src",codeurl); //赋值扫码src
	            $("#qrcodepaydiv").show(); //扫码弹框展示
	            autoQrcodeQuery = setInterval(QrcodeQueryinfo, 2000); //加定时器两秒轮询一次查询用户扫码状态
            }
        });
    };

    //查询二维码状态接口
    function QrcodeQueryinfo(){
        $.ajax({
            type:"get",
            dataType:"jsonp",
            url:"http://api.ddp.vip.pptv.com/qrcodepay/scanstatus?username="+username+"&token="+token+"&qrcode="+qrcode+"&format=jsonp&cb=?",
            success:function(data) {                
                if (data.errorcode== 0) {                          
                    if(data.scanStatus == '2'){//已创建订单
                        //console.log("创建订单");
                        $("#qrscstate").show();
                    }else if(data.scanStatus == '3'){//过期
                        //console.log("已过期");
                        $("#qrbettering").show();
                        clearInterval(autoQrcodeQuery);      
                    }else if(data.scanStatus == '4'){//已支付,页面刷新
                        window.location.reload();        
                    }
                }else if(data.errorcode== 13){
                	//console.log("已过期");
                    $("#qrbettering").show();
                    clearInterval(autoQrcodeQuery);
                }
            }
        });
    }

    //二维码上点击刷新 >> 重新获取二维码，并且轮询定时器
    $("#qrbettering").click(function() {
    	$("#qrcodepaypic").attr("src",""); //扫码图制空
    	$("#qrbettering").hide();//点击刷新浮层隐藏
    	qrcodepay();
    });

    //点击关闭按钮 >> 浮层隐藏和清除定时器
    $("#qrcodepayclose").click(function() {
    	$("#qrcodepaydiv").hide();
    	clearInterval(autoQrcodeQuery);
    });
});
</script>
    <!-- 根据用户类型确定右侧栏打开收起-js -->
    
<script type="text/javascript">
seajs.use(['jquery','login','user', 'flexible'],function($,login,user, flexible){
	var vt = 3;
	var playnum = 2;
	if(user.isLogined){
		//console.log("用户已经登陆,vipType="+vipType+",vt == "+vt+", vipPrice == "+vipPrice);
		if(vipType == 0){
			$("#goumai_liebiao").attr("class", "mcur");
			$("#juji_liebiao").attr("class", "");
			$("#tab1").hide();
			$("#tab2").show();
		}else{
			if(vipPrice == 0 && vt == 3){//会员免费影片进入剧场模式不打开右侧栏
				//console.log("进入剧场模式");
				playnum = 1;
			}else if(vipPrice > 0 && (vt == 3 || vt == 21)) {//会员付费影片/剧集进入正常模式打开购买信息
				$("#goumai_liebiao").attr("class", "mcur");
				$("#juji_liebiao").attr("class", "");
				$("#tab1").hide();
				$("#tab2").show();			
			}
		}
	}else{
		//console.log("用户未登陆,vt == "+vt);
		if(vt == 21 || vt == 22){
			$("#goumai_liebiao").attr("class", "mcur");
			$("#juji_liebiao").attr("class", "");
			$("#tab1").hide();
			$("#tab2").show();
		}
	}
	$("body").flex({
		playmode:playnum   //展示模式，1：剧场模式 2：正常模式 //剧场模式默认是要收起，正常模式默认是展开
	});	
});
</script>
