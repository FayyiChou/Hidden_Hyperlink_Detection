<script language="javascript">
//var patternSegments = {"fr":0,"ai":1,"bt":2,"hu":3,"ri":4,"ll":5,"lh":6,"bl":7,"bh":8,"pl":9,"ph":10,"":11,"fl":14,"ad":15,"re":16,"lo":17,"zs":18,"or":19,"sp":20,"ha":21,"cc":22,"sw":23,"st":24,"at":25,"tg":26,"to":27};
var patternSegments = {"fr":0,"ai":1,"hu":2,"pl":3,"ph":4,"bl":5,"bh":6,"ri":7,"tg":8,"to":9,"ll":10,"lh":11,"fl":12,"ad":13,"bt":14,"ha":15,"di":16,"dd":17,"pi":18,"hs":19,"zs":20,"or":21,"lo":22,"sw":23,"st":24,"at":25,"sc":26,"mp":27,"so":28};
function focusAction(Id){
var value = $("#"+Id).val();
if(value=="输入小区名称、地址或房源标题"||value=="请输入小区名或地址"||value=="输入小区首字母或名称"){
$("#"+Id).val("");
}
$("#"+Id).css("color","#000");
}
function blurAction(Id){
var value = $("#"+Id).val();
if(value==""){
if(Id=="addressSec"){
$("#"+Id).val("请输入小区名或地址");
}else if(Id=="address"){
$("#"+Id).val("输入小区名称、地址或房源标题");
}else if(Id == "priceSearch"){
$("#"+Id).val("输入小区首字母或名称");
}
$("#"+Id).css("color","#999");
}
}
$(function(){
/* resize_images("resizeleft");
resize_images("resizeright"); */
$("#chengxin").bind("ad-loaded", function(){
if($("#chengxin .ad-item").size() > 0){
$("#chengxin").show();
}
});
});
function resize_images(className){
$("."+className).each(function(){
var imgWidth = $(this).width();
var imgHeight = $(this).height();
var bbs_width = 160;
var bbs_height = 120;
if(className == 'resizeright'){
bbs_width = 190;
bbs_height = 145 ;
}
if(imgWidth>imgHeight){
if(imgWidth >= bbs_width){
var imgHeight = (bbs_width*$(this).height())/imgWidth;
var margins = parseInt((bbs_height-imgHeight)/2);
if(margins>0){
//$(this).attr("style","padding-top:"+margins+"px;");
}
$(this).height(imgHeight);
}
}else{
if(imgHeight >= bbs_height){
var imgWidth = (bbs_height*$(this).width())/imgHeight;
var margins = parseInt((bbs_width-imgWidth)/2);
if(margins>0){
//$(this).attr("style","padding-left:"+margins+"px;");
}
$(this).width(imgWidth);
}
}
});
}
