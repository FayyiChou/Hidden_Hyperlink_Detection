function urlRef(type){
var arrayObj = new Array();
var params = $.purl.params();
params = purlParams(params);
delete params["subscribeID"];
if(type=="buildArea"){
delete params["bl"];
delete params["bh"];
}else if(type=="price"){
delete params["pl"];
delete params["ph"];
}else if(type=="area"){
delete params["ai"];
delete params["zs"];
}else if(type=="layer"){
delete params["ll"];
delete params["lh"];
}else if(type=="address"||type=="ad"){
delete params["ad"];
delete params["fl"];
}else if(type=="linename"){
delete params["sw"];
delete params["st"];
}else if(type=="stationname"){
delete params["st"];
}else if(type=="ri"){
delete params["ri"];
delete params["tg"];
delete params["to"];
}else{
delete params[type];
}
for(var i in params){
if(params[i]){
var number = patternSegments[i];
arrayObj[number] = i+params[i];
}
}
var seoUrl="";
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/sale/search/"+arrayObj[i];
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
if(seoUrl==""||seoUrl=="/sale/search/fr1"||seoUrl=="/sale/search/fr2"){
window.location.href = "/sale/search/fr1.html";
}else{
window.location.href = seoUrl+".html";
}
}
function purlParams(params){
var url=$.purl.attr("path");
var re = /^\/sale\/search\/(.+)\.html/gi;
re.test(url);
var strings = RegExp.$1;
if(strings!=""){
var patternArray = strings.split("_");
for(var i=0; patternArray.length>i; i++){
var search = patternArray[i];
var key = search.substring(0,2);
var value = search.substring(2,search.length);
if(params[key]==null){
params[key]=value;
}
}
}
return params;
}
function goToFloor(){
var priceSearch = $("#priceSearch").val();
var floorId = $("#floorId").val();
if(floorId!=""&&floorId!=0){
window.open("http://xiaoqu.lw.fccs.com/"+floorId+".html");
}else if(priceSearch==""){
alert("请选择要查询的小区");
}else{
$.ajax(
{type : "post",
url : "/sale/doURLEncode.do",
cache : false,
async : false,
data : {"address":priceSearch},
success : function(data){
if(data!=""){
window.open("http://xiaoqu.lw.fccs.com/sale/ad"+data+".html");
}
},
dataType : "html"}
);
}
}
function ShowWindowWidthHeight(areaType){
$.ajax({
url: "/sale/areaJxPic.do",
data: {
"areaType": areaType
},
type: "POST",
success: function(data){
if(data!=""){
$(".mobile_detail").html(data);
$(".mobile_detail").show();
}else{
$(".mobile_detail").hide();
alert("该城市还未绘制区域图");
}
},
dataType: "html"
});
}
