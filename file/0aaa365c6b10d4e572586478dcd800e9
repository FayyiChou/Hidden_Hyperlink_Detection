divSum('houseageSelect','','','buildage','buildage');
$('#houseageSelect .p_m_items').mousedown(function(){
var url = window.location.href;
var arr1 = url.split("search/");
//var u1 = arr1[0]+"search/";
var arr2 = "";
var u2 = arr1[0];
if(arr1.length==2){
arr2 = arr1[1].replace(".html", "");
}
//var u3 = ".html";
var arr_u2 = arr2.split("_");
//参数拼接处理
var arrayObj = new Array();
for(var i=0;i<arr_u2.length;i++){
var item = arr_u2[i];
var key1 = item.substr(0, 1);
var key2 = item.substr(0, 2);
var value = item.substr(2);
var number = patternSegments[key2];
if(key2 != "ha" ){
arrayObj[number] = key2+value;
}
}
if($('#buildageId').val() > 0){
var curr = patternSegments["ha"];
arrayObj[curr] = "ha"+$('#buildageId').val();
}
//拼URL
var seoUrl="";
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/sale/search/"+arrayObj[i];
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
if(seoUrl==""){
window.location.href = "/house";
}else{
window.location.href = seoUrl+".html";
}
});
