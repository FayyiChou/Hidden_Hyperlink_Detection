$("#checkSearch").click(function(){
var arrayObj = new Array();
var params = $.purl.params();
params = purlParams(params);
params["ad"] = $("#address").val();
if(params["ad"] != null){
if(params["ad"] == "输入小区名称、地址或房源标题" || $("#address").val() == ""){
delete params["ad"];
delete params["fl"];
}else if($("#floorId").val()!=""){
if($("#floorId").val()==$("#oldFloorId").val()){
if($("#oldFloor").val() != $("#address").val()){
delete params["fl"];
}else{
params["fl"] = $("#floorId").val();
}
}else{
params["fl"] = $("#floorId").val();
}
}
}
//地址 楼盘 等中文 编码处理
for(var i in params){
if(params[i]){
var number = patternSegments[i];
if(i=="ad"){
$.ajax(
{type : "post",
url : "/sale/doURLEncode.do",
cache : false,
async : false,
data : {"address":params[i]},
success : function(data){
if(data!="")
arrayObj[number] = i+data;
},
dataType : "html"}
);
}else if(i=="fl"){
arrayObj[number] = i+params[i];
}else{
delete params[name];
}
}
}
var seoUrl="";
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/sale/search/"+arrayObj[i];
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
if(seoUrl!=""){
window.location.href = seoUrl+".html";
}
});
$("#fr1submit").click(function(){
var arrayObj = new Array();
var params = $.purl.params();
params = purlParams(params);
params["ri"] = $("#roomIdSearch").val();
params["tg"] = $("#hall").val();
params["to"] = $("#toilet").val();
for(var i in params){
if(params[i]){
var number = patternSegments[i];
arrayObj[number] = i+params[i];
}
}
var seoUrl="";
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/sale/search/"+arrayObj[i];
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
if(seoUrl!=""){
window.location.href = seoUrl+".html";
}
});
//提交查询
$("#fr2submit").click(function(){
var arrayObj = new Array();
var params = $.purl.params();
params = purlParams(params);
params["fr"] = "2";
//var patternSegments = {"fr":0,"ai":1,"hu":2,"pl":3,"ph":4,"bl":5,"bh":6,"ri":7,"tg":8,"to":9,"ll":10,"lh":11,"fl":12,"ad":13,"bt":14};
$("#areaId,#zoneSpell,#houseUseId,#priceLow,#priceHigh,#buildAreaLow,#buildAreaHigh,#roomId,#hall,#toilet,#layerLow,#layerHigh,#addressSec,#addressSearch,#floorId,#buildingTypeId,#lineId,#stationId").each(function(){
var name = "";
if($(this).attr("id")=="areaId"){
name = "ai";
}else if($(this).attr("id")=="zoneSpell"){
name = "zs";
}else if($(this).attr("id")=="houseUseId"){
name = "hu";
}else if($(this).attr("id")=="priceLow"){
name = "pl";
}else if($(this).attr("id")=="priceHigh"){
name = "ph";
}else if($(this).attr("id")=="buildAreaLow"){
name = "bl";
}else if($(this).attr("id")=="buildAreaHigh"){
name = "bh";
}else if($(this).attr("id")=="roomId"){
name = "ri";
}else if($(this).attr("id")=="hall"){
name = "tg";
}else if($(this).attr("id")=="toilet"){
name = "to";
}else if($(this).attr("id")=="layerLow"){
name = "ll";
}else if($(this).attr("id")=="layerHigh"){
name = "lh";
}else if($(this).attr("id")=="floorId"){
name = "fl";
}else if($(this).attr("id")=="addressSec"){
name = "ad";
}else if($(this).attr("id")=="lineId"){
name = "sw";
}else if($(this).attr("id")=="stationId"){
name = "st";
}
$("#roomId").val();
if($(this).val() != "" && $(this).val() != "0"){
params[name] = $(this).val();
}else{
delete params[name];
}
});
if(params["ad"] != null){
if(params["ad"] == "请输入小区名或地址" || $("#address").val() == ""){
delete params["ad"];
delete params["fl"];
}else if($("#floorId").val()!=""){
if($("#floorId").val()==$("#oldFloorId").val()){
if($("#oldFloor").val() != $("#addressSec").val()){
delete params["fl"];
}
}
}
}
if($("#floorId").val()!=""){
if($("#floorId").val()==$("#oldFloorId").val()){
if($("#oldFloor").val() != address){
seoUrl = seoUrl.replace("_fl"+floorId,"");
}
}
}
//地址 楼盘 等中文 编码处理
for(var i in params){
if(params[i]){
var number = patternSegments[i];
if(i=="ad"){
$.ajax(
{type : "post",
url : "/sale/doURLEncode.do",
cache : false,
async : false,
data : {"address":params[i]},
success : function(data){
if(data!="")
arrayObj[number] = i+data;
},
dataType : "html"}
);
}else if(i=="zs"){
$.ajax(
{type : "post",
url : "/sale/doURLEncode.do",
cache : false,
async : false,
data : {"address":params[i]},
success : function(data){
if(data!="")
arrayObj[number] = i+data;
},
dataType : "html"}
);
}else{
arrayObj[number] = i+params[i];
}
}
}
//拼URL
var seoUrl="";
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/sale/search/"+arrayObj[i];
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
window.location.href = seoUrl+".html";
});
$("#areaIdList").click(function(){
var arrayObj = new Array();
var params = $.purl.params();
params = purlParams(params);
var isflag = false;
for(var i in params){
if(params[i]){
if(i=="ai"||i=="zs"||i=="lo"||i=="sw"||i=="st"||i=="sc"||i=="mp"||i=="p"){
delete params["ai"];
delete params["zs"];
delete params["sw"];
delete params["st"];
delete params["lo"];
delete params["sc"];
delete params["mp"];
delete params["p"];
}else{
var number = patternSegments[i];
if(i=="at"){
arrayObj[number] = i+1;
}else{
isflag = true;
arrayObj[number] = i+params[i];
}
}
}
}
var seoUrl="";
if(isflag){
for(var i=0; arrayObj.length>i; i++){
if(arrayObj[i]!=""&&arrayObj[i]!=null){
if(seoUrl==""){
seoUrl="/sale/search/"+arrayObj[i];
}else{
seoUrl+="_"+arrayObj[i];
}
}
}
}
if(seoUrl==""){
window.location.href = "/sale/search/at1.html";
}else{
window.location.href = seoUrl+".html";
}
return false;
});
