$("#address").autocomplete("/sale/floorList.do",{
extraParams:{
pageno : "1",
floor:function(){return $("#address").val();}
},
scrollHeight:300,
parse:function(data){
var parsed = [];
var rows = data.items;
for (var i=0; i < rows.length; i++) {
var row = rows[i];
if (row) {
parsed[parsed.length] = {
value: row.floor,
data: row,
result: row.floor
};
}
}
return parsed;
},
width:258,
formatItem: function(row){
return "<table width=228><tr><td>"+row.floor+"</td><td width=100 align=right>(约<b>"+(row.saleCountHouse+row.saleCountVilla)+"</b>套)</td></tr></table>";
},
dataType : "json"
}).result(function(event, data, formatted){
$("#floorId").val(data.floorId);
$("#checkSearch").click();
});
$("#address").keydown(function(event){
var e = event || window.event;
if(e.keyCode == 13){
$("#checkSearch").click();
}
});
$("#addressSec").keydown(function(event){
var e = event || window.event;
if(e.keyCode == 13){
$("#fr2submit").click();
}
});
