$("#priceSearch").autocomplete("/sale/floorList.do",{
extraParams:{
pageno : "1",
floor:function(){return $("#priceSearch").val();}
},
scrollHeight:300,
parse:function(data){
var parsed = [];
var rows = data.items;
for (var i=0; i < rows.length; i++) {
var row = rows[i];
if (row) {
parsed[parsed.length] = {
value: row.floor,
data: row,
result: row.floor
};
}
}
return parsed;
},
width:125,
formatItem: function(row){
$("#floorId").val("");
return "<table width=128><tr><td>"+row.floor+"</td></tr></table>";
},
dataType : "json"
}).result(function(event, data, formatted){
$("#floorId").val(data.floorId);
});
