    <script type="text/javascript">
        $().ready(function () {
                getLastUpdateVideo("UCeGDTAvfu8YVbkJCp7TA1oQ");
        });


        function getLastUpdateVideo(id) {
            $.ajax({
                type: "GET",
                url: "/gateway.aspx",
                //url: "https://www.googleapis.com/youtube/v3/playlistItems",

                dataType: "json",
                data: {
                    g_url: "https://www.googleapis.com/youtube/v3/playlistItems",
                    part: "id,snippet,contentDetails",
                    playlistId: "UUeGDTAvfu8YVbkJCp7TA1oQ",  //uploads playlist of 黑貓
                    key: "AIzaSyDaBCCFtZ5OjFdaCFhys_ivalwkwqcRSgE",
                    maxResults: 1
                }
            })
          .done(function (ret) {
              //alert(ret.items.length);

              //get video detail
              if (ret.items.length >= 0) {
                  var LastUpdateVideoID = ret.items[0].snippet.resourceId.videoId;

                  //<iframe width="341" id="videoFrame" height="256" src="https://www.youtube.com/embed/eFROb9Iewic?rel=0&amp;controls=0&amp;showinfo=0" frameborder="0" allowfullscreen></iframe>
                  $("#videoFrame").attr("src", "https://www.youtube.com/embed/" + LastUpdateVideoID + "?rel=0&autoplay=1&showinfo=1");
              }
          });
        }    
    </script>


