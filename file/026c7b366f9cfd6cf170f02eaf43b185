    <script>
        $(window).load(function () {
            $("div#contentContainer").css("overflow", "hidden");
        });
    </script>
     

         
        <script type="text/javascript">
            var _isChanging = false;
            var _nowShow = -1;
            var _timerObj;


            function goNext() {
                if (!_isChanging) {
                    _isChanging = true;

                    //取得圖片張數
                    var imgCount = $("ul.bannerview li").length

                    //load圖
                    /*MM_preloadImages(
                            'img/index/segment/' + (((_nowShow + 1) % imgCount) + 1) + '_1.jpg',
                            'img/index/segment/' + (((_nowShow + 1) % imgCount) + 1) + '_2.jpg',
                            'img/index/segment/' + (((_nowShow + 1) % imgCount) + 1) + '_3.jpg',
                            'img/index/segment/' + (((_nowShow + 1) % imgCount) + 1) + '_4.jpg');*/

                    //放圖			
                   


                    //移除loading
                    /*if ($("#lodingBLOCK")) {
                        $("#lodingBLOCK").remove();
                    }*/



                    $("ul.bannerview li[idx='" + _nowShow + "']").css("z-index", 0);
                    $("ul.idxAddot li a").removeClass("on");
                    $("ul.idxAddot li").removeClass("on");
                    $("ul.idxAddot li[idx='" + ((_nowShow + 1) % imgCount) + "'] a").addClass("on");
                    $("ul.idxAddot li[idx='" + ((_nowShow + 1) % imgCount) + "'] a").parent().addClass("on");
                    $("ul.bannerview li[idx='" + ((_nowShow + 1) % imgCount) + "']").css("z-index", 99).fadeIn(1000, function () {
                        _isChanging = false;
                        $("ul.bannerview li[idx='" + _nowShow + "']").hide();
                        _nowShow = (_nowShow + 1) % imgCount;
                        _timerObj = setTimeout("goNext()", 4000);
                    });
                }


            }

            $(window).load(function () {

                //調整小綠點位置
                $(".idxAddot").hide();
                $(".idxAddot").css("left", ($(".idsAdContainer").width() - $(".idxAddot").width()) / 2 + "px");
                $(".idxAddot").fadeIn("slow");

                //先將loading圖load進來
                /*MM_preloadImages('img/loading.gif');*/

                //load第一張圖進來
                /*MM_preloadImages(
                'img/index/segment/1_1.jpg',
                'img/index/segment/1_2.jpg',
                'img/index/segment/1_3.jpg',
                'img/index/segment/1_4.jpg');*/

                /* 先藏起來 */
                $("ul.bannerview li").each(function () {
                    if ($(this).attr("idx") != 0) $(this).hide();
                });

                //開始輪播
                
                _timerObj = setTimeout("goNext(1)", 1000);
                


                $(".idxAddot li").click(function () {

                    if (!_isChanging) {
                        clearTimeout(_timerObj);
                        _isChanging = true;

                        //load圖
                        /*MM_preloadImages(
                        'img/index/segment/' + (parseInt($(this).children("img").attr("idx")) + 1) + '_1.jpg',
                        'img/index/segment/' + (parseInt($(this).children("img").attr("idx")) + 1) + '_2.jpg',
                        'img/index/segment/' + (parseInt($(this).children("img").attr("idx")) + 1) + '_3.jpg',
                        'img/index/segment/' + (parseInt($(this).children("img").attr("idx")) + 1) + '_4.jpg');*/

                        //放圖
                        /*$("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(0).attr("src", $("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(0).attr("psrc"));
                        $("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(1).attr("src", $("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(1).attr("psrc"));
                        $("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(2).attr("src", $("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(2).attr("psrc"));
                        $("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(3).attr("src", $("ul.slides li[idx='" + parseInt($(this).children("img").attr("idx")) + "'] div.img img").eq(3).attr("psrc"));*/


                        $("ul.bannerview li[idx='" + _nowShow + "']").css("z-index", 0);
                        $("ul.idxAddot li a").removeClass("on");
                        $("ul.idxAddot li").removeClass("on");
                        $(this).children("a").addClass("on");
                        $(this).addClass("on");
                        $("ul.bannerview li[idx='" + $(this).attr("idx") + "']").css("z-index", 99).fadeIn(1000, function () {
                            _isChanging = false;
                            $("ul.bannerview li[idx='" + _nowShow + "']").hide();
                            _nowShow = parseInt($(this).attr("idx"));
                            _timerObj = setTimeout("goNext()", 4000);
                        });
                    }


                });

            });
  </script>

  

  
