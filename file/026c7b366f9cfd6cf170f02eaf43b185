  <script type="text/javascript">

      var _bottomBannerCnt;
      var _sliderWidth;//移動本體寬
      var _single_BottomBannerWidth = 159; //單一寬 + Margin
      var _single_BottomBannerHeight = 70; //單一高
      var _moveNum = 4; //一次移動個數
      var _isMoving = false;

      //var _topLeftPoint = 0;//最前端的座標
      var _bottomLeftPoint;//最後的座標

      $(function () {
          //_bottomBannerCnt = $("div#idxbanner_slider a").length;
          _bottomBannerCnt = 8;
          _sliderWidth = _single_BottomBannerWidth * _bottomBannerCnt
          
          //$("div#idxbanner_slider").css("width", _sliderWidth + "px");
          $("div#idxbanner_slider").css("width",((Math.ceil((_bottomBannerCnt+1)/4)+1) *4 *_single_BottomBannerWidth)  + "px");

          _bottomLeftPoint = (Math.ceil(_sliderWidth / _moveNum) * _moveNum - (_moveNum * _single_BottomBannerWidth)) * -1

         //左
          $(".idxbannerContainer .Larrow li a").click(function () {

              var sliderLeft = parseInt($("div#idxbanner_slider").css("left").replace("px", ""));

              if (!_isMoving && sliderLeft < 0) {

                  _isMoving = true;
                  $("div#idxbanner_slider").animate({ left: '+=' + _single_BottomBannerWidth * _moveNum }, 500, function () {
                      _isMoving = false;
                  });
                  clearTimeout(bmTM);
                  bottomBannerAutoMove = false;
                  bmTM = setTimeout("AutoNext()", 5000);
              }
          });

          //右
          $(".idxbannerContainer .Rarrow li a").click(function () {

              var sliderLeft = parseInt($("div#idxbanner_slider").css("left").replace("px", ""));

              if (!_isMoving) {
                  if (sliderLeft <= 0 && sliderLeft > _bottomLeftPoint) {

                      _isMoving = true;
                      $("div#idxbanner_slider").animate({ left: '-=' + _single_BottomBannerWidth * _moveNum }, 500, function () {
                          _isMoving = false;
                      });

                      clearTimeout(bmTM);
                      bottomBannerAutoMove = false;
                      bmTM = setTimeout("AutoNext()", 5000);
                  } else if (bottomBannerAutoMove && sliderLeft <= _bottomLeftPoint) {
                    // slide dummy
                      $("div#idxbanner_slider").animate({ left: '-=' + _single_BottomBannerWidth * _moveNum }, 500, function () {
                          //back to origin
                          $("div#idxbanner_slider").css("left", "0px");
                          _isMoving = false;
                      });
                      clearTimeout(bmTM);
                      bottomBannerAutoMove = false;
                      bmTM = setTimeout("AutoNext()", 5000);
                  }
              }


          });


          if ($("div#idxbanner_slider a").length > 4) {
              bmTM = setTimeout("AutoNext()", 5000);
          }




      });

      var bottomBannerAutoMove = false;
      function AutoNext() {
          bottomBannerAutoMove = true;
          $(".idxbannerContainer .Rarrow li a").trigger("click");
      }
  </script>
  


    
  <script type="text/javascript">
      var m_bottomBannerCnt;
      var m_sliderWidth;//移動本體寬
      var m_single_BottomBannerWidth = 216; //單一寬 + Margin
      var m_single_BottomBannerHeight = 110; //單一高
      var m_moveNum = 1; //一次移動個數
      var m_isMoving = false;

      //var _topLeftPoint = 0;//最前端的座標
      var m_bottomLeftPoint;//最後的座標

      $(function () {
          //_bottomBannerCnt = $("div#idxbanner_slider a").length;
          m_bottomBannerCnt = 5;
          m_sliderWidth = m_single_BottomBannerWidth * m_bottomBannerCnt;

          //$("div#idxbanner_slider").css("width", _sliderWidth + "px");
          //$("div#idxblackbanner_slider").css("width", ((Math.ceil((m_bottomBannerCnt + 1) / 3) + 1) * 3 * m_single_BottomBannerWidth) + "px");
          $("div#idxblackbanner_slider").css("width", ((m_bottomBannerCnt+3) * m_single_BottomBannerWidth) + "px");

          //m_bottomLeftPoint = (Math.ceil(m_sliderWidth / m_moveNum) * m_moveNum - (m_moveNum * m_single_BottomBannerWidth)) * -1
          m_bottomLeftPoint = m_sliderWidth * (-1) + m_single_BottomBannerWidth * 1;

          //左
          $(".idxblackbannerblock .Larrow li a").click(function () {

              var sliderLeft = parseInt($("div#idxblackbanner_slider").css("left").replace("px", ""));

              if (!m_isMoving && sliderLeft < 0) {

                  m_isMoving = true;
                  $("div#idxblackbanner_slider").animate({ left: '+=' + m_single_BottomBannerWidth * m_moveNum }, 300, function () {
                      m_isMoving = false;
                  });
                  clearTimeout(mbmTM);
                  middleBannerAutoMove = false;
                  mbmTM = setTimeout("mAutoNext()", 5000);
              }
          });

          //右
          $(".idxblackbannerblock .Rarrow li a").click(function () {

              var sliderLeft = parseInt($("div#idxblackbanner_slider").css("left").replace("px", ""));

              if (!m_isMoving) {
                  if (sliderLeft <= 0 && sliderLeft > m_bottomLeftPoint) {

                      m_isMoving = true;
                      $("div#idxblackbanner_slider").animate({ left: '-=' + m_single_BottomBannerWidth * m_moveNum }, 300, function () {
                          m_isMoving = false;
                      });

                      clearTimeout(mbmTM);
                      middleBannerAutoMove = false;
                      mbmTM = setTimeout("mAutoNext()", 5000);
                  //} else if (mbottomBannerAutoMove && sliderLeft <= m_bottomLeftPoint) {
                  } else  {
                      // slide dummy
                      $("div#idxblackbanner_slider").animate({ left: '-=' + m_single_BottomBannerWidth * m_moveNum }, 300, function () {
                          //back to origin
                          $("div#idxblackbanner_slider").css("left", "0px");
                          m_isMoving = false;
                      });
                      clearTimeout(mbmTM);
                      mbottomBannerAutoMove = false;
                      mbmTM = setTimeout("mAutoNext()", 5000);
                  }
              }


          });


          if ($("div#idxblackbanner_slider a").length > 3) {
              mbmTM = setTimeout("mAutoNext()", 5000);
          }

      });

      var mbottomBannerAutoMove = false;
      function mAutoNext() {
          mbottomBannerAutoMove = true;
          $(".idxblackbannerblock .Rarrow li a").trigger("click");
      }
  </script>
  

  
