<script type="text/javascript">
 function checkForm(){
     var keyStr =document.getElementById("keyword")
     if(keyStr.value == ""){
         alert("請您先輸入關鍵字");
         return false;
     }
     return true ;

 }

 function GSearch() {
     //alert($("#keyword").val());
     //return;
     if ($("#keyword").val() == "") {
         alert("請您先輸入關鍵字");
         return false;
     }
     //alert('location');
     location='/result.aspx?cx=005475758396817196247%3avpg-mgvhr44&cof=FORID:11&ie=UTF-8&q=' + $("#keyword").val();
     return false;
 }
 </script>


<script type="text/javascript">
        // menuid is defined in decorator
        $(document).ready(function () {
            $("#mainNav li").hover(makeShow, makeHide);
       }); 

        function makeShow() {
            $(this).children('ul').show();
        }

        function makeHide() {
            $(this).children('ul').hide();
        }

</script>
