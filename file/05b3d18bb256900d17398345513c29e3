window.addEvent('domready', function(){
    var currenturl = document.location.href;
    $('nav').getElements('ul li').each(function(e){
        var linkurl = e.getElement("a").get("href");
        if(currenturl.indexOf(linkurl)!=-1){
            $('nav').getElements('ul li').removeClass("cur");
            e.addClass("cur");
        }
        e.addEvents({'mouseenter':function(){
            var sub = this.getElement('.sub_menu');
            if (sub){
                sub.setStyle('display', 'block');
            }
            
        }, 'mouseleave':function(){
            var sub = this.getElement('.sub_menu');
            if (sub){
                sub.setStyle('display', 'none');
            }
        }});
    });
});
