<script language="javascript" type="text/javascript">
    var thisPage = "Profiles.aspx";

    $(function () {
        //当前URL地址
        var url = window.location.href;
        //如果地址栏关键字存在，则把值填写到文本框中
        if (url.indexOf("kw=") != -1) {
            var kwV = url.split("kw=")[1].split("&")[0];
            if (kwV != "") { kwV = $.url.decode(kwV); }
            $("#kwords").val(kwV);
        }
        
        //点击搜索按钮时触发的事件
        $("#sBtn").click(function () {
            var kw = $("#kwords").val();
            kw = $.url.encode(kw);
             
            window.location.href = thisPage + "?kw=" + kw;
        });
        $(".searchBox").keyup(function (event) { if (event.keyCode == 13) { $(".searchBox #sBtn").trigger("click"); } });
    });
   
 
</script>

