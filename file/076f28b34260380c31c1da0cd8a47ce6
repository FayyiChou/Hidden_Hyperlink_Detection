            $(function () {
                var aObj = $(".tMenu a");
                var url = window.location.href;
                var index = 0;
                var size = aObj.size();
                for (var i = 0; i < size; i++) {
                    var turl = aObj.eq(i).attr("href");
                    if (url.indexOf(turl) != -1) {
                        index = i;
