	  <script>
	  function checktwipt() {
		  var sword = document.getElementById("sdfipt").value;
		  if (sword == "详细描述你的疑问") {
			  document.getElementById("sdfipt").value = "";
		  }
	  }
	  function sdfsb(){
		  var sword = document.getElementById("sdfipt").value;
		  if (sword == "详细描述你的疑问"||sword == "") {
			  alert("请详细描述你的疑问!");
			  return false;
		  }else{
		  	sdf.submit();
		  }
	  }
      </script>
