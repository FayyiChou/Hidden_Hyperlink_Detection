      <script type="text/javascript">
function peopleMode(){
	var id=jQuery("#peopleMode"),
		map=id.find("map area"),
		change=id.find(".rentichange"),
		ctrl=id.find(".rentictrl"),
		bg=id.find(".rentibg"),
		zm="ctrlz",
		bm="ctrlb";
	var people={}
	this.show=showname;/*传入显示位置 如：z1，b1接受类型字符串*/
	
	change.find("em").hide();
	showzm();
	map.mouseenter(function(e) {
		var _cha=$(this).attr("alt");
		show(_cha);
		
	});
	ctrl.find("."+zm).click(function(){
		showzm();
	});
	ctrl.find("."+bm).click(function(){
		showbm();
	});
	function showname(name){
		var _name=$.trim(name);
		var _num=_name.substring(1);
		switch(_name.charAt(0)){
			case 'z':
				showzm();
				break;
			case 'b':
				showbm();
				break;
		}
		show("c"+_name);
	}
	function show(x){/*显示class为for的内容*/
		change.find("em").hide();
		ctrl.find("li").hide();
		change.find("."+x).css({"display":"block"});
		ctrl.find("li."+x).show();
	}
	function showzm(){/*切换正面*/
		change.find("em").hide();
		bg.find("img").hide();
		ctrl.find("li").hide();
		bg.find("."+zm).show();
		ctrl.find(".ctrlzf a,img").removeClass("now");
		ctrl.find("."+zm).addClass("now");
	}
	function showbm(){/*切换正面*/
		change.find("em").hide();
		bg.find("img").hide();
		ctrl.find("li").hide();
		bg.find("."+bm).show();
		ctrl.find(".ctrlzf a,img").removeClass("now");
		ctrl.find("."+bm).addClass("now");
	}
}
var people0=new peopleMode();				
			</script> 
      <!--新renti END--> 
