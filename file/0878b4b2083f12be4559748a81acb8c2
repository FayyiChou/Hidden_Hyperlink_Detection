	<span class="next"></span>
	<script type="text/javascript">
		function RunFocus(){
			jQuery(".m-focus").hover(function(){ jQuery(this).find(".prev,.next").stop(true,true).fadeTo("show",0.5) },function(){ jQuery(this).find(".prev,.next").fadeOut() });

			jQuery(".m-focus").slide({ titCell:".hd ul", mainCell:".bd ul", effect:"fold",  autoPlay:true, autoPage:true, trigger:"click",
				startFun:function(i){
					var curLi = jQuery(".m-focus .bd li").eq(i);
					if( !!curLi.attr("_src") ){
						curLi.css("background-image",curLi.attr("_src")).removeAttr("_src");
					}
				}
			});
		}
		RunFocus();
	</script>
