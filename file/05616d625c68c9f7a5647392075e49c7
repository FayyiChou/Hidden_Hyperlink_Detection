    //弹出遮罩及反馈意见窗口
    function showFeedback() {
        $(".maskLayer").fadeIn(100);
        $(".feedback").slideDown(100);
        $('#feedBtn').removeClass().addClass('Submit-btn');
        $('#feedBtn').attr('disabled', false);
    }

    function SetHome(obj, url) {
        try {
            obj.style.behavior = 'url(#default#homepage)';
            obj.setHomePage(url);
        } catch (e) {
            if (window.netscape) {
                try {
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                } catch (e) {
                    alert("抱歉,此操作被浏览器拒绝！\n\n请在浏览器地址栏输入“about:config”并回车然后将[signed.applets.codebase_principal_support]设置为'true'");
                }
            } else {
                alert("抱歉,您所使用的浏览器无法完成此操作。\n\n您需要手动将【" + url + "】设置为首页。");
            }
        }
    }


    function alertMsg() {
        alert('暂未开放,敬请关注！');
    }

    var pageType = 'VIDEO';
    var pageColumns = 'VIDEO';
    // 拖拽
    $(document).ready(function () {
        //定义遮罩的宽高
        var bh = $("body").height();
        var bw = $("body").width();
        $(".maskLayer").css({
            height: "100%",
            width: bw
        });
        //关闭遮罩及反馈意见窗口
        $(".dialog-title .close").click(function () {
            $(".maskLayer").fadeOut(50);
            $(".feedback").slideUp(50);
        });

        //拖拽
        var _move = false; //移动标记
        var _x, _y;
        var index_x, index_y;

        $(".dialog-title").bind("mousedown", downIndex); //div鼠标按下事件绑定
        $(document).bind("mousemove", mmove); //鼠标移动事件绑定
        $(document).bind("mouseup", mup); //鼠标弹起事件绑定
        function mmove(e) {
            if (_move) {
                var x = e.pageX - _x; //移动时根据鼠标位置计算控件左上角的绝对位置
                var w = $("body").width();
                var h = window.outerHeight - 75;
                if (x < 250) x = 250;
                if (x > (w - 250)) x = w - 250;
                var y = e.pageY - _y;
                if (y < 197) y = 197;
                if (y > (h - 187)) y = h - 187;
                $(".feedback").css({top: y, left: x}); //div新位置
//                show(x, y);
            }
        }

        function mup() {
            _move = false;
        }

        function downIndex(e) {
            _move = true;
            _x = e.pageX - parseInt($(".feedback").css("left"));
            _y = e.pageY - parseInt($(".feedback").css("top"));

        }

    });
    //字数至少10个提示
    function poving() {
        var atlest = '反馈意见至少10个字';
        var fbsuccess = '反馈成功';
        var username = document.getElementById("feedbackText");
        if (username.value.length < 10) {
            alert(atlest);
            return false;
        }
        var content = document.getElementById('feedbackText').value;
        var name = document.getElementById('name').value;
        var phonenum = document.getElementById('phonenum').value;
        if (!!document.getElementById('qqnum')) {
            var qqnum = document.getElementById('qqnum').value;
        }
        //点击完成，将按钮置灰
        $('#feedBtn').removeClass().addClass('Submit-btn-grey');
        $('#feedBtn').attr('disabled', 'disabled');
        $.ajax({
            url: 'login.setFeedBack.do',
            type: 'get',
            data: {problemDes: content, username: name, phoneNum: phonenum, qq: qqnum},
            success: function (data) {
                if (data == true) {
                    //清空数据
                    $('#name').val('');
                    $('#phonenum').val('');
                    $('#qqnum').val('');
                    $('#feedbackText').val('');
                    $(".dialog-title .close").trigger('click');
                    alert(fbsuccess);
                }
            }
        });
    }
</script>

