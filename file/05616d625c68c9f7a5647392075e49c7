                    function getCookie(name) {
                        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
                        if (arr = document.cookie.match(reg))
                            return arr[2];
                        else
                            return null;
                    }

                    function deleteCookie(name) {
                        var domain = '13322.com/';
                        var date = new Date();
                        date.setTime(date.getTime() - 10000);
                        document.cookie = name + "=; expire=" + date.toGMTString() + "; path=/;domain=." + domain.substr(0, domain.length - 1);
                    }
                    function exitLogin() {
                        $.ajax({
                            type: "POST",
                            url: "/login.checkOutLogin.do",
                            async: false,
                            success: function (obj, textStatus) {
                                deleteCookie("uid");
                                deleteCookie("passport_lyy_com");
                                deleteCookie("permanent_token_id");
                                if (lang == "zh") {
//                                    QC.Login.signOut();
                                    WB2.logout();
                                }
                                $("#unLogin").show();
                                $("#isLogin").hide();
                            }
                        });
                    }
                    function getUserCookie() {
                        if (getCookie("uid") == null || getCookie("uid") == "") {
                            $("#unLogin").show();
                            $("#isLogin").hide();
                        } else {
                            $("#isLogin").children("span:eq(1)").html(decodeURIComponent(getCookie("uid")));
                            $("#unLogin").hide();
                            $("#isLogin").show();
                        }
                    }
                    $(function () {
                        getUserCookie();
                    })
                </script>
