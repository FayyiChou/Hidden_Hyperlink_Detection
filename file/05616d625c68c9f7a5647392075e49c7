    <script type="text/javascript">
        function videoPlayerFoward(sptId, type, dt, cid) {
            var form = document.createElement("form");
            dt = dt.substring(0, 10);
            dt = replaceAll(dt, "-", "");
            form.action = tvBase + "bifen_" + sptId + "_" + dt + "_" + type + "_" + cid + ".html";
            document.body.appendChild(form);

            form.target = "_blank";
            form.method = "POST";
            form.submit();
            document.body.removeChild(form);
        }
        function replaceAll(str, old, re) {
            while (str.indexOf(old) >= 0) {
                str = str.replace(old, re);
            }
            return str;
        };
    </script>
    
