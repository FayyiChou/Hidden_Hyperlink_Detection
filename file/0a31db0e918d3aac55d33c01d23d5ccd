<script type="text/javascript">
	$(function(){
	     		$(".menu .u1 li").mouseover(function() {
		               var index =$(this).index();	 
		  
		               $(".submenu").css({"display":"none"});
		               $(".submenu:eq("+index+")").show();	                
		            });
	
	    
	            $(".submenu").mouseover(function() {
	                $(this).show();
	            });
	            $(".submenu").mouseout(function() {
	                $(this).hide();
	            });
	     $("#Information").click(function(){
	     	  $.ajax({
                    type: "post",
                    url: "/page/CheckLogin",
                    data: {},
                    async: false,
                    success: function(data) {
                  	   if (data) {
                         switch (data) {
                            case "NO":
                             alert("请登录！");
                            break;
                            case "YES":
                             window.open("/comment");
                            break;
                         }
                       }
                    },
                   error: function() {
               		 alert("error");
         			}
                });
	     });
	});
</script>
