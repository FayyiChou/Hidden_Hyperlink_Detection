<script>
function fx(el,attr,to){
	el=typeof el=='string'?document.getElementById(el):el;
	if(to>0)el.style.display='block';
	var w=el['offset'+attr.replace(/^(.)/,new Function('return arguments[1].toUpperCase()'))];
	var Int=Math[to-w>0?'ceil':'floor'];
	clearInterval(el.timer);
	el.timer=setInterval(function(){
		w+=Int((to-w)*0.1);
		el.style[attr]=w+'px';
		if(w==to){	 
			if(to==0)el.style.display='none';
			clearInterval(el.timer);
		};	
	},10);
};
window.onload = function (){
	var a=document.getElementById("menu_1");
	var b=document.getElementById("menu_2");
	var c=document.getElementById("menu_3");
	var d=document.getElementById("menu_4");
	var e=document.getElementById("menu_5");
	var f=document.getElementById("menu_6");
	var g=document.getElementById("menu_7");
	var d1=document.getElementById("menu_xial_1");
	var d2=document.getElementById("menu_xial_2");
	var d3=document.getElementById("menu_xial_3");
	var d4=document.getElementById("menu_xial_4");
	var d5=document.getElementById("menu_xial_5");
	var d6=document.getElementById("menu_xial_6");
	var d7=document.getElementById("menu_xial_7");

	d1.onmouseover=a.onmouseover=function (){fx('menu_xial_1','height',240)};
	d1.onmouseout=a.onmouseout=function (){fx('menu_xial_1','height',0)};
	d2.onmouseover=b.onmouseover=function (){fx('menu_xial_2','height',240)};
	d2.onmouseout=b.onmouseout=function (){fx('menu_xial_2','height',0)};
	d3.onmouseover=c.onmouseover=function (){fx('menu_xial_3','height',240)};
	d3.onmouseout=c.onmouseout=function (){fx('menu_xial_3','height',0)};
	d4.onmouseover=d.onmouseover=function (){fx('menu_xial_4','height',240)};
	d4.onmouseout=d.onmouseout=function (){fx('menu_xial_4','height',0)};
	d5.onmouseover=e.onmouseover=function (){fx('menu_xial_5','height',240)};
	d5.onmouseout=e.onmouseout=function (){fx('menu_xial_5','height',0)};
	d6.onmouseover=f.onmouseover=function (){fx('menu_xial_6','height',240)};
	d6.onmouseout=f.onmouseout=function (){fx('menu_xial_6','height',0)};
	d7.onmouseover=g.onmouseover=function (){fx('menu_xial_7','height',240)};
