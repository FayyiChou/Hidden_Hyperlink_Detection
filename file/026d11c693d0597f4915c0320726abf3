<!-- 按需jquery -->
<script>!window.jQuery && document.write(unescape('%3Cscript src="http://vhuya.dwstatic.com/common/js/jquery.js"%3E%3C/script%3E'));</script>

<script>
    $(function () {
        var vid = "2239921";
        var jsapi = 'http://v.huya.com/jsapi/';

        //标签样式
        $(".tagvideo").eq(0).addClass('active');
        var text = $(".tagvideo").text();
        if ('' == text) {
            $(".tagvideo").hide();
        }
        ;
        $('.video-tag-list  a').bind('mouseover', function () {
            var index = $(this).index();
            $(this).addClass('active').siblings().removeClass('active');
            $('.sel_tag').eq(index).show().siblings().hide();
        });


        //数字格式转换
        function numFormat(num) {
            var rs;
            if (num > 999999) {
                rs = Math.round(num / 10000) + "万";
                return rs;
            } else if (num > 9999) {
                rs = Math.round(num / 10000 * 10) / 10 + "万";
                return rs;
            } else if (num > 999) {
                return parseFloat(num).toLocaleString();
            } else {
                return num;
            }
        }

        function formatVideoPlayNum(vid, num, channel) {
            var num = parseInt(num);
            var salt = parseInt(vid * 1.2) + '';
            salt = parseInt(salt.substr(salt.length - 3, 3));
            salt = salt > 500 ? salt : (1000 - salt);

            if (-1 == $.inArray(channel, ['lol', 'df']) || 0 == num) {
                num = num + salt;
            }

            num = numFormat(num);
            return num;
        }

        //更新播放数 
        var updateVideoPlayNum = {
            init: function (vid) {
                var _this = this;
                $.get('http://playstats.v.duowan.com/index.php?r=api/get', {vid: vid},
                        function (json) {
                            if (!json[vid]) return;
                            var new_num = formatVideoPlayNum(vid, json[vid], 'mc');
                            var old_num = $("#video_play_num").data('play-num');
                            if (new_num != old_num) {
                                $('#video_play_num').html(new_num);
                                _this.requestUpdate(vid);
                            }
                        }, 'jsonp');
            },

            //请求服务器更新 
            requestUpdate: function (vid) {
                $.get(jsapi + 'updateVideoPlayNum', {vid: vid}, function () {
                }, 'jsonp');
            }
        };

        //更新评论数
        var updateVideoCommentNum = {
            init: function (vid) {
                var _this = this;
                var uniqid = window.comment3Uniqid;
                var domain = window.qianwangbieyongzhegebianliang_domain || 'v.huya.com';
                var url = window.qianwangbieyongzhegebianliang_url || '/play/' + vid + '.html';

                var apiUrl = 'http://v-comments.huya.com/index.php?r=comment/totaljson&uniqid=' + uniqid + '&domain=' + domain + '&url=' + url + '&huya=1';
                $.get(apiUrl, {vid: vid},
                        function (json) {
                            if (!json['show'] || !json['show']['total_num']) return;
                            var new_num = numFormat(json['show']['total_num']);
                            var old_num = $("#first_comment_count").data('comment-num');
                            if (new_num != old_num) {
                                //$('#first_comment_count').html(new_num);
                                _this.requestUpdate(vid);
                            }
                        }, 'jsonp');
            },

            //请求服务器更新
            requestUpdate: function (vid) {
                $.get(jsapi + 'updateVideoCommentNum', {vid: vid}, function () {
                }, 'jsonp');
            }
        };

        //提供给播放器的全局变量，需要谨慎处理
        window.vhuyaPlayerNextVideo = {'title': '', 'url': ''};
        var setNextVhuyaPlayerNextVideo = function (data) {
            var videoInfo = $('#v-alist-list').find('.curPage ~ a');
            var alisNextUrl = videoInfo.attr('href');
            var alisNextTitle = videoInfo.eq(0).text();
            if (typeof alisNextUrl !== 'undefined' && alisNextUrl != '') {
                vhuyaPlayerNextVideo.title = alisNextTitle;
                vhuyaPlayerNextVideo.url = alisNextUrl;
            } else if (data) {
                vhuyaPlayerNextVideo.title = data.video_title;
                vhuyaPlayerNextVideo.url = data.video_url;
            }
        };

        //加载相关视频
        var videoRelate = {
            nextVideo: null,
            tpl_render: function (tpl, data) {
                return tpl.replace(/\{([\w\_]+)\}/g, function () {
                    return (typeof data[arguments[1]] === 'undefined') ? '' : data[arguments[1]];
                });
            },

            getRelateVid: function (vid) {
                var _arr = window.location.href.match(/relate_vid=(\d+)/);
                return _arr && _arr[1] ? _arr[1] : vid;
            },

            renderData: function (vid, relate_vid, json) {
                if (!json || !json[0]) return;
                var tpl = '{onlive}\
			                	<a href="{video_url}" data-dwclick="dwclick" data-ckstat="play_video_relate{k}" target="_blank" class="video-wrap">\
			                		<div class="video-play">\
			                			<img src="{video_cover}"  class="video-cover">\
			                			<div class="video-mask"><span class="video-icon-s"></span></div>\
			                			{playing}\
			                		</div>\
			            			<p class="video-title">{video_title}</p>\
			            			<div class="video-meta">\
			            				<span class="video-meta-pnum"><i class="icon-video-pnum2"></i>{video_play_num}</span>\
			            				<span class="video-meta-comment"><i class="icon-video-comment"></i>{video_comment_num}</span>\
			            				<span class="video-meta-date"></span>\
			            			</div>\
			            		</a>\
							 </li>';

                var html = '', curIndex = 0;
                for (var i = 0; i < json.length; i++) {
                    json[i]['k'] = i;

                    if (vid == json[i]['vid']) {
                        curIndex = i;
                        json[i]['playing'] = '<div class="video-info"><span class="video-duration">正在播放</span></div>';
                        json[i]['onlive'] = '<li class="video-right-list-item clearfix onlive">';
                        if (typeof json[i + 1] !== 'undefined' && !this.nextVideo) {
                            this.nextVideo = json[i + 1];
                        }
                    } else {
                        json[i]['playing'] = '<div class="video-info"><span class="video-duration">' + json[i]['video_duration'] + '</span></div>';
                        json[i]['onlive'] = '<li class="video-right-list-item clearfix">';
                    }

                    if (i < 15) {
                        json[i]['video_url'] = json[i]['video_url'] + '#relate_vid=' + relate_vid;
                    }
                    html += this.tpl_render(tpl, json[i]);
                }
                if (!this.nextVideo && vid != json[0]['vid']) {
                    this.nextVideo = json[0];
                }
                $('#videoRelate').html(html);
                //Set ScrollTop
                var top = (curIndex - 1) * 100;
                if (top > 0) {
                    $('#relateRight').niceScroll().doScrollTop(top);
                }
            },

            init: function (vid) {
                try {
                    var _this = this;
                    var relate_vid = _this.getRelateVid(vid);
                    $.get(jsapi + 'getVideoRelate', {relate_vid: relate_vid}, function (json) {
                        _this.renderData(vid, relate_vid, json);
                        setNextVhuyaPlayerNextVideo(_this.nextVideo);
                    }, 'jsonp');
                } catch (e) {
                }
            }
        };

        var addPlayRecord = function (vid) {
            $.get(jsapi + 'addPlayRecord', {vid: vid}, function () {
            }, 'jsonp');
        }

        updateVideoPlayNum.init(vid);
        updateVideoCommentNum.init(vid);
        videoRelate.init(vid);
        addPlayRecord(vid);

        window.videoRelate = videoRelate;

        window.stop_cb = function () {
            if (window.vhuyaPlayerNextVideo.url) {
                var image = new Image;
                var statUrl = 'http://clickstats.duowan.com/index.php?r=stats/index&event=vhuya_continue_play';
                image.src = statUrl;
                window.location.href = window.vhuyaPlayerNextVideo.url;
                return;
            }
        };

        $('.zan').on('click', function () {
            var vid = $(this).data('vid');
            if (false == vid)return false;
            $.get('/index.php?r=video/support', {vid: vid}, function (data) {
                if (data.rs) {
                    var support = $('.zan').text();
                    support = parseInt(support) + 1;
                    $('.zan').html("<i></i>" + support);
                    $('.zan').addClass('zan--actived');
                }
            }, 'json')
        });

        function getIsLive() {
            var user_id = '1121252923';
            $.get('/index.php?r=video/huyalive', {user_id: user_id}, function (data) {
                if (1 == data['code']) {
                    $("#isLive").attr('href', data['data']).show();
                }
            }, 'json')
        }

        getIsLive();
    });

</script>



