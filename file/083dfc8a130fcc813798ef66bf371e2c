<!--Hide Home page Jackpot div on tablet and phone -->
<script type="text/javascript">
    $(window).resize(function() {
        if( $(window).width() <= 1024)
            {
              $('#jackpot-img').hide();
            } 
            else{
                $('#jackpot-img').show();
            }
    }); 
    </script>

    <!--Hide promotions download and play buttons on tablet and phone -->
