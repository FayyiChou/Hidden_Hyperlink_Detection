	$('.refresh').on('click',function(){
		 window.location.reload(true);
	});
	var pTop = $('.position').offset().top;
	$(window).on('scroll',function(){
		var scrollTop = $(window).scrollTop();
		if(scrollTop >= pTop){
			$('.blank').show();
			$('.top-layer').show();
		}else{
			$('.blank').hide();
			$('.top-layer').hide();
		}
	});
	var timer = null;
