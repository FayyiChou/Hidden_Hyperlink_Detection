<STYLE type="text/css">
#fly{
	position:absolute;
	left:16px;
	top:80px;
	width:300px;
	height:100px;
	z-index:0;
}
#closeAdv{
	position:absolute;
	left:250px;
	top:180px;
	z-index:2;
	cursor:hand;
	width: 58px;
	height: 12px;
}
</STYLE>
<SCRIPT>

  var x = 50,y = 55;  //浮动广告初始位置
  var i = 100, j = 100;

  var xin = true, yin = true;  //xin为真向右运动，否则向左，yin为真向下 运动，否则向上运动

  var step = 1;   //移动的距离

  var delay = 35;  //移动的时间间隔
  

  function floatAD() {

     var L=T=0;			//L左边界，T上边界
	 var Q=Z=0;

     var R= document.body.offsetWidth-document.getElementById("fly").offsetWidth;  //层移动的右边界
	 var m= document.body.offsetWidth-document.getElementById("closeAdv").offsetWidth;
     var B = document.body.offsetHeight-document.getElementById("fly").offsetHeight; //层移动的下边界
	 var n=document.body.offsetHeight-document.getElementById("closeAdv").offsetHeight;
     document.getElementById("fly").style.left = x;  //层移动后的左边界
	 document.getElementById("closeAdv").style.left = i; 
     document.getElementById("fly").style.top = y;   //曾移动后上边界
	document.getElementById("closeAdv").style.top = j; 
     x = x + step*(xin?1:-1);		//判断水平方向

     if (x < L) { xin = true; x = L; }   //到达边界后的处理 

     if (x > R){ xin = false; x = R;} 

     y = y + step*(yin?1:-1); 

     if (y < T) { yin = true; y = T;} 

     if (y > B) { yin = false; y = B;} 
	 
	  i = i + step*(xin?1:-1);		//判断水平方向

     if (i < Q) { xin = true; i = Q; }   //到达边界后的处理 

     if (i > m){ xin = false; i = m;} 

     j = j + step*(yin?1:-1); 

     if (j < Z) { yin = true; j = Z;} 

     if (j > n) { yin = false; j = n;} 
	 sett=setTimeout("floatAD()", delay)		//隔多长时间调用一次

  }
  function closeAdv(){
  	document.getElementById("fly").style.display="none";
	document.getElementById("closeAdv").style.display="none";
  }
</SCRIPT>
<center>
<DIV id="fly" onMouseOver="clearTimeout(sett)" onMouseOut="setTimeout('floatAD()', delay)">
 <a href="qzlx/index.asp" target="_blank"><img src="images/piao.jpg" width="366" height="127" /></a></DIV>
<DIV id="closeAdv"  onClick="closeAdv()"  onMouseOver="clearTimeout(sett)" onMouseOut="sett=setTimeout('floatAD()', delay)"></DIV>
