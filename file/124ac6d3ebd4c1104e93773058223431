	<script type="text/javascript">
	
	

		$(function(){
			setTimeout('loginshow()',50); 
		 });
		function loginshow(){
			 var myprovince = remote_ip_info['province']; 
			 var mycity = remote_ip_info['city'];
			 document.getElementById("myprovince").innerHTML=myprovince;
			  document.getElementById("mycity").innerHTML=mycity;
			  document.getElementById("loginmyprovince").innerHTML=myprovince;
			  document.getElementById("loginmycity").innerHTML=mycity;
				var url=EER_GLOBAL_CONTEXT+"/front/login/gudgeland.do";
			 	 $.ajax({
						type : 'post',
						url : url,
						data : {},
						async:false,
						dataType : "jsonp",
						jsonpCallback:"success_jsonpCallback",//callback的function名称
						jsonp: "callbackparam",//服务端用于接收callback调用的function名的参数
						success : function(data){
							 if(data==null||data==''||data=='null'){
								$("#userloginNot").show(); 
								$("#userloginYes").hide();
							}else{
								$("#userloginYes").show(); 
								$("#userloginNot").hide();		
								$("#landname").html(data);		
							}
						}
						
					});
		}
	</script>
