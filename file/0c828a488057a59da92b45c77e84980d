  <script type="text/javascript">
var disabledDays = [];
function nationalDays(date) {
  var m = date.getMonth(), d = date.getDate(), y = date.getFullYear();
  for (var i in disabledDays) {
    if($.inArray(y + '-' + (m+1) + '-' + d,disabledDays) != -1 || new Date() > date) {
      return [false];
    }
  }
  return [true];
}
function noWeekendsOrHolidays(date) {
  var noWeekend = jQuery.datepicker.noWeekends(date);
  return noWeekend[0] ? nationalDays(date) : noWeekend;
}
$(function() {
	var dates = $("#from, #to").datepicker({
		minDate: 0,
		changeMonth: true,
		numberOfMonths: 1,
		onSelect: function( selectedDate ) {
			var option = this.id == "from" ? "minDate" : "maxDate",
			instance = $( this ).data( "datepicker" ),
			date = $.datepicker.parseDate(
			instance.settings.dateFormat ||
			$.datepicker._defaults.dateFormat,
			selectedDate, instance.settings );
			dates.not( this ).datepicker( "option", option, date );
			if (this.id == 'from'){
				var fDay = $('#from').datepicker('getDate') ;
				var date = new Date(fDay);
				date.setDate(date.getDate() + 1);
				date = date.getFullYear() + "-" +   (date.getMonth()+1) + "-" + date.getDate();
				$('#to').datepicker('setDate',date);
			}
		}
	});
});
function checkPic(obj){
	var id = obj.attr('id').replace('small','');
	id = "#big" + id;
	$("#bigImg li").hide();
	$(id).fadeIn(300);
	//$("#gallery img").fadeOut(350).attr('src',obj.attr('alt')).fadeIn(100);
}
$("#smallImg img").click(function (){
	checkPic($(this));
});
$("#next").click(function (){
	var rePic = $("#smallImg li");
	$("#smallImg").append(rePic.first());
	checkPic($($("#smallImg img").first()));
});
$("#prev").click(function (){
	var rePic = $("#smallImg li");
	$(rePic.last()).prependTo($("#smallImg"));
	checkPic($($("#smallImg img")['4']));
});
</script> 
