  <script type="text/javascript">
var date = new Date();
var dateY = date.getFullYear();
var dateM = (date.getMonth()+1) > 9 ? (date.getMonth()+1) : '0' + (date.getMonth()+1);
var dateD = date.getDate() > 9 ? date.getDate() : '0' + date.getDate();
date.setDate(date.getDate() + 1);
var tY = date.getFullYear();
var tM = (date.getMonth()+1) > 9 ? (date.getMonth()+1) : '0' + (date.getMonth()+1);
var tD = date.getDate() > 9 ? date.getDate() : '0' + date.getDate();
var today = dateY + '-' + dateM + '-' + dateD;
$("#from").val(today);
var tomorrow = tY + "-" + tM + "-" + tD;
$("#to").val(tomorrow);
$("#from").focus(function (){
	if ($("#from").val() == today){
		$("#from").val('');
	}
});
$("#from").blur(function (){
	if (!$("#from").val()){
		$("#from").val(today);
	}
});
$("#to").focus(function (){
	if ($("#to").val() == tomorrow){
		$("#to").val('');
	}
});
$("#to").blur(function (){
	if (!$("#to").val()){
		$("#to").val(tomorrow);
	}
});

$("#oForm").submit(function (){
	if ($("#from").val() && $("#to").val()){
		if ($("#hid").val()){
			return getOrder($("#hid").val());
		}
		alert('数据错误,请刷新');
	}
	alert('请选择入住和退房日期');
	return false;
});
function getOrder(id){
	$.getJSON('/json/getorder/?hid=' + id + '&from=' + $("#from").val() + '&to=' + $("#to").val(),function (json){
		if (json){
			$("#orderPoint").html(json);
			return false;
		}
	});
	$("#oForm").attr('action','/order/confirm/');
	return true;
}
</script> 
