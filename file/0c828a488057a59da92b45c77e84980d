function getcomment(page){
	$.getJSON('/json/getcomment/?id=17008&p=' + page,function(json){
		if (json['msg']){
			$("#guest_review").html(json['msg']);
			for (var i in json['id']){
				var obj = '#small_star' + json['id'][i];
				$(obj).raty({readOnly:true,start: json['star'][i]});
			}
			$("#page").html(json['page']);
		}
	});
}

function makePage(pid){
	pid = pid ? pid : 1;
	getcomment(pid);
	var page = 1;
	var pageHtml = pid > 1 ? '<a href="###" onclick="makePage(' + (pid - 1) + ')">上一页</a>' : '<span>上一页</span>';
	for (var m = 1; m <= page; m++){
		pageHtml += (m == pid ? '<b>' + m + '</b>' : '<a href="###" onclick="makePage(' + m + ')">' + m + '</a>');
	}
	pageHtml += pid < page ? '<a href="###" onclick="makePage(' + (pid + 1) + ')">下一页</a>' : '<span>下一页</span>';
	$("#page").html(pageHtml);
}
makePage();

function checkday(){
	$("#getPrice").toggle();
	if ($("#checkday").css('height') == '20px'){
		$("#checkday").css('height','60px');
	}else {
		$("#checkday").css('height','20px');
	}
}
$('#checkday li.openPrice').click(function (){
	var libox = $($(this).parent()).parent();
	$('#viewPrice').html($(this).attr('title'));
	$('#getPrice').html($(this).html());
	checkday();
});
