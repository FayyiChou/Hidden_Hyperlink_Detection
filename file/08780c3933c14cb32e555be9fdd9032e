            //setInterval("timeInfo.innerHTML='  星期'+'日一二三四五六'.charAt(new Date().getDay())+' '+new Date().toLocaleString();",1000);
            $('#notifyContainer').vTicker({
                speed: 500,
                pause: 3000,
                animation: 'fade',
                mousePause: true,
                showItems: 8
            });


            $('.nav-tabs > li > a').hover(function() {
