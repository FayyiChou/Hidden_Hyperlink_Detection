					<script type="text/javascript">
				(function ($) {
					
					$('.priceLinksBoxV3close, .priceAllCatLinkClose').click(function(){
						$('.priceAllCatLinkClose').addClass('priceAllCatLink_hidden');
						$('.priceAllCatLinkOpen').removeClass('priceAllCatLink_hidden');
						$('.priceLinksBoxV3').animate({left: 230}).removeClass('priceLinksBoxV3_shadow');
						return false;
					});
					
					$('.priceAllCatLinkOpen').click(function(){
						$(this).addClass('priceAllCatLink_hidden');
						$('.priceAllCatLinkClose').removeClass('priceAllCatLink_hidden');
						$('.priceLinksBoxV3').animate({left: 473}).addClass('priceLinksBoxV3_shadow');
						return false;
					});
					
					$('.priceBodyTable tr:not(.priceSectionHeader)').click(function(){
						$(this).toggleClass('priceRowSelected');
					});
					
					// показать сортамент     
					$('.showSortamentNameLink').click(function(){
						$('#priceBoxTable').toggleClass('showSortamentName');
						return false;
					});
					
					
					// прокрутка до категории (на главной)
					$('.priceLinksBox a').click(function(){

						var t = $(this);
						var rel = t.attr('rel');
						var pos = $('.'+rel).position().top;
						$('.priceBody').animate({
							scrollTop: pos
						}, 600);
						
						$('.priceLinksBox a').removeClass('priceLinksActive');
						t.addClass('priceLinksActive');
						
						return false;
					});
					
					// Table Search
					$("#priceSearchInput").keyup(function(){
						if( $(this).val() != "") {
							// показываем результаты поиска
							$("#priceBodyTable tbody>tr").hide();
							$("#priceBodyTable td.views-field-title:contains-ci('" + $(this).val() + "')").parent("tr").show();	
							$('#priceSearchLineClear').addClass('priceSearchLineClearVisible');
							$('#priceBoxTable').addClass('showSortamentName');
							var len = $('#priceBodyTable tr:visible').length;
							if(len == 0){
								$('#noResultsBox').addClass('noResultsBoxVisible');
							} else {
								$('#noResultsBox').removeClass('noResultsBoxVisible');
							}
						} else {
							// сбрасываем результаты     
							$("#priceBodyTable tbody>tr").show();
							$('#priceSearchLineClear').removeClass('priceSearchLineClearVisible');
						}
					});
					
					var searchText  = 'Поиск...';
					
					// очистить результаты поиска, нажав крестик       
					$('.priceSearchLineClear, .noResultsClearSearch').click(function(){
						$("#priceBodyTable tbody>tr").show();
						$('#priceSearchLineClear').removeClass('priceSearchLineClearVisible');
						$('#priceSearchInput').attr('value', searchText).addClass('searchPassive');
						$('#noResultsBox').removeClass('noResultsBoxVisible');
						return false;
					});

					// jQuery expression for case-insensitive filter
					$.extend($.expr[":"], {
					    "contains-ci": function(elem, i, match, array) {
							return (elem.textContent || elem.innerText || $(elem).text() || "").toLowerCase().indexOf((match[3] || "").toLowerCase()) >= 0;
						}
					});
					
					
				
					$.fn.textCheck = function(options){
						var defaults = {
							text: this.val()
						},  opts = $.extend(defaults, options);

						//this.val(opts['text']);

						this.focus(function() {
							if($(this).val() == opts['text']) {
								$(this).val('');
								$(this).removeClass('searchPassive');
							} 
						});

						this.blur(function() {
							if($(this).val() == '') {
								$(this).val(opts['text']);
								$(this).addClass('searchPassive');
							}
						});
					};
					
					
					
					

					$('#priceSearchInput').attr('value',searchText);

					if($('#priceSearchInput').length){
						$('#priceSearchInput').textCheck({text: searchText});
					}


				})(jQuery);
		
				
			</script>
				
				
	</div>

