                </div>
            </div>
            <script type="application/javascript">
                function block(b) {
                    var c = document.getElementById(b);
                    c.style.display = "block";
                }
                function none(b) {
                    var c = document.getElementById(b);
                    c.style.display = "none";
                }
                function login_popup_click() {
                    $('.content #register_popup').removeAttr('class');
                    $('.content #login_popup').attr('class', 'cur');
                    $('.content #register_panel').addClass('Hide');
                    $('.content #login_panel').removeClass('Hide');
                    $('.content #type_id').val('login');
                    $('.content #login_img_l').attr('src', "/Index/yzm");
                    $('#okay1').show();
                    $('#okay2').hide();
                }
                function register_popup_click() {
                    $('.content #login_popup').removeAttr('class');
                    $('.content #register_popup').attr('class', 'cur');
                    $('.content #login_panel').addClass('Hide');
                    $('.content #register_panel').removeClass('Hide');
                    $('.content #type_id').val('register');
                    $('.content #login_img_r').attr('src', "/Index/yzm");
                    $('#okay1').hide();
                    $('#okay2').show();
                }
                //点击根据登陆状态不同显示不同弹窗
                function checklogin() {
                    if ($("#userid").val() == 0) {
                        block("shade2");
                    } else {
                        block("shade1")
                    }
                }

                function login_submit1() {
                    var buy_name = document.getElementById("buy_name_l").value;//声明变量的值并且赋值给变量
                    var buy_pass = document.getElementById("buy_pass_l").value;
                    var code = document.getElementById("code_l").value;
                    var code_img = document.getElementById("login_img_l");
                    $.ajax({
                        url: "/Login/buy_veri",//发送请求的地址（默认为当前页面）
                        type: 'POST',//请求方式默认为GET
                        data: {username: buy_name, password: buy_pass, code: code},//发送到服务器的数据
                        dataType: 'html',//预期服务器要返回的数据类型
                        success: function (html) { //上边的成功就回调函数
                            if (html == '1') {
                                information.style.display = "none";  //#.style.display="none" 是隐藏对象
                                window.location = "/User/index"; //返回所使用的web协议
                            } else if (html == '2') {
                                information.style.display = "block";
                                tishitubiao.innerHTML = "登录失败，用户名或密码错误！";//获取HTML当前标签的起始和结束之间的内容
                            } else if (html == '0') {
                                information.style.display = "block";
                                tishitubiao.innerHTML = "登录失败，请检查验证码是否正确！";
                            } else if (html == '3') {
                                alert("抱歉,您的帐号出现异常,如有疑问请联系客服！");
                            } else {
                                information.style.display = "block";
                                tishitubiao.innerHTML = "登录失败,请联系客服！";
                            }
                            if (html != '1') {
                                code_img.src = "/Index/yzm?" + Math.floor(Math.random() * 1001);
                                document.getElementById("code_l").value = '';
                            }
                        }
                    });
                }
                <!--注册-->
                function register_submit2() {
                    var buy_name = document.getElementById("buy_name_r").value;
                    var buy_pass = document.getElementById("buy_pass_r").value;
                    var buy_emall = document.getElementById("buy_email_r").value;
                    var code = document.getElementById("code_r").value;
                    /*var J_reg_agreement=document.getElementById("J_reg_agreement");*/
                    var code_img = document.getElementById("login_img_r");
                    $.ajax({
                        url: "/User/index",   //post过去的页面
                        type: 'POST',
                        data: {username: buy_name, password: buy_pass, email: buy_emall, code: code},
                        dataType: 'json', //C到v的传输类型
                        success: function (data) {
                            if (data.status == '1') {
                                window.location = "/User/index";  //注册成功后跳转到这个/User/index                            } else {
                                $("#buy_name_r").removeClass("Hide"); //$("#buy_user_infor")相当于获取id=buy_user_info然后移除
                                $("#buy_pass_r").removeClass("input-success");
                                $("#buy_email_r").addClass("input-error");
                                $("#login_img_r").html(data.info);
                                code_img.src = "/Yzm/index?" + Math.floor(Math.random() * 1001);
                                document.getElementById("code_r").value = '';
                            }
                        }
                    });
                }
                $(function () {
                    $("#trygoods").click(function () {
                        var uid = $("#userid").val();
                        var gid = $("#gid").val();
                        var alipay = $('.content input[name=taobao_radio]:checked').val();
                        var jifen = $("#jifen").val();
                        var msg = $(".sendMsg").val().trim();
                        applay(uid, gid, alipay, jifen, msg);
                    });
                });
                function applay(uid, gid, alipay, jifen, msg) {
                    $.post("/Goodstry/add_try", {'uid': uid, 'gid': gid, 'alipay': alipay, 'msg': msg, 'jifen': jifen}, function (info) {
                        alert(info.meg);
                        if (info.status == 1) {
                            window.location.reload();
                        }
                    });
                }
            </script>
        </div>

        
