							}
							t.parentNode.className = "selecttag";
							
							$("#tabbox" + type).show().siblings().hide();
							if(type != 0 && c == 1){
								$.ajax({
										url: "/Goodstry/userinfo",
										type: 'POST',
										data: {id: id, type: type},
										dataType: 'json',
										beforeSend: function(){
											$("#tabbox" + type + " .try-user-list").html("数据获取中...");
										},
										success: function(data){
											if(data.status == 1){
												$("#tabbox" + type + " .try-user-list").html(data.html);
