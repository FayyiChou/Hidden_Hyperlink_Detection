  <!-- 接口 -->
<script>
    var warehouse = $.Cookie.get('vip_wh').toLowerCase() || "vip_nh";
    var K_SVR = {
        //根据分类获取档期数据
        getBrands: function(params) {
            //desubfloorment_id: 品牌的部类,
            var defaults = {
                page: 0,
                method: "search.brand.base",
                warehouse: warehouse,
                fields: 'brand_id,name,link,img,index_image,agio,sell_time_from,sell_time_to,desubfloorment_id,new_cat_id,mobile_image_one'
            };
            return $.ajax({
                url: 'http://resys.vip.com',
                type: 'GET',
                dataType: 'jsonp',
                data: $.extend(defaults, params)
            });
        },

        //根据分类获取商品信息
        getGoods: function(params) {
            var defaults = {
                page: 0,
                method: "search.product.base",
                warehouse: warehouse,
                fields: 'pid,name,link,img,foreword,market_price,vipshop_price,agio,brand_id'
            };
            return $.ajax({
                url: 'http://resys.vip.com',
                type: 'GET',
                dataType: 'jsonp',
                data: $.extend(defaults, params)
            });
        }
    };


</script>

<!-- 数据 -->
<script class="">
    var K_FLOORS, K_LOADERS, K_PAGEID, K_BLIST_LDR, K_FConf, K_FLoader;

    K_PAGEID = 190001;
    //强制删除档期
    K_DELETE_THESE_BRANDS = '634884,661364,672296';
    //隐藏过期档期
    K_DELETE_OFFLINE_BRANDS = true;
    //设置楼层顺序
    K_FLOOR_RANKS = 'babycloth,boycloth,girlcloth,shoes,diaper,powder,feed,mom,vehicle,toy,family';

    //楼层配置
    K_FLOORS = {

        addConf: function(oConf) {
            this[oConf.name] = oConf;
        },
        getConf: function(name) {
            return this[name] || null;
        },
        getRanks: function() {
            return this.names || '';
        },
        setRanks: function(names) {
            this.names = names;
        },
        getRankedFloors: function() {
            var names = this.names.split(','), self = this;
            return names.length ? $.map(names, function(name) {return self[name];}) : [];
        }
    };

    //加载器
    K_LOADERS = {
        loaders: [],

        //为楼层需找适合的加载器
        fit: function(oFloor) {
            var i, o, oRet;
            oRet = null;
            i = this.loaders.length;
            while( (i = i-1) >= 0 ) {
                o = this.loaders[i];
                if ( o && o.match && o.match(oFloor) ) {
                    oRet = o;
                    break;
                }
            }

            return oRet;
        },

        //添加一个加载器
        add: function(loader) {
            loader && this.loaders.push(loader);
        }
    };

    //楼层配置
    K_FConf = function(options) {
        if ( !(this instanceof K_FConf) ) {
            return new K_FConf(options);
        }
        $.extend(this, options);
    };

    K_FConf.prototype = {
        constructor: K_FConf,
        name: '',
        tags: [],
        addClass: '',
        navable: false,
        warehouse: {'vip_nh': 1, 'vip_sh': 2, 'vip_bj': 3, 'vip_cd': 4, 'vip_hz': 5}[$.Cookie.get('vip_wh').toLowerCase()] || 1,
        hasTag: function(tag) {
            return this.tags.join(',').indexOf(tag) >= 0;
        }
    };

    //楼层数据加载器
    K_FLoader = function(options) {
        if ( !(this instanceof K_FLoader) ) {
            return new K_FLoader(options);
        }
        $.extend(this, options);
    };

    K_FLoader.prototype = {
        constructor: K_FLoader,
        match: function(oFloor) {
            return false;
        },
        load: function(oFloor) {
            return $.Deferred().resolve().promise();
        }
    };



