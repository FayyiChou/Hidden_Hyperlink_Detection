<script type="text/html" id="J_floors_tpl">
    <ul class="k-floors k-mid-bg k-960" id="J_floors">
        {{each floors as F}}
        <li class="k-floor{{each F.tags as tag}} k-floor-{{tag}}{{/each}}{{if F.navable}} k-floor-navable{{/if}} {{F.addClass}}" data-fname="{{F.name}}" id="J_floor_{{F.name}}">
            {{if F.addrpara}}
            <a class="k-floor-link" href="http://category.vip.com/search-{{F.warehouse}}-0-1.html?{{F.addrpara}}" target="_blank"></a>
            {{/if}}
            {{if F.hasTag('brands')}}
            <div class="k-960">
                <div class="k-brands clearfix">
                    {{each F.brands as brand}}
                    <a class="k-brand" href="{{brand.link}}" target="_blank">
                                    <span class="k-brand-pic">
                                        <img class="k-brand-img lazy" src="http://a.vpimg4.com/upload/actpics/uidesign/2016/2m/0225kidcat/brand-hold.v2.jpg" data-original="{{brand.story_logo | brandLogo: brand.brand_id}}">
                                        <i class="k-brand-imgrect"></i>
                                    </span>
                        <i class="k-brand-name" title="{{brand.name}}">{{brand.name}}</i>
                    </a>
                    {{/each}}
                </div>
            </div>
            {{else if F.hasTag('entries')}}
            <div class="k-960">
                <div class="k-entries clearfix">
                    {{each F.entries as entry}}
                    {{if F.name === 'family'}}
                    <a class="k-entry k-entry-link k-typeB-entry" href="{{entry.link}}" target="_blank">
                        <img class="k-entry-img k-typeB-img lazy" src="http://a.vpimg4.com/upload/actpics/uidesign/2016/2m/0225kidcat/hold-272-131.jpg" data-original="{{entry.img}}">
