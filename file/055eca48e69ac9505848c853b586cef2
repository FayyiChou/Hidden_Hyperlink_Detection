<script type="text/html" id="J_tnav_tpl">
    <div class="k-tnav {{classes}}" id="{{id}}">
        {{each floors as floor}}
        {{if floor.navable && !floor.hide}}
        <div class="J_tnav_item J_tnav_item_{{floor.name}} k-tnav-item k-tnav-item-{{floor.name}} k-tnav-bg k-tnav-ptr {{if floor.kext_is_last_navable}}k-tnav-lastitem{{/if}}" data-floorname="{{floor.name}}" style="width: {{floor.kext_tnav_wid}}" mars_sead="click_topbar_{{floor.name}}">
            <i class="k-tnav-title">{{floor.title}}</i>
        </div>
        {{/if}}
        {{/each}}
    </div>
</script>

<script>

    $.Tpl.helper('brandLogo', function(link, brandId){
        var maps = {
            790455: 'http://a.vpimg4.com/upload/actpics/uidesign/2016/changzhu/qijiandian/haoqi_v1.jpg',
            783321: 'http://a.vpimg4.com/upload/actpics/uidesign/2016/changzhu/qijiandian/haoqi_v1.jpg',
            784741: 'http://a.vpimg4.com/upload/actpics/uidesign/2016/changzhu/qijiandian/haoqi_v1.jpg'
        };
        return maps[brandId] || link;
    });

    K_SNAV = {
        inited: false,
        drawed: false,
        narrow: false,

        _default: {
            //将导航限制在某个容器的上下两边之间
            container: '#dap',
            upperedge: 0,
            loweredge: 0,
            tpl: '',
            aniTime: 200,
            period: 200,
            data: {floors: [], classes: ''},

            getFloor: function(floorname) {
                return $('#floor-' + floorname);
            },

            getNav: function() {
                return $('#J_snav');
            },

            //对楼层进行扩展，扩展之后的属性可以被导航模板使用
            extFloors: function(floors) {
                return floors;
            },

            //导航是否能看到, this 指向 Snav
            visible: function($nav) {
                return $nav.is(':visible');
            },

            //导航是否要显示, this 指向 Snav
            canShow: function(oWin, oBox, oNav) {
                return !this.narrow
                        && oWin.scrollTop >= (oBox.top + this._options.upperedge);
            },

            //导航定位到某处, this 指向 Snav
            locate: function(oWin, oBox, oNav) {
                var iBoxBottom, iNavCssTop;
                iBoxBottom = oBox.top + oBox.height;
                iNavCssTop = (oWin.height - oNav.height)/2;
                iNavCssTop = Math.min(iNavCssTop , iBoxBottom - this._options.loweredge - oNav.height - oWin.scrollTop);
                oNav.jqo.css('top', iNavCssTop + 'px');
            },

            //显示隐藏导航，可以通过样式或类达到效果，此处定制可选择不同方式、甚至采用动画
            show: function($nav) {
                $nav.show();
            },
            hide: function($nav) {
                $nav.hide();
            }
        },
        _options: {},

        /*
         事件绑定(回调频率可控)
         在本模块内被用于绑定scroll事件, 特点是，回调函数触发频率可控
         */
        binder: {
            on: function($dom, evt, callback, period) {
                var iToutid = 0;

                $dom.on(evt, function(event) {
                    if ( iToutid ) {
                        return;
                    }
                    iToutid = setTimeout(function() {
                        iToutid = null;
                        callback();
                    }, period);
                });
            }
        },


        init: function(options) {
            if ( this.inited ) {
                return;
            }
            this.inited = true;
            $.extend(this._options, this._default, options);
            var _this, aFloor;
            _this = this;
            aFloor = this._options.data.floors || [];
            this._options.data.floors = this._options.extFloors(aFloor);
            this.draw();
            this.bind();
        },

        draw: function() {
            //首次绘制
            if ( !this.drawed && (this.drawed = true) ) {
                var fHtml = $.Tpl.compile(this._options.tpl);
                $(this._options.container).append(fHtml(this._options.data));
            }

            //显示隐藏定位
            var $box, $nav, $win, oBox, oNav, oWin, visible, stp;

            $box = $(this._options.container);
            $nav = this._options.getNav();
            $win = $(window);

            oWin = {width: $win.width(), height: $win.height(), scrollTop: $win.scrollTop(), jqo: $win};
            oBox = $.extend($box.offset(), {jqo: $box, width: $box.width(), height: $box.height()});
            oNav = $.extend($nav.offset(), {jqo: $nav, width: $nav.width(), height: $nav.height()});
            visible = this._options.visible.call(this, $nav);

            //显示隐藏
            if ( this._options.canShow.call(this, oWin, oBox, oNav) ) {
                if ( !visible ) {
                    this._options.show($nav);
                    visible = true;
                }
            } else {
                if ( visible ) {
                    this._options.hide($nav);
                    visible = false;
                }
            }

            //可见才定位
            if ( visible ) {
                this._options.locate.call(this, oWin, oBox, oNav);
            }
        },

        bind: function() {
            var _this = this, $doc, $nav, $win;

            $.Listeners.sub('k_floor_change').onsuccess(function(data) {
                _this.mark(data.newfloor.name);
            });

            $.Listeners.sub('k_win_too_narrow').onsuccess(function(data){
                _this.narrow = true;
                _this.hide();
            });

            $.Listeners.sub('k_win_wid_enough').onsuccess(function(data) {
                _this.narrow = false;
                _this.show();
            });


            $nav = this._options.getNav();
            $win = $(window);

            $nav.on('click.snav.item', '.J_snav_go_floor', function(event) {
                var $item, floorname;
                $item = $(this);
                floorname = $item.attr('data-floorname');
                if ( floorname && !$item.hasClass('cur') ) {
                    _this.jump(floorname);
                }
            });

            this.binder.on($win, 'scroll.snav', function(event) {
                _this.draw();
            }, this._options.period);

            this.binder.on($win, 'resize.snav', function(event) {
                _this.draw();
            }, this._options.period);


            $('.J_snav_go_top').on('click.snav.backtop', function(event) {
                $('html,body').animate({scrollTop: 0}, _this._options.aniTime);
            });
        },

        show: function() {
            this._options.show(this._options.getNav());
            this.draw();
        },
        hide: function() {
            this._options.hide(this._options.getNav());
        },

        //标记某个楼层为当前楼层
        mark: function(floorname) {
            var $item = $('#J_snav_go_floor_' + floorname);
            if ( $item[0] ) {
                $('.J_snav_go_floor').removeClass('cur');
                $item.addClass('cur');
            }
        },

        //跳转到某个楼层
        jump: function(floorname) {
            var $floor;

            $floor = this._options.getFloor(floorname);

            //有效性
            if ( !($floor && $floor[0]) ) {
                return;
            }

            //定制的跳转
            if ( $.isFunction(this._options.jump) ) {
                this._options.jump($floor);
                return;
            }

            $('html,body').animate({scrollTop: $floor.offset().top}, this._options.aniTime);
        }
    };

    K_TNAV = {
        defaults: {},
        options: {},
        init: function(options) {
            var fNavHtml, sNavHtml;
            fNavHtml = $.Tpl.compile($('#J_tnav_tpl').html());
            $.extend(this.options, this.defaults, options);

            this.ext(options.floors);

            sNavHtml = fNavHtml({
                id: 'J_static_tnav',
                classes: 'k-static-tnav',
                floors: this.options.floors
            });

            $('#J_tnavs').append(sNavHtml);

            sNavHtml = fNavHtml({
                id: 'J_fixed_tnav',
                classes: 'k-fixed-tnav',
                floors: this.options.floors
            });

            $('#J_tnavs').append(sNavHtml);

            this.bind();
        },
        ext: function(floors) {
            //计算每个楼层项的宽度
            var i, count, total, average, lastwidth;

            count = 0;
            $.each(floors, function(i, f) {
                if ( f.navable ) {
                    count++;
                }
            });

            total = $('#J_floors').width();
            average = Math.floor(total/count);
            lastwidth = total - average*(count-1);
            i = 0;
            $.each(floors, function(i, f) {
                if ( f.navable ) {
                    if ( i === count-1 ) {
                        f.kext_tnav_wid = lastwidth + 'px';
                        f.kext_is_last_navable = true;
                    } else {
                        f.kext_tnav_wid = average + 'px';
                    }
                    i++;
                }
            });
        },
        bind: function() {
            var $w, $still, $fixed, iMinSTP, bShowTNav, iTnavHeight;

            $w =  $(window);
            $body = $('body');
            $still = $('#J_static_tnav');
            $fixed = $('#J_fixed_tnav');
            bShowTNav = false;


            $.Listeners.sub('k_win_too_narrow').onsuccess(function(data){
                bShowTNav = true;
                $('#J_static_tnav').show();
                $('#J_fixed_tnav').show();
            });

            $.Listeners.sub('k_win_wid_enough').onsuccess(function(data) {
                bShowTNav = false;
                $('#J_static_tnav').hide();
                $('#J_fixed_tnav').hide();
            });


            $.Listeners.sub('k_floor_change').onsuccess(function(data) {
                var oldFname, newFname, $oldItem, $newItem;

                newFname = data.newfloor.name;
                $newItem = $('.J_tnav_item_' + newFname);

                $oldItem = $('.J_tnav_curitem');
                oldFname = $oldItem.attr('data-floorname');

                if ( !newFname || newFname === oldFname ) {
                    return;
                }

                $oldItem.removeClass('J_tnav_curitem');
                $newItem.addClass('J_tnav_curitem');
            });


            $body.on('click.tnav', '.J_tnav_item', function(event) {
                var $item, $f, name;
                $item = $(this);
                if ( $item.hasClass('cur') ) {
                    return;
                }
                name = $item.attr('data-floorname');
                $f = $('#J_floor_' + name);
                $('html,body').animate({scrollTop: $f.offset().top - $('#J_static_tnav').height()}, 200);
            });

            $w.on('scroll.tnav', function(event) {
                if ( !bShowTNav ) {
                    return;
                }

                if ( $w.scrollTop() < $('#J_static_tnav').offset().top ) {
                    $fixed.hide();
                } else {
                    $fixed.show();
                }
            }).trigger('scroll.tnav');
        }
    };


    $(function() {
        var aFloor, aHtml, aPromise, fFloorHtml;

        aHtml = [];
        aPromise = [];
        aFloor = K_FLOORS.getRankedFloors();


        $.each(aFloor, function(index, oFloor) {

            //优先使用父楼层的加载器
            if (loader = K_LOADERS.fit(oFloor) ) {
                aPromise.push(loader.load(oFloor));
                return;
            }

            //再考虑子楼层的加载器
            $.each(oFloor.subfloors || [] , function(index, oSubFloor) {
                //为楼层寻找合适的加载器并加载
                if ( loader = K_LOADERS.fit(oSubFloor) ) {
                    aPromise.push(loader.load(oSubFloor));
                }
            });
        });


        $.when.apply($, aPromise).done(function() {
            $('#J_footer').before($.Tpl('J_floors_tpl', {floors: aFloor}));

            $.Loader.advScript({
                name: 'lazyload-brand-img',
                def: function () {
                    $('img.lazy').lazyload();
                },
                requires: ['lazyload']
            });

            //导航
            K_NAV_SENSOR.init({
                //触发周期
                scroll_exec_period: 50,
                resize_exec_period: 100,
                //窗口宽度临界值
                critical_win_width: 1260,//1000+左导航宽度
                //各楼层选择器或者jQ对象
                floors: '.k-floor-navable',
                //获取楼层的名字
                getFloorName: function($floor) {
                    return $floor.attr('data-fname');
                },
                //参考点的y坐标, 默认为视口下边缘的视口坐标，可以传递具体的值
                getPtClientY: function(winheight) {
                    return 100;
                }
            });



            var $floor1, offtop;
            $floor1 = $('.k-floor:first-child');
            offtop = $floor1.offset().top + parseInt($floor1.css('padding-top'));
            K_SNAV.init({
                container: '#J_cat_page',
                upperedge: $('#J_banner').height(),
                loweredge: $('#J_footer').height(),

                tpl: $('#J_snav_tpl').html(),
                data: {
                    floors: aFloor,
                    classes: '' //给导航加类
                },
                getFloor: function(name) {
                    return $('#J_floor_' + name);
                },
                extFloors: function(floors) {
                    return floors;
                },
                //导航定位到某处, this 指向 Snav
                locate: function(oWin, oBox, oNav) {
                    // var iBoxBottom, iNavCssTop;
                    // iBoxBottom = oBox.top + oBox.height;
                    // iNavCssTop = (oWin.height - oNav.height)/2;
                    // iNavCssTop = Math.min(iNavCssTop , iBoxBottom - this._options.loweredge - oNav.height - oWin.scrollTop);
                    // oNav.jqo.css('top', iNavCssTop + 'px');
                    // if ( oWin.scrollTop <= offtop ) {
                    //  oNav.jqo.css({top: offtop + 'px', bottom: 'auto'});
                    // } else {
                    //  oNav.jqo.css({top: 'auto', bottom: '145px'});
                    // }

                    if ( oWin.scrollTop + oWin.height - offtop >= 585 ) {
                        oNav.jqo.css({top: 'auto', bottom: '145px'});
                    } else {
                        oNav.jqo.css({top: offtop + 'px', bottom: 'auto'});
                    }

                },

                canShow: function(oWin, oBox, oNav) {
                    return !this.narrow;
                }
            });


            K_TNAV.init({
                floors: aFloor
            });
        });
    });
</script>

 
