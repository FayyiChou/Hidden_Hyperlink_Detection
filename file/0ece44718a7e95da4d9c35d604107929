		<script>
			$.ajax({
				url:'/webnavigation/show',
				type:'post',
				data:{},
				dataType:'json',
				success:function(data){
					console.log(data);
					var children = new Array();
					for(var i = 0 ; i < data.length ; i++) {
						 $(".m-help .inner").append('<dl class="link-list" id="list_'+i+'">'+
								'<dt class="link-hd">'+data[i].navname+'</dt>'+
								'</dl>'	)
								
						if(data[i].children.length != 0){
							for(var j = 0 ; j < data[i].children.length ; j++) {
								$("#list_"+i).append(
								   '<dd class="link-dd">'+
							         '<a href="'+data[i].children[j].linkurl+'" class="link" target="_blank">'+data[i].children[j].navname+'</a>'+
							       '</dd>'
								)
							}
						}
					}
				}
			});
			
		</script>
