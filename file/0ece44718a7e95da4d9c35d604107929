<script type="text/javascript">
	//访问记录统计
	$(function(){
				var host = window.location.host;
				var url = "http://"+host+"//view_count.jsp";
				var view_link = escape(window.location.href);
				$.ajax({
					type:"post",
					url:url,
					async:true,
					data:{
						"view_link":view_link
					},
					success:function(data){
						if(data.indexOf('ok')>=0){
							console.log('访问日志添加'+view_link);	
						}
					}
				});
			});
</script>

