            //添加留言
            function add_message() {

                if ($("#txtUserName").val() == "") {
                    alert("请输入姓名！");
                    return false;
                }
             /*   else if ($("#txtway").val() == "") {
                    alert("请输入年龄！");
                    return false;
                }
                else if ($("#txtcode").val() == "") {
                    alert("请输入学历！");
                    return false;
                }
*/
                else if ($("#txtUserTel").val() == "") {
                    alert("请输入联系电话！");
                    return false;
                }

                /*else if ($("#txtCode").val() == "") {
                    alert("请输入验证码！");
                    return false;
                }*/
                else {

                    $.getJSON("../../../tools/submit_ajax.ashx?r=" + Math.random(), { action: "add_feedbackzhai", _user_name: $("#txtUserName").val(), _user_tel: $("#txtUserTel").val(), _title: "在线报名", _user_content: $("#txtContent").val(), _code: $("#txtCode").val(), _sex: $("input:radio[name=sex]:checked").val(), _address: $("#txtAddress").val(), _company: $("#txtCompany").val(), _user_qq: $("#txtuser_qq").val(), _way: $("#txtway").val(), _code2: $("#txtcode").val() }, function (json) {
                        alert(json.msgbox);
                        if (json.msg == "1") {
                            document.location.reload();
                            $("#txtUserName").val("");
                            $("#txtway").val("");
                            $("#txtcode").val("");
                            $("#txtCompany").val("");
                            $("#txtUserTel").val("");
                            $("#txtuser_qq").val("");
                            $("#txtCode").val("");
                            $("#txtAddress").val("");
                            $("#txtContent").val("");
                        }
                    });
                }
            }
    </script>
