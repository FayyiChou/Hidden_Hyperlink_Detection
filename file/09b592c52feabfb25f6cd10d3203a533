            <script>
jq(function(){
String.prototype.Right = function(i) { //为String对象增加一个Right方法
                    return this.slice(this.length - i,this.length); //返回值为 以“该字符串长度减i”为起始 到 该字符串末尾 的截取字符串
                    };
                    
jq(".ch_sb").each(function(){
//alert(jq(this).attr("src"));
                        var text = jq(this).attr("src");
                        var newtext = text.Right(11);
                        if(newtext != "nophoto.gif"){
                            jq(this).parent().parent().show();
                            } 
                        });

});
            </script>
            
            
