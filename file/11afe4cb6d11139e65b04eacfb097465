var step1=0;
function slideit1(){
	var oImg1 = document.getElementById("javascript.img1");
	if (document.all){oImg1.filters.blendTrans.apply();}
	oImg1.src=ImgSrc1[step1];
	if (document.all){oImg1.filters.blendTrans.play();}
	step1 = (step1<(ImgSrc1.length-1))?(step1+1):0;
	(new Image()).src = ImgSrc1[step1];
}
