<script type="text/javascript">
    var isShowComboInfo = false;
    function bindCommentLst(type) {
        location.href = "/hotel/dianping/5771542_p1t" + type + ".html";
    }
    
    function gotoAnchor() {
        var url = window.location + '';
        var res = url.match(/#(dianping|book)/i);
        if (res) {
            var obj = document.getElementById(res[1].toLowerCase());
            if (obj) {
                var rect = obj.getBoundingClientRect();
                var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
                window.scrollTo(0, rect.top + scrollTop);
            }
        }
    }
    gotoAnchor();
    function commentsubmit(obj) {
        var a = obj.id;
        if (document.getElementById("__SSO_iframe_0")) {
            document.getElementById("__SSO_iframe_0").src = "/Domestic/LoginCheck.aspx?a=" + a + "&t=0&b=C6023559E1443B966993539FA4A1C272";
        }
    }

    function repaint() {
        if (Raphael != undefined) {
            $('.J_bp svg').remove();
            $('.J_bp shape').parentNode().remove(); //for IE7
            $('.J_bp').each(function(item,idx){
                var fn = eval("(" + $(item).attr("params") + ")");
                fn && fn.data && fn.data();
            })
        }
    }
</script>
