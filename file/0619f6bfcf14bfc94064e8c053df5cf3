    function LoadAjaxGetHotelRelationInfo() {
        var scrollWaiter = {
            disabled : false,
            _requestCallback : false,
            _eventCallback : false,
            start: function (delay, targetElementId, requestCallback) {
                if (targetElementId && requestCallback) {
                    this._requestCallback = requestCallback;
                    this._bindEvents(delay, targetElementId);
                }
            },
            _bindEvents: function (delay, targetElementId) {
                var self = this,
                    timeout = null;

                var callbackFn = function () {
                    var $target = $(targetElementId);
                    if (self._isComingUp($target)) {
                        self._request();
                    }
                };

                var eventFn = function () {
                    clearTimeout(timeout);
                    timeout = setTimeout(callbackFn, delay || 200);
                };

                this._eventCallback = eventFn;

                $(window).bind('scroll', eventFn);
                $(window).bind('resize', eventFn);
            },
            _request: function () {
                if (this.disabled) {
                    return;
                }
                this.disabled = true;
                $(window).unbind('scroll', this._eventCallback);
                $(window).unbind('resize', this._eventCallback);

                this._requestCallback && this._requestCallback();
            },
            _isComingUp: function (el) {
                var offset = el.offset(),
                    docOffsetTop = document.documentElement.scrollTop || document.body.scrollTop,
                    viewAreaHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
                return offset.top <= docOffsetTop + viewAreaHeight + 500;
            }
        };

        var relatePanel = (hotelDomesticConfig.bookingOver || hotelDomesticConfig.bookOverView) ? '#AdviceHotelHasPrice2' : '#AdviceHotelHasPrice';
        var relationInfoAjax = function() {
            $.ajax(addressUrlConfig.AjaxGetHotelRelationInfo, {
                cache: true,
                onsuccess: function (response) {
                    if (response && response.responseText) {
                        if ((hotelDomesticConfig.isghi == "T" || isShowResult == false) && $('#adviseHotel').length > 0) {
                            $('#adviseHotel').html(response.responseText);
                            $(relatePanel).html('');
                        } else {
                            $(relatePanel).html(response.responseText);
                        }
                        var around = $("#around_same_type_hotel");
                        if(around.length > 0){
                            var aroundsametype_htllist = [];
                            around.find("li").each(function(dom,index){
                                aroundsametype_htllist.push({
                                    hotelid:dom.attr("hotel-id"),
                                    amount:dom.attr("hotel-amount")
                                });
                            });
                            var value = { hotelid: $("#hotel").value(), "aroundsametype_htllist": aroundsametype_htllist };
                            window['__bfi'].push(['_tracklog', "hotel_inland_detail_nearbyhtl", $.stringifyJSON(value)]);
                        }                     
                    }
                }
            });
        };
        if (hotelDomesticConfig.bookOverView || (isShowResult == false && $('#adviseHotel').length > 0 && $('#adviseHotel').html() == "")) {
            relationInfoAjax();
        } else {
            var $relatePanel = $(relatePanel);
            scrollWaiter.start(200, relatePanel, relationInfoAjax);
        }
        
    }
    var $globalPad = true; //support ctrip on pad
    var $isPad = /pad/.test(navigator.userAgent.toLowerCase());
    $LAB.script({ src: 'http://webresource.c-ctrip.com/code/cquery/cQuery_110421.js', charset: 'utf-8' }).wait()
        .script({ src: 'http://webresource.c-ctrip.com/reshotelcasonline/R6/js/butterfly.js?releaseno=2017-03-31-18-42-00', charset: 'utf-8' }).wait()
        .script({ src: 'http://webresource.c-ctrip.com/ResHotelOnline/R5/search/js.merge/base-bmap.js?releaseno=2017-03-31-18-42-00', charset: 'utf-8' }).wait()
.script({ src: 'http://webresource.c-ctrip.com/ResHotelOnline/R5/search/js.merge/showhotelinformation.js?releaseno=2017-03-31-18-42-00', charset: 'utf-8' }).wait(loadCallback);
</script>

<script type="text/javascript">
    function initialAdvertiseIds()
    { 
        var valueIds = '';
        if(valueIds != ''){
            var advValue ="version=1.0&channelid=4&moduleid=hod_dl_ad_def&hoteladvertiseid_list="+valueIds;
            _tracklog("hotel.adimpression",advValue);
        }
    }

    //class为 list 的a标签bindclick
      
    function _tracklog(key,value){
        if(typeof window['__bfi'] == 'undefined') window['__bfi'] = [];
        window['__bfi'].push(['_tracklog', key, value]);
    };

    function hiddenAdvertise() {
        $(".discount_box").addClass("hidden");
    }
</script>

