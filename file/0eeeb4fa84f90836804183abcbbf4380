</div>


<script type="text/javascript">
    // 渲染Fake页
    (function(){
        console.log('渲染fake页 -- start');
        var abtest= decodeURIComponent($dom("meta[name='SwiftABTest']").getAttribute("content")||"");
        if (abtest&&abtest.indexOf('170320_fld_newsf:B')<0&&abtest.indexOf('170320_fld_newsf:E')<0) {
            var abinput= document.createElement('input');
            abinput.id="ab_testing_tracker";
            abinput.type="hidden";
            abinput.value=abtest;
            document.body.appendChild(abinput);
        };
        var matrixFake = {
            passengerType: 1,
            searchitem: [
                {
                    dtime: new Date().getTime() + (24 * 60 * 60 * 1000)
                }
            ]
        }
        if(localStorage.getItem('MATRIX_F_SEARCH_INFO')){
            matrixFake = JSON.parse(localStorage.getItem('MATRIX_F_SEARCH_INFO')).value;
        }

        try {
            setActiveFlightTab();
            $dom('.js_city_depart_input').setAttribute('value', matrixFake.dname || '上海');
            $dom('.js_city_arrive_input').setAttribute('value', matrixFake.aname || '北京');

            $dom('#js_index_depart .js_s_index_depart').textContent = formatDate(matrixFake.searchitem[0].dtime, 'MM月dd日') || '';
            $dom('#js_index_depart .js_s_tipes').textContent = getWeek(matrixFake.searchitem[0].dtime) || '';

            if(matrixFake.searchitem[1]){
                $dom('#js_index_arrive .js_s_index_arrive').textContent = formatDate(matrixFake.searchitem[1].dtime, 'MM月dd日') || '';
                $dom('#js_index_arrive .js_s_tipes').textContent = getWeek(matrixFake.searchitem[1].dtime) || '';
            }
        }catch(e){

        }

        console.log('渲染fake页 -- end');

        function $dom(id) {
            return document.querySelector(id);
        };

        function addClass (el, className) {
            if (el.classList){
                el.classList.add(className);
            } else {
                el.className += ' ' + className;
            }
        }

        function removeClass (el, className) {
            if (el.classList) {
                el.classList.remove(className);
            } else {
                el.className = el.className.replace(new RegExp('(^|\\b)' + className.split(' ').join('|') + '(\\b|$)', 'gi'), ' ');
            }
        }

        function setActiveFlightTab () {
            if(localStorage.getItem('MATRIX_F_SEARCH_INFO')){
                var fmode = JSON.parse(localStorage.getItem('MATRIX_F_SEARCH_INFO')).value.fmode || 1;
            }else{
                var fmode = 1;
            }
            var $pass = $dom('.js_passenger');
            if(fmode === 1){
                addClass($dom('[data-flight-type="inland"]'), 'current');
                removeClass($dom('[data-flight-type="inter"]'), 'current');
            }else{
                addClass($dom('[data-flight-type="inter"]'), 'current');
                removeClass($dom('[data-flight-type="inland"]'), 'current');
            }

            var passengerType = matrixFake.passengerType || 1;
            if (passengerType == 2) {
                $pass.querySelector('label').innerHTML = '儿童';
                $pass.querySelector('.passenger-tips').style.display = '';
            } else {
                $pass.querySelector('label').innerHTML = '成人';
                $pass.querySelector('.passenger-tips').style.display = 'none';
            }
            if ( fmode === 1) {
                removeClass($pass, 'dis-non');
                addClass($pass, 'srhpd3_animate_hide');
            } else {
                removeClass($pass, 'dis-non');
                removeClass($pass, 'srhpd3_animate_hide')
            }
        }

        function formatDate (str, fmt) {
            var date = (typeof str == 'string') ? str.match(/\d+(?=\-){1}/g)[0] : new Date().getTime();
            date = new Date(+date);
            var o = {
                "M+": date.getMonth() + 1, //月份
                "d+": date.getDate(), //日
                "h+": date.getHours(), //小时
                "m+": date.getMinutes(), //分
                "s+": date.getSeconds(), //秒
                "q+": Math.floor((date.getMonth() + 3) / 3), //季度
                "S": date.getMilliseconds() //毫秒
            };
            if (/(y+)/.test(fmt)) fmt = fmt.replace(RegExp.$1, (date.getFullYear() + "").substr(4 - RegExp.$1.length));
            for (var k in o)
                if (new RegExp("(" + k + ")").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
            return fmt;
        }
        //获取今天的周信息
        function getWeek(str) { 
            var weekName = ['周日', '周一', '周二', '周三', '周四', '周五', '周六'];

            var date = (typeof str == 'string') ? str.match(/\d+(?=\-){1}/g)[0] : new Date().getTime();
            date = new Date(+date);
            var offDay = parseInt((date.setHours(0, 0, 0, 0) - new Date().setHours(0, 0, 0, 0)) / (24 * 60 * 60 * 1000));
            switch (offDay) {
                case 0:
                    return '今天';
                case 1:
                    return '明天';
                case 2:
                    return '后天';
                default:
                    return weekName[date.getDay()];
            }
        }

    })();
</script>



