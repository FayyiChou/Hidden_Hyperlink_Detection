<script type="text/javascript" crossorigin="" src="http://pages.anjukestatic.com/usersite/site/js/bbv6.js"></script>
<script type="text/javascript" crossorigin="" src="http://include.anjukestatic.com/anjuke-zu/res/20171405/b/Zufang_Web_Rent_List_List.js"></script>
<script type="text/javascript">

$(function(){
    ajk.citySelector = new ajk.DropDown($('#switch_apf_id_5'),$('#city_list'),500);
});

$(function(){
	function Searchbar(op){
    	var search = location.search;
        var query  = '';
        var params = {};
        search.replace(/\?(.*)/g, function(all, q) {
            query = q;
            return '';
        });
        var arr = query.length ? query.split('&') : [];
        for(var i = 0; i< arr.length; i++){
            var keyVal = arr[i].split('=');
            params[keyVal[0]] = decodeURIComponent(keyVal[1]);
        }

		if( params.kw !== undefined && params.kw != ''){
			$('#search-rent').val(params.kw);
			$('#search-close').show();
		}

		var auto = new ajk.Autocomplete($('#search-rent'),{
		        insertDom:$('#search-rent').parent(),
		        template : _.template($('#search-tpl').html()),
		        dataSource: function(keyword,setData){
		            $.ajax({
		                url: op.url,
