<script type="text/javascript">
    var Page = 0;
    $(".pop .ti li").on("click",function(){
        var _i = Array.prototype.indexOf.call(this.parentNode.children,this);
        var _now = Array.prototype.indexOf.call(this.parentNode.children,$(".active",this.parentNode).get(0));
        if(_now == _i) return;
        var pop = this.parentNode.parentNode.parentNode;
        $(".box",pop).get(_now).style.display = 'none';
        $(".box",pop).get(_i).style.display = 'block';
        $(".ti li",pop).eq(_now).removeClass("active");
        $(".ti li",pop).eq(_i).addClass("active");
    });
	    $("#source").on("click",function(){
        $("#select").addClass("show");
        $(".popup-mask").get(0).style.display = 'block';
    });
	    $(".popup-mask").on("click",function(){
        $("#select").removeClass("show");
        $(".popup-mask").get(0).style.display = 'none';
    });
    $("#select li").on("click",function(){
        var _i = this.index;
        $(".select",this.parentNode).eq(0).removeClass("select");
        $(this).addClass("select");
        $("#select").removeClass("show");
        $(".popup-mask").get(0).style.display = 'none';
        var _img = $("img",this).eq(0).Attr("src");
        $("#source img").Attr("src",_img);
        var _href = $(this).Attr("data-url");
        $("#play").Attr("href",_href);
        $(".pop").get(Page).style.display = 'none';
        $(".pop").get(_i).style.display = 'block';
        Page = _i;
    });
    $("#play").on("click",function(){
        var _href = $(this).Attr("href");
        recordId(_href);
    });
    $(".box a").on("click",function(){
        var _href = $(this).Attr("href");
        recordId(_href);
    });
	var nav_open = false;
    $("#Morechn").on("click",function(){
        var _i = $("i",this);
        var head = $("#fix_head");
        if(nav_open){
            head.animate({"height":70},150);
            _i.removeClass("up");
            nav_open = false;
        }
        else{
            head.animate({"height":112},150);
            _i.addClass("up");
            nav_open = true;
            setTimeout(function(){
                if(nav_open){
                    head.animate({"height":70},150);
                    _i.removeClass("up");
                    nav_open = false;
                }
            },3600);
        }
    });
</script>
