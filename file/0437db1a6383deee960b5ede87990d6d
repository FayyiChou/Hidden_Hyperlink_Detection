	function notExist(js) {
		var flag = true;
		var scripts = document.getElementsByTagName("script");
		if(scripts) {
			for(var i = 0; i < scripts.length; i++) {
				var src = scripts[i].src;
				if(src) {
					if(src.indexOf(js) != -1) {
						flag = false;
						break;
					}
				}
			}
		}
		return flag;
	}


	function userStat() {
		var cookies = getGACookie("uin");

		if(cookies && cookies != "") {
			var info = cookies.split("|");
			var result = info[2];
			if(result == 'true') {
				return 3;//作者
			}
			else {
				return 1;//用户 或 VIP暂不考虑
			}
		} else {
			return 0;//游客
		}

	}

	function getGACookie(name) {
		var cookieValue = null;
		if(document.cookie) {
			var cookies = document.cookie.split(';');
			for(var i = 0; i < cookies.length; i++) {
				var cookie = cookies[i].replace(/^[\s\xA0]+|[\s\xA0]+$/g, '');
				if(cookie.substring(0, name.length + 1) == (name + '=')) {
					cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
					break;
				}
			}
		}
		return cookieValue;
	}

