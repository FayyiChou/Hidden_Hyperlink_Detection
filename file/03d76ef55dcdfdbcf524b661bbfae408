if('1490538402' !='') {
	var firsttime = '1490538402';
} else {
	var firsttime = '1490558892';
}

var F = {
	last_ctime:'',
	type:   '-1',
	max:	eval(firsttime),
	min:	eval('1490522355'),
	date:	eval('20170326'),
	sec:	eval('60')*1000,
	ids:	[],
	start: function(){
		Mod.market();
		Mod.advert();
		Mod.recommend();
		Mod.plateRank( $('.plateRank') );
		//Mod.article_click(F.ids);
		if(App.mute==1) $C.find('#sound>a').removeClass('on');
		App.set('news', App.news = F.max);
		F.timer = setTimeout('F.listener()', F.sec);
		$C.find('#sound').click(function(){
			$(this).find('a').toggleClass('on');
			App.mute = $(this).find('a').hasClass('on')?0:1;
			App.set('mute',App.mute);
		});

		$C.find('div.more>a').click(function(){
			var $this = $(this);;
			$this.addClass('loading');
			$.post(ROOT+'v2/article/h5_get_article',{type:'-1',count:15,staid:F.last_ctime},function(data){
					$this.removeClass('loading');
					if(data.errno>0){
						return false;
					}
					F.draw(data.data);
			},'json');
		});		
		$(document).on('click', '.reading', function() {
			var id = $(this).parent().attr('articleid');
			var num = parseInt($(this).text().substr(3))+1;
			$(this).html('已阅 '+num);
			Mod.article_click(id);
		})
		$(document).on('click', '.opening', function() {
			var deny_comment=$(this).attr("deny_comment");
			if(deny_comment==1){
				Mod.deny_comment();
				return false;
			}
			var id = $(this).parent().attr('articleid');
			$li = $(this).parent().next('li.comment');
			var loaded = $li.hasClass('loaded');
			if(!loaded) $.getJSON("http://www.cailianpress.com/index.php/v2/AntiComment/get_comments", { articleid:id,'count':20}, function(ret) {
				$li.addClass('loaded');
                        $li.find(".code-img").html(ret.img);
				    if(ret.errno > 0 || ret.data =="") return false;
						var html = '';
						var ips = uss = '';
						var total  = ret.data.total_number; //总评论数
						var cursor = ret.data.total_cursor; //显示的评论数
						$.each(ret.data, function(i,j) {
							html += '<ul class="fix">';
							html += '<li class="avatar avatar-'+j.userId+'"><img src="http://img.myhongzuan.com/small.jpg" /></li>';
							html += '<li class="date">'+ App.timestamp(j.time, 'y-m-d h:i')+'</li>';
							if(j.name!='') {
								html += '<li class="name">'+j.name+'</li>';
								if(j.userId>0) uss += j.userId+',';
							}
							else {
								html += '<li class="name"><span class="ip_'+(j.ipaddress==null?'':j.ipaddress.replace(/\./g,'_'))+'"></span>网友</li>';
								if(j.ipaddress!=null) ips += j.ipaddress+',';
							}
							html += '<li>'+ j.content + '</li>';
							html += '</ul>';
						});
/*						if(cursor < total) {
							html += '<div id="more_comment " class="more_comment"><a class="" next_cursor="'+ret.data.next_cursor+'" href="javascript:;" onclick="draw_comment('+ret.data.next_cursor+')">加载更多评论</a></div>'
						}*/
						$li.find('.list').html(html);
						if(ips.length>0) {
							var url = 'http://int.dpool.sina.com.cn/iplookup/iplookup.php?format=js&ip=127.0.0.1,'+ips.substring(0,ips.length-1);
							$.getScript(url, function(ret){
								$.each(remote_ip_info, function(i,j) {
									var $span = $li.find('.ip_'+i.replace(/\./g,'_'));
									if(j==-1) $span.html(i);
									else $span.html(j.province);
								})
							});
						}
						/*if(uss.length>0) {
							var url = 'http://www.myhongzuan.com/index.php?app=api&mod=User&act=getUserFace&uids='+uss.substring(0,uss.length-1);
							$.getJSON(url, function(ret) {
								$.each(ret, function(i,j) { $('.avatar-'+j.uid).html('<img src="'+j.avatar_small+'" />'); });
							});
                            }*/	

                    });
                
					$li.slideToggle('fast');
						
		})
		$(document).on('click', '.comment .submit', function() {
			$li = $(this).parent();
			$li.find('.error').html('');
			if( $.trim($li.find('.content').val())=='' ) $li.find('.error').html('请说点什么吧');
			else {
				var id = $li.attr('articleid');
				var content = $.trim($li.find('.content').val());
				if('' == '') {
					$li.find('.error').html('请先登录');
					return false;
				}
				var post = {
					id:id, content:content, captcha:$li.find(".code-input").val()
				} ;
				$.post("http://www.cailianpress.com/index.php/v2/AntiComment/insert_comment", post, function(j){
					if(j.errno===0) {
						var oldpnum = $li.prev().find('i.pnum').html();
						var oldpnum = $li.prev().find('i.pnum').html(++oldpnum);
						
						// $li.prev().find('.opening').html('评论');
						$li.find('.content').val('');
						$li.find('.list').prepend('<ul><li class="date">'+ App.timestamp(new Date()*0.001, 'y-m-d h:i')+'</li><li class="name red">我说</li><li>'+j.data.content+'<li></ul>');
					} else {
						if(j.errno ==20017) {
							$li.find('.error').html('有非法内容');
						} else if(j.errno ==20013) {
							$li.find('.error').html('重复评论');
						} else if(j.errno ==10006) {
							$li.find('.error').html('您无法评论');
						} else if (j.errno ==20002) {
							$li.find('.error').html('请先登录');
                        } else if( j.errno == 10007 ){
							$li.find('.error').html('验证码有误，请重新输入');
                            } else {
                            $li.find('.error').html(j.content);
                            }
                    }
                    $.get("http://www.cailianpress.com/index.php/v2/AntiComment/get_comments?articleid="+id+"&count=1",function(ret){
                            $li.find(".code-img").html(ret.img);
                            $li.find(".code-input").val("");
                    })
				});
			}
			return false;
		})		
		$(document).on('click', 'a.weibo', function(){
			var title = $(this).parent().parent().find('.content').text();
			var href = 'http://' + location.hostname + $(this).attr('href');
			var url = 'http://service.weibo.com/share/share.php?title='+encodeURI(title)+'&url='+href+'&searchPic=false';
			window.open(url);
			return false;
		});		
		$(document).on('click', 'a.space', function(){
			var title = $(this).parent().parent().find('.content').text();
			var href = 'http://' + location.hostname + $(this).attr('href');
			var url = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?title='+encodeURI(title)+'&url='+href;
			window.open(url);
			return false;
		});
	},
	draw: function(ret, prepend) {
		var prepend = typeof(prepend)!='undefined';
		var html = '';
		var clicks = [];
		$.each(ret, function(i,j) {
			if(App.timestamp(j.time, 'ymd')!=F.date && !prepend) {
				html += '<div class="topic">';
				html += '<span class="date">'+App.timestamp(j.time, 'y-m-d')+'</span><strong>小时即时电报</strong>';
				html += '</div>';		
				F.date = App.timestamp(j.time, 'ymd');
			}
            //console.log(j);
            html += '<ul class="fix '+(prepend?'new':'')+'">';
			html += '<li class="time pconly">'+App.timestamp(j.time, 'h:i')+'</li>';
			html += '<li class="content '+(j.recommend==1?'recommend':'')+' '+(j.bold==1?'bold':'')+'">'+j.content+'</li>';
			if(j.relate_article.length>0){
				html+='<li class="article_relative">';
				$.each(j.relate_article,function(m){
					html+='<a class="relative_titile" target="_blank" href="'+j.relate_article[m].redirect_url+'">'+j.relate_article[m].title+'</a>'
				})
				html+='</li>';
			}
			html += '<li class="share" articleid="'+j.id+'">';
			html += '<a href="javascript:;" class="c1 opening">评论 <i class="pnum" style="float:right">'+j.pinglun+'</i></a>';
			html += '<a href="javascript:;" class="c2 reading">已阅 '+j.num+'</a>';
			html += '<span class="pconly">分享到</span>';
			html += '<a  href="/single/'+j.id+'.html" class="weibo pconly"></a>';
			html += '<a  href="/single/'+j.id+'.html" class="space pconly"></a>';
			html += '</li>';
			html += '<li class="comment" articleid="'+j.id+'">';
			html += '<div class="list"></div>';
			html += '<textarea name="content" class="content"></textarea><div class="error"></div>';
            html += ' <div class="code-img"></div><input type="text" name="captcha" class="code-input"/>';
            html += '<button type="submit" class="submit">发布评论</button>';
			html += '</li>';
			html += '<li class="mob-time hidden">'+App.timestamp(j.time, 'h:i')+'</li>';			
			html += '</ul>';
			if(prepend != 1) {
				F.last_ctime = j.time;
			}
			//clicks.push(j.id);
		});		
		if(prepend) {
			$('#entries').prepend(html);
		}else {
			$('#entries').append(html);
		}
		//Mod.article_click(clicks);
	},
	listener: function() {
	$.getJSON(ROOT+'v2/article/get_roll',{ type:-1, staid: App.news, count:20 ,flow:1}, function(ret){
				F.timer = setTimeout('F.listener()', F.sec);
				if(ret.errno == 0) {
					if(App.mute==0) document.getElementById('audio').play();
					F.max = App.news = ret.data[0].time;
					F.draw(ret.data, 1);
					App.set('news', App.news);	
				}	
			
		},'json');
	}
}
$(function(){ 
	F.start(); 
	$('.ads').click(function() {
		var id = $(this).attr('id');
		$.post("http://www.cailianpress.com/v2/advert/click",{'id':id});
		return true;
	});

})
