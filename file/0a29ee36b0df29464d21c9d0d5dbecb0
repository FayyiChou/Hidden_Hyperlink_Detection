<script type="text/javascript">
    $(function () {
        //搜索选择按钮
        $("#classBox").hover(function () {
            $(".selectBox").show();
        }, function () {
            $(".selectBox").hide();
        });

        $("#selectBox>li").each(function () {
            try {
                if ($(this).attr("selectItem") == selectType) {
                    $("#selectBox>li").attr("class", "");
                    $(this).attr("class", "now");
                    $("#nowSelected").html($(this).html());
                    document.getElementById("selectType").value = selectType;
                }
            } catch (e) {
            }
            $(this).click(function () {
                $("#selectBox>li").attr("class", "");
                $(this).attr("class", "now");
                $("#nowSelected").html($(this).html());
                $(".selectBox").hide();
                document.getElementById("selectType").value = $(this).attr("selectItem");
            });
        });
        //提交搜索表单
        $(".searchBtn").click(function () {
            if (document.getElementById("searchText").value.replace(/\s+/g, "") == "") {
                window.location.href = "http://ba2.win007.com";
                return;
            }
            document.getElementById("searchForm").submit();
        });
    });
</script>

