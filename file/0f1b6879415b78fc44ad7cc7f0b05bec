<script type="text/javascript">
    $(function(){
        $('.nav ul a').hover(function(){
            $('#'+$(this).attr('data-hover')).show();
        },function(){
            $('#'+$(this).attr('data-hover')).hide();
        });

        $('.banner-item').hover(function(){
            $(this).show();
            $('.nav ul a[data-hover="'+$(this).attr('id')+'"]').addClass('bg-hover');
        },function(){
            $('.nav ul a[data-hover="'+$(this).attr('id')+'"]').removeClass('bg-hover');
            $(this).hide();
        });

        $('.xw-item h3 em').on('click',function(){
            var he = $(this).parent().children('em');
            he.removeClass('active')
            $(this).addClass('active');
            var xb = $(this).parent().parent().children('.xw-block');
            xb.hide();
            xb.eq(he.index(this)).show();
        });
    });
function setHomepage(pageURL) {
    if (document.all) {
        document.body.style.behavior='url(#default#homepage)';
        document.body.setHomePage(pageURL);
    }
    else {
            try { //IE
                netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
            } catch (e) {
    try{ //Firefox
     var prefs = Components.classes['@mozilla.org/preferences-service;1']
.getService(Components. interfaces.nsIPrefBranch);
     prefs.setCharPref('browser.startup.homepage',pageURL);
    } catch(e) {
                alert( "您的浏览器不支持该操作，请使用浏览器菜单手动设置." );
    }
            }
    }
}


function addToFavorite(obj) {
    var url = "http://www.zb120.cn/";
    var title = "淄博市医疗急救指挥中心";
    var ua = navigator.userAgent.toLowerCase();
    if (ua.indexOf("360se") > -1) {
        alert("由于360浏览器功能限制，请按 Ctrl+D 手动收藏！");
    }
    else if (ua.indexOf("msie 8") > -1) {
        window.external.AddToFavoritesBar(url, title); //IE8
    }
    else if (document.all) {
        try {
            window.external.addFavorite(url, title);
        } catch (e) {
            alert('您的浏览器不支持,请按 Ctrl+D 手动收藏!');
        }
    }
    else if (window.sidebar) {
        obj.href = url;
        window.sidebar.addPanel(title, url, "");
    }
    else {
        alert('您的浏览器不支持,请按 Ctrl+D 手动收藏!');
    }
}
</script>
