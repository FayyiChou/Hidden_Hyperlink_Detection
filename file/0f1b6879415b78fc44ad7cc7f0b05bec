<script type="text/javascript">
    $(function(){
        $('.arrow-prev').on('click',function(event){
            var num= parseInt($('.in-slider ul').css('marginLeft'));
            if (num >= 0) {
                $('.in-slider ul').css({marginLeft:'0px'});
                return false;
            }
            $('.in-slider ul').stop().animate({marginLeft: (num+316)+'px'}, 200 );
        });

        $('.arrow-next').on('click',function(event){
            var num= parseInt($('.in-slider ul').css('marginLeft'));
            var w = ($('.in-slider ul li').size()-3)*316;
            if (num + w <= 0) {
                $('.in-slider ul').css({marginLeft:'-'+w+'px'});
                return false;
            }
            $('.in-slider ul').stop().animate({marginLeft: (num-316)+'px'}, 200 );
        });

        $('.arrow-prev2').on('click',function(event){
            var num= parseInt($('.in-slider2 ul').css('marginLeft'));
            if (num >= 0) {
                $('.in-slider2 ul').css({marginLeft:'0px'});
                return false;
            }
            $('.in-slider2 ul').stop().animate({marginLeft: (num+316)+'px'}, 200 );
        });

        $('.arrow-next2').on('click',function(event){
            var num= parseInt($('.in-slider2 ul').css('marginLeft'));
            var w = ($('.in-slider2 ul li').size()-3)*316;
            if (num + w <= 0) {
                $('.in-slider2 ul').css({marginLeft:'-'+w+'px'});
                return false;
            }
            $('.in-slider2 ul').stop().animate({marginLeft: (num-316)+'px'}, 200 );
        });


    });
</script>
