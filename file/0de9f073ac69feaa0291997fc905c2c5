$(function(){
    // 隐藏商桥图标，只保留右下角的咨询模块
    function checkIconContainer() {
        var iconContainer = $('#IconContainer');
        var t;
        if(!iconContainer.length) {
            t = setTimeout(checkIconContainer, 200);
        }
        else {
            iconContainer.hide();
            clearTimeout(t);
            t = null;
        }
    }

    checkIconContainer();
});
</script>
