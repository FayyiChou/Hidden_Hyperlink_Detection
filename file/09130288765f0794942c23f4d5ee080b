	$("#"+submitFormIdPupup+ " p.logonMessagePPupup").hide();
	$("#"+submitFormIdPupup+ " span.loginMessagePupup").html('');
	window.document.getElementById(submitFormIdPupup+"submitLoginBtn").style.visibility = "";
	getNewImageLogin();
	flag = validateSessionAsy("refreshPage",$.browser.msie);
	//移除缓冲层
	//alert(flag);
}
