//信息验证
function toValidate(){
	var chargeProductType = getProductType();
	var areaCode=$("#areaCode").val();
	var chargeAccNbr=document.getElementById("chargeAccNbr").value;
	var againchargeAccNbr=document.getElementById("againchargeAccNbr").value;
	var chargeMoney = getChargeMoney();
	var imageCode=document.getElementById("imageCode").value;
	
	var activitycode=document.getElementById("activitycode").value;
	var reCommendMan=document.getElementById("reCommendMan").value;
	if(reCommendMan == "可不填写"){
		reCommendMan ="";
	}
	var isGreenPassFlag = "1";
	var reqchargeorderid=document.getElementById("reqchargeorderid").value;
	var otherchargeAccNbr=document.getElementById("chargeAccNbr").value;
	var chargeType=document.getElementById("chargeType").value;
	$.ajax({
        url: "/pay/bankChargeAffirm.action",   
		type: "post",   
		dataType: "json", 
		async:false,  
	    data: {"otherchargeAccNbr":otherchargeAccNbr,"chargeProductType":chargeProductType,"chargeAccNbr":chargeAccNbr,
		       "chargeType":chargeType,
				  "chargeMoney":chargeMoney,"imageCode":imageCode,
				  "areaCode":areaCode,"reCommendMan":reCommendMan,
				  "activitycode":activitycode,"reqchargeorderid":reqchargeorderid,"againchargeAccNbr":againchargeAccNbr,
				  "isGreenPassFlag":isGreenPassFlag
        },complete: function(){
	        $('input').each(function(){
	        		this.disabled=false;
	        });
	        $('select').each(function(){
	        		this.disabled=false;
	        });  
		},error: function(){   
				showAlertAwake(new Date(),400,"系统提示","操作失败，请稍候再试！",1);
		},success: function(msg){
				getNewImage();	
				returnSucess6(msg);
		}
		});
}
//结果查询
function submitForm6(){
	document.forms['inputFormabc16'].submit();
return true;			
}
//处理返回结果
function returnSucess6(msg){
		var actionResultVo =msg.actionResultVo;	
		//查询失败，显示错误描述和错误码
		if(!actionResultVo.resultFlag){
			if('10043000interfaceInfo is null'==actionResultVo.errorDesc)
				showAlertAwake(new Date(),400,"系统提示","对不起，该功能只对江苏省内的用户充值",1);
				else{
					var msg = actionResultVo.errorDesc.replace(actionResultVo.errorCode,"");
					msg = msg.replace(":","").replace("/*","").replace("*/","");
					showAlertAwake(new Date(),400,"系统提示",msg,1);
				}
		}
		else{
			
			var key = getProductType();
			var showNbr = msg.showNbr;
			if(key=='3'){
      			var number=msg.showNbr.substring(0,3);
      			if(number=='025'){
				showNbr=msg.showNbr.substring(3);
				}else{
				showNbr=msg.showNbr.substring(4);
				}
			}
			$("#la_number").html(showNbr);
			$("#la_area").html(selectAreaName(msg.areaCode));
			$("#la_chargetime").html(msg.chargeDate);
			$("#la_chargetype").html(selectChargeType(msg.chargeProductType));
			$("#la_chargecount").html(msg.chargeMoney+"元");
			
			$("#st_orderid").html(msg.businessOrder);
			$("#st_chargemoney").html(msg.chargeMoney+"元");
			$("#st_chargenum").html(msg.chargeAccNbr);
			$("#st_areaname").html(selectAreaName(msg.areaCode));
			
			var fundsType1 = $("#fundsType1").val();
  			document.inputFormabc1.fundsType.value=fundsType1;
	 		document.inputFormabc1.source.value=msg.source;
	 		document.inputFormabc1.businessOrder.value=msg.businessOrder;
	 		document.inputFormabc1.areaCode.value=msg.areaCode;
	 		document.inputFormabc1.businessType.value=msg.businessType;
	 		document.inputFormabc1.chargeFee.value=msg.chargeFee;
	 		document.inputFormabc1.returnURL.value=msg.returnURL;
	 		var sd="";
	 		if(msg.merchantURL!=null)
			sd=msg.merchantURL;
	 		document.inputFormabc1.merchantURL.value=sd;
	 		document.inputFormabc1.sign.value=msg.sign;
	 		document.inputFormabc1.chargeProductType.value=msg.chargeProductType;
	 		document.inputFormabc1.chargeAccNbr.value=msg.chargeAccNbr;
	 		document.inputFormabc1.showNbr.value=msg.showNbr;
	 		document.inputFormabc1.chargeDate.value=msg.chargeDate;
	 		document.inputFormabc1.orderId.value=msg.businessOrder;
	 		document.inputFormabc1.chargeMoney.value=msg.chargeMoney;
	      	//document.infoForm6.orderId6.value=msg.businessOrder;
	      	document.inputFormabc16.businessOrder.value=msg.businessOrder;
	      	
			kdlt_layer();	 	
		}	    
}
/**
 * 地区转换
 */
function selectAreaName(id){
	if(id=="025")return "南京";
	else if(id=="0510")return "无锡"; 
	else if(id=="0511")return "镇江"; 
	else if(id=="0512")return "苏州"; 
	else if(id=="0513")return "南通"; 
	else if(id=="0514")return "扬州"; 
	else if(id=="0515")return "盐城"; 
	else if(id=="0516")return "徐州"; 
	else if(id=="0517")return "淮安"; 
	else if(id=="0518")return "连云港"; 
	else if(id=="0519")return "常州"; 
	else if(id=="0523")return "泰州"; 
	else if(id=="0527")return "宿迁"; 
	else return "江苏省";
}
/**
 * 充值类型转换
 */
function selectChargeType(id){
	if(id=="2") return "手机";
	else if(id=="3") return "宽带";
	else if(id=="0") return "固话";
	else return "未知类型";
}
/**
 * 充值第二步
 */
function steptwo(){
	diag.close();
	$("#stepone_div").hide();
	$("#steptwo_div").show();
}
/**
 * 获取充值的产品类型
 */
function getProductType()
{
	var key;
	var i = divIndex;
	var productTypes = $("#jz_products_list li");
	if(i=='1'){
		productTypes = $("#jz_products_list1 li");
	}
	for (var i = 0; i < productTypes.length; i ++)
	{
		if ($(productTypes[i]).attr("class") == "jz_active")
		{
			key = $(productTypes[i]).attr("value");
			return key;
		}
	}
}
/**
 * 获取欠费查询的产品类型
 */
function getSearchType()
{
	var key;
	var productTypes = $("#search_ul li");
	
	for (var i = 0; i < productTypes.length; i ++)
	{
		if ($(productTypes[i]).attr("class") == "jz_active")
		{
			key = $(productTypes[i]).attr("value");
			return key;
		}
	}
}
function changeProdType(type){
	if(type=="2000001"){return "0";}
	else if(type=="2000003"){return "1";}
	else if(type=="2000004"){return "2";}
	else if(type=="2000002"){return "3";}
	else if(type=="2000006"){return "54";}
}

//查询实时消费
function queryArrears(){
	
	var userInfo =getSessionInfo();
	//登陆类型
	var productId = changeProdType(userInfo.userLoginType);
	//登陆号码
	var areaName = selectAreaName(userInfo.areaCode);
	var changeUserID = userInfo.productId;
	var queryArrearsUrl = loadingPath+"/chargeQuery/chargeQuery_queryArrears.action";
	$.ajax({   
     url:queryArrearsUrl,  //后台处理程序   
     data:{
	     productId:productId,
	     changeUserID:changeUserID,
	     isGreenPassFlag:"1"
	     },
     type:'post',    //数据发送方式   
     dataType:'json',   //接受数据格式           
     beforeSend: function(){ 
     	if (flag){
     		showLoading("ssfyCenterId","ssfyBgSelserId");//调用缓冲层
     	}
    },
     complete: function(){
     	if (flag){
        	removeLoaing("ssfyCenterId","ssfyBgSelserId");//移除缓冲层
        }
    }, 
     error: function(){
     	showAlertAwake(new Date(),200,"系统提示","系统忙，请稍候再试！",1);
     },   
     success:function(data, textStatus) {
    		var resultli ="";
			var obj = eval(data);
			var acctItemChargeSum = 0;
			var dueSum = 0;
			var acctItemChargeDueSum = 0;
	        $.each(obj,function(key,val){
		        if('201013'==val.TSR_RESULT){
		        	resultli = "您查询的帐号无欠费信息，可正常使用。";
			    }
	            if('0'==val.TSR_RESULT){
	            	resultli = "<table width='500' border='0' ><tr><th>序号</th><th>欠费月份</th><th>欠费金额(元)</th><th>滞纳金(元)</th><th>合计(元)</th></tr>"
	            	if (typeof (val.arrearsList) == "object") {
		            	var arrearsList = val.arrearsList;
		            	$.each(arrearsList,function(bkey,bval){
		            		var acctItemCharge = parseInt(bval.acctItemCharge);
		    				var due = parseInt(bval.due);
		    				var acctItemChargeDue = acctItemCharge+due;
		    				acctItemChargeSum += acctItemCharge;
		    				dueSum += due;
		    				
		            		resultli += "<tr><td align='center'>"+(bkey+1)+"</td><td align='center'>"+bval.cycleEndDate+"</td><td align='center'>"+changeTwoDecimal_fzhl(acctItemCharge/100)+"</td><td align='center'>"+changeTwoDecimal_fzhl(due/100)+"</td><td align='center'>"+changeTwoDecimal_fzhl(acctItemChargeDue/100)+"</td></tr>";
		            	});
		            	resultli += "<tr><td align='center'>合计</td><td align='center'></td><td align='center'>"+changeTwoDecimal_fzhl(acctItemChargeSum/100)+"</td><td align='center'>"+changeTwoDecimal_fzhl(dueSum/100)+"</td><td align='center'>"+changeTwoDecimal_fzhl((acctItemChargeSum+dueSum)/100)+"</td></tr>";
	                }
	            	resultli += "</table>";
	            }else{
	            	//$("#noresult").show();
	            }
	       	});
			//$("#arrearsSumInfo").text(changeTwoDecimal_fzhl((acctItemChargeSum+dueSum)/100));
			//查询提示信息赋值
         	$("#search_num").html(changeUserID);
         	$("#search_area").html("("+areaName+")");
			$("#search_mes").html(resultli);
    		kdlt_cz();
		}
  });   
}
function changeTwoDecimal_fzhl(x)  
{  
	var f_x = parseFloat(x);  
	if (isNaN(f_x))  
	{  
	alert('function:changeTwoDecimal->parameter error');  
	return false;  
	}  
	var f_x = Math.round(x*100)/100;  
	var s_x = f_x.toString();  
	var pos_decimal = s_x.indexOf('.');  
	if (pos_decimal < 0)  
	{  
	pos_decimal = s_x.length;  
	s_x += '.';  
	}  
	while (s_x.length <= pos_decimal + 2)  
	{  
	s_x += '0';  
	}  
	return s_x;  
}
//检查充值密码
function cz_checkpwd()
{
	 var flag=false;
	 $('.input_pwd').each(function(i,val){
	 	var pwd=val.value;
	 	if(pwd==''||pwd=='请输入充值卡密码'){
	 	$(this).parent().children('#1_pwd_qe').html('充值密码不能为空').addClass('jz_check_tip_01');
	 		flag=false;
	 		return false;
	 	}else if(pwd.length!=18){
	 		flag=false;
	 		return false;
	 	}else{
	 		flag=true;
	 		return true;
	 	}
	 });
	 return flag;
}
/**
 * 获取充值金额
 */
function getChargeMoney()
{
	var chargeMoney ;
	// 获取充值金额
	var otherChargeMoney=$("#other_chargeMoney").val();
	if(otherChargeMoney!='输入其他金额' && otherChargeMoney!=''){
		chargeMoney=otherChargeMoney;
	}else{
	var chargeMoneys = $("#chargeMoney_choose li");
	for (var j = 0; j < chargeMoneys.length; j++)
	{
	    // 选择其他，输入金额
		if ($(chargeMoneys[j]).attr("class") == "jz_active" )
		{
			chargeMoney = $(chargeMoneys[j]).attr("monInfo");
			break;
		}
	}
	}
	// 去掉“元”
	//chargeMoney = chargeMoney.replace("元", "");
	return chargeMoney;
}
//展现所有信息，确认提交
function showrechargeinfo()
{	
  	if(cz_checkcities()&&cz_checkczNum()&&cz_checkczqrnum()&&cz_checkpwd()&&cz_checkyzm())
	{ 	 
  		$("#cz_num").html($("#czNum").val());
  		var batchNum = $("#batchDiv  input").size();
  		var qecztype ="1";
  		var batchPWD = $("#czpwd_qe").val();
  		if(batchNum>1) { //qecztype 为2代表批量充值 ，为1 代表普遍充值
  			qecztype = "2";
  		}
  		for(i=1;i<batchNum;i++)
  		{
  			batchPWD+= "<br/>"+$("#batchDiv  input").eq(i).val();
  		}
  		$("#cz_pwds").html(batchPWD);
	 	show_czInfo();
	    return false;
	  }
	else{
	 return false;
	 }
}
//充值信息提交
function cz_steptwo(){
	var actionUrlForWare;
	//得到充值类型 默认全额充值
	var v_recharge_type = "1"; 
	//得到充值付费类型
	var v_czfftype = $("#cz_typeall").val(); 
    //得到产品类型
	//var cptype = $("#cptype").val();
	var cptype=getProductType();
	
	//循环取的充值卡密码
	var batchPWD = $("#czpwd_qe").val()+"|";
	
	var batchNum = $("#batchDiv  input").size();
	var qecztype ="1";
	if(batchNum>1) { //qecztype 为2代表批量充值 ，为1 代表普遍充值
		qecztype = "2";}
	for(i=1;i<batchNum;i++)
	{
		batchPWD+= $("#batchDiv  input").eq(i).val()+"|";
	}
	 //触发等待图标
	showLoading("myyjDiv2","myyjbgDiv2");	
	
	//得到号码所在地
	var hmszd = $("#cities").val(); 
	//如果是刚登录页面，则从session中获得用户的登录号码和登录类型
	actionUrlForWare = "/pay/rechargetj.action";
		$.post(actionUrlForWare, {
			"recharge_type" : v_recharge_type,
			"czfhtype" : v_czfftype,
			"cptype" : cptype,
			"hmszd" : hmszd,
			"czhm" : $("#czNum").val(),
			"czpwd_qe" : batchPWD,
			"tjr" : $("#tjr").val(),
			"yzm" : $("#yzm").val(),
			"qecztype":qecztype,
			"param":"null",
			"isGreenPassFlag":"1"
		}, function(data, textStatus) {
		diag.close();
		cz_getNewImage();
		//结束等待图标
		removeLoaing("myyjDiv2","myyjbgDiv2");
			
		
			if (data == "1") {
				$("#l_yzm_error").css("display", "block");
				showAlertAwake(new Date(),400,"信息提示","验证码错误，请重新填写","1");
				return ;
			}else if(data==null){
				showAlertAwake(new Date(),400,"信息提示","对不起，系统繁忙，请稍候再试！","1");
				return ;
			} else {
				$("#cz_stepone").hide();
				$("#cz_result").show();
			  $("#cz_msg_p").html(data["tuxedo_msg"]);
			   if(data["result"]!="0") {
			   		$("#cz_mes img").attr("src","/activity/recharge/images/fail_icon.gif");
			   } else {
			        $("#cz_mes img").attr("src","/activity/recharge/images/success_icon.jpg");
			   }
				var varray = new Array();
				varray = data["subResult"];
			 $("#cz_mes").append(data["message"]);
			 
			 for(i=0;i<varray.length;i++)
			 {
			 	 $("#cz_success_table").append("<tr><td><b>"+varray[i]["subResSerial"]+"</b></td><td><em>"+varray[i]["czNumber"]+"</em></td><td><em>"+varray[i]["rechAmount"]+"<em></td><td><em>"+varray[i]["result"]+"<em></td><tr>");
			 }
			}
		}, "json");
}
//充值信息展示
function show_czInfo(){
	diag = new Dialog();
	diag.Width = 520;
	//diag.Height = 380;
	diag.Title = "充值信息提示";
	diag.InvokeElementId="cz_infodiv"
	diag.show();
	
}
//充值提交
function toCharge(){

    var bbankId=$("input[name='bankId']:checked").val();
	if(bbankId!=undefined)
		document.inputFormabc1.bbankId.value=bbankId;
	else{
		if(bbankId==""||bbankId==undefined)
	 		showAlertAwake(new Date(),400,"系统提示","尊敬的用户您好，请先选择您要支付的银行！",1);
			else if(bbankId=="33"){
				showAlertAwake(new Date(),400,"系统提示","尊敬的用户您好，腾讯财付通暂不支持支付,请先选择其它的银行支付！",1);
				return;
			}
		}

		document.forms['inputFormabc1'].submit();
		document.getElementById("steptwo_div").style.display="none";
		document.getElementById("p6").style.display="block";		
    	return true;
}			
function still_cz(){
	$(".kdlt_main_tab li:eq(1)").trigger("click");
}

function toQueryArrears() {
	//if($("p.validateCodePupup").get(0).style.display != "") {
	if(!validateParam(submitFormIdPupup)){
		return ;
	}
	var logintype = getSearchType();
 	$("#phoneUserType").val(logintype);
	
		var codeValue = $("#"+submitFormIdPupup+ " input[name='validateCode']").val();
		codeValue = $.trim(codeValue);
		if(codeValue == "") {
			$("#"+submitFormIdPupup+ " p.logonMessagePPupup").show();
			$("#"+submitFormIdPupup+ " span.loginMessagePupup").addClass("jz_check_tip_01");
			$("#"+submitFormIdPupup+ " span.loginMessagePupup").html("请输入验证码");
			return;
		}
	//}
	$("#"+submitFormIdPupup+ " p.logonMessagePPupup").show();
	$("#"+submitFormIdPupup+ " span.loginMessagePupup").removeClass("jz_check_tip_01");
	$("#"+submitFormIdPupup+ " span.loginMessagePupup").addClass("loadingcss");
	$("#"+submitFormIdPupup+ " span.loginMessagePupup").html("登录/查询中，请稍候……");
	var myform = document.getElementById(submitFormIdPupup);
	window.document.getElementById(submitFormIdPupup+"submitLoginBtn").style.visibility = "hidden";
	
	var postData = $("#"+submitFormIdPupup).serialize();
	$.ajax({
        type: "POST",
        dataType:"json",
        url:webRootPath+"/self_service/validateLogin.action",
        async:true,
        data:postData,
        error: function(data) {
			$("#"+submitFormIdPupup+ " p.logonMessagePPupup").show();
			$("#"+submitFormIdPupup+ " span.loginMessagePupup").addClass("jz_check_tip_01");
			$("#"+submitFormIdPupup+ " span.loginMessagePupup").html("系统忙,请稍稍再试");
			window.document.getElementById(submitFormIdPupup+"submitLoginBtn").style.visibility = "";
			
			// 登录失败，更新验证码
			changeValidateImage();
		},
        success:function(data) {
        	if(data.result == 'success') {
        		var ds = data.ds;
        		var paramUam = data.paramUam;       	 
        		document.getElementById("ds").value = ds;
        		document.getElementById("paramUam").value = paramUam;
        		var requestXml = data.requestXml;
        		document.getElementById("SSORequestXML").value = requestXml;
        		
    			document.loginformPupup.submit.click(); 
        	} else {
        		
        		$("#"+submitFormIdPupup+ " p.logonMessagePPupup").show();
				$("#"+submitFormIdPupup+ " span.loginMessagePupup").addClass("jz_check_tip_01");
				$("#"+submitFormIdPupup+ " span.loginMessagePupup").html(data.errorMsg);
				window.document.getElementById(submitFormIdPupup+"submitLoginBtn").style.visibility = "";
				
				// 登录失败，更新验证码
				changeValidateImage();
        	}
        }
    });
}
function validateParam(commintForm){
	var productId = $("#ser_num").val();//产品号码
	var password = $("#ser_pwd").val();
	var  verificationCode = $("#loginImageCode").val();
	productId = trimStr(productId);
	var logintype = getSearchType();
	if(logintype=="2000004"){
		if(productId==''||productId==null){
			Dialog.alert("请您填写手机号码");
			return false;
		}else{
			if(!checkMobileNumber(productId)){
				return false;	
			} 
		}
	}else if (logintype=="2000002"){
		if(productId==''||productId==null){
			Dialog.alert("请您填写宽带号码");
			return false;
		}else{
			if(productId.length<4){
				Dialog.alert("宽带号码不少于4位");
				return false;	
			} 
		}
	}else if(logintype=="2000001"){
		var phoneRegWithArea = /^(\d){8}$/;//固话
		if(productId==''||productId==null){
			Dialog.alert("请您填写固话号码");
			return false;
		}else{
			if(!phoneRegWithArea.test(productId)){
				Dialog.alert("请输入正确的固话号码(号码前无需加区号)");
				return false;	
			} 
		}
	}
	
	if(password==''||password==null){
		Dialog.alert("请您填写密码");
		return false;
	}else if(password.length<6){
		Dialog.alert("请您填写正确格式的密码");
		return false;
	}
	
	if(verificationCode==''||verificationCode==null){
		Dialog.alert("请您输入验证码");
		return false;
	}else if(verificationCode.length<4){
		Dialog.alert("请您填写正确格式的验证码");
		return false;
	}
	
	return true;
}
//检查手机号码格式
function checkMobileNumber(mobile){
	var regMobile=/^0?1(3|5|8|7)\d{9}$/;
	if(!regMobile.test(mobile)){
		Dialog.alert("请输入正确手机号码");
		return false;
	}
	return true;
}
//去空格函数
function trimStr(str) { 
	var re = /\s*(\S[^\0]*\S)\s*/; 
	re.exec(str); 
	return RegExp.$1; 
}
