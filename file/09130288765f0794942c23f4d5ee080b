function kdlt_search(){
	$.ajax({
        type: "POST",//用POST方式传输
        dataType:"text",//数据格式:JSON 
        url:webRootPath+"/showUserLoginDiv.action?modelPage="+actionCode+"&jsFunctionName="+jsFunctionName+"&isIphone="+isIphone,//目标地址 
        async:false,//作用是防止在ajax成功调用之前就调用这个时候数据个数还没有初始化
        error: function(data) {
			showAlertAwake(new Date(),400,"系统提示","系统正忙,请稍候再试！",1);	
		},
        success:function(text) {
        }
    }); 
	diag = new Dialog();
	diag.Width = 420;
	//diag.Height = 380;
	diag.Title = "应交费用查询";
	diag.InvokeElementId="kdlt_search"
	diag.show();
}
function preToValidate(){
	
	var key = getProductType(); 		
	
	var chargeAccNbr=document.getElementById("chargeAccNbr").value;
	var againchargeAccNbr=document.getElementById("againchargeAccNbr").value;
	
	// 获取充值金额
	var chargeMoney = getChargeMoney();
	var imageCode=document.getElementById("imageCode").value;
	var areaCode=$("#areaCode").val();		
   // if (checkParam(key, chargeAccNbr, againchargeAccNbr, chargeMoney, areaCode, imageCode))
   if(key=='2'){
   	if(checkcznum()&&checkczqrnum()&&checkyzm()&&validateMoney(chargeMoney,this))
    {
   		toValidate();
		return true;
	}
	else
	{
		return false;
	}	
   }else{
   	if(checkcities()&&checkcznum()&&checkczqrnum()&&checkyzm()&&validateMoney(chargeMoney,this))
    {
   		toValidate();
		return true;
	}
	else
	{
		return false;
	}
	}
}
//归属地选择
function checkcities(){
	//$('#areaCode').blur(function(){
		var areaCode=$('#areaCode').val();
	if(areaCode=='0000'){
		$('#ssquhao').addClass('jz_check_tip_01');
		$('#ssquhao').html('请选择号码所在地');
		return false;
	}else{
		$('#ssquhao').removeClass('jz_check_tip_01');
		$('#ssquhao').html('');
		return true;
	}		
//	});
}
//验证充值号码
function checkcznum(){
	$('#jzcjs').html('');
	$('#jzcjs').removeClass('jz_check_tip_01');
	var key=getProductType();
	var chargeAccNbr=$('#chargeAccNbr').val();
	var s=chargeAccNbr.substring(0,3);
	var regu=/^[1][0-9][0-9]{9}$/;
	var phoneRegWithArea = /^(\d){8}$/;//固话
	if(key==2){
		if(chargeAccNbr=='请输入充值号码'||chargeAccNbr==''){
			$('#chargeAccNbr').css("color","#aaa");
			$('#jzcjs').addClass('jz_check_tip_01');
			$('#jzcjs').html('充值号码不能为空');
			$('#chargeAccNbr').val('请输入充值号码');
			return false;
		}else if(!regu.test(chargeAccNbr)){
			$('#jzcjs').addClass('jz_check_tip_01');	
			$('#jzcjs').html('请输入正确的11位手机号码');
			return false;
		}else if(s!="133"&&s!="153"&&s!="189"&&s!="180"&&s!="181"&&s!="177"){
			var hmString=chargeAccNbr.substring(0,5);
			if(hmString!="10649"){
			$('#jzcjs').addClass('jz_check_tip_01');
			$('#jzcjs').html('仅支持江苏电信号码');
			return false;
			}else{
				$('#jzcjs').html('');
			return true;
			}
		}
		else{
			return getPhoneArea();
			//return true;
		}
	}else if(key==0){
		if(chargeAccNbr==''||chargeAccNbr=='号码前无需加区号'){
		$('#jzcjs').addClass('jz_check_tip_01');
		$('#jzcjs').html('充值号码不能为空');
		$('#chargeAccNbr').val('号码前无需加区号');
		return false;
	}else if(!phoneRegWithArea.test(chargeAccNbr)){
		$('#jzcjs').addClass('jz_check_tip_01');	
		$('#jzcjs').html('请输入正确的固话号码(号码前无需加区号)');
		return false;
	}else{
		$('#jzcjs').html('');
		return true;
	}
	}else if(key==3){
	if(chargeAccNbr==''||chargeAccNbr=='请输入宽带帐号'){
		$('#jzcjs').addClass('jz_check_tip_01');
		$('#jzcjs').html('充值号码不能为空');
		$('#chargeAccNbr').val('请输入宽带帐号');
		return false;
	}else if(chargeAccNbr.length<4){
		$('#jzcjs').addClass('jz_check_tip_01');
		$('#jzcjs').html('宽带号码不少于4位');
		return false;
	}else {
		$('#jzcjs').html('');
		return true;
	}
	}
}
//检查充值号码是否为电信号码
function checkCZNumDX(hmobj)
{ 
  var hmString = $("#"+hmobj).val().substring(0,3);
  if(hmString!="180"&&hmString!="189"&&hmString!="153"&&hmString!="133"&&hmString!="181"&&hmString!="177")
  {
  	hmString = $("#"+hmobj).val().substring(0,5);
  	if(hmString!="10649"){
 	         $("#l_czNum").html('仅支持江苏电信号码').addClass('jz_check_tip_01');
	    	return false;
  	}else{
  		
  		$("#l_czNum").html('').removeClass('jz_check_tip_01');
  		return true;
  	}    	
  }
  else
  {	
  	return getPhoneArea();
  }
}
//归属地选择
function cz_checkcities()
{		
var v_cp=getProductType();
if(v_cp=='1'||v_cp=='2'||v_cp=='4'||v_cp=='7'){
   var v_ct = $("#cities").val(); 
	 if(v_ct=="")
	 {
	 	$("#l_cities").html('请选择号码所在地').addClass('jz_check_tip_01'); 
	 	return false;
	 }
	 else
	 {
	    $("#l_cities").html('').removeClass('jz_check_tip_01');
	    return true;
	 }
	 }else{
	 	return true;
	 }
}
//检测充值号码
function cz_checkczNum()
{   
  var v_cznum_value =$("#czNum").val();
  //var v_cptype_qe  = $("#cptype").val();
  var v_cptype_qe=getProductType();
  if($.trim(v_cznum_value)==""||$.trim(v_cznum_value)=='请输入充值号码'||$.trim(v_cznum_value)=='号码前无需加区号'||$.trim(v_cznum_value)=='请输入宽带帐号')
  {
  	if(v_cptype_qe=="3"){
  		$('#czNum').val('请输入充值号码');
  	}else if(v_cptype_qe=="4"){
  		$('#czNum').val('请输入宽带帐号');
  	}else{
  		$('#czNum').val('号码前无需加区号');
  	}
  	 $("#l_czNum").html('充值号码不能为空').addClass('jz_check_tip_01');
  	 return false;
  }
  else if(v_cptype_qe=="1")
  { 
  	if(v_cznum_value.length!=8) {
  	 $("#l_czNum").html('');
  	  $("#l_czNum").html('请输入正确的固话号码（号码前无需加区号）').addClass('jz_check_tip_01');
    return false;
    } else
    {	
    	$("#l_czNum").html('').removeClass('jz_check_tip_01');
    	return true;
    }
  }else if(v_cptype_qe=="4"){
 		if(v_cznum_value.length<4){
 		   	  $("#l_czNum").html('请输入正确的宽带号码').addClass('jz_check_tip_01');
 		   	  return false;
 		   		}else{
 		   			$("#l_czNum").html('').removeClass('jz_check_tip_01');
 		   			return true;
 		   		}
 }else if(v_cptype_qe=="3"){
  		
  		if(v_cznum_value.length!=11){
  	  $("#l_czNum").html('请输入正确的11位手机号码').addClass('jz_check_tip_01');
  	  return false;
  		}else{
  			return checkCZNumDX("czNum");
  		}
  }
}
//充值卡密码
function clearpwd(){
  	var v_czpwd_qe =$("#czpwd_qe").val();
  	if(v_czpwd_qe=='请输入充值卡密码'){
  		$("#czpwd_qe").val('');
  		$("#l_pwd_qe").css("display","none");
  	}
  }

//根据输入的手机号码获取归属地 
function getPhoneArea(){
	var i = divIndex;
	var typeid = getProductType();
	var flag = false;
	var eleName = "";
	if(i=="0" && typeid=="2"){
		flag = true;
		eleName = "chargeAccNbr";
		aleName = "#jzcjs";
	}
	if(i=="1" && typeid=="3"){
		flag = true;
		eleName = "czNum";
		aleName = "#l_czNum";
	}
	var s='';
	var chargeAccNbr = "";
	var numElement=document.getElementById(eleName);
	if(numElement!=null && numElement!=""){
		chargeAccNbr = document.getElementById(eleName).value;
	}
	if(flag && chargeAccNbr!="" && chargeAccNbr!="请输入充值号码"){
		$.ajax({
			url:"/pay/queryAreaCode.action"/*+"?chargeAccNbr="+chargeAccNbr*/,
			type:"post",
			dataType:"json",
			data:{"chargeAccNbr":chargeAccNbr},
			async:false,
			success:function(msg){
				var guisudi=msg.areaName;
				var areaCode=msg.areaCode;
				if(areaCode=='025'||areaCode=='0510'||areaCode=='0511'||areaCode=='0512'||areaCode=='0513'||areaCode=='0514'||areaCode=='0515'||areaCode=='0516'||areaCode=='0517'||areaCode=='0518'||areaCode=='0519'||areaCode=='0523'||areaCode=='0527'){
				$(aleName).html(guisudi);
				var index=guisudi.indexOf('江苏');
				if(index=='0'){
					var str=guisudi.substring(2);
					$(aleName).html(str);
				}else{
					$(aleName).html(guisudi);
				}
				s=true;
				}else{
					$(aleName).html('仅支持江苏电信号码').addClass('jz_check_tip_01');
					s=false;
				}
			}
		});
		
	}
	return s;
}	
//验证充值确认号码
function checkczqrnum(){
	var chargeAccNbr=$('#chargeAccNbr').val();
	var againchargeAccNbr=$('#againchargeAccNbr').val();
	$('#jzcheck').html('');
	$('#jzcheck').removeClass('jz_check_tip_01');
	if(againchargeAccNbr==''|| againchargeAccNbr=='请确认充值号码'){
		$('#againchargeAccNbr').css("color","#aaa");
		$('#againchargeAccNbr').val('请确认充值号码');
		$('#jzcheck').addClass('jz_check_tip_01');
		$('#jzcheck').html('确认号码不能为空');
		return false;
		}else if(chargeAccNbr!=againchargeAccNbr ){
		$('#jzcheck').addClass('jz_check_tip_01');
		$('#jzcheck').html('您两次输入的号码不同');
		return false;
	}else{
		return true;
	}
	}
