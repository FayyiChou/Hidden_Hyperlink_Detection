//清空提示
function clearTips(){
	for (var i = 0;i<arguments.length;i++){
		$(arguments[i]).html('').removeClass("jz_check_tip_01");
	}
}
//清空值
function clearValues(){
	for(var i=0;i<arguments.length;i++){
		$(arguments[i]).val("");
	}
}
function trim(s){
	return s.replace(/^\s+|\s+$/g,"");
}
//根据输入的金额是否符合要求，显示提示信息
function validateMoney(MoneyNum,input){
	if(!isRightNum(MoneyNum)){
	$('#jecheck').html('充值金额必须是大于等于1元且小于等于10万元的整数').addClass('jz_check_tip_01');
	return false;
	}
	else{
		//$(input).parents(".chargeCount").find(".error_remind1").remove();
		$('#jecheck').html('').removeClass('jz_check_tip_01');
		return true;
	}
}

//验证输入的金额是否是大于1且小于100000的正整数
function isRightNum(theNum){
	theNum = trim(theNum);
	if(theNum=="输入其他金额")
		return true;
	if(isNaN(theNum))
		return false;
	if(theNum.indexOf(".") != -1)
		return false;
	if(parseInt(theNum)<1 || parseInt(theNum)>100000)
		return false;
	return true;
}
//输入框失去焦点
function jz_text_blur(jz_id,jz_val){
	var jz_now_value=$(jz_id).val();
	if(jz_now_value==""){
		$(jz_id).val(jz_val);
		$(jz_id).css("color","#aaa");
	}
	//手机提示判断
	var typeid = getProductType();
	if(typeid=="2" && jz_now_value==""&& divIndex=="0"&&jz_id == "#chargeAccNbr"){
		$(jz_id).val("请输入充值号码");
		$(jz_id).css("color","#aaa");
	}
	if(typeid=="3" && jz_now_value==""&& divIndex=="1" && jz_id == "#czNum"){
		$(jz_id).val("请输入充值号码");
		$(jz_id).css("color","#aaa");
	}
}
//输入框获取焦点
function jz_text_focus(jz_id,jz_val){
	var jz_now_value=$(jz_id).val();
	if(jz_now_value==jz_val){
		$(jz_id).val("");
		$(jz_id).css("color","#404040");
	}
	//手机提示判断
	var typeid = getProductType();
	if(typeid==2 && jz_now_value=="请输入充值号码" && divIndex=="0"){
		$(jz_id).val("");
		$(jz_id).css("color","#404040");
	}
	if(typeid==3 && jz_now_value=="请输入充值号码" && divIndex=="1"){
		$(jz_id).val("");
		$(jz_id).css("color","#404040");
	}
}
function batchNumber(obj)
{  
	$(obj).parent().before(" <div class='add_row1' style='height:26px;'><input type='text' id='czpwd_qe' class='input_text input_pwd'   onfocus=\"clear(this)\"  maxlength=\"18\" /><span class='del_row_btn' onclick='removeBatchNumber(this)'></span>&#12288;<span id='1_pwd_qe'></span></div>");
	inputFocus();
	$(obj).parent().find(".error_remind2").remove();	
	if($("#batchDiv input").size()==4)
	{
		$(obj).after("<span class='error_remind2'>最多只支持5张充值付费卡进行充值</span>");
	}
	if($("#batchDiv input").size()>4){
		$(obj).parent().hide();
		return false;
	}
}
//删除批量输入框
function removeBatchNumber(obj)
{
	 $(obj).parent().remove();
	$("#batchDiv").find(".error_remind2").remove();
	var inputLen = $("#batchDiv input").size();
	//caihao 
	if(inputLen<=4){
		$("#addInputRow").show();
		if(inputLen == 4){
			$("#addInputRow").append("<span class='error_remind2'>最多只支持5张充值付费卡进行充值</span>");
		}
	}
}
//新增批量框后，设置起焦点
function inputFocus() {
   var v_inputIndex = $("#batchDiv input").size(); 
   $("#batchDiv input").eq(v_inputIndex-1).focus(); 
}

