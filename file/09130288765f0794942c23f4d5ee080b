}

$(function(){
	var jz_bank_value="";
	var net_bank_list_count=$(".jz_net_bank_list li").size();
	for(var jz_ki=7;jz_ki<=net_bank_list_count-2;jz_ki++){
		$(".jz_net_bank_list li").eq(jz_ki).hide();
	}
	$(".jz_net_bank_list li:last").click(function(){
		var last_text=$(this).find("p").text();
		if(last_text == "选择其他" || last_text == "重新选择"){
			$(".jz_net_bank_list li").show();
			$(this).find("p").text("收起");
			$(this).find(".triangle_02").attr("class","triangle_03");
		}else if(last_text=="收起"){
			var jz_bank_checked=$(this).parents(".jz_payment_active").size();
			if(jz_bank_checked){
				$(".jz_net_bank_list li").hide();
				$("input[value="+jz_bank_value+"]").parents("li").show();
				$(".jz_net_bank_list li:last").show();
				$(this).find("p").text("重新选择");
				$(this).find(".triangle_03").attr("class","triangle_02");
			}else{
				for(var jz_ki=7;jz_ki<=net_bank_list_count-2;jz_ki++){
					$(".jz_net_bank_list li").eq(jz_ki).hide();
				}
				$(this).find("p").text("选择其他");
				$(this).find(".triangle_03").attr("class","triangle_02");
			}
		}
	});
	$("input[name=bankId]").change(function(){
		jz_bank_value=$(this).val();
		var jz_payment_way_index=$(this).parents(".jz_payment_way li").index();
		
		if(jz_payment_way_index>6){
			$(".jz_net_bank_list li").hide();
			$(this).parents(".jz_payment_way li").show();
			$(".jz_net_bank_list li:last").find("p").text("重新选择");
			$(".jz_net_bank_list li:last").find(".triangle_03").attr("class","triangle_02");
		}else{
			$(".jz_net_bank_list li").show();
			for(var jz_ki=7;jz_ki<=net_bank_list_count-2;jz_ki++){
				$(".jz_net_bank_list li").eq(jz_ki).hide();
			}
			$(".jz_net_bank_list li:last").find("p").text("选择其他");
			$(".jz_net_bank_list li:last").find(".triangle_03").attr("class","triangle_02");
		}
		$(".jz_net_bank_list li:last").show();
			
		$(".kdlt_bank .jz_payment_way").removeClass("jz_payment_active");
		$(this).parents(".jz_payment_way").addClass("jz_payment_active");
	});
});
function validateSessionAsy(url,browseIe) {		//url 登录之后转跳的地址，operationCode 弹出层显示的关闭弹出层的文子，1不登陆直接订购，2取消，其他也是取消
	jsFunctionName = url;
	var isSessionOn = true;
	$.ajax({
        type: "POST",
        dataType:"text", 
        url:"/isSessionOn.action", 
        async:browseIe,
        success:function(text) {
        	removeLoaing("201110312219","20111031221912");
        	if(text == "false") {
        		kdlt_search();
        		isSessionOn = false;
        	}else{
        		queryArrears();
        	}
        }
    });
    return isSessionOn;
}
