			$('#czNum_ok').val('请确认充值号码');
			if(jz_pro_num==0){//选择宽带
				$('#czNum').val('请输入宽带帐号');
			}
			$("#cities").parents("li").show();
		}else{
			$('#czNum').val('请输入充值号码');
			$('#czNum_ok').val('请确认充值号码');
			$("#cities").parents("li").hide();
		}
	});
	$(".money_input").focus(function(){
		$(this).parents(".chargeCount").find(".money_list")
			.children("li").removeClass("jz_active");
		jz_text_focus(this,"输入其他金额");
		$('#jecheck').html('');
		$(this).trigger("keyup");
	}).blur(function(){
		jz_text_blur(this,"输入其他金额");
		if(trim(this.value)=="输入其他金额"){
			$(this).css("color","#aaa");
			//$(this).parents(".chargeCount").find(".money_list")
			//	.children("li:first").addClass("jz_active");
			$('#jecheck').html('充值金额不能为空').addClass('jz_check_tip_01');
			flag= false;
		}else{
		flag=validateMoney(this.value,this);}
		//$(this).trigger("keyup");
	}).keyup(function(){
		var moneyNum = parseFloat(trim(this.value));
		$(this).parents(".chargeCount").find(".money_list")
			.children("li").each(function(){
				var moneyListNum = parseFloat($(this).text());
				$(this).removeClass("jz_active");
				if(moneyNum == moneyListNum){
					$(this).addClass("jz_active");
				}
			});
		var isNum = isNaN(this.value);
		if(!isNum){
			$(this).css("color","#404040");
		}
		if(trim(this.value)=="输入其他金额"){
			$(this).parents(".chargeCount").find(".money_list")
				.children("li:first").addClass("jz_active");
		}
		flag= validateMoney(this.value,this);
	}).trigger("keyup");
	$("#czpwd_qe").live("blur",function(){
		var v_czpwd_qe =$(this).val();
  	 if(v_czpwd_qe==''||v_czpwd_qe=='请输入充值卡密码'){
  	 	$(this).parent().children('#1_pwd_qe').html('充值密码不能为空').addClass('jz_check_tip_01');
  	 }else if(v_czpwd_qe.length<18||v_czpwd_qe.length>18)
  	 {
  	 	$(this).parent().children('#1_pwd_qe').html("请输入18位充值卡密码").addClass('jz_check_tip_01');
  	 }else{
  	 $(this).parent().children('#1_pwd_qe').html("").removeClass('jz_check_tip_01');

  	 }
	});
});
/**
