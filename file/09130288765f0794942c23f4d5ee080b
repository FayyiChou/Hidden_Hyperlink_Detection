$(function(){
	
	$(".kdlt_main_tab li").click(function () {
		
		$(this).addClass("on").siblings().removeClass("on");
		var index = $(this).index();
		$(this).parents(".kdlt_main").find(".kdlt_main_box").hide().eq(index).show();
		
		divIndex = $(".kdlt_main_tab .on").index();
		if(divIndex=="0"){
			clearTips("#jzcjs","#jzcheck","#yzmcheck","#jecheck","#ssquhao");
			$("#chargeMoney_choose li").eq(0).trigger("click");
			clearValues("#imageCode");
			getNewImage();
			$("#stepone_div").show();
			$("#steptwo_div").hide();
			$("#p6").hide();
		}else{
			clearTips("#l_czNum","#l_cities","#cz_jzcheck","#1_pwd_qe","#l_yzm_error");
			$("#cz_stepone").show();
			$("#cz_result").hide();
			cz_getNewImage();
		}
	});
	
	$("ul.jz_box_radio li").live("click",function(){
		$(this).parents("ul.jz_box_radio").find("li").removeClass("jz_active");
		$(this).addClass("jz_active");
		
	});
	$("#chargeMoney_choose li").click(function(){
		var jz_index_num=$(this).index();
		var jz_list_num=$("#chargeMoney_choose li").size();
		if(jz_index_num+1==jz_list_num){
			$("#other_chargeMoney").removeAttr("disabled");
		}else{
			$("#other_chargeMoney").val("输入其他金额");
			$("#other_chargeMoney").css("color","#aaa");
			$("#other_chargeMoney").attr("disabled","disabled");
		}
	});
	
	$("#jz_products_list li").click(function(){
		var jz_pro_num=$(this).index();
		clearValues("#imageCode");
	 	clearTips("#jzcjs","#jzcheck","#yzmcheck","#jecheck","#ssquhao");
		if(jz_pro_num==0 || jz_pro_num==2){
			$('#chargeAccNbr').val('号码前无需加区号');
			$('#againchargeAccNbr').val('请确认充值号码');
			if(jz_pro_num==0){//选择宽带
				$('#chargeAccNbr').val('请输入宽带帐号');
			}
			$("#areaCode").parents("tr").show();
		}else{
			$('#chargeAccNbr').val('请输入充值号码');
			$('#againchargeAccNbr').val('请确认充值号码');
			$("#areaCode").parents("tr").hide();
		}
		
	});
	//查询li
	$("#search_ul li").click(function(){
		var jz_pro_num=$(this).index();
		clearValues("#ser_num","#loginImageCode","#ser_pwd");
