            window.location.href = 'member.php?mod=logging&action=login&mobile=2';
            return ;
        }

        var url = '';
        datapid = obj.attr('data-pid');
        if(datapid){
            url = 'forum.php?mod=misc&action=postreview&do=support&tid=555062&pid='+datapid+'&hash=2a812a47&inajax=1&isappinstalled=1';
        }else{
            url = 'forum.php?mod=misc&action=recommend&do=add&tid=555062&hash=2a812a47&inajax=1&isappinstalled=1';
        }

        $.ajax({
                type: 'GET',
                url: url,
                dataType: 'xml'
            })
            .success(function (s) {
                evalscript(s.lastChild.firstChild.nodeValue);
            })
            .error(function () {
                popup.close();
            });
    }

