    var _edit_pic_SafePoster = {
        _upload: function(id) {
            _upload._ok = _edit_pic_SafePoster._uploadOk;
            $(id).parent().submit();
        },
        _uploadOk: function(state, error, saveUrl) {
            if (state != "0") {
                _toast._show(error);
                _loading._hide();
                return;
            };

            var index = $("#property_img_"+detail_join_party_model.upImgId).attr("data-listindex");
            detail_join_party_model.join_property_list[index].property_value = saveUrl.replace("http://img2.hudongba.com","");
            detail_join_party_model.cache_all_join_property();

            _loading._hide();
            _edit_pic_SafePoster._clearFile($('#image_upload_join_'+detail_join_party_model.upImgId));
        },
        _clearFile: function(o) {
            $(o).html("");
            $(o).html("<input class=\"uploadInput\" type=\"file\" name=\"file\" title=\"上传图片\" onchange=\"inputImagechange($(this))\" >");
        }
    };
    var _upload = {
        _ok: null
    };
    function inputImagechange(id){
        var v = $(id).val();
        detail_join_party_model.upImgId = $(id).parent().attr("id").replace("image_upload_join_","");
        console.log(detail_join_party_model.upImgId);
        _loading._show("上传中...");
        if(v.indexOf('.jpg')!=-1 || v.indexOf('.png')!=-1 || v.indexOf('.gif')!=-1 || v.indexOf('.jpeg')!=-1 || v.indexOf('.JPG')!=-1 || v.indexOf('.PNG')!=-1 || v.indexOf('.GIF')!=-1 || v.indexOf('.JPEG')!=-1){
            if(_._fileSize($(id)) > 8*1024){
                _toast._show("图片大小不能超过8M");
                _loading._hide();
                return;
            }
            _edit_pic_SafePoster._upload(id);
        }else{
            _toast._show("只能选择图片文件");
            _loading._hide();
            _edit_pic_SafePoster._clearFile($('#image_upload_join_'+detail_join_party_model.upImgId));
        }
    }

    //上传文件
    var _edit_file_SafePoster = {
        _upload: function(id) {
            _uploadFile._ok = _edit_file_SafePoster._uploadOk;
            $(id).parent().parent().parent().submit();
        },
        _uploadOk: function(state, error, saveUrl) {
            if (state != "0") {
                _toast._show(error);
                _loading._hide();
                return;
            }

            var index = $("#property_file_"+detail_join_party_model.upImgId).attr("data-listindex");
            detail_join_party_model.join_property_list[index].property_value = saveUrl.replace("http://img2.hudongba.com","");
            detail_join_party_model.cache_all_join_property();

            _loading._hide();
            _edit_file_SafePoster._clearFile($('#file_upload_join_'+detail_join_party_model.upImgId));
        },
        _clearFile: function(o) {
            var upFileId = detail_join_party_model.upImgId;
            $(o).html("<p class=\"reupLoadAi\"><i class=\"upload_icon_2\"></i><span></span><a class=\"reupLoadAi_btn\">重新上传<input class=\"uploadAI\" type=\"file\" name=\"file\" title=\"选择文件\" onchange=\"uploadFileChange($(this))\"><input id=\"file_type_" + upFileId + "\" type=\"hidden\" name=\"fileType\" value=\"\"></a></p>");
            $(o).find("span").html($("#file_path_"+upFileId).val());
        }
    };
    var _uploadFile = {
        _ok: null
    };
    function uploadFileChange(id){
        var fileName = $(id).val();
        if(_._fileSize($(id)) > 8*1024){
            _toast._show("文件大小不能超过8M");
            _loading._hide();
            return;
        }
        var fileType = fileName.substring(fileName.lastIndexOf(".") + 1);
        detail_join_party_model.upImgId = $(id).parent().parent().parent().attr("id").replace("file_upload_join_", "");
        $("#file_type_" + detail_join_party_model.upImgId).val(fileType);
        var filaNameNoPath = fileName.substring(fileName.lastIndexOf("\\") + 1);
        $("#file_path_"+ detail_join_party_model.upImgId).val(filaNameNoPath);
        _loading._show("上传中...");
        _edit_file_SafePoster._upload(id);
    }

