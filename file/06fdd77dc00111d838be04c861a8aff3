$(document).ready(function(){
  $("li").hover(function(){
    $(this).find('div').css('display','block');
    },function(){
    $(this).find('div').css('display','none');
  });
});

        $(document).ready(function(){$(".news_rb_left ul a").mouseover(function(){
          $(this).addClass("on")})
        });
        $(document).ready(function(){$(".news_rb_left ul a").mouseout(function(){
              $(this).siblings().removeClass("on");})
        });

 function nTabs(thisObj, Num,content) {
            if (thisObj.className == "active") return;
            var tabList = document.getElementById("myTab").getElementsByTagName("a");
            for (i = 0; i < tabList.length; i++) {
                if (i == Num) {
                    thisObj.className = "active";
                    document.getElementById("myTab_Content" + i).style.display = "block";
                    $('#img_bt_p').html(content);
                } else {
                    tabList[i].className = "normal";
                    document.getElementById("myTab_Content" + i).style.display = "none";
                }
            }
        }

function interval() {
 	setInterval("changeImg()", 3000);
 }
 var i = 0;
 function changeImg(){
 	i++;
 	if(i == 3){
 		i = 0;
 	}
 	
 	$("#bar_ul").find("li").hide();
 	$("#bar_"+i).show();
 	$("#_imgNum").find("a").removeClass("active");
 	$("#ac_"+i).addClass("active");
 }
    
    window.onload = interval;
   //header悬浮
    $(window).scroll(function(){
      if ($(window).scrollTop()>102) {
        $(".header").addClass('active');
      } 
      if ($(window).scrollTop()<102) {
        $(".header").removeClass('active');
      } 
       })
    //
</script>


