function headerNav(){
	$(".nav-tabg").hide();
	//$("h1.logo a").append("<i></i>")
	$(".bt_nav_cen").hover(function(){
	},function(){
		$(".bt_nav_cen ul li").removeClass("selected");
		//$(".topNav .menuCon .subcon").hide();
		//$(".nav-tabg").stop().hide()
		var window_width=$(window).width();
	       if(window_width>768){
			   $(".nav-tabg").stop().hide()
	     }
		 else{
			 $(".nav-tabg").show()
			 }
	})
	$(".bt_nav_cen ul li").hover(function(){
		var subobj = $(this).find("span").attr("class").replace("m", "menus");
		var $this = $(this);
		index = $this.index();
        curNav=$this.addClass("selected").siblings().removeClass("selected");
		//$(".subcon").hide();
		//$('.'+subobj).show();   
        $(".nav-tabg").show()
        var pos=120*index;
        $(".nav-tabg").stop().animate({"left":pos},500,function(){})     
        setTimeout("curNav",2000)
    })
}
function head_mini(){
		var window_width2=$(window).width();
	       if(window_width2>768){
	         }
		 else{
			 $(".nav-tabg").show()
			 }
	$(".nav-tabg").click(function(){
		$(this).next("ul").slideToggle();
		})
	}
</script>
