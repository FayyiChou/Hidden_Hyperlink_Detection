    function fav() {
        var url = window.location.href;
        try {
            window.external.addFavorite(url, document.title);
        } catch (err) {
            try {
                window.sidebar.addPanel(document.title, url, "");
            } catch (err) {
                alert("请使用Ctrl+D为您的浏览器添加书签！");
            }
        }
    }
