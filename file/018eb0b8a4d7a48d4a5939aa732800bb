var desc = $("#wx_share_desc").text();
var imgUrl = $("#wx_share_img").text();

if(imgUrl.length==0){
	var imgs=$('#img-content img');
	if(imgs && imgs.length>0){
		imgUrl=imgs[0].src;
	}
}
if(imgUrl.length==0)imgUrl="http://microcard.lentor.net/images/android_icon-72@2x.png";
if(desc.length==0){
	desc = $("#img-content").text().substr(0,60).trim();
}
