  $(document).ready(function(e) {
	var b=$(document).scrollTop();
	if(b>155){
		$(".daohang").show();
		}
    $(window).scroll(function(){
		  var a=$(document).scrollTop();
		  if(a>155){
			    $(".daohang").fadeIn(200);
			  }
		  else{
			  $(".daohang").fadeOut(200)
			  }	   
		})
	
});
</script>
