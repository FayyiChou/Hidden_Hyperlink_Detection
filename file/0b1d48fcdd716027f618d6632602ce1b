<script>
	EDU.Comment.getInstance().addEventHandler(window,'scroll',scroll_func,false);
	var MaxTop = 0;  //最大高度
	var page = data.page;  //当前页数
	var max_page = data.max_page; //最大页数
	var code = data.code;
	var k = data.word;
	var status = 1;
	function scroll_func(){
		var wTop = document.documentElement.scrollTop || document.body.scrollTop;
		 if(wTop > MaxTop){
			MaxTop = wTop;
		 }
		var wHeight = document.documentElement.scrollHeight || document.body.scrollHeight;
		var cHeight = document.documentElement.clientHeight || document.body.clientHeight;
		var nowpage = page*1+1;
		if(wTop >= MaxTop && status==1){
			if((cHeight+wTop+100)>wHeight){
				if(nowpage <= max_page){
					status = 0;
					$(".zhul_tk_jz").html('<a href="javascript:void(0);">加载中</a>');
					//少于总页数，请求
					$.post('/mobile/getSearch',{'k':k,'c':code,'p':nowpage,'Time':Math.random()},function(data){
						var obj = eval("("+data+")");
						$(".zhul_tk_jz").html(''); 
						if(obj.errNo==0){
							var html = '';
							var myDate;
							for(var i = 0;i<obj.result.list.length;i++){
								html+='<div class="zhul_sy_tklist">';
								html+='<a href="'+obj.result.list[i].url+'">';
    							html+='<p class="zhul_tk_tit">';
								if (obj.result.list[i].worksid > 0) {
    								html += '<span class="tk_tit_yc">原创</span>&nbsp;';
    							}
								html+=obj.result.list[i].title+'</p>';
								html+='<p class="zhul_tk_img"><img src="'+obj.result.list[i].pic_max+'" width="100%" /></p>';
								html+='<p class="zhul_tk_date">';
        						html+='<span class="zhul_f_l">';
								myDate = new Date(obj.result.list[i].create_time*1000);
								html+= myDate.getFullYear()+'-'+myDate.getMonth()+'-'+myDate.getDate()+'</span>';
								html+='<span class="zhul_f_r"><img src="/img/sj_img/zhul_imgs_icon.png" width="16" />&nbsp;'+obj.result.list[i].pic_num+'&nbsp;&nbsp;&nbsp;&nbsp;<img src="/img/sj_img/zhul_pls_icon.png" width="18" />&nbsp;'+obj.result.list[i].reply_num+'</span></p>';
								html+='<p class="zhul_tk_nrong">'+obj.result.list[i].content+'</p>';
								html+='</a></div>';
							}
							$(".zhul_sy_tklist").last().after(html);
							page = page*1+1;
							status=1;
						}
					});
				}else{
					//$(".zhul_tk_jz").html('<a href="javascript:void(0);">没有更多数据</a>');
				}
			} 
			
		}
	}
	
</script>
