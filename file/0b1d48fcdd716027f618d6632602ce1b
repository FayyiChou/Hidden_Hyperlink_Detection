<script>
   var mObj = document.getElementById("menu");
   EDU.Comment.getInstance().addEventHandler(mObj,'click',SetStyle,false);

   function SetStyle(e){
	
	 var NavObj = document.getElementById("edu_nav");
	 if( NavObj.style.display=="none"){
		NavObj.style.display="block";
	 }else{
		NavObj.style.display="none";
	 }
	 e.preventDefault();
   }
  document.onclick = function(){
	var e = e || window.event; //浏览器兼容性 
	var elem = e.target || e.srcElement; 
	while (elem) { //循环判断至跟节点，防止点击的是div子元素 
	if (elem.id && elem.id=='menu') { 
		return; 
	} 
		elem = elem.parentNode; 
	} 

	var NavObj = document.getElementById("edu_nav");
	NavObj.style.display="none";
  }
</script><div class="zhul_sy_search">
