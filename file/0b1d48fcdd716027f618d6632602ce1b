	function checkform(){
		var v=$.trim($(".zhul_sj_stext").val());
		if(v=="" || v==""){
			 return false;
		}
		if(v!=$(".zhul_sj_stext").val()) $(".zhul_sj_stext").val(v);
		return true;
	}
