function search_(){
	var keywords = document.getElementById('keywords').value;
	keywords = keywords.replace(/(^\s*)|(\s*$)/g, '');
	if(keywords==null || keywords == ''){
		alert('请输入关键字!');
		return;
	}
	window.location.href="http://che.hexun.com/m/search/"+keywords+'/';
}


