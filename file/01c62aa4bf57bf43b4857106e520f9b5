   $('#tabs li').click(function(){
   	  $(this).addClass('ck').siblings().removeClass('ck');
   	  var _index = $(this).index();
   	  $('.tab-cont').eq(_index).css('z-index','999').siblings().css('z-index','99')
   })
</script>

