<script>
$(function(){
  $('.pop .con p a').click(function(){
      $('.mask').show();
      $('.online_service').show();
  });
  $('.pop .p1 a').click(function(){
     $('.online_service_con dl:first').show().siblings().hide();
     $('.online_service_nav a:first').addClass('current').siblings().removeClass('current');
  });
  $('.pop .p2 a').click(function(){
      $('.online_service_con dl:last').show().siblings().hide();
      $('.online_service_nav a:last').addClass('current').siblings().removeClass('current');
  });   
  $('.online_service_nav a').click(function(){
      $(this).addClass('current').siblings().removeClass('current');
     var tot =$('.online_service_nav a').index($(this));
     $('.online_service_con dl').eq(tot).show().siblings().hide();
  });
  $('.online_service .close').click(function(){
     $('.mask,.online_service').hide(); 
  });
  $('.pop .close').click(function(){
     $('.pop').hide(); 
  });
})
</script>
