<script>
(function(){
	var speed = 10; //滚动速度
    var lis = 255; //单个单位的宽度
	var i = 0;
    var t = true;
	var a = parseInt($('#room').css('width'));
    var b = parseInt($('#room2 li').length * lis);
    $('#room2').css('width',b)
	var distance = b-a;
    // console.log(distance);
    function add(){
        i++;
    }
    function reduce(){
        i--;
    }
    function jia(){
        if((i< distance || i<i) && t){
            add();
        }else if(i >= distance || !t){
            t= false;
            reduce();
        }
        if(i==0){
            t = true;
        }
    $('#room2').css('left',-i)
    }
    var d = setInterval(jia, speed);
	$('#room').mouseover(function(){
		clearInterval(d);
	});
	$('#room').mouseleave(function(){
		d = setInterval(jia, speed);
	});
}());
</script>	 
