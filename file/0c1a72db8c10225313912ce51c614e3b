	$(function () {
		function imBuriedPointCachedScript(url, callback) {
			callback = (typeof callback != 'undefined') ? callback : {};
		    $.ajax({
		         type: "GET",
		         url: url,
		         success: callback,
		         dataType: "script",
		         cache: true
		     }); 
