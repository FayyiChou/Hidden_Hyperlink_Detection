	$(function () {
		function imBuriedPointCachedScript(url, callback) {
			callback = (typeof callback != 'undefined') ? callback : {};
		    $.ajax({
		         type: "GET",
		         url: url,
		         success: callback,
		         dataType: "script",
		         cache: true
		     }); 
		}
		     
		function loadJS(urls, callback) {
			var url = urls.shift();
			if(url){
				imBuriedPointCachedScript(url, function(data) {
				    loadJS(urls, callback);
				});
			} else if(callback){
				callback();
			}
		}
		
