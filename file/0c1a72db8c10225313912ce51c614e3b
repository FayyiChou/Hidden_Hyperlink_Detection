<div class="feixiangOnlineConsultation"><script>
//由于一个页面中可能会出现多个在线客服的埋点，所以需要避免重复多次定义函数
var openTalkDialog_ = openTalkDialog_ || (function() {
	//定义一个变量，保证用户不能频繁打开窗口。如果条件为true就表示可以打开窗口，如果为false就表示在短时间只能已经有被打开窗口了。
	var canOpenWindow = true;
	
	//暂停打开新窗口的请求，短时间之后恢复
    function updateWEBIMFlag() {
        canOpenWindow = false;
		//3秒后恢复
		setTimeout(function(){canOpenWindow=true;}, 3000); 
	}
	
	return function(span) {
	    //保证能够打开窗口和打开窗口的方法有效
	    if (canOpenWindow && imFront_checkIsLogin) {
	        updateWEBIMFlag();
	        
	        var spanDom = $(span);
			var merchantId = spanDom.attr("merchantid");
			var positionId = spanDom.attr("positionid");
			var mcSiteId = spanDom.attr("mcsite");
			var orderCode = spanDom.attr("orderCode");
			var productId = spanDom.attr("productId");
			var supplierId = spanDom.attr("supplierid");
			var pmInfoId = spanDom.attr("pmInfoId");
			imFront_checkIsLogin(merchantId,positionId,mcSiteId,orderCode,productId,supplierId,pmInfoId);
	    }
	}
})();

</script>
