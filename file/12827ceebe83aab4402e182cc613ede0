<script type="text/javascript">
$(document).ready(function(){ 
	var autospeed = 6000;	
	var n=0;
	var $con = $('#gg'), $btns = $con.find('#ggBtns')
	function autoroll(){
		if(n==4){
			n=0;
			}
			$("#ggBox a").fadeOut(500);
			$("#ggBox a").eq(n).fadeIn(500);
			$btns.find('span:eq(' + n + ')').addClass('ggOn').siblings().removeClass('ggOn');
			n++;
			timer = setTimeout(autoroll,autospeed);
		};
		autoroll();
		$btns.find('span').click(function () {
			clearTimeout(timer);
			n = $(this).index();
        autoroll(n);
    	});
});
</script>

