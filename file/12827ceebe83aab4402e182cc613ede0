<script type="text/javascript">
function hide_drop(){
		$("#weibo_drop").hide("fast");
		$("#weixin_drop").hide("fast");
	}
	function show_weibo_drop(){
		hide_drop();
		$("#weibo_drop").show("fast");
	}
	function show_weixin_drop(){
		hide_drop();
		$("#weixin_drop").show("fast");
	}
//弹出菜单开始
$(document).ready(function(){
  $('.nav_li').mousemove(function(){
  $(this).find('.nav_box').show();
  });
  $('.nav_li').mouseleave(function(){
  $(this).find('.nav_box').hide();
  });
});
//弹出菜单结束
</script>

