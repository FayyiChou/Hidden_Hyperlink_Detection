<script type="text/javascript" src="/SysNew/JS/alert.js"></script>
<script type="text/javascript">
    var closeAlert = "closeAlertIndex";
    
    //展开课程
    function showHidCourse(studyTableId) {
        $("#tbShowStudyCourse" + studyTableId).hide();
        $("#tbStudyCourse" + studyTableId).show();
        $("#openUp" + studyTableId).hide();
        $("#packUp" + studyTableId).show();
    }
    //收起课程
    function hideCourse(studyTableId) {
        $("#tbStudyCourse" + studyTableId).hide();
        $("#tbShowStudyCourse" + studyTableId).show();
        $("#packUp" + studyTableId).hide();
        $("#openUp" + studyTableId).show();
    }
</script>



