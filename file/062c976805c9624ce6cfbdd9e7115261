		<script type="text/javascript">
           	function refreshYzm(){
            	//禁用footer
                $.ui.removeFooterMenu();
            	//验证码
            	$(".yanzhengma").text(createCode(4));
            	//设置整体样式
                $("#afui").attr("class","android");
            }
            function reset(){
            	document.getElementById("nsrsbh_").value = "";
            	document.getElementById("nsrmc_").value = "";
            	document.getElementById("yzm_").value = "";
            }
            
            //禁用header动画效果
            $.ui.animateHeaders = false;
            var nsrsbh , nsrmc;
            $.ui.ready(function(){
                //禁用footer
                $.ui.removeFooterMenu();
                //验证码
            	$(".yanzhengma").text(createCode(4));
                //设置整体样式
                $("#afui").attr("class","android");
                
                $('#kprq_').mobiscroll({
                    preset: 'date',
                    dateFormat:'yyyy-mm-dd',
                    dateOrder:'yymmdd',
                    invalid: { daysOfWeek: [0, 6], daysOfMonth: ['5/1', '12/24', '12/25'] },
                    theme: "android-holo",
                    mode: "scroller",
                    lang: "zh",
                    display: "modal"
                });
            });
            function submit(){
            	 nsrsbh = $("#nsrsbh_").val();
            	 nsrmc = $("#nsrmc_").val();
            	var yzm = $("#yzm_").val();
            	if(nsrsbh=="" && nsrmc==""){
                    opt.message="纳税人识别号和纳税人名称，不能同时为空";
                    $.ui.popup(opt);
                    return;
                }else if(yzm==""){
                    opt.message="请输入验证码";
                    $.ui.popup(opt);
                    return;
                }else if(!yanzheng(yzm,$(".yanzhengma").text())){
                	opt.message="验证码错误";
                	$.ui.popup(opt);
                	$("#yzm_").focus();
                	return;
                }
            	$.ajax({
                    type:"post",
                    url:"/search/jsp/zhcx/service.jsp?action=getNsrztResult",
                    data:{'nsrsbh':nsrsbh,'nsrmc':nsrmc},
                    success:function(data){
                    	var html = viewData(data);
                    	$("#cyjg").find(".cxjg .content").html(html);
                    	
                    },complete:function(a,b,c){
                    	$("#cyjg").find(".nsrmc").text(nsrmc);//赋值
                		$("#cyjg").find(".nsrsbh").text(nsrsbh);//赋值
                		
                    	//alert(a);
                    	//alert(b);
                    	//alert(c);
                    }
                });
                $.ui.loadContent("#cyjg_panel",false,false,"slide");
            }
            //显示结果
            function viewData(data){
            	var result=eval("("+data+")");
                 //console.log(result);
                 if(result.flag=="success"){
                	var html="";
                	for(var i=0;i<result.data.length;i++){
                		nsrmc = result.data[i].nsrmc;
                		nsrsbh = result.data[i].nsrsbh;
                		//$("#cyjg").find(".nsrmc").text(nsrmc);//赋值
                		//$("#cyjg").find(".nsrsbh").text(nsrsbh);//赋值
                		var nsrmc = "<tr><td text-align=\"left\"><strong>纳税人名称&nbsp;&nbsp;</strong>"+result.data[i].nsrmc+"</td></tr>";
                		var nsrsbh = "<tr><td text-align=\"left\"><strong>纳税人识别号&nbsp;&nbsp;</strong>"+result.data[i].nsrsbh+"</td></tr>";
                		var nsrztmc = "<tr><td text-align=\"left\"><strong>纳税人状态&nbsp;&nbsp;</strong>"+result.data[i].nsrztmc+"</td></tr>";
                		var swjgmc = "<tr><td text-align=\"left\"><strong>主管税务机关名称&nbsp;&nbsp;</strong>"+result.data[i].swjgmc+"</td></tr>";
                		html+="<table class='dataList' style='width:100%'>"+nsrmc+nsrsbh+nsrztmc+swjgmc+"</table>";
                	}
                 	return html;
                		
                }else{
                	return result.msg;
                }
            }
        </script>
	</head>
