$(function(){
    var t = '';
    $('.onsub').hover(function(){
        clearTimeout(t);
        $(this).addClass('on');
        $('.pro_sub').addClass('show');
    },function(){
        var self = this;
        t = setTimeout(function(){
            $(self).removeClass('on');
            $('.pro_sub').removeClass('show');
        },100);
    });

    $('.pro_sub').hover(function(){
        clearTimeout(t);
    },function(){
        var self = this;
        t = setTimeout(function(){
            $('.onsub').removeClass('on');
            $(self).removeClass('show');
        },100);
    });
});
</script>
