<script>
jQuery('.links_btn').click(function(e){
	e.stopPropagation();
	if($(this).hasClass('on')){
		$(this).removeClass('on');
		$(this).find('.span2').fadeIn(150);
		$(this).find('.links_list').slideUp(200);
	}else{
		$(this).addClass('on');
		$(this).find('.span2').fadeOut(150);
		$(this).find('.links_list').slideDown(200);
	}
});

jQuery('body').click(function(){
	jQuery('.links_btn').removeClass('on');
	jQuery('.links_btn').find('.span2').fadeIn(150);
	jQuery('.links_btn').find('.links_list').slideUp(200);
});

jQuery('#follow .i3').hover(function(){
	$(this).find('.erwei').fadeIn(200);
},function(){
	$(this).find('.erwei').fadeOut(200);
});

jQuery('#follow .i4').click(function(){
	$("html, body").animate({ scrollTop: 0 }, 300);
});

$(window).scroll(function(){
   if($(window).scrollTop()>750){
       $('#follow').show();
   }else{
       $('#follow').hide();
   }
});
</script>
