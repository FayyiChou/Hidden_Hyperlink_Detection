<div id="footerJs">
<script src="/resource/js/jquery.cookie.min.js" type="text/javascript"></script>
                
                
                
<script type="text/javascript">
$(document).ready(function () {
		$('img[data-original]').lazyload({ threshold: 100 });
		/**首页幻灯片开始**/
		$.showbox = $('#fullSlider>li'); //总图片div
		len = $.showbox.length; //计算列表总长度(个数)
		pages = Math.ceil(len / 1); //计算展示版面数量
		var li = '';
		for (i = 0; i < len; i++) { li += '<li></li>'; }
		var w = 30 * len - 5; //显示条的总宽度
		$('#slideMark').css({ 'width': w }).append(li).find('li:last').css({ 'marginRight': 0 });
		$.tab = $('#slideMark>li');
		$.showbox.eq(0).show(); //默认显示第一张
		$.tab.eq(0).addClass('cur'); //默认显示第一张
		var timer = setInterval(imgRotation, 5000);
		var index = 1;
		$.tab.mouseover(function () {
				clearInterval(timer);
				index = $.tab.index(this);
				$.showbox.hide();
				$.showbox.eq(index).fadeIn(500);
				$.tab.eq(index).addClass('cur').siblings().removeClass('cur');
				index++;
				timer = setInterval(imgRotation, 5000);
		})
		function imgRotation() {
				if (index >(pages - 1)) {
						index = 0;
				}
				$.showbox.hide();
				$.showbox.eq(index).fadeIn(500);
				$.tab.eq(index).addClass('cur').siblings().removeClass('cur');
				index++;
		}
		/**首页幻灯片结束**/
		menuToggle();
		var navPos = 0;
		$('#allClass li').each(function (i) { //首页快速导航右侧模块重新定位
				$(this).find('.sub').css({ 'top': navPos });
				navPos -= 44;
		});
});
function menuToggle() {
		var arrayli = $('#allClass li');
		arrayli.each(function (i) {
			$(this).hover(
				function () { $(this).addClass('this'); },
				function () { $(this).removeClass('this'); }
			);
			$(this).find('em').click(function () { arrayli.removeClass('this'); });
		});
}
</script>
</div>
