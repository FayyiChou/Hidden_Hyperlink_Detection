function trsform_onsubmit() {	
	    var SQL="";//最终提交词
	    var search = document.trsform.search.value;
               	    	    
	     if(search==""){
	     alert("检索词不能为空");
	        return false;}	
	    if(search.length>0){
	    search = search.replace( /(^[\s??]*)|([\s??]*$)/g, "");
	   	search = search.replace(/\\-/g,'`').replace(/`/g,'\\-');
	   	search = search.replace(/'/g,'\\\'');
              }//过滤特殊字符  
  	    document.trsform.searchword.value=search ; 	
             return true;	
}
