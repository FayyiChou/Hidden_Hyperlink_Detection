       var staticTranslations = new Object();
                function translate(input){
                    var translateElement = $('<div></div>');
                    if(staticTranslations[input]!==undefined){
                        translateElement.html(staticTranslations[input]);
                        return translateElement.html();
                    }
                    return input;
                }
                staticTranslations['Выбрано'] = 'Выбрано';
                staticTranslations['Search'] = 'Поиск';
                staticTranslations['None selected'] = 'Любые';
                staticTranslations['All selected'] = 'Все';
                staticTranslations['Select all'] = 'Выбрать все';
                staticTranslations['Selected'] = 'Выбрано';
                staticTranslations['Choose Hour'] = 'Выберите час';
                staticTranslations['Hour'] = 'Час';
                staticTranslations['Choose Minutes'] = 'Выберите минут';
                staticTranslations['Minutes'] = 'Минут';
                staticTranslations['Choose'] = 'Выбрать';
