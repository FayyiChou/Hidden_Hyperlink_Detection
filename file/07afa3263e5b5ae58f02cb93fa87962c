 var speed=65
  link_logo2.innerHTML=link_logo1.innerHTML
    function Marquee2(){
	  if(link_logo2.offsetTop-link_logo.scrollTop<=0)
	     link_logo.scrollTop-=link_logo1.offsetHeight
		   else{
		    link_logo.scrollTop++
			   }
			      }
				     var MyMar2=setInterval(Marquee2,speed)
