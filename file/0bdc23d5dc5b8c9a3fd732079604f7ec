    $(document).ready(function(){
        $('.menumask').bind('click',function(){
                $('.rheader').removeClass('showmenu');
                $('.menubtn').removeClass('hidebtn');
                $('.menumask').fadeOut();
                $('.menubtn').css("position","relative");
        });
        $('.menubtn').bind('click',function(){
            if( $('.rheader').hasClass('showmenu')){
                $('.rheader').removeClass('showmenu');
                $('.menumask').fadeOut();
                $('.menubtn').css("position","relative");
            }else{
                $('.rheader').addClass('showmenu');
                $('.menumask').fadeIn();
                $('.menubtn').css("position","fixed");
            };
            if( $('.menubtn').hasClass('hidebtn')){
                $('.menubtn').removeClass('hidebtn');
                
            }else{
                $('.menubtn').addClass('hidebtn');
            };
        });
        var windw = window.innerWidth;
        if( windw < 414){
            remove

        }
    });
  	//选项卡特效
    function setTab(name,cursel,n){
     for(i=1;i<=n;i++){
      var menu=document.getElementById(name+i);
      var con=document.getElementById("con_"+name+"_"+i);
      menu.className=i==cursel?"hover":"";
      con.style.display=i==cursel?"block":"none";
     }
    }
    function logout(){
    	$.ajax({
    		type: "POST",
    	    url: "/index.php/Home/Index/logout.html",
    	    success: function (data) {
    	    	alert('退出成功！');
    	    	setTimeout(function () {
	    			window.location.href = "/";
	    	    }, 1500);
    	    },
    		cache: false,
    		error:function(){
    			
    		}
    	});
    }
    </script>
