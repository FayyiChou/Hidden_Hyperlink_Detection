    <script type="text/javascript">
        (function(){
            var imgList = HX.$$('#cpFocus .cleft1 div');
            var liList = HX.$$('#cpFocus .cright1 ul li');
            var len = Math.min(imgList.length,liList.length);
            var timer = null;
            var marker = 0;
            var gaper = 3000;
            var _fx = function() {
                marker+=1;
                if(marker==len) marker = 0;
                _change(marker);
            };
            var _change =function(i) {
                clearTimeout(timer);
                for(var j=0;j<len;j++) {
                    var classes = liList[j].className;
                    classes = (classes.indexOf('_on')!=-1)?(classes.replace('_on','')):classes;
                    if(i==j) {
                        marker = j;
                        imgList[j].style.display = '';
                        liList[j].className = classes+'_on';
                        timer = setTimeout(function(){_fx();},gaper);
                    }
                    else {
                        imgList[j].style.display = 'none';
                        liList[j].className = classes;
                    }
                }
            };
            for(var i=0;i<len;i++) {
                liList[i].onmouseover = (function(i) {
                    return function() {
                        _change(i);
                    }
                })(i);
            }
            timer = setTimeout(function(){_fx();},gaper);
        })();
    </script>
  </div>
</div>
