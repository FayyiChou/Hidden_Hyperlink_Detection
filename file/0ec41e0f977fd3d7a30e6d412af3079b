<script>
 function chg(num){
  var menuObj;
  var conObj;
  for(i = 0;i<12;i++){
    menuObj = document.getElementById('gpmodb_m'+i);
    conObj = document.getElementById('gpmodb_con'+i);
	if(i==num){
	  menuObj.className = 'selected';
      conObj.style.display = 'block';
	}else{
	  menuObj.className = '';
      conObj.style.display = 'none';
	}
  }
  
 }
</script>
