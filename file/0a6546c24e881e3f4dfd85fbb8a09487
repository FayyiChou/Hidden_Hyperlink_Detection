		  <SCRIPT>
				var cp=1;
				function ShowRNp()
				{
				   /*$("http://www.5icool.orgBtnLNp").show();
					if(cp==5)
					{
					   return;
					} */
				   $("#NP"+cp).hide();
				   cp=cp+1;
				   if(cp==3)
					{
					   //$("http://www.5icool.orgBtnRNp").hide();
					   cp=1;
					} 
				   $("#NP"+cp).show();
				   
					
				}
				
				function ShowLNp()
				{
					/*$("http://www.5icool.orgBtnRNp").show();
				   if(cp==1)
					{
						return;
					}*/
				   $("#NP"+cp).hide();
				   
				   cp=cp-1;
					if(cp==0)
					{
					   //$("http://www.5icool.orgBtnLNp").hide();
						cp=2;
					} 
					
				   $("#NP"+cp).show();
				   
				}
				 
				</SCRIPT>
		  </LI></UL>
