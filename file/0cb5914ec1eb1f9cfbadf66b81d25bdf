var count_ad=0;
function flash_ad(){	
    if(document.getElementById("slider")){
	var sliderImg=document.getElementById("slider").getElementsByTagName("a");
	var sliderImgLen=sliderImg.length;
	for(var j=0;j<sliderImgLen;j++){
		sliderImg[j].style.opacity=0;
		sliderImg[j].style.zIndex=1;
	}
	sliderImg[count_ad].style.opacity=1;
	sliderImg[count_ad].style.zIndex=10;
	count_ad++;
	if(count_ad==sliderImgLen){count_ad=0;}
    }    
}
setInterval("flash_ad()",3000);
</script>
