    <script type="text/javascript">

var Zus_Itm=null,Zus_Idx=-1,ZuInterval=0;

function ZusChange(idx){

	if(Zus_Itm==null) Zus_Itm=$GetByClassName($Get("#zus_items"),"itm");

	if(idx>=Zus_Itm.length) idx=0;	Zus_Idx=idx; var m="";

	for(var i=0;i<Zus_Itm.length;i++){

		Zus_Itm[i].style.display=Zus_Idx==i?"block":"none";

		m+="<a "+(Zus_Idx==i?"class='ok'":"")+" href='javascript:void(0)' onclick='ZusChange("+i+")'>"+(i+1)+"</a>";

	}$Get("#zus_btn").innerHTML=m;

}

function ZusStart(){if(ZuInterval==0)ZuInterval=setInterval(function (){	ZusChange(Zus_Idx+1);	},5000);}

function ZusStop(){if(ZuInterval!=0){clearInterval(ZuInterval);ZuInterval=0;}}

ZusStart();ZusChange(0);

</script> 
