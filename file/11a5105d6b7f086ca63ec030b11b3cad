	<script type="text/javascript">
		//	показ попапа для оплаты
		function showPayForm( $sType, $iElementId, $iIblockId ){
			$.fancybox( {
				type : 'ajax',
				href: "/personal/pay/",
				ajax : {
					type: "POST",
					data: "sType="+$sType+"&iElementId="+$iElementId+"&iIblockId="+$iIblockId
				},
				height: "auto",
				showCloseButton: true,
				padding: 10
			} );
			return false;}
	</script>
	
	<script type="text/javascript">
		//	обновление селект с городом и регионом в фильтре
		nFilterUpdateSelect = {
			
			onChange: function( $sType, $this ){
				if( $sType!="region" && $sType!="city" ){
					return;
				}
				var $arrRequest = {
					"sType": $sType,
					"iElementId": false,
					"value": $this.value,
					
					"bGEOIsAjax": "ajax"
				};
				
				$.post(
					"",
					$arrRequest,
					nFilterUpdateSelect.onResponse,
					"html"
				);
				
				nFilterUpdateSelect.disable();
			},
			
			disable: function(){
				$("#filter_BASE_REGION_input").attr( "disabled", "disabled" );
				$("#filter_BASE_CITY_input").attr( "disabled", "disabled" );
				$("#filter_BASE_DISTRICT_input").attr( "disabled", "disabled" );
				$("#filter_BASE_STREET_input").attr( "disabled", "disabled" );
			},
			unDisable: function(){
				$("#filter_BASE_REGION_input").removeAttr( "disabled" );
				$("#filter_BASE_CITY_input").removeAttr( "disabled" );
				$("#filter_BASE_DISTRICT_input").removeAttr( "disabled" );
				$("#filter_BASE_STREET_input").removeAttr( "disabled" );
			},
			
			
			
			onResponse: function( $arrData ){
				$arrData = eval("["+$arrData+"]");
				$arrData = $arrData[0];
				
				for( var $sType in $arrData.arrTypeToChange ){
					nFilterUpdateSelect.replaceSelect( $sType, $arrData.arrTypeToChange[$sType] );
				}
				
				nFilterUpdateSelect.unDisable();
			},
			
			replaceSelect: function( $sType, $arrData ){
				if( $sType=="city" ){
					var $sId = "filter_BASE_CITY_input";
				} else if( $sType=="district" ){
					var $sId = "filter_BASE_DISTRICT_input";
				} else {
					var $sId = "filter_BASE_STREET_input";
				}
				
				var $sHtml = "<option value=''></option>";
				
				for( var $iId in $arrData ){
					var $option = $arrData[$iId];
					var $sName = ( $sType=="city" ? $option["NAME"] : $option );
					
					$sHtml += "<option value='"+$iId+"'>"+$sName+"</option>";
				}
				
				if( document.getElementById($sId) ){
					document.getElementById($sId).innerHTML = $sHtml;
				}
			},
			
			
			end: true
		}
	</script>

