           (function() {
              if(window.name==""){ 
                  window.name = 0; 
              } else{ 
                  window.name = eval(window.name) + 1; 
              } 
             if(window.name > 0)
             {
                document.getElementById("li_news1").style.display="none";
                document.getElementById("li_news2").style.display="";
             }else
             {
                document.getElementById("li_news1").style.display="";
                document.getElementById("li_news2").style.display="none";
             }
           })();
