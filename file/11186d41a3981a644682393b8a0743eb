<script type="text/javascript">
$(".app_close").click(function(){
	$(".brand_app_btm").hide();
});

$(function(){
	if(window.location.href.indexOf("noapp=true") == -1){
		checkHasApp();
	}
});
//获取当前窗口地址中的参数值
function getUrlValue(qs){
	var s = location.href;
	s = s.replace("?", "?&").split("&");
	var re = "";
	for (i = 0; i < s.length; i++) {
		if (s[i].indexOf(qs + "=") == 0) {
			re = s[i].replace(qs + "=", "");
			break
		}
	}
	return re;
}
function checkHasApp(){
	hmsr = getUrlValue('hmsr');
	hmpl = getUrlValue('hmpl');
	hmcu = getUrlValue('hmcu');
	send_id = getUrlValue('send_id');
	window.location.href="egouapp://?type=sms&title=&url=http:/m.egou.com?page=egouapp://wode&hmsr="+hmsr+"&hmpl="+hmpl+"&hmcu="+hmcu+"&send_id="+send_id+"";

}
