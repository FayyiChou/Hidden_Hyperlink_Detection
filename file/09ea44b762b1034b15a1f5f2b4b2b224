<script language="javascript">
var x=50,y=60;   //广告初始位置

var xin=true,yin=true; //xin为真，则向右运动，否则向左
var step=1; //移动距离
var delay=10;  //移动时间间隔

function floatAd()
{
var ad=document.getElementById('Layer1');
var L=T=0; //L左边界,T上边界
var R=document.body.clientWidth-ad.clientWidth;  //层移动的右边界
var B=document.body.clientHeight-ad.clientHeight;//层移动的下边界

ad.style.left=x+document.body.scrollLeft;
ad.style.top=y+document.body.scrollTop;

x=x+step*(xin?1:-1);
if(x<L)
{
xin=true;x=L;
}
if(x>R)
{
xin=false;x=R;
}

y=y+step*(yin?1:-1)
if(y<T)
{
yin=true;y=T;
}
if(y>B)
{
yin=false;y=B;
}

ad.onmouseover=function(){clearInterval(a)}
ad.onmouseout=function(){a=setInterval("floatAd()",10)}
}
var a=setInterval("floatAd()",10);

var initTop=0;
//var closeTop=0;
function init()
{
initTop=document.getElementById('Layer1').style.pixelTop;
document.getElementById('Layer2').style.pixelRight=document.getElementById('Layer1').style.pixelRight+294;
//closeTop=document.getElementById('Layer2').style.pixelTop;
}
function move()
{
document.getElementById('Layer1').style.pixelTop=initTop+document.body.scrollTop;
//document.getElementById('Layer2').style.pixelTop=closeTop+document.body.scrollTop;
}
function closeWindow()
{
document.getElementById('Layer2').style.display="none";
document.getElementById('Layer1').style.display="none";
}
window.onscroll=move;

</script>

