		　　	return str.replace(/(^\s*)|(\s*$)/g, "");
		　　}			String.prototype.replaceAll = function(s1,s2) { 				return this.replace(new RegExp(s1,"gm"),s2); 			}
			function dosearching(){
				var soval = document.getElementById("so_val");
				if($trim(soval.value).length != 0){
					location.href="/gallery--n%2C" + encodeURIComponent(soval.value).replaceAll("-","%20") + "-0--1--grid.html";
				}else{
					soval.value = "请输入搜索关键词";
				}
			}
