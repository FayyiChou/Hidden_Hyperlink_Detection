			 if(null !== list && '' !== list){
				 list = eval('(' + list + ')'); 
		        }
			 var availableResult=[];//筛选后的结果集
			 for(var i=0;i<conditionDiv.find("span").length;i++){
				 var select = conditionDiv.find("span").eq(i).data("select");
				 var val = conditionDiv.find("span").eq(i).data("val");
				 if(select == 0){
					 condition0.push(val);
				 }else if (select == 1){
					 condition1.push(val);
				 }else if (select == 2){
					 condition2.push(val); 
				 }else if (select == 3){
					 condition3.push(val);
				 }else if(select == 4){
					 condition4.push(val);
				 }else if(select == 5){
					 if(conditionDiv.find("span").eq(i).hasClass("JS_arrival")){
						 condition6.push(val);
					 } else {
						 condition5.push(val); 
					 }
				 }else if(select == 6){
					 condition6.push(val);
				 }
			 }
			 if(condition0.length==0&&condition1.length==0&&condition2.length==0&&condition3.length==0&&condition4.length==0&&condition5.length==0&&condition6.length==0){//没有筛选条件被选中
				 availableResult = list;
			 	return availableResult;
			 }
			var length = list.length;
			for(var i=0;i<length;i++){
				//0,起飞时段
				//1,到达时段
				//2,航空公司
				//3,舱位级别
				//4,计划机型
				//5,起飞机场
				//6,到达机场
				var flag= true;//用于标识某一个航班是否有效
				if(condition0.length!=0 && !condition0.contains(list[i].departureTimeSegment)){//起飞时断
					flag= false;
				}
				if(condition1.length!=0 && !condition1.contains(list[i].arrivalTimeSegment)){//到达时段
					flag= false;
				}
				if(condition2.length!=0 && !condition2.contains(list[i].carrierCode)){//航空公司
					flag= false;
				}
				if(condition4.length!=0 &&!condition4.contains(list[i].airplaneModel.airplaneType)){//计划机型
					flag= false;
				}
				if(condition5.length!=0 &&!condition5.contains(list[i].departureAirportCode)){//起飞机场
					flag= false;
				}
				if(condition6.length!=0 &&!condition6.contains(list[i].arrivalAirportCode)){//到达机场
					flag= false;
				}
				if(flag){
					availableResult.push(list[i]);
				}
				//stringVar.tolocaleLowerCase()
			}
			var availableResults=[];
			if(condition3.length !=0){
				for(var i=0;i<availableResult.length;i++){
					if(!condition3.contains("Y")){
						availableResult[i].ySeats= [];
					}
					if(!condition3.contains("F")){
						availableResult[i].fSeats= [];
					}
					if(!condition3.contains("C")){
						availableResult[i].cSeats= [];
					}
					//国航
					var n =0;
					if(null != availableResult[i].caOfficalSeats && undefined != availableResult[i].caOfficalSeats){
						var caofficalseats = availableResult[i].caOfficalSeats;
						var selectcaseats = [];
						for(var m =0;m <caofficalseats.length;m++){
							var seatType=caofficalseats[m].seatClassType;
							if(condition3.contains(seatType)){
								selectcaseats.push(caofficalseats[m]);
							}
						}
						availableResult[i].caOfficalSeats=selectcaseats;
						n=availableResult[i].caOfficalSeats.length;
					}
					//国航end
					//深航
					var k =0;
					if(null != availableResult[i].zhOfficalSeats && undefined != availableResult[i].zhOfficalSeats){
						var zhOfficalSeats = availableResult[i].zhOfficalSeats;
						var selectzhseats = [];
						for(var m =0;m <zhOfficalSeats.length;m++){
							var seatType=zhOfficalSeats[m].seatClassType;
							if(condition3.contains(seatType)){
								selectzhseats.push(zhOfficalSeats[m]);
							}
						}
						availableResult[i].zhOfficalSeats=selectzhseats;
						k=availableResult[i].zhOfficalSeats.length;
					}
					//深航end
					//去除所有舱位都被设置为[]的航班
					if(!(availableResult[i].ySeats.length ==0 && availableResult[i].cSeats.length ==0 && availableResult[i].fSeats.length == 0 && n ==0 && k==0)){
						availableResults.push(availableResult[i]);
					}
					
				}
			} else {
				availableResults = availableResult;
			}
			return availableResults;
		}
