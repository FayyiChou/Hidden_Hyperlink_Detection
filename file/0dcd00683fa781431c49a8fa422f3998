<script type="text/javascript">
    Do( function(){
        $( "._menu_drop").hover( function(){
            var _self = $( this);

            _self.addClass( "down");
            _self.children('a').addClass( "opacity5");
        }, function(){
            var _self = $( this);

            _self.removeClass( "down");
            _self.children('a').removeClass( "opacity5");
        });
    });
</script>

<script type="text/javascript">
    Do(function(){
        //初始输入url
        var link_url = window.location.href;

        if(link_url.indexOf('/?is_web') != -1) {//首页
            $('.header ul.xl li').eq(0).find('a').addClass('on');
        } else if(link_url.indexOf('/app') != -1) {//马上用车or下载客户端
            $('.header ul.xl li').eq(3).find('a').addClass('on');
        } else if(link_url.indexOf('/help') != -1) {//帮助中心
            $('.header ul.xl li').eq(2).find('a').addClass('on');
        } else {
            $('.header ul.xl li').eq(0).find('a').addClass('on');
        }

        //点击tab页
        $('.header ul.xl li').click(function() {
            $(this).find('a').addClass('on');
            $(this).siblings().find('a').removeClass('on');
        });

    });
    
</script>

