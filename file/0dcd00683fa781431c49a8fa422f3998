<script type="text/javascript">
    Do( function(){
        var $announcement_bg = $( "#announcement_bg"),
            $announcement = $( "#announcement"),
            $announcement_close = $( "#announcement_close"),
            $win = $( window);
            winWidth = $win.width(),
            elemWidth = $announcement_bg.width();

        if( $.trim( $announcement.find( "span").html()).length > 0){
            $announcement_bg.add( $announcement).css( {
                left: ( winWidth - elemWidth) / 2,
                top: 216
            }).show();

            $announcement_close.click( function(){
                $announcement_bg.add( $announcement).hide();
            });
        }
    });
</script>
