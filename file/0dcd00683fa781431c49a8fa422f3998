<script type="text/javascript">
    Do( "j.styleSelect");
    Do( function(){
        $('.hdshow li').hover(function(){
            $(this).children('a.alink').addClass('focus');
        },function(){
            $(this).children('a.alink').removeClass('focus');
        });
        var bannerIndex = 0,
                $banner = $('div.top_banner'),
                $imgs = $banner.find('li'),
                $slide = $('div.dian a'),
                stopAnim = false;

        function timeoutRound(){
            if( !stopAnim){
                bannerShow();
            }
            window.setTimeout( timeoutRound, 4000);
        }
        function bannerShow(){

            $imgs.hide();
            $imgs.eq( bannerIndex).show();
            $slide.removeClass('on');
            $slide.eq( bannerIndex).addClass('on');

            bannerIndex = bannerIndex >= $imgs.length - 1 ? 0 : bannerIndex + 1;
        }

        window.setTimeout( timeoutRound, 2000);

        $slide.hover( function( index){

            stopAnim = true;
            bannerIndex = $( this).index();
            bannerShow();
        }, function(){
            stopAnim = false;
        });

        var selectCityElem = $( "._selectCity"),
                cityPYElem = $( "#city");

        Do( "c.yc.citys", "c.business.city", "c.common", function(){

            selectCityElem.ycBusinessCitys().bind( "business.city.change", function( event, city){
                cityPYElem.val( city.py);
                selectCityElem.find( ".activeSelect").html( "<em>$1</em>".yc_push( city.name));
                onCityChange( city);
            });

            var defaultCityData = GET_CITYS( cityPYElem.val());
            if( defaultCityData)
                selectCityElem.find( ".activeSelect").html( "<em>$1</em>".yc_push( defaultCityData.name));
        });


        var productSelect = $( "#product_type");

        function onCityChange( city){

            var py = ( city && city.py) || cityPYElem.val();

            $.getJSON( "/ajax/getProductByCity.php?city=" + py, function( json){

                productSelect.empty();
                var arr = [];
                $.each( ( json && json.productList) || {}, function( name, value){
                    arr.push( { key: parseInt( name, 10), value: value});
                });

                arr.sort( function( a, b){
                    return a.key >= b.key;
                });

                $.each( arr, function( index, obj){
                    productSelect.append( "<option value='" + obj.key + "'>" + obj.value + "</option>");
                });

                Do( "c.common", function(){
                    YCUtils.flushStyleSelect( productSelect);
                });
            });
        }

        onCityChange();

        Do('j.placeholder', function() {
            $(':text[placeholder]').placeholder();
            var timer = null,
                    last = 0;
            function showMessage(o, time) {
                if (timer) {
                    clearInterval(timer);
                }
                last = time;
                timer = setInterval(function() {
                            if (last == 0) {
                                o.val('免费下载');
                                clearInterval(timer);
                                return;
                            }
                            o.val(last + 's');
                            last--;
                        },
                        1000);
            }
            function showResultMessage(input, clsName, msg) {
                var popdiv = $("#J_msg_box");
                if (popdiv.length == 0) {
                    popdiv = $('<div id="J_msg_box" class="popdiv t-c"></div>');
                    popdiv.appendTo(document.body);
                }
                // popdiv.find( "._content").html( msg);
                $("#J_msg_box_next").hide();
                popdiv.html('<i class="icons '+clsName+' mr10 vm"></i><span class="vm _content">'+msg+'</span>');
                popdiv.show( 10).delay( 3000).fadeOut( 1000,function(){ $("#J_msg_box_next").show();});
            }
            $(".C_download").click(function(e) {
                if (last > 0) {
                    return;
                }
                var o = $(this),
                        input = o.parents("div").find("input.cellphone");
                if (!/^1[34568]\d{9}$/.test(input.val())) {
                    showResultMessage(input, 'icons-error-mini', '手机号码格式错误');
                } else {
                    $.ajax({
                        url: "/ajax/verify_code.php",
                        type: "POST",
                        data: {
                            cellphone: input.val(),
                            type: "send_link"
                        },
                        dataType: "JSON"
                    }).done(function(data) {
                        if (data.code == 200) {
                            showResultMessage(input, 'icons-success-mini', "已经将链接发送到您手机上");
                        } else {
                            if (data.code == 407) {
                                showMessage(o, data.result);
                            }
                            showResultMessage(input, "icons-success-mini", data.msg);
                        }
                    });
                }
            });
        });

      
        function startMarquee(){
            var $marg = $( "#marquee"),
                    stopMargAnimate = false;

            new Movement({
                marg: $marg,
                onMarquee: function( obj){
                    obj.easing.ver_animate( $marg, obj.dir, function() {
                        obj.finishFn();
                    }, { speed: "slow"});
                },
                timeout: 2000
            });

        }

        function startSpeek(){
            var $speek = $( "#speek"),
                    stopSpeekAnimate = false;

            new Movement({
                marg: $speek,
                onMarquee: function( obj){
                    obj.easing.ver_animate( $speek, obj.dir, function() {
                        obj.finishFn();
                    }, { speed: 1500});
                },
                timeout: 4000
            });

        }
        startSpeek();

    });


</script> 


<script type="text/javascript">
    Do(function(){
        checkCookie();

        function getCookie(c_name) {
            if(document.cookie.length>0) {
                var c_start = document.cookie.indexOf(c_name + "=");
                if(c_start != -1) { 
                    c_start = c_start + c_name.length + 1;
                    c_end = document.cookie.indexOf(";",c_start);
                    if (c_end==-1) {
                        c_end=document.cookie.length;
                    }
                    return unescape(document.cookie.substring(c_start,c_end));
                } 
            }
            return "";
        }

        function setCookie(c_name,value,expire_time) {
            document.cookie = c_name+ "=" +escape(value) + ((expire_time==null) ? "" : ";expires="+expire_time.toGMTString());
        }

        function checkCookie() {
            var xiaohao = getCookie('xiaohao');
            if (xiaohao!=null && xiaohao!="") {
                
            } else {
                $('.cookie').show();

                var end_time = new Date();
                end_time.setHours(23);
                end_time.setMinutes(59);
                end_time.setSeconds(59);
                end_time.setMilliseconds(999);
                var expire_time = new Date(end_time.getTime());

                setCookie('xiaohao','treat me, call me',expire_time);
            }
        }

        //点击关闭按钮关闭浮层
        $('.cross_icon').click(function(){
            $('.cookie').hide();
        });

    })

</script> 




