<SCRIPT language=javascript type=text/javascript>
	//var my_glider = new Glider('my-glider', {duration:0.5});
		//首页媒体报道的重新排版-选项卡
	$(".yindex_newtabcont dl").hover(function(){
		$(this).addClass("cur");
		},function(){
			$(this).removeClass("cur");
		});
	$(".yindex_newtab dl.dlmenu dt span").eq(0).addClass("cur");
	$(".yindex_newtabwrap .yindex_newtabcont").eq(0).show();
	$(".yindex_newtab dl.dlmenu dt span").click(function(){
		var i=$(this).index();
		$(this).addClass("cur").siblings().removeClass("cur");
		$(".yindex_newtabwrap .yindex_newtabcont").eq(i).show().siblings().hide();
		if(i == 0) {
			$("#media_href").attr("href","/dynamics.html?newsType=1");
		} else if(i == 1) {
			$("#media_href").attr("href","/mediaList.html");
		}
	});	
		
	$(".yindex_notice_menu span").eq(0).addClass("cur");
	$(".yindex_notice_tabwrap .yindex_notice_tabcont").eq(0).show();
	$(".yindex_notice_menu span").click(function(){
		var i=$(this).index();
		$(this).addClass("cur").siblings().removeClass("cur");
		$(".yindex_notice_tabwrap .yindex_notice_tabcont").eq(i).show().siblings().hide();
	});	
</SCRIPT>



