<script type="text/javascript">

$(document).ready(function(){
	var frontguidePage = "false";
	if(frontguidePage == "true")
	{
		var d = new Date().getDate();
		frontguidePage = getCookie("frontguidePage");

		if(frontguidePage == null || d != frontguidePage)
		{
			setCookieWithTimeRange("frontguidePage", d, 24*60*60*1000);
			$(function(){
				$(".focWrapper").show();
			});
		}
	}
	
	
});
</script>
