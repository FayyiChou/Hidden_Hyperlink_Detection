<script type="text/javascript">
 $(document).ready(function() {
 	$(".phoneli").hover(function(e) {//控制二维码显示
 		var _offset=$(".phoneli").offset();
 		var rtop=_offset.top+45;
 		var rleft=_offset.left-80;
 		$(".rcode").offset({left:rleft,top:rtop});
 		$(".rcode").css({left:rleft+"px",top:rtop+"px"});
 		$(".rcode").show();
 	}, function() {
 		$(".rcode").hide();
 	});
 	
 });
</script>
<script type="text/javascript">
$(document).ready(function() {
	//回到顶部
	$(function() {
		$(window).scroll(function(){
			var scrolltop=$(this).scrollTop();		
			if(scrolltop>=350){		
				$("#btopId").fadeIn('slow');
			}else{
				$("#btopId").fadeOut('slow');
			}
		});		
		$("#btn_up_page").click(function(){
			$("html,body").animate({scrollTop: 0}, 500);	
		});
	});
	
	//顶部显控二维码
	$(".throughqr").hover(function() {
		$(this).addClass('qruerr');

	}, function() {
		$(this).removeClass('qruerr');
	});
	
	//右边浮窗
	$(".float-fd>li").hover(function() {
		$(this).addClass('enable');

	}, function() {
		$(this).removeClass('enable');
	});
	
	$(".fillForm a").click(function(){
		var index = $(".fillForm a").index($(this));
		$(this).siblings().removeClass("fourr"); 
        $(this).addClass("fourr");
		
	});
	
	//浮窗关闭按钮
	$(".close_btn").click(function(){
		if($(".close_btn").hasClass("cur02")){
			$(".close_btn").removeClass("cur02");
			$(".float-fd").removeClass("cur");
			$(".float-fd").animate({"width":"124px"},500);
			$(".float-fd li span.thetitle").animate({"width":"57px"},200);
		}
		else{
	
			$(".close_btn").addClass("cur02");
			$(".float-fd").addClass("cur");
			$(".float-fd").animate({"width":"39px"},200);
			$(".float-fd li span.thetitle").animate({"width":"0px"},200);
			
		}
		
	});

	
});
	function overview(){
		window.location.href="overview.html";
	}
	function mySmartAccount(){
		window.location.href="mySmartAccount.html";
	}
	function queryMyInvest(){
		window.location.href="queryMyInvest.html";
	}
	function queryAccountDetails(){
		window.location.href="queryAccountDetails.do";
	}
	function outp(){
		window.location.href="logout.html";
	}
	
	function queryInviteIndex(){
		window.location.href="queryInviteIndex.do";
	};
	
</script>


