<script type="text/javascript"> 
function displaySubMenu(li) { 
var subMenu = li.getElementsByTagName("ul")[0]; 
subMenu.style.display = "block"; 
$(".navigationA").css({"background":"url(../static/img/account_hover.png?v=1998f6&v=e246ea&v=3817&v=212) no-repeat"});
} 
function hideSubMenu(li) { 
var subMenu = li.getElementsByTagName("ul")[0]; 
subMenu.style.display = "none"; 
$(".navigationA").css({"background":"url(../static/img/account.png?v=60ccb3&v=3736) no-repeat"});
} 


