<script type="text/javascript">
var timeList = new Array();
var InterValObj;

function CountDownHome(index,remainTime){
	this.index = index;
	this.remainTime = remainTime;
}
	
$(function(){
	var nowDate = new Date("2016/09/20 19:17:26");
	$(".remainTimeBox").each(function(){
		var index = $(this).attr("index");
		var tenderTimeStr = $(this).attr("tenderTime");
		var tenderTime = new Date(Date.parse(tenderTimeStr));
		var remainTime = Math.round((tenderTime.getTime()-nowDate.getTime())/1000);
		if(remainTime>0){
			timeList.push(new CountDownHome(index,remainTime));
		}
	});
	if(timeList.length>0){
		InterValObj = window.setInterval(startCountDown, 1000);
	}
	
	$(".Actab").click(function(){
		window.location.href="frontSmart.html";
	});
});
	
function startCountDown(){
	if(timeList.length>0){
		var i;
		for(i=0;i<timeList.length;i++){
			var showObj = "#remainTimeBox"+timeList[i].index;
			var remainTime = timeList[i].remainTime;
			showRemainTime(showObj,remainTime,i);
		}
	} else {
		window.clearInterval(InterValObj); 
	}
}
	
function showRemainTime(showObj,remainTime,index){
	if (remainTime >0) {
		timeList[index].remainTime = remainTime - 1;
		if(remainTime==0) window.location.href = "/";
		var second = Math.floor(remainTime % 60); 
		var minite = Math.floor((remainTime / 60) % 60); 
		var hour = Math.floor((remainTime / 60) / 60);
		if(second<10)second="0"+second;
		if(minite<10)minite="0"+minite;
		if(hour<10)hour="0"+hour;
		var times = "<u class='time'>"+"<span>投资倒计时</span>" +"<span class='daojishi'>"+hour+"："+minite+ "："+second + "</span>"+"</u>";
		$(showObj).html(times);
	}else{
		$(showObj).html("");
		timeList.splice(index,1);
		window.location.href="/";
	}
}
</script><link href="/static/css/arrow.css?v=5cea52" rel="stylesheet" type="text/css" />
