    /** 理财金计划计算器界面产品类型下拉框onchange事件
        @param 选中的产品类型Id
      */
  	var inverest=new Array(3);
  	inverest[0]=["5.0%","5.5%","6.0%"];
  	inverest[1]=["6.0%","6.5%","7.0%","7.5%","8.0%","8.5%"];
  	inverest[2]=["9.0%","10.0%","11.0%"];
    function changeYearlyRateList(){
    	var moneyMgmtPlanType = document.getElementById("moneyMgmtPlanType");
		var yearlyRateSelct = document.getElementById("yearlyRateSelct");
		var rateTypeInverest = inverest[moneyMgmtPlanType.selectedIndex-1];
		yearlyRateSelct.length=1;
		for(var i =0 ;i<rateTypeInverest.length; i++){
			yearlyRateSelct[i+1]=new Option(rateTypeInverest[i],rateTypeInverest[i]);
		}
    }
    
    function resetMoneyMgmtPlan(){
    	$("#buyAmount").val("");
    	$("#moneyMgmtPlanType").val("0");
    	$("#yearlyRateSelct").val("0");
    	$("#resultRtn").html("0.00元");
    }
   
	function calculateReturn() {
		var type = $("#totalselect01 .pitchin").attr("selectid");
		var buyAmount = $("#buyAmount").val();
		var yearlyRate = $(".selectCountAot .pitchin2").attr("selectid");
		
		if(buyAmount=='' || isNaN(buyAmount) || yearlyRate=='' || isNaN(yearlyRate)){
	       // alert('请输入有效买入金额');
	        return ;
	    }
		buyAmount = parseFloat(buyAmount);	
		yearlyRate=parseFloat(yearlyRate);
		var dailyRtn = buyAmount*yearlyRate/(100*365);
		dailyRtn=dailyRtn.toFixed(2);
		var rtn;
		if (type == 2) {
              rtn=dailyRtn*7;
		} else if (type == 3) {
			rtn=dailyRtn*30;
		} else if (type == 4) {
			rtn=dailyRtn*365;
		}
		rtn=rtn.toFixed(2);
		$("#resultRtn").html(rtn+"元");
	}

		function radio() {
			var isradio = $('input:radio[name="isDayThe"]:checked').val();
			if (isradio == 0) {
				$("#investTime").removeClass("yue");
				$("#investTime").addClass("today");
			} else {
				$("#investTime").removeClass("today");
				$("#investTime").addClass("yue");
			}
		}

		function cacInterest() {
			var paymentMode = $("#totalselect02 .pitchin3").attr("selectid");
			var deadline = $(".caea  #investTime").val();
			var yearRate = $(".caea #annualRate").val();
			var amount = $(".caea #investAmount").val();

			var isDayThe = $(".caea .fourr").attr("data-value");
			var realAmount = amount;
			var totalInterest = 0;
			var monthRate = (yearRate * 0.01) / 12;
			var payAmount = 0;
			var transAmtReg = /^[1-9]\d*$/;
			if (amount == "100的倍数" || amount == "") {
				showTip();
				$(".caea #interestHeader").html("0.00元");
				return;
			} else if (!transAmtReg.test(amount)) {
				showTip("投资金额应为正整数");
				return;
			}

			if (paymentMode == '1') {//按月付息,到期还本
				monPayRate = monthRate;
				if (isDayThe == 1) { // 月标
					totalInterest = monPayRate * deadline;
					totalInterest = totalInterest * (realAmount);
				} else { // 天标
					totalInterest = (monPayRate * deadline * realAmount) / 30;
				}
			} else if (paymentMode == '2') {//一次性还款
				monPayRate = monthRate;
				if (isDayThe == '1') {//月标
					totalInterest = monPayRate * deadline;
					totalInterest = totalInterest * realAmount;
				} else {//天标
					totalInterest = (monPayRate * realAmount) * deadline / 30;
				}
			}
			totalInterest = Math.round(totalInterest * 100) / 100;
			if (isNaN(totalInterest)) {
				$(".caea  #interestHeader").html(0.00 + "元");
			} else {
				$(".caea  #interestHeader").html(totalInterest + "元");
			}

			showTip();
		}

		function transformToInteger(source) {
			source.value = source.value.replace(/\D/g, '');
			source.value = source.value.replace(/^[0][0123456789]$/, '');
		}
		function transform(source) {
			source.value = source.value.replace(/[^\d.]/g,"");  //清除“数字”和“.”以外的字符
		    source.value = source.value.replace(/^\./g,"");  //验证第一个字符是数字而不是.
		    source.value = source.value.replace(/\.{2,}/g,"."); //只保留第一个. 清除多余的.
		}

		function showTip(info, css) {
			if (info == null) {
				$("#tip-content").html("");
				$(".tip-wrapper").hide();
				$(".tip-wrapper").removeClass("tip-for-code");
			} else {
				$("#tip-content").html(info);
				$(".tip-wrapper").show();
				if (css != null) {
					$(".tip-wrapper").addClass("tip-for-code");
				}
			}
		};

		//重置
		function resets() {
			$("#investAmount").val("");
			$("#investTime").val("");
			$("#annualRate").val("");
			$("#amount").val("");
			$("#interestHeader").html("0.00元");
		};
		
		$("#totalselect01 .totalopt li").click(function(){
			var index = $("#totalselect01 .totalopt li").index($(this));
			$(this).siblings().removeClass("pitchin"); 
	        $(this).addClass("pitchin");
			$(".selectCountAot").hide();
			$(".selectCountAot").eq(index).show();
			
			$(".selectCountAot .totalopt li").removeClass("pitchin2"); 
			$(".selectCountAot .seltot").html("请选择年化利率");	
		});
		
		$(".selectCountAot .totalopt li").click(function(){
			var index = $(".selectCountAot .totalopt li").index($(this));
			$(this).siblings().removeClass("pitchin2"); 
	        $(this).addClass("pitchin2");
		});
		
	 	$("#totalselect02 .totalopt li").click(function(){
			var index = $("#totalselect02 .totalopt li").index($(this));
			$(this).siblings().removeClass("pitchin3"); 
	        $(this).addClass("pitchin3");
		});
	</script>
	


