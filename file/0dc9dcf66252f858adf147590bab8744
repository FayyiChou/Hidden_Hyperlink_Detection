<script type="text/javascript">
var date1=new Date("2014/06/04");  //开始时间
var date2=new Date();    //结束时间
var date3=date2.getTime()-date1.getTime();  //时间差的毫秒数

//计算出相差天数
var days=Math.floor(date3/(24*3600*1000));
$(".logoNum").html(days);

	settings.userId='';
	
	$(".login_password").keyup(function(e) {
		if(e.keyCode== 13){
			$("#btn_login").click();
		}
	});
</script>
<script type="text/javascript">




/** 添加理财金计划计算器开始 */
$(function(){
	
	$(".calc_menu a").click(function(){
		var index = $(".calc_menu a").index($(this));
		$(this).siblings().removeClass("active"); 
        $(this).addClass("active");
		$(".calc_item").hide();
		$(".calc_item").eq(index).show();
		
	});
		
	
		$(".buy_je_inp").focus(function(){
			$(".buy_je_inp").val("");
		});
	
	
	
	$(".sy_tab_wrap").find(".sy_tabcont").eq(0).show();
	$(".y_ll").find("p").eq(0).show();
	
	
	
	$("#y_se").change(function(){
		var y_se_op= $(this).val();
		if(y_se_op == 0){
			$(".y_ll p").eq(y_se_op).show().siblings().hide();
			$(".sy_tab_wrap .sy_tabcont").eq(y_se_op).show().siblings().hide();
			
		}
		else if(y_se_op == 1)
		{
			$(".y_ll p").eq(y_se_op).show().siblings().hide();
			$(".sy_tab_wrap .sy_tabcont").eq(y_se_op).show().siblings().hide();
		}
		else
		{
			$(".y_ll p").eq(2).show().siblings().hide();
			$(".sy_tab_wrap .sy_tabcont").eq(2).show().siblings().hide();
		}
		
		$(".buy_je_inp").val("您买入的金额");
	});
	
	$(".cz_btn").click(function(){
		$(".buy_je_inp").val("");	
		$(".sy_tabcont p span").html("0.00");
		
	});
	
		
});
/** 添加理财金计划计算器结束 */

(function($){ 
	$.fn.hoverDelay = function(options){ 
	var defaults = { 
	hoverDuring: 300, 
	outDuring: 300, 
	hoverEvent: function(){ 
		$.noop(); 
	}, 
	outEvent: function(){ 
		$.noop(); 
	} 
	}; 
	var sets = $.extend(defaults,options || {}); 
	var hoverTimer, outTimer; 
	return $(this).each(function(){ 
	$(this).hover(function(){ 
		clearTimeout(outTimer); 
		hoverTimer = setTimeout(sets.hoverEvent, sets.hoverDuring); 
	},function(){ 
		clearTimeout(hoverTimer); 
		outTimer = setTimeout(sets.outEvent, sets.outDuring); 
	}); 
	}); 
	} 
})(jQuery); 

var hrefValue = window.location.href;
hrefValue = String(hrefValue);

if(hrefValue.indexOf("www.youyadai.com")>=0){
	window.location.href="/static/html/interval.html";
}

