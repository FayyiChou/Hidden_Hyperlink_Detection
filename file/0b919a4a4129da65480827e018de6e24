var errorMsg='';
function killErrors(){
    var arglen=arguments.length;   
	errorMsg="错误："+arglen+"个\n";   
    for(var i=0;i<arglen;i++){   
    errorMsg+="错误"+(i+1)+"："+arguments[i]+"\n";
	}
	window.onerror=null;   
	return true;
}
window.onerror = killErrors;
