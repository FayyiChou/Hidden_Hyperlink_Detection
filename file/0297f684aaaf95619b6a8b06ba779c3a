<script language="JavaScript" type="text/javascript">

var headerSearchbar = {

      init : function(isSearchBarDisplayed)
      {
        if (($("#collapse_searchbar").length==0) || ($("#btn_search").length==0) || ($("#search_plus_minus").length==0)){
          return false;
        }else{
          headerSearchbar.collapseSearchBar = document.getElementById("collapse_searchbar");
          headerSearchbar.btnSearch = document.getElementById("btn_search");
          headerSearchbar.searchPlusMinus = document.getElementById("search_plus_minus");
          headerSearchbar.initialized = true;
        }
        if (isSearchBarDisplayed){
          headerSearchbar.showSearchBar();
        }
      },
      collapseSearchBar: null,
      btnSearch : null,
      searchPlusMinus : null,
      isHeaderSearchbarVisible : false,
      initialized: false,

      changeHiddenProperty  : function(event)
      {
        event.preventDefault();
        if(headerSearchbar.searchPlusMinus.className.indexOf("fa-plus")!=-1)
        {
          headerSearchbar.showSearchBar();
        }
        else
        {
          headerSearchbar.hideSearchBar();
        };
        $.ajax({
          type: "POST",
          url: "scholaroneAJAXServlet?XIK_TAGACT=xik_24wJrao6stfSUCAwGYgSBVxqsnCanRVspQUNCVmZ96vYZ4zXaG38HBSgGHuZc6zi3S",
          data:{isVisible: headerSearchbar.isHeaderSearchbarVisible},
          success: function(data){
            //nothing
          }
        });
      },

      showSearchBar : function()
      {
        headerSearchbar.collapseSearchBar.style.overflow = "visible";
        headerSearchbar.btnSearch.style.display = "inline-block";
        headerSearchbar.collapseSearchBar.style.height = "auto";
        headerSearchbar.searchPlusMinus.className = "fa fa-minus plus-minus";
        headerSearchbar.isHeaderSearchbarVisible = true;
      },

      hideSearchBar : function()
      {
        headerSearchbar.collapseSearchBar.style.overflow = "hidden";
        headerSearchbar.btnSearch.style.display = "none";
        headerSearchbar.searchPlusMinus.className = "fa fa-plus plus-minus";
        headerSearchbar.collapseSearchBar.style.height = "0px";
        headerSearchbar.isHeaderSearchbarVisible = false;
      }
}

$("#headerSearchbar").on('click', function(event){
  if (headerSearchbar.initialized===true){
    headerSearchbar.changeHiddenProperty(event);
  }
});

headerSearchbar.init(false);

</script>

      



