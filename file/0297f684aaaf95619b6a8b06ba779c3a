<script language="JavaScript" type="text/javascript">
function handleContainer()
{
  var btnMedia = document.getElementById("btn-media");
  var topHeaderLine = document.getElementById("top-header-line");
  var logInBreadcrumbs = document.getElementById("login-breadcrumbs");
  var searchBox=document.getElementById("search-box");
  searchBox.style.height="50px";
  searchBox.style.overflow="visible";
  if(topHeaderLine.style.height=="0px")
  {
    if(logInBreadcrumbs!=null)
    {
      logInBreadcrumbs.style.height="auto";
    }
    topHeaderLine.style.height = "auto";
    btnMedia.className = "btn btn-navbar";
  }
  else
  {
    if(logInBreadcrumbs!=null)
    {
      logInBreadcrumbs.style.height="0px";
    }
    topHeaderLine.style.height = "0px";
    btnMedia.className = "btn btn-navbar collapsed";
  }
}
</script>
