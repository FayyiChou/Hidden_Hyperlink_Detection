<script>
$(function() {
  
  $(window).load(function () { //全ての読み込みが完了したら実行
  $(".box1").animate({left: '10px', opacity: '1'},'4000');
  $(".box2").animate({left: '15px', opacity: '1'},'6000');
  $(".box3").animate({left: '20px', opacity: '1'},'8000');
  $(".box4").animate({top: '30px', opacity: '1'},'14000');
  });
  
  $(window).scroll(function () { //スクロールで実行
    var top = $(this).scrollTop();
    if(top > 2900) {
      $(".box6").animate({opacity: '1', top: '30px'},'3000');
    }
    if(top > 2940) {
      $(".box7").animate({opacity: '1', top: '30px'},'3000');
    }
    if(top > 2980) {
      $(".box8").animate({opacity: '1', top: '30px'},'3000');
    }
    if(top > 3500) {
      $(".box9").animate({opacity: '1', top: '30px'},'3000');
    }
    if(top > 3530) {
      $(".box10").animate({opacity: '1', top: '30px'},'3000');
    }
    if(top > 3560) {
      $(".box11").animate({opacity: '1', top: '30px'},'3000');
    }
    if(top > 3590) {
      $(".box12").animate({opacity: '1', top: '30px'},'3000');
    }

    
  });
});
</script>


<script src="http://www.xmobile-tw.com/js/tabbox.js"></script>

<script>
	$(function(){
  $('.box5').css("opacity",".0");
  $(window).scroll(function (){
    $('.box5').each(function(){
      var imgPos = $(this).offset().top;    
      var scroll = $(window).scrollTop();
      var windowHeight = $(window).height();
      if (scroll > imgPos - windowHeight + 200){
        $(this).animate({ 
          "top": "5px",
          "opacity": "1"
        }, 300);
      }
    });
  });
});
</script>
<script>
	$(function(){
  $('.mid_h1').css("opacity",".0");
  $(window).scroll(function (){
    $('.mid_h1').each(function(){
      var imgPos = $(this).offset().top;    
      var scroll = $(window).scrollTop();
      var windowHeight = $(window).height();
      if (scroll > imgPos - windowHeight + 200){
        $(this).animate({ 
          "top": "30px",
          "opacity": "1"
        }, 800);
      }
    });
  });
});
</script>
