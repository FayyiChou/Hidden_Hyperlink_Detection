    //广告换一批
    $(function () {
        $("#change").click(function () {
            var _this = $(this);
            _this.addClass("on");
            setTimeout(function () {
                $.ajax({
                    url: '/Adv/DivShortRangeList',
                    type: 'post',
                    data: { type: '1' },
                success: function (data) {
                    _this.removeClass("on");
                    if (data != 'undefind' && data != '') {
                        $(".ad-list").html(data);
                    }
                }
                });
            },500)
        });
    });
