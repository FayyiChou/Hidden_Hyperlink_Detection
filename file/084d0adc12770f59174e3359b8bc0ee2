  <script type="text/javascript">
        $(".back").removeAttr("onclick");//移除返回按钮的click事件。因为地下已经定义了返回事件
        function fback() {
            if (window.history.length > 1) {
                window.history.back();
            }
            else {
                window.location.href = "/cnews/";
            }
        }
        //微信自定义分享声明
        wxJSAPI.Config({
            fx_url: 'http://m.rz.ccoo.cn/cnews/shareInfo.aspx?id=5808261',
            fx_title: '这条微信专属农村人，一般城里人是看不懂滴！',
            fx_memo: '我们不做信息搬运工，我们是日照微信原创车间！大众网日照频道新...',
            fx_pic: 'http://imgref.ccoo.cn/ajax/ImgRef.ashx?url=http://mmbiz.qpic.cn/mmbiz/TQ33WDZpQQEEdbSrgWqibz4n34eAw2T0JMKkv8MV5DicuA6nYUBtib72MLpewO2wTqvOGIib1JTe42yLkp1mwHg8XA/0',
            fx_success: function () {
                tipFun("分享成功~");
            },
            fx_fail: function () {
                tipFun("分享失败~");
            }
        });

        function GetRelatedNews() {
            var zwh = ' ';
            if (zwh==1) {
                var Did = '';
                $.ajax({
                    type: "post",
                    url: "/Zwh/GetRelatedNews",
                    dataType: 'text',
                    data: {Did:Did,id:'5808261'},
                    success: function (data) {
                        if (data!="") {
                            $("#RelatedNews").html(data);
                        } else {
                            $("#Zwhxiangguan").attr("style", "display:none;");
                        }
                    }
                });
            }
        }
    </script> 
  <script type="text/javascript">
    $(function() {
        //顶部点击
        $(".show-all-list").click(function() {
            $(".all-list-mask").toggle();
            $(".show-all-listcon").slideToggle();
            $("div.tie_menu").slideUp();
            if (!$('body').hasClass('noslide')) {
                $('body').addClass('noslide');
            } else {
                $('body').removeClass('noslide');
            }
        });
        function closeList() {
            $(".show-all-listcon").slideUp();
            $(".all-list-mask").hide();
            if ($('body').hasClass('noslide')) $('body').removeClass('noslide');
        }
        $(".all-list-mask").click(function() {
            closeList();
        });
        $(".show-all-listcon").find("li").click(function() {
            closeList();
        });
    });
    //点击按钮，消息框弹出
    $("#btn").bind("click", function () {
        $("div.slide_wrap").addClass("show");
        $("div.slide_nav").html("<div style='width:78%;height:30px;color:#fff;text-align:center;position:absolute;top:50%;left:0;margin-top:-15px;'>正在努力加载~</div>").load("/Index/UserInfo/");
        if ($(".post-wrap").is(":visible")) {
            $(".post-wrap").hide();
            $(".quick-p").removeClass("on");
        }
    });
    //点击空白处，消息框隐藏
    $("div.slide_wrap").click(function (e) {
        if (e.target.className == "slide_wrap show") {
            $(this).removeClass("show");
        }
    });
</script> 
