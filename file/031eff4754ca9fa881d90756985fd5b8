        function othera(){

            var display = $('#otherform').css('display');
            if(display=='none'){
                $('#otherform').css('display','block')
            }
            else{
                $('#otherform').css('display','none')
            }
        }
    </script>


    <script src="/Scripts/resultstatus.js"></script>
    <script>

        function errorModal(msg) {
            $('#errorModal').modal('show');
            $('#errorModalMsg').text(msg);
        }

        $(function () {
            $.ajaxSetup({
                contentType: "application/x-www-form-urlencoded;charset=utf-8",
                complete: function (XMLHttpRequest, textStatus) {
                    //通过XMLHttpRequest取得响应头，sessionstatus，
                    var sessionstatus = XMLHttpRequest.getResponseHeader("sessionstatus");
                    if (sessionstatus == "timeout") {
                        alert("登录超时，请重新登录");
                        //如果超时就处理 ，指定要跳转的页面
                        location.reload();
                    }
                }
            });

            $('#cb2_2').change(function () {
                if (document.getElementById('cb2_2').checked == true) {
                    $("#equDiv2").css('display', 'block');
                }
                else {
                    $("#equDiv2").css('display', 'none');
                }
            })

            var u = navigator.userAgent;
            var isAndroid = u.indexOf('Android') > -1 || u.indexOf('Adr') > -1; //android终端
            var isiOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/); //ios终端
            if (isAndroid == false && isiOS == false) {
                $('#mh').attr('src', '/contents/front/a7.html');
                $('#ifDiv').css('display', 'block');
            }

            $('a').tooltip('hide');

        })

        function join2() {
            var joinuser = $('#joinuser2').val();
            var joinphone = $('#joinphone2').val();
            var joinlab = $('#joinlab2').val();
            var joindepart = $('#joindepart2').val();
            var joinmail = $('#joinmail2').val();
            var joinequ = $('#joinequ2').val();

            $('#errorMsg2').css('display', 'none');

            if (document.getElementById('cb1_2').checked == true && document.getElementById('cb2_2').checked == false) {
                window.location.href = '/account/registration?nickname=' + joinuser + "&phonenum=" + joinphone + "&mail=" + joinmail;
                return;
            }

            if (joinuser == '') {
                $('#errorMsg2').text("必须输入姓名！");
                $('#errorMsg2').css('display', 'block');
                return;
            }
            if (joinphone == '') {
                $('#errorMsg2').text("必须输入联系号码！");
                $('#errorMsg2').css('display', 'block');
                return;
            }
            if (joinmail == '') {
                $('#errorMsg2').text("必须输入联系邮箱");
                $('#errorMsg2').css('display', 'block');
                return;
            }
            if (joinlab == '') {
                $('#errorMsg2').text("必须输入实验室");
                $('#errorMsg2').css('display', 'block');
                return;
            }
            if (joindepart == '') {
                $('#errorMsg2').text("必须输入单位");
                $('#errorMsg2').css('display', 'block');
                return;
            }


            if (document.getElementById('cb2_2').checked == true) {
                if (joinequ == "") {
                    $('#errorMsg2').text("勾选提供仪器后，请输入会提供共享的实验仪器");
                    $('#errorMsg2').css('display', 'block');
                    return;
                }
            }

            $('#joinbtn2').attr('disabled', 'disabled');
            $('#joinbtn2').text("正在提交...");
            $.post("/front/joinmail", { 'joinuser': joinuser, 'joinphone': joinphone, 'joinlab': joinlab, 'joindepart': joindepart, 'joinmail': joinmail, 'cb1': document.getElementById('cb1_2').checked, 'cb2': document.getElementById('cb2_2').checked, 'joinequ': joinequ }, function (data) {
                if (resultstatus(data.status, data.msg, '')) {
                    alert("信息提交成功：我们的工作人员会马上联系您！");
                    if (document.getElementById('cb1_2').checked == true) {
                        window.location.href = '/account/registration?nickname=' + joinuser + "&phonenum=" + joinphone + "&mail=" + joinmail;
                        return;
                    }
                    else {
                        location.reload();
                    }
                }
                else {
                    $('#errorMsg2').text(data.msg);
                    $('#errorMsg2').css('display', 'block');
                }
                $('#joinbtn2').removeAttr('disabled');
                $('#joinbtn2').text("马上加入联盟");
            }, 'json');
        }

    </script>


</body>
</html>
