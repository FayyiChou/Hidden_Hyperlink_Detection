	//职位举报用到的弹出框--start
	function ShareOrThirdPopup(selector, width){
		if(! (this instanceof ShareOrThirdPopup)){
			return new ShareOrThirdPopup(selector, width);
		}
		var pop = new PopupContentV2(selector, width);
		this.showPopup = function(data){
			pop.showPopup(data);
		};
	}
	//职位举报用到的弹出框 end
	
	
	jQuery("#report").click(function(){
		var pop = new ShareOrThirdPopup("#ui-job-report", 550);
		pop.showPopup();
	});
	//编辑公司弹出框--start
	jQuery(".j-editCompany").live('click',function(){
		var pop = new ShareOrThirdPopup("#ui-editCompany",550);
		pop.showPopup();
	});
	//编辑公司弹出框第一期--start
	
	jQuery("#ui-editCompany-submit").live('click',function(){
		var pop = new ShareOrThirdPopup("#ui-editCompanySuccess", 500);
		var popFail = new ShareOrThirdPopup("#ui-editCompanyFail",300);
		var companyID = "11037270";
		var adress = $('#ui-company-adress').val();
		var webside = $('#ui-company-webside').val();
		var phone = $('#ui-company-phone').val();
		var email = $('#ui-company-email').val();
		$.ajax( {
			type : 'post',
			url : '/process/',
			data : {
				formAction : "company_info_editmode",
				companyID : companyID,
				adress : adress,
				webside : webside,
				phone : phone,
				email : email
			},
			dataType : 'json',
			success : function(result) {
				if(result['result'] > 0){
					pop.showPopup();
				}else{
					popFail.showPopup();
					//$submit.tipsBox("failure", "请选择举报类型！");
					setTimeout(isClose, 3000);
				}
			}
		});
	});
	
	$('#ui-ensure').live('click',function(){
		jQuery("#ui-v2-pop-close").click();
	});
	//编辑公司弹出框 end
	
	//公司产品与服务 start
	$(function(){
		var $productCategory = $('.pdBox:gt(3)');
		$productCategory.hide();
		var productlist = $(".pdCase > div"),
	    	productLast = productlist.eq(-2),
	    	productSecond = productlist.eq(-3),
			$toggleBtn = $('.j-moreBtn'),
			pdNumber = $(".pdBox").length;
		if(pdNumber>4){
			$toggleBtn.show();
		}else{
			$toggleBtn.hide();
			productLast.removeClass('box');
	    	productSecond.removeClass('box');
		}
		$toggleBtn.click(function(){
			if($productCategory.is(":visible")){
			$productCategory.hide();
			$toggleBtn.removeClass('upBtn').addClass('downBtn');
		}else{
			$productCategory.show();
			$toggleBtn.removeClass('downBtn').addClass('upBtn');
		}
		return false;
		});
	});
	//公司产品与服务 end

	//编辑网店框 start
	$(".j-footIco li").mouseover(function(){
		var isShow = $(this).attr('data-show');
		if(isShow == 1 ){
			var $ico = $(this).children();
			var title = $ico.attr('title');
			var href = $ico.attr('data-web');
			var type = $ico.attr('data-type');
			var $tipsBox = $(this).children(".j-webTips");
			if(type != 'BD' && type != 'ZH'){
				if($tipsBox.length > 0){
					$tipsBox.show();
				}else{
					if(type=="WX"){
						var weChat = $("#weChat").val();
						if(weChat == true){
							var wechatURL = $("#wechatURL").val();
							var tipsHTML = '<span class="web-msg web-msg-WX j-webTips" style="z-index:20;width:140px;height:150px;text-align:center;">'+
							'<em class="sanjiao">♦</em>'+
							'<img src="'+wechatURL+'" alt="" style="width:129px;height:129px;" />'+
							'<div style="color:#fff;text-align:center;margin-top:3px;">打开微信扫一扫</div>'
						'</span>';
							$(this).append(tipsHTML);
						}else{
							var tipsHTML = '<span class="web-msg web-msg-WX j-webTips" style="z-index:20">'+
							'<em class="sanjiao">♦</em>'+
							'<span class="webName">'+title+'</span>'+
							'<a href="'+href+'" class="webLink" target="_blank" rel="nofollow">'+href+'</a>'+
							'</span>';
							$(this).append(tipsHTML);
							$(".web-msg-WX").children("a").attr("href","javascript:;").removeAttr("target").text("微信号："+href);
						}
					}else{
						var tipsHTML = '<span class="web-msg web-msg-'+type+' j-webTips">'+
						'<em class="sanjiao">♦</em>'+
						'<span class="webName">'+title+'</span>'+
						'<a href="'+href+'" class="webLink" target="_blank" rel="nofollow">'+href+'</a>'+
						'</span>';
						$(this).append(tipsHTML);
					}
					$tipsBox = $(this).children(".j-webTips");
					var icoLeft = $ico.position().left;
					var icoTop = $ico.position().top;
					var icoWidth = $ico.innerWidth();
					var tipsBoxWidth = $tipsBox.innerWidth();
					var left = icoLeft - tipsBoxWidth / 2 + icoWidth/2 + 5;
					var top = icoTop+45;
					var obj = {
					  'left':left,
					  'top':top
					};
					$tipsBox.css(obj).show();
				}
			}
		}
	}).mouseout(function(){
		$(this).children(".j-webTips").hide();
	})
	//编辑网店内容框 end	

		//弹出框--start
	function ShareOrThirdPopup(selector, width){
		if(! (this instanceof ShareOrThirdPopup)){
			return new ShareOrThirdPopup(selector, width);
		}

		var pop = new PopupContentV2(selector, width);
		
		this.showSharePopup = function(){
			pop.showPopup();
		};
	}
	
	function otherShow(){
		var jobNameVal = jQuery("#ui-company-salary-jobname").val();
		var $otherJob = jQuery("#ui-company-salary-otherJob");
		if(jobNameVal === "other"){
			$otherJob.show();
		}else{
			$otherJob.hide();
		}	
	}
	
	//弹出框--end
	(function(){
		var pop = new ShareOrThirdPopup("#ui-company-salary", 550);
		jQuery(".j-updateSalary").click(function(){
			var companyID = $("#companyID").val();
			$.ajax( {
				type : 'post',
				url : '/process/',
				data : {
					formAction : "company_salary_companyJobList",
					companyID : companyID
				},
				dataType : 'json',
				success : function(data) {
					if(data['result'] > 0){
						jQuery("#ui-company-salary-jobname").append(data['html']);
						pop.showSharePopup();
						otherShow();
					}
				}
			});
		});
	})();
	
	jQuery("#ui-company-salary-jobname").change(otherShow);
	
	//数字正则
	function checkslalrynum(slalrynum){
		var str = slalrynum;
		var reg = /^[0-9]*$/;
		var len = str.length;
		if(reg.test(str) && len>0){
			return true;
		}else{
			return false;
		}
	}
	
	jQuery('#ui-company-salary-num').blur(function(){
		var slalrynum = $(this).val();
		var slalrynumlen = slalrynum.length; 
		if(checkslalrynum(slalrynum) && slalrynumlen < 7){
			$("#ui-company-salary-num-tips").text("正确！").css("color","#999999");
		}else{
			$("#ui-company-salary-num-tips").text("请填写少于6位的数字！").css("color","red");
		}
	});
	//地区选择
	(function(){
		var provinceSelect = jQuery(".j-pro").Select();
		var citySelect = jQuery(".j-city").Select();
		provinceSelect.addSubscriber(citySelect);
		//初始化
		provinceSelect.initialize(areaEvenData);
		jQuery(".j-pro").change(function(){
			provinceSelect.change();
		});
	})();

	jQuery("#ui-company-radio").click( function(event){
		if(event.target.tagName.toLowerCase() === "input"){
			var $this = jQuery(this);
			var $checkRadio = $this.find("input:checked");
			var $leaveTime = jQuery("#ui-company-salary-leavetime");
			var value = $checkRadio.val();
			if(value === "N"){
				$leaveTime.show();
			}else{
				$leaveTime.hide();
			}
		}
	});

	//表单提交处理
	jQuery("#formsubmit").submit(function(){
		return false;
	});
	(function(){
		jQuery("#formsubmit-submit").click(function(){
			var $this = jQuery(this.form);
			var $submit = jQuery("#formsubmit-submit");
			var salary = jQuery('#ui-company-salary-num').val();
			var salarylen = salary.length;
			if(!checkslalrynum(salary) || salarylen > 7){
				$submit.tipsBox("failure", "薪酬格式错误！");
				return false;
			}
			var salaryNum = parseInt(salary,10);
			if( salaryNum < 1000 || salaryNum > 50000){
				$submit.tipsBox("failure", "请输入1000~50000之间的整数");
				return false;
			}
			var $otherJob = jQuery("#ui-company-salary-otherJob");
			if($otherJob.length > 0 && $otherJob.val() == $otherJob.attr("placeholder")){
				$submit.tipsBox("failure", "职位名称不可为空！");
				return false;
			}
			var workLength = jQuery("#ui-company-salary-workLength").val();
			var workLengthTips = jQuery("#ui-company-salary-tips");
			if(workLength == 0){
				workLengthTips.text("请选择你的工作经验").css({"color":"#C5262A"});
				$submit.tipsBox("failure", "请选择你的工作经验");
				return false;
			}else{
				workLengthTips.text("选择您在该公司工作时的工作经验(*必选)").css({"color":"#999999"});
			}
			$submit.attr("disabled", "disabled");
			$submit.attr("disabled", "disabled").tipsBox("loading", "正在提交数据...");
			jQuery.ajax({
				type : 'post',
				url : '/process/',
				data : $this.serialize(),
				dataType : 'json',
				success : function(data) {
					if( data['result'] >0 ){
						$submit.removeAttr("disabled").removeTipsBox().tipsBox("success", "提交成功",function(){
							jQuery("#ui-v2-pop-close").click();
						});
					}else if( data['result'] == -1 ){
						$submit.removeAttr("disabled").removeTipsBox().tipsBox("failure", "提交失败！");
					}else if( data['result'] == -2 ){
						$submit.removeAttr("disabled").removeTipsBox().tipsBox("failure", "薪酬格式错误！");
					}else if( data['result'] == -3 ){
						$submit.removeAttr("disabled").removeTipsBox().tipsBox("failure", "请填写小于6位的数字！");
					}else if( data['result'] == -4 ){
						$submit.removeAttr("disabled").removeTipsBox().tipsBox("failure", "职位名称不可为空！");
					}else if( data['result'] == -5 ){ 
						$submit.removeAttr("disabled").removeTipsBox().tipsBox("failure", "请选择您的工作经验！");
					}else if( data['result'] == -6 ){
						$submit.removeAttr("disabled").removeTipsBox().tipsBox("failure", "职位名称含有敏感词！");
					}
				}
			})
			return false;
		});
	})();

	/*管理团队加载更多 开始*/
	$(".j-team-more").live("click",function(){
		var companyID = 11037270;
		$.ajax({
			type: "post",
			url: "/process/",
			data: {
				companyID: companyID,
				formAction: "company_info_moreTeamList"
			},
			dataType: "json",
			success: function(result){
				if(result["result"] > 0){
					$(".j-teamList").children("li").remove();
					$(".j-teamList").append(result['html']);
					$(".j-team-more").remove();
				}
			}
		});
	});
	/*管理团队加载更多 结束*/
