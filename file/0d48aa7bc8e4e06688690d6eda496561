<script type="text/javascript">
	//查看联系方式 引导登录
	if(document.cookie.indexOf("isloginStatus") != -1){
		$(".j-shower1").show();
	}else{
		$(".j-shower2").show();
	}
	if($("#showRegCompanyInfo").length >= 1){
		$(".j-companyClaim").show();
	}
	(function(){
		var sa = parseInt($("#rating_simple1").val());
		$(".j-totalGrade").val(sa);
		if(sa==NaN){
			sa=3;			
		}
		$(".ui-company-tag-all a:lt("+sa+")").addClass('ui-company-tag-item-star-selected');
		jQuery(".ui-company-tag-all").reviewStar("a", "ui-company-tag-item-star-selected", "input[type=hidden]", "em[class$=item-tips]", {"1":"非常不满意", "2":"不满意", "3":"一般", "4":"满意", "5":"非常满意"});
	})();
	/*滚动跟随--start*/
		jQuery("#navTab").scrollTopVerticalFixed("scroll-shadow");
	/*滚动跟随 end*/
	/*职友集页面统计start*/
	var uiNowSite = "site_company";
	/*职友集页面统计end*/
	/*显示更多公司介绍 start*/
	(function(){
		var $textShowMore = jQuery("#textShowMore");
		var setLine = 6;
		var lineHeight = parseInt($textShowMore.css("line-height"),10);
		var currentHeight = $textShowMore.height();
		var $showMoreButton = jQuery('<p><span class="textShowMore gray9" href="javascript:;" title="查看完整公司介绍">展开<em></em></span></p>');
		var $showLessButton = jQuery('<p><span class="textShowLess gray9" href="javascript:;">收起<em></em></span></p>');
		if(currentHeight > setLine*lineHeight){
			$showMoreButton.insertAfter($textShowMore).show();
			$showLessButton.insertAfter($textShowMore).hide();
			$textShowMore.css({height: setLine * lineHeight + "px", overflow:"hidden"});
			
			$showMoreButton.click(function(){
				$textShowMore.removeAttr("style");
				$showMoreButton.hide();
				$showLessButton.show();
			});
			$showLessButton.click(function(){
				$textShowMore.css({height: setLine * lineHeight + "px", overflow:"hidden"});
				$showMoreButton.show();
				$showLessButton.hide();
			});
		}
	})();
	/*显示更多公司介绍 end*/
	//各项评分
		jQuery('.j-grade').click(function(){
			var $this = jQuery(this);
			var $sibling = $this.closest(".j-iterview").find(".j-gradeinfo");
			$this.toggleClass("showgrade");
			$sibling.toggle();
		});
	//面试经验请求  站内公司，没面试经验的请求  暂时不请求
	
	//新闻提示--start
		jQuery("#news .j-news").explainItem({left:20, top:32, width:480});
	//新闻提示 end
	//这是调用提示框功能--start
		jQuery("#SB-name").lenovoPrompt();
	//这是调用提示框功能 end
	//图片因点击而滚动--start
		jQuery("#evPic").eventScrollImage(6);
	//图片因点击而滚动 end
	//图片上传JS--start
		(function(){
			var formAction = "company_info_upImage";
			var companyID = "11037270";
			var up = new UpPicture(550, "/process/", {formAction: formAction, companyID: companyID},null,null);
			jQuery(".uploadImage").click(function(){
				up.showUpPicture();
			});
		})();
	//图片上传JS end

	/*******弹出评论后的星星评分********/
	jQuery("#allfeel").reviewStar("a", "rwcnt-allfeel-star-selected", "input[type=hidden]", "em[class$=tips]", {"1":"非常不满意", "2":"不满意", "3":"一般", "4":"满意", "5":"非常满意"});
