	<script>
		(function(){
			var url = window.location.pathname;
			var urlSplit = url.split('/');
			var navTab = document.getElementById('navTab');
			var navTabElement = navTab.getElementsByTagName('a');
			if(urlSplit[3] == "" && urlSplit[1] != "job"){
				navTabElement[0].className = "selected";
			}else if(urlSplit[3] == "" && urlSplit[1] == "job"){
				navTabElement[1].className = "selected";
			}else{
				for(var i = 1; i < navTabElement.length; i++){
					var nte = navTabElement[i];
		 			var href = nte.href;
	 				var hrefSplit = href.split('/');
		 			if(hrefSplit[5] == urlSplit[3] || (urlSplit[3] =='comment' && hrefSplit[5] == 'review') || (urlSplit[3] =='question' && hrefSplit[5] == 'review')){
		 				nte.className = "selected";
		 				break;
		 			}
				}
			}
		})();
