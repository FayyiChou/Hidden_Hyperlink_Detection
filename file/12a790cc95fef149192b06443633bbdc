<SCRIPT>
var speed=15
demo32.innerHTML=demo31.innerHTML
function Marquee2(){
if(demo32.offsetWidth-demo3.scrollLeft<=0)
demo3.scrollLeft-=demo31.offsetWidth
else{
demo3.scrollLeft++
} }
var MyMar2=setInterval(Marquee2,speed)
demo3.onmouseover=function() {clearInterval(MyMar2)}
demo3.onmouseout=function() {MyMar2=setInterval(Marquee2,speed)}
</SCRIPT></div>
