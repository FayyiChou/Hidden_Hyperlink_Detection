<script type="text/javascript">
	 var games_portal=true,windowHeights=$(window).height(),games_portalpt=$("#games_portal").offset().top;
  $(window).scroll(function(){
    if(games_portal && $(this).scrollTop()+windowHeights>games_portalpt-100){
      console.log("aaa");
      games_portal=false;
      /* 异步 begin */
     dp.init({
      caller: 'portal',
      sign: 'KTgYZQloFHNq3',
      scenarioId: '56e8c287cca0843f23bcf8a2',
      pageId: '543459',
      userId: '9ed131c4-a74d-4706-88e5-44ae0d8d954f',
      debug: null,
      mode: null,
      statTag: 'dp-stat'
    });
  	dp.getReco({
      params: {
        limit: 9
      },
      onload: function(data){
        var lis = [];
        var items = data.data.items;
        for(var i = 0; i < items.length; i++){
        	var stat = items[i]['dp-stat'];
            var li = '<a class="recom-con" ' + stat + ' href="' + items[i].zoneUrl + '">' +
                     '<img ' + stat + ' src="' + items[i].imgUrl + '">' +
                     '<p ' + stat + '>' + items[i].name + '</p>' +
                	 '<span ' + stat + '  class="radius70"></span>' +
                     '</a>';
             lis.push(li);
		}
        document.getElementById('get_games_portal').innerHTML = lis.join('');
        document.getElementById('games_portal').style.visibility = "visible";
	  },
      onerror: function(){ } 
    });
        /* 异步 end */
    }
  });
   /*替换用户信息板块*/
  function replaceUserInfo(data){
    // 参数校验
    if(data==null || data.uid==null || data.uid<=0){
    	return;
    }
    // 变量定义
    var memberUrl = "http://myspace.9game.cn/";
    var memberCss = "";
    if(data.member!=null && data.member.memberType!=null && data.member.memberType==1 && data.member.memberGrade>0){
    	memberUrl = "http://huiyuan.9game.cn/";
      	memberCss = "vip link vip"+data.member.memberGrade;
    }
    // 开始替换
    var userInfoBlock = $("li#ajaxLogin");
    var userMsgBlock = $("li#ajaxRegister");
    var userInfoHtml = $("li#ajaxUserInfo");
    var userMsgHtml = $("li#ajaxMsg");
    userInfoBlock.attr("class", userInfoHtml.attr("class"));
    userMsgBlock.attr("class", userMsgHtml.attr("class"));
    $("img[ajaxName=userLogo]").attr("src", data.logoImgSrc);
    $("a[ajaxName=userName]").attr("class", memberCss);
    $("a[ajaxName=userName]").attr("href", memberUrl);
    $("a[ajaxName=userName]").text(data.nickName);
    $("span[ajaxName=msgNum]").text(data.msg.msgNum);
    $("span[ajaxName=sysMsgNum]").text(data.msg.sysMsgNum);
    $("span[ajaxName=actMsgNum]").text(data.msg.actMsgNum);
    $("span[ajaxName=ghMsgNum]").text(data.msg.ghMsgNum);
    $("span[ajaxName=kaMsgNum]").text(data.msg.kaMsgNum);
    $("span[ajaxName=bbsMsgNum]").text(data.msg.bbsMsgNum);
    $("span[ajaxName=dialogMsgNum]").text(data.msg.dialogMsgNum);
    $("span[ajaxName=imMsgNum]").text(data.msg.imMsgNum);
    userInfoBlock.html( userInfoHtml.html());
    userMsgBlock.html(userMsgHtml.html());
  }
  /*替换用户信息板块*/
  function replaceDownBtn(data, platformId){
    // 参数校验
    if(data==null){
    	return;
    }
    if(platformId==null || platformId<=0){
    	return;
    }
    // 开始替换
    if(platformId==2){
      	var downBtnBlock = $("a[ajaxName=androidDownBtn]");
      	if(data.count>=1){
          	downBtnBlock.attr("href", data.downUrl);
            if(data.downUrl.indexOf("gameid=")>0){
              	downBtnBlock.html("<span class='icon'></span>免费进入");
            }
      		if(data.count>1){
          		var mulDownBtnHtml = $("span[ajaxName=androidMulDownBtn]");
              	downBtnBlock.replaceWith(mulDownBtnHtml.prop("outerHTML"));
        	}
        }else{
        	var noDownBtnHtml = $("span[ajaxName=androidNoDownBtn]");
          	downBtnBlock.replaceWith(noDownBtnHtml.prop("outerHTML"));
        }
    }else if(platformId==3){
    	var downBtnBlock = $("a[ajaxName=iosDownBtn]");
        if(data.count>=1){
        	downBtnBlock.attr("href", data.downUrl);
        }else{
        	var noDownBtnHtml = $("span[ajaxName=iosNoDownBtn]");
          	downBtnBlock.replaceWith(noDownBtnHtml.prop("outerHTML"));
        }
    }
  }
  /* 异步获取用户登录信息 */
  window.onload = function(){
    $.ajax({
      	url: "http://www.9game.cn/tpl/pc2/ajaxinfo/ajax_login_info.html",
  		type: 'POST',
		timeout: 15e3,
  		data: null,
		success: function(data,state){
          	if(data!=null && data.uid!=null && data.uid>0){
				replaceUserInfo(data);
          	}
		},
  		dataType: "json"
	});
    var dataA = {
		"gi": '543459',
	    "pi": 2
	};
    $.ajax({
      	url: "http://www.9game.cn/tpl/pc2/ajaxinfo/ajax_download_info.html",
  		type: 'POST',
		timeout: 15e3,
  		data: dataA,
		success: function(data,state){
          	if(data!=null){
				replaceDownBtn(data, dataA.pi);
          	}
		},
  		dataType: "json"
	});
    var dataI = {
		"gi": '543459',
	    "pi": 3
	};
    $.ajax({
      	url: "http://www.9game.cn/tpl/pc2/ajaxinfo/ajax_download_info.html",
  		type: 'POST',
		timeout: 15e3,
  		data: dataI,
		success: function(data,state){
          	if(data!=null){
				replaceDownBtn(data, dataI.pi);
          	}
		},
  		dataType: "json"
	});
  }
</script>

</body></html>