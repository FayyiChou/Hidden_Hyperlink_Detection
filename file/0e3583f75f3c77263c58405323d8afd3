    <script type="text/javascript">
        var bdMap = function () {
            var province = $('#bdmap .province').text();
            var address = $('#bdmap .addr').text();
            var map = new BMap.Map("bdmap");
            map.centerAndZoom(new BMap.Point(116.404, 39.915), 3);
            var myGeo = new BMap.Geocoder();
            myGeo.getPoint(address, function (point) {
                if (point) {
                    map.centerAndZoom(point, 16);
                    map.addOverlay(new BMap.Marker(point));
                }
            }, province);
            var opts = {type: BMAP_NAVIGATION_CONTROL_SMALL}
            map.addControl(new BMap.NavigationControl(opts));
            var copyCtrl = new BMap.CopyrightControl({anchor: BMAP_ANCHOR_BOTTOM_RIGHT});
            copyCtrl.addCopyright({id: 1, content: "中国供应商"});
            map.addControl(copyCtrl);
        };
        bdMap();
    </script>
