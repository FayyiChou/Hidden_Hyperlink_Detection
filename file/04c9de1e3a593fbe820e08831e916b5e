                        <script>
						var wife = 290;
						var lend2 = $('#demo1').children().length;
						var znum  = (lend2-1)*wife;
						
						
                        function Marquee(){
							
							if(znum <= demo.scrollLeft){
								demo.scrollLeft=0    
							}else{
								demo.scrollLeft++   
							}
						}
                        var MyMar=setInterval(Marquee,40)
                        demo.onmouseover=function() {clearInterval(MyMar)} 
                        demo.onmouseout=function() {MyMar=setInterval(Marquee,40)} 
                        </script>
                  
                </div>
            </div>
        
