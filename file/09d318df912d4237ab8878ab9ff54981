function doSearch()
{
    var finds = $('find').get('value');
	var industry =$('industry').get('value');
	if (industry==0)
	{
		window.location = "plist.xhtml?find=" + encodeURI(finds);		
		return;
	}

	window.location =  "category.xhtml?industry=" + encodeURI(industry) + "&find=" + encodeURI(finds);
}
</script>    
