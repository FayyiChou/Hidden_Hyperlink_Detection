            setTimeout(function() {
                //ios10 GPS定位失效，所以取ip定位补充
                
                if (!$.getCookie('uarea')) {
                    
                    var carea = $.getCookie('area');
                    var areaNum=carea.split('|')[0];
                    
                    areaNum = areaNum - (areaNum % 100);
                    var pPinyin = cityPinyin[areaNum - (areaNum % 100)];
                    if (!pPinyin) {
                        pPinyin=provincePinyin[areaNum - (areaNum % 100)];
                    }
                    
                    if (!pPinyin) {
                        pPinyin = 'beijing'; //如果没找到ip定位的城市地址，则用北京兼容
                    }
                    
                    $.setCookie('uarea', areaNum + '|' + pPinyin, { domain: '.che168.com', expireHours: 24 * 30 });
                    window.location.href = '/' + pPinyin + '/';
                }
                
            }, 500);
            
            /**********IOS10 GPS定位失效 end*************/
            //防止切换菜单时uarea为null的情况
            if ($.getCookie('uarea') == 'null|null') {
                var carea = $.getCookie('area');
                var areaNum=carea.split('|')[0];
                    
                areaNum = areaNum - (areaNum % 100);
                var pPinyin = cityPinyin[areaNum - (areaNum % 100)];
                if (!pPinyin) {
                    pPinyin=provincePinyin[areaNum - (areaNum % 100)];
                }
                    
                if (!pPinyin) {
                    pPinyin = 'beijing'; //如果没找到ip定位的城市地址，则用北京兼容
                }
 
                $.setCookie('uarea', areaNum + '|' + pPinyin, { domain: '.che168.com', expireHours: 24 * 30 });
            }
        })();

        /* 获取所有有认证车的城市ID，写入Cookie */
        (function () {
            var authenticationAreaList = $.getCookie('authenticationarealist', '');
            if (authenticationAreaList.length < 4) {
                $.ajax({
                    type: 'GET',
                    dataType: 'json',
                    url: '/handler/authentication/getallauthenticationcity.ashx',
                    success: function (data) {
                        if (data && data.code === '0') {
                            $.setCookie('authenticationarealist', data.cids, { domain: '.che168.com', expireHours: 1 });
                        }
                    }
                });
            }
        })();
        //发送地理定位统计
        function SendAreaPvInfo(lat,lng,pinyin,areaid,ref,cur){
            var _url="https://collectionpv.che168.com/collect/page_event.ashx";
            var _data = new Object();
            _data.eventkey = 'l_m_2sc_location';
            _data.info = lat+'|'+lng+'|'+pinyin+'|'+areaid;
            if (ref)
            {
                _data.ref = ref;
            }
            if (cur)
            {
                _data.cur = cur;
            }
            _data.rm = new Date().getTime();
            $.ajax({
                url: _url,
                type: 'get',
                dataType: 'jsonp',
                data: _data,
                complete: function() {
                }
            });
        }
        /*输出广告使用的当前城市的ID*/
        var ad_Province=0,ad_City=0,cityCode=0,ProvinceAreaID=0,areaId=0; 
        var _ad_area = $.getCookie('uarea');
        if (_ad_area && _ad_area.split('|').length === 2 && _ad_area.split('|')[0].length ==6) {
            areaId=_ad_area.split('|')[0];
        }  
		  
        if(areaId && !isNaN(areaId) && areaId.length==6 ){
            var cotyKey=areaId;
            if(areaId.substring(2,areaId.length)=="0000"){//直辖市获取省会省市ID
                cotyKey=parseInt(areaId)+100;
                if(cotyKey!=110100 && cotyKey!=310100 && cotyKey!=120100 && cotyKey!=500100){
                    ad_Province=cityJson[cotyKey].pid;           
                    ProvinceAreaID=cityJson[cotyKey].opid;
                }else{                      
                    ad_City=cityJson[cotyKey].cid;
                    ad_Province=cityJson[cotyKey].pid;
                    cityCode=cityJson[cotyKey].ocid;
                    ProvinceAreaID=cityJson[cotyKey].opid;
                }
            }else{
                if(cityJson[areaId]){
                    ad_City=cityJson[areaId].cid;
                    ad_Province=cityJson[areaId].pid;
                    cityCode=cityJson[areaId].ocid;
                    ProvinceAreaID=cityJson[areaId].opid;
                }
            }
        } 
        /* 如果是全国（或者只选择省没选择市）则给城市特殊值 100100 */
        if (ad_City === 0) {
            ad_City = 100100;
        }
    </script>

