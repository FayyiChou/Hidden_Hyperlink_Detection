        /* 因为选择地区页面选择的直辖市是整数所以要再次判断一下 */
        function isHaveCid(areaList) {
            var zhiXiaShi = [110000, 500000, 120000, 310000];
            for (var i = 0; i < zhiXiaShi.length; i++) {
                if (areaList.indexOf(zhiXiaShi[i] + 100) > -1) {
                    return true;
                }
                return false;
            }
        };    

