<script type="text/javascript">
$(function() {
    var uarea = $.getCookie("uarea");

    if (uarea) {
        var areaArr = uarea.split('|');

        if (areaArr.length > 1) {
            var areaPy = areaArr[1];
            if (areaPy == "" || areaPy == null) {
                areaPy = "china";
            }
            $("#a_buyCar").attr("href", "/" + areaPy + "/list/#pvareaid=102211");
            $("#a_index").attr("href", "/" + areaPy + "/#pvareaid=102210");
        } else {
            $("#a_buyCar").attr("href", "/china/list/#pvareaid=102211");
            $("#a_index").attr("href", "/#pvareaid=102210"); //如果没找到城市cookie 或者为null则走302跳转，不跳转则为全国
        }
    }else{
        $("#a_buyCar").attr("href", "/china/list/#pvareaid=102211");
        $("#a_index").attr("href", "/china/#pvareaid=102210");
    }
});


(function () {
    var uarea = $.getCookie('uarea'), area = $.getCookie('area');
    var uareaid = 0;
    var c2bcitys = $('#c2bcitys').val();

    if (uarea != undefined && uarea != "") {
        uareaid = parseInt(uarea.split('|')[0]);
        if (uareaid == 110000 || uareaid == 500000 || uareaid == 120000 || uareaid == 310000) {
            uareaid += 100;
        }
    } else if (area != undefined && area != "") {
        uareaid = parseInt(area);
    } else {
        uareaid = 0;
    }
    var locurl = '';
    if (c2bcitys.indexOf(',' + uareaid + ',') != -1) {
        locurl = "/user/car/add/p2/";
    } else {
        locurl = "/user/car/add/?isfree=1";
    }
    if ($('#salecar')) {
        $('#salecar').attr('href', locurl + "#pvareaid=101405");
    }
    if ($('#MeSaleCar')) {
        $('#MeSaleCar').attr('href', locurl + "#pvareaid=102076");
    }
    $('#footsalecar').attr('href', locurl + "#pvareaid=102212");
})();

