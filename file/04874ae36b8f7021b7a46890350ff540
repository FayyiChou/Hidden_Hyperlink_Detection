        /* 获取城市ID，写入Cookie */
        (function () {
            //菜单点击事件
            $("body").on("click", function (event) {
                var elem = event.target;
                if ($(elem).attr("_onclick")) {
                    eval($(elem).attr("_onclick"));
                    $.setCookie('uarea', $(elem).attr("aid") + '|' + $(elem).attr("apy"), { domain: '.che168.com', expireHours: 24 * 30 });
                }

            });
        })();

        /* 地理定位 */
        (function () {
            var isLocalStorage = false,
                isGeolocation = false,
                lat = 0, lng = 0,
                areaPinYin = '',
                 ref = document.referrer,
                coords = null,
                selectArea = '/selectarea.aspx?backurl=/&ref=' + ref,
                area = $.getCookie('uarea', ''),
                curUrl = location.href.toLowerCase(),
                pvid = 0,
                locationDay = new Date().getDate();

            //页脚电脑版pc首页跳转
            var areapinyin = area.split('|').length == 2 ? area.split('|')[1] : 'china';
            $("#pcIndex").attr('href', 'http://www.che168.com/' + areapinyin + '/?pvareaid=103098');

            if (ref !== '' && ref !== null && ref.length > 0) {
                ref = '?ref=' + escape(ref);
            }
          
          
            if (curUrl.indexOf('pvareaid=') > -1 && curUrl.split('pvareaid=')[1].length > 5) {
                pvid = curUrl.split('pvareaid=')[1].substr(0, 6);
                if (ref.length > 0) {
                    ref += '&pvareaid=' + pvid;
                } else {
                    ref = '?pvareaid=' + pvid;
                }
            }

            if (area !== '' && area.split('|').length === 2 && area.split('|')[1].length > 0) {
                if (location.href.split('/').length === 4) {
                    location.href = '/' + area.split('|')[1] + '/' + ref;
                }
            } else {
                if (navigator.geolocation) { isGeolocation = true; };
                if (isGeolocation) {
                    function getPosSuccess(position) {
                        coords = position.coords;
                        lat = coords.latitude, lng = coords.longitude;
                        $.ajax({
                            type: 'GET',
                            dataType: 'json',
                            url: '/handler/getpositionarea.ashx',
                            data: { 'lat': lat, 'lng': lng },
                            success: function (data) {
                                if (data !== undefined && data.code === 0 && data.pinyin !== 'china') {
                                    var areaId = 0;
                                    if (data.pinyin === 'beijing' || data.pinyin === 'chongqing' || data.pinyin === 'shanghai' || data.pinyin === 'tianjin') {
                                        areaId = parseInt(data.areaId / 10000) * 10000;
                                    } else {
                                        areaId = data.areaId;
                                    }
                                    $.setCookie('uarea', areaId + '|' + data.pinyin, { domain: '.che168.com', expireHours: 24 * 30 });
                                    SendAreaPvInfo(lat,lng,data.pinyin,areaId,document.referrer,location.href);//发送统计数据
                                    setTimeout(function () { location.href = '/' + data.pinyin + '/' + ref; }, 200);
                                    $.setCookie('isAddScreen', '2', { domain: '.che168.com', expireHours: 24 * 30 * 6 });
                                } else {
                                    //location.href = selectArea;
                                }
                            }
                        });
                    };
                    function getPosError(err) {
                        switch (err) {
                            case err.PERMISSION_DENIED:
                                console.log("您拒绝了共享位置，可手动选择城市。");
                                break;
                            case err.POSITION_UNAVAILABLE:
                                console.log("无法获取当前位置");
                                break;
                            case err.TIMEOUT:
                                console.log("获取位置超时");
                                break;
                            default:
                                console.log("未知错误");
                                break;
                        }
                        //location.href = selectArea;

                    };
                    navigator.geolocation.getCurrentPosition(getPosSuccess, getPosError, null);
                } else {
                    //location.href = selectArea;
                };
            }

            
