   <script>
var Picspeed30=15
demo300.innerHTML=demo30.innerHTML
function Marquee30(){
if(demo300.offsetWidth-demo3000.scrollLeft<=0)
demo3000.scrollLeft-=demo30.offsetWidth
else{
demo3000.scrollLeft++
}
}
var MyMar30=setInterval(Marquee30,Picspeed30)
demo3000.onmouseover=function() {clearInterval(MyMar30)}
demo3000.onmouseout=function() {MyMar30=setInterval(Marquee30,Picspeed30)}
   </script>

