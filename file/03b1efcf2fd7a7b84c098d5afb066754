<script>
$(window).resize(function(){

          var _w = $(this).width();

          if (_w > 640){

            $(".nav").show();

          }else{

            $(".nav").hide();

          }

        });
    $(document).ready(function() {
        $(".mobileNav_border").click(function() {
            $(this).toggleClass("active");
            $(".nav").slideToggle();
        });
        $(".nav > ul > li:has(ul) > a").append('<div class="arrow-bottom"></div>');

        $(".selectStyle").click(function() {
            var list = $(this).find("ul");
            list.toggle();
        });
        
        $(".selectStyle ul li").click(function(){		
            var e = $(this).closest(".selectStyle");
            e.find("span").text($(this).text());
            e.find('input').val($(this).text());
            
            $(this).find('ul').trigger('click');
        });
    });
    
    
</script>
    
