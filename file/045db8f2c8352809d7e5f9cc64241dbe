<script type="text/javascript">
function chkform1() {
var form = document.getElementById("yysjs");

var UserName = document.getElementById("UserName").value;

if (UserName.indexOf(" ") >= 0) {
	alert('姓名中不能包含空格');
	document.yysjs.UserName.focus();
	return false;
}

if (UserName == '请输入您的姓名' || UserName == '') {
	alert('姓名不能为空');
	document.yysjs.UserName.focus();
	return false;
}

var TelePhone = document.getElementById("TelePhone").value;

var re;
re = '/^0?(13[0-9]|15[0-9]|18[0-9]|14[0-9]|17[0-9])[0-9]{8}$/';
if (!re.test(TelePhone)) {
	alert("您输入的手机号格式不正确,请重新输入！");
	document.yysjs.TelePhone.focus();
	return false;
}
 if(document.yysjs.xiaoqu.value==""){
 alert('请填写小区名称！');
 document.yysjs.xiaoqu.focus();
 return false;
 }
else
return true;
}
</script>     
