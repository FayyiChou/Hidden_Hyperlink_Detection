<script language="javascript">
$(document).ready(function() {
	
/**
* 底部报名
*/
  $(function(){
  $("#diClose").click(function(){
  $(".onlinebox").css("display","none");
  });
	})

$("#adTanchuang_onclick").click(function(){
  $("#adTanchuang").css("display","none");
  })

$("#imageField").click(function(){
	
	var chenghuval=$("#xm").val();
	if ($("#xm").val()=="" || $("#xm").val()=="请输入您的称呼")
	{
		alert("请输入您的称呼");
		$("#xm").focus();
		return false;	
	}
	var shoujival=$("#sj").val();
	if ($("#sj").val()=="")
	{
		alert("请输入手机号码");
		$("#sj").focus();
		return false;	
	}
	var leixingval=$("#lx").val();
	if (!$("#sj").val().match(/^0?(13[0-9]|15[0-9]|18[0-9]|14[0-9]|17[0-9])[0-9]{8}$/)) { 
	alert("手机号码格式不正确！"); 
	$("#sj").focus(); 
	return false; 
	} 

	$.ajax({
		type:"POST",
		url:"/save_book.asp",
		data:{chenghu:encodeURI(chenghuval),Telephone:shoujival,class_id:leixingval,action:"mfty"},
		success: function(data,st){
		
		if (data==1||data==11)
		{
			alert("提交成功,我们的客服人员会尽快和您联系!");	
		}
		else if (data==2)
		{
			alert("您在最近一小时发布的信息超过10次，请稍后再试！");		
		}
		else
		{
			alert("报名失败,请联系在线客服人员处理!"+data);		
		}
		$("#xm").val("");
		$("#sj").val("");
		},
		error:function(data,st){
			alert('error'+data+"||"+st);
			}
	});
	
})
 

//links
$("#hezuon").hide();$("#linksn").hide();
$("#brand").click(function(){$("#brandn").show();$("#hezuon").hide();$("#linksn").hide();$("#brand").css('font-weight','bold');$("#hezuo").css('font-weight','normal');$("#links").css('font-weight','normal');})

$("#hezuo").click(function(){$("#brandn").hide();$("#hezuon").show();$("#linksn").hide();
$("#brand").css('font-weight','normal');$("#hezuo").css('font-weight','bold');$("#links").css('font-weight','normal');})

$("#links").click(function(){$("#brandn").hide();$("#hezuon").hide();
$("#linksn").css('display','block');$("#brand").css('font-weight','normal');$("#hezuo").css('font-weight','normal');$("#links").css('font-weight','bold');
})	


$("#lingquyusuan").click(function(){
	$("#footertan").css('display','block');
	$("#bchenghu").val("");
	$("#bshouji").val("");
})
$("#lingquyusuan2").click(function(){
	$("#footertan").css('display','block');
	$("#bchenghu").val("");
	$("#bshouji").val("");
})

$("#youshangjiaox").click(function(){ $("#footertan").css('display','none');})
$(".closea").click(function(){ $("#footertan").css('display','none');})


/**
* 报价弹窗
*/
$("#quedingbaoming").click(function(){
			
			var chenghuval=$("#bchenghu").val();
			if ($.trim($("#bchenghu").val())=="" || $("#bchenghu").val()=="请输入您的称呼")
			{
				alert("请输入您的称呼!");
				$("#bchenghu").focus();
				return false;	
			}
			
			var shoujival=$("#bshouji").val();
			if ($.trim($("#bshouji").val())=="" || $("#bshouji").val()=="请输入您的手机号码")
			{
				alert("手机号码不能为空,请输入手机号码!");
				$("#bshouji").focus();
				return false;	
			}
			
			var jieshouval=$("#jieshou").val();
			if ($.trim($("#jieshou").val())=="" || $("#jieshou").val()=="请输入您的小区")
			{
				alert("您的小区不能为空,请输入小区名称!");
				$("#jieshou").focus();
				return false;	
			}
			
			if (!$("#bshouji").val().match('/^0?(13[0-9]|15[0-9]|18[0-9]|14[0-9]|17[0-9])[0-9]{8}$/')) { 
			alert("手机号码格式不正确！"); 
			$("#bshouji").focus(); 
			return false; 
			}
			
			var yusuanval=$("input[name='byusuan']:checked").val();
			var huxingval=$("input[name='bhuxing']:checked").val();
			var leixingval=47
			$.ajax({
			type:"POST",
			//url:"../../baoming_ajax_case.php?t=2",
			url:"/save_book.asp",
			data:{chenghu:encodeURI(chenghuval),Telephone:shoujival,jieshou:encodeURI(jieshouval),class_id:leixingval,action:"mfty"},
			success: function(data,st){
			if (data==1)
			{
				alert("透明装修报价领取成功，我们会第一时间将报价发送给您！!");	
			}
			else if (data==2)
			{
				alert("您在最近一小时发布的信息超过10次，请稍后再试！");		
			}
			else
			{
				alert("报名失败,请联系在线客服人员处理!");		
			}
			}
			});
			$("#footertan").css('display','none');
		});

});
</script>
