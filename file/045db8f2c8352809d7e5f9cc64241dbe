<script type="text/javascript">
var selectListShow = 0;
var tp="case";
$(function(){
	$("#type,.searchselectbtn").click(function(){
			if (selectListShow){
				$("#selectTypeList").slideUp("fast");
				selectListShow = 0;
			}else{
				$("#selectTypeList").slideDown("fast");
				selectListShow = 1;
			}
			return false;
		});
	$("body").click(function(){
		if (selectListShow){
			$("#selectTypeList").slideUp("fast");
			selectListShow = 0;
		}
	});
		
	$("#selectTypeList li").click(function(){
		$("#type").text($(this).text());
 		document.getElementById("type_s").value=document.getElementById("type").innerHTML//输出值
		var hrf= $(this).attr("ahref");
	    //$("#form1").attr("action",hrf);
		tp= $(this).attr("tp");
		$("#type").attr("typename",$(this).attr("typename"));
		$(this).parent().hide();
	});
	

});	

  function searchv()
  {
	if($("#searchkey").val()=="")
	{
		alert('请输入要查询的关键字');
		$("#searchkey").focus();
		return false;	
	}
	
	if($("#searchkey").val()=="请输入关键字")
	{
		alert('请输入要查询的关键字');
		$("#searchkey").focus();
		return false;	
	}

		var sk =$("#searchkey").val();
		//sk =  encodeURIComponent(sk);
		if(tp=="case"){
			location.href="/anli/s"+sk;
			}else if(tp=="news"){
			location.href="/search/s"+sk;
			}else if(tp=="team"){
			location.href="/team/s"+sk;
			}else if(tp=="construction"){
			location.href="/construction/s"+sk;
			}else if(tp=="tuan1"){
			location.href="/tuan1/s"+sk;
			}else if(tp=="zwj"){
			location.href="/zwj/s"+sk;
			}
		
	  }	
</script>

