    function chkform2() {
    var form = document.getElementById("XQanli");
    
    
    var Name = document.getElementById("Name").value;
    
    if (Name.indexOf(" ") >= 0) {
        alert('姓名中不能包含空格');
        document.XQanli.Name.focus();
        return false;
    }
    
    if (Name == '请输入您的姓名' || Name == '') {
        alert('姓名不能为空');
        document.XQanli.Name.focus();
        return false;
    }
    
    var Tel = document.getElementById("tel").value;
    
    var re;
  	re = /^0?(13[0-9]|15[0-9]|18[0-9]|14[0-9]|17[0-9])[0-9]{8}$/;
    if (!re.test(Tel)) {
        alert("您输入的手机号格式不正确,请重新输入！");
        document.XQanli.tel.focus();
        return false;
    }
     if(document.XQanli.loupan.value==""||document.XQanli.loupan.value=="请输入您的楼盘名称"){
     alert('请填写楼盘名称！');
     document.XQanli.loupan.focus();
     return false;
     }
    else
    return true;
    }
