<script>
function fcbmchk(){
	var fcbm=document.getElementById("fcbm");
	var title=document.getElementById("title").value;
	if(title == ""){
		alert("请输入您的称呼");
		document.fcbm.title.focus();
		return false;
		}
	var tel=document.getElementById("tel").value
	if(tel == ""){
		alert("请输入您的手机号");
		document.fcbm.tel.focus();
		return false;
		}
	var rr;
	rr = /^0?(13[0-9]|15[0-9]|18[0-9]|14[0-9]|17[0-9])[0-9]{8}$/;
	if(!rr.test(tel)){
		alert("您输入的手机号格式不正确，请重新输入！");
		document.fcbm.tel.focus();
		return false;
		}
	var loupan=document.getElementById("loupan").value;
	if(loupan == ""){
		alert("请输入小区名");
		document.fcbm.loupan.focus();
		return false;
		}
	return true;
}
</script>
