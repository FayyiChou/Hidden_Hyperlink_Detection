  function searchv1()
  {
	if($("#searchValue1").val()=="")
	{
		alert('请输入要搜索案例的关键字');
		$("#searchValue1").focus();
		return false;	
	}
	
	if($("#searchValue1").val()=="搜索案例")
	{
		alert('请输入要搜索案例的关键字');
		$("#searchValue1").focus();
		return false;	
	}

		var sk =$("#searchValue1").val();
		location.href="/anli/s"+sk;
		
	  }	
</script>


