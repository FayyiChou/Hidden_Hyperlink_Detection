function OpenBaoMing(id,class_id)
{
	var diag = new Dialog();
	diag.Width = 420;
	diag.Height = 300;
	diag.Title = "申请免费预约";
	diag.URL = "/baoming.asp?id="+id+"&class_id="+class_id;
	diag.show();
}
