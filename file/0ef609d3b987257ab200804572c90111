jQuery('#catalog_zone_index').dialog({'autoOpen':false,'resizable':false,'draggable':false,'modal':true,'width':400});

        var zone_index_correct = $("#zone_index_correct");
        $("#zone_index_input").keyup(function(){
            zone_index_correct.val(0);
            if ( this.value.length == 6 ) {
                $("#zone_index_input_error,#zone_index_input_error2").hide();
                jQuery.ajax({
                    "success":function(d){
                        if (d.error) {
                            if (d.error == 1) {
                                $("#zone_index_input_error2").show()
                            } else if (d.error == 2) {
                                $("#zone_index_input_error").show()
                            }
                        } else {
                            $("#zone_index_correct").val(d.zone);
                        }
                        return false;
                    },
                    "type":"POST",
                    "url":"/cart/changeSubzoneIndex",
                    "cache":false,
                    "data": {
                        "index" : this.value,
                        "orgid" : $("#org_id").val()
                    },
                    dataType: "json"
                });
            }
        });
    
jQuery('#mistake_dialog').dialog({'autoOpen':false,'resizable':false,'draggable':false,'modal':true,'width':600,'title':'Сообщить об ошибке'});
});
