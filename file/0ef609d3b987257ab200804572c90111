$('#yw0').qtip({content:'<div>В зависимости от выбранного региона, Вам показываются те издания, на которые можно подписаться в этом регионе.<br> Регион можно сменить в дальнейшем при оформлении заказа.<br> Для оформления подписки в подарок, выберите регион получателя.</div>',  show: 'mouseover',hide: {fixed: true,delay: 240},style: { background: '#fff',color:'#fff',border: {width: 1,color: '#538CB9'},padding : 1,name: 'blue'}});
$('#yw1').qtip({content:'<div>Выберите желаемого подписное полугодие: (1) - первое, (2) - второе. Если отсутствуют нужные Вам подписные месяцы, смените подписное полугодие. При этом необходимо учитывать, что издания, относящиеся к разным подписным полугодиям, оформляются разными заказами.</div>',  show: 'mouseover',hide: {fixed: true,delay: 240},style: { background: '#fff',color:'#fff',border: {width: 1,color: '#538CB9'},padding : 1,name: 'blue'}});
$('#yw2').qtip({content:'<div>Оплатить с помощью пластиковой карты, через аппараты QIWI или распечатав счёт для оплаты через банк. <A href="http://vipishi.ru/document/57/oplata-zakaza#oplata-bankovskoi-kartoi" target=_blank>Подробнее..</A></div>',  show: 'mouseover',hide: {fixed: true,delay: 240},style: { background: '#fff',color:'#fff',border: {width: 1,color: '#538CB9'},padding : 1,name: 'blue'}});
$('#yw3').qtip({content:'<div>Оплата в отделениях ФГУП "Почта России" или ФГУП "Почта Крыма"</div>',  show: 'mouseover',hide: {fixed: true,delay: 240},style: { background: '#fff',color:'#fff',border: {width: 1,color: '#538CB9'},padding : 1,name: 'blue'}});

        $("#org_id").change(function(){
            var subzones = {"518":1,"1080":1,"1085":1,"678":1,"1088":1,"1007":1,"1089":1,"551":1};
            if ( subzones && subzones[$(this).val()]  ) {
                $("#zone_index_cancel").unbind("click").click(function(){
                    $("#zone_index,#zone_id").val("-1");
                    $("#topparams_form").submit();
                });

                $("#zone_index_ok").unbind("click").click(function(){
                    if ( parseInt($("#zone_index_correct").val()) != 0 ) {
                        $("#zone_index").val( $("#zone_index_input").val() );
                        $("#zone_id").val( $("#zone_index_correct").val() );
                        $("#topparams_form").submit();
                    }
                });
                $("#catalog_zone_index").dialog("open");
            } else {
                $("#zone_index,#zone_id").val("-1");
                $("#topparams_form").submit();
            }
        });

        var catalogs = {"1":["30","31"],"2":["30","31"],"4":["30","31"]};

        $("#fastSub_catalog_post,#fastSub_catalog_inet").click(function(){
            var cat_type = ( $(this).val() == "post" ? 4 : ($("#usertype").val() == "inet" ? 1 : 2) );
            var o = $("#group_id option").addClass("gray").attr("disabled", "disabled").filter(function(){
                return catalogs[cat_type] && jQuery.inArray($(this).val(), catalogs[cat_type]) != -1;
            }).removeClass("gray").removeAttr("disabled").filter(":selected");
            if ( !o.length ) {
                o.end().filter(":eq(0)").attr("selected", "selected");
            }
        });

    

var top_resizer = function(){
    $("#topBlockContainer").myfluwi({boxQuery: "div.goodsbox"})
};
$(window).resize(top_resizer);
top_resizer();


var qtip_conf = {
    position: { adjust: { screen: true } },
    show: 'mouseover',
    hide: {fixed: true,delay: 240},
    style: {
        background: '#538CB9',
        color:'#fff',
        border: {
            width: 1,
            color: '#fff'
        },
        padding : 10,
        name: 'blue',
        classes: {
            tooltip : 'qtip-many'
        }
    }
};
$('#topBlockContainer .photo a').filter('[title^=""]').qtip(qtip_conf);
qtip_conf.position = {corner: {target: 'bottomLeft',tooltip: 'topLeft'}, adjust: { screen: true }};
$('#topBlockContainer .title a').filter('[title^=""]').qtip(qtip_conf);


jQuery('#s').autocomplete({'minLength':'3','showAnim':'fold','select': function(event, ui) {
            window.location = ui.item.href;
            return false;
        },'open':function(event, ui) {
            if ( $.browser.msie && $.browser.version == "6.0") {
                $("#fast_subscription_block select, #topparams_block select, #cabinet_header_block").css("visibility", "hidden");
            }
        },'close':function(event, ui) {
            if ( $.browser.msie && $.browser.version == "6.0") {
                $("#fast_subscription_block select, #topparams_block select, #cabinet_header_block").css("visibility", "");
            }
        },'source':function( request, response ) {
                        if ( self.xhr ) {
                            self.xhr.abort();
                        }
                        self.xhr = $.ajax({
                            url: "/catalog/search/autocomplete/usertype/inet/org_id/330/group/31",
                            data: { s: request.term },
                            dataType: "json",
                            success: function( data, status, xhr ) {
                                if ( xhr === self.xhr ) {
									response( data );
                                }
                                self.xhr = null;
                            },
                            error: function( xhr ) {
                                if ( xhr === self.xhr ) {
                                    response( [] );
                                }
                                self.xhr = null;
                            },
                            complete: function(){$("#header_search_loader").hide();},
                            beforeSend:function(){$("#header_search_loader").show();}
                        });
                    }});
