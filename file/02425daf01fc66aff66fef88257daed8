                url : '//shop.vipstatic.com/js/public/common/header_com-hash-094455cd.js?12017033002'

            });
        }
        if ( /\.vipglobal\.hk$/.test( window.location.hostname ) ) {
            //同步完cookie再执行
            $.Listeners.sub('syncCookieDone').onsuccess(function() {
                loadHeaderJs();
            });
        } else {
            loadHeaderJs();
        }
        //抛出已加载头部消息
        $.Listeners.pub('commHeaderLoaded').success();
    });
</script>


