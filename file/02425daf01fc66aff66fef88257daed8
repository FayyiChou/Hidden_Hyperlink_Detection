			vip_wh='';
		}
		if(typeof(VipRUID)=='undefined'){
			VipRUID='';
		}
		if (typeof(VipRNAME) == "undefined") { 
			VipRNAME='';
		}
       	var pageId = 223;
        window.VTM = {wareHouse: vip_wh,sessionUser: {id: VipRUID,name: VipRNAME},pageId: pageId};
    </script>

    <script type="text/javascript" src="http://ms.vipstatic.com/vtm/vtm_config_res/prod/vtm.js"></script>

</body></html>