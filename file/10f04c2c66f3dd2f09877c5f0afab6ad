<script>
$('.promo-nav a').click(function(){

      var url = $(this).attr('href');
      var fade = 1;
      $('.nav-area a').each(function(){
        if($(this).attr('href') == url)
        {
          $(this).trigger('click');
          fade = 0
        }
      })
      if(fade)
      {
            $('#overlay').fadeIn() 
          $.ajax({
          	url:	 $(this).attr('href'),
          	type:	 'POST',
            data:    'ajax=1',
          	success: function(response){
          	  $('#main').html(response)
              $('html,body').animate({scrollTop:0},'normal')
              $('#overlay').fadeOut()
          	}
          });
      }
      return false;
})
</script>			</div>
            <script>
                $('.section-link').click(function(){     
                    window.location.hash = $(this).attr('href')+'?nohash=1';
                    $('#overlay').fadeIn()
                     var _nid = $(this).attr('rel')
                      $.ajax({
                      	url:	 '/home/sales',
                      	type:	 'POST',
                        data:    'ajax=1&_nid='+_nid,
                      	success: function(response){
                      	  $('#main').html(response)
                          $('html,body').animate({scrollTop:0},'normal')
                          $('#overlay').fadeOut()
                      	}
                      });

                    return false;
                })
                $('.visual-news li a').click(function(){
                    window.location.hash = $(this).attr('href')+'?nohash=1';
                    $('#overlay').fadeIn()
                     var _nid = $(this).attr('rel')
                      $.ajax({
                      	url:	 '/home/news',
                      	type:	 'POST',
                        data:    'ajax=1&_nid='+_nid,
                      	success: function(response){
                      	  $('#main').html(response)
                          $('html,body').animate({scrollTop:0},'normal')
                          $('#overlay').fadeOut()
                      	}
                      });

                    return false;
                })
                $('.special-offer .more').click(function(){
                  $('.home-sales').trigger('click')
                  return false;
                })
                $('.visual-news .more').click(function(){
                  $('.menu-311 a').trigger('click')
                  return false;
                })

                $('body').attr('class','domestic-users alt')
                $('#wrapper').attr('class','')
                $('#main').attr('class','')
                document.title = 'Интернет-провайдер CityLink | Интернет | Телевидение | Телефония'

            </script>

