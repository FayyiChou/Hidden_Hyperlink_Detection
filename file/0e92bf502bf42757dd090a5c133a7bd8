            var btn1 = jQuery('#favo-btn1'),
                btn2 = jQuery('#favo-btn2'),
                pop1 = jQuery("#favo-pop1"),
                pop2 = jQuery("#favo-pop2"),
                timer1,
                timer2;
            function cancel(from) {
                jQuery.get("/ashx/article_favo.ashx", {
                    uid: autoHeaderObj.user.uid,
                    id: "93621",
                    type: 3,
                    r: Math.random()
                },
                    function (result) {
                        result = eval("(" + result + ")");
                        if (result && result.result) {
                            //btn1.attr("title", "收藏视频").html('<i class="icon16 icon16-start1"></i>收藏视频');
                            btn1.attr("title", "收藏视频");
                            $('.colbtnicon', btn1).removeClass('active');
                            btn1.attr("data-state", 2);
                            btn2.attr("title", "收藏视频").html('<i class="icon16 icon16-start1"></i><span>收藏视频</span>');
                            btn2.attr("data-state", 2);
                            jQuery(".layer-content").html('<span class="fn-left"><i class="icon33-ok"></i>取消成功！</span><p><a href="//i.autohome.com.cn/favor?tabid=12">查看我的收藏&gt;&gt;</a></p>');
                            popShow(from)
                        }
                    })
            }
            function submit(from)
            {
                jQuery.get("/ashx/article_favo.ashx", {
                    uid: autoHeaderObj.user.uid,
                    id: "93621",
                    type: 2,
                    title: encodeURIComponent("照顾“面子”需求 试驾雪佛兰科沃兹"),
                    url: window.location.href,
                    imgurl: 'http://www2.autoimg.cn/newsdfs/g9/M06/8E/B7/240x180_0_autohomecar__wKgH0Fga8LmALpBTAAHI4Tt2sn8366.jpg',
                    r: Math.random()
                },
                    function (result) {
                        result = eval("(" + result + ")");
                        if (result && result.result) {
                            //btn1.attr("title", "取消收藏").html('<i class="icon16 icon16-start2"></i>取消收藏');
                            btn1.attr("title", "取消收藏");
                            $('.colbtnicon', btn1).addClass('active');
                            btn1.attr("data-state", 1);
                            btn2.attr("title", "取消收藏").html('<i class="icon16 icon16-start2"></i><span>取消收藏</span>');
                            btn2.attr("data-state", 1);
                            jQuery(".layer-content").html('<div class="fn-clear"><span class="fn-left"><i class="icon33-ok"></i>收藏成功！</span><p><a href="//i.autohome.com.cn/favor?tabid=12">查看我的收藏&gt;&gt;</a></p></div>');
                            popShow(from)
                        }
                    })
            }
            function popShow(from) {
                switch (from) {
                    case 1:
                        {
                            timer1 && clearTimeout(timer1);
                            pop1.stop(true, true).hide();
                            pop1.fadeIn(500,
                                function () {
                                    timer1 = setTimeout(function () {
                                        pop1.fadeOut(500)
                                    },
                                        3e3)
                                })
                        };
                        break;
                    case 2:
                        {
                            timer2 && clearTimeout(timer2);
                            pop2.stop(true, true).hide();
                            pop2.fadeIn(500,
                                function () {
                                    timer2 = setTimeout(function () {
                                        pop2.fadeOut(500)
                                    },
                                        3e3)
                                })
                        };
                        break
                }
            }
            function btnOnClick(from) {
                switch (btn1.attr("data-state")) {
                    case "1":
                        {
                            cancel(from)
                        };
                        break;
                    case "2":
                        {
                            submit(from)
                        };
                        break
                }
            }
            function init()
            {
                //debugger;
                if (autoHeaderObj.user.isLogin) {
                    jQuery.ajax({
                        url: "//msi.autohome.com.cn/ajax/collection/exist_by_accountId",
                        data: {
                            accountId: autoHeaderObj.user.uid,
                            objId: "93621",
                            typeid: 12
                        },
                        dataType: "jsonp",
                        jsonp: "callback",
                        success: function (data) {

                            if (data) {
                                btn1.attr("title", "取消收藏");
                                $('.colbtnicon', btn1).addClass('active');
                                //.html('<i class="icon16 icon16-start2"></i>取消收藏');
                                btn1.attr("data-state", 1);
                                btn2.attr("title", "取消收藏").html('<i class="icon16 icon16-start2"></i><span>取消收藏</span>')
                            } else {
                                btn1.attr("data-state", 2);
                                btn2.attr("data-state", 2)
                            }
                        }
                    });
                    btn1.click(function () {
                        btnOnClick(1);
                    });
                    btn2.click(function () {
                        btnOnClick(2);
                    })
                } else {
                    btn1.attr("href", "//account.autohome.com.cn/?backurl=" + location.href);
                    btn2.attr("href", "//account.autohome.com.cn/?backurl=" + location.href)
                }

                $.get("//reply.autohome.com.cn/api/like/getcount.json?_appid=v&appid=4&liketype=1&objid=93621&datatype=jsonp", function(c) {
                    if (0 != c.result.count) {
                        $('.zanbtn .zanbtntxt').html(c.result.count);
                    }
                }, 'jsonp');

                $('.zanbtn').click(function() {

                    if($('.zanbtn .zanbtnicon').hasClass("active"))
                        return;

                    $.get("//reply.autohome.com.cn/api/like/set.json?_appid=v&appid=4&Liketype=1&objid=93621&datatype=jsonp", function(c) {

                        var count = parseInt($('.zanbtn .zanbtntxt').text());
                        $('.zanbtn .zanbtntxt').text(count + 1);

                    }, 'jsonp');

                    $('.zanbtn .zanbtnicon').addClass('active');
                });
                //为智能推荐的视频点击添加pv统计
            }
            init();
        </script>
        <script type="text/javascript">
            var waitInterval;
            var delayTime = 500;
            function showHothover(op) {
                clearTimeout(waitInterval);
                waitInterval = window.setTimeout("nTab(" + op + ")", delayTime);
            }
            function clearTO2() {
                clearTimeout(waitInterval);
            }
            function nTab(op) {
                var sfEls = document.getElementById("tab").getElementsByTagName("li");
                for (var i = 0; i < sfEls.length; i++) {
                    document.getElementById("hot" + i).style.display = i == op ? "block" : "none";
                    sfEls[i].className = i == op ? "cur" : "";
                }
            }

            function statue(op) {
                if (op == 0) {
                    if (document.getElementById("leterId").innerHTML == "收起") {

                        document.getElementById("leterId").innerHTML = "详细";
                        document.body.onclick = null;
                    }
                    else {
                        //document.getElementById("descriptStatue").style.display = "";
                        document.getElementById("leterId").innerHTML = "收起";
                        var h = document.getElementById("descriptStatue").offsetHeight;

                        //获得父页面的iFrame标签
                        var iframe = document.getElementById("myFrame");
                        iframe.height = h + "px";

                        document.body.onclick = function (e) {
                            e = e || window.event;
                            var b = e.target || e.srcElement, f = true;
                            document.getElementById("leterId").innerHTML = "详细";
                            if (b.id == 'pin_clo' || b.className == 'fontw') {
                                closeDiv();
                                return false;
                            }
                            function getParentID(obj) {
                                if (obj.parentElement) {
                                    if (obj.parentElement.id == 'descriptStatue') {
                                        f = false;
                                        return;
                                    }
                                    arguments.callee(obj.parentElement);
                                }
                            };
                            function closeDiv() {
                                document.getElementById("descriptStatue").style.display = "none";
                                document.body.onclick = null;
                            };
                            getParentID(b);
                            if (f) closeDiv();
                        };
                    }
                }
            }
        </script>
        <script type="text/javascript">
            /*写入cookie*/
            function SetTipCookie() {
                var expire_days = 36000; //(保存的天数)
                var expire_date = new Date();   //new   Date("December   31,   3666");//长期有效的办法
                var ms_from_now = expire_days * 24 * 60 * 60 * 1000;
                expire_date.setTime(expire_date.getTime() + ms_from_now);
                var expire_string = expire_date.toGMTString();
                document.cookie = "cookieKnowTip=1;expires=" + expire_string + ";path=/;domain=v.autohome.com.cn";
                return false;
            }
            function GetTipCookie(name) {
                var a = document.cookie.indexOf(name + "=");
                if (a != -1) {
                    var b = document.cookie.substring((a + name.length + 1), document.cookie.length);
                    var c = b.split(";");
                    var d = c[0];
                    return d;
                }
            }
            function byClass(name, tag, content) {
                var arr = [], nodes = (content || document).getElementsByTagName((tag || '*')),
                    len = nodes.length, pattern = new RegExp("(\\s|^)" + name + "($|\\s)"), i = 0;
                for (; i < len; i++) pattern.test(nodes[i].className) && arr.push(nodes[i]);
                return arr;
            };

            function videoToggle(left, right, video) {

                var min = byClass('btn_in')[0],
                    max = byClass('btn_out')[0],
                    vParent = byClass('video')[0],
                    userBox = byClass('user-box')[0],
                    left = document.getElementById(left),
                    video = document.getElementById(video);
                right = document.getElementById(right);

                if (!$('.user-box')) {
                    right = false;
                }

                if (4 == 6) {
                    video && (video.style.width = '990px');
                    left && (left.style.display = 'block');
                    right && (right.style.display = 'none');
                    userBox && (userBox.style.marginTop = (video.clientHeight + 10) + 'px');
                    $('.user-box').addClass('user-box-l');
                }

                min.onclick = function () {
                    this.style.display = 'none';
                    max.style.display = 'block';
                    video && (video.style.width = '990px');
                    left && (left.style.display = 'block');
                    right && (right.style.display = 'none');
                    //userBox && (userBox.style.marginTop = (video.clientHeight + 10) + 'px');
                    //
                    userBox && (userBox.style.display = 'none');
                    //
                    $('.user-box').addClass('user-box-l');
                };

                max.onclick = function () {
                    this.style.display = 'none';
                    min.style.display = 'block';
                    video && (video.style.width = '690px');
                    left && (left.style.display = 'none');
                    right && (right.style.display = 'block');
                    //userBox && (userBox.style.marginTop = '0px');
                    //
                    userBox && (userBox.style.display = 'block');
                    //
                    $('.user-box').removeClass('user-box-l');
                };

                vParent.onmouseover = function () {
                    if (video.style.width.indexOf('990') == -1) {
                        min.style.display = 'block';
                    }
                };

                vParent.onmouseout = function () {
                    if (video.style.width.indexOf('990') == -1) {
                        min.style.display = 'none';
                    }
                };

                min.style.display = max.style.display = 'none';
            };

            videoToggle('videoList_L', 'videoList_R', 'youkuplayer');
        </script>

