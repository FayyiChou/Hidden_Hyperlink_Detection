    </script><script type="text/javascript">
    //搜索框数据
    var brands = '摩托,越野,漂移,俄罗斯,原创,事故,车祸,碰撞测试,直线加速,RC,Ken Block,试驾,兰博基尼,法拉利,保时捷,迈凯伦,路虎,奔驰,奥迪,宝马,福克斯,高尔夫,日产GT-R,布加迪威航,SUV,卡车';
    var ite = brands.split(',');
    function searchEnter(evt) {
        evt = evt ? evt : (window.event ? window.event : null);
        if (13 == evt.keyCode) {
            evt.returnValue = false;
            evt.cancel = true;
            searchClick()
        }
    }
    function searchClick() {
        var txt = document.getElementById("q");
        if (txt.value == "" || txt.value == "请输入关键字") {
            txt.value == "请输入关键字";
            return
        }
        else {
            window.open("//sou.autohome.com.cn/shipin?q=" + escape(txt.value) + "&entry=6#pvareaid=106546");
            return false
        }
    }
    function focusBlur() {
        var q = document.getElementById("q");
        if (q && q.value == '') {
            q.value = ite[Math.floor(Math.random() * ite.length)]
        }
    }
    focusBlur();

    </script><script type="text/javascript">
    logincallback = function () {

        ValidateIsSubscribed();
    };
    loginoutcallback = function () {
        ValidateIsSubscribed();
    };
    var subscribeclick, cancelsubscribeclick;
    seajs.use(['jquery', 'popLogin', 'dialog', 'md5'], function ($, popLogin, dialog) {
        window.changefans = function (add) {
            var cur = $('.user-box-info3 em').html();
            cur = parseInt(cur);
            if (add)
                cur++;
            else
                cur--;
            if (cur < 0) cur = 0;
            $('.user-box-info3 em').html(cur);
        }
        /*首页推荐是否订阅*/
        window.ValidateIsSubscribed = function () {
            if (autoHeaderObj.user.isLogin) {
                var userIds = $('#subscribe').data('uid');
                if(!userIds)
                    return;

                $.ajax({
                    url: "/Comm/GetSubscribedUsers",
                    data: {
                        userIds: userIds
                    },
                    cache: false,
                    dataType: 'json',
                    success: function (data) {
                        if (data.returncode == 0) {
                            if (data.result.length > 0) {
                                $('#subscribe').html("取消订阅");
                                $('#subscribe').data('state', 1);
                                $('#subscribe').attr('class', 'btn btn-small btn-disabled');
                            }
                        }
                    }
                });
            } else {
                $('#subscribe').html("订阅");
                $('#subscribe').attr('class', 'btn btn-small btn-orange');
                $('#subscribe').bind('click', subscribeclick);
            }
        }
        ValidateIsSubscribed();

        

        cancelsubscribeclick = function () {
            $.ajax({
                url: "/Comm/CancelSubscribe",
                data: {
                    userId: $('#subscribe').data('uid')
                },
                cache: false,
                dataType: 'json',
                success: function (data) {
                    if (data == 1) {
                        changefans(false);
                        $('#subscribe').html('订阅');
                        $('#subscribe').data('state', 2);
                        $('#subscribe').attr('class', 'btn btn-small btn-orange');
                    }
                }
            });

            _trackEvent.push({ 'eid': '1|1|155|590|200127|200059', 'val': 1, 'a1': autoHeaderObj.user.isLogin ? autoHeaderObj.user.uid : 0, 'a2': $('#subscribe').data('uid') });
        };
        subscribeclick = function () {
            //请求订阅 并且修改页面样式为已订阅
            /// 1：订阅成功
            /// 0：已经订阅过改用户
            /// -1：当前用户无效
            /// -2：被关注用户无效，不是视频发布者
            if (autoHeaderObj.user.isLogin) {
                if (autoHeaderObj.user.uid == $('#subscribe').data('uid')) {
                    window.alert('自己不能订阅自己哦~');
                    return;
                }
                $.ajax({
                    url: "/Comm/GetSubscribe",
                    data: {
                        userIds: $('#subscribe').data('uid')
                    },
                    cache: false,
                    dataType: 'json',
                    success: function (data) {
                        if (data == 1) {
                            changefans(true);
                            $('#subscribe').html('取消订阅');
                            $('#subscribe').data('state', 1);
                            $('#subscribe').attr('class', 'btn btn-small btn-disabled');
                        } else if (data == 0) {
                            $('#subscribe').html('取消订阅');
                            $('#subscribe').data('state', 1);
                            alert('已经订阅过该用户');
                        }
                    }
                });

            }
            else {
                var registerType = 0; //0简单注册，1完整注册
                popLogin.init(registerType, "");
                return false;
            }

            _trackEvent.push({ 'eid': '1|1|155|590|200127|200058', 'val': 1, 'a1': autoHeaderObj.user.isLogin ? autoHeaderObj.user.uid : 0, 'a2': $('#subscribe').data('uid') });
        }
        $('#subscribe').on("click", function () {
            if ($('#subscribe').data('state') == 1) {
                cancelsubscribeclick();
            } else {
                subscribeclick();
            }
        });

    })
    </script>
<script type="text/javascript">
    function formateDateTime(str) {
        var time = new Date(parseInt(str.substr(6)));
        var tt = time.getFullYear() + '-' + (time.getMonth() + 1) + '-' + time.getDate() + ' ' + time.getHours() + ':' + (time.getMinutes() > 9 ? time.getMinutes() : '0' + time.getMinutes());
        return tt;
    }

    //关闭浮层操作
    var closeAccountPop;
    //登录成功回调操作
    var callBackFunAccountPopLogin;

    //var gohref = "//v.autohome.com.cn";
