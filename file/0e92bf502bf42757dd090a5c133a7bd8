        function shareJfenIsShow()
        {
            //alert(autoHeaderObj.user.isLogin);
            if (!autoHeaderObj.user.isLogin) //如果没有登录
            {
                $('.jflogin').show();
                $('.jflogined').hide();
            }
            else //如果登录
            {
                $('.jflogin').hide();
                $('.jflogined').show();
            }
        }
        //轮循判断是否登录改变显示
        var timer;
        setTimeout(function(){
            shareJfenIsShow();
            clearTimeout(timer);
            timer = setTimeout(arguments.callee, 1000);
        },1000)
        //发送积分
