    <script type="text/javascript">

        var pvTrack = function() {};
        pvTrack.site = 1;
        pvTrack.category = 155;
        pvTrack.subcategory = 590;
        pvTrack.object = 93621;
        pvTrack.series = '4105';
        pvTrack.bcTypeId = 'vtagid,23;vuserid,19987472';

        if (pvTrack.series.split(',').length > 1)
        {
            pvTrack.series = pvTrack.series.split(',')[Math.floor(Math.random() * pvTrack.series.split(',').length)] * 1;
        }


       

    </script>

