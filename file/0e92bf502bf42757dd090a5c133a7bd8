<!-- 视频详细 展开收起 -->
<script type="text/javascript">
    $('#shortBtn').click(function () {
        $('.video-title-short').css('display', 'none');
        $('.video-title-long').css('display', 'block');
    })
    $('#longBtn').click(function () {
        $('.video-title-long').css('display', 'none');
        $('.video-title-short').css('display', 'block');
    })
</script>

<script>
    var $kind = $('#kind');
    var $kindContainer = $('.js_kindcontainer', $kind);
    var $kindtitle = $('.kindtitle', $kind);
    var $kindname = $('.js_kindname', $kind);
    var $kindlink = $('.kindname', $kind);
    var $arrow = $('.js_arrow', $kind);
    var kindHeight = $kindContainer.height();
    var kindnameHeight = $kindname.height();
    var kindtitleWidth = $kindtitle.width();
    var totalWidth = $kindContainer.width() + kindtitleWidth;
    var left = kindtitleWidth;
    var w;
    if (kindnameHeight > kindHeight) {
        for (var k = 0; k < $kindlink.length; k++) {
            w = $kindlink.eq(k).width() + 10;
            left += w;
            if (left > totalWidth) {
                left = left - w;
                break;
            }
        }

        $arrow.css('left', left).show().on('click', function() {
            if ($kind.data('show') == '1') {
                $kind.height(kindHeight);
                $('.js_arrow', $kind).removeClass('js_expand');
                $kind.data('show', '0');
            } else {
                $kind.height(kindnameHeight);
                $('.js_arrow', $kind).addClass('js_expand');
                $kind.data('show', '1');
            }
        });
        $arrow.show();
    } else {
        $arrow.hide();
    }
</script>
