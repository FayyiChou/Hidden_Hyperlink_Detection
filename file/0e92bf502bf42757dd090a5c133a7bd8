        //首页订阅覆层绑定数据
        function loadUnReadVideos() {
            if (!autoHeaderObj.user.isLogin) return;
            stop = true;

            $.ajax({
                url: "/Comm/GetUnReadVideos?r=" + $.now(),
                cache: false,
                dataType: 'json',
                success: function (data) {
                    if (data.returncode == 0 && data.result != null && data.result.length > 0) {
                        var $list = $('#popTake ul');
                        var row = '<li {4}><a onclick="ClearUnReadVideoCount();" href="{0}"><i class="video-icon"></i>{1}</a><div><em>{2}</em><span>{3}</span></div></li>';
                        for (var i = 0; i < data.result.length; i++) {

                            $list.append(row.replace('{0}', '/v-' + data.result[i].Id + '.html').replace('{1}', data.result[i].Title.substr(0, 15)).replace('{2}', data.result[i].Author.NickName).replace('{3}', formateDateTime(data.result[i].PublishTime)).replace('{4}', (i == data.result.length - 1 ? " class=\"last\"" : "")));
                        }
                    }
                }
            });
        }

        /* 我的订阅pop下拉 */
        (function () {
            loadUnReadVideos();

            var time;
            $('.myding').mouseenter(function () {


                var t = $(this).offset().top;
                var l = $(this).offset().left - 316 /*popTake's width*/ + 68;


                var wT = $(window).scrollTop();
                clearTimeout(time);
                if (autoHeaderObj.user.isLogin && UnReadVideoCount > 0) {
                    $('#popTake').css({ 'visibility': 'visible', 'top': (t + 39) + 'px', 'left': l + 'px' }).removeClass('fn-hide');
                }
            }).mouseleave(function () {
                    time && clearTimeout(time), time = setTimeout(function () {
                        $(document).on('mousemove', function (e) {
                            var $target = $(e.target);
                            var i = $target.closest('.myding').length == 0 ? $target.closest('#popTake').length : $target.closest('.myding').length;
                            0 == i && $('#popTake').addClass('fn-hide'), $(document).off('mousemove');
                        });
                    }, 100)
                })
            $('#popTake').mouseleave(function () {
                time && clearTimeout(time), time = setTimeout(function () {
                    $('#popTake').addClass('fn-hide');
                }, 100);
            });

        })();
    });
    </script>
<script type="text/javascript">
    var UnReadVideoCount = 0;
    seajs.use(['jquery'], function ($) {
        //订阅的小红点
        window.GetUnReadVideoCount = function () {
            $.ajax({
                url: "/Comm/GetUnReadVideoCount",
                cache: false,
                dataType: 'json',
                success: function (data) {
                    if (data > 0) {
                        UnReadVideoCount = data;
                        if (data > 4) {
                            $('#moreVideo').show();
                        }
                        $('.myding i').show();
                    } else {
                        $('.myding i').hide();
                        UnReadVideoCount = 0;
                    }
                }
            });
        };
        window.ClearUnReadVideoCount = function () {
           
            $.ajax({
                url: "/Comm/ClearUnReadVideoCount",
                cache: false,
                dataType: 'json',
                success: function (data) {
                    if (data > 0) {
                        $('.myding i').hide();

                        $('#popTake').remove();
                    }
                }
            });
        };
        GetUnReadVideoCount();
    });
    </script>
