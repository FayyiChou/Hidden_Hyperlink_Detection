        <script type="text/javascript">(function (B) { var $ = setInterval("hackTitle()", 5), _ = 300, A = document.title = document.title.split("#")[0]; function C() { if (_ === 0) { clearInterval($); return } if (document.title !== A) document.title = A; _-- } B.hackTitle = C })(window)</script>

<script type="text/javascript">
    function clickReply() {
        var txtusername = document.getElementById("txtusername");
        if (txtusername) {
            txtusername.focus();
        }
        else {
            document.getElementById("content").focus();
        }
        document.documentElement.scrollTop = document["body"].offsetHeight;
        document.body.scrollTop = document["body"].offsetHeight;
    }
    var positionTimer = null;
    var $floatDiv = jQuery("#floatDiv");
    function setPos() {
        var $window = jQuery(window);
        positionTimer && clearTimeout(positionTimer);
        var isie6 = navigator.userAgent.indexOf("MSIE 6.0") > 0;
        positionTimer = setTimeout(function () {
            var sT = $window.scrollTop(),
                sW = $window.width();
            if (sT > 0) {
                jQuery("#returnTop").show();
            }
            else {
                jQuery("#returnTop").hide();
            }
            if (isie6) {
                $floatDiv.css({ 'bottom': '', 'top': sT + $window.height() - $floatDiv.outerHeight() - 30 });
            }
            else {
                $floatDiv.css({ 'bottom': 30 });
            }
        }, 10);
    }
    jQuery(window).on('resize scroll', setPos);
    setPos();
    </script>



