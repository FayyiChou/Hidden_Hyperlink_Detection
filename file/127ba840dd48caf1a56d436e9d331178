 <script>
 	$(".parent_navigation").each(
		function($i){
			$(this).mouseover(function(e){
				var class_id=$(this).attr('class_id');
				$(".parent_navigation").attr("background","images/index_07.jpg");					   
				$(this).attr("background","images/index_05.jpg");
				if( class_id == 1 ||  class_id == 7 ||  class_id == 8 ){
					$("#nav_son_tr").hide();
				}else{
				$("#nav_son_tr").show();
				  $(".navigation_div").hide();
				$("#son_navigation_content_"+class_id).show();
				}
			});
		}							 
	);
 </script>
