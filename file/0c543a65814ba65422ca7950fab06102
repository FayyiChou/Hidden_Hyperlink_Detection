<script>
$(function(){
	var slider_box = $("#slider_box");	
	var ul = $("#slider_box ul");
	var li = $("#slider_box ul li");
	var _li = li.eq(0);
	var step = _li.outerWidth()+parseInt(_li.css("margin-left"))+parseInt(_li.css("margin-right"));//宽度
	var width = step*Math.ceil(li.length/3)*3;
	var _clr = null;
	var delay = 25;
	var _left = 0;
	//复制一份
	ul.width(width);
	ul.html(ul.html()+ul.html());
	$("#slider_box ul div").width(width/3);
	function slider_func(){
		_left = parseInt(ul.css("left"));
		if(0-_left > width/3){
			ul.css("left",0);
		}else{
			ul.css("left",_left-1);
		}
	}
	function play(){
		_clr = setInterval(function(){
			slider_func();	
		},delay);
	}
	$("#slider_box").bind({
		'mouseover':function(){
			clearInterval(_clr);
		},	
		'mouseout':function(){
			play();
		}
	});
	play();
});
</script>
