uid = Number(jQuery.cookie('mamaid'));
var passport_jsonp_url = 'https://passport.mama.cn/ssoapi/getCode/?key=www.zsmama.com&service=http%253A%252F%252Fwww.zsmama.com%252Findex.php%253F&time=1487351398&sign=8ce98d1998a5cd6529099f0574c2ae92&uid='+uid;

jQuery.ajax({
 url:'https://passport.mama.cn/ssoapi/getCode/?key=www.zsmama.com&service=http%253A%252F%252Fwww.zsmama.com%252Findex.php%253F&time=1487351398&sign=8ce98d1998a5cd6529099f0574c2ae92',
 dataType:"jsonp",
 jsonp:"callback",
 cache:false,
 success:function(data){
 	passportcode = data.code;
 },
 statusCode: {
200: function(){
var url = 'http://www.zsmama.com/passport.php?callback=checkcallback&httpcode=200&code='+passportcode+'&i='+Math.floor(Math.random()*1048576);
jQuery.getScript(url);
userinfo();

}
 }
});


})();

function userinfo(){
 	var myurl = 'http://www.zsmama.com/passport.php?mod=userinfo&callback=initUserInfo&i='+Math.floor(Math.random()*100024);
jQuery.getScript(myurl);
}
function checkcallback (result){
if(typeof result != 'undefined') {
if(typeof result['reload'] != 'undefined' ) {
//实现php页面自动刷新;
location.reload();
} else if(typeof result['logout'] != 'undefined' ) {
jQuery('#mamaTopLoginBox').show(); 
jQuery('#mamaInfo').hide();
location.reload();
}
}

}


//用于大首页和论坛首页显示
function initUserInfo(data) {
if(data['uid']){
jQuery('#mamaTopLoginBox').hide(); 
jQuery('#mamaInfo').show();
var uid = data['uid'];
var u = data||{};
jQuery('.userSpaceUrl').attr('href', "http://www.zsmama.com/space-uid-" + uid + ".html");
jQuery('#pcname').html(u.username);
jQuery('.userSpaceUrl').attr('href', "http://www.zsmama.com/space-uid-" + uid + ".html");
if (u.newprompt > 0) {
jQuery('#myprompt').html('<a href="http://www.zsmama.com/home-space-do-notice.html" target="_blank">提醒(' + u.newprompt + ')</a>').addClass('tips new');
}
if (u.newpm > 0) {
jQuery('#pm_ntc').html('<a href="http://www.zsmama.com/home-space-do-pm.html" target="_blank">短消息(' + u.newpm + ')</a>').addClass('mes new');
}

   }else{
   		return false;
   }

}     

 

