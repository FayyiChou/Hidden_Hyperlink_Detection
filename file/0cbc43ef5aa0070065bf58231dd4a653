<script language="javascript"> 
var jq = jQuery.noConflict();

jq(document).ready(function(){ 
var menuYloc = jq("#box").offset().top;
var data = {};
deraction= ('right'=='right') ? "right" : "left";
jq(window).scroll(function (){ 
var offsetTop = menuYloc +"px"; 
jq("#box").animate({top : offsetTop },{ duration:600 , queue:false }); 
}); 	
jq(".btton").bind("mouseenter",
function(){	
data[deraction] = '0px';
jq("#box").animate(data,"normal");
}
);
jq("#box").bind("mouseleave",
function(){
data[deraction] = '-187px';
jq("#box").animate(data,"normal");		
}
);
});
</script>
