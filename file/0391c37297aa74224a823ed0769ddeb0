// <![CDATA[
if (/Android|iPhone|iPad|iPod|BlackBerry|Windows Phone/i.test(navigator.userAgent)) {
    jQuery('.iOS span').remove();
    //此處針對所有iOS區塊下的span做移除動作
    jQuery('.iOS a[rel]').each(function (index, domEle) {
        jQuery('.iOS a').attr('href', jQuery(domEle).attr('rel')).removeAttr('target');
    });
     // 將rel取代原本href連結,並且將target屬性移除
} else {
swfobject.embedSWF('images/index.swf', 'mapSwf', '100%', '650', '10', 'images/expressInstall.swf', {}, {menu: false, wmode: 'transparent'}, {});
    //此處放置原本的flash引用寫法，若同頁有多個就接著放下去
}
	
// ]]>
</script>
