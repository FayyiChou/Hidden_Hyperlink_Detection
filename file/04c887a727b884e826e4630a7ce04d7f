<script >
$(function(){//根据商品类别判断链接高亮


    var if_current=$("#gc_id_2").val();

    if(if_current==26) {
    	  $("#album_print").addClass("current");
        $("#album").removeClass("current");
    }

    var index_sign=$("#index_sign").val();


    if(index_sign=='pointshop'&&index_sign!='0'&&if_current==undefined)
    {
        $("#album").addClass("current");
    }
    if(index_sign=='pointshop2'&&index_sign!='0'&&if_current==undefined)
    {
        $("#album_print").addClass("current");
    }


})
</script>

