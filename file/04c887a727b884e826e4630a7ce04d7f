            $("#nch-sortbar-location-home-id").css({'display':'block','left':offset.left,'position':'absolute'});

            $("#home-area").addClass("home-area-hover");
            $("#home-area").css("z-index","99101");
            //var close_left=offset.left+360;

        });
        $(".home-area-close").click(function(){
            //alert("home-area-close----1");
            $("#nch-sortbar-location-home-id").css({'display':'none'});
            $("#home-area").removeClass("home-area-hover");
            $("#home-area").css("z-index","1");
        });
    
