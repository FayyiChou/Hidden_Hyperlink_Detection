//获取购物车中的商品个数
$(function(){
	
	
  $.get("http://www.ddmeitu.com/shop/index.php?act=cart&op=hasShop",function(data,status){
    	var count = parseInt(data);
		var isShow = (count>0) ? 'block' : 'none';
		$('#new_msg_gwc').css('display',isShow);
  });
	
});
    //动画显示边条内容区域
$(function() {
	$(function() {
		$('#activator').click(function() {
			$('#content-cart').animate({'right': '-250px'});
			$('#content-compare').animate({'right': '-150px'});
			$('#ncToolbar').animate({'right': '-60px'}, 300,
			function() {
				$('#ncHideBar').animate({'right': '59px'},	300);
			});
	        $('div[nctype^="bar"]').hide();
		});
		$('#ncHideBar').click(function() {
			$('#ncHideBar').animate({
				'right': '-79px'
			},
			300,
			function() {
				$('#content-cart').animate({'right': '-250px'});
				$('#content-compare').animate({'right': '-250px'});
				$('#ncToolbar').animate({'right': '0'},300);
			});
		});
	});
    $("#compare").click(function(){
    	if ($("#content-compare").css('right') == '-210px') {
 		   loadCompare(false);
 		   $('#content-cart').animate({'right': '-210px'});
  		   $("#content-compare").animate({right:'50px'});
    	} else {
    		$(".close").click();
    		$(".chat-list").css("display",'none');
        }
	});
    $("#rtoolbar_cart").click(function(){
        if ($("#content-cart").css('right') == '-210px') {
         	$('#content-compare').animate({'right': '-210px'});
    		$("#content-cart").animate({right:'50px'});
    		if (!$("#rtoolbar_cartlist").html()) {
    			$("#rtoolbar_cartlist").load('index.php?act=cart&op=ajax_load&type=html');
    		}
        } else {
        	$(".close").click();
        	$(".chat-list").css("display",'none');
    }
	});
	
	
	$("#new_msg_member_list > div").bind("mouseover",function(){ 
	$(this).css("background-color","#e5e5e5"); 
	$(this).css("color","#333333"); 
	}) 
	$("#new_msg_member_list > div").bind("mouseout",function(){ 
	$(this).css("background-color","#ffffff");
	$(this).css("color","#A6A6A6");  
	}) 
	
            $("#home-area-c2-a").click(function(){
            var obj=$("#home-area");
