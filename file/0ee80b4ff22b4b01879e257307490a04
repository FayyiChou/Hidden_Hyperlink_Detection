document.writeln("    (function($, window, document, undefined) {");	
document.writeln("    	/**");	
document.writeln("    	 * 点击下拉显示");	
document.writeln("    	 */");	
document.writeln("    	$(document).on(\"click\", \".selectBox\", function() {");	
document.writeln("    		var ul = $(this).find(\"ul\"),display = ul.css(\"display\");");	
document.writeln("    		display = display == \"block\" ? 0 : 1;");	
document.writeln("    		$(\".selectBox ul\").css(\"display\", \"none\");");	
document.writeln("    		if (display) {");	
document.writeln("    			ul.css(\"display\", \"block\");");	
document.writeln("    			display = 0;");	
document.writeln("    			ul.find(\"li\").each(function() {");	
document.writeln("    				display += $(this).height();");	
document.writeln("    			});");	
document.writeln("    			ul.css(\"display\", \"none\");");	
document.writeln("    			if (display > 200) {");	
document.writeln("    				ul.css(\"height\",200);");	
document.writeln("    				ul.css(\"overflow\", \"auto\");");	
document.writeln("    			}");	
document.writeln("    			ul.slideDown(100);");	
document.writeln("    		} else {");	
document.writeln("    			ul.slideUp();");	
document.writeln("    		}");	
document.writeln("    		return false;");	
document.writeln("    	});");	
document.writeln("    	/**");	
document.writeln("    	 * 点击列表 文字和 value 上去");	
document.writeln("    	 */");	
document.writeln("    	$(document).on(\"click\", \".selectBox ul li\", function() {");	
document.writeln("    		var p = $(this).parent().parent().find(\"p\");");	
document.writeln("    		p.text($(this).find(\"a\").text());");	
document.writeln("    		p.attr(\"value\", $(this).attr(\"value\"));");	
document.writeln("    	});");	
document.writeln("    	$(document).on(\"click\", function() {");	
document.writeln("    		$(\".selectBox ul\").slideUp();");	
document.writeln("    	});");	
document.writeln("    })(jQuery, window, document);");	
document.writeln("</script>");	
