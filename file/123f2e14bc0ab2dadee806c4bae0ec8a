<script type="text/javascript">
$(function(){
	var newsId=$("#newsId").val();
	var userid = $.cookie('new_loginid');
	
	var flag='0';
	var islogin='0';
	var control=0;
	var locUrl='/detail/getZanCaiResult.do';

	 if(userid==null || userid=='undifined' || userid==''){
		 userid='0'; 
	 }else{
		 islogin='1';
	 }
	if(location.hostname.indexOf('www.fang.com')==0){
		locUrl="http://news.fang.com/detail/getZanCaiResult.do?userid="+userid+"&islogin="+islogin+"&newsId="+newsId+"&flag="+flag;
	}
	 $('#zanClick').click(function(){
		 if(control==0){
			 flag='1';
			 control=1;
		 }else{
			 flag='0'; 
		 }
		 if(location.hostname.indexOf('www.fang.com')==0){
				locUrl="http://news.fang.com/detail/getZanCaiResult.do?userid="+userid+"&islogin="+islogin+"&newsId="+newsId+"&flag="+flag;
		} 
		 $.ajax({
			 type: "GET",
	         url: locUrl,
	         data: {"userid":userid, 
	        	 "islogin":islogin,
	        	 "newsId":newsId,
	        	 "flag":flag
	        	},
	         success: function(data){
	        	 $('.zan-num').html(data[0]);
    			 if(newsId!=22267726){
    				 $('.cai-num').html(data[1]);
        			 $('.c-bar').css('width',data[3]);
        			 $('.z-bar').css('width',data[2]);
    			 }
    			 else{
    				 $('.cai-num').html(0);
    				 $('.c-bar').css('width',0);
    				 data[2] = '100%';
        			 $('.z-bar').css('width',data[2]);
    			 }

	         }
		});
		 
	 });
	 $('#caiClick').click(function(){
		 if(control==0){
			 flag='-1';
			 control=1;
		 }else{
			 flag='0'; 
		 }
		 if(location.hostname.indexOf('www.fang.com')==0){
				locUrl="http://news.fang.com/detail/getZanCaiResult.do?userid="+userid+"&islogin="+islogin+"&newsId="+newsId+"&flag="+flag;
		} 
		 $.ajax({
			 type: "GET",
	         url: locUrl,
	         data: {"userid":userid, 
	        	 "islogin":islogin,
	        	 "newsId":newsId,
	        	 "flag":flag
	        	},
	         success: function(data){
	        	
	        	 $('.zan-num').html(data[0]);
    			 if(newsId!=22267726){
    				 $('.cai-num').html(data[1]);
        			 $('.c-bar').css('width',data[3]);
        			 $('.z-bar').css('width',data[2]);
    			 }
    			 else{
    				 $('.cai-num').html(0);
    				 $('.c-bar').css('width',0);
    				 data[2] = '100%';
        			 $('.z-bar').css('width',data[2]);
    			 }

	         }
		});	
	 });
	 
	$.ajax({
		 type: "GET",
         url: locUrl,
         data: {"userid":userid, 
        	 "islogin":islogin,
        	 "newsId":newsId,
        	 "flag":flag
        	},
         success: function(data){
        	 
    			 $('.zan-num').html(data[0]);
    			 if(newsId!=22267726){
    				 $('.cai-num').html(data[1]);
        			 $('.c-bar').css('width',data[3]);
        			 $('.z-bar').css('width',data[2]);
    			 }
    			 else{
    				 $('.cai-num').html(0);
    				 $('.c-bar').css('width',0);
    				 data[2] = '100%';
        			 $('.z-bar').css('width',data[2]);
    			 }

         	
         }
	});
	

	
});
