<script language="javascript">
$(document).ready(function(){
	$("#tq_bottomCont > div:not(:first)").hide();
	var myLi = $("#tq_bottomTop > ul > li");
	var myDiv =$("#tq_bottomCont > ul");
	myLi.each(function(i){
		$(this).click(function(){
			myLi.removeClass("hover");
			$(this).addClass("hover");
			myDiv.hide();
			myDiv.eq(i).show();
		})
	})
})

$(function(){

$(".tabcont1 .tqshow1").click(function(){
	$("#tq24_box").show();
	$("#tq24_boxbg").show();
})

$("#moretq24").click(function(){
	$("#tq24_box").show();
	$("#tq24_boxbg").show();
})

$("#close_tq24").click(function(){
$("#tq24_box").hide();
$("#tq24_boxbg").hide(); 
});
})


$(function(){		
	$("#shzc_on").click(function(){
	$("#shzc_txt").removeClass("h345");
	$("#shzc_on").hide();
	$("#shzc_off").show();
	});
	
	$("#shzc_off").click(function(){
	$("#shzc_txt").addClass("h345");
	$("#shzc_on").show();
	$("#shzc_off").hide();
	});
})
</script>
