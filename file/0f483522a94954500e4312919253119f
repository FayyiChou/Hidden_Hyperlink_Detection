$(function(){
	$("ul#top_news_list").css("display","none");
	$("a#top_news_op").click(function(){
		if($("+ul#top_news_list",this).css("display")=="none"){
			$("+ul#top_news_list",this).slideDown("slow");
		} else {
			$(this).siblings("ul#top_news_list").slideUp("slow");
		}
	});
});
$(function(){
