    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    <script type="text/javascript">

    google.load("feeds", "1");

    function initialize() {
      var feed = new google.feeds.Feed("http://blog.balleggs.co.jp/feed");
      feed.setNumEntries(10);
      feed.load(function(result) {
        if (!result.error) {
          var container = document.getElementById("top_blog_list");
          var htmlstr = "";

          for (var i = 0; i < result.feed.entries.length; i++) {
            var entry = result.feed.entries[i];
            var NewPubDate = new Date(entry.publishedDate) ;
            yy = NewPubDate.getFullYear();
            mm = ( '0' + (NewPubDate.getMonth() + 1)).slice( -2 );
            dd = ( '0'  + NewPubDate.getDate()).slice( -2 );
            var CompPubDate = yy + "." + mm + "." + dd;

            htmlstr += '<li><span>' + CompPubDate + '</span><a href="' + entry.link + '" target="_blank">' + entry.title + '</a></li>';
//            var div = document.createElement("li");
//            div.appendChild(document.createTextNode(entry.title));
          }

          container.innerHTML = htmlstr;
        } else {
          alert(result.error.code + ":" + result.error.message);
        }
      });
    }
    google.setOnLoadCallback(initialize);

    </script>

