    function imgAuto(){         
          var v_width=$("#browse .con ul li").width();
          if(page==4){
            $("#browse .con ul").stop(true).animate({left:"0px"},300);
            page=1;       
          }else{
            $("#browse .con ul").stop(true).animate({left:'-='+v_width},300);
            page++;   
          }
          $(li).eq(page-1).addClass("high").siblings().removeClass("high");
    }
  });
