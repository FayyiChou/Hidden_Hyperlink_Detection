<script type="text/javascript"> 
    var isdrag=false;   
    var tx,x,ty,y;    
    function touchStart(e){   
       isdrag = true; 
       e.preventDefault();
       tx = parseInt($("#tagcom").css('left'));    
       //ty = parseInt($("#tagcom").css('top'));  
       x = e.touches[0].pageX;
       //y = e.touches[0].pageY;  
    }   
    function touchMove(e){   
      if (isdrag){
       e.preventDefault();
           var n = tx + e.touches[0].pageX - x;
          // var h = ty + e.touches[0].pageY - y;   
           $("#tagcom").css("left",n); 
           //$("#tagcom").css("top",h);    
       }  
    }    
</script>
