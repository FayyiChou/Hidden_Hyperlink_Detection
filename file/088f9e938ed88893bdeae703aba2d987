<script type="text/javascript">
var where=-1;
jQuery(document).ready(function(){
  jQuery("#menus ul li").each(function(index){ //遍历URL与导航条URL是否对应
    var url=jQuery(this).find("a").attr("href");
    var path=window.location.pathname;
            //设置积分俱乐部-jsp页面所在的菜单
        if(path.indexOf("/service/transaction/vip/club") >= 0 || path.indexOf("/service/jf") >= 0){
            where = 5;
        }
        //设置帮助与支持-jsp页面所在的菜单
        else if(path.indexOf("/newcmsweb/up2") >= 0){
            where = 6;
        }else{
            if(path.indexOf(url)>=0){
              where=index;
            }
        }
  } );  
  if(where!=-1){
    jQuery("#menus ul li:eq("+where+")").addClass("mdq"); //home为导航上需要高亮的LI的ID
    }
    jQuery("#menus ul li").hover(function(){
          jQuery(this).addClass("mdq").siblings().removeClass("mdq");
          },function(){
              if(where!=-1){
              jQuery("#menus ul li:eq("+where+")").addClass("mdq").siblings().removeClass("mdq");   //home为导航上需要高亮的LI的ID
              }
    });
    });  
</script>
