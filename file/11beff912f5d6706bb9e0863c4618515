function menuswap(num){
	for(var id = 0;id<=16;id++)
	{
		if(id==num)
		{
			$("#menu_con"+id).css("display","block");
			$("#mynav"+id).addClass("nav_on");
		}
		else
		{
			$("#menu_con"+id).css("display","none");
			$("#mynav"+id).removeClass("nav_on");
		}
	}
};
</script>
