                    //合作方实现newsRender接口
                    function newsRender_pic(data){
                        var html = '';	
                        html = getTpl_pic(data);
                        document.getElementById('outer_pic').innerHTML = html;
                    }
                    function getTpl_pic(data){
                        var html = '',_item = null;
                        for(var j=0,l=2;j<l;j++){
                            _item = data[j];
                            html+='<li><a href='+ decodeURIComponent(_item.url) +' target="_blank"><img src="'+ decodeURIComponent(_item.img) +'" width="130" height="90" border="0"><span>'+ _item['title'] +'</span></a></li>';

                        }
                        return html;
                    }
           </script>
