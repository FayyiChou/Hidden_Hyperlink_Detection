                                <script type="text/javascript">
    function checkbtn(id)
    {
        for(var i=1 ; i < 3 ; i++)
        {
            document.getElementById("td_0"+i).className="td_01";
            document.getElementById("table_0"+i).style.display="none";
        }
        document.getElementById("td_0"+id).className="td_02";
        document.getElementById("table_0"+id).style.display="inline";
    }
                                </script>

