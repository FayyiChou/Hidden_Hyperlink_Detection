        //当前模块色差显示
        $(function () {
            var currentPath = window.location.pathname;
            var isOk = true;//标识,符合条件跳出遍历
            $("#nav2 ul li").each(function () {
                if (isOk) {
                    var thisObject = $(this);
                    var thisUrl = thisObject.find("a").attr("href");
                    if (currentPath == "/") {//首页分开判断
                        thisObject.addClass("current");
                        isOk = false;
                    } else {
                        if (thisUrl != "/" && currentPath.indexOf(thisUrl) > -1) {//除去首页，判断是否为当前模块，符合条件则添加Current样式
                            thisObject.addClass("current");
                            isOk = false;//用于跳出循环
                        }
                    }
                }
            });
        })
