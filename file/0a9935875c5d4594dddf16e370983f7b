		chatPub2.min	= chatPub2.smin;
		chath3.min		= chath3.smin;
		allChath.min	= allChath.smin;
		onlyChath.min	= onlyChath.smin;
		userListh.min	= userListh.smin;
	}else{ // >=1400
		_extHeight = Math.max(0, _docHeight - _bodyh.min);
		if(_docWidth >= 1500){
			bodyClass = 'page-w1500';
		}else{
			bodyClass = '';
		}
	};

	window.bodyClass = bodyClass;
	_styles.push('#all_chat{height:'+(allChath[_hType]+_extHeight)+'px}');
	_styles.push('#only_chat{height:'+(onlyChath[_hType]+_extHeight)+'px}');
	_styles.push('#chat_body .watch-song, #chat_body .watch-gift, #chat_body .watch-game, #chat_body .watch-seatcar{height:'+(chath3[_hType]+_extHeight)+'px}');
	_styles.push('#watchChat_pub{height:'+(chatPub[_hType]+_extHeight)+'px}');
	_styles.push('#only_chat .talk-public{height:'+(chatPub2[_hType]+_extHeight)+'px}');
	_styles.push('#room_viewer_panel .ulist-spectator{height:'+(userListh[_hType]+_extHeight)+'px}');
	_styles.push('#watchSpectator_lianmai .ulist-spectator{height:'+(userListh[_hType]+_extHeight-32)+'px}');


	var _playHeight = Math.min( _playerSize._maxHeight, Math.max(_playerSize._minHeight, _extHeight+ gifth - 50 + _playerSize.height) ); // 100 隐藏的抢座高度
	if(page.isVertical){
		//_styles.push('#my_site{display:none} #room_gift_panel{min-height:50px}');
		_styles.push('#room_gift_panel{min-height:50px}');
		_styles.push('#player {width:' +_playerSize.width+ 'px; height:'+ _playHeight +'px}');
		_styles.push('#room_gift_panel{height:'+ (gifth+_extHeight - (_playHeight-_playerSize.height) ) +'px}');
	}else{
		_styles.push('#player {width:' +_playerSize.width+ 'px; height:'+ _playerSize.height +'px}');
		_styles.push('#room_gift_panel{height:'+ (gifth+_extHeight) +'px}');
	}

  	document.write('<style  type="text/css">'+_styles.join('')+'</style>');
})();

</script><style type="text/css">#all_chat{height:366px}#only_chat{height:366px}#chat_body .watch-song, #chat_body .watch-gift, #chat_body .watch-game, #chat_body .watch-seatcar{height:442px}#watchChat_pub{height:202px}#only_chat .talk-public{height:280px}#room_viewer_panel .ulist-spectator{height:322px}#watchSpectator_lianmai .ulist-spectator{height:290px}#player {width:530px; height:398px}#room_gift_panel{height:150px}</style>
</head>
