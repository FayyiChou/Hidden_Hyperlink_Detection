	document.getElementById("menu7").className="menunormal";
	
	document.getElementById("menu8").className="menunormal";
	
	document.getElementById("menu"+id).className="menuhover";
}
function searchtFocus(obj) 
	{
		if (obj.value == '请输入搜索内容') 
		{
			obj.value = '';
		}
	}

	function searchtBlur(obj) 
	{
		if (obj.value == '')
		 {
			obj.value = '请输入搜索内容';
		}
	}
//-->
