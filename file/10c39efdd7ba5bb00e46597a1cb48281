function Showhzmtdiv(divid)
{
	if(divid=='1')
	{
		document.getElementById("jcjxtitle1").className="gznewshover";
		document.getElementById("jcjxtitle2").className="gznewsnormal";
		document.getElementById("jcjxtitle3").className="gznewsnormal";
		document.getElementById("jcjxtitle4").className="gznewsnormal";
		document.getElementById("jcjxtab1").style.display="block";
		document.getElementById("jcjxtab2").style.display="none";
		document.getElementById("jcjxtab3").style.display="none";
		document.getElementById("jcjxtab4").style.display="none";
	}
	else if(divid=='2')
	{
		document.getElementById("jcjxtitle1").className="gznewsnormal";
		document.getElementById("jcjxtitle2").className="gznewshover";
		document.getElementById("jcjxtitle3").className="gznewsnormal";
		document.getElementById("jcjxtitle4").className="gznewsnormal";
		document.getElementById("jcjxtab1").style.display="none";
		document.getElementById("jcjxtab2").style.display="block";
		document.getElementById("jcjxtab3").style.display="none";
		document.getElementById("jcjxtab4").style.display="none";
	}
		else if(divid=='3')
	{
		document.getElementById("jcjxtitle1").className="gznewsnormal";
		document.getElementById("jcjxtitle2").className="gznewsnormal";
		document.getElementById("jcjxtitle3").className="gznewshover";
		document.getElementById("jcjxtitle4").className="gznewsnormal";
		document.getElementById("jcjxtab1").style.display="none";
		document.getElementById("jcjxtab2").style.display="none";
		document.getElementById("jcjxtab3").style.display="block";
		document.getElementById("jcjxtab4").style.display="none";
	}
	else
	{
		document.getElementById("jcjxtitle1").className="gznewsnormal";
		document.getElementById("jcjxtitle2").className="gznewsnormal";
		document.getElementById("jcjxtitle3").className="gznewsnormal";
		document.getElementById("jcjxtitle4").className="gznewshover";
		document.getElementById("jcjxtab1").style.display="none";
		document.getElementById("jcjxtab2").style.display="none";
		document.getElementById("jcjxtab3").style.display="none";
		document.getElementById("jcjxtab4").style.display="block";
	}
}
