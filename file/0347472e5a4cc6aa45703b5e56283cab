<script>
function isM(){
   var userAgentInfo = navigator.userAgent;
    var Agents = ["Android", "iPhone","SymbianOS", "Windows Phone","iPad", "iPod"];
    var flag = false;
    for (var v = 0; v < Agents.length; v++) {
        if (userAgentInfo.indexOf(Agents[v]) > 0) { flag = true;break;}
    }
    return flag;
}

function colseapptip(){
document.getElementById('btip').style.display = 'none';
setcookie('closeapptip',1,3600*12);

}

(function(){
var ismobile = isM();
var closeapptip = getcookie('closeapptip');
if(ismobile && closeapptip != 1){
document.getElementById('btip').style.display = 'block';
}else{
document.getElementById('btip').style.display = 'none';
}
})();
</script>


