    <div style="position:absolute;margin-left:-10px;margin-top:-108px"> 
     <!--   <script type="text/javascript" src="http://tg.xyxy01.com/banner.php?oid=24613" ></script> --> 
     <script type="text/javascript" src="http://js.feitian001.com/js/c/6181_367.js"></script> 
