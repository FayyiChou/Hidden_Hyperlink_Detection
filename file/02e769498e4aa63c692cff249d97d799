   <script type="text/javascript">
	
	$(document).ready(function() {
		var bar = $('.assistant-bar');
		var anchor = $('.main-anchor');
		var margin_top_org = parseInt(bar.css('margin-top').replace('px',''));
		var margin_bottom_org = parseInt(bar.css('margin-bottom').replace('px',''));
		window.onscroll = function () {
			var top = bar.offset().top;
			var bottom = bar.offset().top + bar.height();
			var view_bottom = $(document).scrollTop() + $(window).height();
			var view_top = $(document).scrollTop();
			var new_margin_top = margin_top_org;
			if (view_bottom > bottom) {
				if (view_bottom > anchor.offset().top + anchor.height()) {
					new_margin_top = anchor.height() - bar.height() + margin_top_org + margin_bottom_org;
				} else {
					new_margin_top = view_bottom - bar.height() - bar.parent().offset().top;
				}
				bar.css('margin-top', new_margin_top + 'px');
			} else if (view_top < top) {
				var min_top = bar.parent().offset().top + margin_top_org;
				if (view_top > min_top) {
					new_margin_top = view_top - bar.parent().offset().top;
				}
				bar.css('margin-top', new_margin_top + 'px');
			}
		}
	});
	
	</script> 
