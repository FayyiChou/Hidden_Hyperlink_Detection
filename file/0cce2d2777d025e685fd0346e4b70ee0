    function reloadPabAddr() {
         // 重新加载个人通讯录
        try {
            if (parent.document.getElementById("loadAddData") == null) {
                return;
            }
        } catch (e) {
            // cross domain access denied
        }
        parent.document.getElementById("loadAddData").contentWindow.location.reload();
    }

</script>
