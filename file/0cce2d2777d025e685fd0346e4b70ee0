<script type="text/javascript">
    var copyright_area=$('copyright_area')?$('copyright_area'):parent.$('copyright_area');
    if(copyright_area){
        Element.hide(copyright_area);
    }

    // fixed 当前module隐藏的问题
    if (parent.showMutiTab) {
        parent.MM.showModule(parent.GE.curModule);
    }

    function goBackHistory() {
        if (parent.showMutiTab) {
            parent.MM.closeModule(parent.GE.curModule);
        } else {
            history.back();
        }
    }
</script>

