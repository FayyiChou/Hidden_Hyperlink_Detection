<div id="vknew" style="position:absolute">
<!--链接地址--><a href="http://www.vdechina.com/DetailNoticeNew.aspx?id=31&&type=5">
<!--图片地址--><img src="/images/qzcl.jpg" border="0">
</a>
</div>
<script> 
var x = 50,y = 60 
var xin = true, yin = true 
var step = 1 
var delay = 50 
var obj=document.getElementById("vknew") 
function floatvknew() { 
var L=T=0
//by www.vknew.com
var R= document.body.clientWidth-obj.offsetWidth 
var B = document.body.clientHeight-obj.offsetHeight 
obj.style.left = x + document.body.scrollLeft 
obj.style.top = y + document.body.scrollTop 
x = x + step*(xin?1:-1) 
if (x < L) { xin = true; x = L} 
if (x > R){ xin = false; x = R} 
y = y + step*(yin?1:-1) 
if (y < T) { yin = true; y = T } 
if (y > B) { yin = false; y = B } 
} 
var itl= setInterval("floatvknew()", delay) 
obj.onmouseover=function(){clearInterval(itl)} 
obj.onmouseout=function(){itl=setInterval("floatvknew()", delay)} 
</script>
<!--图片漂浮广告代码结束-->