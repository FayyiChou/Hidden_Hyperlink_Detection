   var speed=30
   document.getElementById("dome2").innerHTML=document.getElementById("dome1").innerHTML
   function Marquee(){
   if(document.getElementById("dome2").offsetTop-document.getElementById("dome").scrollTop<=0)
   document.getElementById("dome").scrollTop-=document.getElementById("dome1").offsetHeight
   else{
   document.getElementById("dome").scrollTop++
   }
   }
   var MyMar=setInterval(Marquee,speed)
   document.getElementById("dome").onmouseover=function() {clearInterval(MyMar)}
   document.getElementById("dome").onmouseout=function() {MyMar=setInterval(Marquee,speed)}
