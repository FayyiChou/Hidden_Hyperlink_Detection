//显示加载元素（图片）
function  show(o,imgId){  
	var  m= document.getElementById(imgId)  //$("#"+imgId);
	m.style.pixelLeft=getL(o)  
	m.style.pixelTop=getT(o)+o.offsetHeight  
	m.style.visibility=''
	//m.show();
}  
//隐藏显示加载元素（图片）	 
function  hide(imgId){  
	document.getElementById(imgId).style.visibility='hidden'  
}  
