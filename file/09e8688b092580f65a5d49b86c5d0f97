    <script>
        var reg = new RegExp("(^|&)keyword=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        var keyword = "";
        if (r != null)
            keyword = decodeURI(r[2]);
        document.getElementById("txtKeyword").value = keyword;

        var url = window.location.href;
        if (url.indexOf("http://m.zhidao.189.cn/Qudao") > -1 || url.indexOf("http://m.zhidao.189.cn/qudao") > -1)
        {
            if (parent.document) {
                var iframeobj = parent.document.getElementsByTagName("iframe"); //$("body", parent.document).find(".contentFrame");
                if (iframeobj.length>0) {
                    parent.window.location.href = url;
                }
            }
        }
    </script>
    
