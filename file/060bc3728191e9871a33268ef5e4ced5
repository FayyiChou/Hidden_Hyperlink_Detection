		var xmlhttp;
		function createXMLHttpRequest() {
			try{
				xmlhttp=new ActiveXObject("Msxml2.XMLHTTP");
			}
			catch (e) {
				try{
					xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
				}
				catch(e) {
					try {
						xmlhttp=new XMLHttpRequest();
					}
					catch(e) {
						alert("failure");
					}
				}
			}
		}
		function submitFrom(userName,password){
			var url = "doWebLogin.shtml";
			$.ajax({
				type : "GET",
				url : url,
				data : {
					userName:userName,
					password:password
				},
				success : function(msg) {
					if(msg){
						var result=eval('('+msg+')');
						if(result.flag == true && result != null){
							document.getElementById("unitUserID").value=result.unitUserID;
							document.getElementById("param").value=result.param;
							document.getElementById("form1").submit();
						}else{
							alert("用户名或密码错误，请重新输入!");
							document.getElementById("userName").value='';
							document.getElementById("password").value='';
						}
					}
				}
				
			});
 		}
		
		function checkValue() {
			var userName = document.getElementById("userName").value;
			var password = document.getElementById("password").value;
			var flag = true;
			if(userName.length == 0 ){
				flag = false;
				alert("请输入用户名！");
				return flag;
			}
			if(password.length == 0 ){
				flag = false;
				alert("请输入密码！");
				return flag;
			}
			if(flag){
				submitFrom(userName,password);
			}
		}
		
		$(document).ready(function(){  
	    		
			var oDiv=document.getElementById('flimg');
			var i=-240;
			var j=-60;
			
			function move(){
				oDiv.style.left=i+'px';
				oDiv.style.top=j+'px';
				i++;
				j++;
				
				if(i==1000){
					i=-240;
					j=-60;
				}
	
			};
			
			var timer=setInterval(move,30);
			
			oDiv.onmouseover=function(){
				clearInterval(timer);
			};
			
			oDiv.onmouseout=function(){
	
				timer=setInterval(move,30);
			};

    		
		});  
		
		</script>

