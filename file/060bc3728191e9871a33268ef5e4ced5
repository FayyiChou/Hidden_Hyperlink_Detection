<script type="text/javascript">
	function sleep(mill){
		var date=new Date().getTime();
		var limit=date+mill;
		while(true){
		   date=new Date().getTime();
			if(date>limit){
			  return;
			}
		}
	}
	window.onload=function(){
		var odiv1=document.getElementById('link');
		var odiv2=document.getElementById('dt_link');
	    var tmier=null;
	  
		odiv2.onmouseover=odiv1.onmouseover=function(){
											//setTimeout( function(){odiv2.style.display='block';},600);
											sleep(000);
											odiv2.style.display='block';
										    clearInterval(tmier);
										}
		odiv2.onmouseout=odiv1.onmouseout=function(){
											tmier=setTimeout( function(){odiv2.style.display='none';},300);
										}
			
		var Obnt=document.getElementById('tab');
		var Obnt0=Obnt.getElementsByTagName('h1');
		var odiv=Obnt.getElementsByTagName('ul');
		var i=0;
		for( i=0; i<Obnt0.length; i++){      
			Obnt0[i].index=i;
			Obnt0[i].onclick=function(){ 
								for(i=0; i< Obnt0.length; i++){
									Obnt0[i].className='';
									odiv[i].style.display='none';
								}
								this.className='hot';
								odiv[this.index].style.display='block';
							}
		}
		
		var Olad=document.getElementById('lab');	
		var Olad0=document.getElementById('lad_n');
		var Olad_1=document.getElementById('lab_1');	
		var Olad0_1=document.getElementById('lad_n_1');
		Olad.onmouseover=function(){
									Olad0.style.display='block';
							}
		Olad.onmouseout=function(){
									Olad0.style.display='none';
							}	
		Olad_1.onmouseover=function(){
									Olad0_1.style.display='block';
							}
		Olad_1.onmouseout=function(){
									Olad0_1.style.display='none';
							}	
		
		//图片滚动
		var oul=document.getElementById('soll');
		var oli=oul.getElementsByTagName('span');
		var iSpeed=-1;
		oul.innerHTML+=oul.innerHTML;
		oul.style.width=oli[0].offsetWidth*oli.length+'px';  
		var timerl=null;
		function oroll(){
			oul.style.left=oul.offsetLeft+iSpeed+'px';
			if(oul.offsetLeft<-oul.offsetWidth/2){
				oul.style.left='0px';
			}
		}
		timerl=setInterval(oroll,50);
		oul.onmouseover=function(){
									clearInterval(timerl);
						}
		oul.onmouseout=function(){
									timerl=setInterval(oroll,50);
						}
		
		var bjxwRoll = document.getElementById('bjxwRoll');
		if("Y" == bjxwRoll.value){
			//文字滚动
			var oUl=document.getElementById('scroll_wenzi');
			var oA=oUl.innerHTML+=oUl.innerHTML;
			var aSpeed=-1;
			var timer=null;
			function oRoll(){ 
				oUl.style.top=oUl.offsetTop+aSpeed+'px';
				if(oUl.offsetTop<-oUl.offsetHeight/2){ 
					oUl.style.top='0px';	
				}
			}
			timer=setInterval(oRoll,50);
			oUl.onmouseover=function(){
										clearInterval(timer);
							}
			oUl.onmouseout=function() {
									timer=setInterval(oRoll,50);
							}
		}
	}
