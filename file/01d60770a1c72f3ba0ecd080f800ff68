                                        <td align="center" style="position:relative;">
                                            <div><input type="password" id="txtpwd" onkeypress="return gonext(event);" class="inputtext"></div>
                                            <input type="hidden" name="txtPassword" id="txtPassword">
                                            <div id="getYzm" class="getYzm">显示</div>
                                            <script type="text/javascript">
                                                var getYzm = document.getElementById("getYzm");
                                                var txtPassword = document.getElementById("txtPassword");
                                                var needMingWen=0;
                                                function init() {
                                                    var txtpwd = document.getElementById("txtpwd");
                                                    txtpwd.onblur = txtpwd.onchange = function () {
                                                        var str=this.value;
                                                        var newstr=str.dbc2sbc();
                                                        if(str!=newstr)this.value=newstr;
                                                        txtPassword.value = newstr;
                                                    }
                                                }
                                                function gonext(evt) {
                                                    evt = window.event || evt;
                                                    if (evt.keyCode == 13) {
                                                        var btnContinue = document.getElementById("btnContinue");
                                                        if(!btnContinue)
                                                            return false;
                                                        var txtpwd = document.getElementById("txtpwd");
                                                        if(txtpwd){
                                                            txtpwd.onchange();
                                                        }
                                                        evt.returnValue = false;
                                                        btnContinue.click(); 
                                                        return false;
                                                     }
                                                    return true;
                                                 }
                                                init();
                                                getYzm.onclick = function () {
                                                     var txtpwd = document.getElementById("txtpwd");
                                                    if (this.hasShow) {
                                                        txtpwd.parentNode.innerHTML="<input type='password'  value='"+txtpwd.value+"' id='txtpwd' class='inputtext'  onkeypress='return gonext(event);'/>";
                                                    }
                                                     else
                                                        txtpwd.parentNode.innerHTML = "<input type='text'  value='"+txtpwd.value+"' id='txtpwd' class='inputtext'  onkeypress='return gonext(event);'/>";
                                                    this.innerHTML = this.hasShow ? "显示" : "隐藏";
                                                    this.hasShow = this.hasShow ? false : true;
                                                    init();
                                                }
                                                if(needMingWen){
                                                    getYzm.style.display="none";
                                                    if(txtPassword.value){
                                                        var txtpwd = document.getElementById("txtpwd");
                                                        if(txtPassword.value)
                                                            txtpwd.value=txtPassword.value;
                                                    }
                                                    getYzm.onclick();
                                                }
                                            </script>
                                        </td>
