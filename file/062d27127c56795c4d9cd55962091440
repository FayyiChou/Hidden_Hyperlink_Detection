    }

    // 显示小秘书
    setTimeout(function(){
        if($('#xms_menulist').size() == 0 && typeof isRegPage == 'undefined' && typeof isLoginPage == 'undefined'){
             typeof xiaomishuConf !== 'undefined' ? okXiaoMiShu(xiaomishuConf) : okXiaoMiShu() ;
        }
    },1000)


    
</script>



