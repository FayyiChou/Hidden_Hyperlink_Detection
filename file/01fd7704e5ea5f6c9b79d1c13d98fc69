<script type="text/javascript">
var readFace = {
t1 : null,
t2 : null,
closeId : null,
show : function(id) {this.t1 = setTimeout(function() {getObj(id).style.display = ''; closeId=id}, 500);},
clear : function(target) {clearTimeout(this.t1); (closeId==target)&&this.t2&&clearTimeout(this.t2);},
close : function(id) {this.t2 = setTimeout(function() {getObj(id).style.display = 'none';}, 200); }
}
function ajaxPingcp(href) {
ajax.send(href,'',function(){
//	console.log(ajax.runscript(ajax.request.responseText));
/pid=(\d+)/.exec(href);
var pid = RegExp.$1;
if(pid == 0) pid = 'tpc';
 getObj('pingtable' + pid).innerHTML = ajax.runscript(ajax.request.responseText);
});
return false;
}
//贴子附件hover处理
function postAttImgHover(menuid,tdid){
var menu = getObj(menuid);
var td = getObj(menuid);
menu.style.display = '';
td.parentNode.onmouseout = menu.onmmouseout = function(e) {
menu.style.display = 'none';
};
menu.onmouseover = function() {this.style.display = '';}
}
</script>

