$("#read_tpc").html(data.html);
});
return false;
}
jQuery(".readTop .code").hover(function(){
var _this = this;
$(this).children(".bigimg").show();
$.getJSON("apps.php?q=ajax&a=urlqr&tid=4116923", {}, function(data){
$(_this).find("img").addClass("ma").attr("src",data.imgurl);
});
},function(){
$(this).children(".bigimg").hide();
});
</script>
<script>
(function(){
if(xzGlobal.isverify == 0){
$('.editor_bd').click(function(){
showVerifyBox();
return false;
});
$('.quickpost>a').click(function(){
showVerifyBox();
return false;
});
$(document).on('click','.js-floor-reply',function(){
showVerifyBox();
});
}
function showVerifyBox(){
if($('#verify_box').length >0){
$('#verify_box').show();
}else{
$('body').append('<div id="verify_box"><div onclick="$(\'#verify_box\').hide();" style="position: fixed;z-index: 1001;top: 0;left: 0;right: 0;bottom: 0;background-color: #000;opacity: .5;filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=50);"></div><div id="verify_box_body" style="width: 410px;height: auto;background-color:#fff;position: fixed;z-index: 1002;left: 50%;top: 50%;margin-top: -200px;margin-left: -205px;border-radius: 6px;font-family: Microsoft Yahei;"></div></div>');
$('#verify_box_body').html('<h2 style="padding:15px 0 0 0;text-align:center;font-size:24px;font-weight:600;color:#00CCFF;margin-bottom:4px;">温馨提示</h2><p style="padding: 0 20px; margin: 15px 0 25px;font-size: 13px;">快上车！来不及了， 经西子云检测，您的账号安全性较低，存在盗号风险，请绑定手机以提升用户安全等级！</p><p style="text-align:center;border-top: 2px solid #ccc;line-height:40px"><a href="http://my2.xizi.com/user/reward-bind" target="_blank" style="font-size:14px;display: block;">马上绑定</a></p>');
}
}
var dcha = $(".dcbm"),
total = dcha.find("dl").length,
current = 1;
dcha.find("input.dbsubmit").click(function(){
if($(this).hasClass("disabled")){
return false;
}
if($(this).hasClass("prev")){
if(current > 1){
current --;
}
}
if($(this).hasClass("next")){
if(current == total){
return true;
}
if(current < total){
current ++;
}
}
showQ(current);
return false;
});
dcha.find("dd input").click(function(){
if($(this).parents("dl").find("input:checked").length == 0){
dcha.find(".next").addClass("disabled");
}else{
dcha.find(".next").removeClass("disabled");
}
});
function showQ(n){
if(n < total){
dcha.find(".next").removeClass("disabled");
}else{
dcha.find(".prev").removeClass("disabled");
}
if(n > current){
dcha.find(".next").removeClass("disabled");
}else{
dcha.find(".prev").removeClass("disabled");
}
if(current == 1){
dcha.find(".prev").addClass("disabled");
}
if(current == total){
dcha.find(".next").val("提交");
}else{
dcha.find(".next").val("下一题");
}
var length = dcha.find("dl").eq(n - 1).find("input:checked").length;
if(length == 0){
dcha.find(".next").addClass("disabled");
}

dcha.find(".current").text(current).next(".total").text(total);
dcha.find("dl").eq(n - 1).show().siblings("dl").hide();
}
showQ(current);
})();
</script>
