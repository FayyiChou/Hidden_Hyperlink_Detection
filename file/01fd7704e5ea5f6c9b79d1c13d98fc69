<script type="text/javascript">
var getQuotings = function(object, uid, pid, username, gender) {
$.ajax({
url:'pw_ajax.php',
type:'POST',
dataType:'json',
data:{
action:'quoting',
pid:pid,
tableId:1
},
success:function(data) {
result = eval(data);
if (result.ret === 0) {
var html = '';
html += '<div class="aboutlou">';
html += '	<i></i>';
$.each(result.data, function() {
html += '	<div class="al-item cc">';
html += '<a class="img"><img src="http://bbs.xizi.com/avatar.php?uid='+ uid +'&size=middle&random=1&gender='+ gender +'"></a>';
html += '<dl>';
html += '	<dt><!--<span class="fr">*15楼*</span>--><a href="#">'+ username +'</a><span>回复 </span><a href="#">'+ $(this)[0].author +'</a><span>'+ $(this)[0].postdate +'</span>';
html += '	<a title="回复此楼" href="javascript:;" onclick="postreply(\'回 cfrank 的帖子\',\'pw_ajax.php?action=quickpost&fid=101&tid=4116923&pid='+ $(this)[0].pid +'&article=1\');" class="quick_reply">引用回复</a>';
html += '	</dt>';
html += '	<dd>'+ $(this)[0].content +'</dd>';
html += '</dl>';
html += '	</div>';
});
html += '</div>';
object.parent().parent().after(html);
object.attr('collapsed', 1);
}
},
error:function() {
object.attr('collapsed', 0);
}
});
};
$('.quotingsBtn').click(function() {
if ($(this).attr('collapsed') == 0) {
$(this).attr('collapsed', 1);
getQuotings($(this), $(this).attr('uid'), $(this).attr('pid'), $(this).attr('username'), $(this).attr('gender'));
}
else {
$(this).parent().parent().next().html('');
$(this).attr('collapsed', 0);
}
});
</script>
