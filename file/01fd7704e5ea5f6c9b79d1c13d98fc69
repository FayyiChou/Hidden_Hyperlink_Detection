<script>
seajs.use("lib/jquery", function($){
var identityBox, identityLongName, timing;
$(".identity_img").live("mouseover", function(){
var target = $(this),
offset = target.offset();
timing && clearTimeout(timing);
if(!identityBox){
$("body").append('<div id="identity_box"><div class="inner_box"><a href="xz_ucenter.php?action=identity" target="_blank"><img /></a><p class="iden_desc"></p><span class="iden_date"></span></div></div>');
identityBox = $("#identity_box");
identityBox.hover(function(){
timing && clearTimeout(timing);
}, function(){
timing = setTimeout(function(){
identityBox.hide();
}, 300);
});
}
identityBox.find("img").attr("src", target.data("src")?target.data("src"):target.attr("src"));
identityBox.find(".iden_desc").html(target.data("desc") + (target.data("num")?"，编号: <em>"+target.data("num").toString()+"</em>":""));
if(target.data("date")){
identityBox.find(".iden_date").html("身份有效期至：" + target.data("date"));
}else {
identityBox.find(".iden_date").html("");
}
identityBox.css({position:"absolute", left:offset.left, top: offset.top});
identityBox.show();
});
$(".identity_img").live("mouseout", function(e){
timing = setTimeout(function(){
identityBox.hide();
}, 300);
});

$(".identity_item").live("mouseover", function(){
var target = $(this),
detail = target.data("detail"),
offset = target.offset();
if(!detail){return;}
if(!identityLongName){
$("body").append('<i class="identity_long_name"></i>');
identityLongName = $(".identity_long_name");
identityLongName.hover(function(){
$(this).show();
}, function(){
$(this).hide();
});
}
identityLongName.text(detail).css({position:"absolute", left:offset.left + 58, top: offset.top + 6}).show();
});
$(".identity_item").live("mouseout", function(e){
identityLongName && identityLongName.hide();
});
});
//发私信
