<!--预加载 add by junver -->
<script>
$(function(){
var timing = null;
$(".fast_jump").live("mouseover", function(){
clearTimeout(timing);
showForum($(this));
});
$(".fast_jump, #fastforum").live("mouseout", function(){
hideForum();
});
$("#fastforum").live("mouseover", function(){
clearTimeout(timing);
});
function hideForum(){
timing = setTimeout(function(){
$("#fastforum").hide();
}, 300);
}
function showForum(target){
var dd = document.documentElement,
db = document.body,
offset = target.offset(),
top = offset.top,
right = offset.right,
scrollTop = dd.scrollTop || db.scrollTop,
clientH = dd.clientHeight,
h = target.outerHeight(),
w = target.outerWidth(),
curTop = "";

if(scrollTop+clientH-top-h < $("#fastforum").outerHeight()){
curTop = top-$("#fastforum").outerHeight()+1;
} else {
curTop = top+h-1;
}
$("#fastforum").css({display:"block", position:"absolute", right:right-$("#fastforum").outerWidth()+w, top:curTop});
}
});
</script>

