<script type="text/javascript">
(function(){
var hasClass = function(elem, className){
var reg = new RegExp("\s*"+className+"\s*", "ig");
if(reg.test(elem.className)){
return true;
} else {
return false;
}
}
var getClass = function(className, referElem){
var elem = (referElem || document).getElementsByTagName("*"),
i = 0,
len = elem.length,
tmpArr = [];
for(; i<len; i++){
if(hasClass(elem[i], className)){
tmpArr.push(elem[i]);
}
}
return tmpArr;
}
var addClass = function(elem, className){
if(!hasClass(elem, className)){
elem.className = elem.className + " " + className;
};
}
var removeClass = function(elem, className){
var reg = new RegExp("\s*"+className, "ig");
if(hasClass(elem, className)){
elem.className = elem.className.replace(reg, "");
}
};
var multilevel = getClass("multilevel");
for(var i=0,len=multilevel.length; i<len; i++){
multilevel[i].onmouseover = function(){
addClass(this, "hover");
this.getElementsByTagName("ul")[0].style.display = "block";
};
multilevel[i].onmouseout = function(){
removeClass(this, "hover");
this.getElementsByTagName("ul")[0].style.display = "none";
};
}
})();
</script>
<script>seajs.config({charset:"gbk"}); seajs.use("http://my.xizi.com/themes/xizi/js/modules/siteNav/siteNav.js?20150514");</script>
<script type="text/javascript">
(function(){
var hasClass = function(elem, className){
var reg = new RegExp("\s*"+className+"\s*", "ig");
if(reg.test(elem.className)){
return true;
} else {
return false;
}
}
var getClass = function(className, referElem){
var elem = (referElem || document).getElementsByTagName("*"),
i = 0,
len = elem.length,
tmpArr = [];
for(; i<len; i++){
if(hasClass(elem[i], className)){
tmpArr.push(elem[i]);
}
}
return tmpArr;
}
var addClass = function(elem, className){
if(!hasClass(elem, className)){
elem.className = elem.className + " " + className;
};
}
var removeClass = function(elem, className){
var reg = new RegExp("\s*"+className, "ig");
if(hasClass(elem, className)){
elem.className = elem.className.replace(reg, "");
}
};
var multilevel = getClass("multilevel");
for(var i=0,len=multilevel.length; i<len; i++){
multilevel[i].onmouseover = function(){
addClass(this, "hover");
this.getElementsByTagName("ul")[0].style.display = "block";
};
multilevel[i].onmouseout = function(){
removeClass(this, "hover");
this.getElementsByTagName("ul")[0].style.display = "none";
};
}
})();
</script>
