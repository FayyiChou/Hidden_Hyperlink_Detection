<script type="text/javascript">

    function selectedToolbar(hlk, sIdx)
    {
        var td = document.getElementById("tbtd" + sIdx);
        if (td != null)
        {
            td.style.backgroundImage = "url('img/style2/mnuT_c1.gif')";
            td.previousSibling.style.backgroundImage = "url('img/style2/mnuT_l1.gif')";
            td.nextSibling.style.backgroundImage = "url('img/style2/mnuT_r1.gif')";
        }
    }

    function onover(obj)
    {
        obj.style.color = "#cc0000";
    }

    function onout(obj)
    {
        obj.style.color = "#606060";
    }


</script>

