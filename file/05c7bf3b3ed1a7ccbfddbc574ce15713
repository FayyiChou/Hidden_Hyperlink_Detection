        function openNews(newID)
        {
            var url = "dt_Show.aspx?did=" + newID;
            openWindow(url);
        }

        function openWindow(url)
        {
            var fT = 1;                             //Y
            var fH = window.screen.availHeight - 40;  //高度
            var fW = 1024;                           //宽度
            if (window.screen.availWidth < 800)
                fW = 600;
            else if (window.screen.availWidth == 800)
                fW = 800;
            var fL = (window.screen.availWidth - fW) / 2 - 5;   //X
            window.open(url, "", "width=" + fW + ",height=" + fH + ",top=" + fT + ",left=" + fL + ",location=0,scrollbars=yes");
        }
        
