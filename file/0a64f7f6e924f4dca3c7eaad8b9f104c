	<script type="text/javascript" language="javascript">
		//初始化申请工作层
		$(function() {
			$("#applyJob").dialog("destroy");
			$("#applyJob").dialog({title:'申请工作',width:506,autoOpen:false,modal:true,resizable:false});
		});
		var xhr;
		function createXHR(){
			if (window.XMLHttpRequest) {
				xhr = new XMLHttpRequest();
			} else if (window.ActiveXObject) {
				try {
					xhr = new ActiveXObject("Microsoft.XMLHTTP");
				} catch(e){
					try {
						xhr = new ActiveXObject("Msxml2.XMLHTTP");
					} catch (e2) {
						alert("您的浏览器有问题！");
					}
				}
			}
		}
		//申请前的准备工作，如加载子属单位
		function toApply(bulletinId,unitName,objId,zppc){
			
			var userId='';
			if(userId==""){
				$.alert("请先登录，登录后才能申请工作");
				return;
			}
			var confirmFlag='';
			if(confirmFlag!="1"){
				$.alert("简历未确认，确认后方可申请工作");
				return;
			}
			var timeApply=false;
			if(timeApply!=true){
				$.alert("您的毕业时间不在本次招聘范围内");
				return;
			}
			var max_intention='';//意向数量
			for(var j=1;j<=max_intention;j++){
			$("#yx"+j).empty();
			}
			createXHR();
			var url = "/member/ApplyAction.do?method=beforeApply&bulletinId="+bulletinId+"&objId="+objId+"&zppc="+zppc;
			url=encodeURI(url);
			xhr.onreadystatechange = function (){
			  if(xhr.readyState == 4){
			     if(xhr.status == 200){
				     var content = xhr.responseText;
				     if(content!="success"){
				     	if(content=="notFitApplyCondition"){
					     	$.alert("对不起，你的学习经历不符合应聘条件");
					     }else if(content=="appliedThisUnit"){
					     	$.alert("你已经申请过发布该公告的单位");
					     }else if(content=="appliedThis"){
					     	$.alert("对于该公告你已经申请了工作");
					     }else{
					     	$.alert("最多只能报名"+content+"家网省公司");
					     }
					     return;
				     }else{
						$("#objId").attr("value",objId);
						$("#bulletinId").attr("value",bulletinId);
						$("#unitName").attr("value",unitName);
						$("#showUnitName").attr("innerHTML",unitName);
						
						$("#zppc1").attr("value",zppc);
						
						createXHR();
						var url = "/member/BulletinAction.do?method=getIntentions&objId="+objId+"&zppc="+zppc;;
						url=encodeURI(url);
						xhr.onreadystatechange = function (){
						  if(xhr.readyState == 4){
						     if(xhr.status == 200){
							    var content = xhr.responseText;
							    var max_intention='';//意向数量
							    if(content!=""){
							    	
                                  var aaa=content.split("###");
							    	
							    	
							    	
							    	content=aaa[0];
							    	var unitArr=content.split(";");
							    	//填充各意向下拉菜单数据
							    	for(var j=1;j<=max_intention;j++){
							    		$("#yx"+j).empty();
							    		$("#yx"+j).append("<option value=''>请选择</option>");
							    		for(var i=0;i<unitArr.length;i++){
								 			var perUnit=unitArr[i].split(",");
								 			if(perUnit[1]!=null&&perUnit[1]!=""){
								 				$("#yx"+j).append("<option value='"+perUnit[0]+"'>"+perUnit[1]+"</option>");
								 			}
								 		}
							    	}
							    }else{
							    	
							    	for(var j=1;j<=max_intention;j++){
							    		$("#yx"+j).empty();
							    		$("#yx"+j).append("<option value=''>请选择</option>");
							    		$("#yx"+j).hide();
							    		$("#itend").hide();
							    	}
							    }
							    // 填充志愿顺序下拉菜单
						    	var max_unit='';//志愿数量(可申报数量)
						    	$("#unit_rank").empty();
						    	$("#unit_rank").append("<option value=''>请选择</option>");
						    	for(var j=1;j<=max_unit;j++){
						    		$("#unit_rank").append("<option value='"+j+"'>第 "+j+" 志愿</option>");
						    	 }
						    	//session中存储的志愿顺序
						    	
						    	
// 						    	var unitRanks='';
// 						    	var rank=unitRanks.split(',');
// 						    	for(var m=0;m<rank.length;m++){
// 						    		if(rank[m]!=""){
// 						    			$("#unit_rank option[value='"+rank[m]+"']").remove();
// 						    		}
// 						    	}
						    	//在当前页申请工作后存储的志愿顺序
						    	 var page_rank=aaa[1];
						    	//var page_rank=rankeds.split(',');
						    	for(var n=0;n<page_rank.length;n++){
						    		if(page_rank[n]!=""){
						    			$("#unit_rank option[value='"+page_rank[n]+"']").remove();
						    		}
						    	}
						     }
						  }
						}
					   xhr.open("GET",url,true);
					   xhr.setRequestHeader("If-Modified-Since","0");
					   xhr.send(null);
					   $('#applyJob').dialog('open');//打开申请工作层
					   $("#img_toapply").attr("style","cursor:pointer;display:view;");
					   $("#img_can_notapply").attr("style","display:none;");
					 }
			     }
			  }
		   }
		   xhr.open("GET",url,false);
		   xhr.setRequestHeader("If-Modified-Since","0");
		   xhr.send(null);
		}
		//申报工作前，给出申报提示
		function warnForApply(){
			if(checkInten()){
				$.alert("所选意向不能重复");
			}else{
				var unit_rank=$('#unit_rank').val();
				if(unit_rank!=""){
					createXHR();
					var url = "/member/ApplyAction.do?method=isChooseThisWish&unit_rank="+unit_rank;
					url=encodeURI(url);
					xhr.onreadystatechange = function (){
					  if(xhr.readyState == 4){
					     if(xhr.status == 200){
						    var resText = xhr.responseText;
						    if(resText=="choosed"){
						    	$.alert("你已经选择过该志愿顺序");
						    }else{
						    	$.confirm("确认报名后，对应的志愿、意向单位和志愿顺序不能进行修改，请慎重考虑。如需调整志愿，请咨询已报名招聘单位。是否确认报名？",function(){
									$("#img_toapply").attr("style","display:none;");
									$("#img_can_notapply").attr("style","cursor:pointer;display:view;");
									apply();
								});
						    }
					     }
					  }
					}
				    xhr.open("GET",url,true);
				    xhr.setRequestHeader("If-Modified-Since","0");
				    xhr.send(null);
			   	}else{
			        $.alert("请选择志愿顺序");
			   	}
			}
		}
		//判断所选择意向是否重复
		function checkInten(){
			var intenRepeat=false;//默认不重复
			var max_intention='';//意向数量
			for(var j=1;j<=max_intention;j++){
				var yx=$('#yx'+j)[0];
				if(yx.value!=""){
					for(var j=1;j<=max_intention;j++){
						var reYx=$('#yx'+j)[0];
						if(yx.id!=reYx.id){
							if(yx.value==reYx.value){
								intenRepeat=true;//所选意向有重复
							}
						}
					}
				}
			}
			return intenRepeat;
		}
		var rankeds="";//已申请的志愿顺序
		//申报工作
		function apply(){
			var zppc=$('#zppc1').val();//招聘批次
			
			var objId=$('#objId').val();//单位ID
			var bulletinId=$('#bulletinId').val();//公告ID
			var unitName=$('#unitName').val();//单位名称
			var unit_rank=$('#unit_rank').val();//志愿顺序
			//是否同意网省调剂，单选按钮获取选中的值
			var itemradio =$("input[name=z00hrsftywstj]");
		    var adjustFlag = '';
		    for(i=0;i<itemradio.length;i++){
		        if(itemradio[i].checked){
		            adjustFlag = itemradio[i].value;
		        }
		    }
			createXHR();
			var url = "/member/ApplyAction.do?method=apply&bulletinId="+bulletinId+"&unitName="+
			unitName+"&objId="+objId+"&adjustFlag="+adjustFlag+"&unit_rank="+unit_rank+"&zppc="+zppc;
			var max_intention='';//意向数量
			for(var j=1;j<=max_intention;j++){
				var yx=$('#yx'+j).val();
				url=url+"&yx"+j+"="+yx;//传递各意向单位编码
			}
			url=encodeURI(url);
			xhr.onreadystatechange = function (){
			  if(xhr.readyState == 4){
			     if(xhr.status == 200){
				    $('#applyJob').dialog('close');//关闭“我的申请”层
			     	$("#appliedNum").attr("innerHTML",parseInt('')+1);
			     	rankeds+=unit_rank+",";
			     	$.alert("工作申请成功！");
			     }
			  }
			}
		   xhr.open("GET",url,true);
		   xhr.setRequestHeader("If-Modified-Since","0");
		   xhr.send(null);
		}
		//查看公司详情
		function goUnit(unitId){
			var otype='1';
			createXHR();
			var url = "/html/che_subunit/" + unitId;
			url=encodeURI(url);
			xhr.onreadystatechange = function (){
			  if(xhr.readyState == 4){
			     if(xhr.status == 200){
				    var content = xhr.responseText;
				    if(content=="have"){
				    	otype='1';
				    }else{
				    	otype='0';
				    }
			     }
			  }
			}
		    xhr.open("GET",url,false);
		    xhr.setRequestHeader("If-Modified-Since","0");
		    xhr.send(null);
		    
			var url = "/html/unit/"+unitId+"/"+otype;
			window.open(url,'_blank');
		}
	</script>
	
