    <script type="text/javascript">

        function ShowContent_social(d, content) {
            document.getElementById("lblContentHover").innerHTML = content;
            if (d.length < 1) { return; }
            var dd = document.getElementById(d);
            dd.style.display = "block";
            AssignPos(dd);
        }

        function AssignPos(d) {
            if (self.pageYOffset) {  // all except Explorer
                rX = self.pageXOffset;
                rY = self.pageYOffset;
                oH = window.innerHeight;
            }
            else if (document.documentElement && document.documentElement.scrollTop) { // Explorer 6 Strict                
                rX = document.documentElement.scrollLeft;
                rY = document.documentElement.scrollTop;
                oH = eval(document.documentElement.clientHeight);
            }
            else if (document.body) {  // all other Explorers            
                rX = document.body.scrollLeft;
                rY = document.body.scrollTop;
                oH = eval(document.body.clientHeight);
            }

            if (document.all) {
                cX += rX;
                cY += rY;
            }

            d.style.left = eval(cX - (d.offsetWidth / 2)) + "px";
            d.style.top = (cY + 15) + "px";

            if (eval(rY + oH) - eval(cY + 15 + d.offsetHeight) < eval(d.offsetHeight)) {
                d.style.top = eval((cY - 15 - d.offsetHeight)) + "px";
            }

        }

    </script>   
    
   

