<script type="text/javascript">
    $(document).ready(function () {
        // Get a reference to the container.
        var container = $("#currencylist");

        $("#currencyselect").click(function (e) {

            
            if (container.is(":visible")) {
                // Hide - slide up.
                container.hide();
                $(this).removeClass("langselect_active");
                e.stopPropagation();
            } else {
                // Show - slide down.
                container.show();
                $(this).addClass("langselect_active");

                $("html").click(function () {
                    if (container.hasClass("block")) {
                        container.show();
                    }
                    else {
                        container.hide();
                        $("#currencyselect").removeClass("langselect_active");
                    }
                });
            }
            
        });


        $("#currencyselect").mouseover(function () {
            container.addClass("block");
        }).mouseout(function () {
            container.removeClass("block");
        });
    });

    function selectCurrency(currencyCode) {
        //Assign new selected currency code            
        $('#CurrDropdown').text(currencyCode);

        //showCurrency(false);
        //do post back to  a page.
        var btnCurrencyChange = document.getElementById("ctl00_ddlCurrency_btnCurrencyChange");
        var hidCurrencyChange = document.getElementById("ctl00_ddlCurrency_hidCurrencyChange");

        hidCurrencyChange.value = currencyCode;
        btnCurrencyChange.click();
    }
</script>




<div id="ctl00_ddlCurrency_pnAB3072">
		
<script type="text/javascript">
    function selectCurrencyAB3072(currencyCode, id){

        var hidCurrencySelected = $('#'+'ctl00_ddlCurrency_hidCurrencySelected').val(id);

        selectCurrency(currencyCode);
    }
</script>

