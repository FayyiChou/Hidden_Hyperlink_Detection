        <script type="text/javascript">
            function loadIframe(iframeName, url) {
                var $iframe = $('#' + iframeName);
                if ( $iframe.length ) {
                    $iframe.attr('src',url);   
                    return false;
                }
                return true;
            }
    
            // Show/Hide SignIn overlay panel.
            $(document).ready(function(){
                var clickedSignin = false;
                $("#rewardsDropDown").click(function(e){
                    if ($(".signinsignuplabel").is(":visible")) {
                        $("#currencylist").hide();
                        $("#Lang2794Div").hide();
                        clickedSignin = true;
                        if($(".signinOnTop").hasClass("hide")){
                            $('.signinOnTop').removeClass("hide");
                            var iframeUrl = $("div.signinOnTop").data().url;
                            var iframeUrlSrc = $("#frmsignin").attr("src");
                            if(!iframeUrlSrc && iframeUrlSrc.length == 0){
                                e.stopPropagation();
                                $("#frmsignin").attr("src", iframeUrl);
                                loadIframe('frmsignin','https://www.agoda.com/zh-cn/rewards/signinv2.html')
                            }
                        }
                        else {
                            $('.signinOnTop').addClass("hide"); 
                        }                    
                    }
                });
                
                $('body').click(function(){
                    if(!clickedSignin) {
                        $('.signinOnTop').addClass("hide"); 
                    }
                    clickedSignin = false;
                });
            });
        </script>
    


	<script language="javascript" type="text/javascript">

	    var tb_closeText = "Close Window";

	    //Some pages(page_map.aspx) thick_box is not in the UpdatePanel so that it doesn't need to call tb_init again when async postback.
	    var require_tb_init_again = true;
	    //Fixed the thickbox doesn't work when it's under the UpdatePanel 
	    function pageLoad() {
	        try {
	            var isAsyncPostback = Sys.WebForms.PageRequestManager.getInstance().get_isInAsyncPostBack();

	            if (isAsyncPostback) {
	                if (require_tb_init_again) {
	                    tb_init('a.thickbox, area.thickbox, input.thickbox');
	                    $(".thickbox").attr("style", "display:inline;");
	                }

	                if (enableToolTip && $.isFunction(enableToolTip)) {
	                    enableToolTip();
	                }
	            }
	        } catch (e) { }
	    }

	    $(document).ready(function () {
	        try {
	            $(".thickbox").attr("style", "display:inline;");
	        } catch (e) { }
	    });
	</script>

