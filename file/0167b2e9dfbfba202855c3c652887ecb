    $(document).ready(function () {
        var view_state_field = $("input[id=__VIEWSTATE][type=hidden]");

        if (view_state_field != null) {
            var page_type_id = "641";
            var view_state_length = view_state_field.val().length;

            if (page_type_id != null &&
                view_state_length != null &&
                view_state_length > 0) {
                $.ajax({
                    url: "//www.agoda.com/handlers/ViewStateDataTrackingAB3908.ashx",
                    type: "post", async: true, dataType: "json", timeout: 30000,
                    data: { "view_state_length": view_state_length, "page_type_id": page_type_id },
                    error: function (textStatus, errorThrown) {
                        //alert("An error has occurred making the request: " + textStatus);
                    },
                    success: function (data, textstatus) {
                    }
                });
            }
        }
    });
</script>


