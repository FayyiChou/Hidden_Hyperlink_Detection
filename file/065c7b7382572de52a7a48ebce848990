        function resetSendButtonStyle() {
            phoneNumber = $.trim($("#set_pass_cellphone").val());
            if (checkPhoneNumber(phoneNumber) && !isSendReset ) {
                $(".reset_sendverify").css("background", "#4397FF");
                $(".reset_sendverify").css("color", "#FFF");
            } else {
                $(".reset_sendverify").css("background", "4397FF");
            }
        }

        //验证手机号
        function checkPhoneNumber(phoneNumber) {
            if (phoneNumber == '') return false;
            if (!phonePreg.test(phoneNumber)) return false;
            //if(remainNumber >0) return false;
            return true;
        }


        $('#sendVerfiyToPhone').click(function () {
            $('#reg_info').html('');
            var mapcode = $.trim($('#pgverify').val());
            if (mapcode.length != 4) {
                $('#reg_info').html('验证码错误，请重新输入！');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }else{
                sendVerfiyToPhone();
            }
        })

        $('#resetSendVerfiyToPhone').click(function () {
            $('#set_return_info').html('');
            if (checkPhoneNum($('#set_pass_cellphone').val()) != false) {
                resetSendVerfiyToPhone();
            } else {
                $('#set_return_info').html('手机号码格式不正确！');
                return;
            }
        })
        $('#reset_pass').click(function () {
            geToNextSetupPassword();
        })
        //点击发送按钮(登录验证 )
        function loginSendVerfiyToPhone(){
            if(!isVerify || isSendLogin){
                return;
            }
            ////数据包装
            var phone_number = $('#cl_input_username').val();
            var childaccount = $('#cl_input_childaccount').val();
            if(phone_number != '' && isVerify){
                var jsonData = {};
                jsonData['phone_number'] = phone_number;
                jsonData['childphone'] = childaccount;
                jsonData['platform'] = 'web';
                var dataSend = {};
                dataSend['data'] = jsonData;
                $.post("/sqe.php?s=/AccountSeller/send_login_sms",dataSend, function (data) {
                    if (data.status == 1) {
                        isSendLogin = true;
                        remainNumber = remainSetting;//设置倒计时时间
                        //发送成功  执行倒计时等操作
                        $(".login_time").show();
                        $(".login_sendverify").css("color", "#FCFCFC");
                        $(".login_sendverify").css("background", "#E6E6E6").html("重新发送验证码");
                        settingTimerText('#login_remainNumber', '.login_time', '.login_sendverify');
                        $("#login_info").html('').hide();
                    } else{
                        $('#login_info').html(data.info).show();
                        return false;
                    }
                }, 'json');
            }
        }
        //点击发送按钮
        function resetSendVerfiyToPhone() {
            if(isSendReset){
                return;
            }
            //  codeNumber  =  checkVerifyCode('reset_pass_verfiyCode','reset_pass_verfiyCode_info');
            var cellphoneNumber = $('#set_pass_cellphone').val();
            var checkNumber = checkPhoneNum(cellphoneNumber);

            if (checkNumber == false) {
                $('#set_return_info').html('手机号码格式不正确！');
                return;
            }
            var mapcode = $.trim($('#fpverify').val());
            if (mapcode.length != 4) {
                $('#set_return_info').html('验证码错误，请重新输入！').show();
                return;
            }
            ///数据包装
            var jsonData = {};
            jsonData['phone_number'] = cellphoneNumber;
            jsonData['mapcode'] = mapcode;
            jsonData['platform'] = 'web';
            jsonData['num'] = Math.random();
            var dataSend = {};
            dataSend['data'] = jsonData;
            $.post("/sqe.php?s=/AccountSeller/ajax_send_forget_sms_code",dataSend, function (data) {
                if (data.status == 1) {
                    isSendReset = true;
                    remainNumber = remainSetting;//设置倒计时时间
                    //发送成功  执行倒计时等操作
                    $(".reset_time").show();
                    $(".reset_sendverify").css("color", "#FCFCFC");
                    $(".reset_sendverify").css("background", "#E6E6E6").html("重新发送验证码");
                    settingTimerText('#reset_remainNumber', '.reset_time', '.reset_sendverify');
                } else{
                    $('#set_return_info').html(data.info);
                }
            }, 'json');
        }

        //点击发送按钮
        function sendVerfiyToPhone() {
            if(isSendReg){
                return;
            }
            var phoneNumber = $('#phoneNumber').val();
            var pgverify = $('#pgverify').val();
            if (!checkPhoneNumber(phoneNumber)) {
                return;
            }
            ///数据包装
            var jsonData = {};
            jsonData['phone_number'] = phoneNumber;
            jsonData['verify'] = pgverify;
            jsonData['platform'] = 'web';
            jsonData['num'] = Math.random();
            var dataSend = {};
            dataSend['data'] = jsonData;
            $.post(
                    "/sqe.php?s=/AccountSeller/ajax_send_sms_code",
                    dataSend,
                    function (data) {
                        if (data.status == 1) {
                            isSendReg = true;
                            remainNumber = remainSetting;//设置倒计时时间
                            //发送成功  执行倒计时等操作
                            $(".reg_time").show();
                            $(".reg_sendverify").css("color", "#FFFFFF");
                            $(".reg_sendverify").css("background", "#E6E6E6").html("重新发送验证码");
                            // clickNumber--;
                            settingTimerText('#reg_remainNumber', '.reg_time', '.reg_sendverify');

                            //alert('success');
                        } else {
                            // clickNumber--;
                            $('#reg_info').html(data.info);
                        }
                    }, "json");
            // }
        }

        //点击下一步
        function geToNextSetupPassword() {
            $('#set_return_info').hide();

            if ($.trim($('#set_pass_cellphone').val()).length <= 0) {
                $('#set_return_info').html('手机号不能为空！').show();
                return;
            }
            if($.trim($('#fpverify').val()).length <= 0)
            {
                $('#set_return_info').html('验证码错误，请重新输入！').show();
                return;
            }
            if ($.trim($('#reset_pass_verfiyCode').val()).length <= 0 || isNaN($('#reset_pass_verfiyCode').val())) {
                $('#set_return_info').html('短信验证码错误，请重新输入！').show();
                return;
            }
            // if(clickNumber<=0){
            var cellphoneNumber = $.trim($('#set_pass_cellphone').val());
            var checkNumber = checkPhoneNum(cellphoneNumber);
            var codeNumber = checkverifyCode2($.trim($('#reset_pass_verfiyCode').val()));

            if (checkNumber == false) {
                $('#set_return_info').html('手机号码格式不正确！').show();
                return;
            }

            if (codeNumber == false) {
                $('#set_return_info').html('短信验证码不正确！').show();
                return;
            }

            var phoneNumber = $.trim($('#set_pass_cellphone').val());
            verfiyCode = $.trim($('#reset_pass_verfiyCode').val());
            var code = $.trim($('#fpverify').val()); //#modify huangtian 添加图形验证码验证
            //clickNumber++;
            $('#set_return_info').html('<img src="https://public.chuchutong.com/publicdwxkpc/SQE/img/loading.gif"/>').show();
            ////数据包装
            var jsonData = {};
            jsonData['phone_number'] = phoneNumber;
            jsonData['verify'] = code;
            jsonData['sms_code'] = verfiyCode;
            jsonData['platform'] = 'web';
            jsonData['num'] = Math.random();
            var dataSend = {};
            dataSend['data'] = jsonData;
            $.post("/sqe.php?s=/AccountSeller/ajax_verify_sms_code", dataSend, function (data) {
                if (data.status == 1) {
                    //跳转
                    $('#reset_cellphone').val(phoneNumber);
                    $('#set_return_info').html('手机号验证成功，正在为你跳转...').show();
                    setTimeout(function () {
                        $("#backPwd").hide();
                        $('#updatePwd').show();
                    }, 1000);
                }else {
                    $("#set_return_info").html(data.info).show();
                }
            }, 'json');
            // }
        }

        //设置倒计时文本内容
        function settingTimerText(remainObj, timeObj, sendObj, buttonSendObj) {
            if (remainNumber > 0) {
                $(timeObj).show();
                remainNumber--;
                $(remainObj).html(remainNumber);
                setTimeout(function () {
                    settingTimerText(remainObj, timeObj, sendObj, buttonSendObj);
                }, 1000);
            } else {
                $(timeObj).hide();
                $(sendObj).css("background", "#4397FF");
                $(sendObj).css("color", "#FDF9F9");
                if(remainObj == '#login_remainNumber'){
                    isSendLogin = false;
                }else if(remainObj == '#reset_remainNumber'){
                    isSendReset = false;
                }else if(remainObj == '#reg_remainNumber'){
                    isSendReg = false;
                }
                //alert("时间到");#4397FF
            }
        }

        //弹窗显示错误信息
        function ArtDialogView(message, timeLength) {
            var timeLength = timeLength || 5;
            art.dialog({content: message, lock: true, time: timeLength});
        }

        function settingReadProtocol() {
            if ($("#protocolCheck").is(":checked")) {
                //alert(1);
                $("#protocolCheck").removeAttr("checked");
                protocolStatus = false;
            } else {
                //alert(2);
                $("#protocolCheck").attr("checked", 'checked');
                protocolStatus = true;
            }
        }

        function submitRset() {
            $('#reset_return_info').html('');
            var phoneNumber = $('#reset_cellphone').val();
            var password = $('#reset_passsword').val();
            var comfrim_password = $('#comfrim_password').val();
            var psdGrade = checkPasswordGrade(password);
            if(psdGrade < 2){
                $('#reset_return_info').html("<font color='red' >密码强度等级较低，请重新设置！</font>");
                return false;
            }
            if (password != comfrim_password){
                $('#reset_return_info').html("<font color='red' >两次密码不相等，请重新输入</font>");
                return false;
            }
            if (!checkPhoneNum(phoneNumber)){
                return false;
            }
            if (!checkPassword2(password)){
                return false;
            }
            $('#reset_return_info').css({"font-size":"12px","color":"red"});
            $('#reset_return_info').html('<img src="https://public.chuchutong.com/publicdwxkpc/SQE/img/loading.gif"/>').show();
            password = $.md5(password);
            ///包装数据
            var jsonData = {};
            jsonData['phone_number'] = phoneNumber;
            jsonData['password'] = password;
            jsonData['sms_code'] = verfiyCode;
            jsonData['platform'] = 'web';
            jsonData['num'] = Math.random();
            var dataSend = {};
            dataSend['data'] = jsonData;
            $.post("/sqe.php?s=/AccountSeller/ajax_reset_password",dataSend, function (data) {
                if (data.status == 1) {
                    $('#reset_return_info').html("密码修改成功，正在为你跳转...");
                    if (data.info== 1) {
                        location.href = "/sqe.php?s=/User/home";
                    } else {
                        setTimeout(function () {
                            location.reload();
                        }, 1000);
                    }
                } else if(data.status == 169){
                    $('#reset_return_info').html(data.info);
                    setTimeout(function () {
                        location.reload();
                    }, 2000);
                }else{
                    $('#reset_return_info').html(data.info);
                }
            }, 'json');
        }

        $(function () {
            var base64 = new Base64();
            var user_model_data = eval("(" + base64.decode('eyJ1c2VyX2lkIjotMSwibmlja19uYW1lIjoiXHU2NzJhXHU3NjdiXHU1ZjU1XHU3NTI4XHU2MjM3IiwicGhvbmVfbnVtYmVyIjoiIn0=') + ")");
