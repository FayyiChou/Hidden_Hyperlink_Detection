                }
            });

            checkPassEqual('comfrim_password', 'comfrim_password_info', 'reset_passsword');
            checkPassEqual('reg_comfrim_password', 'reg_comfrim_password_info', 'password');
            $('.enter-login').hide();
            if (!current_user.is_anonymous()) {
                $('#img_user_head_image').attr('src', current_user.head_image_url);
                $('#user_nick_name').html(current_user.nick_name);
                //$('#user_nick_name').attr('href', "/sqe.php?s=/CCKItem/viewCCKItemList");
                $('#loginInfo').removeClass('Nodisplay_index');
            } else {
                $('#loginInfo').empty();
                var str = ' <p  class="top_right"><a href="javascript:;" class="button lg" >登录</a></p>';
                $('#loginInfo').html(str);
                $('#loginInfo').removeClass('Nodisplay_index');
            };



            // 处理其他系统跳转过来的登录 直接 跳出登录框 登录成功后跳转回去
            var redirect_uri = window.location.hash.split('=')[1];
            if (redirect_uri != undefined && redirect_uri != '' && redirect_uri.match(/^https?\:\/\/([a-zA-Z0-9\-]{0,32}\.){0,3}chuchujie\.com(:8080){0,1}(\/|$)/)) {
                if (current_user.is_anonymous()) {
                    $('#redirect_uri').val(redirect_uri);
                    $("._cl_input").val("");
                    $("#dl-a").click();
                    $("#IDlogin").show();
                    loginBoxInitialize();
                } else {
                    window.location.href = redirect_uri;
                }
            }

        })
        function loginBoxInitialize(){
            $('#set_return_info,#cl_input_childaccount,#login_info,#cl_input_username,#cl_input_password, #sms_code,#loginverifySmsCode,#verify').val('');
            //$("#loginbtn").attr("disabled", true);
            $("#login_info").html('');
            $('#code_li').remove();
            $('#sms_li').remove();
            $('#sms_p').remove();
        }
        function ComeAtOnce() {
            $('#login_type').val(2);
            $('#register').attr('href', "/sqe.php?s=/User/reg_by_phone/g_t/2");
            if (!current_user.is_anonymous()) {
                if (current_user.shop === false) {
                    location.href = '/sqe.php?s=/Shop/view_businessChoose';
                } else {
                    location.href = '/sqe.php?s=/Shop/openzhinan';
                }
            } else {

                $("._cl_input").val("");
                $("#dl-a").click();
                $("#IDlogin").show();
                loginBoxInitialize();
            }
        }

