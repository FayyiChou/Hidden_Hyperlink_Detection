        /**
         * getCookie
         * @param name
         * @returns 对应cookie值，没有返回null
         */
        function getCookie(name) {
            var reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)"),
                    r = document.cookie.match(reg);
            return r ? decodeURIComponent(r[2]) : null;
        }

        /**
         * setCookie
         * @param name
         * @param value
         * @param days
         */
        function setCookie (name, value, days) {
            var d = new Date();
            d.setTime(d.getTime() + (days * 24 * 3600 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = name + "=" + value + ";path=/;domain=.daweixinke.com;" + expires;
        }



        /////轮播图
        $(function(){
            var clintW = document.documentElement.clientWidth || document.body.clientWidth;

            if($('#bgLoad').height() == 0){
                $('#bgLoad').on('load',function(){
                    var imgH = $('#bgLoad').height();
                    var imgs = $('.banner-tit  a');
                    /*$('.banner .banner-pic li').css('width',clintW);
                     $('.ban_con').css({'width':clintW,'height':imgH});
                     $('.banner-pic').css({'width':4*clintW,'height':imgH});*/

                    var step = 0;
                    var len = $('.banner-pic li').length -1;
                    var timer = setInterval(autoMove,3500);
                    var Lis = $('.banner-pic li');
                    Lis.eq(0).find('p').toggleClass('animate-bounce-up');
                    function autoMove(){
                        if(step == len){
                            step = 0;
                            $('.banner .banner-pic').css('left',-clintW*step);
                            Lis.eq(step).toggleClass('animate-bounce-up');
                        }
                        step++;
                        $('.banner .banner-pic').stop().animate({'left':-clintW*step},500,function(){
                            $.each(Lis, function(index,item) {
                                $(Lis.eq(index).find('p').removeClass('animate-bounce-up'));
                                if(index == step){
                                    $(this).find('p').addClass('animate-bounce-up');
                                }
                            });
                        });
                        focus();
                    }
                    function focus(){
                        var temp = step == len?0:step;
                        $.each(imgs, function(index,item) {
                            if(index == temp){
                                $(this).addClass('active').siblings().removeClass('active');
                            }
                        });
                    }


                    $.each(imgs, function(index,item) {
                        $(this).mouseover(function(){
                            step = index;
                            $('.banner .banner-pic').stop().animate({'left':-clintW*step},500);
                            focus();
                            clearInterval(timer);
                        }).mouseout(function(){
                            timer = setInterval(autoMove,3500);
                        })
                    });
                });
            }else{
                var imgH = $('#bgLoad').height();
                var imgs = $('.banner-tit  a');
                /*$('.banner .banner-pic li').css('width',clintW);
                 $('.ban_con').css({'width':clintW,'height':imgH});
                 $('.banner-pic').css({'width':4*clintW,'height':imgH});*/
                var step = 0;
                var len = $('.banner-pic li').length -1;

                var timer = setInterval(autoMove,3500);
                var Lis = $('.banner-pic li');
                Lis.eq(0).find('p').toggleClass('animate-bounce-up');
                function autoMove(){
                    if(step == len){
                        step = 0;
                        $('.banner .banner-pic').css('left',-clintW*step);
                        Lis.eq(step).toggleClass('animate-bounce-up');
                    }
                    step++;
                    $('.banner .banner-pic').stop().animate({'left':-clintW*step},500,function(){
                        $.each(Lis, function(index,item) {
                            $(Lis.eq(index).find('p').removeClass('animate-bounce-up'));
                            if(index == step){
                                $(this).find('p').addClass('animate-bounce-up');
                            }
                        });
                    });
                    focus();
                }
                function focus(){
                    var temp = step == len?0:step;
                    $.each(imgs, function(index,item) {
                        if(index == temp){
                            $(this).addClass('active').siblings().removeClass('active');
                        }
                    });
                }

                $.each(imgs, function(index,item) {
                    $(this).mouseover(function(){
                        step = index;
                        $('.banner .banner-pic').stop().animate({'left':-clintW*step},500);
                        focus();
                        clearInterval(timer);
                    }).mouseout(function(){
                        timer = setInterval(autoMove,3500);
                    })
                });
            };

            ////点击立即注册 到说明页
            $('#regiterApp').off().on('click',function(){
                if(location.href.indexOf('daweixinke') > -1){
                    location.href = '/sqe.php?s=/BusinessInto/start';
                }else{
                    location.href = '/sqe.php?s=/BusinessIntoRegion/businessInto';
                }
            });
        });
