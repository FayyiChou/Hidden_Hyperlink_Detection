        //$("#loginbtn").attr("disabled", false);
        function bindPhoneNumCheck(obj, obj_info) {
            $('#' + obj).focus(function () {
                $('#' + obj_info).html('');
                if (obj == 'phoneNumber') {
                    $(".reg_sendverify").css("background", "#4397FF");
                    //$(".reg_sendverify").css("color", "#999");
                } else if (obj == 'set_pass_cellphone') {
                    $(".reset_sendverify").css("background", "#4397FF");
                    //$(".reset_sendverify").css("color", "#999");
                }
            });
            //商家注册时查看手机号是否被注册,用于注册时离开手机号文本框事件
            $('#' + obj).blur(function () {
                if (checkPhoneNum($('#' + obj).val())) {

                    if (obj == 'phoneNumber') {
                        //===============
                        // 数据包装
                        var jsonData = {};
                        jsonData['phone_number'] = $('#' + obj).val();
                        jsonData['platform'] = 'web';
                        var dataSend = {};
                        dataSend['data'] = jsonData;
                        $.ajax({
                            type : "post",
                            url : "/sqe.php?s=/AccountSeller/ajax_reg_phone_num",
                            data :dataSend,
                            dataType: 'json',
                            success : function(data){
                                if (data.status == 1) {
                                    $('#' + obj_info).html('<em></em>');
                                    $('#reg_info').html("");
                                    settingSendButtonStyle();
                                }else{
                                    $('#' + obj_info).html('<em class="active"></em>');
                                    $('#reg_info').html(data.info);
                                    $('#reg_info').css({"font-size":"12px","color":"red"});
                                }
                            }
                        });

                    } else if (obj == 'set_pass_cellphone') {
                        $('#' + obj_info).html('<em></em>');
                        resetSendButtonStyle();
                    }
                }
                else {
                    $('#reg_info').html('手机号码格式不正确');
                    $('#reg_info').css({"font-size":"12px","color":"red"});
                    $('#' + obj_info).html('<em class="active"></em>');
                }
            })

        }
        window.onkeydown = function (event) {
            var e = event ? event : (window.event ? window.event : null);
            if($('#IDlogin').css('display') == 'block'){
                if (e.keyCode == 13) {
                    if ($('#action_type').val() == 1) {
                        userLogin();
                        return;
                    }
                    if ($('#action_type').val() == 2) {
                        userRegister();
                        return;
                    }
                }
            }

        }
        var codeUrl = dwxk_online_host+'/sqe.php?s=/AccountSeller/verify';

        function pgreCode() {
            document.getElementById('pgverifyimg').src = codeUrl + '/' + Math.random();
        }
        function fpgreCode(){
            document.getElementById('fpverifycode').src = codeUrl + '/' + Math.random();
        }
        function checkPhoneNum(phonenum) {
            if (phonenum.length != 11) {
                return false;
            } else {
                var reg0 = /^1\d{10}$/; //130--139。至少7位
                var my = false;
                if (phonePreg.test(phonenum))my = true
                if (!my) {
                    return false;
                } else if (!!isNaN(phonenum)) {
                    return false;
                }else {
                    return true;
                }
            }
        }

        function checkPassword2(password, pass_obj) {

            var strText = '密码强度验证为中等以上方可通过';
            $("#"+pass_obj).html(strText);
            var psdGrade = checkPasswordGrade(password);
            if(psdGrade >= 2){
                var strGradeText = psdGrade == 2 ? '强度等级:中' : '强度等级:强';
                $("#"+pass_obj).html(strText + '&nbsp;&nbsp;&nbsp;&nbsp;<font color="green">' + strGradeText + '</font>');
                return true;
            }
            $("#"+pass_obj).html(strText + '&nbsp;&nbsp;&nbsp;&nbsp;<font color="red">强度等级:弱</font>');
            /* if (!!password.match(/[a-zA-Z]/gi) && !!password.match(/^\w+$/gi) && password.length > 6) {
             return true;
             } */
            return false;
        }

        function checkPassword(pass_obj, pass_info,passGradeobj) {
            $('#' + pass_obj).bind("focus", function () {
                $('#' + pass_info).html('');
            })
            $('#' + pass_obj).bind("blur", function () {
                var password = $('#' + pass_obj).val();
                if (checkPassword2(password, passGradeobj)) {
                    $('#' + pass_info).html('<em></em>');
                }
                else {
                    $('#' + pass_info).html('<em class="active"></em>');
                }
            })
        }

        function checkverifyCode2(code_obj) {
            if (!!code_obj.match(/[1-9][0-9]{5}/gi) && code_obj.length >= 6) {
                return true;
            }
            return false;
        }

        function checkVerifyCode(code_obj, code_info) {

            $('#' + code_obj).bind("focus", function () {
                $('#' + code_info).html('');
            })

            $('#' + code_obj).bind("blur", function () {
                if (checkverifyCode2($('#' + code_obj).val())) {
                    $('#' + code_info).html('<em></em>');
                } else {
                    $('#' + code_info).html('<em class="active"></em>');
                }

            })

        }

        function checkPassEqual(comfirm_obj, comfirm_obj_info, pre_comfirm_obj) {
            $('#' + comfirm_obj).bind("focus", function () {
                $('#' + comfirm_obj_info).html('');
            })

            function cl_t() {
                var str1 = $('#' + comfirm_obj).val();

                if (str1 == "") {
                    $('#' + comfirm_obj_info).html('');
                } else {
                    if ($('#' + pre_comfirm_obj).val() == str1) {
                        $('#' + comfirm_obj_info).html('<em ></em>');
                    }
                    else {
                        $('#' + comfirm_obj_info).html('<em class="active"></em>');
                    }
                }

            }

            $('#' + pre_comfirm_obj).bind("blur", function () {
                cl_t();
            });
            $('#' + comfirm_obj).bind("blur", function () {
//            if(checkPassword2($('#'+pre_comfirm_obj).val(), $('#'+comfirm_obj).val()) ) {
                cl_t();
            });
        }

        function checkPassEqual2(password1, password2) {
            if (checkPassword2(password1) && checkPassword2(password2) && password1 == password2)
                return true;
            return false;
        }

        function reCode() {
            $('#verifycode').attr('src',codeUrl + '/' + Math.random());
        }

        function fpreCode()
        {
            document.getElementById('fpverifycode').src = codeUrl + '/' + Math.random();
        }

        function showCode() {
            if (!($('#code_li').length > 0)) {
                $('#pass_li').after('<li class="small" id="code_li"><input type="text" class="wd2" id="verify" name="verify"><img class="yzm" src="' + codeUrl + '" id="verifycode" onclick="reCode()" /></li>');
            }
        }
        function showSms(){
            if($("#sms_li").length <= 0){
                $('#pass_li').before('<p style="color:red;margin-bottom:4px;" id="sms_p">为账号安全，请验证您的手机信息。</p> <li id="sms_li"> <input type="text" id="loginverifySmsCode"/> <label class="code login_sendverify" onclick="loginSendVerfiyToPhone();" >获取短信验证码</label> <label class="time login_time" style="display: none"><em id="login_remainNumber">60</em>秒后重新发送</label> </li>');
            }
        }

