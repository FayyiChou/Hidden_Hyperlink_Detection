        var phonePreg = /^880\d{8}$|13[0-9]{1}[0-9]{8}$|15[^4]{1}[0-9]{8}$|17[0-9]{1}[0-9]{8}$|18[0-9]{1}[0-9]{8}|14[0-9]{1}[0-9]{8}$/;
