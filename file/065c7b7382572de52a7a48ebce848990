            $("#set_pass_cellphone").bind('paste', function (e) {
                setTimeout(function () {
                    resetSendButtonStyle()
                }, 100);
            });



            checkPassword('reset_passsword', 'reset_passsword_info', 'PsdResetGrade');
            checkPassword('password', 'reg_password_info', 'psdRegGrade');

            bindPhoneNumCheck('set_pass_cellphone', 'set_pass_cellphone_info');
            bindPhoneNumCheck('phoneNumber', 'reg_phoneNumber_info');


            $('#pgverify').blur(function () {
                var mapcode = $.trim($('#pgverify').val());
                if (mapcode.length != 4) {
                    $('#reg_info').html('验证码错误，请重新输入！');
                    $('#reg_info').css({"font-size":"12px","color":"red"});
                    return;
                }else{
