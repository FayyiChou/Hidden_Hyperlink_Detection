        function md5_pass() {
            var pass = $("input[name='pass']").val();
            $("input[name='pass']").val('');
            $("input[name='pass']").attr('name', '');
            $("input[name='password']").val($.md5(pass));
        }
        $("#lg").live("click",function(){

            loginBoxInitialize();
        })
        function checkPasswordGrade($str){
            $score = 0;
            if($str.match(/[0-9]+/))
            {
                $score ++;
            }
            if($str.match(/[0-9]{3,}/))
            {
                $score ++;
            }
            if($str.match(/[a-z]+/))
            {
                $score ++;
            }
            if($str.match(/[a-z]{3,}/))
            {
                $score ++;
            }
            if($str.match(/[A-Z]+/))
            {
                $score ++;
            }
            if($str.match(/[A-Z]{3,}/))
            {
                $score ++;
            }
            if($str.match(/[_|\-|+|=|*|!|@|#|$|%|^|&|(|)]+/))
            {
                $score += 2;
            }
            if($str.match(/[_|\-|+|=|*|!|@|#|$|%|^|&|(|)]{3,}/))
            {
                $score ++ ;
            }
            if($str.length >= 10)
            {
                $score ++;
            }
            if($score >= 0 && $score <= 5){
                return 1;
            }
            if($score >= 6 && $score <= 8){
                return 2;
            }
            if($score >8){
                return 3;
            }
            return 0;
        }
