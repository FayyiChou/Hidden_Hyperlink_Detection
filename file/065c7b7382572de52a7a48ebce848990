        //user check
        function checkAccountUsername(accountuser){
            if(accountuser.indexOf("shop") >= 0){
                if(childAccountPreg.test(accountuser)){
                    return true;
                }else{
                    return false;
                }
            }else{
                if(phonePreg.test(accountuser)){
                    return true;
                }else{
                    return false;
                }
            }
        }
        function userLogin() {
            $('#login_info').hide();
            if ($('#cl_input_username').val() == '') {
                $('#login_info').html('用户名不能为空！').show();
                $('#login_info').css({"font-size":"12px","color":"red"});
                return;
            }
            var checkResult = checkAccountUsername($('#cl_input_username').val());
            if(!checkResult){
                $('#login_info').html('用户名格式不正确！').show();
                $('#login_info').css({"font-size":"12px","color":"red"});
                return ;
            }
            if ($('#cl_input_password').val() == '') {
                $('#login_info').html('密码不能为空！').show();
                $('#login_info').css({"font-size":"12px","color":"red"});
                return;
            }
            if($("#sms_li").length > 0 && $("#loginverifySmsCode").val() == ''){
                $("#login_info").html('请您输入短信验证码').show();
                $('#login_info').css({"font-size":"12px","color":"red"});
                return;
            }
            if($('#code_li').length > 0 && $('#verify').val() == ''){
                $("#login_info").html('请您输入验证码').show();
                $('#login_info').css({"font-size":"12px","color":"red"});
                return;
            }
            $('#login_info').html('<img src="https://public.chuchutong.com/publicdwxkpc/SQE/img/loading.gif"/>').show();
            var username = $.trim($('#cl_input_username').val());
            var password = $.md5($.trim($('#cl_input_password').val()));
            var login_type = $.trim($('#login_type').val());
            var redirectUri = $.trim($('#redirect_uri').val()); // 添加跳转
            var sms_code = $.trim($('#loginverifySmsCode').val());
            var loginUrl = "/sqe.php?s=/AccountSeller/login";

            var postArr = {};
            postArr.username = username;
            postArr.password = password;
            postArr.login_type = login_type;
            postArr.sms_code = sms_code;
            postArr.redirect_uri = redirectUri;
            if ($('#code_li').length > 0) {
                postArr.verify = $('#verify').val();
            }
            // begin login
            // 数据包装
            postArr['platform'] = 'web';
            var dataSend = {};
            dataSend['data'] = postArr;
            $.post(loginUrl, dataSend, function (e) {
                if(e.status === 1){
                    $('#login_info').html('登录成功！正在为您跳转...').show();
                    $('#login_info').css({"font-size":"12px","color":"red"});
                    if (redirectUri == '') {
                        setTimeout(function () {
                            ///location.href = e.data;
                            location.href = "/sqe.php?s=/User/home";

                        }, 1000);
                    } else {
                        setTimeout(function () {
                            location.href = redirectUri;
                        }, 1000);
                    }
                }else if(e.status >=100 || e.status == 0){
                    if(e.status == 103){
                        //$("#loginbtn").attr("disabled", true);
                    }else if(e.status == 104 || e.status == 105){
                        showCode();
                        reCode();
                    }else if(e.status == 106){
                        if(e.data.is_need_code){
                            showCode();
                        }else{
                            $('#code_li').remove();
                        }
                        if(e.data.is_need_sms){
                            showSms();
                            $(".login_sendverify").css("background", "#4397FF");
                            $(".login_sendverify").css("color", "#FFF");
                            $('#login_info').hide();
                            isVerify = true;
                            if(typeof(e.data.m) != "undefined" && e.data.m.length > 10) {
                                $('#cl_input_childaccount').val(e.data.m);
                            }
                            //settingLoginSendButtonStyle();
                        }else{
                            $('#sms_li').remove();
                            $('#sms_p').remove();
                        }
                        /* if(e.data.status == 1){
                         //$('#login_info').html('为了保证您的账号安全，请您输入短信验证码').show();
                         }else{
                         $('#login_info').html('用户名或密码错误，当前还剩余'+e.data.verify+'次登录机会').show();
                         } */
                    }
                    $('#login_info').html(e.info).show();
                    $('#login_info').css({"font-size":"12px","color":"red"});
                    return false;
                }else{
                    //$("#loginbtn").attr("disabled", true);
                    $('#login_info').html('登录异常，请刷新后重新登录').show();
                    $('#login_info').css({"font-size":"12px","color":"red"});
                    return false;
                }
            }, 'json');
        }
        //商家注册
        function userRegister() {
            $('#reg_info').html('');

            if ($('#phoneNumber').val().length <= 0) {
                $('#reg_info').html('手机号码不能为空!');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }
            if(!checkAccountUsername($('#phoneNumber').val())){
                $('#reg_info').html('手机号格式不正确！').show();
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return ;
            }
            if ($('#password').val().length <= 0) {
                $('#reg_info').html('密码不能为空！');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }

            if ($('#reg_comfrim_password').val().length <= 0) {
                $('#reg_info').html('确认密码不能为空！');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }

            if ($('#verifyCode').val().length <= 0) {
                $('#reg_info').html('验证码不能为空！');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }

            var regPhoneNumber = checkPhoneNum($('#phoneNumber').val());
            var regPass = checkPassword2($('#password').val());
            var regComfrimPass = checkPassEqual2($('#reg_comfrim_password').val(), $('#password').val());
            var regVerifyCode = checkverifyCode2($('#verifyCode').val());

            if (regPass == false) {
                $('#reg_info').html('密码格式不正确！');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }

            if (regComfrimPass == false) {
                $('#reg_info').html('两次输入的密码不一致！');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }

            if (regVerifyCode == false) {
                $('#reg_info').html('验证码格式不正确！');
                $('#reg_info').css({"font-size":"12px","color":"red"});
                return;
            }
            $('#reg_info').html('<img src="https://public.chuchutong.com/publicdwxkpc/SQE/img/loading.gif"/>').show();
            var cellphone = $('#phoneNumber').val();
            var password = $.md5($('#password').val());
            var verifyCode = $('#verifyCode').val();
            ///数据包装
            var jsonData = {};
            jsonData['phone_number'] = cellphone;
            jsonData['password'] = password;
            jsonData['sms_code'] = verifyCode;
            jsonData['platform'] = 'web';
            jsonData['num'] = Math.random();
            var dataSend = {};
            dataSend['data'] = jsonData;
            $.post("/sqe.php?s=/AccountSeller/addNewSeller", dataSend, function (data) {

                if (data.status == 1) {
                    $("#IDlogin").hide();
                    $('#success').removeClass('Ndisplay');
                    setTimeout(function () {
                        location.href = successRegInWeidianUrl;
                    }, 1000)
                } else {
                    $('#reg_info').html(data.info);
                    $('#reg_info').css({"font-size":"12px","color":"red"});
                }
            }, 'json');

        }
        function settingSendButtonStyle() {
            phoneNumber = $.trim($("#phoneNumber").val());
            if (checkPhoneNumber(phoneNumber)) {
                $(".reg_sendverify").css("background", "#4397FF");
                $(".reg_sendverify").css("color", "#FFF");
            } else {
                $(".reg_sendverify").css("background", "#4397FF");
            }
        }
