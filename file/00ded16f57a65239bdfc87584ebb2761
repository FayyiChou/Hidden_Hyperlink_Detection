/**
 * F7 添加
 * 初始化其他地方，显示评论数
 */
function otherCommentNumber() {
  var count,
      topElem = document.getElementById("top_comment_number"),
      sideElem = document.getElementById("side_comment_number");
  try {
    if ( typeof(commentnum.news.topnum) == "undefined" ) {
      count = commentnum.news.commentnum;
    } else {
      count = commentnum.news.commentnum+commentnum.news.topnum;
    };
  } catch(ex) {
    count = 0;
  };

  if ( topElem ) {
    topElem.innerHTML = count;
  };
  if ( sideElem ) {
    sideElem.innerHTML = count;
  };
};
//otherCommentNumber();
