      var   speed=30  
      demofang2.innerHTML=demofang1.innerHTML   
      demofang3.innerHTML=demofang1.innerHTML   
      function   Marquee(){   
      demofang.scrollLeft++   
      if   ((demofang.scrollLeft)>=demofang1.offsetWidth)   
      demofang.scrollLeft=0   
      }   
      var   MyMar=setInterval(Marquee,speed)   
      demofang.onmouseover=function()   {clearInterval(MyMar)}   
      demofang.onmouseout=function()   {MyMar=setInterval(Marquee,speed)}   
