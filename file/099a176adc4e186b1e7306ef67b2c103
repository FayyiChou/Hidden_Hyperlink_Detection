    $.ajax({
        type:'get',
        url:"http://taiyuan.liebiao.com/ajax/postclicks/?postid=231787300",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
