function check()
{
	if(form1.xingming.value =="")
	{
		alert("请输入您的姓名");
		form1.xingming.focus();
		return false;
	}

	if(form1.tel.value =="")
	{
		alert("请输入您的电话");
		form1.tel.focus();
		return false;
	}

	if(form1.address.value =="")
	{
		alert("请输入您的联系地址");
		form1.address.focus();
		return false;
	}		
	
	if(form1.post.value =="")
	{
		alert("请输入您的邮政编码");
		form1.post.focus();
		return false;
	}		
	
	if (form1.xiangmu.value == "请选择您的项目")
  {
     alert("请选择您的项目");

	 form1.xiangmu.focus();
	 return (false);
  }
	
	if(form1.notes.value =="")
	{
		alert("请输入您的留言内容");
		form1.notes.focus();
		return false;
	}	
	
	return true;
}

function fIsNumber (sV,sR){
	var sTmp;
	if(sV.length==0){ return (false);}
	for (var i=0; i < sV.length; i++){
		sTmp= sV.substring (i, i+1);
		if (sR.indexOf (sTmp, 0)==-1) {return (false);}
	}
	return (true);
}

function fIsNullNumber (sV,sR){
	var sTmp;
	if(sV.length==0){ return (false);}
	for (var i=0; i < sV.length; i++){
		sTmp= sV.substring (i, i+1);
		if (sR.indexOf (sTmp, 0)!=-1) {return (false);}
	}
	return (true);
}

function fIsOnNumber (sV,sR,sNUM){
	var sTmp,j;
	j=1;
	//if(sV.length==0){ return (false);}
	for (var i=0; i < sV.length; i++){
		sTmp= sV.substring (i, i+1);
		if (sR.indexOf (sTmp, 0)==-1){
		 
		 j=1;
		 //return (false);
		 }
		else{
			if(j>sNUM){
			return (false);
			}
			j++;		
		}
	}
	return (true);
}

function fIsCountNumber (sV,sR,sNUM){
	var sTmp,j;
	j=1;
	//if(sV.length==0){ return (false);}
	for (var i=0; i < sV.length; i++){
		sTmp= sV.substring (i, i+1);
		if (sR.indexOf (sTmp, 0)!=-1){
		 
			if(j>sNUM){
			return (false);
			}
			j++;		
		}
	}
	return (true);
}

function is_email(object_name)
{
	var string;
	string=new String(object_name);
	var len=string.length;
	if (string.indexOf("@",1)==-1||string.indexOf(".",1)==-1||string.length<7)
		{
		alert("电子邮箱的格式不对，请重新填写！");
		return false;
		}
	if (string.charAt(len-1)=="."||string.charAt(len-1)=="@")
		{
		alert("电子邮箱的格式不对，请重新填写！");
		return false;
		}
}

function CArrayISNull(frm,Field){
   var tmpsid='';

  for(var i=0;i<frm.elements.length;i++){
    var e=frm.elements[i];


		if((e.name).indexOf(Field)!=-1){
			
			alert(e.name);
			if(e.value==''){
				
				return (false);
			}	
 	 	}
	}
	return (true);
}


function CArrayCheck(frm,Field){
  var tmpsid='';
 
  for(var i=0;i<frm.elements.length;i++){
    var e=frm.elements[i];
    
		if((e.name).indexOf(Field)!=-1){
	
			if (e.checked==true){
        		 if (tmpsid!=''){
            		tmpsid+='|'+e.value;
		         }else{
				    tmpsid=e.value;
			 	}
  	  		}	
 	 	}
	}

	if(tmpsid==''){
		return false;
	}
	return (true);
}

