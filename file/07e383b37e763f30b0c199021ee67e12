    <script type="text/javascript" src="http://s.autoimg.cn/as/static/js/jquery-1.7.2.min.js"></script>
    <script src="http://x.autoimg.cn/qa/summary/js/highcharts.js"></script>
    <script type="text/javascript" src="http://x.autoimg.cn/as/js-2.0.2/sea.js"></script>
    
    <script type="text/javascript" src="http://x.autoimg.cn/qa/summary/js/jquery.jplayer.min.js?v=20170302"></script>
    
    <script type="text/javascript" src="http://x.autoimg.cn/qa/summary/js/audio-answer-min.js?v=20170316"></script>
    <script type="text/javascript">
        $.Summary = { userCllects: {}, pageIndex: 2,seriesId:2863 };
        seajs.config({ version: "1399451042313" });
        seajs.use(["jquery", "button", "layer", "gotop","pop"], function ($) {
            window.UICtrl = $;

        });
        $(document).ready(function () {
            //用户收藏列表
            if (autohome.loginUser.isLogin) {
                // 如果用户已经登录，得到用户收藏列表
                $.post("/Ajax/getCollects", {}, function (data) {
                    if (data.returncode == 0) {
                        $.Summary.userCllects = data.result.list;

                        $(".qa-item-inner").each(function () {
                            var self = $(this);
                            var btnCollect = self.find(".cllectq");
                            var tid = self.attr("tid");
                            for (var i = 0; i < $.Summary.userCllects.length; i++) {
                                if (tid == $.Summary.userCllects[i].CollectionId) {
                                    btnCollect.html('<i class="icon16 icon16-star2"></i>取消收藏');
                                    self.attr("opt", 2);
                                }
                            }
                        })
                    }
                }, 'json');
            }
                
            //收藏/取消收藏
            $("#qlist").on("click", ".cllectq", function () {
                autohome.initUser();
                if (!autohome.UserValidate(1, function(){
                    autoHeaderObj.init();
                })) return false;

                var self = $(this);
                var q = self.parents(".qa-item-inner");
                var tid = q.attr("tid");
                var opt = parseInt(q.attr("opt"));
                $.post("/ajax/CollectQuestion",
                    {
                        qid : tid,
                        opt : opt
                    },
                    function (data) {
                        if (data.result) {
                            if(opt == 1){
                                self.html('<i class="icon16 icon16-star2"></i>取消收藏');
                                q.attr("opt", 2);
                                $("#collecNotice").attr("qid", tid);

                                $("#collecNotice").attr("checked","checked");
                                window.UICtrl("#layer3").layer("show");
                                setTimeout(function () { window.UICtrl("#layer3").layer("hide") },3000);
                            }
                            else{
                                self.html('<i class="icon16 icon16-star1"></i>收藏问题');
                                q.attr("opt", 1);
                            }

                        } else {
                            if(opt == 1){
                                alert("收藏失败");
                            }
                            else{
                                alert("取消收藏失败");
                            }
                        
                        }
                    });
            });
            $("#collecNotice").click(function () {
                var isnotice;
                if ($(this).attr("checked")) {
                    isnotice = 1;
                } else {
                    isnotice = 0;
                }
                var qId = $(this).attr("qid");

                $.post("/ajax/UpdateNotice",
                    {
                        qId: qId,
                        isnotice: isnotice
                    },
                    function (data) {
                    });
            });

            //查看更多
            $("#qlist").on("click", "#more", function () {
                var url = "/summary/Questions?seriesId=" + $.Summary.seriesId + "&pageIndex=" + $.Summary.pageIndex + "&pvareaid=104202"
                var sendData = {seriesId:$.Summary.seriesId,pageIndex:$.Summary.pageIndex};
                $.get(url,{},function(data){
                    $.Summary.pageIndex++;
                    $("#qlist .more").prev().removeClass("lastinner");
                    $("#qlist .more").remove();
                    $("#qlist").append(data);
                });
            });

            var j_chart_wrapper = function (eleid, chartData) {
                $('#' + eleid).highcharts({
                    credits: {
                        enabled: false
                    },
                    chart: {
                        plotBackgroundColor: null,
                        plotBorderWidth: 0,
                        plotShadow: false,
                        margin: [0, 0, 0, 0],
                        spacing: [0, 0, 0, 0]
                    },
                    title: {
                        text: ''
                    },
                    tooltip: {
                        headerFormat:'',
                        pointFormat: '{point.name}{point.percentage:.2f}%'
                    },
                    legend: {
                        align: 'left',
                        layout: 'vertical',
                        x: 200,
                        itemMarginBottom: 8,
                        itemStyle: {"fontSize": "12px", "fontWeight": "normal", "fontFamily": 'tahoma,arial,sans-serif,"Hiragino Sans GB","\\5B8B\\4F53"'},
                        verticalAlign: 'top'
                    },
                    plotOptions: {
                        pie: {
                            slicedOffset: 0,
                            allowPointSelect: false,
                            cursor: 'pointer',
                            center: [110, '50%'],
                            dataLabels: {
                                enabled: false//,
                                // formatter: function () {
                                // 	return this.y > 5 ? this.point.name : null;
                                // },
                                // color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black',
                                // distance: -30
                            },
                            showInLegend: true,
                            states: {
                                hover: {
                                    brightness: 0.1,
                                    enabled: true,
                                    lineWidth: 1,
                                    lineWidthPlus: 1,
                                    halo: {
                                        attributes: null,
                                        opacity: 0.25,
                                        size: 1
                                    }
                                }
                            }
                        }
                    },
                    series: [{
                        type: 'pie',
                        name: '',
                        data: chartData,
                        cursor: 'pointer',
                        enableMouseTracking: true,
                        point: {
                            events: {
                                click: function () {
                                    window.open(this.options.url);
                                }
                            }
                        }
                    }]
                });
            }
            // 加载饼图
            $.get("/Summary/GetSeriesPieChart", {seriesId : $.Summary.seriesId}, function(data){
                j_chart_wrapper('cp_chart', data);
            }, 'json');

            //$("#cp-list dd a").not(".noclick").click(function(){
            //    var self = $(this);
            //    var sid = self.attr("sid");
            //    $.get("/Summary/GetSeriesPieChart#pvareaid=104198", {seriesId : sid}, function(data){
            //        j_chart_wrapper('cp_chart', data);
            //        $("#cp-list .current").removeClass("current");
            //        self.parent().addClass("current");
            //    }, 'json');
            //});
        });
    </script>
