<script>
$(function(){
	$(".indexcase .tab a").mouseover(function(){
		$(this).addClass('on').siblings().removeClass('on');
		var index = $(this).index();
		number = index;
		$('.indexcase .content li').hide();
		$('.indexcase .content li:eq('+index+')').show();
	});
	
	var auto = 1;  //等于1则自动切换，其他任意数字则不自动切换
	if(auto ==1){
		var number = 0;
		var maxNumber = $('.indexcase .tab a').length;
		function autotab(){
			number++;
			number == maxNumber? number = 0 : number;
			$('.indexcase .tab a:eq('+number+')').addClass('on').siblings().removeClass('on');
			$('.indexcase .content ul li:eq('+number+')').show().siblings().hide();
		}
		var tabChange = setInterval(autotab,3000);
		//鼠标悬停暂停切换
		$('.indexcase').mouseover(function(){
			clearInterval(tabChange);
		});
		$('.indexcase').mouseout(function(){
			tabChange = setInterval(autotab,3000);
		});
	  }
});
</script>

