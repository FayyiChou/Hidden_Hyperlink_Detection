<script type="text/javascript"> 
    function autoScroll(obj){  
      $(obj).find("ul").animate({  
        marginTop : "-20px"  
      },500,function(){  
        $(this).css({marginTop : "0px"}).find("li:first").appendTo(this);  
      })  
    }  
    var scroll = setInterval('autoScroll(".TextDiv")',3000);
       $('.TextDiv').hover(
                function(){
                    clearInterval(scroll);//鼠标经过暂停
                },function(){
                    scroll = setInterval('autoScroll(".TextDiv")',3000);//离开继续轮播
                }
            )
    
</script>
