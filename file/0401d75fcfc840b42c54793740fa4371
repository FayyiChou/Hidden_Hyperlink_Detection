  <script>
  $(function($){
    var i = 0;
    var j = $('.img li').length;
    console.log(j);
    function banner(){
      if(i==j-1){
        i=0}else{
          i++
        }
        $('.img li').hide().eq(i).show();
        $('.point span').css('background','white').eq(i).css('background','#ccc');
    }
    var time = setInterval(banner,3000);
    $('.banner').hover(
        function(){
          clearInterval(time);//鼠标经过暂停
        },function(){
          time = setInterval(banner,3000);//离开继续轮播
        }
      )
    $('.point span').mouseover(
      function(){
        i=$(this).index();
        $('.point span').css('background','white').eq(i).css('background','#ccc');
        $('.img li').hide().eq(i).show();
      }

      )

  })  

</script>

