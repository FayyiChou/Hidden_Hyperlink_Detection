<script type="text/javascript">
	function shwoOrHidden11(){
		if(document.getElementById("d111").style.display=='block'){
			document.getElementById("d111").style.display='none';
			document.getElementById("d211").style.display='block';
		}else if(document.getElementById("d111").style.display=='none'){
			document.getElementById("d111").style.display='block';
			document.getElementById("d211").style.display='none';
		}
	}
</script>
<script type="text/javascript">
$(document).ready(function(){

	$change_li = $(".titleChar li");
	$change_li.each(function(i){
		$(this).mouseover(function(){
			$(this).addClass("on").siblings().removeClass("on");
			$(".cityListBox16 dl").eq(i).show().siblings().hide();
		})
	});
		
});
</script>
