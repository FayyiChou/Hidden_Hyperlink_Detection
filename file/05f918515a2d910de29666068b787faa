<script>
$(document).ready(function(e) {
	  $('.adcon').show().delay(3000);
	  $('.close').click(function(){
		$('.rightad').show().delay(3000);
		$('.adcon').hide();  
                $('.ceng').hide();
                $('.close2').show();
		  });
          $('.close2').click(function(){
		$('.rightad').hide(); 
                $(this).hide();
		  });
                  
});
</script>              
