<script>
	$(function() {
		if ($.browser.msie && $.browser.version == 7.0) {
			//ie7的密码输入框 placeholder样式
			$("input[type='password']").live("focus", function() {
				if ($(this).val() == "") {
					$(this).siblings("label").css("visibility", "hidden");
				}
			}).live("blur", function() {
				if ($(this).val() == "") {
					$(this).siblings("label").css("visibility", "visible");
				}
			}).change(function() {
				if ($(this).val() == "") {
					$(this).siblings("label").css("visibility", "visible");
				} else {
					$(this).siblings("label").css("visibility", "hidden");
				}
			});
		}

	});
</script>
