			function getCookie(name) {
				var matches = document.cookie.match(new RegExp(
				"(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
				));
				return matches ? decodeURIComponent(matches[1]) : false;
			}

			function setCookie(name, value, options) {
			  options = options || {};

			  var expires = options.expires;

			  if (typeof expires == "number" && expires) {
			    var d = new Date();
			    d.setTime(d.getTime() + expires * 1000);
			    expires = options.expires = d;
			  }
			  if (expires && expires.toUTCString) {
			    options.expires = expires.toUTCString();
			  }

			  value = encodeURIComponent(value);

			  var updatedCookie = name + "=" + value;

			  for (var propName in options) {
			    updatedCookie += "; " + propName;
			    var propValue = options[propName];
			    if (propValue !== true) {
			      updatedCookie += "=" + propValue;
			    }
			  }

			  document.cookie = updatedCookie;
			}

			if(isMobile() === true && getCookie('FULL_VERSION') === false) {
				var arMobilePath = [
					'/company',
					'/contacts',
					'/electronic_shop',
					'/news',
					'/products',
					'/show_attention'
				];

				var mobileRedirect = false;

				for (var i = 0; i < arMobilePath.length; i++)
				{
					if (window.location.pathname.indexOf(arMobilePath[i]) != -1)
					{
						mobileRedirect = true;
					}
				}

				if (window.location.pathname == '/')
				{
					mobileRedirect = true;
				}

				if (window.location.pathname == '/products/etim-download/')
				{
					mobileRedirect = false;
				}

				if (mobileRedirect)
				{
					window.location.href  = 'http://m.russvet.ru' + window.location.pathname;
				}
			}
