	var rsOrpho = getCookie('rsOrpho');
	if (rsOrpho == '30day') 
	{
		$(".rs-orpho.absolute-rs-orpho").css( "display", "none" );
	}

	$( "#close-orpho-trigger" ).on(
		"click", function() {
			var date = new Date;
			date.setDate(date.getDate() + 30);
			setCookie('rsOrpho', '30day', date);

