                            $(".smdl-cont img").attr("src", result["QRUrl"]);
                            $("#qrcode").val(result["QRCode"]);

                        });
                    }

                });
            });
            $(".ui-smdl .smdl-close a").click(function () {
                $(".ui-smdl").hide();
            });
        });
