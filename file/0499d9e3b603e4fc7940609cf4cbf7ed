        $(".m-app-close").click(function () {
            $(this).animate({ left: "-100%" }, "normal", "swing", function () {
                $(".m-app-open").show();
                $(".m-app-open").animate({ left: "0" }, "normal", "swing");
            });
        });

        $(".m-app-open .btn-close").click(function () {
            $(this).parents(".m-app-open").animate({ left: "-100%" }, "normal", "swing", function () {
                $(".m-app-close").show();
                $(".m-app-close").animate({ left: "0" }, "normal", "swing", function () {
                    $(this).parents(".m-app-open").hide();
                });
            });
        });

        $(function () {
            $(".login-t .smdl").click(function () {
                $(".ui-smdl").show();

