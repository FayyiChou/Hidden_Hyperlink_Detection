<script>
    $(function() {
        $(".indx-ask ul.ubtn li a").click(function () {
            var id = $(this).parents("ul.ubtn li").attr("data-id");
            $(this).parents(".indx-ask").find(".ask-content-mid ul").hide();
            $(this).parents(".indx-ask").find(".ask-content-mid ul[data-id='" + id + "']").show();
            $(this).parents("ul.ubtn").find("li").removeClass("curr");
            $(this).parent().addClass("curr");
        });
    });



</script>

