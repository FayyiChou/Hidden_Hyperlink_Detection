<script type="text/javascript">
    $(document).ready(function(){
        // $(obj,obj2) obj顶部对象 obj2横向导航对象的选中状态
        $("#navigate > a").click(
            function(){
                $("#navigate > a").removeClass();
                $("#navigate > a[data-role='"+$(this).attr('data-role')+"']").addClass("current");
                $("#content").children().hide().end().find('div[data-role='+$(this).attr('data-role')+']').show();
            }
        );
        // 隐藏所有大于1条以上的日志信息
        $(".pro_list .dp").each(
            function(){
                $(this).find('p').slice(1).hide();
            }
        );
        // 控制节点显示隐藏
        $("span[data-role='update']").toggle(
            function(){
                $(this).html('收起').attr('title','收起')
                    .parent().parent().next(".dp").find('p').show();
            },
            function(){
                $(this).html('更新日志').attr('title','更新日志')
                    .parent().parent().next(".dp").find('p').slice(1).hide();
            }
        );

    });
</script>
<script type="text/javascript">
var show_error_report = function() {
    $.ajax({
        url: '/ajax/app/show_report_win',
        type: 'post',
        data: {},
        dataType: 'json',
        success: function(rs) {
            VeryCD.hidden_loading();
            if (rs.code == 0) {
                var config = JSON.parse(rs.msg);
                if (typeof config == 'object') {
                    VeryCD.win({'message':config.message, 'width':440, 'height':220, 'title':config.title, 'supperjs':true});
                }
            } else {
                VeryCD.error(rs.msg);
            }
        },
        beforeSend: function(){
            VeryCD.show_loading();
        },
        error: function() {
            VeryCD.hidden_loading();
            VeryCD.error('抱歉，网络故障，请重试！');
        }
    });
    return false;
};
if (typeof (window.ymInitialization) =='function'){
