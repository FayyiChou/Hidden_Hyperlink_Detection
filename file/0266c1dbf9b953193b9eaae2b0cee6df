<script>
//弹出隐藏层
function ShowDiv(show_div,bg_div){
	var showDiv = document.getElementById(show_div);
	var showDivWidth = 867;
	var showDivHeigth = 386;
	var screenWidth = $(window).width();
    var screenHeight = $(window).height();
	var positionLeft = (screenWidth - showDivWidth) / 2 + $(document).scrollLeft();
    var positionTop = (screenHeight - showDivHeigth) / 2 + $(document).scrollTop();
	showDiv.style.left = positionLeft+'px';
	showDiv.style.top = positionTop+'px';
	//showDiv.style.display='block';
var bgdiv = document.getElementById(bg_div);
bgdiv.style.width = document.body.scrollWidth;
// bgdiv.style.height = $(document).height();
$("#"+bg_div).height($(document).height());
//document.getElementById(bg_div).style.display='block' ;
$('#'+show_div).fadeIn(500);
	$('#'+bg_div).fadeIn(500);

};

//关闭弹出层
function CloseDiv(show_div,bg_div){
	$('#'+show_div).fadeOut(500);
	$('#'+bg_div).fadeOut(500);
//document.getElementById(show_div).style.display='none';
//document.getElementById(bg_div).style.display='none';
};

</script>

