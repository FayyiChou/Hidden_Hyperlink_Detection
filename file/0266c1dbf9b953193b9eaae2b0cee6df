$(window).scroll(function(){

	if(scrollact(".d01",50)){
		if(!$(".d01").hasClass("show")){
			$(".d01").addClass("show");			
		}
	} 
	if(scrollact(".d02",50)){
		if(!$(".d02").hasClass("show")){
			$(".d02").addClass("show");			
		}
	} 
	if(scrollact(".d03",50)){
		if(!$(".d03").hasClass("show")){
			$(".d03").addClass("show");			
		}
	}
	if(scrollact(".d04",50)){
		if(!$(".d04").hasClass("show")){
			$(".d04").addClass("show");			
		}
	}
	if(scrollact(".d05",50)){
		if(!$(".d05").hasClass("show")){
			$(".d05").addClass("show");			
		}
	}
	if(scrollact(".d06",50)){
		if(!$(".d06").hasClass("show")){
			$(".d06").addClass("show");			
		}
	}
	
});
$("#fade").click(function(){
	$("#MyDiv").fadeOut(500);
	$("#fade").fadeOut(500);
});
$("#fade").click(function(){
	$("#MyDiv02").fadeOut(500);
	$("#fade").fadeOut(500);
});
</script>



<script type="text/javascript">
    $(function(){
	$('.college_con02 .imageLink ul li').hover(function(e) {
        $('.college_con02 .imageLink').stop().animate({'bottom':'-206px'},500);
		$(this).find('.shuoming').stop().animate({'bottom':'0px'},500); 
    },function(){
		$('.college_con02 .imageLink .shuoming').stop().animate({'bottom':'-206px'},500);
	});
 }) 
</script>
