<script type="text/javascript">

$(document).ready( function(){

	$("#stock").keyup(function(event){
		getSuggestions($(this).val());

		if(event.keyCode == 13){
			getResults();
		}
	});

});

function setStock( value ){
	$('#stock').val( value );
	$('#suggestions').empty();
	getResults();
}

var vacant = true;

function getSuggestions( val ){
	if(vacant && val.length > 2){
		vacant = false;
		var cars = [];
		$('#suggestions').empty();
		$.get( "isapi_xml.php?module=getSuggestions&stock="+ val ).done( function( text ) {
			cars = JSON.parse(text);
			for( var a in cars ){
				if( typeof(cars[a]["stockno"]) != "undefined") $('#suggestions').append("<li onclick='setStock(\""+ cars[a]["stockno"] +"\")'>"+ cars[a]["type"] +" "+ cars[a]["year"] +" "+ cars[a]["make"] +" "+ cars[a]["model"] +" Stock# "+ cars[a]["stockno"] +"</li>");
			}
			vacant = true;
		});
	}
}

function getResults(){

	var loc = "new-inventory.htm";
	var cars = [];
	if( $('#stock').val().length > 1 ){ 
		$.get( "isapi_xml.php?module=getSuggestions&stock="+$('#stock').val() ).done( function( text ) {
			if( text == "null" ){
				alert( "We're Sorry, Your search resulted in no result. \nPlease, try another Keyword.");
			}else{
				cars = JSON.parse(text);
				if( cars[0]['type'] == 'Used') loc = "used-inventory.htm";

				document.cookie = "filter="+ JSON.stringify( {stockno: $('#stock').val() } ) +"::/"+ loc;
				location.href = "http://www.shinglespringshonda.com/"+ loc;				
			}
		});
	}
}
</script>
