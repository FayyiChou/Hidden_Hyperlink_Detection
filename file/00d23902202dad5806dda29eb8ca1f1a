<script>
    function is_weixin(){
        var ua = navigator.userAgent.toLowerCase();
        if(ua.match(/MicroMessenger/i)=="micromessenger") {
            return true;
        } else {
            return false;
        }
    }
    var tipBox=$("#tipBox"),
            mask=$("#mask");
    var wdwVal=$("#wdw").val(),
            iosVal=$("#ios").val();
    $(".apple").click(function(){
        //是微信访问则点击下载时提示需要在浏览器中打开
        if(is_weixin()){
            tipBox.css({"display":"block"});
            mask.css({"height":$(document).height()+"px","display":"block"});
            tipBox.animate({"height":"8.33333rem"});
        }else{
                            window.location.href="https://itunes.apple.com/us/app/wei-dai-wang-hu-lian-wang/id914404898?l=zh&ls=1&mt=8";
                    }
    })
    $(".android").click(function(){
        //是微信访问则点击下载时提示需要在浏览器中打开
        if(is_weixin()){
            tipBox.css({"display":"block"});
            mask.css({"height":$(document).height()+"px","display":"block"});
            tipBox.animate({"height":"8.33333rem"});
        }else{
            window.location.href="http://weidaiosstest.oss-cn-hangzhou.aliyuncs.com/weidaiwang-release.apk";
        }
    })
    //点击提示信息 关闭提示信息
    mask.bind("click", function(){
        tipBox.css({"display":"none"});
        mask.css({"display":"none"});
        tipBox.animate({"height":"0px"});
    });
</script>




