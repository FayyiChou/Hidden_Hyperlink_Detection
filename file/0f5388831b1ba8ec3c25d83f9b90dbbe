    j_nav.delegate('li','mouseover',function(){
        var j_this = $$(this);
        if(j_this.attr('id') != 'nav_hover'){
            clearTimeout(waitHover);
            waitHover = setTimeout(function(){				
                j_nav.find('.v a').removeClass('sele');
                $$('#nav_hover').attr('id','');
                $$("#nav .kind_menu").hide();
                j_this.find('.v a').addClass('sele');
                j_this.attr("id","nav_hover").find('.kind_menu').show();                
            },200);
        }   
    }).delegate('li','mouseout',function(){
        clearTimeout(waitHover);
    }).delegate('li','click',function(){
        var j_this = $(this);
        if(j_this.attr('id') != 'nav_hover'){
            j_nav.find('.v a').removeClass('sele');
            $$('#nav_hover').attr('id','');
            $$("#nav .kind_menu").hide();
            j_this.find('.v a').addClass('sele');
            j_this.attr("id","nav_hover").find('.kind_menu').show();
        }
    });	
