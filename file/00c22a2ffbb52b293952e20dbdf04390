            <script>
                function shizhong(id) {
                    var _obj = document.getElementById("recommend_" + id);
                    var _zan = document.getElementById("vzan_" + id);
                    if (_zan.value == "0") {
                        var type = 2;
                        $.ajax({
                            url: "/Home/RecommendAdd",
                            data: { ID: id, Type: type },
                            beforeSend: function () {
                                _obj.innerText = parseInt(_obj.innerText) + 1;
                            },
                            type: "post",
                            success: function (result) { }
                        });
                        _zan.value = "1";
                    } else {
                        $.dialog.tips('不能重复点赞', 1, 'remove-round.png');
                    }
                }
            </script>
