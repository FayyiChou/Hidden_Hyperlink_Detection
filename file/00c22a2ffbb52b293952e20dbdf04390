    <!--------矢量--------->
    <script>
        function vthing(id)
        {
            var v = $("#vt_thing_"+id).val();
            if (v == 0) {
                $.get("/Home/VectorUpdateThing?ID=" + id, function (data, status) {
                    $("#vt_thing_" + id).val("1");
                    $("#vthing_"+id).text(data);
                });
            } else {
                $.dialog.tips('不能重复点赞', 1, 'remove-round.png');
            }
        }
        function down(id, url, file)
        {
            var _login = $("#login").val();
            if (_login == 1) {
                $.get("/Home/VectorUpdateDown?ID=" + id, function (data, status) {
                });
                var _u = url;
                if (_u == "")
                    _u = file;
                window.open(_u);
            } else {
                $.dialog.tips('请先登录!', 1, 'remove-round.png');
            }
        }
    </script>
