                        <script type="text/javascript">
                        $(function () {
                            //输入跳转
                            $("#logo_skip").click(function () {
                                var KeyWord = encodeURI($("#Title").val());
                                var curPage = parseInt($("#logo_skip_no").val());
                                var totalPage = parseInt($("#logo_TotalPage").val());
                                if (curPage <= totalPage && curPage > 0) {
                                    location.href = "/Home/?type=1&CurrentPage=" + curPage + "&SortID=0&KeyWord=" + KeyWord;
                                }
                            });
                        });
                        </script>
