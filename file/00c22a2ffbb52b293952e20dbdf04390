    <script type="text/javascript">
        function vcolor(x)
        {
            var _obj = document.getElementById("vu_key");
            if (x!="")
                _obj.className = "o";
            else
                _obj.className = "";
        }
        function vgo(o)
        {
            var _key = encodeURI($("#Title").val())
            window.location.href = "/Home/?type=" + o + "&KeyWord=" + _key;
        }
        function soc(o)
        {
            if(o==0)
                o = $("#vu_type").val();
            $("#vu_type").val(o);
            for (i = 1; i < 4; i++)
            {
                document.getElementById("c" + i).className = "";
                document.getElementById("box" + i).style.display = "none";
            }
            document.getElementById("c" + o).className = "o";
            document.getElementById("box" + o).style.display = "";
        }
        function gosearch()
        {
            var _type = $("#vu_type").val();
            var _key = encodeURI($("#vu_key").val())
            if (_key != "") {
                window.location.href = "/Home/?type=" + _type + "&KeyWord=" + _key;
            }
            else
            {
                window.location.href = "/Home/";
            }
        }
        function gosearchKey(val) {
            var _type = $("#vu_type").val();
            var _key = encodeURI(val)
            if (_key != "") {
                window.location.href = "/Home/?type=" + _type + "&KeyWord=" + _key;
            }
            else {
                window.location.href = "/Home/";
            }
        }
        function clickButton() {
            if (event.keyCode == 13) {
                document.getElementById("vso").click();
                return false;
            }
        }
    </script>
