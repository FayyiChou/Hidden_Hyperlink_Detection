    <script>
        function fthing(id)
        {
            var v = $("#ft_thing_"+id).val();
            if (v == 0) {
                $.get("/Home/FontsUpdateThing?ID=" + id, function (data, status) {
                    $("#ft_thing_" + id).val("1");
                    $("#fthing_"+id).text(data);
                });
            } else {
                $.dialog.tips('不能重复点赞', 1, 'remove-round.png');
            }
        }
    </script>
    <div id="box2">
        <div id="list-layout">
            <ul class="vlist">
