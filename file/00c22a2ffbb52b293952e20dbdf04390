                        <script type="text/javascript">
                            $(function () {
                                //输入跳转
                                $("#fonts_skip").click(function () {
                                    var KeyWord = encodeURI($("#Title").val());
                                    var curPage = parseInt($("#fonts_skip_no").val());
                                    var totalPage = parseInt($("#fonts_TotalPage").val());
                                    if (curPage <= totalPage && curPage > 0) {
                                        location.href = "/Home/?type=2&CurrentPage=" + curPage + "&KeyWord=" + KeyWord;
                                    }
                                });
                            });
