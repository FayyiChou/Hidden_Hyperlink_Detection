            //广告切换
            function imgad(f, b, a, e, c, d) {
                    var f = $(f);
                    var b = $(b);
                    f.hide();
                    b.hide();
                    b.slideDown(a);
                    setTimeout(function() {
                        b.slideUp(e, function() {
                            f.slideDown(c)
                        })
                    }, d)
            }
           // imgad('.js-slideS','.js-slideB', 1000, 1000, 1000, 5000);
           
           
