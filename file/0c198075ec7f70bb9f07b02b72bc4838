        var lottery_type = LotteryType.PK10;

        $(document).ready(function(){
            initCurrentPhase();
        });
        function getDataByDay(opt) {
            if (opt.value != "") {
                window.location.replace("/pk10/bigSmallTrend.action?recentDay="+opt.value+"&onlyBody=false&phaseOrder=up");
            }
        }
        var recentDay = "";
        $("select#selectRecentDay").val(recentDay);

        if (recentDay == "") {
            var recentPhase = "30";
            var issue = {"30":0,"50":1,"100":2};

            $("div.select_by_day a")[issue[(recentPhase=="")?"30":recentPhase]].style.color = "red";
        }
    </script>
