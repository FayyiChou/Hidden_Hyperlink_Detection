      <script language="javascript" type="text/javascript">
    jQuery(function () {
        jQuery("#order_login").dialog({
            autoOpen: false,
            bgiframe: false,
            resizable: false,
            width: 675,
            height: 295,
            modal: true,
            open: function () {
                jQuery("select").hide();
            },
            close: function () {
                jQuery("select").show();
            }
        });
        jQuery("#o_user_name").click(function () {
            if (jQuery(this).val() == "手机号/邮箱") {
                jQuery(this).val("");
            }
        });
        jQuery("#o_user_name").blur(function () {
            if (jQuery(this).val() == "") {
                jQuery(this).val("手机号/邮箱");
            }
        });
        jQuery("#e_mail").blur(function () {
            if (jQuery(this).val().length > 0) {
                if (jQuery.Validater.Validate(jQuery(this).val(), jQuery.Validater.Regs.email)) {
                    if (!ZTravle.CheckExists("user", jQuery("#e_mail").val())) {
                        alert("Email已经存在！");
                        jQuery(this).val("");
                    }
                } else {
                    jQuery(this).val("");
                    alert("Email格式错误！");
                }
            }
        });
        jQuery("#order_login").prev().remove();
        jQuery("#order_login").parent().css("padding", "0px");
        jQuery("#btnCloseOrderLogin").click(function () {
            jQuery("#order_login").dialog("close");
        });
        jQuery("#book").submit(function () {
            if (jQuery("#o_user_name").val() == "手机号/邮箱/昵称" && jQuery("#e_mail").val() == "") {
                alert("请输入登录信息！");
                jQuery("#o_user_name").trigger("click");
                jQuery("#o_user_name").trigger("focus");
                return false;
            }
            return true;
        });
    });
</script> 
