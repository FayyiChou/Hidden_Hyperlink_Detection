<!-- 浮动广告2-->
<script type="text/javascript">
// 漂浮广告2
$.fn.floatAD = function (option) {
    var setting = {
        start: ["random", "random"],
        delay: 30,
        step: 1
    };
    return this.each(function () {
        if (option) var opts = $.extend({}, setting, option);

        var $this = $(this);
        var range = [$(window).width(), $(window).height()]
        var width = $this.width();
        var height = $this.height();
        var interval = null;
        var stepX = opts.step;
        var stepY = opts.step;
        var yPos = 0;

        var init = function () {
            var startX = (isNaN(opts.start[0])) ? Math.floor((range[0] - width - 5) * Math.random()) : Math.floor(opts.start[0]);
            var startY = (isNaN(opts.start[1])) ? Math.floor((range[1] - height - 5) * Math.random()) : Math.floor(opts.start[1]);
            $this.css({
                "position": "absolute",
                "left": startX,
                "top": startY
            }).show();
            yPos = startY;
            interval = setInterval(move, opts.delay);
        };

        var move = function () {
            var scrollTop = $(document).scrollTop();
            stepX = ($this.offset().left <= 5) ? opts.step : stepX;
            stepX = (($this.offset().left + width + 5) < range[0]) ? stepX : -stepX;
            $this.css({
                "left": $this.offset().left + stepX,
                "top": yPos + scrollTop
            });

            stepY = (yPos <= 5) ? opts.step : stepY;
            stepY = ((yPos + height + 5) < range[1]) ? stepY : -stepY;
            yPos = yPos + stepY;
        };

        var startHandle = function () {
            interval = setInterval(move, opts.delay);
        };
        var stopHandle = function () {
            clearInterval(interval);
        };

        $this.hover(
            function () {
                stopHandle();
            },
            function () {
                startHandle();
            }
        );

        init();
    });
};

$("#j_floatAD li").floatAD({});
</script>
<!-- 浮动广告2 end-->
</body>
</html>