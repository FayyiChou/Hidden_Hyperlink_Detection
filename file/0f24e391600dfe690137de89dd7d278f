if($(this).html()=="Tropical Flowers" || $(this).html()=="Signature Collection" || $(this).html()=="Classic Collection"){
$(this).replaceWith($('<h2><span>'+$(this).html()+'</span></h2>'))
}
});
$(".mega a").each(function(){
if($(this).html()=="Tropical Flowers" || $(this).html()=="Signature Collection" || $(this).html()=="Classic Collection" ||$(this).html()=="Occasions" ){
$(this).replaceWith($('<h2><span>'+$(this).html()+'</span></h2>'))
}

});	  
$(".pt_menu a").hover(function(event){

if($(this).html()=="<span>Occasions</span>" ||$(this).html()=="Occasions" ){
$(".mega").show();
}
}, function(){
if($(this).html()=="<span>Occasions</span>" ||$(this).html()=="Occasions" ){
$(".mega").hide();
}
});	
$(".mega").hover(function(){$(this).show()},function(){
$(this).hide()
});
});
