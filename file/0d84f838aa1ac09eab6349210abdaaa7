<script language="javascript">
$(function(){
      $("#ny_classTop > ul > li").mouseover(function(e){          
		  if(this == e.target){
            var container = $(this).parent();
            var index = $.inArray(this, $(this).parent().find("li"));
			var panels,temStyle;
			panels = $('#ny_classCont').find(".cont");					
			if (panels.eq(index).is(".cont")){
				if($(container).children().is(".hover")){
				  $(container).children().removeClass("hover").eq(index).addClass("hover");
				}
					panels.addClass("conNo").eq(index).removeClass("conNo");				
				}            			
          }
      });//tab切换
});//jquery的onready ui-tabs-panel divNone0
</script>
