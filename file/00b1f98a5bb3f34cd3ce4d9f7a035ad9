		<div id="index_search">
			<DIV id="clk_searchbox_y">
				<script>
				var old_search_default;
				var search_par,search_method_str;
				//var search_txt,search_action,search_key,search_defaultvalue,search_sub;

				function search_show(n){
					/*for(var i=1;i<=5;i+=2){
						if( i == n ){
							GetObj("qt_"+i).className = "stag";
							GetObj("qt_"+(i-1)).style.display = "none";
							GetObj("qt_"+(i+1)).style.display = "none";
							if( n != 1 ) GetObj("qt_0").style.display = "";
						}else{
							GetObj("qt_"+i).className = "stag1";
							GetObj("qt_"+(i+1)).style.display = "";
						}		
					}*/
					search_method_str = "get"
					switch(n){
						case 1:
							search_txt = "文章搜索";
							search_action = "/article/searchlist.asp";
							search_key = "";
							search_defaultvalue = "";
							search_sub = "kw";
							break;
						case 3:
							search_txt = "软件搜索";
							search_action = "/download/searchlist.asp";
							search_key = "";
							search_defaultvalue = "";
							search_sub = "kw";
							break;
						case 5:
							search_txt = "贴子搜索";
							search_action = "/bbs/query.asp?boardid=0&sType=2";
							search_key = "";
							search_defaultvalue = "";
							search_sub = "keyword";
							search_par = "&boardid=0&sType=2"
							search_method_str = "post"
							break;
					}
					GetObj("search_fm").action = search_action;
					GetObj("search_fm").method = search_method_str;
					GetObj("kw").name = search_sub;
					GetObj("kw").defaultValue = search_defaultvalue;
					GetObj("search_btn").value = search_txt;
					if( old_search_default == GetObj("kw").value )
					{
						GetObj("kw").value = search_defaultvalue;
					}
					else
					{
						if( search_defaultvalue && !GetObj("kw").value ) GetObj("kw").value = search_defaultvalue;
					}
					old_search_default = search_defaultvalue;

					tmp = "";
					var search_more = search_key.split(",");
					var search_str = "";
					for (var i=0; i<search_more.length; i++) {
						if (search_more[i].length>0) {			
								tmp += search_more[i];
								if( tmp.length > 11 ) break;
								search_str += "<a href=" + search_action + "?"+search_sub+"=" + search_more[i] + search_par +" target=\"_blank\">" + search_more[i] + "</a> ";
						}
					}
					GetObj('more_kw').innerHTML = search_str;
				}

				function check_fm(){
					var obj = GetObj("kw");
					if( obj.value == obj.defaultValue ){
						obj.value = "";
					}	
				}
			</script>
				<!-- 分类搜索 -->
