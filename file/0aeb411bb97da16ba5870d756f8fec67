	var sort = 'c';
	//var cityScope = '北京';
	//var cityScope = encodeURI(encodeURI('北京'));
	var cityScope = encodeURI(encodeURI(so_city));
	
/*     map.put("g", "gqzt");  
	    map.put("u", "zt");
