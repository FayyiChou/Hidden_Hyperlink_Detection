			type: "get",
			url:"https://passport.fang.com/login.api",                            
			data: { "Uid": escape(uid), "Pwd":escape(encryptedString(key_to_encode,password)), "Service": escape(service), "Ip": escape(ip)},
			xhrFields: {withCredentials: true},
			crossDomain: true,
			success: function (json) {
				
				var mm =json.Message;
				if(mm.indexOf("Err")>-1){alert(json.Tip);
				return;}
				
				
                      	document.cookie =  "new_loginname=" + json.UserName;
                      	document.cookie =  "new_loginid=" + json.UserID;
                      	window.location.reload();
			}
		});
	}else if (typeof XDomainRequest != "undefined") {
		jQuery.ajax({
			type: "get",
			url: "https://passport.fang.com/login.api",                            
			data: { "Uid": escape(uid), "Pwd": escape(encryptedString(key_to_encode,password)), "Service": escape(service), "Ip": escape(ip) },
			xhrFields: { withCredentials: true},
			crossDomain: true,
			success: function (json) {

				var mm =json.Message;
				if(mm.indexOf("Err")>-1){alert(json.Tip);
				return;}
				
				
				    document.cookie = "new_loginname=" + json.UserName;
				    document.cookie =  "new_loginid=" + json.UserID;
				    window.location.reload();
			}
	});
	} else {
		jQuery.ajax({
			type: "get",
			url:  "https://passport.fang.com/login.api",                              
			dataType: "jsonp",
			jsonp: "callback",
			data: { "Uid": escape(uid), "Pwd": escape(encryptedString(key_to_encode,password)), "Service": escape(service), "Ip": escape(IP) },
			xhrFields: {	withCredentials: true				                        },
			crossDomain: true,
			success: function (json) {

				var mm =json.Message;
				if(mm.indexOf("Err")>-1){alert(json.Tip);
				return;}
				   document.cookie =  "new_loginname=" + json.UserName;
				   document.cookie =  "new_loginid=" + json.UserID;
				   window.location.reload();
				// alert(json.UserName + '---' +  json.UserID + '---' + json.Message + '---' + json.Tip + '---' + json.UrlReferrer + '---' +  json.Ip);
			}
		});
	}
});
})
	    </script>



