function checkKeyWord(keyword,cityScope){

	jQuery.ajax({
		type:'get',
        dataType:'jsonp',
        url:"http://act.news.fang.com/news/checkKeyWord",
        data:"keyWord="+keyword+"&city="+cityScope,
        complete:function(){
        },
        error:function(){
        },
        success:function(msgRes){
        	var res = msgRes.feed.result;
        	if(res!=null && res=='no'){
        		//window.location.href='http://news.fang.com';
        		window.open('http://gz.fang.com/','_self')
        	}else{
        		//jQuery('#searchForm').attr('target','_blank');
        		jQuery('#searchForm').submit();
        	}
        }
	});
	return false;
}
