	var arr =	haun(5,huanNum,ranSize);
	for(var x = 0 ;x<arr.length; x++){
		$(".tj-news")[arr[x]].style.display='table';
	}

});
function haun(n,huanNum,ranSize){
	 var arr=[];
		if(huanNum*5<=ranSize){
			for(var i = 0 ;i<5;i++){
				arr[i] = huanNum*5+i
			}
		}
	 return arr;
	}
