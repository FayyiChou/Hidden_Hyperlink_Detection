<script type="text/javascript">

$(function(){
$("a").hover(function(){
 
   var hrefStr=$(this).attr("href");
   var index=hrefStr.indexOf("ztzh_uuid");
  
   if(index>0)
	{
	   $(this).attr("trueHref",hrefStr);
	   hrefStr=hrefStr.substring(0,hrefStr.indexOf("?"));
	}
	$(this).attr("href",hrefStr)
});

$("a").click(
function(){
var trueHref=$(this).attr("trueHref");
if(trueHref!="undefined"){
$(this).attr("href",trueHref)
}});

});


</script>
