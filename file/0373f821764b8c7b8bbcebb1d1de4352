	}
	//跳转页面方法
	function toPage(_event,_obj){
		if(_event.keyCode!=13){
			return false;
		}
		var _num = _obj.value;
		var countPage = 1;
		if(isNaN(_num)){
			alert("请输入数字");
			return false;
		}
		var url = "index"+"_"+(_num-1)+"."+"htm";
		if(_num<=1||_num==null)
			location.href = "index"+"."+"htm";
		else if(_num>countPage)
			alert("本栏目最多"+countPage+"页");
		else
			location.href = url;
	}
