  <script>
    $(function(){
        function GetQueryString(name)
        {
            var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if(r!=null)return  unescape(r[2]); return null;
        }
        var isempower = GetQueryString('platform');
        if(isempower === 'wxempower'){
            $('.wx-empower-btn').show();
            $('.wx-empower').hide();
        }else{
            $('.wx-empower-btn').hide();
            $('.wx-empower').show();
        }
        

        window.onscroll=function(){
            var scrolltopheght=document.documentElement.scrollTop || document.body.scrollTop;

            if(scrollwidth<=768){
                return;
            }else{
                if(scrolltopheght>=10){
                    $('.navbar').addClass('navbar-show');
                    $('.navbar-brand').css({'background':'url(./source/images/LOGO-04.png) no-repeat','background-size':'146px 44px'});
                }else{
                    $('.navbar').removeClass('navbar-show');
                    $('.navbar-brand').css({'background':'url(./source/images/logo.png) no-repeat','background-size':'auto 44px'});
                }
            }
            
        };
        

        $('.carousel').carousel({
            interval: 8000
        })

        $('.carousel-list').hover(function(){
            $(this).trigger('click');
            return;
        })
        var scrollwidth=document.body.clientWidth;
        if(scrollwidth<=768){
            $('.navbar').addClass('navbar-default');
            return false;
        }
        if(window.location.protocol === 'https:'){
            $('#webchat_js').attr('ewei-domain','https://help.ewei.com');
            $('#webchat_js').attr('src','https://help.ewei.com/source/scripts/eweiBox.new.js');
        }else{
            $('#webchat_js').attr('ewei-domain','http://help.ewei.com');
            $('#webchat_js').attr('src','http://help.ewei.com/source/scripts/eweiBox.new.js');
        }
        
    })
  //百度收录
  $(function() {
    var hostName = window.location.hostname;
    if (hostName.indexOf("ewei.org") == -1) {
      var bp = document.createElement('script');
      bp.src = '//push.zhanzhang.baidu.com/push.js';
      var s = document.getElementsByTagName("script")[0];
      s.parentNode.insertBefore(bp, s);
    }
    if(hostName.indexOf("ewei.org") == -1){
        $('.CMS-case').attr('href','http://www.ewei.com/case.html')
        $('.CMS_sitemap').attr('href','http://www.ewei.com/sitemap.html')
    }else{
        $('.CMS-case').attr('href','http://www.ewei.org/case.html')
        $('.CMS_sitemap').attr('href','http://www.ewei.org/sitemap.html')
    }
    $(".close-mask").on("click", function() {
      $(".video-mask").hide();
      videoDom.pause();
    });
  });

  function showVideo() {
    videoMask.style.display = "block";
    videoDom.play();
  }
