<tr><td class="detail_padding detail_slideshow" id="detailid_155256"><a name="d155256" id="d155256"></a>

<script language="javascript" type="text/javascript">

var dipNavTimeVar_155256 = 1;
$(document).ready(function() {
 		  
	 $("a.dipLeft").click(function(){
		dipNavigationDelay_155256('p');
	 });
		 
	$("a.dipRight").click(function(){
		dipNavigationDelay_155256('n');
	 });
	
	if($('a.dipNav').length){
	 $("a.dipNav").click(function(){
		var id = $(this).attr('id');
		id = id.replace('nav_155256','');
		dipNavigationDelay_155256(id);
	  });
	}
	 
	 //run slideshow
	 rotateDip_155256(1);
	  
	 //pause slideshow
	 var slideshowHover
	 if($('#slideshowHover_155256').length){
		 slideshowHover = $('#slideshowHover_155256').val();
	 }else{
		 slideshowHover = 'imageDipShell_155256';
	 }
	 
	 $('#' + slideshowHover).hover(function(){
		 clearTimeout(slideshowTimer_155256);
		 },function(){
		reRunSlideshow_155256();
	});

	//video hover
	$('.videoShell_155256').hover(function(){
		 clearTimeout(slideshowTimer_155256);
		 },function(){
		reRunSlideshow_155256();
	});
	
	$('.videoPlayerClose').hover(function(){
		clearTimeout(videoVloseTimeOut_155256);
		 $(this).html('Close X');
		 },function(){
		 $(this).html('X');
	});
	
});
	
	//to fix onload run of old slideshow - temp solution
	function runSlideShow155256(){
	}
	
	function reRunSlideshow_155256(){
		var currentNum_155256 = document.getElementById('currentNum_155256').value;
		var time = document.getElementById('slideshowSpeed_155256').value;
		slideshowTimer_155256 = setTimeout(function(){rotateDip_155256(currentNum_155256);},time);
	}
	
	//slideshow
	function rotateDip_155256(num){
		var time = document.getElementById('slideshowSpeed_155256').value;
		slideshowTimer_155256 = setTimeout("switchDip_155256(" + num + ")",time);
	}
	
	function dipNavigationDelay_155256(nums,speed){
			
			var slideshowMotionSpeed = slideshowMotionSpeed_155256();
			if(speed == '' || speed == null){
				if(slideshowMotionSpeed == '' || slideshowMotionSpeed == null){
					speed = 800;
				}else{
					speed = slideshowMotionSpeed;
				}
			}
			
			if(dipNavTimeVar_155256 == 1){
				dipNavigation_155256(nums,speed);
			}else{
				if(typeof dipNavTime_155256 !== "undefined"){
					clearTimeout(dipNavTime_155256);
				}
				dipNavTime_155256 = setTimeout(function(){dipNavigationDelay_155256(nums,speed)},speed);
			}
	}
	
	function dipNavigation_155256(nums,speed){
		clearTimeout(slideshowTimer_155256);
		
		/*if(typeof slideshowJump_155256 !== "undefined"){
			clearTimeout(slideshowJump_155256);
		}*/
		dipNavTimeVar_155256 = 0;
		var slideshowMotion = slideshowMotion_155256();
		var slideshowMotionSpeed = slideshowMotionSpeed_155256();
		var slideshowQuickNavSpeed = slideshowQuickNavSpeed_155256();
		var complete = document.getElementById('complete_155256').value;
		var currentNum = document.getElementById('currentNum_155256').value;
		var time = document.getElementById('slideshowSpeed_155256').value;
		var totalDips = document.getElementById('totalDips_155256').value;
		var direction = directions_155256(slideshowMotion);
		var directionOn = direction.directionOn;
		var directionOff = direction.directionOff;
		var directionSkip = 0;
		
		if(speed == '' || speed == null){
			if(slideshowMotionSpeed == '' || slideshowMotionSpeed == null){
				speed = 800;
			}else{
				speed = slideshowMotionSpeed;
			}
		}
		
		if(currentNum != nums && complete == 1){
		
			if(nums == 'p'){nums = parseInt(currentNum) - 1;}
			if(nums == 'n'){nums = parseInt(currentNum) + 1;}
			nums = parseInt(nums);
			
			if(slideshowMotion != 'f'){
				if(currentNum < nums){directionOn = direction.directionOn;directionOff = direction.directionOff;
				}else{directionOn = direction.directionOff;directionOff = direction.directionOn;}
				//if(currentNum < parseInt(nums) - 1){nums = parseInt(currentNum) + 1;}
				//if(currentNum > parseInt(nums) + 1){nums = parseInt(currentNum) - 1;}
			}
			
			if(nums < 1){nums = totalDips;}
			if(nums > totalDips){nums = 1;}
			
			/*if(!currentNum < parseInt(nums) - 1 && !currentNum > parseInt(nums) + 1){
					newNum = nums;
				}*/
			
			if($('#nav_155256' + currentNum).length){
				$('#nav_155256' + currentNum).removeClass('on');
			}
			if($('#nav_li_155256' + currentNum).length){
				$('#nav_li_155256' + currentNum).removeClass('on');
			}
			if(slideshowMotion != 'f'){
				$('#dipImage_155256' + currentNum).hide("slide", { direction: directionOff }, speed);
			}else{
				$('#dipImage_155256' + currentNum).fadeOut(speed);
			}

			if($('#videoShell_155256' + currentNum).length){
					closePlayer_155256(currentNum);
					$('#videoShell_155256' + currentNum).fadeOut(speed);
				}
			if($('#dipContent_155256' + currentNum).length){
				$('#dipContent_155256' + currentNum).fadeOut(speed);
			}
			
			if($('#nav_155256' + currentNum).length){
				$('#nav_155256' + (nums)).addClass('on');
			}
			if($('#nav_li_155256' + currentNum).length){
				$('#nav_li_155256' + (nums)).addClass('on');
			}
			if(slideshowMotion != 'f'){
				$('#dipImage_155256' + (nums)).show("slide", { direction: directionOn }, speed, function(){
					document.getElementById('complete_155256').value = 1;
					dipNavTimeVar_155256 = 1;
				});
			}else{
				$('#dipImage_155256' + (nums)).fadeIn(speed);
				dipNavTimeVar_155256 = 1;
			}
			if($('#videoShell_155256' + nums).length){
					$('#videoShell_155256' + nums).fadeIn(speed);
				}
			if($('#dipContent_155256' + nums).length){
				$('#dipContent_155256' + (nums)).fadeIn(speed);
			}
			
			document.getElementById('currentNum_155256').value = nums;
			//slideshowTimer=setTimeout("switchDip_155256(" + nums + ")",time);
			//currentNum = document.getElementById('currentNum_155256').value;
			
			/*if(slideshowMotion != 'f'){
				if(currentNum < (nums - 1) && currentNum != totalDips){
					slideshowJump_155256=setTimeout("dipNavigation_155256(" + (nums + 1) + "," + slideshowQuickNavSpeed + ")",speed);
				}
				if(currentNum > (nums + 1)){
					slideshowJump_155256=setTimeout("dipNavigation_155256(" + (nums - 1) + "," + slideshowQuickNavSpeed + ")",speed);
				}
			}*/
			//dipNavTimeVar_155256 = 1;
		}
	}
	
	function switchDip_155256(num,pause){
		clearTimeout(slideshowTimer_155256);
		var timer;
		var nums = num;
		var slideshowMotion = slideshowMotion_155256();
		var speed = slideshowMotionSpeed_155256();
		var currentNum = document.getElementById('currentNum_155256').value;
		var totalDips = document.getElementById('totalDips_155256').value;
		var video = document.getElementById('video_155256').value;
		if(speed == '' || speed == null){
			speed = 800;
		}
		
		var direction = directions_155256(slideshowMotion);
		var directionOn = direction.directionOn;
		var directionOff = direction.directionOff;
		
		if(totalDips > 1 && video == 0){
		
			if(nums < 1){nums = 1;}
			if(nums > totalDips){nums = totalDips;}
			
			if($('#nav_155256' + nums).length){
				$('#nav_155256' + nums).removeClass('on');
			}
			
			if($('#nav_li_155256' + nums).length){
				$('#nav_li_155256' + nums).removeClass('on');
			}
			
			if(slideshowMotion != 'f'){
				if($('#videoShell_155256' + nums).length){
					$('#videoShell_155256' + nums).hide();
				}
				$('#dipImage_155256' + nums).hide("slide", { direction: directionOff }, speed);
			}else{
				$('#dipImage_155256' + nums).fadeOut(speed);
			}
			
			if($('#videoShell_155256' + nums).length){
				$('#videoShell_155256' + nums).fadeOut(speed);
			}
			if($('#dipContent_155256' + nums).length){
				$('#dipContent_155256' + nums).fadeOut(speed);
			}
			
			if(nums >= totalDips){nums = 1;}else{nums = nums + 1;}
			
			if($('#nav_155256' + nums).length){
				$('#nav_155256' + nums).addClass('on');
			}
			
			if($('#nav_li_155256' + nums).length){
				$('#nav_li_155256' + nums).addClass('on');
			}
			
			if(slideshowMotion != 'f'){
				$('#dipImage_155256' + nums).show("slide", { direction: directionOn }, speed, function(){
				if($('#videoShell_155256' + nums).length){
					$('#videoShell_155256' + nums).fadeIn(speed);
					}	
				});
			}else{
				$('#dipImage_155256' + (nums)).fadeIn(speed);
			}
			
			if(slideshowMotion == 'f'){
				if($('#videoShell_155256' + nums).length){
					$('#videoShell_155256' + nums).fadeIn(speed);
				}
			}
			if($('#dipContent_155256' + nums).length){
				$('#dipContent_155256' + (nums)).fadeIn(speed);
			}
				
			if(num < totalDips){timer = num + 1;}else{timer = 1;}
			rotateDip_155256(timer);
			document.getElementById('currentNum_155256').value = nums;
		}
	}
	
	function slideshowMotion_155256(){
		if($('#slideshowMotion_155256').length){
			slideshowMotion = document.getElementById('slideshowMotion_155256').value;
		}else{
			slideshowMotion = 'f';
		}
		if(slideshowMotion == ''){slideshowMotion = 'f'}
		return slideshowMotion;
	}
	
	function slideshowMotionSpeed_155256(){
		if($('#slideshowMotionSpeed_155256').length){
			slideshowMotionSpeed = document.getElementById('slideshowMotionSpeed_155256').value;
		}else{
			slideshowMotionSpeed = '';
		}
		return slideshowMotionSpeed;
	}
	
	
	function slideshowQuickNavSpeed_155256(){
		if($('#slideshowQuickNavSpeed_155256').length){
			slideshowQuickNavSpeed = document.getElementById('slideshowQuickNavSpeed_155256').value;
		}else{
			slideshowQuickNavSpeed = 50;
		}
	
		return slideshowQuickNavSpeed;
	}
	
	//motion direction
	function directions_155256(slideshowMotion){
		switch(slideshowMotion){
			case 'sr':
			directionOn = 'left';
			directionOff = 'right';
			break;
			case 'su':
			directionOn = 'down';
			directionOff = 'up';
			break;
			case 'sd':
			directionOn = 'up';
			directionOff = 'down';
			break;
			default:
			directionOn = 'right';
			directionOff = 'left';
		}
		
		return {directionOn: directionOn, directionOff: directionOff};
	}

	//video
	function playvideo_155256(num){
		getVideoiframeSrc_155256(num);
		$('#videoPlayerClose_155256' + num).html('Close X');
		$('#videoPlayer_155256' + num).fadeIn(800);
		$('#video_155256').val(1);
		videoVloseTimeOut_155256 = setTimeout("$('#videoPlayerClose_155256" + num + "').html('X')",2500);
	}
	
	function closePlayer_155256(num){
		$('#videoPlayer_155256' + num).fadeOut(200);
		$('#video_155256').val(0);
		var video = $('#VideoPlayback_155256' + num).attr("src");
		$('#VideoPlayback_155256' + num).attr("src","");
		$('#VideoPlayback_155256' + num).attr("src",video);
		reRunSlideshow_155256();
	}
	
	function getVideoiframeSrc_155256(num){
		var iframeSrc;
		if($('.Videoiframe_155256' + num).length){
			iframeSrc = $('.Videoiframe_155256' + num).attr('src');
			var n = iframeSrc.indexOf("?");
			if(n != -1){
				$('.Videoiframe_155256' + num).attr('src',iframeSrc + '&wmode=opaque');
			}else{
				$('.Videoiframe_155256' + num).attr('src',iframeSrc + '?wmode=opaque');
			}
			
		}
	}
	
</script>


