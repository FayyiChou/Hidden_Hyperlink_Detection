<script type="text/javascript" charset="utf-8" src="http://s.baihuaji.cn/js/jquery/jquery18.js?v=20140117.js"></script><script type="text/javascript" charset="utf-8" src="http://s.baihuaji.cn/js/jquery/jquery.list-display-min.js?v=20120903.js"></script><script type="text/javascript" charset="utf-8" src="http://s.baihuaji.cn/js/jquery/slides.js?v=20140117.js"></script><script type="text/javascript" charset="utf-8" src="http://s.baihuaji.cn/js/jquery/jquery.autocomplete-min.js?v=20131030.js"></script><script type="text/javascript" charset="utf-8" src="http://s.baihuaji.cn/js/jquery/countdown/jquery.countdown.pc.js?v=20160510.js"></script><script type="text/javascript">
//显示下拉分类菜单
$("li.nav_on").mouseover(function(){
	$(this).find(".nav_list_cate").addClass("hover");
	$(this).find(".nav_list_show").show();
}).mouseout(function(){
	$(this).find(".nav_list_cate").removeClass("hover");
	$(this).find(".nav_list_show").hide();
}); 
$("#search_site").bind("click",function(){
	var key = trim(G('search_input').value);
	if(key=='' || key==0)
	{
		url='/goods3129.html';
	}
	else
	{
		url='/goods?key='+encodeURIComponent(key)+'&cid=3129';
	}
	document.location = url;
	return false;
});
//按回车键搜索
$(function(){
    $('#search_input').bind('keypress',function(event){
        if(event.keyCode == "13")    
        {
        	var key = trim(G('search_input').value);
    		if(key=='' || key==0)
    		{
    			url='/goods3130.html';
    		}
    		else
    		{
    			url='/goods?key='+encodeURIComponent(key)+'&cid=3130';
    		}
    		document.location = url;
    		return false;
        }
    });
});
//商品列表鼠标进出样式切换
$(".offer_content>ul .offer_li_sp").mouseover(function(){
	$(this).children(".offer_li_hover").addClass("li_hover");
}).mouseout(function(){
	$(this).children(".offer_li_hover").removeClass("li_hover");
}); 
$(".offer_content>ul .offer_li").mouseover(function(){
	$(this).css("border","1px solid #FBDCD9");
// 	$(this).children(".flower_txt").css("border-top","1px solid #FBDCD9");
}).mouseout(function(){
	$(this).css("border","1px solid #E3E3E3");
// 	$(this).children(".flower_txt").css("border-top","1px solid #E3E3E3");
}); 
//小米轮播js
$(function(){
	$("#xmSlide").xmSlide({
		width: 1200,
		height: 455,
		responsiveWidth:1200,
		pagination: {
			effect: "fade"  //可换成"slide"
		},
		effect: {
			fade: {
				speed: 400
			}
		},
		play: {
			effect: "fade", //可换成"slide"
			interval: 4000,
			auto: true,
			pauseOnHover: true,
			restartDelay: 2500
		}
	});
});
//获取相关搜索词
var oAtResp = $('#search_input').autocomplete({
    serviceUrl: '/default/info/xsdata',//获取数据的后台页面
    width: 446,//提示框的宽度
    delimiter: /(,|;)\s*/,//分隔符
    onSelect: {},//选中之后的回调函数
    deferRequestBy: 0, //单位微秒
    params: {index:0},//参数
    noCache: false //是否启用缓存 默认是开启缓存的
});
if(typeof(oAtResp) !== 'undefined' && ! oAtResp)
{
	oAtResp.result(function(event, data, formatted) {
		var key = trim(G('search_input').value);
		if(key=='' || key==0)
		{
			url='/goods3129.html';
		}
		else
		{
			url='/goods?key='+encodeURIComponent(key)+'&cid=3129';
		}
		document.location = url;
		return false;
	});
}

//收藏本站
function AddFavorite(title, url) {
    try {
        window.external.addFavorite(url, title);
    }
    catch (e) {
        try {
            window.sidebar.addPanel(title, url, "");
        }
        catch (e) {
            alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请使用Ctrl+D进行添加");
        }
    }
}
//头部手机网站二维码显示与关闭
$('#mobile_web').bind('click',function(){
	var is_display;
	is_display = $("#qr_code").css('display');
	if(is_display == 'none'){
		$("#qr_code").css('display','block');
	}else {
		$("#qr_code").css('display','none');
	}
});
$('#new_close').bind('click',function(){
	$("#qr_code").css('display','none');
});
/**
 * 秒杀倒计时
 */
$('.countdown').each(function(){
    var ts = (new Date()).getTime() + parseInt($(this).attr("difftime"));
    var _this = this;
    $(_this).countdown({
    	timestamp	: ts,
    	callback	: function(days, hours, minutes, seconds){
    		//
    		if(days==0 && hours==0 && minutes==0 && seconds==0)
    		{
    			//倒计时截至结束
    		}
    	}
    });
});
/**
 * 营销Tab切换
 */
function switchTab(o, n)
{
    $(o).siblings().removeClass('now');
    $(o).addClass('now');
    //
    $(".promot_tabs_bd").hide();
    $("."+n).show();
}
//更多跳转
$(".promot_tabs a.more").on("click", function(){
	var sType = $(".promot_tabs li.now").attr("data");
	location.href = "/activity/" + sType;
});
</script>
