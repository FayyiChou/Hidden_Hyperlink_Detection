    function imgSize() {
        var winWidth = $(window).width();
        var num = $(".detail-imglist li").length;
        $(".detail-imglist li").width(winWidth);
        $(".detail-imglist").width(winWidth * num);
        var Index = $("#J-detail-imglist .on").index();
        $("#J-detail-imglist").css({marginLeft: -Index * winWidth + "px"})
    }

