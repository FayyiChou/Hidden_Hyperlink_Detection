        $("#current-num").html(liIndex_);
        $("#total-num").html(totalNum_);

            $("#album-name-id").html(albumName_);

        imgSize();

        /**** 往左边拉的时候，回复rightItemHave属性值 ****/
        if (!rightItemHave && $(".on").prevAll().length >= 1) {
            rightItemHave = true;
        }
    }

    function callback_fun_right_(winCurWid_) {

