    function loadPic(pageNo,nextAlbumId,albumIndex,callback) {
        var ajaxUrl = "//marry.19lou.com/pic/wap/getPicListByAlbum/" + nextAlbumId + "/3/" + albumIndex + "/" + pageNo + "/2";
        $.post(ajaxUrl, {"searchTag": "", "sort": "createdTime!"}, function (data) {
            var dataItem = data.data;
            if(dataItem.afterHtml.length == ""){
                leftItemHave = false;
            }
            $("#J-detail-imglist").append(dataItem.afterHtml);
            if (dataItem.albumId && parseInt(dataItem.albumId) > 0) {
                curAlbumId = dataItem.albumId;
            }
            if (dataItem.curPageNum && parseInt(dataItem.curPageNum) > 0) {
                wapCurPage = dataItem.curPageNum;
            }
            var winCurWid_ = $("#J-detail-imglist li").width();
            callback(winCurWid_);

        });
    }

    function loadPicPrev(pageNum, albumId, albumIndex,callback) {
        if (albumIndex > 0) {
            var ajaxUrl = "//marry.19lou.com/pic/wap/getPicListByAlbum/" + albumId + "/3/" + albumIndex + "/" + pageNum + "/1";
            $.post(ajaxUrl, {"searchTag": "", "sort": "createdTime!"}, function (data) {
                var dataItem = data.data;
                if (dataItem.afterHtml.length == "") {
                    rightItemHave = false;
                }
                $(dataItem.afterHtml).insertBefore($("#J-detail-imglist li").eq(0));

                if (dataItem.afterHtml.length > 0) {
                    curIndex = $("#J-detail-imglist .on").index();
                    imgSize();
                }
                callback();
            });
        }
    }

    /**** direct  1左边加载   2右边加载 ****/
    function loadPicSingle(direct, pageNo,callback) {
        if(pageNo <= 0){
            if(direct == 1){
                rightItemHave = false;
            }else {
                leftItemHave = false;
            }
            return;
        }
        var ajaxUrl = "//marry.19lou.com/pic/wap/getSinglePicList/3/" + pageNo + "/" + direct;
        $.post(ajaxUrl, {"searchTag": "", "sort": "createdTime!"}, function (data) {
            var dataItem = data.data;
            if (dataItem.afterHtml.length == "") {
                if (direct == 1) {
                    rightItemHave = false;
                } else {
                    leftItemHave = false;
                }
            }
            if(parseInt(direct) == 1){
                $(dataItem.afterHtml).insertBefore($("#J-detail-imglist li").eq(0));
