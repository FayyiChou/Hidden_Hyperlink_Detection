    var a=Math.random();
    $('#loginJs_3')[0].src='/SiteFiles/Inner/login/script.aspx?publishmentSystemID=1&styleID=3&type=state&rom='+a+'';
});
function stlLoginState_3(jsonString){
    $('#notLogin_3').hide();
    $('#loginDone_3').show();
    var obj = eval('(' + jsonString + ')');
    if (obj){

        $('#lgn-name_3').html('您好,' +obj.displayname);
    }
}
function stlLoginCallback_3(jsonString){
    var obj = eval('(' + jsonString + ')');
    if (obj){
        if (obj.isSuccess == 'false'){
            alert(obj.message);
        }else{
            stlLoginState_3(jsonString);
        }
    }
}
function stlLogout_3(){
    var a=Math.random();
    $('#input_login_3').get(0).reset();
    $('#notLogin_3').show();
    $('#loginDone_3').hide();
    $('#loginJs_3')[0].src='/SiteFiles/Inner/login/script.aspx?publishmentSystemID=1&styleID=3&type=logout&rom='+a+'';
    var date = new Date();
    date.setTime(date.getTime() - 10000);
    document.cookie = 'SITESERVER.USER.AUTH' + "=a; expires=" + date.toGMTString();
    document.cookie = 'SITESERVER.USER.USERNAME' + "=a; expires=" + date.toGMTString();
}
function stlLoginSubmit(){
    $('#input_login_3').ajaxSubmit({
        success: function(data){
            var obj = eval('(' + data + ')');
            if(obj.isSuccess == 'false'){
                alert(obj.message);
            }
            else{
                stlLoginCallback_3(data);
            }
        }
    });
}
</script><!-- 登录前 begin -->
