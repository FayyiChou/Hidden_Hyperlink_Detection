            $(".s_text").mouseover(function () {
                $(this).find(".o_text").css("z-index", "99").show();
                $(this).find("i").addClass("i_o");
            })
            $(".s_text").mouseout(function () {
                $(this).find(".o_text").hide();
                $(this).find("i").removeClass("i_o");
