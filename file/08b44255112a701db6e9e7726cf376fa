<script language="JavaScript" type="text/javascript">
<!--
var width = 0;

function doc_width(){
	if (document.documentElement && document.documentElement.clientWidth) {
		width = document.documentElement.clientWidth;
	} else if(document.body && document.body.clientWidth) {
		width = document.body.clientWidth;
	} else if (window.innerWidth) {
		width = window.innerWidth;
	} else 	if (document.all && document.all.item) {
		width = event.clientX;
	}
}

function place() {
	doc_width();
	if (width < 880) {
		transfer_one_to_three();
	} else if (width < 1130) {
		transfer_one_to_two();
	}	
}

function win_resize() {
	var state = 0;
    
	doc_width();
	if (document.getElementById('tdnews2')) {
		state = 1;
	} else if (document.getElementById('tdnews1')) {
		state = 2;
	} else {
		state = 3;
	}
	if (state == 1 && (880 <= width && width < 1130)) {
		transfer_one_to_two();
	} else if (state == 1 && width < 880) {
		transfer_one_to_three();
	} else if (state == 2 && width < 880) {
		transfer_two_to_three();
	} else if (state == 2 && width >= 1130) {
		transfer_two_to_one();
	} else if (state == 3 && width >= 1130) {
		transfer_three_to_one();
	} else if (state == 3 && (880 <= width && width < 1130)) {
		transfer_three_to_two();
	}
}

function transfer_one_to_two() {
	var divnews2, special, tdnews2, trnews;

	divnews2 = document.getElementById('divnews2');
	special = document.getElementById('special');
	special.width = 355;
	special.appendChild(divnews2);
	tdnews2 = document.getElementById('tdnews2');
    trnews = tdnews2.parentNode;
    trnews.removeChild(tdnews2);
}

function transfer_one_to_three() {
	var divnews1, divnews2, spacer1, special, tdnews1, tdnews2, trnews;

	special = document.getElementById('special');
	special.width = 355;
	divnews1 = document.getElementById('divnews1');
	special.appendChild(divnews1);
	divnews2 = document.getElementById('divnews2');
	special.appendChild(divnews2);
	trnews = special.parentNode;
	spacer1 = document.getElementById('spacer1');
	trnews.removeChild(spacer1);
	tdnews1 = document.getElementById('tdnews1');
	trnews.removeChild(tdnews1);
	tdnews2 = document.getElementById('tdnews2');
}


function transfer_two_to_three() {
	var divnews1, divnews2, spacer1, special, tdnews1, trnews;

    divnews1 = document.getElementById('divnews1');
    divnews2 = document.getElementById('divnews2');
    special = document.getElementById('special');
	special.width = 355;
    special.insertBefore(divnews1, divnews2);
    trnews = special.parentNode;
    tdnews1 = document.getElementById('tdnews1');
    trnews.removeChild(tdnews1);
	spacer1 = document.getElementById('spacer1');
	trnews.removeChild(spacer1);
}

function transfer_two_to_one() {
	var divnews2, special, tdnews2, trnews;

    special = document.getElementById('special');
	special.width = 300;
    trnews = special.parentNode;
    tdnews2 = document.createElement('td');
    tdnews2.id = 'tdnews2';
	tdnews2.width = 355;
    trnews.insertBefore(tdnews2, special);
    divnews2 = document.getElementById('divnews2');
    tdnews2.appendChild(divnews2);
}

function transfer_three_to_one() {
	var divnews1, divnews2, img1, spacer1, special, td, tdnews1, tdnews2, trnews;
	
    special = document.getElementById('special');
	special.width = 300;
    trnews = special.parentNode;
    tdnews2 = document.createElement('td');
	tdnews2.width = 355;
	tdnews2.id = 'tdnews2';
    trnews.insertBefore(tdnews2, special);
    divnews2 = document.getElementById('divnews2');
    tdnews2.appendChild(divnews2);
    spacer1 = document.createElement('td');
    spacer1.id = 'spacer1';
    spacer1.width = 20;
	img1 = document.createElement('img');
	img1.width = 20;
	img1.height = 1;
	img1.border = 0;
	img1.src = '/img/spacer.gif';
	spacer1.appendChild(img1);
    trnews.insertBefore(spacer1, tdnews2);
    tdnews1 = document.createElement('td');
    tdnews1.id = 'tdnews1';
	tdnews1.width = 355;
    trnews.insertBefore(tdnews1, spacer1);
    divnews1 = document.getElementById('divnews1');
    tdnews1.appendChild(divnews1);
}

function transfer_three_to_two() {
	var img1, divnews1, spacer1, special, tdnews1, trnews;
	
    special = document.getElementById('special');
	special.width = 355;
    trnews = special.parentNode;
    spacer1 = document.createElement('td');
    spacer1.id = 'spacer1';
    spacer1.width = 20;
	img1 = document.createElement('img');
	img1.width = 20;
	img1.height = 1;
	img1.border = 0;
	img1.src = '/img/spacer.gif';
	spacer1.appendChild(img1);
    trnews.insertBefore(spacer1, special);
    tdnews1 = document.createElement('td');
    tdnews1.id = 'tdnews1';
	tdnews1.width = 355;
