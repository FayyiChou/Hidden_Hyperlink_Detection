kj.onload(function(){
	var w = kj.w();
	if(w<1150) return;
	var left = 1010+(w - 1000)/2;
	kj.set("#id_kjcms_logo" , "style.left" , left+"px");
	kj.show("#id_kjcms_logo");
});
