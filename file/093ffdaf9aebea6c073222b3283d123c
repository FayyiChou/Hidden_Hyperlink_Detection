<script>
pageArea.city_id = kj.toint('');
pageArea.depth = kj.toint('5');
var thisjs = new function() {
	this.tips = function(id) {
		var arr = kj.obj("#id_areatips ul");
		kj.hide(arr);
		kj.show(arr[id]);
	}
}
</script>