<script type="text/javascript">
$(function(){
	var obj=$('.xgcp');
	var len=$('.xgcp li').size();
	console.log(len);
	if(len==0){
		obj.append(addpro(3));
	}else if(len==1){
		obj.append(addpro(2));
	}else if(len==2){
		obj.append(addpro(1));
	}
})
 function addpro(len){
  var str="";
  for(var i=0;i<len;i++){
    pro=$('.rc_pro dd').eq(i).html();
    pro='<li>'+pro+'</li>';
    str+=pro;
  }
  return str;
}
</script>
