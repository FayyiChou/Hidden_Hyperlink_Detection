<script>
    $(function(){
        $('.hosReoc-mp .tab-box').tab({
            type:'mouseover',
            slideTab:{
                sliding:true,
                trigger:'.ant-sliding'
            }
        });
        $('.spHosReoc-mp .tab-box').tab({
            type:'mouseover',
            slideTab:{
                sliding:true,
                trigger:'.ant-sliding'
            }
        });
        $('.jumpBox .cross-ic').on('click',function(){
            $(this).closest('.jumpBox').hide();
        });
        $('.jumpBox .confirm-btn').on('click',function(){
            $(this).closest('.jumpBox').hide();
        });

        $('.selectDescriBox').children('.selectDescri').eq(0).show().siblings().hide();
        $('.table3c-db-thin-yp').find('.tableBodyBox')
                .children('.tableBody').eq(0).show().siblings().hide();
        $('.section-selector .selector').on('mouseover',function(){
            var idx = $(this).index();
            $(this).addClass('current').siblings('.selector').removeClass('current');
            $(this).parent().siblings('.selectDescriBox').children('.selectDescri').eq(idx).show().siblings().hide();
            $(this).parent().parent().siblings('.table3c-db-thin-yp').find('.tableBodyBox')
                    .children('.tableBody').eq(idx).show().siblings().hide();
        });

        $('.itemsBox-exl-mp').children('.items-exl-mp').eq(0).show().siblings().hide();
        $('.evalue-selector .selector').on('mouseover',function(){
            var idx = $(this).index();
            $(this).addClass('current').siblings('.selector').removeClass('current');
            $(this).parent().parent().siblings('.itemsBox-exl-mp').children('.items-exl-mp').eq(idx).show().siblings().hide();
        });

        $('.hosReoc-mp .head-rb-yp').children('a').eq(0).siblings('a').removeClass('dib').addClass('dn');
        $('.hosListBox-rb-yp').children().eq(0).show().siblings().hide();
        $('.hosReoc-mp .selector-head-rb-yp').find('.ant-yahei').children('li').on('mouseover',function(){
            var idx =$(this).index();
            $('.hosListBox-rb-yp').children().eq(idx).show().siblings().hide()
                    .closest('.recommandBox-yp').find('.head-rb-yp').children('a').eq(idx).removeClass('dn').addClass('dib').siblings('a').removeClass('dib').addClass('dn');
        });
        $('.spHosReoc-mp .head-rb-yp').children('a').eq(0).siblings('a').removeClass('dib').addClass('dn');
        $('.hosListBox-simple-rb-yp').children().eq(0).show().siblings().hide();
        $('.spHosReoc-mp .selector-head-rb-yp').find('.ant-yahei').children('li').on('mouseover',function(){
            var idx =$(this).index();
            $('.hosListBox-simple-rb-yp').children().eq(idx).show().siblings().hide()
                    .closest('.recommandBox-yp').find('.head-rb-yp').children('a').eq(idx).removeClass('dn').addClass('dib').siblings('a').removeClass('dib').addClass('dn');
        });


        var jumpTimer;
        var timerToJump = function(t,l){
            if(t>=l){
                $('.notice-query-jb .sec').html(t);
                t--;
                jumpTimer = setTimeout(function(){
                    timerToJump(t,l);
                },1000);
            }else{
                location.href="http://hao123.xywy.com/ask-xnbtyy.htm";
            }
        }
        $('.subBox-sb-headPart-yp .query,.subBox-sb-headPart-yp .reserve').on('click',function(){
            $('.notice-query-jb').show();
            jumpTimer && clearTimeout(jumpTimer);
            timerToJump(5,0);
        });
        $('.notice-query-jb .cross-ic').on('click',function(){
            jumpTimer && clearTimeout(jumpTimer);
        });

        $('.subBox-sb-headPart-yp .infoBox .open').on('click',function(){
            $tObj = $(this).closest('.infoBox').find('.hosDescri');
            if ($tObj.hasClass('hide')){
                $tObj.removeClass('hide');
                $(this).html('收起<<');
            }else{
                $tObj.addClass('hide');
                $(this).html('展开>>')
            }
        });
        $('.subBox-sb-headPart-yp .infoBox .open-ts').on('click',function(){
            $tObj = $(this).closest('.infoBox').find('.treatmentScope');
            if ($tObj.hasClass('hide-ts')){
                $tObj.removeClass('hide-ts');
                $(this).html('收起<<');
            }else{
                $tObj.addClass('hide-ts');
                $(this).html('展开>>')
            }
        });
    });
</script>
<script type="text/javascript">
    function checkFormAsk()
    {
        var obj=document.forms['askform'].elements;
        if(obj['asktitle'].value=='')
        {
            alert("请输入问题！");
            return false;
        }
        if(len(obj['asktitle'].value)<12 || len(obj['asktitle'].value)>8000)
        {
            alert("请输入至少6个字的咨询标题");
            obj['asktitle'].focus();
            return false;
        }
        if(obj['asktitle'].value===obj['asktitle'].defaultValue)
        {
            alert("请输入您要咨询的内容");
            obj['asktitle'].focus();
            return false;
        }
        if(obj['sex'].value=='')
        {
            alert("请选择性别！");
            return false;
        }
        if(obj['age'].value=='')
        {
            alert("请输入年龄！");
            obj['age'].focus();
            return false;
        }
        if(isNaN(obj['age'].value) || obj['age'].value<=0 || obj['age'].value>100)
        {
            alert("年龄应为大于0小于100的正整数！");
            return false;
        }
        window.history.go(0);
    }
    function len(s) {
        var l = 0;
        var a = s.split("");
        for (var i=0;i<a.length;i++) {
            if (a[i].charCodeAt(0)<299) {
                l++;
            } else {
                l+=2;
            }
        }
        return l;
    }
</script>

<script>
    $(function() {
        $("#zam").click(function(){
            var num = $(".dib > span").text();
            var hosid = $("#hosid").val();
            $.ajax({
                url: "hospital.php?a=clickAlike",
                cache: false,
                type:'GET',
                data: 'num='+(num-1)+'&hosid='+hosid,
                dataType:'text',
                success: function(msg){
                    if(msg == '-1'){
                        $(".notice-thumb-jb").show()
                    }else{
                        var val = $("span[class='thumb-num']").text();
                        $("span[class='thumb-num']").text(parseInt(val)+1);
                        //alert(msg);
                    }
                }
            });
        });
    });
</script>
