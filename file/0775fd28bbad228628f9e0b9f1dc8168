<script type="text/javascript">
    CheckUserLogin = function(){        
        $("#dv_Message").hide();

        if ($("#ctl00_ContentPlaceHolder1_txt_UserAccount").val() == "")
        {
            $("#dv_Message").show();
            $("#dv_Message").html("<span class=\"red\">*</span> 请输入用户名！");
            return false;
        }
        
        if($("#ctl00_ContentPlaceHolder1_txt_Password").val() == "")
        {
            $("#dv_Message").show();
            $("#dv_Message").html("<span class=\"red\">*</span> 请输入密码！");
            return false;
        }        
        return true;
    }
</script>

            
