<script>
//初始化
var def = "1";
function mover_zzjs(object) {
//主菜单
var mm = document.getElementById("m_" + object);
mm.className = "m_li_on_zzjs_net";
//初始主菜单隐藏效果
if (def != 0) {
    var mdef = document.getElementById("m_" + def);
    mdef.className = "m_li_zzjs";
}
//子菜单
var ss = document.getElementById("s_" + object);
ss.style.display = "block";
//初始子菜单隐藏效果
if (def != 0) {
    var sdef = document.getElementById("s_" + def);
    sdef.style.display = "none";
}
}
function mout_zzjs_net(object) {
//主菜单
var mm = document.getElementById("m_" + object);
mm.className = "m_li_zzjs";
//初始主菜单
if (def != 0) {
    var mdef = document.getElementById("m_" + def);
    mdef.className = "m_li_on_zzjs_net";
}
//子菜单
var ss = document.getElementById("s_" + object);
ss.style.display = "none";
//初始子菜单
if (def != 0) {
    var sdef = document.getElementById("s_" + def);
    sdef.style.display = "block";
}
}
</script>
