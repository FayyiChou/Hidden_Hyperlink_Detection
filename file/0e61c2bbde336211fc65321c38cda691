        adlf.addVariable("image","afraid.jpg");
        adlf.addVariable("frontcolor","0x546E00");
        adlf.addVariable("autostart","true");
        adlf.write("adleftflash");	

	adlr= new SWFObject("/ad/flash/ad-03.swf","mediaplayer","110","490","1");
        adlr.addParam("allowfullscreen","true");
        adlr.addVariable("width","110");
        adlr.addVariable("height","490");
        adlr.addVariable("backcolor","0xFFFFFF");
        adlr.addVariable("image","afraid.jpg");
        adlr.addVariable("frontcolor","0x546E00");
        adlr.addVariable("autostart","true");
        adlr.write("adrightflash");
playflash();
</SCRIPT>
