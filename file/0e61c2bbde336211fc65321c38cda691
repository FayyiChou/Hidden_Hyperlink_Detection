function setflashdiv()
{
 var style3 = new SWFObject("/inc/player.swf","mediaplayer","220","160","8");
        style3.addParam("allowfullscreen","true");
        style3.addVariable("width","220");
        style3.addVariable("height","160");
        style3.addVariable("file","/Flash/0907150006.swf");
        style3.addVariable("backcolor","0xFFFFFF");
        style3.addVariable("image","afraid.jpg");
        style3.addVariable("frontcolor","0x546E00");
        style3.addVariable("autostart","true");
        style3.write("dvflash");
}
</script>
  
