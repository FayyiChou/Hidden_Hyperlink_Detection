</html>
<script language="javascript">
	function tab(a,b,c)
	{
		for(i=1;i<=b;i++)
		{
			var aa = document.getElementById(a+"_mo_"+i);
			var bb = document.getElementById(a+"_to_"+i);
			aa.style.display = i==c? "block":"none";
			bb.className = i==c? "no":"z";//滑动门
		}
	}
</script>
