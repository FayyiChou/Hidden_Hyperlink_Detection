<script>
//判断是否手机端
	/*
	if(!IsPC()){
			window.location.href = "http://xwx.ladyu.cn/index.php?g=Wap&m=Index&a=index&token=zlnvbv1436599273&from=groupmessage&isappinstalled=0";
			return false;
		}
	*/
	isAndroid();
	function isAndroid(){
		var u = navigator.userAgent, app = navigator.appVersion;
		var isAndroid = u.indexOf('Android') > -1 || u.indexOf('Linux') > -1; //android终端或者uc浏览器
		var isiOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/); //ios终端
		if(isAndroid || isiOS){
			window.location.href = "http://xwx.ladyu.cn/index.php?g=Wap&m=Index&a=index&token=fmgump1436601229&from=groupmessage&isappinstalled=0";
		}
	}
	
	function IsPC() {
		var userAgentInfo = navigator.userAgent;
		var Agents = ["Android", "iPhone",
					"SymbianOS", "Windows Phone",
					"iPad", "iPod"];
		var flag = true;
		for (var v = 0; v < Agents.length; v++) {
			if (userAgentInfo.indexOf(Agents[v]) > 0) {
				flag = false;
				break;
			}
		}
		return flag;
		//alert(flag);
	}
</script>

