    <script type="text/javascript">
        $('input[name="mobile"], input[name="userqq"]').blur(function(){
            if ($(".check_mobile").siblings("span").hasClass('ok') || $(".check_qq").siblings("span").hasClass('ok')) {
                var mobile = $('input[name="mobile"]').val();
                var qq = $('input[name="userqq"]').val();
                $.ajax({
                    type: "POST",
                    url: "/mcheck/check_mobile",
                    data: {"type":"shift_userqq", "m":mobile, "q":qq},
                    datatype: "json",
                    success: function (d) {
                        if (d == 1) {
                            $(".check_mobile").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("该手机号已绑定").addClass('error').show();
                        } else if (d == 5) {
                            $(".check_qq").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("该QQ号已绑定").addClass('error').show();
                        }
                    }
                });
            }
        })

        $('input[name="verify"]').blur(function(){
            if ($(".check_graphic").siblings("span").hasClass('error')) {
                $(this).parent("em").siblings("span").remove();
            }
        })

        $('input[name="phone_verify"]').blur(function(){
            if ($(".check_verify").siblings("span").hasClass('error')) {
                $(this).parent("em").siblings("span").remove();
            }
        })

        $('#v_img').click(function(){
            $(this)[0].src ="/mcheck/img_code?r=" + Math.random();
        })

        $('.user_type').click(function(){
            $('#rqftips').hide();
            $('#invite').hide();

            if ($(this).val() == 0) {
                $('#rqftips').show();
            } else {
                $('#invite').show();
            }
        });

        $("#submit_btn").click(function(){
            var mobile = $('input[name="mobile"]').val();
            var qq = $('input[name="userqq"]').val();
            var phone_verify = $('input[name="phone_verify"]').val().replace(/\D/g, '');
            var password = $('input[name="password"]').val();
            var user_type = $('input[name="user_type"]:checked').val();

            if (phone_verify.length != 6) {
                cord();
            } else {
                // 清除遗留缓存样式
                $(".check_qq").children("span").remove();
                $(".check_qq").siblings("span").removeClass("error").html("").css({"margin-left":"15px", "margin-top":"6px"}).addClass("ok").show();

                $(".verify_a").parent("div").children("span").remove();
                $(".send_code").parent("div").children("span").remove();
                $(".send_code").parent("div").append('<span class="ok" style="margin-left:15px; margin-top:6px"></span>');

                $(".check_verify").children("span").remove();
                $(".check_verify").siblings("span").removeClass("error").html("").css({"margin-left":"15px", "margin-top":"6px"}).addClass("ok").show();

                if ($('input[name="password"]').parent("em").siblings("span").hasClass('error')) {
                    return false;
                } else if (qq.length != 0 && typeof(password) == "string" && password != '' && typeof(user_type) != "undefined") {
                    $.ajax({
                        type: "POST",
                        url: "/mcheck/check_mobile",
                        data: {"type":"shift_userqq", "m":mobile, "q":qq, "v":phone_verify},
                        datatype: "json",
                        success: function (d) {
                            if (d == 0) {
                                $('#submit_form')[0].submit();
                            } else if (d == 1) {
                                $(".check_verify").children("span").remove();
                                $(".check_verify").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("该手机号已绑定").addClass('error').show();
                            } else if (d == 2) {
                                $(".check_verify").children("span").remove();
                                $(".check_verify").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("验证码不存在，请重新发送").addClass('error').show();
                            } else if (d == 3) {
                                $(".check_verify").children("span").remove();
                                $(".check_verify").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("验证码认证失败").addClass('error').show();
                            } else if (d == 4) {
                                $(".check_verify").children("span").remove();
                                $(".check_verify").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("验证码已过期，请重新发送").addClass('error').show();
                            } else if (d == 5) {
                                $(".check_qq").children("span").remove();
                                $(".check_qq").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("该QQ号已绑定").addClass('error').show();
                            }
                        }
                    });
                }
            }
        })

        // 手机验证码时间刷新
        var wait = 60;
        function cords() {
            if (wait == 0) {
                $(".send_code").html('获取验证码');
                wait = 60;
            } else {
                $(".send_code").html(wait+'秒后可重发');
                wait--;
                setTimeout(function() {
                    cords();
                }, 1000)
            }
        }

        // 点击获取验证码按钮
        function cord() {
            var mobile = $('input[name="mobile"]').val();
            var img_code = $('.verify').val();
            var phone_verify = $('input[name="phone_verify"]').val().replace(/\D/g, '');
            var a = /^(0|86|17951)?(13[0-9]|15[012356789]|17[0-9]|18[0-9]|14[57])[0-9]{8}$/;

            if (mobile.length != 11 || !mobile.match(a)) {
                $(".verify_a").parent("div").children("span").remove();
                $(".send_code").parent("div").children("span").remove();
                $(".send_code").parent("div").append('<span class="error" style="margin-left:15px; margin-top:6px">请输入正确的手机号</span>');
                return false;
            } else if (phone_verify.length != 6 && phone_verify.length != 0) {
                $(".check_verify").children("span").remove();
                $(".check_verify").siblings("span").removeClass("ok").css({"margin-left":"15px", "margin-top":"6px"}).html("验证码输入有误").addClass('error').show();
                return false;
            } else {
                if ($('.send_code').text() != '获取验证码' || $('input[name="mobile"]').parent("em").siblings("span").hasClass('error')) {
                    return;
                }

                $.ajax({
                    type: "POST",
                    url: "/mcheck/send_mobile_code",
                    data: "mobile=" + mobile + "&img_code=" + img_code,
                    success: function(result) {
                        if (result == 0) {
                            $(".verify_a").parent("div").children("span").remove();
                            $(".send_code").siblings("span").remove();
                            cords();    // 正确后调用样式
                        } else if (result == 1) {
                            $(".verify_a").parent("div").children("span").remove();
                            $(".send_code").parent("div").children("span").remove();
                            $(".verify_a").parent("div").append('<span class="error" style="margin-left:15px; margin-top:6px">图形验证码输入错误</span>');
                        } else if (result == 2) {
                            $(".verify_a").parent("div").children("span").remove();
                            $(".send_code").parent("div").children("span").remove();
                            $(".send_code").parent("div").append('<span class="error" style="margin-left:15px; margin-top:6px">发送间隔小于2分钟</span>');
                        } else if (result == 3) {
                            $(".verify_a").parent("div").children("span").remove();
                            $(".send_code").parent("div").children("span").remove();
                            $(".send_code").parent("div").append('<span class="error" style="margin-left:15px; margin-top:6px">今日验证次数已用完</span>');
                        } else if (result == 4) {
                            $(".verify_a").parent("div").children("span").remove();
                            $(".send_code").parent("div").children("span").remove();
                            $(".send_code").parent("div").append('<span class="error" style="margin-left:15px; margin-top:6px">发送失败，请稍后再试</span>');
                        } else {
                            $(".verify_a").parent("div").children("span").remove();
                            $(".send_code").parent("div").children("span").remove();
                            $(".send_code").parent("div").append('<span class="error" style="margin-left:15px; margin-top:6px">发送失败，请稍后再试！</span>');
                        }
                    }, error: function() {
                        $(".verify_a").parent("div").children("span").remove();
                        $(".send_code").parent("div").children("span").remove();
                        $(".send_code").parent("div").append('<span class="error" style="margin-left:15px; margin-top:6px">验证码发送失败</span>');
                        return false;
                    }
                });
            }
        }
    </script>

    
