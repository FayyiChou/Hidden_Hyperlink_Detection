<script>
$('.fqrw').show();
$('.fqrw').click(function(){
	var order_id = parseInt($(this).attr('name'));
	var order_status = parseInt($(this).attr('data'));

	if(!order_id || order_status < 0){
		alert('参数错误！');
		return false;
	}

	if(order_status == 0){
		$.post('/task_brush/task_remove_cart', {order_id:order_id}, function(data) {
			var _data = eval('('+data+')');
			if(_data.error==1){
				alert(_data.message);
				///window.location.href='/task_brush/index';
				return false;
			}
			else{
				location.reload();
			}
		});
	}
	else if(order_status > 0){
		list_brush_task_cancel_by_orderid(order_id);
	}
});


</script>
