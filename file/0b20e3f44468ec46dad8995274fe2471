    ;
function configFun() {
	window.APPCONFIG = null;
	(function() { //初始化window.APPCONFIG
		var app, appDetail, baoPkg, appExt, identityInfo;
		app = window.AppInfoData;
		if (app) {
			appDetail = app.appDetail || null;
			baoPkg = app.baoPkg || null;
			appExt = app.appExt || {};
			identityInfo = app.identityInfo || {};
		}
		if (!(app && appDetail && baoPkg)) {
			return;
		}
		window.APPCONFIG = {
			"g_tk": app.g_tk,
			"app": {
				appName: appDetail.appName,
				appId: appDetail.appId,
				apkId: appDetail.apkId,
				appChannel: appDetail.channel || '',
				packageName: appDetail.packageName,
				sigMd5: [].concat(appDetail.signatureMd5, app.appExt ? app.appExt.signatures : null),
				iosUrl: appExt.iosUrl,
				versionCode: appDetail.versionCode,
				versionName: appDetail.versionName,
				relatedPkgList: identityInfo.relatedPkgList || []
			},
			"appbao": {
				appbaoPkg: "com.tencent.android.qqdownloader",
				appbaoUrl: baoPkg.url || '',
				appbaoMd5: baoPkg.md5,
				appbaoName: "应用宝",
				g_f: baoPkg.channel || ''
			},
			"identityInfo": identityInfo || {}
		};
	})();
}
    function initAppdetail(){
    	window.gTimeStamps.push(Date.now());//dom二次渲染开始，22
        var app, appDetail, baoPkg, appExt, tmpl, root;
        app = window.AppInfoData;
        if(app){
            appDetail = app.appDetail || null;
            baoPkg = app.baoPkg || null;
            appExt = app.appExt || {};
        }

        if(templateId != 2) { 
                root = document.querySelector('#appdetail-wrapper');
                tmpl = document.querySelector('#tpl-appdetail').innerHTML;     
        }

        if(!(app && appDetail && baoPkg)){//信息拉取不到说明地址有误
            
            if(templateId != 2) {
                root.innerHTML = '<p style="text-align: center;margin-top: 20px;font-size: 14px;">抱歉，您访问的页面地址有误！</p>';
            }else {
                root = document.querySelector('body');
                root.innerHTML = '<div style="text-align: center;margin-top: 20px;font-size: 14px;">抱歉，您访问的页面地址有误！</div>';
            }
            return;
        }
        
        if(templateId != 2) {
            if(templateId == 1){
                document.querySelector("#detail-slide").innerHTML = ejs.render(tmpl,{
                    app: appDetail});
                if(appDetail.editorIntro){
                    var div = document.createElement("div");
                    div.className = "detail";
                    div.innerHTML = '<h3>小编推荐</h3><p class="text">' + appDetail.editorIntro + '</p>';
                    root.appendChild(div);
                }
            }else{
                root.innerHTML=ejs.render(tmpl,{
                    app: appDetail});  
            } 
        }
        window.gTimeStamps.push(Date.now());//dom二次渲染完成，23
    }
    configFun();
    initAppdetail();
</script>




