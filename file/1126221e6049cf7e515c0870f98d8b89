	/* show the AV about the virtual match DIV */
	function showAVDIV(){    
		
		setTimeout("document.getElementById('AVShowDiv').style.display='';", 1000);
		//document.getElementById('AVShowDiv').style.display='';
		setTimeout("hiddenAVDIV();", 10000);
	}
	
