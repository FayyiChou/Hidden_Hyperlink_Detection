												<script type="text/javascript">   
												var marqueeMove = {   
													init:function(speed){   
														this.ScrollBox = document.getElementById("ScrollBox");   
														this.sHolder1 = document.getElementById("sHolder1");   
														this.sHolder2 = document.getElementById("sHolder2");   
														this.speed = speed;   
														this.pause = false;   
														this.clientH = this.ScrollBox.clientHeight;   
														this.ScrollBox.onmouseover = function(){marqueeMove.pause = true;};    
														this.ScrollBox.onmouseout = function(){marqueeMove.pause = false;};    
														this.sHolder2.innerHTML = this.sHolder1.innerHTML;   
														setInterval(this.scrolling,this.speed);   
													},   
													scrolling:function(){   
														if(marqueeMove.pause) return;   
														marqueeMove.ScrollBox.scrollTop += 1;   
														if(marqueeMove.ScrollBox.scrollTop >= marqueeMove.sHolder1.offsetHeight){   
															marqueeMove.ScrollBox.scrollTop = 0;   
														}   
													}   
												}   
												marqueeMove.init(100);   
												</script>    
											</td>
										</tr>	                                    										
									</table>									
								  </td>
