                                              <SCRIPT language="javascript">  
													var movePicPanel = document.getElementById("movePicPanel");   
													var movePicPanel1 = document.getElementById("movePicPanel1");   
													var movePicPanel2 = document.getElementById("movePicPanel2"); 
													var speed3=15;//速度数值越大速度越慢     
													movePicPanel2.innerHTML=movePicPanel1.innerHTML;

													function Marquee(){  
														if(movePicPanel2.offsetWidth-movePicPanel.scrollLeft<=0){
															movePicPanel.scrollLeft-=movePicPanel1.offsetWidth;
														}else{     
															movePicPanel.scrollLeft++;
														}     
													}   
													  
													var MyMar=setInterval(Marquee,speed3);  
													movePicPanel.onmouseover=function() {clearInterval(MyMar);}
													movePicPanel.onmouseout=function() {MyMar=setInterval(Marquee,speed3);}
													  
													</SCRIPT></td>
