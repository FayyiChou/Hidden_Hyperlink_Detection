    <script type="text/javascript">
        $(function () {
            //版本提示
            if(navigator.userAgent.indexOf("MSIE")>0){
                var browser_v=navigator.userAgent,
                    ie_v=browser_v.substr(browser_v.indexOf("MSIE")+4,2)
                if(ie_v>=6 && ie_v<=8){
                    $(".notice_passport").show();
                }
                else{
                    $(".notice_passport").hide();
                }
            }
            //协议的勾选
            $("#IsServiceCheck").bind("click",function () {
                var isCheck= $("#IsServiceCheck").attr("checked");
                if (isCheck=="checked") {
                    $("#servicecheckinput").attr("value","已勾选");
                    var errordisplay = $("#servicecheckinput").parent("div").next();
                    if (errordisplay.length > 0 && errordisplay[0].className == "msg_error") {
                        $("#servicecheckinput").parent("div").next("div.msg_error").remove();
                    }
                }
                else{
                    $("#servicecheckinput").attr("value","");
                }
            });
            //点击页面隐藏验证码弹框
            bindHideCheckCodeCap();
            //页面加载隐藏code清空
            $("#CheckCode").attr("value","");
            //弹出验证码
            $("#CheckCodeCapt").click(function(e){
                captcha.refresh();
                $("#captcha").show();
                $("#tippassport").show();
                $("#CheckCodeCapt").attr("value","正在验证");
                captcha.initialize(function (res) {
                    if (res!=null&&res.MessageText.length>0) {
                        $("#CheckCode").attr("value",res.MessageText);
                        $("#captcha").css("display","none");
                        $("#tippassport").css("display","none");
                        $("#CheckCodeCapt").attr("value","验证通过").unbind('click').css("color","green");
                        var divs= $(".msg_error");
                        if (divs!=null&&divs.length>0) {
                            for (var i = 0; i < divs.length; i++) {
                                var div_msg_error=$(divs[i]).text();
                                if (div_msg_error=="请完成验证") {
                                    $(divs[i]).empty();
                                }
                            }
                        }

                    }
                    else{
                        captcha.error();
                    }
                });
                var divs_msg_error= $(".msg_error");
                if (divs_msg_error!=null&&divs_msg_error.length>0) {
                    $(divs_msg_error[divs_msg_error.length-1]).empty();
                }
                e.stopPropagation();
            })
        });
        function openFeedback(strTarget) {

            window.open(strTarget, "_blank", "width=1,height=1,left=100,top=60,scrollbars=0,overflow=auto,status=0");
        }
        function openPopup(url, windowname, w, h) {
            var intTop = 0;
            var intLeft = 0;
            var winWidth = window.screen.availWidth - 12;
            var winHeight = window.screen.availHeight - 50;
            var scrollNorY = 'no';
            if (parseInt(h) == 8888) {
                //如果高度为8888,那么全屏显示
                w = window.screen.availWidth - 12;
                h = window.screen.availHeight - 50;
            }
            else { //从中间呈现
                intTop = (window.screen.availHeight - parseInt(h)) / 2;
                intLeft = (window.screen.availWidth - parseInt(w)) / 2;
                if (intTop < 30) intTop = 0;
                if (intLeft < 30) intLeft = 0;
            }
            if (w > winWidth) {
                w = winWidth;
                scrollNorY = 'yes';
            }
            if (h > winHeight) {
                if (h == 6666) scrollNorY = 'no';
                else scrollNorY = 'yes';
                h = winHeight;
            }
            var windowconfig = "status=no,scrollbars=" + scrollNorY + ",top=" + intTop + ",left=" + intLeft + ",resizable=0,width=" + w + ",height=" + h;
            subwin = window.open(url, windowname, windowconfig);
            if (subwin) {
                subwin.focus();
                return subwin;
            }
        }
    </script>
    <script type="text/javascript">
        window.checkcodeshowtime = (new Date()).getTime();
        function showcheckcode() {
            window.checkcodeshowtime = (new Date()).getTime();
            //document.getElementById("checkimg").src='/checkcode/imgrd?r=' + Math.random();
            //document.getElementById("checkimg").src='/chk/GetCap?r=' + Math.random();
        }
        function checkcodefocus() {
            var pastsec = (new Date()).getTime() - window.checkcodeshowtime;

            //认定用户在20秒钟之内可以输入验证码并提交完成验证，所以减去20秒
            var checksec = 580000;

            if(pastsec > checksec){
                showcheckcode();
            }
        }

        $(function () {
            var vOption = {
                onkeyup: false,
                rules: {
                    LoginName: { required: true },
                    Password: { required: true },
                    CheckCode: { required: true },
                    servicecheckinput: { required: true }

                },
                messages: {
                    LoginName: { required: "请输入邮箱或手机号" },
                    Password: { required: "请输入密码" },
                    CheckCode: { required: "请完成验证" },
                    servicecheckinput: { required: "请勾选协议后登录" }
                }
                , errorElement: "div"
            };
            window.formvalidator = $("#form1").validate(vOption);
            var xyuan = "";
            if(xyuan != null && xyuan.length > 0){
                $("#areg").attr("href","http://rd2.zhaopin.com/portal/myrd/reg.asp?za=campus");
            }

            $("#loginbutton").click(function () {
                //如果验证码没有验证，则直接弹出验证码的验证框
                var checkCodeCaptText= $("#CheckCodeCapt").val();
                if (checkCodeCaptText=="点击完成验证") {
                    $("#CheckCodeCapt").click();
                    return false;
                }
                if (formvalidator.form()) {
                    document.getElementById('form1').submit();
                }
                else {
                    formvalidator.focusInvalid();
                    //每次弹窗出现错误，就从新xuanr
                    captcha.refreshNochangevalue();
                    captcha.initialize(function (res) {
                        if (res!=null&&res.MessageText.length>0) {
                            $("#CheckCode").attr("value",res.MessageText);
                            $("#captcha").css("display","none");
                            $("#tippassport").css("display","none");
                            $("#CheckCodeCapt").attr("value","验证通过").unbind('click').css("color","green");
                            var divs= $(".msg_error");
                            if (divs!=null&&divs.length>0) {
                                for (var i = 0; i < divs.length; i++) {
                                    var div_msg_error=$(divs[i]).text();
                                    if (div_msg_error=="请完成验证") {
                                        $(divs[i]).empty();
                                    }
                                }
                            }

                        }
                        else{
                            captcha.error();
                        }
                    });
                    return false;
                }
            });

            $("#LoginName,#Password,#CheckCode").keyup(function (ev) {
                ev = ev || window.event;
                var code = (ev.keyCode || ev.which);
                if (code == 10 || code == 13) {
                    $("#loginbutton").click();
                }
            });

            showcheckcode();

        });
        //点击隐藏验证码的弹窗
        function bindHideCheckCodeCap() {
            $(document).click(function(e){
                var obj=$("#captcha");
                var disValue=obj.css("display");
                if (disValue=="block") {
                    obj.css("display","none");
                    $("#tippassport").css("display","none");
                    captcha.refresh();
                    $("#CheckCodeCapt").attr("value","点击完成验证");
                }
                e.stopPropagation();
            });
        }
    </script>


