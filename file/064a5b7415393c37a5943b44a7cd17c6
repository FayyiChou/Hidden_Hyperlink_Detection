   <script>
   var speed=50
   demoxx2.innerHTML=demoxx1.innerHTML
   function Marquee(){
   if(demoxx2.offsetTop-demoxx.scrollTop<=0)
   demoxx.scrollTop-=demoxx1.offsetHeight
   else{
   demoxx.scrollTop++
   }
   }
   var MyMar=setInterval(Marquee,speed)
   demoxx.onmouseover=function() {clearInterval(MyMar)}
   demoxx.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
   </script>




