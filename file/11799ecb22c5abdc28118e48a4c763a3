	if(startPage<0)startPage=0;
document.write("<ul class='pagenum'>");
	for(var i=startPage;i<endpage;i++){
		if(currentPage==i)
document.write("<li><a class='active'>"+(i+1)+"</a></li>&nbsp;");
	else if(i==0){
document.write("<li><a  href=\"t20161026_8784238.htm\">"+1+"</a></li>&nbsp;");
		 }else
document.write("<li><a href=\"t20161026_8784238"+"_"+i+"."+"htm\" >"+(i+1)+"</a></li>&nbsp;");}


	if(countPage>1&&currentPage!=(countPage-1))
document.write("</ul><a href=\"t20161026_8784238"+"_"+nextPage+"."+"htm\" >下一页</a>&nbsp;");
	else
document.write("</ul><a class='tow'>下一页</a>&nbsp;");
        if (endPage>0)
	document.write("&nbsp;|&nbsp;<a  href='"+docurl+"_"+endPage+".html'>尾页</a>");
        else
	document.write("&nbsp;|&nbsp;<a  href='"+docurl+".html'>尾页</a>");
document.write("</div>");
</script><div class="page"><a href="t20161026_8784238.html">首页</a>&nbsp;|&nbsp;<a>上一页</a>&nbsp;<ul class="pagenum"><li><a class="active">1</a></li>&nbsp;<li><a href="t20161026_8784238_1.htm">2</a></li>&nbsp;<li><a href="t20161026_8784238_2.htm">3</a></li>&nbsp;</ul><a href="t20161026_8784238_1.htm">下一页</a>&nbsp;&nbsp;|&nbsp;<a href="t20161026_8784238_2.html">尾页</a></div>
                            </section>
