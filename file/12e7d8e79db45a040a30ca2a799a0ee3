<div class="ga-wrap">
    <div class="op-bg"></div>
    <div class="loading-wrap">
        <img class="load" src="/img/loading.gif">
    </div>
    <div class="img-wrap">
        <img src="">
    </div>
    <div class="controll-wrap clearfix">
        <a class="go" href="javascript:;" target="_blank">查看原图</a>
        <a class="close-btn" href="javascript:;" title="关闭">
            <img src="/img/pic_close_z.png">
        </a>
    </div>
    <div class="prev"></div>
    <div class="close-panel"></div>
    <div class="next"></div>
    <div class="count-wrap"></div>
