<script type="text/javascript">
$(document).ready(function() {	
	//get and set user dropdownlist offset position
	var _rightOffset=($('.u-name').width()+15-106)*0.5;
	$(".new_zhul_arrowxia").css("right", _rightOffset+'px');
	
	if(low_IE){
		$(".newzl_search_kuang").removeClass("newsearch_kuang2");
		$(".newzl_search_kuang").prepend('<img src="/img/pubimg/top_search_left.png" style="float: left;" alt="装饰符号">');
		$(".newzl_search_kuang").append('<img src="/img/pubimg/top_search_right.png" style="float: left;" alt="装饰符号">');
	}
	// low verison ie broswer
	$("#sou").click(function() {
		$("#kuang").animate({
			width:'250px',
			paddingLeft:'10px'
		});
		$(this).hide();
		$("#sousuo").show();
		$("#kuang").attr("placeholder","请输入关键词");
	});
	$("#kuang").focus(function(){
		$(this).animate({
			width:'250px'
		});
		$("#sousuo").show();
		$("#kuang").attr("placeholder","请输入关键词");
	})
	$(document).mouseup(function(e){
		var _con = $(".newzl_search_kuang"); 
		if(!_con.is(e.target) && _con.has(e.target).length === 0){
			$("#kuang").animate({
				width:'140px'
			});
			$("#kuang").val("");
			$("#sousuo").hide();
			$("#sou").show();
			$("#kuang").attr("placeholder","");
		}
	});
		// search check 
	$("#sousuo").click(function(event) {
		var value = $("input[name=q]").val();
		if (value == "") {
			alert("请输入关键词");
			event.preventDefault();
		} else {
			$('#search form').submit();
		}
	});

	$("#clicksou").click(function(event) {
		var value = $("input[name=q]").val();
		if (value == "") {
			alert("请输入关键词");
			event.preventDefault();
		} else {
			$('#clicksou').attr('href', 'http://s.zhulong.com/search.php?q=' + value + '&topic=info');
		}
	});

	
	/*
	 * function moreshowHandle()
	 */
	function moreshowHandle() {
		var show = 0;
		$("#more").mousemove(function() {
			show = 1;
			$("#detail").css("display", "block");
		});
		$("#more").mouseout(function(obj) {
			show = 0;
			setTimeout(function() {
				if (show == 0) {
					$("#detail").css("display", "none");
				}
			}, 500);
		});
		$("#detail").mousemove(function() {
			show = 1;
			$("#detail").css("display", "block");
		});
		$("#detail").mouseout(function(obj) {
			show = 0;
			setTimeout(function() {
				if (show == 0) {
					$("#detail").css("display", "none");
				}
			}, 500);
		});
	}
	moreshowHandle();
});
function formsub() {
	var value = $("input[name=q]").val();
	if (value == "") {
		alert("请输入关键词");
		return false;
	} else {
		return true;
	}
}
