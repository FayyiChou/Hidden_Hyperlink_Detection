<script>

    function getuseragent(){
        var userAgent = navigator.userAgent; //取得浏览器的userAgent字符串
        var isOpera = userAgent.indexOf("Opera") > -1; //判断是否Opera浏览器
        var isIE = userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1 ; //判断是否IE浏览器
        isIE11 = (userAgent.toLowerCase().indexOf("trident") > -1 && userAgent.indexOf("rv") > -1);
        var isChrome = userAgent.indexOf('Chrome') > -1; //判断是否是Chrome浏览器
        var isFirefox = userAgent.indexOf('Firefox') > -1; //判断是否是Firefox浏览器

        if(isIE11){ //IE 11
            return 'IE9+';
        }
        if(isChrome){
            return 'Chrome';
        }
        if(isFirefox){
            return 'Firefox';
        }
        if(isIE){
            var IE5 = IE55 = IE6 = IE7 = IE8 = false;
            var reIE = new RegExp("MSIE (\\d+\\.\\d+);");
            reIE.test(userAgent);
            var fIEVersion = parseFloat(RegExp["$1"]);
            IE55 = fIEVersion == 5.5;
            IE6 = fIEVersion == 6.0;
            IE7 = fIEVersion == 7.0;
            IE8 = fIEVersion == 8.0;
            if(IE55){
                return 'IE55';
            }else if(IE6){
                return 'IE6';
            }else if(IE7){
                return 'IE7';
            }else if(IE8){
                return 'IE8';
            }else{
                return 'IE9+';
            }
        }
    }
    if(mp3path != ''){
        if('Chrome' == getuseragent() || 'Firefox' == getuseragent() || 'IE9+' == getuseragent()){
            document.write('<audio controls style="display: none" id="myaudio"> ' +
                    '<source src="'+mp3path+'" type="audio/mp3" id="mp3"> </audio>');
        }else if('IE55' == getuseragent() || 'IE6' == getuseragent() ||'IE7' == getuseragent() ||'IE8' == getuseragent()){
            document.write('<EMBED id="myaudio" src="'+mp3path+'" autoplay="false" hidden=no style="scrollbar-3dlight-color: #000"></EMBED>');
        }
    }

</script>

