function sina_share(is_save) {
	if(typeof uid=="undefined" || uid<=0){
		widow_Loading();return;
		return ;
	}

    WB2.anyWhere(function(W){
    var not_share = true;
    var text = encodeURIComponent("工程定位测量记录");
	var pic = encodeURI("");
	var tid = '6289141';
	var url_short = window.location.href;
	if(url_short.indexOf('#')>-1){
		url_short=url_short.substr(0,url_short.indexOf('#'));
	}
	var share_sina_url = '/share/Sina?url='+url_short+'&title='+text+'&pic='+pic+'&tid='+tid;
    if(is_save == 1) {
    	$.ajax({
			type:'GET',
			url:'/user/CheckSendSinaWb',
			dataType:"json", 
			async:false,
			success:function(data) {
    			if(data['errNo'] == 0) {
    				window.location.href=share_sina_url;
                	return true;
        		}else{
                   pubMsgbox('今天分享的太多了，明天再分享吧','400','1') 
                   return false;
                }
    			not_share = false;
			}
    	});	

    	if(not_share) {
			return false;
        }
	}
	//验证通过  可以分享
	window.open(share_sina_url,'share_sina');
		return false; 
    });
};
