    <script>
    $(window).bind("scroll", backToTopFun);
    $(function(){backToTopFun();});
   function backToTopFun(){
   		var top = document.documentElement.scrollTop||document.body.scrollTop;
        if (top > 200) {
            $(".back_top").css('display','block');
        }
        else{
            $(".back_top").css('display','none');
        }
   }
	</script>	
