<script type="text/javascript">
    var imgColl = [],
        _index = 1;
    $(".zhul_xx_content img,.zhul_xx_pingtCon img").not($("img[src*='emotion/images']")).each(function(index){
                    
        //原图路径
        var _srcImg=$(this).attr("source_src")||$(this).attr("data-original");
        if(_srcImg!=undefined&&_srcImg.length>0){
            imgColl[index] = _srcImg;
            
            if(($(this).parents("a").length)==0){

                $(this).attr("title","点击查看大图");
                $(this).css("cursor","zoom-in");

                $(this).click(function(){
                    /* Act on the event */
                    var cu_Index = $(".zhul_xx_content img,.zhul_xx_pingtCon img").not($("img[src*='emotion/images']")).index($(this)) + 1;
                    //var cu_Index = $(".zhul_xx_content img[source_src]").not(($(this).parent("a").length)==0).index($(this)) + 1;
                    _index = cu_Index;
                    $(".ga-wrap").show();
                    if ( $.browser.msie && (parseInt($.browser.version)<=7) ){
                        $("html,body").addClass('overflow-b0');
                    }else{
                        $("body").addClass('overflow-b0');
                    }
                    generalImage(imgColl[cu_Index - 1]);
                });
            }
        }
    });
    //loading a image
    function generalImage(_src) {
        $(".ga-wrap .go").attr('href',_src)
        $(".count-wrap").text((_index) + '/' + imgColl.length);
        if (_index < imgColl.length) {
            $('.ga-wrap').find('.next').removeClass('opa').addClass('a-useful');
        }else{
            $('.ga-wrap').find('.next').addClass('opa').removeClass('a-useful');
        }
        if (_index > 1){
            $('.ga-wrap').find('.prev').removeClass('opa').addClass('a-useful');;
        }else{
            $('.ga-wrap').find('.prev').addClass('opa').removeClass('a-useful');
        }
        $('.img-wrap img').hide();
        $('.loading-wrap').show();
        var img = new Image();
        img.src = _src;

        if (img.complete) {

            $('.img-wrap img').attr('src', _src).fadeIn('fast');
            $('.loading-wrap').hide();
            resizeImg();

        } else {
            img.onload = function() {
                $('.img-wrap img').attr('src', _src).fadeIn('fast');
                $('.loading-wrap').hide();
                img.onload = null;
                resizeImg();
            };
        }
        $(window).bind('resize', resizeImg);
        if(mp3path != ''){
            document.getElementById("myaudio").play();
        }

    }

    function nextImg(){
        if (_index < imgColl.length) {
            $(".next").removeClass('opa');
            _index++;
            generalImage(imgColl[_index - 1]);
        }else{
            $('.img-wrap img').addClass('bounce-from-right');
            setTimeout(function(){
                $('.img-wrap img').removeClass('bounce-from-right');
            }, 400);
        }
    }
    function prevImg(){
        if (_index > 1) {
            _index--;
            $(".prev").addClass('opa');
            generalImage(imgColl[_index - 1]);
        }else{
            $('.img-wrap img').addClass('bounce-from-left');
            setTimeout(function(){
                $('.img-wrap img').removeClass('bounce-from-left');
            }, 400);
        }
    }

    $(".next").click(nextImg);
    $(".prev").click(prevImg);
    $(".close-panel").click(function(event) {
        $(".ga-wrap").hide();
        $(window).unbind('resize', resizeImg);
        if ( $.browser.msie && (parseInt($.browser.version)<=7) ){
            $("html,body").removeClass('overflow-b0');
        }else{
            $("body").removeClass('overflow-b0');
        }
        if(mp3path != ''){
            //document.getElementById("myaudio").load();
            document.getElementById("myaudio").pause();
        }

    });
    $(".ga-wrap .close-btn").click(function(event) {
        $(".ga-wrap").hide();
        $(window).unbind('resize', resizeImg);
        if ( $.browser.msie && (parseInt($.browser.version)<=7) ){
            $("html,body").removeClass('overflow-b0');
        }else{
            $("body").removeClass('overflow-b0');
        }
        if(mp3path != ''){
            //document.getElementById("myaudio").load();
            document.getElementById("myaudio").pause();
        }


    });

    function resizeImg() {
        var wh = $(window).height() - 100;
        var ww = $(window).width() - 150;
        var imgNH = $('.img-wrap img').naturalHeight();
        var imgNW = $('.img-wrap img').naturalWidth();
        var _r = wh / ww;
        var _rImg = imgNH / imgNW;

        var imgTH = imgNH * _r;
        var imgTW = imgNW * _r;

        if (wh < imgNH || ww < imgNW) {

            if (_r > _rImg) {
                $('.img-wrap img').width(ww);
                $('.img-wrap img').height(ww * _rImg);
            } else {
                $('.img-wrap img').height(wh);
                $('.img-wrap img').width(wh / _rImg);
            }
        }else{
            $('.img-wrap img').height(imgNH);
            $('.img-wrap img').width(imgNW);
        }
        //position
        $('.img-wrap img').css({
            'top': ($(window).height() - $('.img-wrap img').height()) / 2 + 'px',
            'left': ($(window).width() - $('.img-wrap img').width()) / 2 + 'px'
        });
    }

    (function($) {
        var props = ['Width', 'Height'],
            prop;
        while (prop = props.pop()) {
            (function(natural, prop) {
                $.fn[natural] = (natural in new Image()) ?
                    function() {
                        return this[0][natural];
                    } :
                    function() {
                        var node = this[0],
                            img, value;

                        if (node.tagName.toLowerCase() === 'img') {
                            img = new Image();
                            img.src = node.src,
                                value = img[prop];
                        }
                        return value;
                    };
            }('natural' + prop, prop.toLowerCase()));
        }
    }(jQuery));
</script>
