<script type="text/javascript" language="javascript">
/**
 * for menu-drop type
 */
var type = "";
if (type == 'click') {
	$(function(){
		$('.pro_type_01 li:has(ul)').click(function(event){
	    	if (this == event.target) {
	        	if ($(this).children().is(':hidden')) {
	                $(this).css({background:'url(template/zhaoming-100929-a20/images/minus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'})
	                .children().show();
	                
	              	$(this).siblings().each(function (){
	              		$(this).find("ul").hide();
	              		if ($(this).children().is("ul")){
	              			$(this).css({background:'url(template/zhaoming-100929-a20/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'});
	              	    }
	              	});
	            } else {
	                $(this).css({background:'url(template/zhaoming-100929-a20/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'})
	                .find("ul").hide();
	            }  
	        }
		}).css('cursor','pointer').find("ul").hide().click();
	          
	    $('.pro_type_01 li:has(ul)').css({background:'url(template/zhaoming-100929-a20/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'});
	        	
	    $('.pro_type_01 li:not(:has(ul))').css({cursor: 'pointer','list-style-image':'none'});
	});
} else {
	$(".pro_type_01>ul").droppy();
	$(".pro_type_01 ul ul li:last-child").css("border","0px");
}
</script>

