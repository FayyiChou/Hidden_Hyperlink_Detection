<script type="text/javascript">
//导航条
                $(".sub-menu").hover(function(){
                        $(this).children(".nav-content").show();
                    },function(){
                            $(this).children(".nav-content").hide();
                });
                //.lt-search-select交互效果
                $(".ltss-current,.ltss-option-list").hover(function(){
                    $(".lt-search-select").addClass("lt-search-select-hover");
                },function(){
                    $(".lt-search-select").removeClass("lt-search-select-hover");
                });
                $('#cat').val('zhishi');    //刷新后重新默认为知识
                function set_cat(cat,cat_name){
                    $('#cat').val(cat);
                    $('.ltss-current').html(cat_name);
                    $(".lt-search-select").removeClass("lt-search-select-hover");
                }
    
                function check(){
                    var wd = $('#wd').val();
                    var cat = $('#cat').val();
                    
                    if(wd=="请输入搜索关键词!"){
                        window.open('http://so.lawtime.cn/');
                        return false;
                    }
    
                    var url = '';
                    switch(cat){
                        case 'lawtime': url = 'http://so.lawtime.cn/search/lawtime.php?m=index';break;
                        case 'zixun'  : url = 'http://so.lawtime.cn/search/ask.php?m=zixun';break;
                        case 'zhishi'  : url = 'http://so.lawtime.cn/search/zhishi.php?m=zhishi';break;
                        case 'lawyer'  : url = 'http://so.lawtime.cn/search/lawyer.php?m=lawyer';break;
                        case 'fagui'  : url = 'http://so.lawtime.cn/search/law.php?m=fagui';break;
                        case 'firm'  : url = 'http://so.lawtime.cn/search/firm.php?m=firm';break;
                        case 'anli'  : url = 'http://so.lawtime.cn/search/case.php?m=anli';break;
                        case 'wenji'  : url = 'http://so.lawtime.cn/search/lawyerart.php?m=wenji';break;
                    }
                    $('#search-form').attr('method','GET');
                    $('#search-form').attr('action',url);
                    return true;
                }
</script>


