    var changeindex1 = 1;
    var changeindex2 = 1;
    function autochangeby() {
        if (autoChangeState1 == 0) {
            setTab('rxtj', changeindex1, 6, 'rxtjtitle');
            changeindex1++;
            if (changeindex1 > 6) {
                changeindex1 = 1;
            }
        }
        if (autoChangeState2 == 0) {
            setTab('xptj', changeindex2, 5, 'xptjtitle');
            changeindex2++;
            if (changeindex2 > 5) {
                changeindex2 = 1;
            }
        }

        setTimeout(autochangeby, 10000);
    }
    autochangeby();

