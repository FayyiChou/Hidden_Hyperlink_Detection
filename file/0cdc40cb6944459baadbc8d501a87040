        $(function () {
            $("#daliyday,#daliymoday").mouseover(function () {
                var p = $(this).css("margin-left");
                var id = $(this).attr("id");
                if (id == "daliymoday" && p == "15px") {
                    $(this).attr("src", "http://www.19online.cn/images/daliymodayL.jpg?r=" + Math.random());
                    $("#daliyshow").stop(true, true).animate({ marginTop: "-263px" });
                    //$("#daliyhide").stop(true, true).show(1000);
                    $("#daliyhidepic0,#daliyhidepic1,#daliyhidepic2,#daliyhidepic3,#daliyhidepic4").attr("class", "daliymodaycom");
                }
                else if (id == "daliyday" && p == "355px") {
                    $(this).attr("src", "http://www.19online.cn/images/daliydayL.jpg?r=" + Math.random());
                    $("#daliyshow").stop(true, true).animate({ marginTop: "-263px" });
                    //$("#daliyhide").stop(true, true).show(1000);
                    $("#daliyhidepic0,#daliyhidepic1,#daliyhidepic2,#daliyhidepic3,#daliyhidepic4").attr("class", "daliydaycom");
                }
                else if ((id == "daliyday" && p == "210px") || (id == "daliymoday" && p == "-270px")) {
                    $("#daliyshow").stop(true, true).animate({ marginTop: "0px" });
                    //$("#daliyhide").stop(true, true).hide(2000);
                }
            });

            $("#daliyday,#daliymoday").mouseleave(function () {
                var p = $(this).css("margin-left");
                var id = $(this).attr("id");
                if (id == "daliymoday" && p == "15px") {
                    $(this).attr("src", "http://www.19online.cn/images/daliymodayA.jpg?r=" + Math.random());
                    setTimeout(function () {
                        $("#daliyshow").stop(true, true).animate({ marginTop: "0px" });
                    }, 3000);
                }
                else if (id == "daliyday" && p == "355px") {
                    $(this).attr("src", "http://www.19online.cn/images/daliydayA.jpg?r=" + Math.random())
                    setTimeout(function () {
                        $("#daliyshow").stop(true, true).animate({ marginTop: "0px" });
                    }, 3000);
                }
