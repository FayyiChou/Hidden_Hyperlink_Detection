       var autoChangeState1 = 0;
       var autoChangeState2 = 0;
       function initwinebg() {
           $("#winenav1").css("background-color", "");
           $("#winenav2").css("background-color", "");
           $("#winenav3").css("background-color", "");
           $("#winenav4").css("background-color", "");
           $("#winenav5").css("background-color", "");
       }
       function onwinenavover(obj, id) {
           var o = $(obj);
           initwinebg();
           o.css("background-color", "#FFFFFF");
           $("#winenavdetail" + id).css("display", "");
           $("#ul_change_a2").removeClass("ul_change_a2");
           $("#winenav").css("z-index", "10000");
       }
       function onwinenavout(obj, id) {
           initwinebg();
           $("#winenavdetail" + id).css("display", "none");
           $("#ul_change_a2").addClass("ul_change_a2");
           $("#winenav").css("z-index", "999");
       }
       function onhpmover(o, id) {
           for (var i = 1; i <= 3; i++) {
               if (id == i) {
                   $("#mark" + o + "_" + i).stop().animate({ opacity: '0' }, 300);
               }
               else {
                   $("#mark" + o + "_" + i).stop().animate({ opacity: '0.5' }, 300);
               }
           }
       }

       function onhpout(o) {
           for (var i = 1; i <= 3; i++)
               $("#mark" + o + "_" + i).stop().animate({ opacity: '0' }, 300);
       }
       function onmzthover(id) {
           for (var i = 1; i <= 10; i++) {
               if (id == i) {
                   $("#mzth" + i).stop().animate({ opacity: '0' }, 300);
               }
               else {
                   $("#mzth" + i).stop().animate({ opacity: '0.5' }, 300);
               }
           }
       }
       function onmzthout() {
           for (var i = 1; i <= 10; i++)
               $("#mzth" + i).stop().animate({ opacity: '0' }, 300);
       }
       function setTab(name, cursel, n, cn) {

           for (i = 1; i <= n; i++) {
               var menu = document.getElementById(name + i);
               var con = document.getElementById("con_" + name + "_" + i);
               try {
                   menu.className = i == cursel ? cn + "on" : cn;
                   con.style.display = i == cursel ? "block" : "none";
               } catch (e) {

               }
           }
       }
       function searchFun(obj) {
           var Objs = $("#" + obj);
           var keyword = encodeURIComponent($.trim(Objs.val()));
           if (keyword == "") {
               alert("关键字不能为空！");
               Objs.focus();
               return;
           }
           window.location.href = "/search/" + keyword + "/.html";
       }
       function onWinenavdetailtableOver(o, id, num) {
           for (var i = 1; i <= num; i++) {
               if (id == i) {
                   $("#winenavdetailtable" + o + "_" + i).css("background-color", "#FBFBFB");
               }
               else {
                   $("#winenavdetailtable" + o + "_" + i).css("background-color", "#FFFFFF");
               }
           }
       }
       function onWinenavdetailtableOut(o, num) {
           for (var i = 1; i <= num; i++)
               $("#winenavdetailtable" + o + "_" + i).css("background-color", "#FFFFFF");
       }
       function showzhaoshan() {
           $("#zhaoshan").show();
       }
       function hidezhaoshan() {
           $("#zhaoshan").hide();
       }
    </script>
