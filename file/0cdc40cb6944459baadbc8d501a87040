    <script type="text/javascript">
        function getNumImg(num, px1, px2) {
            var n1 = num.toString().substring(0, 1);
            var n2 = num.toString().substring(1, 2);
            return "<img src=\"http://www.19online.cn/templates/1/images/2013skin/" + n1 + ".gif\" style=\"padding-left:" + px1 + "px;padding-top:20px;float:left;\">" +
            "<img src=\"http://www.19online.cn/templates/1/images/2013skin/" + n2 + ".gif\" style=\"padding-left:" + px2 + "px;padding-top:20px;float:left;\">";
        }
        var time_now_server, time_now_client, time_end, time_server_client, timerID;
        var nowTime = "";
        var endTureTime = ""; var dadiypicNum = 0; var dadiypicChange = 0;

        function webtime() {
            $.ajax({
                type: "POST",
                url: "/Common/GetSysTime.aspx?r=" + Math.random(),
                async: false,
                success: function (msg) {
                    var jsons = eval(msg);
                    try {
                        for (var i = 0; i < jsons.length; i++) {
                            nowTime = jsons[i].nowTime; //服务器时间
                            time_now_server = new Date(nowTime.replace(/-/g, "/"));
                            time_now_server = time_now_server.getTime();
                            time_now_client = new Date();
                            time_now_client = time_now_client.getTime();
                            time_server_client = time_now_server - time_now_client;
                            show_time();
                        }
                    } catch (e) {
                    }
                }
            });
        }
        webtime();
        //setTimeout("show_time()", 500);
        function show_time() {
            var time_now, time_distance, str_time;
            var int_day, int_hour, int_minute, int_second, int_ms;
            var time_now = new Date(); //当前时间
            time_now.setTime(time_now.getTime() + time_server_client);
            time_now = time_now.getTime();
            var beginTime = new Date(); //开始时间
            beginTime.setTime(beginTime.getTime() + time_server_client);
            beginTime.setHours(24);
            beginTime.setMinutes(0);
            beginTime.setSeconds(0);
            beginTime.setMilliseconds(0);
            beginTime = beginTime.getTime();
            var endTime = new Date(); //结束时间
            endTime.setTime(endTime.getTime() + time_server_client);
            endTime.setHours(20);
            endTime.setMinutes(0);
            endTime.setSeconds(0);
            endTime.setMilliseconds(0);
            endTime = endTime.getTime();
            if (beginTime < time_now && time_now < endTime) {
                endTureTime = endTime;
                dadiypicChange = 0;
            }
            else {
                if (time_now < beginTime) {
                    endTureTime = beginTime;
                }
                else {
                    endTureTime = beginTime;
                    endTureTime = new Date(beginTime + 86400000);
                    endTureTime = endTureTime.getTime();
                }
                dadiypicChange = 1;
            }
            time_distance = endTureTime - time_now;
            if (time_distance > 0) {
                int_day = Math.floor(time_distance / 86400000)
                time_distance -= int_day * 86400000;
                int_hour = Math.floor(time_distance / 3600000)
                time_distance -= int_hour * 3600000;
                int_minute = Math.floor(time_distance / 60000)
                time_distance -= int_minute * 60000;
                int_second = Math.floor(time_distance / 1000)

                time_distance -= int_second * 1000;
                int_ms = Math.floor(time_distance / 16.667)

                if (int_hour < 10)
                    int_hour = "0" + int_hour;
                if (int_minute < 10)
                    int_minute = "0" + int_minute;
                if (int_second < 10)
                    int_second = "0" + int_second;
                if (int_ms < 10)
                    int_ms = "0" + int_ms;

                str_time = "<span class=\"timertext1\">" + getNumImg(int_hour, 1, 7) + "</span><span class=\"timertext2\">"
                    + getNumImg(int_minute, 14, 7) + "</span><span class=\"timertext3\">" + getNumImg(int_second, 14, 7) + "</span>";
                $("#timer").html(str_time);
                // setTimeout("show_time()", 16.667);
                setTimeout("show_time()", 500);
            }
            if (dadiypicNum != dadiypicChange) {
                setDaliypic(dadiypicChange);
                dadiypicNum = dadiypicChange;
            }
        }
        $("#timergo").html("<img src=\"http://www.19online.cn/templates/1/images/2013skin/timersbg.gif\">");
        function setDaliypic(h) {
            if (h == 0) {
                $("#daliyday").css("margin-left", "210px").attr("src", "/images/daliydayL.jpg");
                $("#daliymoday").css("margin-left", "15px").attr("src", "/images/daliymodayA.jpg");
            }
            else {
                $("#daliyday").css("margin-left", "355px").attr("src", "/images/daliydayA.jpg");
                $("#daliymoday").css("margin-left", "-270px").attr("src", "/images/daliymodayL.jpg");
            }
        }
</script>
