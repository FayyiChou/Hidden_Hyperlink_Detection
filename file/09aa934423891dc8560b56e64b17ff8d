   jq("#smxzapp_box").load("/templates/default/jq_ajax_lxt.htm");
   jq("#smxzapp_box").fadeIn();
   jq("#smxzapp_box").mouseover(function(){
	   jq("#smxzapp_box").addClass("wx_box");
   });
   jq("#smxzapp_box").mouseout(function(){
	   jq("#smxzapp_box").removeClass("wx_box");
   });
}
