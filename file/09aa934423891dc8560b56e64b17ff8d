<script>
function showadv_rumortips(){
  hide_rumortips=getcookie('hide_rumortips');
  if(hide_rumortips == 1){
	jq("#gzczTipsArea").slideUp();
    $('tips_close').style.display = "none";
    $('tips_open').style.display = "block";
  }else{
	jq("#gzczTipsArea").slideDown();
    $('tips_close').style.display = "block";
    $('tips_open').style.display = "none";
  }
}
function settipscookie(value){
  setcookie('hide_rumortips',value,86400000);
}
function hideGzczTips() { 
  settipscookie(1);
  jq("#gzczTipsArea").slideUp();
  $('tips_close').style.display = "none";
  $('tips_open').style.display = "block";
}
function showGzczTips() { 
  settipscookie(0);
  jq("#gzczTipsArea").slideDown();
  $('tips_close').style.display = "block";
  $('tips_open').style.display = "none";
}
function width_toggle(){
  bun=jq("#width_toggle");
  if(bun.hasClass("wide")){
    setcookie('width_toggle',1,86400000);
    bun.toggleClass("wide narrow").html("宽版");
    jq("#wrap").css("width","1000px");
  }else{
    setcookie('width_toggle',0,86400000);
    //bun.attr("value",0).html("切换到窄版");
    bun.toggleClass("wide narrow").html("窄版");
    jq("#wrap").css("width","99%");
  }
}

showadv_rumortips();
if(getcookie('width_toggle')==1){//默认窄版
  jq("#width_toggle").removeClass("wide").addClass("narrow").html("宽版");
  jq("#wrap").css("width","1000px");
}else{
  jq("#width_toggle").removeClass("narrow").addClass("wide").html("窄版");
  jq("#wrap").css("width","99%");
}

//加载、显示、关闭微信
function show_weixin_box() { 
   jq("#weixin_box").load("/templates/default/jq_ajax_weixin.htm");
   jq("#weixin_box").fadeIn();
   jq("#weixin_box").mouseover(function(){
	   jq("#weixin_box").addClass("wx_box");
   });
   jq("#weixin_box").mouseout(function(){
	   jq("#weixin_box").removeClass("wx_box");
   });
}
function hide_weixin_box() { 
   jq("#weixin_box").fadeOut();
}
