			  var imageURLs = [
				   "siteimages/man.jpg"
				 , "siteimages/woman.jpg"
			  ];
			  function getImageTag() {
				var img = '<img src=\"';
				var randomIndex = Math.floor(Math.random() * imageURLs.length);
				img += imageURLs[randomIndex];
				img += '\" width="220" height="320" alt=\"Some alt text\"/>';
				return img;
			  }
