<script>
    $(function() {
        $('#close-ad').click(function () {
            $(this).parent().remove();
        });
    });
    $(function () {
        $("body").delegate('.layui-layer-shade', 'click', function () {
            layer.closeAll();
        });
        $('#keywords').bind({
            focus: function () {
                $(this).addClass('color-3');
            },
            blur: function () {
                $(this).removeClass('color-3');
            },
            keypress: function (event) {
                if (event.keyCode == "13") {
                    if ($("#keywords").val() != '') {
                        //sSearch();
                        window.location.href = "/search/" + $("#keywords").val();
                    } else {
                        $("#keywords").attr("placeholder", "搜索节目、明星、频道");
                    }
                }
            }
        });
        $(".search-top-btn").click(function () {
            if ($("#keywords").val() != '') {
                setTimeout("window.location.href = '/search/" + $("#keywords").val() + "/'", 100);
            } else {
                $("#keywords").attr("placeholder", "请输入关键字");
            }
        });
    });

</script>
<script>
	$(function(){
		//顶
		$("body").delegate('.recommend-ding','click',function(){
			var obj = this;
			var recommenddataid = $(this).attr('data-id');
			var postData = {
				"groupid": 'naujqy7ian',
				"recommenddataid": recommenddataid,
			}
			var url = "/tvsou/group/ajaxDoRecommendDing";
			$.ajax({type: "POST",data: postData,url: url,dataType: 'json',
				success:function(data) {
					if (data.status == "1") {
						if(data.bang == '1'){
							location.reload();
						}else{
							if($(obj).children('em').length > 0){
								$(obj).children('em').addClass('zan-active');
							}else{
								$(obj).addClass('zan-active');
							}
							var num1 = Number($('.recommenddataid-'+recommenddataid).html());
							$('.recommenddataid-' + recommenddataid).html(num1 + 1)
							if(data.recommenddataid != ''){
								var num2 = Number($('.recommenddataid-'+data.recommenddataid).html());
								$('.recommenddataid-'+data.recommenddataid).html(num2 - 1)
								$('.recommenddataid-'+data.recommenddataid).parent().children('a').children('em').removeClass('zan-active');
							}
						}
					}else{
						layer.msg(data.msg, {time: 1000});
					}
				}
			});
		});
	});
</script>
