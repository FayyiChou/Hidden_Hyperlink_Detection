<script>
    $(function(){
        $('.vote-option').click(function(){
            if($(this).children('.info').children('em').hasClass('active')){
                $(this).children('.info').children('em').removeClass('active');
            }else{
                $(this).children('.info').children('em').addClass('active');
            }
            $('.vote-option').children('.info').children('label').removeClass('label');
            if($(this).children('.info').children('label').hasClass('label')){
                $(this).children('.info').children('label').removeClass('label');
            }else{
                $(this).children('.info').children('label').addClass('label');
            }
        });
        $('.vote-btn').click(function(){
                        userLogin();
                    });
    });
</script>

