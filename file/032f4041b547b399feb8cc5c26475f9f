<script>
	$(function(){
		//提交推荐
		$("body").delegate('.recommend-info-view','click',function(){
			var itemname = $(this).attr('data-name');
			var postData = {
				"recommendid": $(this).attr('data-id'),
			}
			var url = "/tvsou/group/ajaxGetRecommendInfo";
			$.ajax({type: "POST",data: postData,url: url,dataType: 'json',
				success:function(data) {
					if (data.status == "1") {
						var title = '将 《'+itemname+'》 顶上首页';
						var html = '';
						if(data.data.status == 'null'){
							html = template('tpl_recommend_start_item', data.data);
						}else if(data.data.status == 'success'){
							html = template('tpl_recommend_success_item', data.data);
							var title = '第'+data.data.recommendnum+'次推荐';
						}else{
							html = template('tpl_recommend_failed_item', data.data);
							var title = '第'+data.data.recommendnum+'次推荐';
						}
						layer.open({type: 1,title: title,closeBtn: 1,shade:0.6,area: ['780px','720px'],content: html,scrollbar: false});

						$("body").switchTab({"titCell": ".st-tabnav span", "titOnClassName": "active"})
					}else{
						layer.msg(data.msg, {time: 1000});
					}
				}
			});
		});

	})
</script>

