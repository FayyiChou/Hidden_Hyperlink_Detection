        $('.share-window').click(function(){
            callbackStr="shareCallback";
            var title = '分享片源:星光灿烂';
            var html = '<div class="impression-box relative border-radius-10"><div class="img-main-up share-html"><div class="share-tc"><div><input type="text" name="share-url" id="share-url" value="" placeholder="在这里粘贴片源链接" class="share-input borer-5"/></div><p class="text-c"><a class="font-12 color-9">支持网站</a></p></div><ol class="share-tb ov text-c">'+siteHtml+'</ol><div class="up-img-btn" style="border-top: 0;"><div class="up-img-info ov"><input type="button" value="下一步" class="r color-9 input-tc text-c borer-5 cur gshare-btn"></div></div></div></div>';
            layer.open({type: 1,title: title,closeBtn: 1,shade:0.6,area: ['660px','520px'],content: html,scrollbar: false});
        });
        //字数统计
        $("body").delegate('#share-url','keyup',function(){
            $('.gshare-btn').removeClass('input-active');
            if($(this).val().length > 0){
                if(!$('.gshare-btn').hasClass('input-active')){
                    $('.gshare-btn').addClass('input-active');
                }
            }
        });

        //提交分享
        $("body").delegate('.gshare-btn','click',function(){
            var reg=/(((^https?:(?:\/\/)?)(?:[-;:&=\+\$,\w]+@)?[A-Za-z0-9.-]+|(?:www.|[-;:&=\+\$,\w]+@)[A-Za-z0-9.-]+)((?:\/[\+~%\/.\w-_]*)?\??(?:[-\+=&;%@.\w_]*))?)(.*?)$/;
            $('.gshare-btn').removeClass('input-active');
            if(!reg.test($("#share-url").val())){
                layer.msg('片源链接地址格式错误', {time: 1000});
                $('.gshare-btn').addClass('input-active');
                return false;
            }
            callbackStr="submitShareCallback";
            var postData = {
                "groupid": 'naujqy7ian',
                "itemid": '9e63afdc',
                url:$("#share-url").val(),
                eitemid:$("#eitemid").val(),
                content:$("#content").val(),
                code:$("#code").val(),
                isSubmit:$("#isSubmit").val()
            };
            var shareUrl = $("#share-url").val();
            var index = layer.load(2, {time: 6*1000});
            var url = "/tvsou/group/ajaxDoShare";
            $.ajax({type: "POST",data: postData,url: url,dataType: 'json',
                success:function(data) {
                    layer.close(index);
                    $('.gshare-btn').addClass('input-active')
                    if (data.status == "1") {
                        $('.share-html').html('<div class="share-tc"><h2 class="text-c">提交成功！</h2><p class="font-14 color-3 text-c">您提交的分享网址需要审核，通过审核后</p> <ul class="font-12 color-6"><li style="margin-top: 9px;"><img src="http://imgv2.tvsou.com/images/share_user.png" width="17" height="16" class="" /><label style="vertical-align: top;">其他用户将不能再分享这个片源</label></li></ul></div><div class="up-img-btn" style="border-top: 0;"><div class="up-img-info ov"><input type="button" value="完成" class="r color-9 input-tc text-c borer-5 cur input-active share-close"></div></div>');
                    }else if (data.status == "99") {
                        userLogin();
                    }else if (data.status == "11") {
                        $("#surl").val(shareUrl)
                        $("#isSubmit").val(1)
                        $('.share-html').html('<input type="hidden" name="eitemid" id="eitemid" value="'+"9e63afdc"+'"><input type="hidden" name="share-url" id="share-url" value="'+shareUrl+'" class="share-input borer-5"/><div class="share-info-box"><p><label class="color-9" style="margin-right: 10px;">链接</label><a href="'+shareUrl+'" class="blue a" target="_blank">'+shareUrl+'</a></p><div class="charge-ji"><label class="color-9 l">选集</label><div class="up-info-list relative font-12 l"><em class="color-3 cur episode-select">'+'星光灿烂'+'</em><dl class="absolute hide episode-di">'+eHtml+'</dl></div></div><div class="textarea borer-5 relative "><span class="font-12 color-9 tc-sum absolute hide"><label class="content-count">0</label>&nbsp;/<label>100</label></span><textarea name="content" id="content" max class="color-3" placeholder="在这里填写简介(非必填)" maxlength="200"></textarea></div><div><input type="text" name="code" id="code" value="" maxlength="30" placeholder="提取码(非必填)" class="color-3 share-input borer-5 extract"></div></div><div class="up-img-btn" style="border-top: 0;"><div class="up-img-info ov"><input type="button" value="完成" class="r color-9 input-tc text-c borer-5 cur input-active gshare-btn"></div></div></div>');
                    }else if (data.status == "12") {
                        $('.share-html').html('<div class="share-tc"><div class="relative"><input type="text" value="'+shareUrl+'" class="color-9 share-input borer-5" style="background: #e6e6e6;" disabled="disabled"/><em class="recommend-no recommend-icon absolute"></em></div><div class="share-list text-c"><div class="border-one borer-5"><img src="'+"http://file.tvsou.com/f/"+data.siteInfo.photo+"/zm78-78"+'" width="78" height="78" class="borer-5" /></div><div class="mt-20"> <img src="'+"http://file.tvsou.com/f/"+data.userInfo.photo+"/zm24-24"+'" width="24" height="24" class="border-radius vel"/><label class="color-6">'+data.userInfo.nickname+'</label></div><p class="font-12 mt-28">好遗憾，  '+data.siteInfo.name+'  的片源已经被他分享过了 换个网站试试吧</p></div></div><div class="up-img-btn" style="border-top: 0;"><div class="up-img-info ov"><input type="button" value="上一步" class="r color-9 input-tc text-c borer-5 input-active cur share-up"></div></div>');
                    }else{
                        layer.msg(data.msg, {time: 3000});
                    }
                }
            });
        });
        $("body").delegate('.share-up','click',function(){
            $('.share-html').html('<div class="share-tc"><div><input type="text" name="share-url" id="share-url" value="" placeholder="在这里粘贴片源链接" class="share-input borer-5"/></div><p class="text-c"><a class="font-12 color-9">支持网站</a></p></div><ol class="share-tb ov text-c">'+siteHtml+'</ol><div class="up-img-btn" style="border-top: 0;"><div class="up-img-info ov"><input type="button" value="下一步" class="r color-9 input-tc text-c borer-5 cur gshare-btn"></div></div>');
        });
        $("body").delegate('.share-close','click',function(){
            layer.closeAll();
        });
        $('#content').focus(function(){
            $(this).parent().children('.tc-sum').show();
        }).blur(function(){
            $(this).parent().children('.tc-sum').show();
        });
        //字数统计
        $("body").delegate('#content','keyup',function(){
            $('.content-btn').removeClass('input-active');
            var num = $(this).val().length;
            if(num < 0){
                num = 0;
            }
            if(num > 100){
                $('.content-count').parent().addClass('sum');
            }else{
                $('.content-count').parent().removeClass('sum');
                $('.gshare-btn').addClass('input-active');
            }
            $('.content-count').html(num)
        });
        //集数选择
        var _episode_timeout;
        $("body").delegate('.episode-select','hover',function(event){
            var type = event.type;
            if(type == "mouseenter"){ //移入
                clearTimeout(_episode_timeout);
                $('.episode-di').show();
            }else{//mouseleave 移出
                _episode_timeout = setTimeout(function () {
                    $('.episode-di').hide();
                }, 500);
            }
        });
        $("body").delegate('.episode-di','hover',function(event){
            var type = event.type;
            if(type == "mouseenter"){ //移入
                clearTimeout(_episode_timeout);
                $('.episode-di').show();
            }else{//mouseleave 移出
                _episode_timeout = setTimeout(function () {
                    $('.episode-di').hide();
                }, 500)
            }
        });
        $("body").delegate('.episode-option','click',function(){
            $('#eitemid').val($(this).attr('data-id'));
            $(this).parent().parent().children('em').html($(this).html());
            $(this).parent().hide();
        });
    })
</script>
</body></html>