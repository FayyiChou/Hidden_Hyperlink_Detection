<script>
    $(function(){
        $('.reply-more').click(function(){
            var obj = this;
            var postData = {
                page:$(this).attr('data-page'),
                postsid:$(this).attr('data-postsid'),
            };
            var url = "/tvsou/comment/ajaxGetReplyList";
            $.ajax({type: "POST",data: postData,url: url,dataType: 'json',
                success:function(data) {
                    if(data.status == 1){
                        var itemHtml = template('tpl_reply_list_item', data.data);
                        $(obj).parent().children('.reply-list').append(itemHtml);
                        $(obj).attr('data-page',parseInt($(obj).attr('data-page')+1));
                        if(data.data.num <= 0){
                            $(obj).remove();
                        }
