        $('.add-reply').click(function(){
                        userLogin();
                    });
        $("body").delegate('.reply-cancel','click',function(){
            $(this).parent().parent().remove();
            $('.add-reply').show();
            $('.add-reply-reply').show();
        });
        $("body").delegate('.reply-btn','click',function(){
            var content=$(this).parent().parent().children('.reply-content').val();
            var mpreid=$(this).attr('data-mpreid');
            var preid=$(this).attr('data-preid');
            loginCallback=function(){
                submitComment('/tvsou/group/ajaxDoComment','naujqy7ian','71f26b040d',content,mpreid,preid);
            }
            submitComment('/tvsou/group/ajaxDoComment','naujqy7ian','71f26b040d',content,mpreid,preid);
        });
        $('.add-reply-reply').click(function(){
                        userLogin();
                    });
    });
</script>
