<script>
    var	oldchatid = "";
    var	newchatid = "";
    function ajaxLoadNewChat(){
        $('.ajax-more').css('background','#fff');
        $('.ajax-more').html('<span class="color-6">更多</span>');
        $('.ajax-more').addClass('ajax-more');
        $('.ajax-more').addClass('cur');
        var postData = {
            groupid: "naujqy7ian",
            chatid: newchatid,
            type: 'new',
        }
        var url = "/tvsou/group/ajaxGetNewChat";
        $.ajax({type: "POST",data: postData,url: url,dataType: 'json',
            success:function(data){
                if (data.status == "1") {
                    var len = data.data.length;
                    for(var i = 0;i < len; i++){
                        if(data.data[i].chatid != newchatid){
                            $(".group-info").prepend(data.data[i].html);
                        }
                    }
                    newchatid = data.data[0].chatid;
                }
            },
        });
        setTimeout("ajaxLoadNewChat();",20000);
    }
    $(function(){
        $('#close-ad').click(function(){
            $(this).parent().remove();
        });
        $("body").switchTab({"titCell": ".tab-list a", "titOnClassName": "active",mainCell: ".list-info",defaultIndex:0})
        $('.episodes-tab').click(function(){
            $('.episodes-dd').hide()
            $('.'+$(this).attr('data-id')).show()
            $('.episodes-tab').parent().removeClass('active');
            $(this).parent().addClass('active');
        });
        $('.episodes-more-btn').click(function(){
            $('.episodes-min').hide()
            $('.episodes-more').show()
            $('.episodes-0').show()
            $('a[data-id="episodes-0"]').parent().show();
            $('a[data-id="episodes-0"]').parent().addClass('active');
        });

        var _episode_timeout;
        $(".e-list").hover(
            function () {
                clearTimeout(_episode_timeout);
                $('.e-option').show();
            },
            function () {
                _episode_timeout = setTimeout(function () {
                    $('.e-option').hide();
                }, 500);
            }
        );
        $(".e-option").hover(
            function () {
                clearTimeout(_episode_timeout);
                $('.e-option').show();
            },
            function () {
                _episode_timeout = setTimeout(function () {
                    $('.e-option').hide();
                }, 500)
            }
        );

        //加载更多
        $("body").delegate('.ajax-more','click',function(){
            var obj = this;
            $(obj).show();
            $(obj).html('<i class="tv-loading-white">加载中…</i>');
            $(obj).css('background','none');
            $(obj).removeClass('ajax-more');
            $(obj).removeClass('cur');
            var postData = {
                groupid: "naujqy7ian",
                chatid: oldchatid,
                type: 'old'
            }
            var url = "/tvsou/group/ajaxGetNewChat";
            $.ajax({type: "POST",data: postData,url: url,dataType: 'json', async: false,
                success:function(data) {
                    _timeout = 0;
                    if(data.status == 1){
                        var len = data.data.length;
                        if(newchatid == ''){
                            newchatid = data.data[0].chatid;
                        }
                        for (var i = 0; i < len; i++) {
                            $(".group-info").append(data.data[i].html);
                            oldchatid = data.data[i].chatid;
                        }
                        $(obj).css('background','#d0d0d0');
                        $(obj).html('<span class="color-6">更多</span>');
                        $(obj).addClass('ajax-more');
                        $(obj).addClass('cur');
                    }else{
                        $(obj).css('background','#e6e6e6');
                        $(obj).html('<span class="color-6">暂时就这么多了</span>');
                        $(obj).addClass('btn-boxs');
                        $(obj).removeClass('cur');
                    }
                }
            });
            setTimeout("ajaxLoadNewChat();",20000);
        });
        var isget = true;
        var adTop = $(".get-chat").offset().top - $(window).height();
        if(document.body.scrollHeight > adTop){
            window.onscroll = function () {
                var scrollTop = document.body.scrollTop || document.documentElement.scrollTop;
                if (scrollTop > adTop && isget) {
                    isget = false;
                    $('.ajax-more').click();
                }
            }
        }else{
            $('.ajax-more').click();
        }
    });
</script>
<input type="hidden" name="isSubmit" id="isSubmit" value="0">
