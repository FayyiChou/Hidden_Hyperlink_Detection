<script type="text/javascript">
Tinker.UI.tabShow('tab_nav', 'tab_cont', {delay:0});
function goChange(){
    var ele = document.getElementById('common_list'),
        elems = ele.getElementsByTagName('li'),
        length = elems.length,
        i = 0,
        temp = null;
    var randomNum =  function(min, max, n){
        var tempArr = [],
                i = 0;
        for(var j = min; j <= max; j++){
            tempArr[i++] = j;
        }
        tempArr.sort(function(){
            return 0.5 - Math.random();
        });
        return tempArr.slice(0, n);
    };
   temp = randomNum(0 , length-1 , 12);
    for(; i < length; i++){
       ele.appendChild(elems[temp[i]]); 
    }
}
</script>
