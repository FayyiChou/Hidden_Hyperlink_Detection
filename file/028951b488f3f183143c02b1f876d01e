				var defaultValue = '您可以找医院、找专家、查药品.';
				function trim(v){
					return v.replace(/^\s+/, '').replace(/\s+$/, '');
				}
				document.getElementById("keyword").onfocus = function(){
					if(this.value === defaultValue){
						this.value = '';
					}
				}
				document.getElementById("keyword").onblur = function(){
					if(this.value === defaultValue || trim(this.value) === ''){
						this.value = defaultValue;
					}
				}
				 function validate() {
					if(trim(document.getElementById("keyword").value) == '' || document.getElementById("keyword").value === defaultValue) {
					   return false;
					}else{
					   return true;
					}
				 }
