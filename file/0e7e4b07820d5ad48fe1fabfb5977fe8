<script type="text/javascript">
function ScrollImgMlsc(){
var speed=20
var scroll2_begin = document.getElementById("scroll2_begin");
var scroll2_end = document.getElementById("scroll2_end");
var scroll2_div = document.getElementById("scroll2_div");
scroll2_end.innerHTML=scroll2_begin.innerHTML
function Marquee(){
    if(scroll2_end.offsetWidth-scroll2_div.scrollLeft<=0)
      scroll2_div.scrollLeft-=scroll2_begin.offsetWidth
    else
      scroll2_div.scrollLeft++
}

/* 鼠标动作 */
var MyMar=setInterval(Marquee,speed)
scroll2_div.onmouseover=function() {clearInterval(MyMar)}
scroll2_div.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
}
</script>			<div id="scroll2_div" class="scroll2_div">
