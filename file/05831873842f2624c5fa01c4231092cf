     <script type="text/javascript">
    function getCookies(name) {
        var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
        if (arr != null) return unescape(arr[2]);
        else
            return null;
    };
    (function (d) {
        var setCookies = function (name, value) {
            if (value.indexOf("?") > 0 && value.substring(0, value.indexOf("?")).indexOf("jianke.com/") < 0) {
                var Days = 7;
                var exp = new Date();
                exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);
                d.cookie = name + "=" + escape(value) + "; expires=" + exp.toGMTString() + ";path=/;";
            }
        };
        setCookies("baiduSource", document.referrer.toLowerCase());
    })(document);

</script>

