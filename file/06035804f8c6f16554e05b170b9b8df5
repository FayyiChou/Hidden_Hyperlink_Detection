	var content = '';//全文缓存
	function havePic(t){
		if($(t).attr('checked')){
			$('.cont-wrap').html(content);
		}
		else{
			content = content==''?$('.cont-wrap').html():content;
			$('.cont-wrap')[0].innerHTML =  $('.cont-wrap').html().replace(/<img\s*[^>]*[\/]?>/img,'');
		}
	}
