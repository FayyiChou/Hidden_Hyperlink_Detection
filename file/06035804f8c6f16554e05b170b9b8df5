  <script>
	//alert(document.body.clientWidth);
	
	function dom(id){
		
		return document.getElementById(id);
	}
	
	$(function(){
		
		var _index=0;//图片索引
		var timer = null;//定时触发器
		var t = 3000; //轮播时间
		///alert(111);
		var pictrue = dom("pictrue");
		$("#pictrue").find("li").eq(0).css("display","block").siblings().toggle();
		//alert(banner.children.length);
		//拿到图片对应li的个数
		var len = pictrue.children.length;
		//动态添加小图标
		for(var i=0;i<len;i++){
			if(i==0){
				$("#flag").append("<li  class='active select'></li>");
			}else{
				$("#flag").append("<li class='active'></li>");
			}
		}
		//给flag设置宽度 使小图标居中
		var cwidth = 10*len+8*(len+1);
		$("#flag").width(cwidth);
		//拿到可视化页面的宽度  赋给图片对应的li
		var width = document.body.clientWidth;
		for(var i=0;i<len;i++){
			$("#pictrue").find("li").eq(i).width(width);
		}
		//拿到li的高度 赋给banner 为小图标定位用
		$("#banner").height($("#pictrue").find("li").eq(0).height());
		
		//窗口变化时 重新给图片对应的li设置宽阔 给banner设置高度
		window.onresize = function(){
			for(var i=0;i<len;i++){
				$("#pictrue").find("li").eq(i).width(document.body.clientWidth);
			}
			$("#banner").height($("#pictrue").find("li").eq(0).height());
		}
		//小图标点击事件
		$("#flag").on("click","li",function(){
			//关闭定时器
			clearInterval(timer);
			_index = $(this).index();
			//小图标颜色的变化
			$("#flag").find("li").eq(_index).addClass("select").siblings().removeClass("select");
			//图片的淡入淡出
			$("#pictrue").find("li").eq(_index).fadeIn(800).siblings().fadeOut(800);
			
		});
		timer = setInterval(playBanner,t);
		//图片轮播
		function playBanner(){
			_index++;
			if(_index==len)_index=0;
			$("#flag").find("li").eq(_index).addClass("select").siblings().removeClass("select");
			$("#pictrue").find("li").eq(_index).fadeIn(800).siblings().fadeOut(800);
		}
		//鼠标放在banner上关闭轮播
		$("#banner").mouseover(function(){
			clearInterval(timer);
		});
		//鼠标离开banner 开启轮播
		$("#banner").mouseout(function(){
			clearInterval(timer);
			timer = setInterval(playBanner,t);
		});


	});
  </script> 
