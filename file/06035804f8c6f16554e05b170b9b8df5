  <script>
	
	$(function(){
	
		$("#menu_box>li>ul>li>ul>li").hover(function(){
		
			if($(this).find(".threeMenu").html()){
				
				$(this).find(".threeMenu").css("display","block");
			}else{
				$(this).find(".threeMenu").css("display","none");
			}
		},function(){
		
			$(this).find(".threeMenu").css("display","none");
		});
	});
