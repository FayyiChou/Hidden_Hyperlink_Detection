				// The slider being synced must be initialized first
				$('#carousel').flexslider({
					animation: "slide",
					controlNav: false,
					animationLoop: false,
					slideshow: true,
					itemWidth: 60,
					itemMargin: 0,
					minItems: 3,
					maxItems: 3,
					asNavFor: '#slider'
				});

				$('#slider').flexslider({
					animation: "fade",
					controlNav: false,
					animationLoop: true,
					slideshow: true, //Set to true per http://punchlist.ingeniux.com/issues/3115
					sync: "#carousel",
					after: function (slider) {
					    if (slider.currentSlide == 0) {
					        $('#carousel ul.slides').css('transform', 'translate3d(0,0,0)');
					        $('#carousel ul.slides').css('-webkit-transform', 'translate3d(0,0,0)');
					        $('#carousel ul.slides li:eq(0)').addClass('flex-active-slide');
					    }
					}	//Added after function per http://punchlist.ingeniux.com/issues/3115				
				});

				// Homepage panels
				$('div.pane div.copy').css({opacity:0});

				// What Happes on Hover
				$('a.pane1').hover(function () {
					$('div.pane').hide();
					$('div.pane.p1').show();
					$('div.pane.p1 div.copy').show().delay(800).animate({
						'opacity':'1',
						queue:false
					}, 600);
				}, function() {
					$('div.pane.p1 div.copy').stop(true).animate({
						'opacity':'0',
						queue:false
					}, 600);
					$('div.pane.default').show();
				});

