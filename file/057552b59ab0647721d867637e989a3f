	<script language="javascript">
	var mebtnopenurl = rootIndex;
	window.shareData = {
			"imgUrl": mebtnopenurl+"games/colorfind/icon.png",
			"timeLineLink": mebtnopenurl+"games/colorfind/index.html?game_code=39&bs=wx",
			"tTitle": "看你有多色-2345游戏",
			"tContent": "想试试自己到底有多色吗？"
	};

	function goHome(){
		window.location=mebtnopenurl;
	}
	function dp_share(){
		document.title = "啊啊啊,我居然是【"+myData.scoreName+"】，色色指数高达"+myData.score+"分，你以为你好的到哪去吗?";
		window.shareData.tContent = document.title;
	}
	function dp_shareing(score){
		document.title = "啊啊啊,我的色色指数高达"+score+"分，你以为你好的到哪去吗?";
		window.shareData.tContent = document.title;
	}
	function dp_Ranking(){
		//alert("到更多");
		window.location=mebtnopenurl;
	}

	</script>

		<script type="text/javascript">

		var myData = {};
		function dp_submitScore(vScore,vLevel){

		crossAjax({
	url:'/m/index.php?c=shouyou&d=uploadScore',
	data:{
		c:'shouyou',
		d:'uploadScore',
		game_code:QueryString.getParameter('game_code'),
		game_level:vLevel,
		game_score:vScore,
		bs:QueryString.getParameter('bs')
	},
	type:'post',
	dataType:'json',
	success:function(res){
		//this added by JobsFan
		if (QueryString.getParameter('bs').indexOf('wx')!=-1){
			//dp_share(vScore);//不能写在这里，因为游戏分享的部分可能不一样
			$j(".nowrecord .num").html(vScore);
			$j(".bestrecord .num").html(res.bestscore);
			$j(".scoreinfo").show();
		}
		if(res.flag=='f0'){
			if(res.ulevel>0){
//                   if(confirm('你的分数为'+res.uscore+'，排名为第'+res.ulevel+'名，是否跳转到排行榜？')){
//                       location.href=rootIndex+'games/toplist?game_code='+QueryString.getParameter('game_code');
//                   }
			}
		}else if(res.flag=='f1'){
			if(loginCounter==1||confirm('您尚未登录，游戏积分无法上传到排行榜，是否登录？')){
				location.href=server.PageLoginURL+'?service='+encodeURIComponent(location.href);
			}else{
				loginCounter++;
			}
		}else{
			alert(res.msg);
		}
	}
});
		//(lv, score) lv分数
			myscore=vScore;
			myData.score = vScore;
			myData.scoreName = vLevel;
			if (confirm("鉴定完毕!你是【"+vLevel+"】级，色色指数："+vScore+"分！\n赶紧分享给小伙伴一起来玩吧!")){
				dp_share();
			}
		}


$j(document).ready(function(){
	if(/Android|Windows Phone|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent)){
		//
	}else{
		$j(".aliForPc").show();
	}
});

		</script>

