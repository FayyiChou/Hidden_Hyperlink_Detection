    var indexObj = {
        ua: navigator.userAgent.toLowerCase(),
        isWeixin: null,
        init: function () {
            if (this.ua.match(/MicroMessenger/i) == "micromessenger") {
                this.isWeixin = true;
            } else {
                this.isWeixin = false;
            }
        }
    };
    indexObj.init();

    var mask = document.getElementById("mask");
    mask.addEventListener('click', function () {
        this.style.display = 'none';
    }, false);

    function downLoadApk() {
        var ch = getParam(location.href, "ch");
        remoteLog(clickLogBase + window.location.search, ch);
        var ua = navigator.userAgent.toLowerCase();
        if (ua.match(/MicroMessenger/i) == "micromessenger") {
            // 微信朋友圈分享，同时下载链接为应用宝，不展示mask
            if (downloadHref.indexOf('http://a.app.qq.com/o/simple.jsp?pkgname=com.coohuaclient') === -1) {
                if (mask) {
                    document.getElementById('mask').style.display = 'block';
                }
                return;
            }
        }
        setTimeout(function () {
            location.href = downloadHref;
        }, 1000);
    }

    function getParam(url, param) {
        var r = new RegExp("\\?(?:.+&)?" + param + "=(.*?)(?:[\?&].*)?$");
        var m = url.match(r);
        return m ? m[1] : "";
    }

    var unique = (function () {
        var time = (new Date()).getTime() + '-', i = 0;
        return function () {
            return time + (i++);
        }
    })();

    var remoteLog = function (url, ch) {
        // 与其他统计不同在于，多了source参数，source后携带渠道
        var img = new Image();
        var uid = unique();
        img.onload = img.onerror = (function (img) {//销毁一些对象
            return function () {
                img.onload = img.onerror = null;
                delete img;
            };
        })(img);
        if (url.indexOf("?") == -1) {
            url += '?_uid=' + uid + '&source=binvitation';
        } else {
            url += '&_uid=' + uid + '&source=binvitation';
        }
        img.src = url;
    };
    //取得cookie
    function getCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');    //把cookie分割成组
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];                      //取得字符串
            while (c.charAt(0) == ' ') {          //判断一下字符串有没有前导空格
                c = c.substring(1, c.length);      //有的话，从第二位开始取
            }
            if (c.indexOf(nameEQ) == 0) {       //如果含有我们要的name
                return unescape(c.substring(nameEQ.length, c.length));    //解码并截取我们要值
            }
        }
        return false;
    }

    //清除cookie
    function clearCookie(name) {
        setCookie(name, "", -1);
    }

    //设置cookie
    function setCookie(name, value, seconds) {
        seconds = seconds || 0;   //seconds有值就直接赋值，没有为0，这个根php不一样。
        var expires = "";
        if (seconds != 0) {      //设置cookie生存时间
            var date = new Date();
            date.setTime(date.getTime() + (seconds * 1000));
            expires = "; expires=" + date.toGMTString();
        }
        document.cookie = name + "=" + escape(value) + expires + "; path=/";   //转码并赋值
    }

    function uuid(len, radix) {
        var chars = '0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz'.split('');
        var uuid = [], i;
        radix = radix || chars.length;
        if (len) {
            // Compact form
            for (i = 0; i < len; i++) uuid[i] = chars[0 | Math.random() * radix];
        } else {
            // rfc4122, version 4 form
            var r;

            // rfc4122 requires these characters
            uuid[8] = uuid[13] = uuid[18] = uuid[23] = '-';
            uuid[14] = '4';

            // Fill in random data.  At i==19 set the high bits of clock sequence as
            // per rfc4122, sec. 4.1.5
            for (i = 0; i < 36; i++) {
                if (!uuid[i]) {
                    r = 0 | Math.random() * 16;
                    uuid[i] = chars[(i == 19) ? (r & 0x3) | 0x8 : r];
                }
            }
        }

        return uuid.join('');
    }


    function dateFormat(date, format) {
        if (format === undefined) {
            format = date;
            date = new Date();
        }
        var map = {
            "M": date.getMonth() + 1, //月份
            "d": date.getDate(), //日
            "h": date.getHours(), //小时
            "m": date.getMinutes(), //分
            "s": date.getSeconds(), //秒
            "q": Math.floor((date.getMonth() + 3) / 3), //季度
            "S": date.getMilliseconds() //毫秒
        };
        format = format.replace(/([yMdhmsqS])+/g, function (all, t) {
            var v = map[t];
            if (v !== undefined) {
                if (all.length > 1) {
                    v = '0' + v;
                    v = v.substr(v.length - 2);
                }
                return v;
            }
            else if (t === 'y') {
                return (date.getFullYear() + '').substr(4 - all.length);
            }
            return all;
        });
        return format;
    }

    (function loaded() {
        var url = window.location.href;
        var idPattern = /src=(\d*)/i;
        var ele = document.getElementById('invite');
        if (idPattern.test(url)) {
            
            if (ele) {
                ele.innerHTML = url.match(idPattern)[1];
            }
        }else{
             ele.innerHTML = 80008;
        }

        var getRandomDownloadUrl = function (arr) {
            var date = Math.floor(Math.random() * (9999999 - 1000000) + 1000000);
            var len = arr.length;
            var i = date % len;
            return arr[i];
        };
        if (channelList) {
            var ch = getParam(url, "ch");
            var chLink = channelList[ch];
            if (chLink) {
                if (chLink instanceof Array) {
                    downloadHref = getRandomDownloadUrl(chLink);
                } else {
                    downloadHref = chLink;
                }
            }
        }
        if (!getCookie('coohua_session_id')) {
            setCookie("coohua_session_id", dateFormat(new Date(), 'yyyy-MM-dd') + "-" + uuid(8, 16), 3600 * 24 * 365);
        }
    })();

</script>
