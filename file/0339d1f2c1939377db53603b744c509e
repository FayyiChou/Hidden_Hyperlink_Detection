<script src="/static/js/jquery/jquery-ui.min.js"></script>
<script src="/static/js/date/jquery.datePicker-min.js"></script>
<script src="/static/js/jquery.YM.js"></script>
<script type="text/javascript">
	$(document).ready(function(){
		var _h = $(document).height();
		$('#mask').css({height:_h+'px'});
		$('#ts,#ts1').click(function(){
			$('#mask').show();
			$('#ts_con').show().simplefloat({left:'center',top:'center',right:'auto',bottom:'auto'},true);
		})

		$('#yxfw').click(function(){
			$('#mask').show();
			$('#ts_con').show().simplefloat({left:'center',top:'center',right:'auto',bottom:'auto'},true);
		})


		$('#mask').click(function(){
			$(this).hide();
			$('#ts_con').hide();
		})

		$('#myform').submit(function(){
			$.ajax({
				url:'/plus/diy.php',
				data:$('#myform').serialize(),
				type:'post',
				dataType:'json',
				success:function(data){
					if(data.errcode=='ok'){
						alert(data.msg);
						$('#mask').hide();
						$('#ts_con').hide();
					}else{
						alert(data.msg);
					}
				}
			})

			return false;
		})
	})
</script>
