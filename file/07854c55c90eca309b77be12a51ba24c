<script type="text/javascript">
    function canShowAppDownloadFooter(){
        if (document.cookie.indexOf('appDownloadFooterNotDisplay=')>-1){
            return false;
        } else {
            return true;
        }
    }
    (function(){
        if (_RUNTIME.app == 'web'){
            if (canShowAppDownloadFooter() == false ){
                return;
            }
            $('#js-appdown-footer').show();
            var handown = $('#js-app-download');
            //微信下载
            if ( navigator.userAgent.indexOf('MicroMessenger') > -1){
                handown.attr('href' , handown.attr('data-wechat')).attr('target' , '_blank');
                return;
            }
            if (navigator.userAgent.indexOf('Android') > -1){
                handown.attr('href' , handown.attr('data-android')).attr('target' , '_blank');
            } else {
                handown.attr('href' , handown.attr('data-ios')).attr('target' , '_blank');
            }
        }
    })();
    $('#js-appDownClose').click(function(){
        $('#js-appdown-footer').hide();
        var d = new Date();
        d.setTime( d.getTime() + 86400 *1000 );
        document.cookie = "appDownloadFooterNotDisplay=yes;expires=" + d.toGMTString();
    });
</script>
