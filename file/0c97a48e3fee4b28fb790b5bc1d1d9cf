            $(function () {
                //编辑按钮在层上打开

                $(".QuickMessage").click(function () {

                    var url = $(this).attr("href");
                    var txt = $(this).attr("title");
                    OpenLayer(url, txt);
                    return false;
                });
            });

            //   加载层框架脚本
            function OpenLayer(url, txt) {
                // art.dialog.open(url, { title: txt, width: 800, height: 240 }).lock();
                art.dialog.open(url, { title: txt }, 'false').lock();
                return false
            }
