<script>
function paihang(str)
{
//	alert(str)
//	document.getElementById("ph1").className="link1";
//	document.getElementById("ph2").className="link1";
//	document.getElementById("ph3").className="link1";
//	document.getElementById("ph4").className="link1";
//	document.getElementById("ph5").className="link1";
//	
//	document.getElementById(str).className="link2";
	     $("#ph1").removeClass("link2");
		 $("#ph2").removeClass("link2");
		 $("#ph3").removeClass("link2");
		 $("#ph4").removeClass("link2");
		 $("#ph5").removeClass("link2");
		 $("#"+str).removeClass("link1");
		 $("#"+str).addClass("link2");
}

</script>

