//箭头跳动
var x=0
var tTop=setInterval(runTop,300)
function runTop(){
if(x<1){
x=x+1
}else{
x=0
$(".foot_box span").animate({top:5},300)
}
$(".foot_box span").stop().animate({top:-10*x})
}
//弹出框
$(".foot_box span").click(
function(){
$(".pop_box").show()
}
)//点击箭头出现弹框
$(".close").click(
function(){
$(".pop_box").fadeOut()
}

)//点击隐藏弹框
</script>   
