<script>
   $(function(){
      var href=window.location.href;
      var hrefs = href.split('/');
      var hrefl = hrefs[hrefs.length-1].substr(0,4);
      var hover = 0;
      
      $(".nav li").each(function(){
          var nhref=$(this).find("a").attr("href"); 
          var nhrefs = nhref.split('/');
          var nhrefl = nhrefs[nhrefs.length-1].substr(0,4);
          
          if(hrefl == nhrefl)
          { 
          
            $(".nav li").find("a").removeClass("hover");            
             $(this).find("a").addClass("hover");
             hover = 1;
             return;
          }
      });
      if(hover==0)
      {
     
      hrefl = hrefs[hrefs.length-2].substr(0,4);
      $(".nav li").each(function(){
          var nhref=$(this).find("a").attr("href");   
          var nhrefs = nhref.split('/');
          var nhrefl = nhrefs[nhrefs.length-1].substr(0,4);
          if(hrefl == nhrefl)
          { 
            $(".nav li").find("a").removeClass("hover");            
             $(this).find("a").addClass("hover");
             hover = 1;
             return;
          }
      });
      }
   });
</script>

