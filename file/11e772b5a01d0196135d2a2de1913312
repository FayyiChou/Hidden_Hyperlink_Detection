<script type="text/javascript">
$(function(){
//当前位置修改
 
var posArrA=$("#currentPos a");
 
var url=$(posArrA[1]).attr("href");
$(posArrA[0]).attr("href",url);
//remove
$(posArrA[1]).remove();
var ArrObjct=($("#currentPos").html()).split("&nbsp;&gt;&nbsp;");

var displayInfo="";
for(var i=0,length=ArrObjct.length;i<length;i++)
{
if(ArrObjct[i]=="")
{
continue;
}

if(i!=length-1)
{
 displayInfo+=ArrObjct[i]+"&nbsp;&gt;&nbsp;";
}
else
{
displayInfo+=ArrObjct[i];
}

}

$("#currentPos").html(displayInfo);
});
 
</script>
