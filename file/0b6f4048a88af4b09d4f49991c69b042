<script>
(function(window) {
    function readCookie(name) {
        var ret, matchArr;
        if ((matchArr = String(window.document.cookie).match(
            new RegExp('(?:^| )' + name + '(?:(?:=([^;]*))|;|$)')))) {
            if (matchArr[1]) {
                ret = matchArr[1];
            }
        }
        return ret;
    }

    if (typeof MooTools != 'undefined')
    {
        var __top_fav_tips=readCookie('top_fav_tips');
        var __display_pocowo_community_promote_tips=readCookie('__display_pocowo_community_promote_tips');
        function __top_slide_fav(div_id,direction)
        {
            var per=4;
            var total_height=40;
            if(direction=='up') writeCookie('top_fav_tips', '1', 31536000, 'poco.cn')
            void function(){
                var slide_time=setInterval(function(){
                    var div_height= $(div_id).style.height;
                    var height=parseInt(div_height.replace("px",''));
                    if(direction=="up")
                    {
                        if(height>0)$(div_id).style.height=(height-4)+"px";
                        else   clearInterval(slide_time);
                    }
                    else
                    {
                        if(height<total_height) $(div_id).style.height=(height+4)+"px";
                        else clearInterval(slide_time);
                    }
                },50);
            }()
        }
        if(__top_fav_tips!=1 && 'hiden'==__display_pocowo_community_promote_tips)
        {
            var slide_html='<div id="__top_fav_tips" style="background:url(http://www.poco.cn/module_common/footer_v3/repeat-x.jpg) repeat-x;"><div style="height: 40px; overflow: hidden; background:url(http://www.poco.cn/module_common/footer_v3/new-1920-39.jpg) no-repeat center 0 "><div style="height:40px;line-height:40px;margin:0 auto;width:994px;"><div><div>      <div style="width:713px;text-indent:-9999px;float:left;">手机上poco</div></div><a title="关闭" href="#this" onclick=__top_slide_fav("__top_fav_tips","up") style="background:url(http://www.poco.cn/module_common/footer_v3/close-19-19.png) 0 1px no-repeat;display: block;float: right;height: 19px; margin: 10px 0 0 11px;text-indent: -9999em;width: 19px;margin-right:20px;">close</a><a href="#this" onclick=__top_slide_fav("__top_fav_tips","up"); style="float:right;color:#737373;">不再提示</a></div></div></div></div>';

            if(!$('__top_fav_tips'))
            {
                var new_obj_div=new Element('div',{'id':'__top_fav_tips'});
                new_obj_div.injectTop(document.body);
                new_obj_div.setStyle("height","0px");
                new_obj_div.setStyle("overflow","hidden");
                $('__top_fav_tips').innerHTML=slide_html;
            }
            window.addEvent('domready', function() {__top_slide_fav("__top_fav_tips",'down');});
        }
    }
})(window);
</script>
