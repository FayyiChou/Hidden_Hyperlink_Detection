show_mode = (__is_livephoto) ? 'livephoto' : show_mode;// 如果有livephoto的XML_KEY,肯定是用动感模式显示
show_mode = (! __is_livephoto && show_mode=='livephoto')?'list':show_mode;	// 如果cookie是livephoto，但事实上没有动感，转为多图
show_mode = in_array(show_mode, ['livephoto', 'wall', 'list'])?show_mode:'list'; // 默认多图
show_mode = in_array(__location_hash, ['livephoto', 'wall', 'list'])?__location_hash:show_mode;


var img_pagesize = 1;
var img_count = photoImgArr.length; // 图片总数
var total_page = 1;	// 总页数
var now_page = 1;	// 当前页数
var row_num = 3; //缩略图列表显示数量


switch(show_mode)
{
	case 'wall':
		img_pagesize = 1;
		total_page = img_count;
		break;
	case 'list':
		img_pagesize = img_count;
		total_page = 1;
		break;
	case 'livephoto':
		img_pagesize = 0;
		total_page = 0;
		break;
}
