<script> 

//引用
function vip_cmt_set_quote(num,quote_head)
{
	var sHtml = vip_cmt_contents_original[num];
	if(quote_head)
	{
		var tmp_str;
		tmp_str = sHtml;
		var new_cmt_content_objs = document.getElementsByName('new_cmt_content');
		
		for (var i in new_cmt_content_objs)
		{
			try
			{
				new_cmt_content_objs[i].focus();
				new_cmt_content_objs[i].value += '[QUOTE='+quote_head+']'+tmp_str+'[/QUOTE]\n';
			}
			catch(err)
			{
				continue;
			}
		}
		
	}
}

window.setTimeout("vip_CmtPosedPaserUBB();", 500);
window.setTimeout("vip_CmtPosedPaserUBB();", 800);

