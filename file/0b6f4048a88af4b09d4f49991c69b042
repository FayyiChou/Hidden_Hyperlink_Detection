//热门留言UBB处理
var vip_cmt_contents_original = new Array();
function vip_CmtPosedPaserUBB()
{
	if(vip_cmt_contents_original.length==0)
	{	
		try
		{
			var vip_cmt_content_obj= document.getElementsByName('vip_cmt_contents');
			for (var i=0;i<vip_cmt_content_obj.length;i++)
			{
				vip_cmt_contents_original[vip_cmt_content_obj[i].getAttribute("cmt_id")] = vip_cmt_content_obj[i].innerHTML + '';
				if( vip_cmt_content_obj[i].getAttribute("uid")==0 )
				{
					vip_cmt_content_obj[i].innerHTML = convert_ubb_tohtml(vip_cmt_content_obj[i].innerHTML, true);
				}
				else
				{
					vip_cmt_content_obj[i].innerHTML = convert_ubb_tohtml(vip_cmt_content_obj[i].innerHTML);
				}
				vip_cmt_content_obj[i].innerHTML = convert_ibf_emoticons(vip_cmt_content_obj[i].innerHTML);
			}
		}catch(err){}
	}
}



function share_res()
{
	var user_id = login_id;
	
	user_id = parseInt(user_id);
	
	this.blur();
	if (user_id < 1) {
		alert("对不起,你尚未登录,请先登录");
		user_login_opts(1);
	} else {
		window.open("http://my.poco.cn/apps/share/?share_title=Hello%CF%E3%B8%DB+%CC%D8%C9%AB%C3%D7%B7%DB%2B%CC%D8%C9%AB%D4%C6%CD%CC%C3%E6&share_url=" + escape(window.location.href), '_blank');
	}
}

