function food_copy_link()
{
	var food_link=escape("http://food.poco.cn/commendDetail-htx-id-692195.shtml");

	// 添加统计
	var stat_img = new Image();
    stat_img.src = "http://imgtj.poco.cn/food_detail_copy.css?wid=0&"+food_link+"&tmp="+Math.random()*1000000;

window.clipboardData.setData('Text',document.getElementById('copy_textarea').value+'\r\n');alert('链接已复制到剪贴板，打开好友qq或msn聊天窗口，按ctrl+v可以推荐给好友')
	}
