var last_item_index = 1;
var new_page=0;  //翻页的新页数
function write_sel_show_mode_module(position)
{
	var livephotohtml='', html_str = '<div id="show_type_sel_'+position+'" class="show_type_sel">';

	if (__is_livephoto)
	{
		livephotohtml = '<a href="javascript:switch_show_mode(\'livephoto\');">动感模式</a>　'
	}

	switch (show_mode)
	{
		case 'wall':
			html_str += '浏览方式：' + livephotohtml + '<a class="bold" href="javascript:switch_show_mode(\'wall\');">图片墙</a><img src="http://my.poco.cn/v3/images/new.gif" />　<a href="javascript:switch_show_mode(\'list\');">多图</a>';
		break;
		case 'list':
			if(old_show_mode=='wall')
			{
				html_str += '浏览方式：' + livephotohtml + '<a class="bold" href="javascript:switch_show_mode(\'wall\');">图片墙</a><img src="http://my.poco.cn/v3/images/new.gif" />　<a href="javascript:switch_show_mode(\'list\');">多图</a>';
			}
			else
			{
				html_str += '浏览方式：' + livephotohtml + '<a href="javascript:switch_show_mode(\'wall\');">图片墙</a><img src="http://my.poco.cn/v3/images/new.gif" />　<a class="bold" href="javascript:switch_show_mode(\'list\');">多图</a>';
			}
		break;
		case 'livephoto':
			if(in_array(login_id,['3650071','38701983','45819098','54885029','38496700','39578743', '44634066']))
			{
				html_str += '<a href="http://my.poco.cn/v3/livephotomaker.php?xml_key=' + __livephoto_xml_key + '" target="_blank">下载动感图片到本地</a> ';	
			}
				html_str += '浏览方式：<a class="bold" href="javascript:switch_show_mode(\'livephoto\');">动感模式</a>　<a href="javascript:switch_show_mode(\'wall\');">图片墙</a><img src="http://my.poco.cn/v3/images/new.gif" />　<a href="javascript:switch_show_mode(\'list\');">多图</a>';
		break;
	}

	if(is_preview!=1)
	{
		html_str += '　<a href="'+__act_channel_url+'" target="_blank">社区作品</a>';
	}
	html_str += '</div>';

	document.write(html_str);
}

// 切换浏览方式
function switch_show_mode(show_mode)
{
	writeCookie('lastphoto_show_mode', show_mode);
	var location_url = get_browser_url();
	location_url += '#'+show_mode;
	document.location.href = location_url;
}

// 打印上一页下一页部分
function write_next_and_pre_module()
{
	var html_str = '';
	switch (show_mode)
	{
		case 'list':
			if(total_page!=1)
			{
				html_str += '<div class="next_and_pre_items"><input class="bnt_bg1" name="pre_img_list" onclick="next_and_pre_module(this);" value="上一页" type="button" /><font class="mlr10">本作品共 (<span>1</span>/' + total_page + ') 页图片</font><input class="bnt_bg1" name="next_img_list" onclick="next_and_pre_module(this); " value="下一页" type="button" /></div>';
			}
		break;
		case 'livephoto':
		break;
	}
	document.write(html_str);
}

// 上一页下一页
function next_and_pre_module(obj) {
	window.location.href = '#show_type_sel_top';
	switch (obj.name)
	{
		case 'next_img_list':
			new_page++;
		break;
		case 'pre_img_list':
			new_page--;
		break;
	}
	output_img_list_module(new_page, false);
}

// 打印图片列表部分
function output_img_list_module(page, is_init)
{
	$$('.next_and_pre_items span').each(function(i){
		i.innerHTML = page;
	});
	var html_str = '';

	/*var pre_flip_bnt = document.getElementsByName('pre_img_list');
	var next_flip_bnt = document.getElementsByName('next_img_list');
	for (var i = 0, len = pre_flip_bnt.length; i < len; i++)
	{
		if (page == 1) pre_flip_bnt[i].disabled = true;
		else pre_flip_bnt[i].disabled = false;
	}

	for (var i = 0, len = next_flip_bnt.length; i < len; i++)
	{
		if (page == total_page) next_flip_bnt[i].disabled = true;
		else next_flip_bnt[i].disabled = false;
	}*/
	switch (show_mode)
	{
		case 'list':
			var ifor = photoImgArr.length;
			for (var i = 0; i < ifor; i++)
			{
				html_str += get_img_box_html(i);
			}
		break;
	}
	$('J_img_list').innerHTML = html_str;
	
	if (show_mode === 'list')
	{
		if (typeof itemCollect == 'undefined')
		{
			getScript('http://wwwcdn.poco.cn/js_common/??NUY4QkMxQ0I=.js&t=201201121437121234.js', {
				callback:function(){
					itemCollect.init({
						'photoItemIds':window.photoCollectIds,
						newTopicTxt:{
							intro:'Hello香港 特色米粉+特色云吞面'
						}
					});
				}
			});
		}
		else
		{
			window.photoCollectIds.length && itemCollect.append(window.photoCollectIds);
		}
	}
	return new_page = page;
}

// 获得一个图片的HTML
function get_img_box_html(img_mun)
{
	if(show_mode == 'wall')
	{
		var photo_list_li = '<li><img id="J_item_image" item="'+photoImgArr[img_mun].id+'" src="' + photoImgArr[img_mun].src + '" /><strong class="img_title">' + photoImgArr[img_mun].alt + '</strong></li>';
		
		return photo_list_li;
	}
	else
	{
		var win = window, photo_list_li = [];
		photo_list_li.push('<li><a href="javascript:void(0);" class="img_box" onclick="call_swf(\'' + photoImgArr[img_mun].src + '\');return false;"><img title="' + photoImgArr[img_mun].alt + '" item="' + photoImgArr[img_mun].id + '" src="' + photoImgArr[img_mun].src + '" /></a>');
		photo_list_li.push('<div class="img_list_info clearfix"><div class="fr ext_operate" style="width:250px;">');
		photo_list_li.push('<span class="fav_photo"><a href="javascript:void(0);" id="J_item_collect_handle_' + photoImgArr[img_mun].id + '" data-photo-item-id="' + photoImgArr[img_mun].id + '" data-act-id="' + __act_id + '"');
		if (win.__act_item_id) photo_list_li.push(' data-act-item-id="' + win.__act_item_id + '"');
		if (win.__act_type_id) photo_list_li.push(' data-act-type-id="' + win.__act_type_id + '"');
		if (win.__user_id) photo_list_li.push(' data-owner-id="' + win.__user_id + '"');
		
		photo_list_li.push(' class="item_fav_handle" hidefocus="true"><i></i>收录<strong>');
		
		var tmpItemFavData = win._itemFavData;
		if (typeof tmpItemFavData != 'undefined' && parseInt(tmpItemFavData[photoImgArr[img_mun].id])>0)
		{
			photo_list_li.push('(' + parseInt(tmpItemFavData[photoImgArr[img_mun].id]) + ')');
		}
		photo_list_li.push('</strong></a>');
				if (img_mun == 0 && readCookie('like_img_tips') != 'hidden')
		{
			photo_list_li.push('<div id="J_item_collect_pop_layer_tips" class="item_collect_pop_layer">');
			photo_list_li.push('<div class="context">');
			photo_list_li.push('<p>如果喜欢这张图片，可以收录起来。</p>');
			photo_list_li.push('<p><a href="http://photo.poco.cn/like/" target="_blank">点此看其他人的收录&gt;&gt;&gt;</a></p>');
			photo_list_li.push('<a href="javascript:void(0);" class="J_cls_tips_btn cls_btn" onclick="clsItemFavTipsModule();"></a>');
			photo_list_li.push('</div>');
			photo_list_li.push('<div class="asarrow"></div>');
			photo_list_li.push('</div>');
		}
				photo_list_li.push('</span>');
		
		photo_list_li.push('<span class="share_singleimg">');
		photo_list_li.push('<a href="javascript:void(0);" class="share_handle" hidefocus="true" role="showWidget" data-photo-item-id="' + photoImgArr[img_mun].id + '" data-photo-url="' + photoImgArr[img_mun].src + '">分享</a>');
		photo_list_li.push('</span>');
		
		/*** 点评这张 ***/
		photo_list_li.push('<span class="cmt_photo">');
		photo_list_li.push('<a href="javascript:void(0);" id="J_cmt_item_handle_' + photoImgArr[img_mun].id + '"  class="cmt_item_handle" onclick="cmt.addPhotoCmt(this);" data-cmt-topic-url="' + photoImgArr[img_mun].cmt_topic_url + '" data-photo-item-id="' + photoImgArr[img_mun].id + '" data-photo-item-url="' + photoImgArr[img_mun].src + '" data-init-str="" hidefocus="true"><i></i>赞/评论</a></span>');
		/*** End 点评这张 ***/
		
		photo_list_li.push('</div>');
		
		var maxLen = (is_preview==1) ? (767-190-76) : (758-190-76),
		strLen = photoImgArr[img_mun].alt.clen(), padLeft = 93;
		if (strLen>57) padLeft = maxLen-strLen*7;

		photo_list_li.push('<div class="txt_content" style="padding-left:' + padLeft + 'px"><span>' + photoImgArr[img_mun].alt + '</span></div>');
	
		photo_list_li.push('</div><div class="alpha_line"></div></li>');

		window.photoCollectIds.push(photoImgArr[img_mun].id);
	
		return photo_list_li.join('');
	}
}

function clsItemFavTipsModule()
{
	if ($('J_item_collect_pop_layer_tips'))
	{
		$('J_item_collect_pop_layer_tips').style.display='none';
		writeCookie('like_img_tips', 'hidden', 24, '.poco.cn', '/');//1日
	}
	return false;
}

//显示隐藏餐厅详细
function restaurant_information(obj) {
	if ($$(obj)[0].getParent().getNext().style.display != 'block') {
		obj.innerHTML = "- 收起";
		$$(obj)[0].getParent().getNext().style.display = 'block';
	} else {
		obj.innerHTML = "+ 了解详情";
		$$(obj)[0].getParent().getNext().style.display = 'none';
	}
}

function show_photowall_simg_list()
{	
	
	var html_str = '<div class="ul_imglist165 pt10"><ul class="clearfix">';	
	var _tr;
	
	for(var i=0, len = photoImgArr.length;i<len;i++)
	{
		// 图片tr
		_tr = (i+1)%row_num;
		html_str += '<li><div class="wall" onclick="call_swf(\''+photoImgArr[i].src+'\')"><div><span class="c_i165x165"><img src="'+photoImgArr[i].simage+'" title="'+photoImgArr[i].alt+'" /></span></div></div></li>';
		if(_tr == 0)
		{
			html_str += '</ul><ul class="clearfix">';
		}
	}
	html_str += '</ul></div>';
	document.write(html_str);
}


function vote_act() {
	try{
		if (login_id<1)
		{
			loginApp.set({
				CustomApi:function(data){
					vote_act();
				}
			});
			loginApp.loginBox();
			return false;
		}
	}
	catch(e){}

	var pars = 'act_id=' + __act_id + '&t=' + timestamp(); //传送的参数
	new Request({
		'url': 'http://' + window.location.host + '/module_common/act/vote_act.js.php',
		'method': 'get',
		//提交方式
		'data': pars,
		'evalScripts': true,
		// 是否执行处理页的JS
		'onComplete': function(res) {

}
	}).send();
}

function custom_callback_act_vote(ret) {
	if (ret) {
		try {
			parent.show_msg_box({
				'title': '提示',
				'content': '您投票成功了！',
				'cover': false
			});
			document.getElementsByName('__act_vote_count_info').each(function(i) {
				i.innerHTML = i.innerHTML + 1
			});
		} catch(e) {};
	} else {
		try {
			parent.show_msg_box({
				'title': '提示',
				'content': '投票失败，您之前已经投过票了！',
				'cover': false
			});
		} catch(e) {
			alert('投票失败，您之前已经投过票了！')
		}
	}
}

function plus_share_res(urlencode_title)
{
	window.open("http://my.poco.cn/apps/share/?share_title="+urlencode_title+"&share_url=" + escape(window.location.href), '_blank');
}


