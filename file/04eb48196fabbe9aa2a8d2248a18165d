	$("#searchBtn").click(function() {
			window.location.href = '/rent/kw'+encodeURI($("#buildname").val());
	})

	$(function(){
		showHis();
	})
	function showHis()
	{
		var newtemp = eval('(' + $.cookie("rent_hisHouse") + ')');
		var newtemp_ = "";
		var currentNum = 0;
		if(newtemp){
		for(var n=newtemp.length-1;n>-1;n--){
			if(currentNum++ >= 10){
				break;
			}
			newtemp_ += '<li><span class="y Cred">'+newtemp[n].price+'</span><a href="/rent/detail/'+newtemp[n].houseId+'" title="'+newtemp[n].housename+'" target="_blank"><span>'+newtemp[n].housename+' <span class="C999">'+newtemp[n].roomCount+'室</span></span></a></li>'
		}
		$("#LeaseData").html(newtemp_);
		}
	}
	
	function clear_hisHouse(){
		$.cookie("rent_hisHouse", '', {expires:-1,path:"/"});
		$("#LeaseData").html('');
	}
	
	$("#search_price").click(function() {
		var lprice = $("#lprice").val();
		if( !g_IsInt(lprice) ){
			tishiBox("请正确填写！！","jinggao");
			$("#lprice").val('');
			$("#lprice").focus();
			return ;
		}
		var uprice = $("#uprice").val();
		if( !g_IsInt(uprice) ){
			tishiBox("请正确填写！！","jinggao");
			$("#uprice").val('');
			$("#uprice").focus();
			return ;
		}
		if( parseInt(lprice) >= parseInt(uprice)  ){
			tishiBox("请正确填写！！","jinggao");
			$("#uprice").val('');
			$("#uprice").focus();
			return ;
		}
		var u = '/rent/';
		if( u == '/rent/'){
			u = u + 'j' + lprice + ',' + uprice + '/';
		}else{
			u = u + '-j' + lprice + ',' + uprice + '/';
		}
		window.location.href = u;
	})
	
	function g_IsInt(s)
	{
		if(s==null) return false;
		 var reg=/^[1-9]\d*$/;
		 if(!reg.test(s)) return false;
		 else return true;
	}

	$("#search_area").click(function() {
		var larea = $("#larea").val();
		if( !g_IsInt(larea) ){
			tishiBox("请正确填写！！","jinggao");
			$("#larea").val('');
			$("#larea").focus();
			return ;
		}
		var uarea = $("#uarea").val();
		if( !g_IsInt(uarea) ){
			tishiBox("请正确填写！！","jinggao");
			$("#uarea").val('');
			$("#uarea").focus();
			return ;
		}
		if( parseInt(larea) >= parseInt(uarea)  ){
			tishiBox("请正确填写！！","jinggao");
			$("#uarea").val('');
			$("#uarea").focus();
			return ;
		}
		var u = '/rent/';
		if( u == '/rent/'){
			u = u + 'a' + larea + ',' + uarea + '/';
		}else{
			u = u + '-a' + larea + ',' + uarea + '/';
		}
		window.location.href = u;
	})
	
	$(".selectBox").mouseenter(function(){
		$(this).find(".popList").css("display","block")
	})
	$(".selectBox").mouseleave(function(){
		$(this).find(".popList").css("display","none")
	})
	
	$(".popList li").click(function(){
		$(this).parent().css("display","none");
		$(this).parent().parent().find("span").html($(this).html());
	})
	
	loadSearch();
	function loadSearch()
	{
		var houseKind = ""; 
		if(houseKind && houseKind!='')
		{
			$("#houseKind_show").html($(".houseKind_ul .ol").html());
		}
		
		var deck = ""; 
		if(deck && deck!='')
		{
			$("#deck_show").html($(".deck_ul .ol").html());
		}
		
		var direction = ""; 
		if(direction && direction!='')
		{
			$("#direction_show").html($(".direction_ul .ol").html());
		}
	}
