function show_newsdi(id,n){
var menu = ['newsli_1','newsli_2','newsli_3'];
var arra = ['newsdi_1','newsdi_2','newsdi_3'];
for(var i = 0; i < arra.length; i++){
document.getElementById(arra[i]).style.display = 'none';
document.getElementById('span_more2_' + i).style.display="none";
document.getElementById(menu[i]).className = 'newsunli';
}
document.getElementById('newsdi_' + id).style.display = '';
document.getElementById('newsli_' + id).className = 'newsli';
document.getElementById('span_more2_' + n).style.display="";
}

function show_spdi(id){
var menu = ['spli_1','spli_2'];
var arra = ['spdi_1','spdi_2'];
for(var i = 0; i < arra.length; i++){
document.getElementById(arra[i]).style.display = 'none';
document.getElementById(menu[i]).className = 'spunli';
}
document.getElementById('spdi_' + id).style.display = '';
document.getElementById('spli_' + id).className = 'spli';
}

function show_zxdi(id){
var menu = ['zxli_1','zxli_2'];
var arra = ['zxdi_1','zxdi_2'];
for(var i = 0; i < arra.length; i++){
document.getElementById(arra[i]).style.display = 'none';
document.getElementById(menu[i]).className = 'spunli';
}
document.getElementById('zxdi_' + id).style.display = '';
document.getElementById('zxli_' + id).className = 'spli';
}

function show_ssdi(id){
var menu = ['ssli_1','ssli_2'];
var arra = ['ssdi_1','ssdi_2'];
for(var i = 0; i < arra.length; i++){
document.getElementById(arra[i]).style.display = 'none';
document.getElementById(menu[i]).className = 'ssunli';
}
document.getElementById('ssdi_' + id).style.display = '';
document.getElementById('ssli_' + id).className = 'ssli';
}
