				<script>
    function substr(str) {
    var sub = 20
    var len = str.length
    if (len&gt;sub) {
        var str1 = str
        var str2 = ""
        var i = 0
        var n = 0
        while ( (str2.length&lt;sub) &amp;&amp; (sub&lt;8) ) {
            i++
            var aa = str1.substring(0,1)

            if ( isW(aa) ) {
                if (n % 2 == 0) 
                    sub++
                n++
            }
            str1 = str1.substring(1)
            str2 = str2 + aa
        }
        if (len&gt;sub) {
            document.write(str.substring(0,(sub-2))+"...")
        }
        else {
            document.write(str)
        }
    }
    else {
        document.write(str)
    }
}
</script>
         

