<script type="text/javascript">
    var footerId = 'footer-newsletter-validate-detail';
    var footerSignUpForm = new VarienForm(footerId, false);

    function submitFooter() {
        if (footerSignUpForm.validator.validate()) {
            var emailAddress = jQuery('#footer-newsletter-validate-detail').serialize();
            jQuery('#footer-newsletter-validate-detail').trigger("reset");
            jQuery.fancybox({
                autoSize: true,
                href: "//www.valextra.com/en-ww/extras/newsletter/ajaxform/",
                type: 'ajax',
                ajax: {
                    type: "POST",
                    data: emailAddress
                }
            });
        }
    }
    new Event.observe(footerId, 'submit', function(e){
        e.stop();
        submitFooter();
    });
</script>
