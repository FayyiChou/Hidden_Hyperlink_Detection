        //判断是否收藏了
     $(function(){
        //判断是否收藏了
             var host = 'http://i.3g.cnfol.com/';
            var url= host+'usercenter/php/AttenApi.php?jsoncallback=?';
            var id=$('#aid').val();
            $.getJSON(url,{action:'getStatus',aid:id,type:3},function(msg){
                //已经收藏过了
                if(msg.msg==1){
                        $('#CzZanStar').removeClass('Fl CzZanStar');
                        $('#CzZanStar').addClass('Fl CzZanStar CzZanedStar');
                }
            });
     });

    function articlecollect(type,uid,title,authorId,url,artId,atime){
         var host = 'http://i.3g.cnfol.com/';
         var oTimeOut=document.getElementById("TimeOut");
         var url= host+'usercenter/php/AttenApi.php?jsoncallback=?';
         var arl = document.location.href;

        if(uid){
                   //收藏文章
                    $.getJSON(url,{action:'collect',aid:artId,title:title,format:'json',aurl:arl,atime:atime,type:3},function(msg){
                    if(msg.error=='0002'){
                       alert(msg.msg);
                    }else{
                        if(msg.msg=='取消关注成功'){
                            alert('取消收藏成功');
                        }else if(msg.msg=='添加关注成功'){
                            alert('添加收藏成功');
                        }

                        window.location.reload();
                    }

                });

        }else{
              $('#CzPhoneYz').show();
              $('#CzMark').removeClass('CzMark CzHide');
                $('#CzMark').addClass('CzMark');
        }
    }
    function hiden(){
        $('#CzPhoneYz').hide();
        $('#CzMark').removeClass('CzMark');
        $('#CzMark').addClass('CzMark CzHide');
    }

