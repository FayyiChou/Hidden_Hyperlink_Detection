    //发表评论
    function checkcmtformComment(id,type){
        var content = $('#PLtxt').val();
        if(content == '' || content == '请输入评论内'){
            alert('请输入评论'); return false;
        }else{
            $.post(baseurl+'/ajaxcomment/Action',$('#'+id).serialize(),function callback(data){
                alert(data.error);
                if((data.errno == '200036') || (data.errno == '200037') || (data.errno == 'check')){
                    $('#PLtxt').val('');
                }else{
                    $('#PLtxt').val('');
                    if(data.errno == 'succ'){
                        var focusNum = parseInt($("#ArticleCommentNum").first().text())+1;
                        $('#ArticleCommentNum').html(focusNum);
                        //getAricleList();
                    }

                }
                if(type !=1){
                    $('#PL_form').removeClass('Show ');
                    $('#PL_form').addClass('Hide');
                }
                $('#PL_form i').removeClass('Ok');

            },'json');
        }
    }

    var baseurl = "http://blog.test1.cnfol.com";
            var userid = '';
            var act = '';
            function delAttention(uid,artct){
                if(uid && artct){
                    userid = uid;
                    act = artct;
                }
                var url = baseurl+"/index.php?c=myfocus&m=action&act="+act;
                var params = {"friendIDs":userid};

                $.getJSON(url+"&r="+new Date().getTime(), params, function(json){
                    if(json.erron=='01'){
                        if(act == "del"){
                            alert("取消关注");
                            var focusNum = parseInt($(".fansNum").first().text())-1;
                            $(".fansNum").each(function(){
                                $(this).text(focusNum);
                            })
                            $("#GuanZhuFocus").removeClass('BtnJian');
                            $("#GuanZhuFocus").addClass('BtnAdd');
                            $("#GuanZhuFocus").attr("href","javascript:delAttention('1323901','add');");
                        }else{
                            alert("关注成功");
                            var focusNum = parseInt($(".fansNum").first().text())+1;
                            $(".fansNum").each(function(){
                                $(this).text(focusNum);
                            })
                            $("#GuanZhuFocus").removeClass('BtnAdd');
                            $("#GuanZhuFocus").addClass('BtnJian');
                            $("#GuanZhuFocus").attr("href","javascript:delAttention('1323901','del');");
                        }
                    }else{
                        alert(json.error);
                    }
                });
            }
