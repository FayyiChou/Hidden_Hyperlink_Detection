        type: 'GET',
        url: baseurl+'/api.php?url='+encodeURIComponent(urlcur),
        cache: false,
        async: false,
        dataType:'json',
        success: function(json){
            appid = json.appId;
            timestamp = json.timestamp;
            nonceStr = json.nonceStr;
            signature = json.signature;
        },
        error: function(html){
        }
    });

wx.config({
    debug: false,
    appId: appid,
    timestamp: timestamp,
    nonceStr: nonceStr,
    signature: signature,
    jsApiList: ['onMenuShareTimeline','onMenuShareAppMessage']
});
