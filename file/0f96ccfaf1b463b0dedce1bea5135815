                             //金币不够转化成人民币支付
                            function postmoney(articleid,userid,touserid,urls){

                                var num = document.getElementById('CzFrTxt').value;
                                   var GiftPrice = document.getElementById('GiftPrice').value;
                                   var RMBMoney = document.getElementById('RMBMoney').value;
                                   var Money = document.getElementById('Money').value;
                                   var callurl=encodeURIComponent(window.location.href);
                                   var key=document.getElementById("key_"+articleid).value;
                                   var allmoney='';
                                   /***************新增***********************/
                                    if(!agreeToop.testAgree())
                                    {
                                        alert("没有同意增值服务协议");
                                        return false;
                                    }
                                      /***************新增***********************/
                                   if(document.getElementById("checkboxID").checked){
                                        //alert("checkbox is checked");
                                        if(GiftPrice>0){
                                        if(GiftPrice>num){
                                             alert('阅读该篇博文至少需要'+GiftPrice+'朵博文');
                                        }else{
                                            allmoney=Number(RMBMoney)*10+Number(Money);
                                            if(num>allmoney){
                                             location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=3&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;
                                            }else{
                                                postrmbflow(articleid,touserid, userid, num, window.location.href);
                                            }
                                        }
                                   }else{
                                        if(num>0){
                                            allmoney=Number(RMBMoney)*10+Number(Money);
                                            if(num>allmoney){
                                                location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=3&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;
                                            }else{
                                                postrmbflow(articleid,touserid, userid, num, window.location.href);
                                            }
                                         }else{
                                            alert('赠送数要大于0');
                                         }
                                   }
                                }else{
                                    if(GiftPrice>0){
                                        if(GiftPrice>num){
                                             alert('阅读该篇博文至少需要'+GiftPrice+'朵博文');
                                        }else{
                                             location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=2&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;
                                        }
                                   }else{
                                        if(num>0){
                                                location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=2&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;

                                         }else{
                                            alert('赠送数要大于0');
                                         }
                                   }
                                }
                            }
                            //使用钱包
                            function postrmbflow(articleid,touserid, userid, num,urls){
                                   var url = baseurl + "/index.php/channel/putRMBflower?userid=" + userid + '&giftnum=' + num + '&touserid=' + touserid + '&url='+urls+ '&articleid='+articleid+'&type=json';
                                $.ajax({
                                            type: 'get',
                                                    url: url,
                                                    dataType: 'json',
                                                    success: function(data){
                                                    if (data==1){
                                                      alert('赠送成功');
                                                       erricon();
                                                       location.reload();
                                                    } else{
                                                      alert('赠送失败');
                                                      erricon();
                                                    }
                                                    },
                                                    error:function (XMLHttpRequest, textStatus, errorThrown) {
                                                    alert('系统繁忙，稍后再试');
                                                    }
                                            });
                            }
                            //赠送鲜花
                            function postflow(articleid,touserid, userid,urls){
                                   var num = document.getElementById('CzFrTxt').value;
                                    var GiftPrice = document.getElementById('GiftPrice').value;
                                    var RMBMoney = document.getElementById('RMBMoney').value;
                                   var Money = document.getElementById('Money').value;
                                   var callurl=encodeURIComponent(window.location.href);
                                   var key=document.getElementById("key_"+articleid).value;
                                   var allmoney='';
                                   if(GiftPrice>0){
                                        if(GiftPrice>num){
                                             alert('阅读该篇博文至少需要'+GiftPrice+'朵博文');
                                        }else{
                                             if(document.getElementById("checkboxID").checked){
                                                allmoney=Number(RMBMoney)*10+Number(Money);
                                                if(num>allmoney){
                                                    location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=3&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;
                                                }else{
                                                    //if(num>Number(Money)){
                                                        postrmbflow(articleid,touserid, userid, num, window.location.href);
                                                    // }else{
                                                    //     pflow(articleid,touserid, userid, num,urls);
                                                    // }

                                                }
                                            }else{
                                                if(num>Number(Money)){
                                                        location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=3&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;
                                                    }else{
                                                        pflow(articleid,touserid, userid, num,urls);
                                                    }
                                            }
                                        }
                                   }else{
                                        if(num>0){
                                                if(document.getElementById("checkboxID").checked){
                                                allmoney=Number(RMBMoney)*10+Number(Money);
                                                if(num>allmoney){
                                                    location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=3&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;
                                                }else{
                                                    //if(num>Number(Money)){
                                                        postrmbflow(articleid,touserid, userid, num, window.location.href);
                                                    // }else{
                                                    //     pflow(articleid,touserid, userid, num,urls);
                                                    // }

                                                }
                                            }else{
                                                if(num>Number(Money)){
                                                        location.href="http://pay.3g.cnfol.com/index.php?r=NewPay/Flower&Type=3&SysUserID=15&IsFreeze=1&FromUserID="+userid+"&ToUserID="+touserid+"&SourceURL="+url+"&SourceTypeDesc=blog3g&GiftCnt="+num+"&CallBack="+callurl+"&Key="+key+"&SourceTabID="+articleid;
                                                    }else{
                                                        pflow(articleid,touserid, userid, num,urls);
                                                    }
                                            }
                                         }else{
                                            alert('赠送数要大于0');
                                         }
                                   }
                            };
                            function pflow(articleid,touserid, userid, num,urls){

                                   var url = baseurl + "/index.php/channel/putflower?userid=" + userid + '&giftnum=' + num + '&touserid=' + touserid + '&url='+urls+  '&type=json&articleid='+articleid;

                                $.ajax({
                                            type: 'get',
                                                    url: url,
                                                    dataType: 'json',
                                                    success: function(data){
                                                    if (data==1){
                                                      alert('赠送成功');
                                                       erricon();
                                                       location.reload();
                                                    } else{
                                                      alert('赠送失败');
                                                      erricon();
                                                    }
                                                    },
                                                    error:function (XMLHttpRequest, textStatus, errorThrown) {

                                                    }
                                            });
                            }
                //点赞
                 function submitvote(aid, memid, appeartime){
                                    //var baseurl = "http://blog.test1.cnfol.com";
                                            var getcode = $("#dd_" + aid).val();
                                            var blogartvote = "";
                                            //alert(getcode);
                                            //判断是否已赞
                                            if (blogartvote != 1){
                                    $.post(baseurl + '/ajaxsetartvotecnt/' + appeartime + '-' + aid + '/' + memid, {code:getcode, tnlvote:$('#showvotes_'+aid).html()}, function(data){
                                    if (data.errno == 'succ'){
                                            $('#showvotes_'+aid).removeClass('ZanImg');
                                            $('#showvotes_'+aid).addClass('ZanImg2');
                                            var num = parseInt($('#showvotes_'+aid).html()) + 1;
                                            $('#showvotes_'+aid).html(num);
                                            blogartvote = 1;
                                            $(function(){
                                            $('#ajaxdiv').load("http://blog.test1.cnfol.com/ajaxlmcount/duid=1323901&mid=93943&dom=zxj710810&bn=%E6%B3%A2%E6%AE%B5%E4%B9%8B%E5%AD%90&aid=127499756&vuid=0&un=0&nn=0&articletime=1490888705&title=【波段之子】3.30股市直播&co=b07f02f598bdf778?" + new Date().getTime());
                                                    $('#ajaxdiv').load("http://blog.test1.cnfol.com/ajaxomcount/art/127499756?" + new Date().getTime());
                                            });
                                    } else{
                                    alert(data.error);
                                    }
                                    }, 'json');
                                    } else{
                                    alert('你已经赞过了');
                                    }
                                    }
                // //加载图

               /*******************************改动*****************************************/
                $("#imgclick img").bind("click",function(){
                   var sliderH=$(window).height();
                   var oMask=document.getElementById('Mask');
                    var oPicBigImg=document.getElementById('PicBigImg');
                    var oImgCls=document.querySelector('#PicBigImg .ImgCls');
                    var oBody = document.getElementsByTagName('body')[0];
                     if (oBody.offsetHeight < document.documentElement.clientHeight) {
                            oMask.style.height = document.documentElement.clientHeight + 'px';

                        } else {
                            oMask.style.height = oBody.offsetHeight + 'px';

                    }
                    oMask.style.display = 'block';
                    oPicBigImg.style.display='block';
                    oImgCls.src=this.src;

                    oPicBigImg.style.top="0px";
                    oPicBigImg.style.height=sliderH+'px';
                    if(oImgCls.offsetHeight<=sliderH)
                    {
                        oImgCls.style.marginTop=((sliderH-oImgCls.offsetHeight)/2)+'px';
                    }
                    else
                    {
                        oImgCls.style.marginTop='0px';
                    }

                    var myScroll = new IScroll('#PicBigImg', { scrollX: true, scrollY: true, mouseWheel: true,click:true});

                    oMask.onclick = function() {
                        oMask.style.display = 'none';
                        oPicBigImg.style.display='none';
                    }

                    oImgCls.onclick=function(){
                        oMask.style.display = 'none';
                        oPicBigImg.style.display='none';
                    }
                });
            /*******************************改动*****************************************/

                //调整小图
                    DifPic();
                    function DifPic( ){
                        var MoreImg = document.querySelectorAll("#imgclick img");
                        var arr=[];
                        for (var i = 0; i < MoreImg.length; i++) {
                            arr.push(MoreImg[i].offsetWidth);
                        };
                        for (var i = arr.length - 1; i >= 0; i--) {
                            if(arr[i]<320)
                            {
                                MoreImg[i].style.width='auto';
                            }
                            else{
                                MoreImg[i].style.width='100%';
                            }
                        };

                    }
</script>
