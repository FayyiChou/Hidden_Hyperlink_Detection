/**************头部漂浮**************/
                flyInfor().play();
                function flyInfor()
                {
                   var oHeader=document.querySelector("#driftHead");
                   var oHidden=document.querySelector("#driftHide");
                    var ttp=oHeader.offsetTop;
                   function scrollFly(){
                        var dis=document.documentElement.scrollTop||  document.body.scrollTop;

                        if(dis>=ttp)
                        {
                            oHeader.style.cssText="position:fixed;left:0;top:0";
                            oHidden.style.display="block";
                        }
                        else
                        {
                            oHeader.style.cssText="position:relative";
                            oHidden.style.display="none";
                        }

                   }
                    var slide={};
                    slide.play=function(){
                        document.onscroll=scrollFly;
                    }
                    return slide;
                }

