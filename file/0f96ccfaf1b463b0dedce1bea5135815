                            function changes(Money, touserid, userid,GiftPrice,url,articleid){
                            var flow = document.getElementById('CzFrTxt').value;
                                    var zeng = document.getElementById('zeng');
                            if (flow > 0){
                                if(GiftPrice>10){
                                if(flow >=GiftPrice){
                                if (flow > Money){
                                zeng.style.display = "none";
                                        $('#chong').removeClass('Ml10 CzHide');
                                        $('#chong').addClass('Ml10');
                                } else{
                                zeng.style.display = "inline-block";
                                        //zeng.href = "javascript:postflow("+articleid+',' + touserid + "," + userid + "," + flow + ",\'"+url+"\')";
                                        $('#chong').removeClass('Ml10');
                                        $('#chong').addClass('Ml10 CzHide');
                                }
                            }else{
                                alert('阅读该篇博文至少需要'+GiftPrice+'朵博文');
                            }}else{
                                    if (flow > Money){
                                zeng.style.display = "none";
                                        $('#chong').removeClass('Ml10 CzHide');
                                        $('#chong').addClass('Ml10');
                                } else{
                                zeng.style.display = "inline-block";
                                        //zeng.href = "javascript:postflow("+articleid+',' + touserid + "," + userid + "," + flow + ",\'"+url+"\')";
                                        $('#chong').removeClass('Ml10');
                                        $('#chong').addClass('Ml10 CzHide');
                                }
                            }
                            } else{
                            alert('赠送数要大于0');
                            }
                            }
