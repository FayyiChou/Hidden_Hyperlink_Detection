$(".searchbutton").click(function(){
   if($("#textfield").val()==""||$("#textfield").val()==null||$("#textfield").val()=="请输入搜索关键词")
   {alert("请输入搜索关键词");return;}
   else{
     myForm.action='http://61.144.227.212/was5/web/search?channelid=275359&searchword='+encodeURI($("#textfield").val());
     myForm.method='POST';
     myForm.submit();
   }
});
function searchfun()
{
   if($("#textfield").val()==""||$("#textfield").val()==null||$("#textfield").val()=="请输入搜索关键词")
   {alert("请输入搜索关键词");return;}
   else{
     myForm.action='http://61.144.227.212/was5/web/search?channelid=275359&searchword='+encodeURI($("#textfield").val());
     myForm.method='POST';
     myForm.submit();
   }
}
</script>

