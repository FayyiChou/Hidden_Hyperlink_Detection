                                        data: { paperId: 346701, modelStr: modelStr, exam: examNum },
                                        type: "POST",
                                        dataType: "json",
                                        success: function(data) {
                                            if (data.S == 10001) {
                                                utils.login({
                                                    initTab: 0, //0:登录界面,1注册界面,默认值为0,
                                                    onSuccess: function () { //登陆成功
