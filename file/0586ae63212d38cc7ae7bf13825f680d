                                                return false;
                                            } else {
                                                if (data.S == 10007) {
                                                    $this.content(data.msg + "---<a href='http://wx.233.com/account/coins/' style='padding-left:30px; color:red'>如何赚取积分</a>");
                                                } else {
                                                    $this.content(data.msg).button(
                                                    {
                                                        name: 'btn_payPaper',
                                                        value: '确定',
                                                        disabled: true
                                                    },
                                                    {
                                                        value: '取消',
                                                        focus: true
                                                    });
                                                }
                                            }
                                        },
                                        error: function(data) {
                                            $this.content(data.msg);
                                            setTimeout(function() {
                                                $this.close().remove();
                                            }, 2000);
                                        }
                                    });
                                    return false;
                                },
                                autofocus: true
                            },
                            {
                                value: '取消'
                            }
                        ]
                    });
                    d.width(440).showModal();

                });
        });

    });


