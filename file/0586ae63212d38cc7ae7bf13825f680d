<script>
    (function () {      
        var html = document.getElementById('timer-output');
        var timeCount = 172800;
        if (html != null) {
            var timer = setInterval(function() {
                if (timeCount <= 0) {
                    clearInterval(timer);
                    html.innerHTML = '已过期';
                }
                timeCount--;
                html.innerHTML = '<i>'+(Math.floor(timeCount/3600))+'</i>时<i>'+(Math.floor((timeCount%3600)/60))+'</i>分<i>'+(Math.floor((timeCount%3600) % 60))+'</i>秒';
            }, 1000);
        }

    })();

    function close_xxxxx(obj) {
        var today = (new Date()).getFullYear() + '/' + ((new Date()).getMonth() + 1) + '/' + (new Date()).getDate();
        document.cookie = "youhuiquan_close=" + escape(today) + ";expires=" + new Date(today+' 23:59:59').toGMTString() + ';domain=233.com;path=/';
        obj.parentNode.style.display = 'none';
    }

