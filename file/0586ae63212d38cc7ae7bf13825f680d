<div class="___wrap" tabindex="0" style="position: fixed; left: 0px; top: 0px; width: 100%; height: 100%; overflow: hidden; -webkit-user-select: none; filter:alpha(opacity=50); opacity: 0.5; z-index:1027; background: rgb(0, 0, 0);display:none"></div>
<!--/删除弹窗-->

<script>
    var artDialogConfig = { cssUri: '//img2.233.com/wx/public/2016/11/css/pub-dialog.css' }
    seajs.config({
        alias: {
            'global.dialog': '//img2.233.com/wx/uc/v2016/public/js/global.dialog.js'
        }
    });
    seajs.use(['jquery', 'utils','template','login','global.dialog'], function ($, utils,template,login,dialog) {
            var paperId = 346701;
            var send = function (options) {
                  var defaultParam = $.extend({
                        type: 'post',
                        dataType: 'json',
                        cache: false
                  }, options);
                  $.ajax(defaultParam);
            };
            var jsonData={pageIndex:1};


            /*template辅助函数 从日期字符串中获得年月日*/
            template.helper('cutDate',function(str){
                  return str.split(' ')[0];
            })

            /*template辅助函数 获得试题难度*/
            template.helper('difficulty',function(index){
                  var arr=['','容易','一般','一般','一般','难']
                  return arr[index];
            })

            /*template辅助函数 从日期字符串中获得评论日期距今的小时数*/
            template.helper('pastTime',function(str){
                  var year = str.split(' ')[0];
                  var hour = str.split(' ')[1].split(':')[0];
                  var min=str.split(' ')[1].split(':')[1]
                  var nowDate= new Date();
                  var nowYear=nowDate.getFullYear();
                  var nowMonth=(nowDate.getMonth()+1)>=10?(nowDate.getMonth()+1):'0'+(nowDate.getMonth()+1);
                  var nowday=nowDate.getDate()>=10?nowDate.getDate():'0'+nowDate.getDate();
                  var nowHour=nowDate.getHours();
                  var nowMin=nowDate.getMinutes();
                  nowYear=nowYear+'-'+nowMonth+'-'+nowday;
                  //如果年月日不相等，则不计算过去时间
                  if(year!=nowYear){
                        return;
                  }else{
                        var nowMin=parseInt(nowHour)*60+parseInt(nowMin);
                        var min=parseInt(hour)*60+parseInt(min);
                        var item=nowMin-min;
                        if(item<60){
                              item=item<0?0:item;
                              return item+'分钟前';
                        }else{
                              return Math.floor(item/60)+'小时前';
                        }
                  }
            })

            /*获取评论列表*/
            var getCommentList=function(jsonData){
                  var noList='<div class="notes-none">暂无试卷评论，赶紧添加解析或评论</div>';
                  send({
                        url:baseAjaxPath+'exam/getReview',
                        data:{
                              paperId:paperId,
                              pageIndex:jsonData.pageIndex
                        },
                        success:function(data){
                              if (data.s == 10006) {
                                    allCount = data.count;
                                    pgIndex = data.pgIndex;
                                    pgSize = data.pgSize;
                                    if (data.list.length > 0) {
                                          var html = template('tpl-comment', {data:data.list});
                                          $('.tk-notes-text').html(html);
                                          $('.reviewedCount').html(allCount);
                                          //分页显示
                                          if (allCount > 0) {
                                                seajs.use('page', function (Page) {
                                                      Page({
                                                            trigger: '#pageBar', //容器
                                                            page: pgIndex, //页码
                                                            pageSize: pgSize, //每页数量
                                                            allCount: allCount, //总记录数
                                                            continuousPage: 7, //连续显示页码，超过后省略号提示
                                                            pagePrevText: '上一页', //上一页文字
                                                            pageNextText: '下一页', //下一页文字
                                                            quickJump: false, //是否显示快速跳转工具
                                                            pageStyle: 'ui-page-center', //分页风格，需配合skin目录下的样式文件
                                                            pageSkin: 'default', //分页皮肤
                                                            onClick: function (_page) {
                                                                  jsonData = $.extend(jsonData, { pageIndex: _page });
                                                                  getCommentList(jsonData);
                                                            }
                                                      })
                                                });
                                          }
                                    }else{
                                          $('.tk-notes-text').html(noList);
                                    }
                              }else{
                                  dialog.fail(data.msg);
                              }
                        },
                        error:function(data){
                            dialog.fail(data.msg);
                        }
                  })
            }

            /*添加页面侦听事件*/
            $(function(){
                  $('body')
                   //删除评论列表
                  .on('click','.del-ico',function(){
                      $('.ui-popup').fadeIn(300);
                      $('.___wrap').fadeIn(300);
                      var delUrl=baseAjaxPath+'exam/DelReview?paperId='+$(this).attr('data-paperId')+'&reviewid='+$(this).attr('data-reviewid'),
                          delAllUrl=baseAjaxPath+'exam/DelAllReview?delUserId='+$(this).attr('data-userid'),
                          isAdmin=$(this).attr('data-isAdmin');
                      if(isAdmin==0){
                          $('._delSC').addClass('del-qx').removeClass('del-jy').html('取消');
                      }else if(isAdmin==1){
                          $('._delSC').removeClass('del-qx').addClass('del-jy').html('删除该用户所有笔记并禁言');
                      }
                      $('.ui-dialog-autofocus').click(function(){
                          send({
                              url:delUrl,
                              type:'GET',
                              success:function(data){
                                  if(data&&data.s===10006){
                                      dialog.success('删除成功');
                                      $('.ui-popup').fadeOut(300);
                                      $('.___wrap').fadeOut(300);
                                      $('.ex-tit em').html(parseInt($('.ex-tit em').html())-1);
                                      setTimeout(function(){
                                          getCommentList(jsonData)
                                      },1000)
                                  }else{
                                      dialog.fail(data.msg);
                                  }
                              },
                              error:function(data){
                                  dialog.fail(data.msg);
                              }
                          })
                      })
                      $('.del-jy').click(function(){
                          send({
                              url:delAllUrl,
                              type:'GET',
                              success:function(data){
                                  if(data&&data.s===10006){
                                      $('.ui-popup').fadeOut(300);
                                      $('.___wrap').fadeOut(300);
                                      dialog.success('删除并禁言成功');
                                      setTimeout(function(){
                                          document.location.reload();
                                      },2000)
                                  }else{
                                      dialog.fail(data.msg);
                                  }
                              },
                              error:function(data){
                                  dialog.fail(data.msg);
                              }
                          })
                      })
                      $('.del-qx,.ui-dialog-close').click(function(){
                          $('.ui-popup').fadeOut(300);
                          $('.___wrap').fadeOut(300);
                      })
                  })
                 //监听评论输入框字数
                  .on('keyup','#commentText',function(){
                      var count=$(this).val().length;
                      if(count>500){
                          $('#limitedText').css('color','red');
                          $('#limitedText').addClass('over');
                      }else{
                          $('#limitedText').css('color','');
                          $('#limitedText').removeClass('over');
                      }
                  })
                  //发表评论
                  .on('click','#commentSubmit',function(){
                      var data={
                          Content:$('#commentText').val(),
                          GroupId:paperId
                      };
                      if($('#commentText').val().length<=0){
                          dialog.tips('发表内容不能为空!')
                      }else if( $('#limitedText').hasClass('over')){
                          dialog.tips('字数超过限制!')
                      }else{
                          //避免重复发送
                          if(!$('#commentText').hasClass('post')){
                              //发表评论内容
                              send({
                                  url:baseAjaxPath+'exam/addReview',
                                  data:data,
                                  success:function(data){
                                      if(data.s===10006){
                                          dialog.success('添加成功!');
                                          $('#commentText').addClass('post');
                                          setTimeout(function(){
                                              $('#commentText').val('');
                                              $('.ex-tit em').html(parseInt($('.ex-tit em').html())+1);
                                              $('.reviewedCount').html(parseInt($('.reviewedCount').html())+1);
                                              getCommentList({pageIndex:1})
                                              $('#commentText').removeClass('post');
                                          },1000);
                                      }
                                          //未登录先登录
                                      else if(data.s===10001&&data.msg=='未登录或已掉线'){
                                          login({
                                              initTab: 0, //0:登录界面,1注册界面,默认值为0,
                                              onSuccess: function () { //登陆成功
                                                  window.location.reload();
                                              }
                                          });
                                      }
                                      else{
                                          dialog.fail(data.msg);
                                          $('#commentText').removeClass('post');
                                      }
                                  },
                                  error:function(data){
                                      dialog.fail(data.msg);
                                      $('#commentText').removeClass('post');
                                  }
                              })
                          }
                      }
                  })
                  //星星点击事件
                  .on('mouseover','.onStar',function(){
                      if(!$('.szhi').hasClass('send')){
                          $('.szhi').attr('data-star','0');
                          $('.onStar').css('background','url(//img2.233.com/wx/uc/v2016/tiku/skin/images/star_icon.png) no-repeat 0 -31px');
                          $(this).css('background','#fff url(//img2.233.com/wx/uc/v2016/tiku/skin/images/star_icon.png) no-repeat 0 -49px');
                          $(this).prevAll().css('background','#fff url(//img2.233.com/wx/uc/v2016/tiku/skin/images/star_icon.png) no-repeat 0 -49px');
                          $('.onStar').bind('mouseout',function(){
                              $('.onStar').css('background','url(//img2.233.com/wx/uc/v2016/tiku/skin/images/star_icon.png) no-repeat 0 -31px');
                          })
                      }
                  })
                        .on('click','.onStar',function(){
                            if(!$('.szhi').hasClass('send')){
                                $(this).css('background','#fff url(//img2.233.com/wx/uc/v2016/tiku/skin/images/star_icon.png) no-repeat 0 -49px');
                                $(this).prevAll().css('background','#fff url(//img2.233.com/wx/uc/v2016/tiku/skin/images/star_icon.png) no-repeat 0 -49px');
                                $(this).nextAll().css('background','url(//img2.233.com/wx/uc/v2016/tiku/skin/images/star_icon.png) no-repeat 0 -31px');
                                $('.onStar').unbind('mouseout');
                                $('.szhi').attr('data-star',$(this).index()+1);
                                //发表试卷评分
                                if($('.szhi').attr('data-star')!=0){
                                    var starData={
                                        paperId:paperId,
                                        paperLevel:$('.szhi').attr('data-star')
                                    };
                                    send({
                                        url:baseAjaxPath+'paper/addLevel',
                                        data:starData,
                                        success:function(data){
                                            if(data.s===10006){
                                                $('.szhi').addClass('send');
                                            }
                                        }
                                    })
                                }
                            }
                        });
                  getCommentList(jsonData);
            })
      })
</script>
    </div>
</div>



