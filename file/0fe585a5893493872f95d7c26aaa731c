<script type="text/javascript">
$.fn.selectRange = function(start, end) {
    return this.each(function() {
        if (this.setSelectionRange) {
            this.focus();
            this.setSelectionRange(start, end);
        } else if (this.createTextRange) {
            var range = this.createTextRange();
            range.collapse(true);
            range.moveEnd('character', end);
            range.moveStart('character', start);
            range.select();
        }
    });
};

function addComment(siteid,aid,typeid,title,url){
        var r_content=$('#r_content').val();
        var len=r_content.length;
        if(len>300){
            alert('最多输入300字');
            return false;
        }
        if(r_content=='欢迎您参加评论' || r_content==''){
            alert('请输入评论内容');
            return false;
        }

       $.ajax({
           type:'post',
           url:'http://www.mfcad.com/plus/comment.php?action=add',
           data:{'siteid':siteid,'aid':aid,'typeid':typeid,'title':title,'url':url,'r_content':r_content},
           //contentType:"application/x-www-form-urlencoded;charset=gbk",
           dataType:'JSON',
           success:function(data){
                data=$.parseJSON(data);
                if(data.status==1){
                    alert('评论成功！正在审核');
                    $("#r_content").val('欢迎您参加评论');
                }else if(data.status==2){
                    $(".loginArea").html('');
                    alert('请登录！');
                    txt='<a href="http://bbs.mfcad.com/member.php?mod=logging&action=login"><font color="#f00">登录</font></a> <span class="break">|</span><a href="http://bbs.mfcad.com/member.php?mod=register" target="_blank">注册</a>';
                    $(".loginArea").html(txt);
                    return false;
                }else{
                    alert('评论失败！');
                }
           }
       }) 
    }
    
    function strlen_verify(obj,max){
        var text=$(obj).val().length;
        if(text>max){
            //alert('最多输入300字');
            $(".attention").html('<font color=#f00>最多输入300字!</font>');
        }else{
            $(".attention").html('');
        }
        $("#maxinput").html(text);
    }
    
    $("#r_content").keyup=function(ext){
        if(ext.keycode==112){
            return ;
        }
    }
    
     
    function more_comment(aid){
        var page = $("#more").attr('rel');
        var current = $("#more").attr('current');
        if(current == 0){
            $("#more").attr('current', 1);
            if(page!=0){
               $("#more").find('span').show();
                $.get('http://www.mfcad.com/plus/comment.php?action=selectall&siteid=1&aid='+aid+'&page='+page,
                    function(list){
                        list=$.parseJSON(list);
                        if(list.status==1){
                            alert('请正常操作！');
                        }else if(list.status==3){
                            $('#more').find('span').hide();
                            $('#more').attr('rel', 0).html('已经全部加载完毕！');
                        }else if(list.status==2){
                            for(i=0;i<list.content.length;i++){
                                var txt='';
                                    txt+='<div id="comment46616" class="com_box"><div class="com_user"><a onclick="" href="javascript:void(0);">';
                                    txt+='<img width="50" height="50" src="http://bbs.mfcad.com/uc_server/avatar.php?uid='+list.content[i].uid+'&size=small"></a>';
                                    txt+='</div><div class="com_bar"><a id="nick_'+list.content[i].uid+'" href="http://my.mfcad.com/'+list.content[i].uid+'" target="_blank">'+list.content[i].username+'</a>&nbsp;</div>';
                                    txt+='<div class="com_con"><div class="com_text"><div id="content_'+list.content[i].id+'">'+list.content[i].content+'</div></div>';
                                    txt+='<div class="com_panel"><div id="reply_'+list.content[i].uid+'" class="com_handle"><a href="javascript:recite(\''+list.content[i].username+'\','+list.content[i].id+')">回复</a>&nbsp;|&nbsp;';
                                    txt+='<a title="这是一个好评论，能解决问题" href="javascript:help('+list.content[i].id+',true)" id="a_post_voteup">支持</a>&nbsp;<em id="post_voteup_'+list.content[i].id+'">('+list.content[i].ishelp+')</em>&nbsp;|&nbsp;';
                                    txt+='<a title="这评论无法解决问题，或者模糊不清" href="javascript:help('+list.content[i].id+',false)" id="a_post_votedown">反对</a>&nbsp;';
                                    txt+='<em id="post_votedown_'+list.content[i].id+'">('+list.content[i].nhelp+')</em>&nbsp;|&nbsp;<a href="javascript:report('+list.content[i].id+')">举报</a>';
                                    txt+='</div><div class="com_info"><span class="com_timestamp">'+list.content[i].senddate+'</span>';
                                    txt+='<span class="com_via">来自<a target="_blank" href="http://www.mfcad.com">沐风网</a></span></div></div></div>';
                                    txt+='</div><div style="clear: both;"></div>';
                                    $(".com_list").append(txt);
                                    $('#more_'+page+'_'+i).fadeIn(600);
                            }
                            $('#more').find('span').hide();
                            $('#more').attr('rel', parseInt(page)+1).attr('current', 0);
                            if(list.content.length==0) $('#more').attr('rel', 0).html('已经全部加载完毕！');
                        }   
                    }
                );
            }
    }
    return false;
}

function help(id,is){
       var url;
       if(getCookie(id)==null){
           setCookie(id,"mfcad.com");
       }else{
           if(getCookie(id)=="mfcad.com"){
               alert("您已经点评过了");
               return ;
           }
       }
       if(is==true){
            url="ishelp=1";
         }else{
            url="nhelp=1";
        }
     $.get("http://www.mfcad.com/plus/comment.php?action=help&id="+id+"&"+url,function(result){
          if(result){
                if(result.n=="ishelp"){
                   var html = "("+result.num+")";
                    $("#post_voteup_"+result.id).html(html);
                  }else{
                     var html = "("+result.num+")";
                     $("#post_votedown_"+result.id).html(html);
                  }
            }
        },'json');            
}
     function report(id){
          $.get("http://www.mfcad.com/plus/comment.php?action=report&id="+id,function(result){
               if(result!==""){
                    alert("举报成功");
                }else{
                    alert("举报失败");
                }
              },'json');    
    }
    
            //跨域通信进行页面刷新
        function mf_reload(){            
            var messenger = Messenger.initInParent(document.getElementById('OpenartDialog'));
            messenger.onmessage = function (data) {
                if(data=='reload'){
                    window.location.reload();
                }
            };
        }  
</script>


<script type="text/javascript">
    //返回顶部
function gotoTop(min_height){
    //预定义返回顶部的html代码，它的css样式默认为不显示
    var gotoTop_html = '<div id="gotoTop">返回顶部</div>';
    //将返回顶部的html代码插入页面上id为page的元素的末尾 
    $("#footer").append(gotoTop_html);
    $("#gotoTop").click(//定义返回顶部点击向上滚动的动画
        function(){$('html,body').animate({scrollTop:0},700);
    }).hover(//为返回顶部增加鼠标进入的反馈效果，用添加删除css类实现
        function(){$(this).addClass("hover");},
        function(){$(this).removeClass("hover");
    });
    //获取页面的最小高度，无传入值则默认为600像素
    min_height ? min_height = min_height : min_height = 600;
    //为窗口的scroll事件绑定处理函数
    $(window).scroll(function(){
        //获取窗口的滚动条的垂直位置
        var s = $(window).scrollTop();
        //当窗口的滚动条的垂直位置大于页面的最小高度时，让返回顶部元素渐现，否则渐隐
        if( s > min_height){
            $("#gotoTop").fadeIn(100);
        }else{
            $("#gotoTop").fadeOut(200);
        };
    });
};
$(document).ready(function(){
    //顶部行业软件
	$("#mf_nav_soft").hover(
		function(){
			$("#mf_top_soft").show();
			$(this).addClass("li01");
			
		},
		function(){
			$("#mf_top_soft").hide();
			$(this).removeClass("li01");
		}
	);
    
    //顶部登录状态
   $.get("/member/ajax_top.php",function(data){
            data=$.parseJSON(data);
            var txt='';
            if(data.uid>0){
                $("#username").html(data.username);
                $("#login").css('display', 'none');
                $("#islogin").css('display','block');
                txt+='<div class="loginArea" id="loginArea">'+data.username+'</div>';
            }else{
                $("#login").css('display', 'block');
                $("#islogin").css('display','none');
                txt+='<div class="loginArea" id="loginArea"><a onclick="openLoginBox()" style="cursor:pointer;">登录</a> <span class="break">|</span><a href="http://bbs.mfcad.com/register" target="_blank">注册</a></div>';
            }
            $("#loadding").hide();
            $("#comments").prepend(txt);
        })
//    //返回顶部代码
    gotoTop();
});
</script>
