                                    txt+='</div><div class="returntop"><a href="#r_content">发表评论</a> &nbsp;&nbsp;&nbsp;&nbsp;<a id="more"    current="0" rel="2" style="cursor:pointer;" onclick="more_comment('+result.aid+');">';
                                    txt+='<span style="display: none;"><img src="/static/images/loading.gif" style="vertical-align:middle;"></span>更多内容</a></div></div>';
                                    $("#loadding").hide();
                                    $("#comments").append(txt);
                                  
                                }    
                            }
                        }); 
                    });
            </script>
