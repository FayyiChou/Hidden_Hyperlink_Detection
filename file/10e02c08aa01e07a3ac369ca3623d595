<SCRIPT language=JavaScript>



function gotourl(obj)



{ 



 var url = obj.options[obj.selectedIndex].value; 



 if (url!="" && url!="#") 



 {        



  window.open(url);



  var o = window.open(url);



  if (o == null)



  {



  alert("请关闭浏览器的弹出窗口过滤功能！");



  } 



  } obj.selectedIndex = 0;



}



  



</SCRIPT>



