<SCRIPT language=javascript src="/publish/cenc/js/common.js"></SCRIPT>



<SCRIPT language=javascript>



function EqTab(labels,defaultClassName,activeClassName){ 



  this.labels = labels; 



  this.defaultClassName = defaultClassName; 



  this.activeClassName = activeClassName; 



  var me = this;  



   



this.init=function(){  



  for(var i=0;i<labels.length;i++){   



 divObj = document.getElementById(labels[i]);   



 divCon = document.getElementById(labels[i]+"_con");   



 divObj.className=me.defaultClassName;   



 divCon.style.display="none";   



 divObj.onclick=function(obj){    



  if(window.event){     



   obj = event.srcElement;    



  }else{     



   obj = obj.target;    



  }    



 if(obj.tagName =='A'){     



   obj=obj.parentElement;    



 }    



 me.activeTab(obj.id);   



    }  



   }  



  this.activeTab(me.labels[0]); 



  } 



  this.activeTab = function(label){  



   if(me.currentLabel == label){   



 return;  



   }  



   document.getElementById(label).style.borderColor= "#4B7DDA";  



   var con = document.getElementById(label+"_con");  



   con.style.display="";  



   if(con.innerHTML==""){   



    con.innerHTML='<img class="map" width="700" src="'+images[label]+'" />';  



   }  



   if(me.currentLabel){   



    document.getElementById(me.currentLabel).style.borderColor = "#e0e0e0"   document.getElementById(me.currentLabel+"_con").style.display="none";  



   }  



   me.currentLabel = label;



  }  



  



  this.init();



 }



 </SCRIPT>



 <SCRIPT type=text/javascript>



 <!--



 // JavaScript Document



 function callMethod(func) { 



  var str = ""; 



  for (var i = 1;i < arguments.length; i ++) {  



   str += convertTo(arguments[i]) + "~"; 



   } 



   sendPost(func,str);



  }



  /*send data to server use post method*/



  function sendPost(func,args) { 



   dtInfog = null; 



   var sendStr = ""; 



   var xmlhttpReq = createXmlHttp(); 



   var callBack = func;  



   



   xmlhttpReq.onreadystatechange = function() {   



    // only if req shows "loaded"     



    if (xmlhttpReq.readyState == 4) { // only if "OK"   



    var ret = xmlhttpReq.responseText;   



    callBack(ret);  



    } 



   }; 



   xmlhttpReq.open("POST", "Transfer.data",true); xmlhttpReq.setRequestHeader("Content-Type","application/x-www-form-urlencoded"); 



   var len = args.length; 



   for (var i=0; i < len; i ++) {  



    sendStr += "#" + args.charCodeAt(i); 



   }    



      xmlhttpReq.send(sendStr);



   }



   function convertBack(str) { 



    //str = str.str = str.replace(/&emp/g,"&emp"); 



    str = str.replace(/#t/g,"~"); 



    str = str.replace(/#a/g,"&"); 



    str = str.replace(/#e/g,"="); 



    str = str.replace(/#1/g,"#"); 



    return str;



    }



    function convertTo(str) { 



     str = str.replace(/#/g,"#1"); 



     str = str.replace(/&/g,"#a"); 



     str = str.replace(/~/g,"#t"); 



     str = str.replace(/=/g,"#e"); 



     return str;



    }



    function createXmlHttp() {  



     var xmlHttp = null;  



     try  



     {   



      xmlHttp = new ActiveXObject("Msxml2.XMLHTTP");  



      } catch(e) {   



       try   



       {    



        xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");  



       } catch(oc) {    



        xmlHttp = null;  



       }  



      }  



   if ( !xmlHttp && typeof XMLHttpRequest != "undefined" )  



   {   



    xmlHttp = new XMLHttpRequest(); 



    } 



    return xmlHttp;



    }



    //-->



    </SCRIPT>



