				<div class="click_next_front"></div>
				<script type="text/javascript">

					(function($) { 
						$('.click_next_front').addClass('top');
						function jump_next () {
							if( $('.click_next_front').hasClass('top') ){
								$('.click_next_front').removeClass('top');
							} else {
								$('.click_next_front').addClass('top');
							}
						};

						$('.click_next_front').hover(function(){
							clearInterval(jump_i);
						},function(){
							jump_i = setInterval(jump_next,700);
						});

						var jump_i = setInterval(jump_next,700);
					})(jQuery);
					
				</script>
				
