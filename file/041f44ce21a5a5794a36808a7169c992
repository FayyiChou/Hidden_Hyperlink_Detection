		function loginAction(){

		var df = document.loginForm;

		var userName=df.userName.value;

		var password=df.password.value;

		var domain=df.domainName.value;

if(userName ==""){

		alert("用户名不能为空");

		document.getElementById('userName').focus(); 

		return false;}

if(password==""){

		alert("密码不能为空");

		document.getElementById('password').focus(); 

		return false;}

if(domain && domain!="" && domain!="null"){

		df.user.value = userName+"@"+domain; 

}else{df.user.value = userName;}

		df.user.value=Validator.Trim(df.user.value);

if( !Validator.Email.test(df.user.value)){

		info("请输入合法的帐户！","Please input valid account!","請輸入合法的帳戶！");

		return false;}else if(Validator.IsNull(password)){

		info("请输入邮箱密码！","Please input  password!","請輸入郵箱密碼！");

		return false;}else{return true;}}
