<script src="http://bbs.xmfish.com/login_iframe.php?jumpurl=http://info.xmfish.com/chanel/market/edit.html?id"></script><form autocomplete="off" accept-charset="utf-8" onsubmit="document.charset='utf-8'; return false" id="loginForm" name="login" method="post" action="http://www.xiaoyu.com/login">
<ul>
<script type="text/javascript">
jQuery(function(jQuery){
    var url = 'http://info.xmfish.com/chanel/market/edit.html?id';
    var href = window.location.href;
    var index1 = href.indexOf("?jumpurl=");
    var index2 = href.indexOf("jumpurl=");
    if(index1 > 0) {
        href = href.substring(0, index1);
    }  else if(index2 > 0) {
        href = href.substring(0, index2);
    }
    if(url == '' || url == null) {
        var jumpurl = document.getElementById('jumpurl');
        jumpurl.value = href;
    }
    var loginurl = document.getElementById('loginurl');
    loginurl.value =href;
    jQuery(".login_li_input input").each(function(){

		if(jQuery(this).val()){
			jQuery(this).siblings("span").hide();
		}else{
			jQuery(this).siblings("span").click(function(){
				jQuery(this).hide().siblings("input").focus();
			});
		}
		jQuery(this).focus(function(){
			jQuery(this).siblings("span").hide();
		});
		jQuery(this).blur(function(){
			var txt = jQuery(this).val();
			if(txt){
				jQuery(this).siblings("span").hide();
			}else{
				jQuery(this).siblings("span").show();
			}
		});
	});

	/*按钮滑过*/
	jQuery('.login_btn').hover(function(){
	    jQuery(this).addClass('login_btn_hover');
	},function(){
	    jQuery(this).removeClass('login_btn_hover');
	});
	/*文本框判断为空*/
	function loginError(textBar,msg){
		jQuery(textBar).parent('li').prepend('<div class="clear login_error"><div class="clear login_error_div"><b class="fl login_error_b">'+msg+'</b><em class="fr login_error_em">×</em></div><i class="login_error_i1"></i><i class="login_error_i2"></i></div>');
		jQuery('.login_error').css('display','block');
		jQuery('.login_error_em').hover(function(){
		    jQuery(this).addClass('login_error_em_hover');
		},function(){
		    jQuery(this).removeClass('login_error_em_hover');
		});
		jQuery(textBar).focus();
		jQuery('.login_error_em').click(function(){
		    jQuery(this).parents('.login_error').remove();
		});
		jQuery(textBar).siblings('span').hide();
		jQuery(textBar).keyup(function(){
		    jQuery(textBar).siblings('.login_error').remove();
		});
		return false;
	}
	jQuery('.login_btn').click(function(){
		if(jQuery('.login_user').val()==""){
		 	return loginError('.login_user','请输入用户名');
		}else if(jQuery('.login_pass').val()==""){
			return loginError('.login_pass','请输入密码');
		}
        jQuery('#loginForm').submit();
	});

	jQuery('.login_notice_em').hover(function(){
		jQuery(this).addClass('login_notice_em_hover');
	},function(){
		jQuery(this).removeClass('login_notice_em_hover');
	});
	jQuery('.login_notice_em').click(function(){
		jQuery(this).parents('.login_notice').hide();
	});
    jQuery(".login_li_input input").each(function(){

        if(jQuery(this).val()){
            jQuery(this).siblings("span").hide();
        }else{
            jQuery(this).siblings("span").click(function(){
                jQuery(this).hide().siblings("input").focus();
            });
        }
        jQuery(this).focus(function(){
            jQuery(this).siblings("span").hide();
        });
        jQuery(this).blur(function(){
            var txt = jQuery(this).val();
            if(txt){
                jQuery(this).siblings("span").hide();
            }else{
                jQuery(this).siblings("span").show();
            }
        });
    });

});
</script>
