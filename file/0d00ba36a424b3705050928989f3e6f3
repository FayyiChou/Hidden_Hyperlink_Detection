                $(".news-pic").hover(function(){
                    $(this).find(".prev,.next").fadeTo("show",0.8);
                },function(){
                    $(this).find(".prev,.next").hide();
                })
                /*鼠标移过某个按钮 高亮显示*/
                $(".prev,.next").hover(function(){
                    $(this).fadeTo("show",1);
                },function(){
                    $(this).fadeTo("show",0.8);
                })
