          <script language="javascript">
 function xm_search(){
   var form=document.getElementById('xm_form');
   var keyword=document.getElementById('xm_keyword').value;
   var xm_title='';
   var xm_content='';
   var author='';
   var xm_title=keyword;
   var xm_content=keyword;
   if(keyword==''||keyword=='招商项目查询'){
      alert('请输入项目名称');
      return;
   }
   
   form.action=encodeURI(encodeURI('http://www.jxgzw.gov.cn:9001/infogate/qy_query.jsp?title='+xm_title+'&content='+xm_content+'&channelid='+619));
   form.submit();
 }
</script>
