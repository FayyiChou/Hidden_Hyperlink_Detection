<script language="javascript">
 function docsearch(){
   var form=document.getElementById('sform');
   var keyword=document.getElementById('keyword').value;
   var type=0;
   var title='';
   var content='';
   var author='';
   if(type==0){
      title=keyword;
   }else if(type==1){
      content=keyword;
   }else{
      author=keyword;
   }
   form.action=encodeURI(encodeURI('http://220.177.254.66:9001/infogate/query.jsp?title='+title+'&content='+content+'&author='+author));
   form.submit();
 }
</script>

