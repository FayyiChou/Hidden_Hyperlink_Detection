            <input id="qy_keyword" name="qy_keyword" type="text" class="txt dis_in_b float_left" value="企业名录查询" onfocus="javascript:if(this.value=='企业名录查询'){this.value='';this.style.color='#444'}" onblur="javascript:if(this.value==''){this.value='企业名录查询';this.style.color='#656565'}" style="color:#656565;" />
            <input type="button" class="btn dis_in_b float_right" value="查询" onClick="qy_csearch()" />
          </form>
          <script language="javascript">
 function qy_csearch(){
   var form=document.getElementById('qy_form');
   var keyword=document.getElementById('qy_keyword').value;
   if(keyword==''||keyword=='企业名录查询'){
      alert('请输入企业名称');
      return;
   }
   var qy_title=keyword;
   var qy_content=keyword;
   form.action=encodeURI(encodeURI('http://www.jxgzw.gov.cn:9001/infogate/qy_query.jsp?title='+qy_title+'&content='+qy_content+'&channelid='+624));
   form.submit();
 }
</script>
          <p class="clear"></p>
