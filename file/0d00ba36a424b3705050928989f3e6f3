jQuery('#shenla dt').click(function (){
jQuery('#shenla dt').each(function (i,items){
jQuery('#shenla dd:eq('+i+')').hide(100);
jQuery('#shenla dt:eq('+i+')').attr('style','line-height:30px; font-size:16px; background:url(images/1376779.gif) right no-repeat');


jQuery('#shenla dt:eq('+i+')').attr('class','bbtgray');


});
var number=jQuery('#shenla dt').index(this);
jQuery('#shenla dd:eq('+number+')').show(100);
jQuery('#shenla dt:eq('+number+')').attr('style','background:none; line-height:30px; font-size:16px; ');
});
</script>
