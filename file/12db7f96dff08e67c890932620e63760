		for(i=1; i <=n; i++){
		if (i==k){document.getElementById(tag+i).className="sb1";
		document.getElementById(content+i).className="showbox";}
		else{
		document.getElementById(tag+i).className="sb2";
		document.getElementById(content+i).className="Hidebox";}
		}
		}
