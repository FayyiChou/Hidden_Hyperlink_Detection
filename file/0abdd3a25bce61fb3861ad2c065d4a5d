   <script type="text/javascript">
        function getUrlArgObject(url) {
            var args = new Object();
            if (url.indexOf("?") >= 0) {
                var query = url.split("?")[1];
                var pairs = query.split(",");//在逗号处断开 
                for (var i = 0; i < pairs.length; i++) {
                    var pos = pairs[i].indexOf('=');//查找name=value 
                    if (pos == -1) 
                    var argname = pairs[i].substring(0, pos);//提取name 
                    var value = pairs[i].substring(pos + 1);//提取value 
                    args[argname] = unescape(value);//存为属性 
                }
                return args;//返回对象 
            } else {
                return;
            }
        }
        $(function() {
            //Start  设置语言切换URl的回调地址参数(redirectUrl)为当前页面URL
            //1. 获得当前页面相对URL 
            var url = document.location.toString();
            var arrUrl = url.split("//");
            var start = arrUrl[1].indexOf("/");
            var relUrl = arrUrl[1].substring(start);

            //2. 获取所有语言切换超链接DOM元素
            $(".selLangId").each(
                    function() {
                        //3. 循环获取DOM元素href值
                        var myHref = $(this).attr('href');
                        //4. 判断是否已存在参数redirectUrl  存在，不做处理：不存在，在后面拼接redirectUrl=[当前页URL]
                        var args = getUrlArgObject(myHref);
                        var redirectUrlExist = 0;
                        for (var arg in args) {
                            if (arg['redirectUrl'] && arg['redirectUrl'].length > 0) {
                                //  arg['redirectUrl']
                                redirectUrlExist = 1;
                                break;
                            }
                        }

                        if (redirectUrlExist == 0) {
                            //5. 不存在参数redirectUrl时设置DOM元素href值为修改后的值
                            if (myHref.indexOf('?') < 0) {
                                myHref = myHref + '?';
                            }
                            myHref = myHref + '&redirectUrl=' + encodeURIComponent(relUrl);
                            $(this).attr('href', myHref);
                        }

                    });
            //End   设置语言切换URl的回调地址参数(redirectUrl)为当前页面URL
        });

        var _bdhmProtocol = (("https:" == document.location.protocol) ? " https://" : " http://");
        document.write(unescape("%3Cscript src='" + _bdhmProtocol + "hm.baidu.com/h.js%3F659ff3bedbb1e74e9f0dd53bf7da30fb' type='text/javascript'%3E%3C/script%3E"));
        var cnzz_protocol = (("https:" == document.location.protocol) ? " https://" : " http://");
        document.write(unescape("%3Cspan id='cnzz_stat_icon_1000063258'%3E%3C/span%3E%3Cscript src='" + cnzz_protocol + "s22.cnzz.com/z_stat.php%3Fid%3D1000063258%26show%3Dpic' type='text/javascript'%3E%3C/script%3E"));
    </script> 
