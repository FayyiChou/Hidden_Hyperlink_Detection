    <script type="text/javascript">
        try {
            if (/Android|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent) && GetRequest('isMobile', '0') == '0') {
                window.location.href = "mobile_home/main.html?clientID=12";
            }
        } catch (e) { }

        //获取url中一个指定的参数值
        function GetRequest(paramName, defaultValue) {
            var search = paramName + "=";
            var FieldValue = "";
            var URL = location.href;
            var offset = URL.indexOf(search);
            if (offset != -1) {
                offset += search.length;
                var end = URL.indexOf("&", offset);
                if (end == -1) {
                    FieldValue = URL.substring(offset);
                }
                else {
                    FieldValue = URL.substring(offset, end);
                }
            }
            if (FieldValue == "") {
                FieldValue = defaultValue;
            }
            return FieldValue.toLowerCase();
        }

        //切换新闻
        function ToggleNews() {
            $("#ulNews").show();
            $("#ulNotice").hide();
            $("#aNotice").removeClass("active");
            $("#aNews").addClass("active");
        }

        //切换公告
        function ToggleNotice() {
            $("#ulNotice").show();
            $("#ulNews").hide();
            $("#aNews").removeClass("active");
            $("#aNotice").addClass("active");
        }

        //切换广告图
        $(function () {
            //图片广告
            var nav = '';
            $('#banner').find('a').each(function (i) {
                var item = $(this),
                    id = 'banner_item_' + i;

                item.attr('id', id);
                nav += '<li class="ui-switchable-trigger'
                    + (i === 0 ? ' ui-switchable-active' : '')
                    + '" data-rel="' + id + '"></li>';
            });


            nav = $('#banner-nav').html(nav).find('li');

            nav.powerSwitch({
                animation: "translate",
                classAdd: 'ui-switchable-active',
                container: $('#banner-control'),
                classPrev: 'ui-switchable-upper pngTransparent',
                classNext: 'ui-switchable-next pngTransparent'
            });
        });
    </script>
