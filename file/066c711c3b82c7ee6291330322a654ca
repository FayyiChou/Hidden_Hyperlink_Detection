<script type="text/javascript">
    function GetHeadUserInfo() {
        $.ajax({
            contentType: "application/json",
            url: "/WS/WSAccount.asmx/GetHeadUserInfo",
            type: "POST",
            dataType: 'json',
            success: function(result) {
                result = eval("(" + result.d + ")");
                if (result.data.valid) {
                    $("#headLogon").html(result.data.html);
                }
            }
        });
    }

    $(function() {
        GetHeadUserInfo();

        var news = $("#head-news");

        function slider(time){
            var item = news.children();

            if(item.length < 2) return;

            var target = item.eq(0);
            target.animate({
                "margin-top": -target.outerHeight(true)
            }, 300, function(){
                news.append(target);
                target.css("margin-top", 0);
                setTimeout(function(){
                    slider(time);
                }, time);
            });
        }

        setTimeout(function(){
            slider(5000);
        }, 5000);
    });
</script>


