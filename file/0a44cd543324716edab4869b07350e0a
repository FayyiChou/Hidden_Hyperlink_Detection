    {{ for(var i = 0,len = it.result.length; i < len; i++){}}
    <li class="groupListItem">
        <div class="view">
          {{if(typeof(it.result[i].Belong)== 'undefined'){}}
            <input class="toggle" type="checkbox" id="{{=it.result[i].GroupId}}"  />
            {{} else if(it.result[i].Belong==true){}}
            <input class="toggle" type="checkbox" id="{{=it.result[i].GroupId}}" checked="true"  />
            {{}}}
            {{if(it.result[i].GroupId==0){it.result[i].GroupName='未分组'}}}
            <label for="{{=it.result[i].GroupId}}">{{=it.result[i].GroupName}}</label>
           
        </div>
    </li>
    {{ }}}

</script>
