var ua = navigator.userAgent.toLowerCase();
if(/msie 6.0/.test(ua)){
jq(window).scroll(function(){
var scrollTop = jq(window).scrollTop();
jq(".go-top").css({"top":(scrollTop+230)+"px"})
})
}

jq('nav.menu strong,.go-down-btn').click(function () {
var t = jq('#nbbs').offset().top;
jq('html,body').animate({scrollTop:t}, 500);
});
jq('.go-top-btn').click(function(){
jq('html,body').animate({scrollTop: 0},500);
});
jq(window).bind('scroll resize', function () {
var w = jq(window);
if (w.scrollTop() >= w.height()) {
jq('a.go-top-btn').css('display', 'block');
jq('a.go-down-btn').hide();
}
else {
jq('a.go-down-btn').css('display', 'block');
jq('a.go-top-btn').hide();
}
})
//关闭
jq('.clsodA').click(function(){
jq('.bbs-act-top').remove();
});


