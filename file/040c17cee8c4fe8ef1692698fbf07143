<script type="text/javascript">
    /*处理input type=number时自动加入逗号的兼容性问题*/
    var numberObj = $('input[type=number]');
    $.each(numberObj, function (index, item) {
        var value = $(item).val();
        if (value.indexOf(',') > -1) {
            $(item).val(value.replace(/,/g, ''));
        }
    });
</script>


