    <script type="text/javascript">
        /*首页Banner图片切换*/
        var bannerpicList = $('.newBanner .banner .imgField .control span');
        var bannerpicDetail = $('.newBanner .banner .imgField .list ul li ');
        var bannerpicCanauto = true;
        var bannerShownum = 0;

        bannerpicList.each(function (i) {
            this.num = i;
            $(this).click(function () {
                bannerpicCanauto = false;
                if (bannerShownum != this.num) {
                    bannerpicChange(this.num);
                }
            });
			
			$(this).hover(function () {
                bannerpicCanauto = false;
            }, function () {
                bannerpicCanauto = true;
            });
			
			
        });

        function bannerpicChange(num) {
            bannerpicList.removeClass('current');
            bannerpicList.eq(num).addClass('current');
			
            bannerpicDetail.eq(bannerShownum).css('z-index','0').stop().fadeOut(500)
            bannerpicDetail.eq(num).css('z-index','1').stop().fadeIn(500);
            bannerShownum = num;
        }

        var bannerpicAuto = setInterval(function () {
            if (bannerpicCanauto) {
                var tempNum = (bannerShownum < bannerpicDetail.length - 1) ? bannerShownum + 1 : 0;
                bannerpicChange(tempNum);
            }
        }, 4000);



    </script>


