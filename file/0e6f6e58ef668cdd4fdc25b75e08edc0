			  $(this).find("dd").show();
		 });
		 //  天猫切换
		 $(function(){
			     $(".Animate dl:gt(0)").hide();
			     var Num=$(".Animate dl").length;
				 var i;
				 for(i=1;i<=Num;i++){
					  $(".Animate p").append("<span></span>");
					 }
				 $("p span:first").addClass("current");
				 $("p span").mouseover(function(){
					   $(this).addClass("current").siblings().removeClass("current");
					   var index=$(this).index();
					   $(".Animate dl").eq(index).show().siblings("dl").hide();
					 });
			 }) 
	  });
	  
	 
           // 鼠标经过二维码显示部分
