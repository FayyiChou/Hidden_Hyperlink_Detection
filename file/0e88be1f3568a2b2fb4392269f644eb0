<script>
    $(function() {
        $('.remove-cart-item').click(function (event) {
            var li = $(this).closest('li');
            var id = $(this).data('id');

            $.post('http://www.stanandco.com/zh-CN/cart/' + id, {
                _method: 'DELETE',
                _token: 'Un32DapNnw4Rpa80z6TSfmUS3m2yszqltwUAPZdD'
            }, function(data) {
                li.remove();
                $('#cart-item-tr' + id).remove();
                $('.cart-total-quantity').html(data.quantity);
                $('.cart-total-amount').html(data.amount);
                $('.cart-total-final-amount').html('￥' + data.amount);
            });
        });
    });

