    <script type="text/javascript">
        $(function () {
            $('#QRCode > #QRcodePlace > canvas').remove();
            $('#QRCode > #QRcodePlace').qrcode({
                'render': 'canvas',
                "size": 128,
                "color": "#000",
                "text": 'http://weixin.qq.com/r/ljiguMHEF5xorYXb922E',
                'fill': '#000',
                'background': "#FFF",
            });

            $('a#weixin-button').on('click', function (event) {
//              if (!/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                event.preventDefault();
                event.stopPropagation();
                var QRCodeDIV = $('#QRCode');
                QRCodeDIV.show();
//              }
            });
        });


        $.fn.showPopWindow = function (windowID, align, topShift, leftShift) {
            var target = $(this);
            align = align || 'right';
            topShift = topShift || 0;
            leftShift = leftShift || 0;
                                var windowWidth = Math.round($(window).width());
            var windowHeight = Math.round($(window).height());
            var windowTop = Math.round($(window).scrollTop());
            var windowLeft = Math.round($(window).scrollLeft());
            var windowRight = windowWidth + windowLeft;
            var windowBottom = windowTop + windowHeight;

                                var buttonWidth = Math.round(target.outerWidth());
            var buttonHeight = Math.round(target.outerHeight());
            var buttonTop = Math.round(target.offset().top);
            var buttonLeft = Math.round(target.offset().left);
            var buttonBottom = buttonTop + buttonHeight;

                                var popwindowWidth = Math.round($(windowID).outerWidth());
            var popwindowHeight = Math.round($(windowID).outerHeight());

                                onScreenTop = buttonTop - windowTop + topShift;
            onScreenLeft = buttonLeft - windowLeft;

            var popTopPosition = 0;
            var popLeftPosition = 0;
                        if ((windowBottom - buttonBottom) < popwindowHeight) {
                popTopPosition = onScreenTop - popwindowHeight + buttonHeight;
            } else {
                popTopPosition = onScreenTop;
            }

                                popTopPosition += topShift;

                                var alignShift = 0;
            switch (align) {
                case 'left':
                                        if (onScreenLeft < popwindowWidth) {
                        popLeftPosition = onScreenLeft + buttonWidth;
                    } else {
                        popLeftPosition = onScreenLeft - popwindowWidth;
                    }
                    break;

                case 'right':
                                        if ((windowRight - (buttonLeft + buttonWidth)) < popwindowWidth) {
                        popLeftPosition = onScreenLeft - popwindowWidth;
                    } else {
                        popLeftPosition = onScreenLeft + popLeftPosition + buttonWidth;
                    }
                    break;
            }
                                popLeftPosition += leftShift;

            $(windowID).css({
                top: popTopPosition,
                left: popLeftPosition,
                position: 'fixed'
            });
            $(windowID).fadeIn('slow');
        }

                $('button#QRclose').on('click', function (event) {
            event.preventDefault();
//            $('#QRCode').fadeOut('slow');
            $('#QRCode').hide();
        });
    </script>
