	$('.hover1,.rides-cs,.cn').each(function() {
            PIE.attach(this);
        });
    }

    $(".nav>ul>li").mouseenter(function () {
        $(".nav ul li").removeClass("hover1");
        $(this).addClass("hover1");
        $(this).find(".downMenu").stop(true, true).slideDown(300);

        if (window.PIE) {
            $('.hover1').each(function () {
                PIE.attach(this);
            });
        }
    });
});
</script>
