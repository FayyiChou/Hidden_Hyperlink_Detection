function searchsubmit()
{
var fieldnameStr = "";   
var keywordStr = "";   
var filter = ""; 
var Keyword = document.getElementById("Keyword").value;    
var Chid = 2;
try{
Chid = "1";
}catch(err){}
	if(Keyword.length!=0) 
	{
	  fieldnameStr+=filter+"title";  
	  keywordStr+=filter+escape(Keyword);   
	  filter="$" ;   
	}
location.href("/search.aspx?chid="+Chid+"&fieldname="+fieldnameStr+"&keyword="+keywordStr);   
}
</script>
  

