    $(function () {
        if (jQuery.cookie("the_cookie") != "the_value") {
            $(".mengban").show();
            $(".mengban").click(function () {
                $(".mengban").hide();
                jQuery.cookie('the_cookie', 'the_value', { expires: 1, path: '/' });
            })
        }
    })
    $(window).scroll(function () {
        if ($(window).scrollTop() > 0) {
            $(".mengban").hide();
        }
    });
</script>
