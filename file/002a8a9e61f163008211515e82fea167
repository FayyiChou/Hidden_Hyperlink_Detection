    <script type="text/javascript">
    $(function(){
        $('.switch_list').each(function(){
            var li_len = $(this).find('li').length;
            var _btn = '<div class="switch_open">展开<span class="font_i">r</span></div><div class="switch_close">收起<span class="font_i">r</span></div>'
            if (li_len > 1) {
                $(this).after(_btn);
            };
            $('.switch_open').click(function(){
                $(this).prev().find('li').slideDown();
                $(this).hide();
                $(this).next().show();
            });
            $('.switch_close').click(function(){
                var path = $(this).parent().find('li');
                path.not($(path.get(0)).show()).slideUp();
                $(this).hide();
                $(this).prev().show();
            });
        });


        var nav_h = $('.nav_outer').height();
        $('.nav_outer').css('height',nav_h);
        var _spacer = $('.prompt').outerHeight();

        var a_len = $('.abox').length;
        for (var i = 0; i < a_len; i++) {
            $($('.abox').get(i)).attr('index',i);
        };

        function fit(){
            var top = document.documentElement.scrollTop || document.body.scrollTop;
            var nav_top = $('.nav_outer').offset().top;
            if(top > nav_top){
                $('.nav_outer ul').css({'position':'fixed','top':'0','left':'50%','margin-left':'-50%'});
            }else{
                $('.nav_outer ul').css({'position':'relative','top':'0','left':'0','margin-left':'0'});
            };

            $('.abox').each(function () {
                var _top = $(this).offset().top - nav_h;
                var _height = $(this).height();
                if ((_top - nav_h*2 - top) <= 0 && (_top + _height) > top) {
                    var index = parseFloat($(this).attr('index'));
                    var _left = 1/4 * index * 100 +'%';
                    $('.nav_outer .bottom_line').stop().animate({'left':_left},300);
                    $('.nav_outer li').not($($('.nav_outer li').get(index)).addClass('on')).removeClass('on');
                };
            });
        };
        fit();

        $(window).scroll(function(){
           fit();
        });


        $('.nav_outer li').on('click',function(){
            $('.nav_outer li').not($(this).addClass('on')).removeClass('on');
            var top = document.documentElement.scrollTop || document.body.scrollTop;
            var index = $(this).index();
            var _dtop = $($('.abox').get(index)).offset().top - (nav_h + _spacer);
            var _left = 1/4 * index * 100 +'%';
            $('.nav_outer .bottom_line').animate({'left':_left},300);
            if (top > _dtop) {
                setInterval(function(){
                    if (top > _dtop) {
                        top -= 40;
                        if((top - _dtop) <=40 ){
                            $(window).scrollTop(_dtop);
                        }else{
                            $(window).scrollTop(top);
                        }
                    };
                },1);
            };
            if(top <= _dtop){
                setInterval(function(){
                    if (top < _dtop) {
                        top += 40;
                        if((_dtop-top) <=40 ){
                            $(window).scrollTop(_dtop);

                        }else{
                            $(window).scrollTop(top);
                        }
                    };
                },1);
            }
        });


    });
    </script>
