<script>
$(".yq").click(function(){
 $(this).children("div").toggle();
});
$("#xianshi").click(function(){
 var aa = parseInt($("#xianshi").attr("xy"));
 if(aa == 1){
  $(".kefu").css("right","-113px");
  $("#xianshi").attr("xy",2)
 }
 if(aa == 2){
  $(".kefu").css("right","0");
  $("#xianshi").attr("xy",1)
 }
});
$("#wx").hover(function(){
 $("#weixin").toggle();
});
$("#wb").hover(function(){
 $("#weibo").toggle();
});
</script>
