    <script type="text/javascript">
        function updateCartSummary(data){
            sessionStorage.setItem('cart', JSON.stringify(data));
            var el = '';
            if (data && data.quantity > 0) {
                el = [
                    '<i>' + data.quantity + '</i>',
                    '<div>',
                        '<div><a href="/cart">Корзина</a>: ' +  data.amount.toString().replace(/(\d)(?=(\d\d\d)+(?!\d))/g, "$1 ") + '<i class="icon-ru13px"></i></div>',
                    '</div>',
                    '<a href="/cart" class="order"><i></i>Оформить заказ</a>'
                ].join('');
            }
            else {
                el = [
                    '<i>' + data.quantity + '</i>',
                    '<div><div>Корзина пуста</div></div>'
                ].join('');
            }
            document.getElementById('basket-preview').innerHTML = el;
        }
        sharedCartConsumer.summaryCallback = updateCartSummary;
        window.addEventListener('load', function(){
            sharedCartConsumer.updateSummary(sharedCartConsumer.summaryCallback);
        });
    </script>
