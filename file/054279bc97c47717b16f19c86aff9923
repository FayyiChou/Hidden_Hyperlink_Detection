<script type="text/javascript">
var dwall= document.getElementById('dw_all');
var xza= sessionStorage;
var zxwflag = xza.getItem("zxwflag");
var oClose2 = document.getElementById('close2');
 
if(zxwflag){
	if(zxwflag==0){
	   dwall.style.display='block';
           dwall.style.zIndex='99999';
        }else{
	   dwall.style.display='none';
       }
}else{
           xza.setItem("zxwflag","0");
     zxwflag = xza.getItem("zxwflag"); 
	   dwall.style.display='block';
           dwall.style.zIndex='99999';	
}

if(oClose2){
  oClose2.onclick = function(){ 
    if(zxwflag==0){
       xza.removeItem("zxwflag","0");
       xza.setItem("zxwflag","1"); 
       zxwflag = xza.getItem("zxwflag");
    } 
   dwall.style.border='none'; 
   dwall.parentNode.removeChild(dwall); 
  }
}
</script>
