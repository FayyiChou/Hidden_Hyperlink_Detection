	           Father.innerHTML="";
	           Father.innerHTML=initial_video;
	           var m_video = document.getElementById('video1');	
                   m_video.src=video_url;
               }
                   if(video_url){
	                if(isD==1)m_video.play();
                    }else{
	              alert("此视频暂时无法播放，请观看和视频其它精彩视频！"); 
                    }
                   flag = true;
                   if(true){
                        if(false){
                           var sctck2 = document.getElementById('sctck');	
                           if(true){
                                 sctck2.innerHTML="当前试播时间仅限2分钟，如需观看全片请付费点播";
                                }else{
                                 sctck2.innerHTML="当前试播时间仅限2分钟，如需观看全片请开通V+会员";
                                }	
	                   sctck2.style.display="block";                 
                           fade2=sctck2.offsetWidth;
                           sctck2.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                           setTimeout(function(){sctck2.style.display="none";},3000);
                          }  	
                    }
                    var m_video = document.getElementById('video1');
                    var player=document.getElementById("player");
                    if(player){
                         player.style.display="block";
                         st_btn.style.display="block";
                         pl_btn.style.display="none";
                         setTimeout(function(){player.style.display="none";contname.style.display="none";},5000);	
                      }

                     if(m_video==null){
                         return;
                     }
                     m_video.addEventListener("timeupdate",testplay,false);
                     m_video.addEventListener("ended",playEnd,false);	
                     isPlay = true;
      
}
function startVieo(pSour){

       m_video = document.getElementById('video1');	
  
    m_video.src=pSour;
    if(pSour){
	m_video.play();
    }else{
	alert("此视频暂时无法播放，请观看和视频其它精彩视频！"); 
    }    
    flag = true;
    m_video.addEventListener("timeupdate",testplay,false);
    m_video.addEventListener("ended",playEnd,false);
}


function re_startVideo(){

      var m_video = document.getElementById('video1');	
  
    startVieo(video_url);
}
//播放器点击事件
if(m_video){
m_video.onclick=function(){
          if(player){
              if(contname.style.display=='none'){
                    contname.style.display="block";
                }else{
                    contname.style.display="none";	
                }
              if(player.style.display=='block'){
                    player.style.display='none';
              }else{
	            player.style.display="block";
                    st_btn.style.display="block";
                    pl_btn.style.display="none";
                     if(isqp=='1'){
                         yy_btn.style.display="block";
                     }else{
                         yy_btn.style.display="none";
                     }
               }
         }
    }
}
}


function onclickplay(){
          if(player){
              if(contname.style.display=='none'){
                    contname.style.display="block";
                }else{
                    contname.style.display="none";	
                }
              if(player.style.display=='block'){
                    player.style.display='none';
              }else{
	            player.style.display="block";
                    st_btn.style.display="block";
                    pl_btn.style.display="none";
                     if(isqp=='1'){
                         yy_btn.style.display="block";
                     }else{
                         yy_btn.style.display="none";
                     }
               }
         }
    
}
if(progress){
     progress.onclick=function(ev){
       if(!ev){ev = window.event;}
       updatebar(ev.pageX);
         };	
      }

var updatebar = function(x) {
   var progress = document.getElementById('progressBar');

           var m_video = document.getElementById('video1');

   var maxduration ='3881';
   var position = x / progress.offsetWidth; 
   var percentage = 80 * position;

   //Check within range
   if(percentage > 80) {
      percentage = 80;
   }
   if(percentage < 0) {
      percentage = 0;
   }
   course01.style.left=percentage+'%';
   course04.style.width=percentage+'%';
   m_video.currentTime = maxduration * parseInt(percentage) / 80;
};

//点击暂停
if(st_btn){
st_btn.onclick=function(){

           var m_video = document.getElementById('video1');
         if(m_video.paused){
           m_video.play();
           st_btn.style.display="block";
           pl_btn.style.display="none";
           }else{
	     m_video.pause();
             st_btn.style.display="none";
              pl_btn.style.display="block";}
          };
}
//点击播放
if(pl_btn){
pl_btn.onclick=function(){

           var m_video = document.getElementById('video1');

  if(m_video.paused){
           m_video.play();
           pl_btn.style.display="none";
           st_btn.style.display="block";
           }else{
	  m_video.pause();
          st_btn.style.display="block";
          pl_btn.style.display="none";} 
       };
  }



//点击全屏

if(qp_btn){
    qp_btn.onclick=function(){
    launchFullscreen(); 
    qp_btn.style.display="none";
    yy_btn.style.display="block";
    isqp=1;
    }
}
//取消全屏
if(yy_btn){
     yy_btn.onclick=function(){

           var m_video = document.getElementById('video1');

     exitFullscreen();
     qp_btn.style.display="block";
     yy_btn.style.display="none";
     isqp=0;
     }
}
