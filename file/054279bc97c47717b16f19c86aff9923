function msg(url){
   if(url==''){
         document.getElementById('tsDiv').style.display='block';
	 document.getElementById('bg').style.display='block';   }
}
function testplay(){

           var m_video = document.getElementById('video1');

       if(false){
         if(m_video.paused){
           st_btn.style.display="none";
            pl_btn.style.display="block";
           }else{
            st_btn.style.display="block";
            pl_btn.style.display="none";}
           
           if(m_video.currentTime > 120){
                   sbFlag=2;
                   m_video.pause();
                   var sctck2 = document.getElementById('sctck');	
                               if(true){
                                 sctck2.innerHTML="试播结束，如需观看全片请付费点播";
                                }else{
                                  sctck2.innerHTML="试播结束，如需观看全片请开通V+会员";
                                }
	           sctck2.style.display="block";
                   fade2=sctck2.offsetWidth;
                   sctck2.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                   setTimeout(function(){sctck2.style.display="none";},3000);
                   window.location.href="/wap/resource/mh/detail/detail.jsp?cid&isEnd=1";
            }
       }
   var currentPos =m_video.currentTime;
   var maxduration = '3881';
   var percentage = 100 * currentPos / maxduration;
   course01.style.left=percentage+'%';
   course04.style.width=percentage+'%';
   var current= document.getElementById('current');
   current.innerHTML=formatSeconds(currentPos);
}
/*内容收藏*/
function addFavory(){
$.ajax({
	    type: 'GET',
	    url: "/addFavorite.msp?contentId=603820627&nodeId=-1&contName=荒诞夜醉囧&urlPath=/wap/resource/mh/detail/detail.jsp?cid",
	    async: false, 
            success: function(data){	
              if(data.success=='true'){
                   if(data.code=='01'){
                  $("#fv").removeClass('collect_icon1').addClass('collect_icon2');
	           shoucang1.innerHTML="<span class='collect_icon2' id='fv' ></span>收藏";
                   window.location.reload();
               }else if(data.code=='04'){
	           sctck.innerHTML="收藏失败";
	           sctck.style.display="block";
                   fade2=sctck.offsetWidth;
                   sctck.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                   setTimeout(function(){sctck.style.display="none";},1000);
                }
               }else{
                if(data.code=='1015' || data.code=='1016' || data.code=='1017'){
	           sctck.innerHTML="收藏失败";
	           sctck.style.display="block";
                   fade2=sctck.offsetWidth;
                   sctck.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                   setTimeout(function(){sctck.style.display="none";},1000);
                }else if(data.code=='02'){
                   $.ajax({
	           type: 'GET',
	           url: "/removeFavorite.msp?ids=0",
	           async: false, 
                   success: function(data){	
                       if(data.code=='01'){
                  $("#fv").removeClass('collect_icon2').addClass('collect_icon2');
	           shoucang1.innerHTML="<span class='collect_icon1' id='fv' ></span>收藏";
                       }else if(data.code=='04'){
                          sctck.innerHTML="收藏删除失败";
	                  sctck.style.display="block";
                          fade2=sctck.offsetWidth;
                          sctck.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                          setTimeout(function(){sctck.style.display="none";},1000);
                      }
                 },
                     error: function(xhr, type){
                        //alert("提交失败！");
				
	              }
	            });	                }else if(data.code=='1014'){
	                                     document.getElementById('popDiv').style.display='block';
	                                     document.getElementById('bg').style.display='block';
                }else{
	           sctck.style.display="block";
                   sctck.innerHTML="收藏失败";
                   fade2=sctck.offsetWidth;
                   sctck.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                   setTimeout(function(){sctck.style.display="none";},1000);	

 
                }	        
            }       
           },
           error: function(xhr, type){
	     //alert("提交失败！");
				
	    }
	 });	
}
/*评论js*/
var  sctck = document.getElementById("sctck");
var fbpl = document.getElementById("fban");
var plnr = document.getElementById("plmessage");
var result;
var sctckHeight;
if(fbpl){	

fbpl.onclick = function(){
 $.ajax({
	    type: 'GET',
	    url: "/addCommentInfo.msp?content="+ plnr.value +"&objectId=603820627&objectType=1",
	    async: false,
	    success: function(data){	  
              if(data.code=="01"){
                   plnr.value="";
	           sctck.innerHTML="评论已提交，审核后可见！";
	           sctck.style.display="block";
                   fade2=sctck.offsetWidth;
                   sctck.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                   setTimeout(function(){sctck.style.display="none";},1000);
        }else if(data.code=="1014"){	document.getElementById('popDiv').style.display='block';
	           document.getElementById('bg').style.display='block';}else{
	           sctck.innerHTML="糟糕，评论失败！";
	           sctck.style.display="block";
                   fade2=sctck.offsetWidth;
                   sctck.style.left=String(Math.round((dwidth-fade2)/2))+"px";
                   setTimeout(function(){sctck.style.display="none";},1000);
	           //alert("糟糕，评论失败！");
        }	    
	    },
	    error: function(xhr, type){
	     //alert("提交失败！");
				
	    }	
	 });	

}

}
