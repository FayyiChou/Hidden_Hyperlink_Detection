function  addhistory(){
	 $.ajax({
	    type: 'GET',
	    url: "/addHistory.msp?contId=603820627&nodeId=-1&currTime=200&playType=1",
	    async: false,
	    success: function(data){	  
              if(data.code=="0000"){
	     console.log("成功");     
        }else{
	 console.log("失败");     
}    
	    },
	    error: function(xhr, type){
	     //alert("提交失败！");
				
	    }	
	 });	
}
