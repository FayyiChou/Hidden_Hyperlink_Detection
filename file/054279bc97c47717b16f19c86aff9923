function adupdateplay(){
        var adtime="";
        var mv = $("#video1").get(0);
        if(adtime&&adtime!=""){
          adtime=parseInt(adtime);
          $("#adlimittime").addClass("adtime");
          var _adtime = adtime-parseInt(mv.currentTime)+"秒";
	  $("#adlimittime").html(_adtime);
        }
        //console.log(parseInt(mv.currentTime)); 
}
function ddplayEnd(){      

         if(overUrl!=''){

             if(pdover==0){
var img = "<img src='"+overUrl+"' width=0 height=0 style='width:0;height:0;' />";
                 $("#adc").append(img);}else{

             for(var i=0;i<overUrl.length;i++){
                 var url=overUrl[i];
                 var img = "<img src='"+url+"' width=0 height=0 style='width:0;height:0;' />";
                 $("#adc").append(img);
                 //addAdRecords(url);
             }}         }
       var UA = navigator.userAgent.toLowerCase();
       if(UA.indexOf("iphone")==-1){
        $(".ggurl").first().hide();
	var m_video = document.getElementById('video1');
         m_video.removeEventListener("timeupdate",adupdateplay,false);
         $("#adlimittime").hide();
         m_video.src="http://vod.gslb.cmvideo.cn/600259/20150703/16/3007907334/3007950169/3007949539_54.mp4.m3u8?msisdn=0487066704870&mdspid=&spid=600259&netType=1&sid=3007907339&pid=2028596513&timestamp=20170402080335&SecurityKey=20170402080335&Channel_ID=1004_10010001005&ProgramID=603820627&ParentNodeID=-99&assertID=3007907339&encrypt=9279fd9f205c6b6358459eb3820d5e36";
