                    function show_favorite_tips(){
                        if ( VeryCD.cookie.get('favoritefolder') ){
                            document.getElementById("folderfavoritatips").style.display="none";
                        } else {
                            document.getElementById("folderfavoritatips").style.display="";
                            var googletotal = function(){
                                try{
                                    VeryCD.Track('/stat/topicfavtips/');
                                }catch(e){
                                    setTimeout(googletotal,100);
                                }
                            };
                            googletotal();
                        }
                    }
                    function hidden_favorite_tips(){
                        var Then = new Date();
                        Then.setFullYear(Then.getFullYear() + 2);
                        VeryCD.cookie.set('favoritefolder','1',Then,'/', '.verycd.com');
                        document.getElementById("folderfavoritatips").style.display="none";
                        return false;
                    }
