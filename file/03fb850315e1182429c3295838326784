		$("#swich01 .next,#swich02 .next,#swich03 .next,#swich04 .next,#swich05 .next").css("opacity", 0.5);
	});
	$("#swich01,#swich02,#swich03,#swich04,#swich05").mouseleave(function(){
		$("#swich01 .prev,#swich02 .prev,#swich03 .prev,#swich04 .prev,#swich05 .prev").css("opacity", 0);
		$("#swich01 .next,#swich02 .next,#swich03 .next,#swich04 .next,#swich05 .next").css("opacity", 0);
	});	
	$("#swich01,#swich02,#swich03,#swich04,#swich05").bind('mouseover',function(event){
		clearInterval(MyMar);
	}).bind('mouseout',function(event){
		MyMar=setInterval(next,SleepTime);
	});
});
function next(){
	$("#next-01").click();
	$("#next-02").click();
	$("#next-03").click();
	$("#next-04").click();
	$("#next-05").click();
}
MyMar = setInterval(next,SleepTime);
</script>
