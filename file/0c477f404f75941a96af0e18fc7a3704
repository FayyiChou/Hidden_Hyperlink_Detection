    var truePage=document.location.href.match(/.*\/([^/]+)(?:\.htm|\.html|\.shtml).*/)[1];		//真实文件名
    var iframe;
    var slideNum;
    var pageUrl;
    var urlPath;

    pageUrl =  window.location.href;
    urlPath = pageUrl.match(/(.*html).*/).slice(1);
    if((pageUrl.indexOf("#p=")>-1)){
        slideNum = parseInt(window.location.href.match(/(.*=)(\d+)$/).slice(2))-1;
   }else{
        window.location.href = urlPath+"#p=1";
        slideNum = 0;
    }

    initial[slideNum].src = initial[slideNum].getAttribute("data-src");
    initial[slideNum].onload = function(){
        document.getElementById("slider").getElementsByTagName("li")[slideNum].style.height = (initial[slideNum].offsetHeight>devHeight)?initial[slideNum].offsetHeight+'px':devHeight+'px';
    }
    allNum = initial.length;
  downurl=document.getElementById("dwonsite");  
	showurl=document.getElementById("introduce");
    showurl.href=listpath+initial[slideNum].getAttribute("data-name")+'.html';
    downurl.href='http://demo1.font.chinaz.com'+initial[slideNum].getAttribute("data-ttf");   

	shareurl=document.getElementById("toshare");
    shareurl.href='http://service.weibo.com/share/share.php?pic='+initial[slideNum].getAttribute("data-src")+'&title='+document.title+'（分享自@站长素材）&url='+window.location.href

    pagediv = document.getElementById("pageNum");
    pagediv.innerHTML = '('+ (slideNum+1) +'/'+ allNum +' )';
    head = document.getElementById("picHead");
    info = document.getElementById("picinfo");
    sroDiv = document.getElementById("slider");
    sroDiv.onclick = function(){
        head.style.display = (head.style.display=="none")?"block":"none";
        info.style.display = (info.style.display=="none")?"block":"none";

        (element)?(element.style.display = (element.style.display=="none")?"block":"none"):0;

    }

    // slider
    var slider = new Swipe(sroDiv,
                    {
                       startSlide: slideNum,
                        speed: 400,
                        auto: 0,
                        callback:function(e,pos,items){
                            window.location.href = urlPath+"#p="+(pos+1);
                            var curImg = items.getElementsByTagName("img")[0];
                            pagediv.innerHTML = '('+ (pos+1) +'/'+ allNum +' )';
                            (curImg.src.indexOf('blank.jpg')>-1)?$(".loadding").show():$(".loadding").hide();
                            curImg.src = curImg.getAttribute("data-src");
