    <script>
      // window.onload=function(){  
      //   changeDivHeight();  
      // }  
      // //当浏览器窗口大小改变时，设置显示内容的高度  
      // window.onresize=function(){  
      //   changeDivHeight();  
      // }  
      // function changeDivHeight(){               
      //   var bodyw = document.body.clientWidth;
      //   var w = 944/((1920-180)/(bodyw-180))-95;
      //   document.getElementById("b1").style.top=w+"px";
      //   // var h = 944/((1920-180)/(bodyw-180))-112;
      //   // document.getElementById('test').style.height=;
      // }
      //document.getElementById("b1").style.bottom=0;
	  	var window_width = $(window).width();
			var window_height = $(window).height();
			var banner_height = $(window).height();
			var banner_width = $('.index_page_left').width();
		$('.page-sidebar2').attr('style','position:fixed;');	
		
		function resize(){
			window_width = $(window).width();
			window_height = $(window).height();
			banner_height = $(window).innerHeight();
			banner_width = $('.index_page_left').innerWidth();
				if(window_height < 690){
					window_height=690;
					$('#b1').attr('style','bottom:10px;');
				}
				if(window_width < 1540){
					$('.index_sinfo').fadeOut();
				}else{
					$('.index_sinfo').fadeIn();
				}
				if(window_width <768){
					banner_height=banner_width*944/1122;
				}
				
								if(window_width>1200){
					$('body').attr('style','height:'+window_height+'px;width:'+window_width+'px; overflow:hidden;');
				}else{
					$('body').attr('style','');
				}
				$('#myCarousel').attr('style','height:'+banner_height+'px;width:100%; overflow:hidden;')
				///alert(banner_width+'*'+banner_height);
				$('.carousel-inner .item').nailthumb({'width':banner_width,'height':banner_height});
								$('.page-sidebar .hei').attr('style','height:'+window_height+'px;position:fixed;z-index:11;');
		
		}
		resize();
		 $(window).on('resize',function() {
			resize();
		});
		
		
    </script>
	
