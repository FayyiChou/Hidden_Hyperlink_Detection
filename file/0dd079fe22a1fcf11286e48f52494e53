<script type="text/javascript">
    $(document).ready(function(){
        //总导航更多弹开收起
        $('.top-menu .more').click(function(){
            $(this).next('.pNav').slideDown();
            $(this).hide();
            $(this).parent().find(".amore").show();
        });
        $('.top-menu .back').click(function(){
            $(this).parent().slideUp();
            $(this).parent().parent().find(".amore").hide();
            $('.top-menu .more').show();
        });
        //股票导航更多弹开收起
        $(".stock-menu .toggleTip").toggle(function(){
            $(".stock-menu-more").slideDown();
            $(this).html("<img src='https://res.cngoldres.com/mobile/images/toggleTip02.png' />");    
        },function(){
            $(".stock-menu-more").slideUp();
            $(this).html("<img src='https://res.cngoldres.com/mobile/images/toggleTip01.png' />");   
        }); 
        //股票全部导航更多弹开收起
        $(".stock-menu .stock-menu-more-tip").click(function(){
          $('.stock-menuAll').css('display',"block");
        }); 
        $('.menuAllclose').click(function(){
          $('.stock-menuAll').css('display',"none"); 
        });
    });
</script>
