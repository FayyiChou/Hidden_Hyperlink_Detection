			],
			init: function(){
				var self = this;
				self.render();
				
				var slideCuttent = 0;
				var appearSlide = function(){
					var obj = $('.slide ul li');
					var t = obj.size();
					if(slideCuttent >= t){ slideCuttent = 0; }
					obj.eq(slideCuttent).fadeOut('slow', function(){
						$(this).siblings().show();
					});
					slideCuttent++;
				}
				window.setInterval(function(){ appearSlide(); }, 5000);

				var objDetails = $('.details');

				$('.stu-list>ul>li').live('mouseenter', function(){
					objDetails.html($(this).find('.desc').clone()).show();
				}).live('mouseleave', function(){
					objDetails.empty().hide();
				});
			},
			show: function(){
				var r = parseInt(Math.random()*32+1, 10);
				var av = $('.stu-list ul li');
				av.eq(r-1).fadeOut("slow");
				av.eq(r-1).fadeIn("fast");
			},
			render: function(){
				var self = this;
				if(self.data.length != 32){ return; }

				var d = self.data;
				var temp = '<li><a href="http://{0}.xuelecn.com/" title="{1}" target="_blank"><img width="80" src="{2}" alt="" /></a>';
				temp = temp + '<div class="desc"><p><b>姓名：</b>{4} 　　<b>班级：</b>{5}</p><p><b>地区：</b>{6}</p><p><b>使用学乐时长：</b>{7}个月</p><p><b>同步测试100分数：</b>{8}个</p><p><b>同步自测学分：</b>{9}分</p><p><b>荣誉勋章：</b>{10}个</p><p><b>发布作品数：</b>{11}个</p><p class="sum">{12}</p></div>';
				temp = temp + '</li>';
				var av = $('.stu-list>ul');
				var hl = '', j = 0;
				for(var i = 0; i < d.length; i++){
					if(i !=0 && i % 8 == 0){ j++; }
					hl = temp.replace('{0}', d[i].id).replace('{1}', d[i].desc.nm).replace('{2}', d[i].av);
					hl = hl.replace('{4}', d[i].desc.nm).replace('{5}', d[i].desc.gc).replace('{6}', d[i].desc.ar).replace('{7}', d[i].desc.tm).replace('{8}', d[i].desc.tx).replace('{9}', d[i].desc.sync).replace('{10}', d[i].desc.ry).replace('{11}', d[i].desc.zp).replace('{12}', d[i].desc.info);
					av.eq(j).append(hl);
				}
				window.setInterval(function(){ self.show(); }, 1500);
			}
		};
		FacesList.init();
	});
	
	$('#n').focus(function(){ 
		if ($(this).val() == msg.nr || $(this).val() == msg.ne){
			$(this).val('');
			$(this).removeClass('mgray');
		}
	}).blur(function(){
		if ($(this).val() == ''){
			$(this).val(msg.nr);
			$(this).addClass('mgray');
		}
	});
	$('#password-clear').focus(function(){
		$(this).hide();
		$('#p').show();
		$('#p').focus();
	});
	$('#p').blur(function(){
		if ($(this).val() == '') {
			$(this).hide(); $('#password-clear').show();
		}
	});
	var msg = {
		nr: '请输入用户名',
		ne: '用户名不正确',
		pr: '请输入密码',
		pe: '密码不正确'
	};
	var prop = {
		Vali: {
			n: function() {
				var v = this.values[0]; 
				if (v.length == 0 || v == msg.nr) {
					this.elm.value = msg.nr; 
					$showtip(msg.nr); 
					return false;
				}
				if (v.length < 1) {
					this.elm.value = msg.ne;
					$showtip(msg.ne);
					return false;
				}
				return true;
			},
			p: function() {
				var v = this.values[0];
				if (v.length == 0) {
					$('#password-clear').val(msg.pr);
					$showtip(msg.pr);
					return false;
				}
				if (v.length < 1) {
					$(this.elm).hide();
					$('#password-clear').val(msg.pe);
					$('#password-clear').show();
					$showtip(msg.pe);
					return false;
				}
				return true;
			}
		},
		Re: function(jsn) {
			if (jsn.state == 1) {
				location.href = jsn.u;
			}else{
				var msg = "登陆失败！";
				switch (jsn.state) {
					case -1:
						msg = "账号不存在！";
						break;
					case -2:
						msg = "密码错误！";
						break;
				}
				$waitpop.innerHTML = gettip(msg);
				$waitpop.Init();
				setTimeout(function(){
					$waitpop.Dispose(); 
					location.href = '?user=' + $('#n').val() + '&e=' + jsn.state;
				}, 2500);
			}
		}, Wait: function(){
			$waitpop.innerHTML = gettip('正在提交登录...');
			$waitpop.Init(); 
		}, ClearWait: function(){
			$waitpop.Dispose();
		}
	};
	</script>
