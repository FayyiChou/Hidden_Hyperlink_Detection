var scores = new Array();
var numTotal = 0;
function initItCL5DJ39JBZ2OA43CMLRV7R8RVY8AALPA() {
    var divObjs = document.getElementsByTagName("div");
    for (var i = 0; divObjs != null && i < divObjs.length; i++) {
        if (divObjs[i].id != undefined && divObjs[i].id != null && divObjs[i].id.indexOf("KB_") != -1 && divObjs[i].id.indexOf("parent") != -1) {
            numTotal++;
            scores[numTotal] = divObjs[i].id.substring(0, divObjs[i].id.length - 6);
        }
    }
    for (var i = 1; i <= numTotal; i++) {
        var whichEl = document.getElementById(scores[i] + "Child");
        whichEl.style.display = "none";
    }
}
function expandItCL5DJ39JBZ2OA43CMLRV7R8RVY8AALPA(el) {
    if(el==""||!el) return;
    var whichEl1 = document.getElementById(el);
    if(whichEl1 == undefined) { return; }
    if (whichEl1.style.display == "none") {
        whichEl1.style.display = "block";
    } else {
        whichEl1.style.display = "none";
    }
}
