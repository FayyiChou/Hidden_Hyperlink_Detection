function searchSubmit() {
	var search = document.getElementById('searchCBCYMEVKS4PLZVQ5H6D4PCFUPJAFV2XC');
	if (search.news.value == ""||search.news.value=="全文搜索") {
		alert("请输入查询关键字");
		search.news.select();
		search.news.focus();
		return;
	}
	search.keyword.value = search.news.value;
	searchCBCYMEVKS4PLZVQ5H6D4PCFUPJAFV2XC.action = "/portal/cmsSearchV2?entryUrl=/fzgb/mz/";
	searchCBCYMEVKS4PLZVQ5H6D4PCFUPJAFV2XC.target = "_blank";
	search.submit();
}
function clearFont() {
	var search = document.getElementById('searchCBCYMEVKS4PLZVQ5H6D4PCFUPJAFV2XC');
	if (search.news.value=="全文搜索") {
		search.news.value="";
 	}
}
