	var isShowcatTitle = "false";
	var categoryName = "社保征缴指引";
	addLoadEvent(onloadScrolM24J6XBHJF8VHI16RIKVVNCQYFMDM76T);
	function onloadScrolM24J6XBHJF8VHI16RIKVVNCQYFMDM76T(){
		if(null != isShowcatTitle && "true" == isShowcatTitle){
		    if(null == categoryName || "" == categoryName){
		    	window.document.title = "梅州市地方税务局门户网站";
		    }else{
				window.document.title = "社保征缴指引" + "_" + "梅州市地方税务局门户网站";
			}
		}else{
			window.document.title = "社保征缴指引_社保专区_" + "梅州市地方税务局门户网站";
		}
	}
	function addLoadEvent(fun){
		var oldonload = window.onload;
		if(typeof window.onload != 'function'){
			window.onload = fun;
		}else {
			window.onload = function(){
				if(oldonload){
					oldonload();
				}
				fun();
			}
		}
	}
