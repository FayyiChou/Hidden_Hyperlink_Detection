<div id="pH6N064OJXDYTHZQIBGF5TLOZT5I9LMFW"> <script language="javascript" type="text/javascript">
	var xfPageNum=1;
	var blockNum=1;
	var totalPage = 2;
		function getElement(v){return document.getElementById(v);}
	function changePage(page)
		{
  				if(isNaN(page)){alert('请输入数字');return;}
  				if(page>parseInt(getElement('totalPage').value)||page<=0){alert('没有该页');return;}
  				xfPageNum = page;
  				if(xfPageNum==blockNum){
  				   alert("已经是当前页面");
  				   return;
  				}
  				getElement('nepage').value=parseInt(xfPageNum)+1;
				getElement('prePage').value=parseInt(xfPageNum)-1;
				getElement('CurpageLbl').innerHTML=xfPageNum;
  				if(window.location.href.indexOf(".htm")!=-1&&getElement('keywords').value!=""){
  					search_htm();
  					return;
  				}
  				if(getElement('DIV'+blockNum))getElement('DIV'+blockNum).style.display="none";
			if(getElement('DIV'+xfPageNum))getElement('DIV'+xfPageNum).style.display="block";
			blockNum=xfPageNum;
		}
	function prePage()
		{
			var curr=getElement('prePage').value;
			if(parseInt(curr)<1){alert('已经是第一页');return;}
			changePage(curr);
		}
	function nextPage()
		{
			var ne=getElement('nepage').value;
			if(parseInt(ne)>parseInt(getElement('totalPage').value)){ne=getElement('totalPage').value;alert('已经是最后一页');return;}
			changePage(ne);
		}
		function search_htm(){
			hidenDiv();
			var page = parseInt(getElement('CurpageLbl').innerHTML);
			var pageSize = 15;
			var begin = (page-1)*pageSize+1;
			var end = page * pageSize;
			var index = 1;
			var hasFound = 0;
			var keyword = getElement('keywords').value;
			var trs = getElement('DIV_htm').getElementsByTagName("tr");
			for(var i=0;i<trs.length;i++){
				if(trs[i].title&&trs[i].title.indexOf(keyword)!=-1){
					if(index>=begin&&index<=end){
						trs[i].style.display="block";
						hasFound++;
					}else{
						trs[i].style.display="none";
					}
					index++;
				}else{
					trs[i].style.display="none";
				}
			}
			index--;
			var totalP = 0;
			if((parseInt(index)%parseInt(pageSize))==0){
				totalP = index/pageSize;
			}else {
				var t = '';
				t = t+(index/pageSize);
				t = t.substring(0,t.indexOf('.'));
				totalP = parseInt(t)+1;
			}
			getElement("totalPage").value = totalP;
			getElement("totalPage_display").innerHTML = totalP;
			getElement("totalRecord_display").innerHTML = index;
			getElement("DIV_htm").style.display = "block";
			blockNum = page;
			try{
				getElement("DIV_htm").removeChild(getElement("emptyTab"));
			}catch(e){}
			if(hasFound<pageSize){
				var emptyTab = document.createElement("table");
				emptyTab.id="emptyTab"
				emptyTab.className = "table-gdblue-01-contentlist-01";
				emptyTab.width = "100%";
				emptyTab.border="0";
				emptyTab.cellpadding="0";
				emptyTab.cellspacing="0";
				for(var k=0;k<(pageSize-hasFound);k++){
					var row = emptyTab.insertRow(0);
					var cell = row.insertCell(0);
					cell.className = "row-gdblue-01-contentlist-01";
					cell.innerHTML = "<img src=\"/skin/style/gdblue/images-gdblue-01/common/dot-contentlist-01.gif\" align=\"absmiddle\" border=\"0\">&nbsp;";
				}
				getElement("DIV_htm").appendChild(emptyTab);
			}
		}
		function hidenDiv(){
			for(var i=1;i<=totalPage;i++){
				getElement("DIV"+i).style.display = "none"
			}
		}
