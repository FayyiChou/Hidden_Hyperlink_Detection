  // The slider being synced must be initialized first
  jQuery('#carouselslider').flexslider({
    animation: "slide",
    controlNav: false,
    animationLoop: false,
    slideshow: false,
    itemWidth: 157,
    itemMargin:0,
    asNavFor: '#slideritems'
  });
   
  jQuery('#slideritems').flexslider({
    animation: "fade",
	directionNav: false,
    controlNav: false,
    animationLoop: false,
    slideshow: true,
    sync: "#carouselslider"
  });
});

</script>	


