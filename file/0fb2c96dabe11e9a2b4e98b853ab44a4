<script language="javascript" type="text/javascript" src="http://www.qjy168.com/js/jquery.js"></script><script language="javascript" type="text/javascript">

$(function()
{

	$("#search_keyword").focus(function(){

		if($("#search_keyword").val() == "输入产品名称")
		{
			$("#search_keyword").val("");
		}

	});

	$("#search_keyword").blur(function(){

		if($("#search_keyword").val() == "")
		{
			$("#search_keyword").val("输入产品名称");
		}

	});

	change_list(1,'demand');
	change_list(1,'demand');

})

function change_list(id,form_post)
{
	var ul_calss	=	$("#search_ul").attr('class');

	if(ul_calss == "hover")
	{
		var nhtml	=	$("#l"+id).html().replace(id,1);
		var ohtml	=	$("#l1").html().replace(1,id);

		$("#l"+id).html(ohtml);
		$("#l1").html(nhtml);

		$("#search_form").attr('action','http://www.qjy168.com/search/search_'+form_post+'.php');
		if(form_post=="provide"){
			$("#search_form").attr('action','http://www.qjy168.com/pifa/detail.php');
		}
		else if(form_post=="member"){
			$("#search_form").attr('action','http://www.qjy168.com/search/search_company_detail.php');
		}else if(form_post=="pic"){
			$("#search_form").attr('action','http://www.qjy168.com/search/search_img_d.php');
		}else if(form_post=="demand"){
			$("#search_form").attr('action','http://www.qjy168.com/caigou/search_buy.php');
		}
		else{
			//$("#search_form").attr('action','http://www.qjy168.com/caigou/search_buy.php');
		}
		$("#search_ul").removeClass();
	}
	else
	{
		$("#search_ul").attr('class','hover');
	}
}

function Search()
{
	var keys = $("#search_keyword").val();

	if(keys == "" || keys == "输入产品名称")
	{
		alert("请输入关键词");
		return false;
	}
	else
	{
		return true;
	}
}


