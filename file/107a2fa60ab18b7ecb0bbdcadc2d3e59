    <script type="text/javascript">

        window.onload = function () {
            var obj = document.getElementById("tabList");
            var oLi = obj.getElementsByTagName("li");
            for (var i = 0; i < oLi.length; i++) {
                oLi[i].index = i;
                oLi[i].onclick = function () {
                    show(this.index);
                }
            }
            function show(a) {
                index = a;
                for (var j = 0; j < oLi.length; j++) {
                    oLi[j].className = "";
                    $("#submit" + j).hide();
                }
                oLi[index].className = "cur";
                $("#submit" + index).show();
            }
        }

        function to_guoji() {
            if ($("#billCode").val() == "")
                return;
            var billCode = $("#billCode").val();
            billCode = billCode.replace(/\n/g, ",");
            window.location.href = "http://www.stointl.com/Home/Api?bagNo=" + billCode
        }
    </script>
