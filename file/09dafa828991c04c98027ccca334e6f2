    <script type="text/javascript">var recentLocations = new aw_RecentLocations('RecentLocationsV5', 3);

    function SiteMenuSelected() {
        var menu = document.getElementById("SiteMenu");

        var URL = menu.value,
                index = menu.selectedIndex;
        menu.selectedIndex = 0; // Switch back to 1st Menu item
        if (URL != "") {
            gaManager.trackEventAndRedirect(['Main Nav', 'Select', menu.options[index].innerHTML.trim()], URL);
        }
    }

    function GetCurrentLocationSuccess(p) {
        // Make the call to get current conditions for the current location
        var lat = p.coords.latitude; //.toFixed(2);
        var lon = p.coords.longitude; //.toFixed(2);
        //alert(lat+","+lon)
        if (lat != undefined && lat != "" && lon != undefined && lon != "") {
            var rn = $('#location-search input[name=rn]').attr('value');
            var url = 'currentlocation.do?language=zh-cn&lat=' + lat + '&lon=' + lon + '&rn=' + rn;
            window.location = url;
        } else {
            alert('找不到当前位置，请输入其他位置');
        }
    }
  
    function GetCurrentLocationError(p) {
        alert('找不到当前位置，请输入其他位置');
       // $("a#UseCurrentLocationLink").hide();
    }

