<script>

var iosUrl = "http://a.mlinks.cc/AaxI";
		var androidUrl = "http://a.mlinks.cc/Aaxr";
		var iosCode = "AaxI";
		var androidCode = "Aaxr";
		var code;

		var deviceInfo = (function(){
			var ua=navigator.userAgent;
			var android = ua.match(/(Android);?[\s\/]+([\d.]+)?/);
		    var ipad = ua.match(/(iPad).*OS\s([\d_]+)/);
		    var ipod = ua.match(/(iPod)(.*OS\s([\d_]+))?/);
		    var iphone = !ipad && ua.match(/(iPhone\sOS)\s([\d_]+)/);
		    return {
		        ios: ipad || iphone || ipod,
		        android: android
		    };
		})();
		if(deviceInfo.ios){
			code = iosCode;
		}else{
			code = androidCode;
		}
		var btn1 = document.getElementById("mopen");
var option1 = {
			mlink: code,
		    button: btn1,
		    autoLaunchApp : false,
		    autoRedirectToDownloadUrl: true,
		    downloadWhenUniversalLinkFailed: false,
		    inapp : false
		};
var options = [option1];
		new Mlink(options);
		
		document.getElementById("close_btn").addEventListener("click", function(){
			document.getElementById("download").style.display = "none";
		});
		document.getElementById("download").addEventListener("click", function(){
				_czc.push(['_trackEvent', 'APP下载', '点击', '','1','']);
				_hmt.push(['_trackEvent', 'APP下载', '点击', '-','-']);
		});
		
	
		</script>
    

    <!-- // footer -->
    <script charset="utf-8" type="text/javascript">$('ul.switch').bind('click', function (e) { e.preventDefault(); $(this).attr('class') == 'switch' ? $(this).attr('class', 'switch active') : $(this).attr('class', 'switch'); }); $('div.radio-list li').bind('click', function (e) { e.preventDefault(); $('li', $('div.radio-list')).removeClass('active'); $(this).toggleClass('active'); }); $('div.set-wrap li').bind('click', function (e) { e.preventDefault(); $(this).attr('class') == 'active' ? $(this).attr('class', '') : $(this).attr('class', 'active'); }); $('div.tog-head>h3').attr('class', 'not-active'); $('div.tog-head>h3').bind('click', function (e) { e.preventDefault(); if ($(this).attr('class') == 'not-active') { $(this).attr('class', 'active'); $(this).next().show(); } else { $(this).attr('class', 'not-active'); $(this).next().hide(); } }); $('.tog-wrap h3').bind('click', function (e) { e.preventDefault(); if ($(this).attr('class') != 'active') { $(this).attr('class', 'active'); $(this).next().show(); } else { $(this).attr('class', 'not-active'); $(this).next().hide(); } }); $('div.module div.tab-1').show(); $('div.module .tab-group li:first-child').addClass('active'); $('div.module .tab-group li').bind('click', function (e) { e.preventDefault(); $(this).closest('ul').find('li').removeClass('active'); $(this).closest('div').find('div').hide(); $(this).addClass('active'); var tabDisplay = $(this).find('a').attr('href'); var hash = []; hash = tabDisplay.split('#'); var curTab = hash.pop(); $(this).closest('div').find('div.' + curTab).show(); }); $('#location-search .inner #autoResults ul li').live('mouseover', function () { $(this).closest('ul').find('li').removeClass('ac_over'); $(this).addClass("ac_over"); var ul = $(this).closest('ul'); active = $('li', ul).index('li.ac_over'); }); $('#location-search .inner #autoResults ul li').live('mouseout', function () { $(this).removeClass('ac_over'); }); function toggleLocationPanel(obj) {
        var divs = document.getElementById('current').getElementsByTagName('div'); var panel; for (var i = 0; i < divs.length; i++) { if (hasClass(divs[i], 'detail')) { panel = divs[i]; } }
        if (hasClass(obj, 'not-active')) { removeClass(obj, 'not-active'); addClass(obj, 'active'); panel.style.display = 'block'; } else if (hasClass(obj, 'active')) { removeClass(obj, 'active'); addClass(obj, 'not-active'); panel.style.display = 'none'; }
    }
    function hasClass(ele, cls) { return ele.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)')); }
    function addClass(ele, cls) { if (!this.hasClass(ele, cls)) ele.className += " " + cls; }
    function removeClass(ele, cls) { if (hasClass(ele, cls)) { var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)'); ele.className = ele.className.replace(reg, ' '); } }</script>

        


</div></body></html>