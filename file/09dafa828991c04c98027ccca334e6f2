<script type="text/javascript">pageRouteName = '';

//http://www.pengoworks.com/workshop/jquery/autocomplete_docs.txt
var options = {
    autoFill: 1,
    inputClass: 'loc_search_box',
    resultsClass: 'loc_search_results',
    minChars: 2,
    cacheLength: 1,
    matchSubset: 1,
    matchCase: 0,
    matchContains: 0,
    maxItemsToShow: 5,
    mustMatch: 0,
    onItemSelect: selectValue
};

/*
 * When a value is selected, get the cityId
 * stored in the extra values of the backend
 * and redirect to the lookup page
 */
function selectValue(li) {
    //create a redirect url
    var url = '/index.do';
    url = url + "?language=zh-cn&id=" + li.id ;
    //redirect
    window.location = url;

}

