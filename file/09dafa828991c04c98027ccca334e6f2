  <script type="text/javascript">
  
	
		    var aqi_value=$(".aqi").attr("value");
			console.log("aqi_value:"+aqi_value);
			var aqi_value=parseInt(aqi_value);

			var aqi_bgcolor_str="";

			if(aqi_value>=0 && aqi_value<=50){
				aqi_bgcolor_str="aqi-bgcolor-1";
			}else if(aqi_value>50 && aqi_value<=100){
				aqi_bgcolor_str="aqi-bgcolor-2";
			}else if(aqi_value>100 && aqi_value<=150){
				aqi_bgcolor_str="aqi-bgcolor-3";
			}else if(aqi_value>150 && aqi_value<=200){
				aqi_bgcolor_str="aqi-bgcolor-4";
			}else if(aqi_value>200 && aqi_value<=300){
				aqi_bgcolor_str="aqi-bgcolor-5";
			}else if(aqi_value>300){
				aqi_bgcolor_str="aqi-bgcolor-6";
			}
			$(".aqi").addClass(aqi_bgcolor_str);
			
			
	



