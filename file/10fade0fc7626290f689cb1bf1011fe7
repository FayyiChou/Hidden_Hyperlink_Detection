	if(jsonpUrl != ''){
		jQuery.ajax({
			url : jsonpUrl,
			dataType : "jsonp",
			jsonp : "callback",
			cache : false,
			success : function(data) {
				if (data.code) {
					jQuery.post(base_url + '/passport/getSession', {
						"code" : data.code
					}, function(result) {
						if (result == 'success') {
							window.location.reload();
						}
					});
				}
			}
		});
	}
