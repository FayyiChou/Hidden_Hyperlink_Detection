<script type="text/javascript">
q_jq(".intro").append("近期视频查得多，桃友们手快有，手快没呀～");
var eleVideo=q_jq("video");
if(eleVideo.find("source").attr('src').length<2){
var vturl=eleVideo.attr('poster');
vturl='https://vtt'+vturl.substr(vturl.indexOf('media')+5).replace("_frame1.jpg",".mp4").replace("_smart1.jpg",".mp4");
eleVideo.find("source").attr('src',vturl);
eleVideo.html(eleVideo.html());
}
if(eleVideo.html().indexOf("tumblr")>0){
eleVideo.hide();
var countdownnum2=5;
if(discuz_uid==2){
countdownnum2=1;
changMFChanel();
eleVideo.after('<br><div ><button style="padding: 10px 150px;" id="recorddelvideo" onclick="recorddelvideo()">delete video</button></div><br>');
}
eleVideo.before("<div id=\"mfloadingdiv2\">正在缓冲视频...<span>"+countdownnum2+"</span><br><a href=\"/thread-247113-1-1.html\" target=\"_blank\"><img src=\"/img/ad260.jpg\" width=\"480\"/></a></div>");



eleVideo.after('<br><br><div id="btnchangmfchanel" style="color:#777"><button style="padding: 10px 150px;" id="" onclick="changMFChanel();">使用免翻墙线路</button></div><br>');
var mylonginv2=setInterval(function(){
countdownnum2--;
q_jq("#mfloadingdiv2 span").text(countdownnum2);
if(countdownnum2<1){
clearInterval(mylonginv2);
q_jq("#mfloadingdiv2").hide();
eleVideo.show();
}
 },1000);
}

function changMFChanel(){
var phtml=q_jq("video source").prop("outerHTML");
if(phtml.indexOf("video_file")>0){
var reg=/video_file.*?"/;
var videourl=phtml.match(reg);
var videourlelent=videourl[0].replace('video_file/','');
if(phtml.indexOf("t:")>0){
var vus=videourlelent.split('/');
videourlelent=vus[1]+"/"+vus[2];
}
}else if(phtml.indexOf("t.tumblr")>0){
var reg=/tumblr_.*?"/;
var videourl=phtml.match(reg);
var videourlelent=videourl[0]
}
videourlelent=videourlelent.replace('"','').replace('.mp4','');

if(q_jq("#tyshortvideo2").length==0){
var countdownnum=8;if(discuz_uid==2){countdownnum=1;}
q_jq("#btnchangmfchanel").after("<div id=\"mfloadingdiv\">正在缓冲视频...<span>"+countdownnum+"</span><br><a href=\"/thread-247113-1-1.html\" target=\"_blank\"><img src=\"/img/ad260.jpg\"  width=\"480\"/></a></div>");

var imgadd="";
phtml=q_jq("video").prop("outerHTML");

var imgs=phtml.indexOf("http");
var imge=phtml.indexOf("jpg")+3;
imgadd=phtml.substr(imgs,imge-imgs);
imgadd=imgadd.replace("https://","");
var cdomain=
q_jq.ajax({
   type: "POST",
   url: "http://"+document.domain+'/colec/getmf.php',
   data: "vn="+videourlelent+"&tid=276231&imgadd="+imgadd,
   success: function(msg){
if(msg.length>4){
if(msg=="toobig"){
q_jq("#mfloadingdiv").html("视频太大没有免翻功能！");
}else{
phtml=q_jq("video").prop("outerHTML");
phtml=phtml.replace(/src="https:.*?"/,"src=\"http://received.taoyin8.info/"+msg+'"');
if(videourlelent.indexOf("/")>0){
videourlelent=videourlelent.substr(0,6)+"/"+videourlelent.replace("/","-");
}else{
videourlelent=msg.replace('videoc/','');
videourlelent=videourlelent.replace('.mp4','');
}

phtml=phtml.replace(/https:\/\/\d+\.media\.tumblr.com.*?\.jpg/,"http://received.taoyin8.info/images/"+videourlelent+'.jpg');
q_jq("#btnchangmfchanel").after("<div id=\"tyshortvideo2\" style=\"display:none\">"+phtml+"</div>");

var mylonginv=setInterval(function(){
countdownnum--;
q_jq("#mfloadingdiv span").text(countdownnum);
if(countdownnum<1){
clearInterval(mylonginv);
q_jq("#mfloadingdiv").hide();
q_jq("#tyshortvideo2 video").show();
q_jq("#tyshortvideo2").show();
}
 },1000);
}
}else{
q_jq("#mfloadingdiv").html("您来慢了！视频已被删了！");
}

   }
});


}
}
function recorddelvideo(){
var videosrc=q_jq("#tyshortvideo2 video source").attr("src");
videosrc=videosrc.replace('http://received.taoyin8.info/videoc','');
q_jq("#modmenu a").eq(0).click();
q_jq.ajax({
   type: "POST",
   url: '/deletevideot.php',
   data: "v="+videosrc,
   success: function(msg){

q_jq("#tyshortvideo2").detach();
   }
});
}
