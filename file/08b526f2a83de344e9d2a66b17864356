                                </div>
                            </div>

                            <script language="javascript" type="text/javascript">

                                var speed = 40; //滚动速度
                                var rows = 27; //每行高度
                                var stim = 20; //停留时间倍数 * speed
                                var stop = 0; //初始化值，不管
                                divlist2.innerHTML = divlist1.innerHTML
                                function Marquee() {
                                    if (divlist.scrollTop % rows == 0 && stop <= stim) {
                                        stop++;
                                        return;
                                    }
                                    stop = 0;
                                    if (divlist2.offsetTop - divlist.scrollTop <= 0)
                                        divlist.scrollTop -= divlist1.offsetHeight
                                    else {
                                        divlist.scrollTop++
                                    }
                                }
                                var MyMar = setInterval(Marquee, speed)
                                divlist.onmouseover = function() { clearInterval(MyMar) }
                                divlist.onmouseout = function() { MyMar = setInterval(Marquee, speed) }

                            </script>

