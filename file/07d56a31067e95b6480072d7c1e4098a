	});
	
	$('#content_home').fadeToggle('slow');  

	//setInterval('AutoScroll("#ticker01")',1000);
	
	var listenGalleryCatClick = function(obj,parentobj) {
		var hr = $(obj).attr("href");
		$(obj).siblings().each(function(index, domEle){
			$(domEle).css('color','#3a1427');
		});
		$(obj).css('color','#000');
	}
	
	var listenGalleryGet = function(obj,data) {
		//$('#ulthumb').html(data);
		obj.html(data);
		/**$("#ulthumb li").hover(
		function() {
			hoverthumb(this);
		} , function() {
			movehoverclass(this);
		});
		$("#ulthumb li a").click(function() {
			imgEffect($('div #main_view'),$(this));
			var desc = $('#ulthumb li a').find('img').attr('alt');
			if(desc == undefined) {
				desc = "";
			}
			$("#g_desc").html(desc);
			return false;	
		});*/
	}
	
	$('#historyCatTd a').click(function(){    
		listenGalleryCatClick($(this),$('#historyCatTd'));
		historypagination($(this));
		return false;
	});
	
	$('#galleryCatTd a').click(function(){
		var hr = $(this).attr("href");
		listenGalleryCatClick($(this),$('#galleryCatTd'));
		pagination($(this));
		return false;
	});
	
	$('#nav_ul li').click(function(){
		var hr = $(this).find('a').attr("href");
		 $( "#nav_ul li" ).siblings().each(function(index, domEle){
		 	$(domEle).find('a').css('color','#3a1427');
		 });
		 $(this).find('a').css('color','#000');
		$.ajax({
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){
				$('#menutable').html(data);
			}
		});
		return false;
	});
	
	var newsimg = function(str) {
			var arr =str;
			var firstimg="";
			var imgli = "";
			for(i=0;i<arr.length;i++) {
				if(i==0) firstimg = arr[i]['picname'];
				img_name = arr[i]['picname'];
				imgli += "<li><img src='up/news/"+img_name+"' width='50px' onclick='getPic(this)'></img></li>";
			}
			var htmlimg = "<td><img id='newimg' class='latestnewsimage' src='up/news/"+firstimg+"'></img></td>";
			htmlimg += "<td><ul>";
			htmlimg += imgli;
			htmlimg += "</ul></td>";
			$('#tr_news_img').html(htmlimg);
	}
	
	$('#news_ul_first li').click(function(){
		var hr = $(this).find('a').attr("href");
		 $( "#news_ul_first li" ).siblings().each(function(index, domEle){
		 	$(domEle).find('a').css('color','#3a1427');
		 });
		  $(this).find('a').css('color','#000');
		$.ajax({
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){
				if(data!=null) {
					var str = data;
					var arr = eval('(' + str + ')');
					//$('#first_content').html(arr.content);
					var picarr = arr.pic;
					if(picarr!=null) {
						newsimg(picarr);
					}
				}
			}
		});
		return false;
	});
	
	$('#second_content p.p1 a').click(function(){
		var hr = $(this).attr("href");
		 $( "#second_content p.p1" ).siblings().each(function(index, domEle){
		 	$(domEle).find('a').css('color','#3a1427');
		 });
		$(this).css('color','#000');
		$.ajax({  
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){
			    var str = data;
			    var arr = eval('(' + str + ')');
				newsimg(arr);
			}
		});
		return false;
	});
	
	var getHistoryCat = function(obj) {
		var hr = $(obj).attr("href");
		$.ajax({
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){
				var str = data;
				var arr = eval('(' + str + ')');
				var li_str = "";
				var arrcat = arr['cat'];
				var previous = arr['previous'];
				var next = arr['next'];
				var isfirst = arr['first'];
				var islst = arr['last'];
				if(arrcat!="") {
					for(i=0;i<arrcat.length;i++) {
						li_str += "<a href='index.php/welcome/getHistoryAjax/"+arrcat[i]['id']+"'>"+arrcat[i]['cname']+"</a>";
					}
				}
				
				if(!isfirst) {
					document.getElementById("prehistorycat").style.display="";
					document.getElementById("prehistorycat").href="http://www.bluesandjazzshanghai.cn/index.php/welcome/getHistoryCat/"+previous;
				} else {
					document.getElementById("prehistorycat").style.display="none";
				}
				
				if(!islst) {
					document.getElementById("nexthistorycat").style.display="";
					document.getElementById("nexthistorycat").href="http://www.bluesandjazzshanghai.cn/index.php/welcome/getHistoryCat/"+next;
				} else {
					document.getElementById("nexthistorycat").style.display="none";
				}
				
				$('#historyCatTd').html(li_str);   // 
				$('#historyCatTd a').click(function(){ //事件
					listenGalleryCatClick($(this),$('#historyCatTd'));
					historypagination($(this));
					return false;
				});
			}
		});	
	}
	
	var getGalleryCat = function(obj) {
		var hr = $(obj).attr("href");
		$.ajax({
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){   
				var str = data;   
				var arr = eval('(' + str + ')');
				var li_str = "";
				var arrcat = arr['cat'];
				var previous = arr['previous'];
				var next = arr['next'];
				var isfirst = arr['first'];
				var islst = arr['last'];
				if(arrcat!="") {
					for(i=0;i<arrcat.length;i++) {
						li_str += "<a href='index.php/welcome/getGalleryAjax/"+arrcat[i]['id']+"'>"+arrcat[i]['cname']+"</a>";
					}
				}
				
				if(!isfirst) {
					document.getElementById("pregallerycat").style.display="";
					document.getElementById("pregallerycat").href="http://www.bluesandjazzshanghai.cn/index.php/welcome/getGalleryCat/"+previous;
				} else {
					document.getElementById("pregallerycat").style.display="none";
				}
				
				if(!islst) {
					document.getElementById("nextgallerycat").style.display="";
					document.getElementById("nextgallerycat").href="http://www.bluesandjazzshanghai.cn/index.php/welcome/getGalleryCat/"+next;
				} else {
					document.getElementById("nextgallerycat").style.display="none";
				}
				
				$('#galleryCatTd').html(li_str);   // 
				$('#galleryCatTd a').click(function(){ //事件
					listenGalleryCatClick($(this),$('#galleryCat'));
					pagination($(this));
					return false;
				});
			}
		});	
	}
	
	$('#prehistorycat').click(function(){
