        				<script>
        					var currentgroup='group1';
        					function togroupprev(){
        						if(currentgroup=='group4'){
									$('#pregallerycat').show();
									$('#nextgallerycat').show();

									$('#gggg1, .group1_fenarea').hide();
									$('#gggg2, .group2_fenarea').hide();
									$('#gggg3, .group3_fenarea').show();
									$('#gggg4, .group4_fenarea').hide();
									currentgroup='group3';
									toshowgroudsgall(1, currentgroup, 1, 14);
								}else if(currentgroup=='group3'){
									$('#pregallerycat').show();
									$('#nextgallerycat').show();

									$('#gggg1, .group1_fenarea').hide();
									$('#gggg2, .group2_fenarea').show();
									$('#gggg3, .group3_fenarea').hide();
									$('#gggg4, .group4_fenarea').hide();
									currentgroup='group2';
									toshowgroudsgall(1, currentgroup, 1, 14);
								}else if(currentgroup=='group2'){
									$('#pregallerycat').hide();
									$('#nextgallerycat').show();

									$('#gggg1, .group1_fenarea').show();
									$('#gggg2, .group2_fenarea').hide();
									$('#gggg3, .group3_fenarea').hide();
									$('#gggg4, .group4_fenarea').hide();
									currentgroup='group1';
									toshowgroudsgall(1, currentgroup, 1, 14);
								}
            				}
        					function togroupnext(){
								if(currentgroup=='group1'){
									$('#pregallerycat').show();
									$('#nextgallerycat').show();

									$('#gggg1, .group1_fenarea').hide();
									$('#gggg2, .group2_fenarea').show();
									$('#gggg3, .group3_fenarea').hide();
									currentgroup='group2';
									toshowgroudsgall(1, currentgroup, 1, 14);
								}else if(currentgroup=='group2'){
									$('#pregallerycat').show();
									$('#nextgallerycat').show();

									$('#gggg1, .group1_fenarea').hide();
									$('#gggg2, .group2_fenarea').hide();
									$('#gggg3, .group3_fenarea').show();
									$('#gggg4, .group4_fenarea').hide();
									currentgroup='group3';
									toshowgroudsgall(1, currentgroup, 1, 14);
								}else if(currentgroup=='group3'){
									$('#pregallerycat').show();
									$('#nextgallerycat').hide();

									$('#gggg1, .group1_fenarea').hide();
									$('#gggg2, .group2_fenarea').hide();
									$('#gggg3, .group3_fenarea').hide();
									$('#gggg4, .group4_fenarea').show();
									currentgroup='group4';
									toshowgroudsgall(1, currentgroup, 1, 14);
								}
            				}
							function toshowgroudsgall(num, group, start, end){
								$('.'+group+'_fen').each(function (){
									if($(this).attr('id')==group+'_fen_'+num){
										$(this).css({'color':'white'});
									}else{
										$(this).css({'color':'black'});
									}
								})
								$.post('http://www.bluesandjazzshanghai.cn/index.php/welcome/moregallery/'+group+'/'+start+'/'+end,function (data){
									$('#galllistarea').html(data);
								})
							}
        				</script>
