	});  

	$('#historypagination a').click(function(){
		historypagination($(this));
		return false;
	});
	
	var pagination = function(obj) {
		var hr = $(obj).attr("href");
		$.ajax({
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){
				if(data!="") {
					var str = data;
					var arr = eval('(' + str + ')');
					listenGalleryGet($('#ulthumb'),arr['glist']);
					$('#pagination').html(arr['create_links']);
					$('#pagination a').click(function(){
						pagination($(this));
						return false;
					});					
				}
			}
		});
	}
	
	var historypagination = function(obj) {
		var hr = $(obj).attr("href");
		$.ajax({
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){
				if(data!="") {
					var str = data;
					var arr = eval('(' + str + ')');
					listenGalleryGet($('#historyulthumb'),arr['glist']);
					$('#historypagination').html(arr['create_links']);
					$('#historypagination a').click(function(){
						historypagination($(this));
						return false;
					});					
				}
			}
		});
	}
	
	var getCalender = function(obj) {
		var hr = $(obj).attr("href");
		$.ajax({
			type:"GET",
			dataType:"html",
			url:hr,
			success:function(data){
				var str = data;
				var arr = eval('('+str+')');
				$('#calendertbl').html(arr['calender']);
				$('#weekspan').html(arr['week']);
				$('#yearmonthspan').html("<i style='color:#9f6803'>"+arr['month']+"&nbsp;&nbsp;"+arr['year']+"</i>");
				document.getElementById('precal').href= 'http://www.bluesandjazzshanghai.cn/index.php/welcome/getCalenderByMonth/'+ arr['premonth'];
				document.getElementById('nextcal').href= 'http://www.bluesandjazzshanghai.cn/index.php/welcome/getCalenderByMonth/'+ arr['nextmonth'];
			}
		});
		
	}
	
	$('#precal').click(function(){
		getCalender($(this));
		return false;
	});
	
	$('#nextcal').click(function(){
		getCalender($(this));
		return false;
	});
	
