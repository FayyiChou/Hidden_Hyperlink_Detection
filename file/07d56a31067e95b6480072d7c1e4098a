	 }

	function loadcss(name) {
		var head = document.getElementsByTagName('HEAD').item(0);
		var style = document.createElement('link');
		style.href =name;
		style.rel = 'stylesheet';
		style.type = 'text/css';
		head.appendChild(style);
	}
</script>
