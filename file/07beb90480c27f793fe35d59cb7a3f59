<script>
function checkLogin()
{
	if (document.all.Account.value=='')
 	{
 		alert('ID required!');
 		document.all.Account.focus();
 		return false;
 	}
 	
 	if (document.all.Password.value=='')
 	{
 		alert('PW required!');
 		document.all.Password.focus();
 		return false;
 	}
 	
 	document.location.href="index.asp?ISPOST=true&Account="+document.all.Account.value+"&Password="+document.all.Password.value;
	return false;
}
</script>

<script>
var x=0,P=0;
var P2=0,iS=0;
var sc1=document.getElementById('sc1');
var sc2=document.getElementById('sc2');
var sc3=document.getElementById('sc3');
var sc4=document.getElementById('sc4');
setInterval(NowTime,1000);

function NowTime()
{
	if (x==5)
	{
		if (P==1)
		{
			document.all.ImgMain.src="http://www.machinecenter.com.tw/images/banner_index_01.gif";
			document.all.LinkMain.href="http://www.machinecenter.com.tw/all-service_en.asp";
			P=0;
		}
		else
		{
			document.all.ImgMain.src="http://www.machinecenter.com.tw/images/banner_index_01-2.jpg";
			document.all.LinkMain.href="http://www.machinecenter.com.tw/how-to-post.asp";
			P=1;
		}
		x=1;
		
		if (iS==0)
		{
			if (P2==1)
			{
				cs1(sc1,1);
				cs1(sc2,0);
				cs1(sc3,1);
				cs1(sc4,0);
				P2=0;
			}
			else
			{
				cs1(sc1,0);
				cs1(sc2,1);
				cs1(sc3,0);
				cs1(sc4,1);
				P2=1;
			}
		}
	}
	else
	{
		x=x+1;
	}
}

function cs1(a,b)
{
	if(a==null)return;
	a.filters.revealTrans.apply();
	if(b==1) a.style.visibility='visible';
	else a.style.visibility='hidden';
	a.filters.revealTrans.play();}

function isScroll(v){
iS=v;
}
</script>
