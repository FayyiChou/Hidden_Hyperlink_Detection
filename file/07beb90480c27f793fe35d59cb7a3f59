<script>
function checkKey(form)
{
	if (form("Keywords").value=='')
	{
 		alert('Keywords required');
 		form("Keywords").focus();	
 		return false;
 	}
 	if (! IsCorrect(form("Keywords").value)) 
 	{
		alert('Please check the spelling of your keyword(s)');
		form("Keywords").focus();
		return false;
	}
 	
	document.location.href="Search.asp?KeyType="+document.all.KeyType.value+"&Keywords="+document.all.Keywords.value;
	return false;
}

function IsCorrect (InStr)
{
	var ChkRegExp = new RegExp('[a-z0-9]','ig')
	if (ChkRegExp.exec(InStr) == null)
		return false;
	    return true;
}
