<script>
function checkKey2(form)
{
	if (form("Keywords2").value=='')
	{
 		alert('Keywords required');
 		form("Keywords2").focus();	
 		return false;
 	}
 	if (! IsCorrect(form("Keywords2").value)) 
 	{
		alert('Please check the spelling of your keyword(s)');
		form("Keywords2").focus();
		return false;
	}
 	
	document.location.href="Search.asp?KeyType="+document.all.KeyType2.value+"&Keywords="+document.all.Keywords2.value;
	return false;
}

function IsCorrect (InStr)
{
	var ChkRegExp = new RegExp('[a-z0-9]','ig')
	if (ChkRegExp.exec(InStr) == null)
		return false;
	    return true;
}
</script>
    </td>
  </tr>
