                    <script language="javascript" type="text/javascript">
                        function Tab(obj) {
                            var ul = obj.parentNode.parentNode;
                            var lis = ul.getElementsByTagName("li");
                            var as;
                            for (var i = 0; i < lis.length; i++) {
                                as = lis[i].getElementsByTagName("a");
                                for (var j = 0; j < as.length; j++) {
                                    if (as[j] != obj) {
                                        as[j].className = "";
                                    }
                                    else {
                                        as[j].className = "on";
                                    }
                                }
                            }
                        }
                    </script>
