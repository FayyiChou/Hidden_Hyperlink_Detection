        function ShowParvalue() {
            var pos = $("#txtKeyword").offset();
            var div = document.getElementById("Parvalue");
            div.style.display = "";
            div.style.left = (pos.left + 162) + "px";
            div.style.top = (pos.top + 20) + "px";
        }
        document.onclick = function CheckClick() {
            if (event.srcElement != document.getElementById("spanParvalue")) {
                document.getElementById("Parvalue").style.display = "none";
            }
            //搜索提示框
            var obj = document.getElementById("searchlist");
            if (obj != null) {
                obj.style.display = "none";
            }
        }
        function SelectParvalue(obj) {
            document.getElementById("spanParvalue").innerHTML = obj.innerHTML;
            document.getElementById("hfldParvalue").value = obj.key;
        }
        function CheckQuery() {
            return true;
        }
    </script>
