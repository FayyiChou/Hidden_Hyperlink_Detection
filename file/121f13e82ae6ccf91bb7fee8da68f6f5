<div class="bottom-bar">
    <div class="bottom-bar-tip">
        手机号码输入不正确
        <i class="icon close"></i>
        <i class="icon red-sanjiao"></i>
    </div>
    <form id="fm">
        <div class="input-data">
            <input type="text" placeholder="请输入手机号码" id="phone" maxlength="11">
        </div>
        <div class="input-data last">
            <span id="city">请选择城市</span><i class="icon down-arrow"></i>
            <input type="hidden" id="city_code">
            <ul class="drop-down">
                <li data-id="110000">北京</li><li data-id="330100">杭州</li><li data-id="500000">重庆</li><li data-id="330400">嘉兴</li><li data-id="330600">绍兴</li><li data-id="510100">成都</li><li data-id="310000">上海</li><li data-id="120000">天津</li><li data-id="320500">苏州</li><li data-id="320200">无锡</li>            </ul>
        </div>
        <div class="btn"><input type="button" class="btn" id="sale_car" value="立刻卖车"></div>
    </form>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $("#phone").on("keyup afterpaste", function () {
            this.value = this.value.replace(/\D/g, '');
        }).on("blur", function () {
            $(this).removeClass('redBorder');
        });
        $("img.lazy").lazyload({
//            threshold: 500,
//            effect: "fadeIn",
            placeholder: "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAACCAYAAABytg0kAAAAEUlEQVQImWP8DwQMQMDEAAUAPfgEAJtlyeIAAAAASUVORK5CYII="
        });

        $(".history-deals img.lazy, .today-auction img.lazy").lazyload({
            event : "sporty"
        });

        $(".history-deals a.next").on("click", function(){
            $(".history-deals img.lazy").trigger('sporty');
        });
        $(".today-auction a.next").on("click", function(){
            $(".today-auction img.lazy").trigger('sporty');
        });

        $("#sale_car").on("click", function () {
            var phoneValue = $("#phone").val(),
                    city = $("#city").text();
            // 手机号验证
            if (phoneValue == "") {
                $("#phone").addClass('redBorder');
                return false;
            }
            if (!validator.isMobile(phoneValue)) {
                $("#phone").addClass('redBorder');
                return false;
            }
            // 选择城市验证
            if (city == "请选择城市") {
                $("#city").addClass('redBorder');
                return false;
            }
            // 可以提交
            var params = {
                'phone': phoneValue,
                'city': $("#city_code").val()
            }
            reserve_sale_car(params, maiche, function () {
                $("#fm").get(0).reset();
                $("#fm span#city").text('请选择城市');
                $("#fm #city_code").val('');
            });
        });
    });
</script>
