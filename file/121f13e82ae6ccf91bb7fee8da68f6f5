<script type="text/javascript">
    $(document).ready(function () {
        $("#free_phone").on("keyup afterpaste", function () {
            this.value = this.value.replace(/\D/g, '');
        }).on("blur", function () {
            $(this).removeClass('redBorder');
        });
        $("#free_sale_car").on("click", function () {
            var phoneValue = $("#free_phone").val();
            // 手机号验证
            if (phoneValue == "") {
                $("#free_phone").addClass('redBorder');
                return false;
            }
            if (!validator.isMobile(phoneValue)) {
                $("#free_phone").addClass('redBorder');
                return false;
            }
            // 可以提交
            var params = {
                'phone': phoneValue
            }
            reserve_sale_car(params, maiche,function () {
                $("#free_phone").val('');
            });
        });
    });
</script>
