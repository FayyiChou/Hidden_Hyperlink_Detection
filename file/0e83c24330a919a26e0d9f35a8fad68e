  <script type="text/javascript" src="http://www.cn-truck.com/statics/js/jquery.qrcode.min.js"></script> 
  <script type="text/javascript">
$("#share_button").on("click",function(){
  var curUrl = window.location.href;
  try{//生成微信二维码
    $("#pc_weixin_share").empty().qrcode({render:"canvas",width:100,height:100,correctLevel:0,text:curUrl});
  }catch(e){
    $("#pc_weixin_share").empty().qrcode({render:"table",width:100,height:100,correctLevel:0,text:curUrl});
  }
  curUrl = encodeURIComponent(curUrl);
  var toUrl = "http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url="+curUrl;
  $("#pc_qq_zone").attr("href",toUrl);
  toUrl = "javascript:window.open('http://service.weibo.com/share/share.php?url="+curUrl+"','newwindow','width=200,height=200')";
  $("#pc_sina_weibo").attr("href",toUrl);
  $("body").append("<div class=\"modal-backdrop  in\"></div>");
  $("#share_modal").removeClass("hide");
});
$("#share_modal .close-btn").on("click",function(){
  $("#share_modal").addClass("hide");
  $(".modal-backdrop").remove();
});
</script> 
