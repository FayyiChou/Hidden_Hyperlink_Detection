<script type="text/javascript">
    var headeritem=$("#tab_97 .w_tab_header .w_tab_header_item").length;
    if(headeritem>0) {
        $("#tab_97").slide({
            titCell: ".w_tab_header .w_tab_header_item",
            mainCell: ".w_tab_container",
            
            trigger: "",
            delayTime: 2000,
            prevCell: null,
            nextCell: null
        });
    }
    $(function() {
        $("#tab_97 .w_tab_header_group li").each(function() {
            if ($(this).attr("disdata") != null && $(this).attr("disdata") != 0) {
                $(this).css("background-image", "url(" + $(this).attr("disdata") + ")");
            }
        });
        
    });

    //重新设置li的宽度, 修复导航边框设置为>1的时候显示错误
    function reSetLiWidth(){
        
        var tab= $('#tab_97');//$($('#mainFrame')[0].contentWindow.document).find("#tab_97");
        var li_borderLeftWidth= parseInt( $('li:first',$('#tab_97')).css('border-left-width')) || 0;
        var li_borderRightWidth= parseInt( $('li:first',$('#tab_97')).css('border-right-width')) || 0;
        var tabWidth=parseInt($('#tab_97').width());
        var li_length=$('li',$('#tab_97')).length;
        var li_width=tabWidth/li_length-li_borderLeftWidth-li_borderRightWidth;

        $('li',$('#tab_97')).each(function(){
            $(this).width(li_width);
        });
    }

    function openUrl(url,target){
        if(url!=""){
            if(target=="当前窗口"||target=="顶层窗口")
            {
                window.location.href=url;
            }else
            {
                window.open(url,"_blank");
            }}
    }

</script></div>
</div>
