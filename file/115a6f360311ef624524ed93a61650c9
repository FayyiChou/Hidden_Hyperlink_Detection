<script type="text/javascript">
$().ready(function(){
  getSign();
});
function getSign(){
    Service.excuteNoLoading('ShopService', false,{'method':'GET_SIGN','goods_id':goods_id}, function(reply){
		var sign=reply['SIGN'];
		//alert(sign);
		if(sign=='true')$('#accept_goods_CERTI_CODE').parent().parent().remove();
	});
}
</script>



