<script>
var user_no = '';
var user_name = '';
var area_code = '';
acpBeanFactory.req_param['user_no'] =user_no;
acpBeanFactory.req_param['area_code'] =area_code;
acpBeanFactory.req_param['user_name'] =user_name;
$(function(){
	acpBeanFactory.genAcpBeanByServNo(serv_no);
	var acpBean = acpBeanFactory.getAcpBeanByServNo(serv_no);
	acpBean.render();
	popComts.orderComments(popComts.CMT_CONSTS.SERV_CMT_ACCEPT,serv_no); //处理服务评论
	if(productId=="80000045"){
		switchTab(2);
	}
});
function switchTab(tabid){
	  if(tabid=="1"&&$("#wire").attr("class")=="noindex"){
		  productId="80000030";
		  $("#wire").attr("class","index");
		  $("#wirless").attr("class","noindex");
		  $('#query_acc_nbr').val("");
		  $('#query_acc_nbr').focus();
		  hideAdslShowInfo();
	  }else if(tabid=="2"&&$("#wirless").attr("class")=="noindex"){
		  productId="80000045";
		  $("#wire").attr("class","noindex");
		  $("#wirless").attr("class","index");
		  $('#query_acc_nbr').val("");
		  $('#query_acc_nbr').focus();
		  hideAdslShowInfo();
	  }
}
function getImg(e){
	if(!document.getElementById("verify_key_div").style.display || document.getElementById("verify_key_div").style.display!="none"){
		var key = document.getElementById("verify_key");
		var time = new Date();
		key.setAttribute("src","/public/v4/common/control/page/image.jsp?date="+ time);
		document.getElementById('logon_valid').focus();
	}
}
</script>
