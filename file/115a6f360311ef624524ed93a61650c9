<script>
	var serv_no ='ADSL_XY';
	var goods_id='null';
	var productId='80000030';
	function parseRequestParameter(){
		    var params = {};
		    
		    	params['serv_no'] = "ADSL_XY";
		    
		    return params;
	  }
	  //设置页面传过来的参数
