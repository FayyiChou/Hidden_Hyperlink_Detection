    <script type="text/javascript">
      $(function() {
        $('#menu').slicknav();
        $('.c-tabs').basicTabs();
        $('ul.sf-menu').superfish();

        $('#osusume .osusume_box_cont .osusume_txt').matchHeight();
        $('.knowhow_txt').matchHeight();

        $(".accordion_search_area span.span_txt").on("click", function() {
          $(this).next().slideToggle();
          $(this).toggleClass("active");
        });
        $(".accordion_search_area span.span_txt").mouseover(function(){
          $(this).addClass("over");
        });
        $(".accordion_search_area span.span_txt").mouseout(function(){
          $(this).removeClass("over");
        });
        //loading中の表示崩れ対策
        var update_custom_selectbox = function() {
          var value = $(this).find('option:selected').html();
          $(this).closest('div').find('span').html(value);
        };
        $('.custom-selectbox select').each(update_custom_selectbox).on('change', update_custom_selectbox);

        var wid = $(window).width();
        if( wid < 640 ){
          $(".accordion_table .tdLeft ").on("click", function(e) {
            $(this).toggleClass("active");
            $(this).next().slideToggle("fast");
          });
        }
      });
    </script>
