        function showCourse(id, name, period) {
            var query = {};
            query.Name = name;
            query.Period = period;
            query.Guid = id;
            var arg = {}
            arg.Query = query;
            window.showModalDialog("../StudentCenter/Studenting.aspx?type=class&Cid=" + id + "&o=" + Math.random(), arg, "dialogWidth=1002px;dialogHeight=768px");
        }
