<script type="text/javascript" language="javascript">
function getQueryString()
{
	var retval = '';
	var queryString = document.location.search;
	if (queryString == null || queryString.length <= 1) return '';
	return decodeURI(decodeURI(queryString.substring(1)));
}
function stlDynamic_ajaxElement_1(pageNum)
{
    try
    {
        var url = "/SiteServer/SiteFiles/Inner/dynamic/output.aspx?publishmentSystemID=79&" + getQueryString();
        var pars = "pageNodeID=79&pageContentID=0&pageTemplateType=IndexPageTemplate&isPageRefresh=False&pageUrl=oheztM0slash0tZ9E0h1xLwj6WMU7VSznuDx7No0ur9aSzt0slash0KqzQ6rJXkxoQ0equals00equals0&ajaxDivID=ajaxElement_1&templateContent=2x9Ik7qhd20eYOOTPGjGHPaBPI90add0gFTKo3rBFP7W5hWxt4oT0add0HeN8xfsAi8q9zIQpFhy2W2JDQEQWMP199QSvg0equals00equals0";
        if (pageNum && pageNum > 0)
        {
            pars += "&pageNum=" + pageNum;
        }
        jQuery.post(url, pars, function(data, textStatus){jQuery("#ajaxElement_1").html(data);});
    }catch(e){}
}
stlDynamic_ajaxElement_1(0);
