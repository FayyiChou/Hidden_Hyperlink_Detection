(function(uiId){
	var w=$(window).width(),h=36,t=800,t1=1500,pr=null,act=null,inow=0,inex=0;
	var o=$("#"+uiId);
	var os=o.find("a");
	if(!o||o.length==0)return;(!os||os.length==0)&&(o.hide());
	if(os.length<=1)return;
	o.on("touchstart touchmove",function(e){stop();});
	o.on("touchend",function(e){clearTimeout(act);act=setTimeout(play,t1);});
	function play(){
		clearTimeout(pr);
		os.css({top:h+"px"});
		os.eq(inow).css({top:"0px"});
		inex=inow+1>=os.length?0:inow+1;
		os.eq(inow).animate({top:-h+"px"},t,"ease");
		os.eq(inex).animate({top:"0px"},t,"ease");
		inow=inex;
		pr=setTimeout(play,t1);
	};
	function stop(){clearTimeout(pr);clearTimeout(act);};
	play();
})("uiGonggaoHome");
</script>
