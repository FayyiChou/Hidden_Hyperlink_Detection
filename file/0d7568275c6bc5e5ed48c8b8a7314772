		var commonConfig;
		if (document.getElementById("commonConfigJson") && document.getElementById("commonConfigJson").value) {
			commonConfig = eval('(' + document.getElementById("commonConfigJson").value + ')'); 	
		}
		if (commonConfig && commonConfig.eventIdList) {
			eventIdList = commonConfig.eventIdList;
		}
		if (eventIdList && eventIdList.length > 0) {
			try{
				for (var i = 0; i < eventIdList.length; i++) {
				    var eventId = eventIdList[i];
					if (eventId.indexOf(";") >= 0) {
						try {
							var array = eventId.split(";");
    						if (array && array.length > 1) {
    							var click = new MPing.inputs.Click(array[0]);//构造虚拟点击UV
    							if (array[1] && array[1] != "null") {
    								click.event_param = array[1];
    							}
    							if (array.length > 2 && array[2] && array[2] != "null") {
    								click.page_name = array[2];
    							}
    							if (array.length > 3 && array[3] && array[3] != "null") {
    								click.page_param = array[3];
    							}
    							if (array.length > 4 && array[4] && array[4] != "null") {
