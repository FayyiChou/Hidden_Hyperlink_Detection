		//自动唤起APP
		function dowloadAPPAutomatic(wareId,sourceFrom){
        	try {
				var sopDist='';
        	    try{
        	    	if(($('#dist')&&$('#dist').val()=="jd")){
        	    		 var dist=$('#dist').val();
        	    		 sopDist=dist;
        		    }
        	    }catch(e){
        		}
				
        		var  INTENT_URL = 'openApp.jdMobile://virtual';
        	    var  INTENT_PARAM_URL={category:'jump',des:'productDetail',skuId:wareId,dist:sopDist};
        	    var downloadAppPlugIn = {
        	        inteneUrl:INTENT_URL,
        	        inteneUrlParams:INTENT_PARAM_URL,
        	        NoJumpDownLoadPage : true,
        	        M_sourceFrom : sourceFrom
        	    };
        	    if(typeof jQuery.downloadAppPlugInOpenApp == 'function'){
        	    	jQuery.downloadAppPlugInOpenApp(downloadAppPlugIn);
        	    }
        	}catch (e) {		
        	}
        }
		try{
		var oCurrentWareId = document.getElementById("currentWareId").value;
		var sourceFrom = document.getElementById("sourceFrom").value;
		//通用设值自动唤起APP参数 xdj 20170217
		if(($('#isOpenAppFlag') && $('#isOpenAppFlag').val()=="true")){
			dowloadAPPAutomatic(oCurrentWareId,sourceFrom);
		}
		}catch(e){}
