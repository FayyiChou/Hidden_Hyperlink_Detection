function addfavor(sURL, sTitle) { 
		if(confirm("网站名称："+sTitle+"\n网址："+sURL+"\n确定添加收藏?")){
		try { 
			window.external.addFavorite(sURL, sTitle); 
		} catch (e) { 
		try { 
			window.sidebar.addPanel(sTitle, sURL, ""); 
			} catch (e) {
				 alert("加入收藏失败，请使用Ctrl+D进行添加"); 
				 } 
			 } 
		 }
	}
function doAction(transName){    
    document.form1.transName.value=transName;
	document.form1.action="/portal/zh_CN/home/search/searchjump/index.html";
	document.form1.submit();
 }
