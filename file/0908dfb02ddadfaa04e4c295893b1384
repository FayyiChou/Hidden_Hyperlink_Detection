      var pattern =/[:]/; 
      if(pattern.test(cmsDomain))
      {
      var domArr = cmsDomain.split(':');
      forwardDomain = cmsDomain.replace(domArr[0],forwardDomain);
      }
      var plogin = cmsLink.replace(cmsDomain,forwardDomain);
      /*去掉所有空格*/
      plogin = plogin.replace(/[ ]/g,"");
      var ploginArr = plogin.split(',');
      var ploginStr = ploginArr[0];
      var ploginType = ploginArr[1];
      window.open(ploginStr.substring(1,ploginStr.length-1),ploginType.substring(1,ploginType.length-1),'left=0,top=0,width='+(screen.availWidth-10)+',height='+(screen.availHeight-50)+',scrollbars,resizable=yes,toolbar=no,location=yes');
}      
function launchWindow(){
      var currentDomain1 = document.domain.substring(4);
      var forwardDomain1 = document.domain;
      if(currentDomain1 == "961200.net")
      	forwardDomain1 = "online.4001961200.com";
      else
      	forwardDomain1 = "online." + currentDomain1;
      var cmsLink1 = "http://online.4001961200.com/webchat/start.jsp?workgroup=srcb_iocc@workgroup.ioccapp&location=http://online.4001961200.com/webchat/";
      var urlArr1 = cmsLink1.split('/');
      var cmsDomain1 = urlArr1[2];
      /*正则，如果带端口号的话将其端口号保留*/
      var pattern1 =/[:]/; 
      if(pattern1.test(cmsDomain1))
      {
      var domArr1 = cmsDomain1.split(':');
      forwardDomain1 = cmsDomain1.replace(domArr1[0],forwardDomain1);
      }
      var plogin1 = cmsLink1.replace(cmsDomain1,"szns.cms");
      /*替换两次*/
      plogin1 = plogin1.replace(cmsDomain1,forwardDomain1);
      plogin1 = plogin1.replace("szns.cms",forwardDomain1);
      /*去掉所有空格*/
      plogin1 = plogin1.replace(/[ ]/g,"");
      launchWin('framemain',plogin1,800,540);
}
function toggle(object){
	target = document.getElementById("new_menu");
	var menuLayers = document.getElementsByName("aaa");
    for (i=0; i<menuLayers.length; i++) {	
	menuLayers[i].className = 'x2';
	}	
	object.className = 'x1';
}
function popup(){
	var popup = "";
	var width=400;
	var height=270;
	var winleft = screen.width - width;
    var winUp = screen.height - height;
	if(popup=="yes"){
    var defaultOptions = "location=no,status=no,toolbar=no,personalbar=no,menubar=no,directories=no,scrollbars=no,resizable=no,top="+ winUp +",left=" + winleft + ",width=" + width + ",height=" + height;
    launchWinWithOptions('main', "/portal/zh_CN/home/zsj/popup.html", defaultOptions);
	}
}
window.onload = function ()
{
	var aForm = document.getElementsByTagName("form");
	//多个表单
	for (var i = 0; i < aForm.length; i++) {
		WellForm(aForm[i]);
	}
	popup();
}
function fanjian(idx){
	  var gzkg = '';//故障开关
	  var currentDomain2 = "";
	  var forwardDomain2 = "";
	  var cmsLink2 = "";
      var plogin = "";
      var thisURL = window.location.href;
	  var biaozhi = document.domain.substring(0,4);//判断当前是简体版还是繁体版
	  if(biaozhi=="www."){
	  	currentDomain2 = document.domain.substring(4);
	  	if(currentDomain2 == "961200.net")
      	{
      		forwardDomain2 = "big5.4001961200.com";
      	}else{
      		forwardDomain2 = "big5." + currentDomain2;
      	}
	  	if(idx==1){
	  	//当前点击为简体版
      		plogin = "#";
	  	}else{
	  	//当前点击为繁体版
	  		if(gzkg=="是"){
			  	alert("繁体页面维护中，给您带来不便，敬请谅解！");
			  	return;
		  	}
	  		cmsLink2 = window.location.href;
	  		var urlArr = cmsLink2.split('/');
	      	var cmsDomain = urlArr[2];
		    /*正则，如果带端口号的话将其端口号保留*/
		    var pattern =/[:]/; 
		    if(pattern.test(cmsDomain))
	    	{
	      		var domArr = cmsDomain.split(':');
	      		forwardDomain = cmsDomain.replace(domArr[0],forwardDomain2);
	      		plogin = cmsLink2.replace(cmsDomain,forwardDomain);
	    	}else{
	    		plogin = cmsLink2.replace(cmsDomain,forwardDomain2);
	    	}
	      	/*去掉所有空格*/
      		plogin = plogin.replace(/[ ]/g,"");
      		window.open(plogin);
	  	}
	  }else{
	  	currentDomain2 = document.domain.substring(5);
	  	if(currentDomain2 == "961200.net")
      	{
      		forwardDomain2 = "www.4001961200.com";
      	}else{
      		forwardDomain2 = "www." + currentDomain2;
      	}
	  	if(idx==1){
	  	//当前点击为简体版
	  		cmsLink2 = window.location.href;
	  		var urlArr = cmsLink2.split('/');
	      	var cmsDomain = urlArr[2];
		    /*正则，如果带端口号的话将其端口号保留*/
		    var pattern =/[:]/; 
		    if(pattern.test(cmsDomain))
	    	{
	      		var domArr = cmsDomain.split(':');
	      		forwardDomain = cmsDomain.replace(domArr[0],forwardDomain2);
	      		plogin = cmsLink2.replace(cmsDomain,forwardDomain);
	    	}else{
	    		plogin = cmsLink2.replace(cmsDomain,forwardDomain2);
	    	}
      		/*去掉所有空格*/
      		plogin = plogin.replace(/[ ]/g,"");
      		window.open(plogin);
	  	}else{
	  	//当前点击为繁体版
      		plogin = "#";
	  	}
	  }
} 
