	$('div.login_div').parents('div.right').eq(0).css({'position': 'inherit', 'z-index': '2'});
	$('div.login_div').parents('div.right').eq(0).prev('div.content').css('z-index', '1');
	$('div.login_div').parent().find('div#banner').css({'margin-bottom': '40px', 'z-index': '0'});
</script>
