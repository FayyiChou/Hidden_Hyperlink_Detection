<script language="javascript" type="text/javascript">
function jumpMenu(id,frm,sel)
{
	var el=document.getElementById(id);
 	var href=el.options[el.selectedIndex].value;
 	var currentDomain = document.domain.substring(4);
    var forwardDomain = document.domain;
      if(currentDomain == "961200.net")
      	forwardDomain = "ebank.4001961200.com";
      else
      	forwardDomain = "ebank." + currentDomain;
    var urlArr = href.split('/');
      var cmsDomain = urlArr[2];
      /*正则，如果带端口号的话将其端口号保留*/
      var pattern =/[:]/;
      if(pattern.test(cmsDomain))
      {
      var domArr = cmsDomain.split(':');
      forwardDomain = cmsDomain.replace(domArr[0],forwardDomain);
      }
    href = href.replace(cmsDomain,forwardDomain);
 	if(sel)
 	{
 		el.selectedIndex=0;
 	}
 	if(href!="")
 	{
 		if('_new'==frm) open(href); else eval(frm+".location='"+href+"'");
 	}
}
</script>
<script language="javascript" type="text/javascript">
function changeDomain(idx){
      var currentDomain = document.domain.substring(4);
      var forwardDomain = document.domain;
      if(currentDomain == "961200.net")
      	forwardDomain = "ebank.4001961200.com";
      else
      	forwardDomain = "ebank." + currentDomain;
	  if(idx==1) {
      var cmsLink = "'https://ebank.4001961200.com/pweb/prelogin.do?LoginType=R&_locale=zh_CN','per',' left=0,top=0,width='+ (screen.availWidth - 10) +',height='+ (screen.availHeight-50) +',scrollbars,resizable=yes,toolbar=no,location=yes'";
      }
      if(idx==2) {
      var cmsLink = "'https://ebank.4001961200.com:8080/eweb/prelogin.do?LoginType=R&_locale=zh_CN','ent',' left=0,top=0,width='+ (screen.availWidth - 10) +',height='+ (screen.availHeight-50) +',scrollbars,resizable=yes,toolbar=no,location=yes'";
      }
      if(idx==4) {
      var cmsLink = "'https://ebank.4001961200.com:4443/eweb','ebs',' left=0,top=0,width='+ (screen.availWidth - 10) +',height='+ (screen.availHeight-50) +',scrollbars,resizable=yes,toolbar=no,location=yes'";
      }
      var urlArr = cmsLink.split('/');
      var cmsDomain = urlArr[2];
      /*正则，如果带端口号的话将其端口号保留*/
      var pattern =/[:]/;
      if(pattern.test(cmsDomain))
      {
      var domArr = cmsDomain.split(':');
      forwardDomain = cmsDomain.replace(domArr[0],forwardDomain);
      }
      var plogin = cmsLink.replace(cmsDomain,forwardDomain);
      /*去掉所有空格*/
      plogin = plogin.replace(/[ ]/g,"");
      var ploginArr = plogin.split(',');
      var ploginStr = ploginArr[0];
      var ploginType = ploginArr[1];
      window.open(ploginStr.substring(1,ploginStr.length-1),ploginType.substring(1,ploginType.length-1),'left=0,top=0,width='+(screen.availWidth-10)+',height='+(screen.availHeight-50)+',scrollbars,resizable=yes,toolbar=no,location=yes');
}
</script>
