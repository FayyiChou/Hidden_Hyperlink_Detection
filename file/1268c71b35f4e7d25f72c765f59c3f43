Dom.ready(function(){
(function(){
function fixTopbar(){
var top = document.body.scrollTop || document.documentElement.scrollTop;
if(top > 55){
W(".fznav").addClass("global-header-fixed");
}else{
W(".fznav").removeClass("global-header-fixed");
}
}
var index = 'discover',
arr = ['profile','show'];
if(arr.indexOf(index) < 0){
var setFix = setInterval(fixTopbar,100);
}else{
W(".fznav").removeClass("global-header-fixed");
}
})();
});
</script> 
