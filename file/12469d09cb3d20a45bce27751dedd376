                           title="{$show_title}" mars_sead="te_list_list_img{$itemNum}"
                           target="_blank">
                            <img class="lazy J_img" id="J_normal{$id}"
                                 src="//shop.vipstatic.com/img/te/vip_loading.gif"
                                 width="235" height="297" alt="{$show_title}"
