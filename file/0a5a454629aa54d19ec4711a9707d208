<script type="text/javascript">
    function submitPost() {
        document.getElementById("SubmitInsertPost").style.display = "none";
        var smid;
        if (document.getElementById("smid") == null)
            smid = postdataSMID;
        else
            smid = document.getElementById("smid").value;
        document.getElementById("txtSmid").value = smid;

        var unknown;
        if (login.document.getElementById("unknowns") != null) {
            unknown = login.document.getElementById("unknowns").checked == true ? 1 : 0;
        }
        else {
            writeBack('200'); //游客禁止发言
            return;
        }
        document.getElementById("txtUnknowns").value = unknown;

        var vcode = URLencode(document.getElementById("ssVcodeInput").value);
        document.getElementById("formedit").action = '/t/insertPostNew.ashx?vcode=' + vcode;

        document.getElementById("formedit").submit();
        //        setTimeout(function () {
        //           document.location.reload(); 
        //        }, 2000);
    };
    function writeBack(state) {
        if (state == '221') {
            alert("您的发言正在审核，请稍候");
            LogNewPost();
            //document.location.reload();
        }
        else if (state == '211') {
            alert("发表成功");
            LogNewPost();
            //document.location.reload();
        }
        else if (state == '488') {
            alert("系统维护中，暂时无法发言，敬请谅解。");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else if (state == '102') {
            alert("发帖过于频繁，请稍候再发");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else if (state == '410') {
            alert("请填写标题");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else if (state == '411') {
            alert("请填写内容");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else if (state == '105') {
            alert("验证码错误");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else if (state == '200') {
            alert("游客不能发言,请先注册或登陆");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else if (state == '401') {
            alert("参数输入有误，请重试。");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else if (state == '402') {
            alert("文章字符数超过4000限制大小");
            document.getElementById("SubmitInsertPost").style.display = "block";
        }
        else {
            alert("发贴异常");
            document.location.reload();
        }
    };
    function LogNewPost() {
        $ajax( //查询发表成功的帖子
            'get',
            '/t/getPostReplyID.ashx?type=post&smid=' + document.getElementById("txtSmid").value + '',
            null,
            function (obj) { //log日志记录新帖
                try {
                    if (obj.readyState == 4 && obj.status == 200 && obj.responseText != "") {
                        var logsrc = 'http://stat.stockstar.com/logStat/ajax.ashx?app=3&op=3&param=' + obj.responseText + '|1';
                        loadScript(logsrc, function () {
                            document.location.reload();
                        });
                    }
                } catch (e) {
                    document.location.reload();
                } 
            },
            null);
    }
    </script>
