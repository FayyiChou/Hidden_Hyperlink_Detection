<script type="text/javascript">
    jQuery.noConflict();
    jQuery(document).ready(function () {
        jQuery("a[href*='blog.stockstar.com']").each(function () {
            var href = $(this).attr("href").replace("blog", "b");
            jQuery(this).attr("href", href);
        });
    });
