				if (result.directionalResult) {
					$.getJSON('/partner/get-partner-code?json=true&placeIds=index_push', function(result) {
						if (result.index_push) {
							var object = $(".index_focus").find("ul li").eq(1);
							var index_bottom = $.parseJSON(result.index_push.replace(/'/ig, '"'));
							object.find('a').attr('href', index_bottom.url);
							object.find('h3').text(index_bottom.title);
							object.find('.text').text(index_bottom.content);
							object.find('img').attr('src', index_bottom.img);
							var imp = index_bottom.imp + '&t=' + new Date();
							$('<img src="' + imp + '" style="display:none;" />').appendTo(document.body);
						}

					});
				}
			});
		}
