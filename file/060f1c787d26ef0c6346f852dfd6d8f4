<script type="text/javascript">
	jQuery(".inBox").slide({ titCell:".inHd li",mainCell:".inBd",trigger:"click" });
	jQuery(".outBox").slide({ effect:"fold",trigger:"click" });
	jQuery(".inBoxs").slide({ titCell:".inHds li",mainCell:".inBds",trigger:"click"});
	jQuery(".outBoxs").slide({ effect:"fold",trigger:"click" });
	jQuery(".slideTxtBox").slide();
	function showBg() { 
		$(".tanchuang").show(); 
		$(".music").hide(); 
		} 
		function closeBg() { 
		$(".tanchuang").hide();
		$(".music").show(); 
		}
</script>

