	function checkQuery()
	{
		var cc = document.getElementById("form_Query");
		if(cc.M_key.value=="" || cc.M_key.value=='请输入关键字')
		{
			alert("请输入关键字!");
			cc.M_key.focus();
			return false;
		}
		tipsWindown("产品搜索","iframe:/Main_Search.asp?M_key="+encodeURI(cc.M_key.value),"800","450","true","","true","leotheme");return false;
	}
