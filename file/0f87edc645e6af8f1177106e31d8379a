<script type="text/javascript" language="JavaScript">
jq(function(){
    //右侧浮窗
    function rigScroll(){
        var winWidth=jq(window).width();
        if(winWidth<1024){
            jq(".gov_weixin_share").css({"right":10})
        }else{
            var winRig=(winWidth-1005)/2-120;
            jq(".gov_weixin_share").css({"right":winRig})
        }
        jq(".closeBtn").click(function(){
            jq(".gov_weixin_share").hide()
        })

    }
    rigScroll()
    jq(window).scroll(function() {
        rigScroll()
    });

    jq(window).resize(function() {
        rigScroll()
    });
})
</script>
