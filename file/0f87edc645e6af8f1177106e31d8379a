<script  type="text/javascript">
 var jq=jQuery.noConflict(); //解决 这个和网易的那个效果库名字冲突，所以从定义jquery 为jq
 function hd1(showContent,myselfObj){
 jq("*[class = 'dxywhd1']").removeClass("dxywhd1");
        myselfObj.className = "dxywhd1";
      jq(".hd01").hide();
      jq("#"+showContent).show();
}
 function hd2(showContent,myselfObj){
      jq("*[class = 'dxywhd2']").removeClass("dxywhd2");
        myselfObj.className = "dxywhd2";
       jq(".hd02").hide();
      jq("#"+showContent).show();
}
 function hd3(showContent,myselfObj){
      jq("*[class = 'gsgghd']").removeClass("gsgghd");
        myselfObj.className = "gsgghd";
       jq(".hd03").hide();
      jq("#"+showContent).show();
 }
function hd4(showContent,myselfObj){
      jq("*[class = 'zcfgbj3']").removeClass("zcfgbj3");
        myselfObj.className = "zcfgbj3";
      jq(".hd04").hide();
      jq("#"+showContent).show();
}

function mydhhd(showContent,myselfObj){
      jq("*[class = 'sydhhd']").removeClass("sydhhd");
        myselfObj.className = "sydhhd";
	 jq(".myccc").hide();
      jq("#"+showContent).show();
}

function mndj(){
      jq("*[class = 'sydhhd']").removeClass("sydhhd");
        jq("#mymn")[0].className = "sydhhd";
	 jq(".myccc").hide();
	 jq("#myc5").show();
}
function xmndj(id,showContent){
      jq("*[class = 'sydhhd']").removeClass("sydhhd");
        jq("#"+id)[0].className = "sydhhd";
	 jq(".myccc").hide();
	 jq("#"+showContent).show();
}
function inputAutoClear(ipt){if(ipt.value==ipt.defaultValue){ipt.value='';}ipt.onfocus=null;
setTimeout(function(){ipt.onfocus=function(){if(this.value==this.defaultValue){this.value='';}};ipt.onblur=function(){if(this.value==''){this.value=this.defaultValue;}};},0);}
</script>
