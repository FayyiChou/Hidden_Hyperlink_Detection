  <script type="text/javascript">
function cookiesave(n, v, mins, dn, path) {
    if (n) {

        if (!mins) mins = 365 * 24 * 60;
        if (!path) path = "/";
        var date = new Date();

        date.setTime(date.getTime() + (mins * 60 * 1000));

        var expires = "; expires=" + date.toGMTString();

        if (dn) dn = "domain=" + dn + "; ";
        document.cookie = n + "=" + v + expires + "; " + dn + "path=" + path;
    }
}
function cookieget(n) {
    var name = n + "=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') c = c.substring(1, c.length);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
    return "";
}
function closeclick() {
    document.getElementById('addshortcut').style.display = 'none';
    cookiesave('closeclickwap', 'closeclickwap', '', '', '');
}
function clickclose() {
    if (cookieget('closeclickwap') == 'closeclickwap') {
        document.getElementById('addshortcut').style.display = 'none';
    } else {
        document.getElementById('addshortcut').style.display = 'block';
    }
}
</script> 
  <div id="note" class="note" style="display:none;">
   <div>
    <a href="javascript:;" onclick="closeclick()" class="closerd"><img src="./mplus/img/closerd.png" height="24" /></a> 
    <p style="color:#39C; line-height:25px; font-size:25px; padding:15px 5px 5px;">欢迎访问百丽吧</p> 
    <p style="color:#555; line-height:16px; font-size:16px; padding:5px 5px 15px;">图示能让您轻松的访问使用本站</p> 
    <p style="text-align:center; padding-bottom:10px;"><img src="./mplus/img/rd.png" height="" /></p>
   </div>
