    <script type="text/javascript">
  function openApp(){

    if(isWeiXin()){
      window.location = 'http://www.belle8.com/download/';
      return false;
    }
            window.location = 'belle8app://threaddetail?tid=2411418';
      return false;
                        window.location = 'belle8app://home';
  }
  function downApp(){
    var ua = navigator.userAgent;
    var isiOS = ua.match(/iPhone|iPod|iPad/i);
    var isAndroid = ua.match(/Android/i);
    var isPC = !isiOS && !isAndroid;

    if(isWeiXin()){
      window.location = 'http://www.belle8.com/download/';
    }else if (isiOS) {
      window.location = 'https://itunes.apple.com/cn/app/bai-li-ba/id813371425?mt=8';
    } else if (isAndroid) {
      window.location = 'http://www.belle8.com/download/';
     } else {
       window.location = 'http://www.belle8.com';
     }

  }


  function isWeiXin(){
    var ua = window.navigator.userAgent.toLowerCase();
    if(ua.match(/MicroMessenger/i) == 'micromessenger'){
        return true;
    }else{
        return false;
    }
}

</script> 
