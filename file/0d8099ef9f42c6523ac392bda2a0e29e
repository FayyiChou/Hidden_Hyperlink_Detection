<script type="text/javascript">
	//onclick on login
	function _login(){
	 	if(_check()){
			$.ajax({
				cache : false,
				type:"POST",
				url:"IndexAction__login.action",
				data: $('#login-form').serialize(),
				success : function(data) {
					eval("jdata=(" + data + ")");
					if (jdata.success == false) {
						$("#msg-error").show();
						$("#msg-error").html(jdata.msg);	
						return false;
					}
					window.location.reload();
				}
	 	    });
	 	}
		}
	
	function _check(){
		if($("#login-form input[name='user.account']").val() == ""){
			alert("请输入用户名");
			$("#login-form input[name='user.account']").focus();
		    return false;
		}
		if($("#login-form input[name='user.password']").val() == ""){
			alert("请输入密码");
			$("#login-form input[name='user.password']").focus();
		    return false;
		}
		return true;
	}
	
	$(function(){
		$(".input-user-acess").on("keydown" , function(e){
			if (e.keyCode == 13) {
				_login(); 
			}
		});
	});
	
	
	function _logout() {
		if(window.confirm('你确定要注销吗？')){
	        //alert("确定");
	        $.ajax({
				cache : false,
				type:"POST",
				url:"logout.action",
				success : function(data) {
					window.location.reload();
				}
	 	    });
	        return true;
	     }else{
	        //alert("取消");
	        return false;
	    }
	}
	function upVideo(){
		location.href = "disclose.action?flag=upVideo";
	}
</script>
	</div>
	</div>
	
