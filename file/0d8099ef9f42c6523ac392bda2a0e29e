			<script src="tlpHcspw/js/jquery.min.js" type="text/javascript"></script>
			<script type="text/javascript" id="asdsadas">
			function getParam(paramName) {
				paramValue = "";
				isFound = false;
				if (this.location.search.indexOf("?") == 0
						&& this.location.search.indexOf("=") > 1) {
					arrSource = unescape(this.location.search).substring(1,
							this.location.search.length).split("&");
					i = 0;
					while (i < arrSource.length && !isFound) {
						if (arrSource[i].indexOf("=") > 0) {
							if (arrSource[i].split("=")[0].toLowerCase() == paramName.toLowerCase()) {
								paramValue = arrSource[i].split("=")[1];
								isFound = true;
							}
						}
						i++;
					}
				}
				return paramValue;
			}
			
			var flag = getParam("flag"); 
			$("#hcspw-navbar-nav-menu > li").removeClass("active");
			var obj = $("#hcspw-navbar-nav-menu > li[flag='" + flag + "']");
			if (!obj || obj.length <= 0) {
				$("#hcspw-navbar-nav-menu > li:first").addClass("active");
			} else {
				obj.addClass("active");
			}
			
			function keyword_search(){
				var key = $("#keyword").val();
				if(key == null || key == ""){
					return;
				}else{
					$("#keyform").submit();
				}
			}
			</script>
			
	
