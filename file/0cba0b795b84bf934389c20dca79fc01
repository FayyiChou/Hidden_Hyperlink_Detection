<script>
$("#submitbuttom").live("click",function(){
   if($("#textfield").val()==""||$("#textfield").val()==null)
   {alert("请输入关键字！");return;}
   else{
     myForm.action='http://61.144.227.212/was5/web/search?channelid=231709&searchword='+encodeURI($("#textfield").val());
     myForm.method='POST';
     myForm.submit();
   }
});
function searchfun()
{
   if($("#textfield").val()==""||$("#textfield").val()==null)
   {alert("请输入关键字！");return;}
   else{
     myForm.action='http://61.144.227.212/was5/web/search?channelid=231709&searchword='+encodeURI($("#textfield").val());
     myForm.method='POST';
     myForm.submit();
   }
}

</script>
