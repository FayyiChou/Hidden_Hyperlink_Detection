var nowchannelid = "44604";
$(function(){
	$(".left li a").each(function(){
		 var desc=$(this).attr("svalue");		 
         if(desc==nowchannelid)
		 {
                        var sdesc=$(this).attr("ssvalue");
			$(this).addClass("over");
			$("#"+sdesc).addClass("over");
                        $("#"+sdesc+"_1").addClass("over");
		 }
	});
});
