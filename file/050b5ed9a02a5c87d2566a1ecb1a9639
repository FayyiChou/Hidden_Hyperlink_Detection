    <script>
        /*初始*/
        $(".container img").hide();
        $(".container img:eq(0)").show();
        /*定时器函数*/
        var n = 0;
        function changeImg() {
            if (n < $(".container img").length - 1) {
                n = n + 1;
            } else {
                n = 0;
            }
            $(".container img").hide();
            $(".container img:eq(" + n + ")").show();
        }


        /*箭头控制*/
        $(".arrow_left").click(function () {
            if (n > 0) { n = n - 1 } else { n = $(".container img").length - 1 }
            $(".container img").hide();
            $(".container img:eq(" + n + ")").show();
        })
        $(".arrow_right").click(function () {
            if (n < $(".container img").length - 1) { n = n + 1; } else { n = 0 }
            $(".container img").hide();
            $(".container img:eq(" + n + ")").show();
        })
    </script>
    <script>
        /*初始*/
        $(".container img").hide();
        $(".container img:eq(0)").show();
        /*定时器函数*/
        var n = 0;
        function changeImg() {
            if (n < $(".container1 img").length - 1) {
                n = n + 1;
            } else {
                n = 0;
            }
            $(".container1 img").hide();
            $(".container1 img:eq(" + n + ")").show();
        }


        /*箭头控制*/
        $(".arrow_left1").click(function () {
            if (n > 0) { n = n - 1 } else { n = $(".container1 img").length - 1 }
            $(".container1 img").hide();
            $(".container1 img:eq(" + n + ")").show();
        })
        $(".arrow_right1").click(function () {
            if (n < $(".container1 img").length - 1) { n = n + 1; } else { n = 0 }
            $(".container1 img").hide();
            $(".container1 img:eq(" + n + ")").show();
        })
    </script>
    <script type="text/javascript">
        $(function () {
            $("#navigation_vert").hover(
                function (e) {
                    $("#dropdown_four").show().css("left", 120).css("top", 10).css("z-index", 999);
                },
                function () {
                    $("#dropdown_four").hide();
                }
            );
            changecode();
        });

        function changecode() {
            $('#imgCode').show();
            $('#imgCode')[0].src = '/components/code.aspx?' + Math.random();
        }

        function submitInfo() {
            var username = $('#UserName').val();
            if (username != "") {
                var mobile = $('#Mobile').val();
                if (/^1[3|4|5|8][0-9]\d{4,8}$/.test(mobile)) {
                    var code = $('#checkCode').val();
                    if (code != "") {
                        var content = $('#Content').val();
                        jQuery.ajax({
                            url: '/API/Handler/RegisterUserInfo.ashx',
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                Action: "bjwd_zhuanti",
                                UserName: username,
                                Mobile: mobile,
                                Content: content,
                                checkCode: code,
                                hiddenTopicId: 30//每个专题特定标识ID 由后台生成
                            },
                            success: function (data) {
                                if (data.Content == "-1000") {
                                    //alert(data.Message);
                                    alert("出错啦！请稍后重试");
                                } else if (data.Content == "-2") {
                                    alert("验证码不正确！");
                                }
                                else {
                                    alert("您已注册成功！祝您考试必过！");
                                }
                            }
                        });
                    } else {
                        alert("请输入验证码！");
                    }
                } else {
                    alert("请输入正确的11位手机号码！");
                }
            }
            else {
                alert("请输入姓名！");
            }
        }
    </script>



