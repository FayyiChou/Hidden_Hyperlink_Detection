<!-- 简体繁体逻辑转换入口 -->
<script type="text/javascript" src="http://sta.qidian.com/javascript/jquery/jquery/1.10.1/jquery.js"></script>

<script type="text/javascript" src="http://sta.qidian.com/javascript/chinese.min.js"></script>
<script>
jQuery(document).ready(function() {
	/*转换成繁体*/
	if (Q_CHINESE.readCookie('lang') == 'zht'){
		jQuery('html').s2t();
	}
	
	//简体
	jQuery("#simpleChinese").click(function(){
		Q_CHINESE.setCookie("lang", "zhs", 365 * 100);
		jQuery('html').t2s();
	});
	
	//繁体
	jQuery("#big5Chinese").click(function(){
		Q_CHINESE.setCookie("lang", "zht", 365 * 100);
		jQuery('html').s2t();
	});
});

</script>



