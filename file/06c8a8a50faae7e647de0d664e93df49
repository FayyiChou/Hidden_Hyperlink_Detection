<script type="text/javascript">
	var IsLogin = false;
	window.onload = function() {
		if (typeof checkLoginByCookie == 'function')
			IsLogin = checkLoginByCookie();
		else {
			var cookie = GetCookie('cmfuToken');
			IsLogin = cookie && cookie.length > 0 ? true : false;
		}
	};
	function Vote(bookId) {
		if (!IsLogin) {
			alert('你还没有登录，不能投票');
			return;
		}
		MyAjax.SJGVote(bookId, ShowServerMessage);
	}
	function ShowServerMessage(result) {
		if (result.value.indexOf('登录') > -1) {
			ShowLoginDiv();
		}
		else {
			var value = result.value.replace("\n", "");
			eval(value);
		}
	}
	function switchTab(activetabid, tabid, showdivid, hiddendivid) {
		$(activetabid).className = "active";
		$(tabid).className = "";
		$(showdivid).style.display = "";
		$(hiddendivid).style.display = "none";
	}
//       (function() { ShowAuthorRecommend4Book([$('SjgList'),$('lastWeekSjgList'),$('SJGQianLiList'),$('SJGLastWeekQianLiList')
//                                        ,$('SJGQunYingList'),$('SJGLastWeekQunYingList'),$('divGoodBookHis')]); })();
	function switchRecommTab(activetabid, tabid, showdivid, hiddendivid){
		$(activetabid).className = "FTbq1";
		$(tabid).className = "FTbq2";
		$(showdivid).style.display = "";
		$(hiddendivid).style.display = "none";
	}
	function TakeSJGTicket() {
		if (!checkLoginByCookie()) {
			alert('你还没有登录，不能领取。');
			return;
		}
		MyAjax.TakeSJGTickets(TakeSJGTicketsCallBack);
	}
	function TakeSJGTicketsCallBack(res){
		alert(res.value);
	}
</script>
