var activitykefuOsOpen = function(url) {
    if (!window.oswinact || window.oswinact.closed) {
        var tool = "";
        if(Snda.browser.safari)
        {
            tool = "toolbar=no,location=no,left=" + (window.screen.width - 640) / 2 +",top=" + (window.screen.height - 480) / 2 + ",directories=no,scrollbars=no,menubar=no,width=455,height=472,resizable=no,status=no";
        }
        else
        {
            tool = "toolbar=no,location=no,left=" + (window.screen.width - 640) / 2 +",top=" + (window.screen.height - 480) / 2 + ",directories=no,scrollbars=no,menubar=no,width=455,height=455,resizable=no,status=no";
        }
        window.oswinact = window.open(url, '', tool);
        
    }
    else {
        alert('您已经打开了一个对话');
    }
};
