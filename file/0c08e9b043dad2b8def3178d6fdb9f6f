<script>
var mySwiper = new Swiper('.swiper-container',{
	pagination: '.mypagination',
	paginationClickable: true,
	speed:600,
	loop:false,
	autoplay:8000
});
$('.arrow-left').on('click', function(e){
	e.preventDefault()
	mySwiper.swipePrev()
});
$('.arrow-right').on('click', function(e){
	e.preventDefault()
	mySwiper.swipeNext()
});

function resetSwiper(){
	if($('.swiper-container .swiper-slide img').eq(0).height() > 0){
		$('.swiper-container').height($('.swiper-container .swiper-slide img').eq(0).height());
	}
	if($('.container').width()==900)
	{
			$('.swiper-container .swiper-slide img').width(900);
	}else if($('.container').width()==860)
	{
			$('.swiper-container .swiper-slide img').width(860);
	}
	setTimeout('resetSwiper()',10);
}
resetSwiper();
function showvideo()
{
	var vheight = $('#p_video_window').height()*0.89; 
	var text = '<iframe src="http://player.youku.com/embed/XOTI4NzExMDQ4" frameborder=0 allowfullscreen class="p-video"></iframe>';
	$('#videobox').html(text);
	$('.p-video').width('100%').height(vheight).css("display","block");;
}
</script>
