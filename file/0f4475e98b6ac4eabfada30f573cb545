var A_list = document.getElementById("A_list");
var A_lista = A_list.getElementsByTagName("li");
function getChildElements(ele,tagName){
	if(!(ele&&ele.nodeType&&ele.nodeType===1)){return false}
	var child=ele.firstChild;
	var a=[]
	if(tagName){
		while(child){
			if(child.nodeType==1&&child.tagName.toLowerCase()==tagName.toLowerCase()){a.push(child);
			}
			child=child.nextSibling;
		}
	}else{
		while(child){
				if(child.nodeType==1){
					a.push(child);
				}
				child=child.nextSibling;
		}
	}
	return a;
}
var L_list =document.getElementById("Xxk1");
var L_lista = getChildElements(L_list,"li");
