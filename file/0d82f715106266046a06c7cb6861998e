function autoformat() 
{ 
var body = "\n"+document.getElementById("fastpostmessage").value; 
body = body.replace(/ |　/ig,""); 
body = body.replace(/\r\n/ig,"\n"); 
body = body.replace(/\n\n/ig,"\n"); 
