<script>
var speed=60;
var colww2=document.getElementById("colww2");
var colww1=document.getElementById("colww1");
var colww=document.getElementById("colww");
colww2.innerHTML=colww1.innerHTML;
function Marquee1(){
if(colww2.offsetTop-colww.scrollTop<=0){
 colww.scrollTop-=colww1.offsetHeight;
 }else{
 colww.scrollTop++
}
}
var MyMar1=setInterval(Marquee1,speed)
colww.onmouseover=function() {clearInterval(MyMar1)}
colww.onmouseout=function(){MyMar1=setInterval(Marquee1,speed)}
