<script type="text/javascript">

function secBoard(n,ed,table,on,not)
{
	var length=table.tBodies.length;
	for(i=0;i<length;i++)
		table.tBodies[i].style.display="none";
	table.tBodies[n].style.display="block";

	for(i=0;i<length;i++)
	{
		document.getElementById(""+ed+"["+i+"]").className=not;
		document.getElementById(""+ed+"["+i+"]").setAttribute("class", not);
	}
	document.getElementById(""+ed+"["+n+"]").className=on;
	document.getElementById(""+ed+"["+n+"]").setAttribute("class", on);
}
	function swap_tabx(n){
	for(var i=1;i<=6;i++){
		var curC=document.getElementById("tabviewx"+i);
		
		var curB=document.getElementById("tabx"+i);
		if(n==i){
			curC.style.display="block";
			
			curB.className="ye";
		}
		else{
			curC.style.display="none";
			curB.className="off";
	    }
	}
}
function tag_tabx(n){
	for(var i=1;i<=3;i++){
		var curC=document.getElementById("tagviewx"+i);
		
		var curB=document.getElementById("tagx"+i);
		if(n==i){
			curC.style.display="block";
			
			curB.className="yo";
		}
		else{
			curC.style.display="none";
			curB.className="yoff";
	    }
	}
}function swap_tagx(n){
	for(var i=1;i<=2;i++){
		var curC=document.getElementById("stabview"+i);
		
		var curB=document.getElementById("stabx"+i);
		if(n==i){
			curC.style.display="block";
			
			curB.className="newon";
		}
		else{
			curC.style.display="none";
			curB.className="newoff";
	    }
	}
}
