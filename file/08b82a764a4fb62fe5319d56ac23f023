	function jiaoji(a){
		var result='';
		if(typeof b=='undefined'){
			var b=new Array();
		}
		for(i in championData){
			b.push(i);
		}
		var c=a.length;
		if(c==1){
			return a;
		}
		for(var i=0;i<c;i++){	
			result=$.richArray.intersect(b,a[i]);
			b=result;
		}
		return result;
	}
