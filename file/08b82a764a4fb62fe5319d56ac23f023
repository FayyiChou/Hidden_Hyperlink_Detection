$(function(){
	var articleids=5302198
		$.ajax({
			url:'http://php.52pk.com/plus/diggphp.php?t=get&callback=?',
			type:'get',
			data:{'aid':articleids},
			dataType:'jsonp',
			jsonp:'callback',
			success:function(msg){
				if(msg==''){
					return false;
				}
				if(msg['test']=='test'){
					return false;
				}
				_html='';
				_html+='支持('+msg.goodpost+')';
				
				$('#newdigg').html('').html(_html);
			},
			error:function(){
				return false;
			}
		})
})
function getDigg(articleid){
	$.ajax({
			url:'http://php.52pk.com/plus/diggphp.php?t=get&callback=?',
			type:'get',
			data:{'aid':articleid},
			dataType:'jsonp',
			jsonp:'callback',
			success:function(msg){
				if(msg==''){
					alert('出错');return false;
				}
				if(msg['test']=='test'){
					alert('出错');return false;
				}
				_html='';
				_html+='支持('+msg.goodpost+')';
				
				$('#newdigg').html('').html(_html);
			},
			error:function(){
				alert('搜索出错');return false;
			}
		})
}
function setDigg(articleid,active){
	$.ajax({
			url:'http://php.52pk.com/plus/diggphp.php?callback=?',
			type:'get',
			data:{'active':active,'aid':articleid},
			dataType:'jsonp',
			jsonp:'callback',
			success:function(msg){
				if(msg==''){
					alert('出错');return false;
				}
				if(msg['test']=='test'){
					alert('出错');return false;
				}
				_html='';
					_html+='支持('+msg.goodpost+')';
				$('#newdigg').html('').html(_html);
			},
			error:function(){
				alert('搜索出错');return false;
			}
		})
}
</script>

