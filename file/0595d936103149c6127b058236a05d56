            }
        })
	
	
})
     
 	
}) 

function displaydo(id){
 obj = document.getElementById(id);
 obj.style.display = obj.style.display == "block"? "none" : "block";
}

  function score_cad_anim(num){
        var fl_scorenum=$(".fl_scorenum"),td=$(".calendard_td");
