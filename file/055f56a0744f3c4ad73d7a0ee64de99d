				function checkcomment()
				{
					var commentform = document.getElementById("commentform");
					var lixi_comment = document.getElementById("lixi_comment");
					if(commentform.comment.value=="")
					{
						commentform.comment.style.border = "solid";
						commentform.comment.style.borderColor = "#FF0000";
						commentform.comment.style.borderWidth = "2px";
						lixi_comment.style.color = "#FF0000";
					}
					else
					{
						commentform.comment.style.border = "solid";
						commentform.comment.style.borderColor = "#000000";
						commentform.comment.style.borderWidth = "1px";
						lixi_comment.style.color = "#000000";
					}
				}
				function checkname()
				{
					var commentform = document.getElementById("commentform");
					var lixi_name = document.getElementById("lixi_name");
					if(commentform.name.value=="")
					{
						commentform.name.style.border = "solid";
						commentform.name.style.borderColor = "#FF0000";
						commentform.name.style.borderWidth = "2px";
						lixi_name.style.color = "#FF0000";
					}
					else
					{
						commentform.name.style.border = "solid";
						commentform.name.style.borderColor = "#000000";
						commentform.name.style.borderWidth = "1px";
						lixi_name.style.color = "#000000";
					}
				}
				function checkemail()
				{
					var commentform = document.getElementById("commentform");
					var lixi_email = document.getElementById("lixi_email");
					var patrn=/^[a-z0-9_\.]+@[a-z0-9_\-]+(\.)[a-z0-9_\-\.]+$/i;
					if(commentform.email.value==""||(!patrn.exec(commentform.email.value)))
					{
						commentform.email.style.border = "solid";
						commentform.email.style.borderColor = "#FF0000";
						commentform.email.style.borderWidth = "2px";
						lixi_email.style.color = "#FF0000";
					}
					else
					{
						commentform.email.style.border = "solid";
						commentform.email.style.borderColor = "#000000";
						commentform.email.style.borderWidth = "1px";
						lixi_email.style.color = "#000000";
					}
				}
				function checkcommentform()
				{
					var commentform = document.getElementById("commentform");
					if(commentform.name.value=="")
					{
						//alert("名字不能为空.");
						commentform.name.focus();
						return false;
					}
					if(commentform.email.value=="")
					{
						//alert("邮箱不能为空.");
						commentform.email.focus();
						return false;
					}
					var patrn=/^[a-z0-9_\.]+@[a-z0-9_\-]+(\.)[a-z0-9_\-\.]+$/i;
					if (!patrn.exec(commentform.email.value))
					{
						//alert("无效的邮箱地址！");
						commentform.email.focus();
						return false;
					}
					if(commentform.comment.value=="")
					{
						//alert("咨询内容不能为空.");
						commentform.comment.focus();
						return false;
					}
					return true;
				}
