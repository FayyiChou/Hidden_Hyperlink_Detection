function loadimage(){ 
	document.getElementById("randImage").src = "http://218.10.232.42/image.jsp?"+Math.random(); 
} 


function wsbslogin(){

	var USER_NO = document.getElementById("wuser_no").value;
	var USER_PASS = document.getElementById("wuser_pwd").value;
	
	if(USER_NO=="" || USER_NO==null){
		alert("请输入登录名");
		return false;	
	}else	if(USER_PASS=="" || USER_PASS==null){
		alert("请输入登录密码");
		return false;	
	}
		
}
