$(document).ready(function(){
	$("ul.nav>li").hover(function(){
		$(this).find(".subnav11").fadeIn()
		$(this).find(".subnav12").fadeIn()
		$(this).find(".subnav13").fadeIn()
		$(this).find(".subnav14").fadeIn()
		$(this).addClass("current")
		},function(){
			$(this).find(".subnav11").hide()
                        $(this).find(".subnav12").hide()
                        $(this).find(".subnav13").hide()
                        $(this).find(".subnav14").hide()
			$(this).removeClass("current")
			})
	})
</script>
