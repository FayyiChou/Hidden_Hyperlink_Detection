                  <script type="text/javascript">	
	// <![CDATA[
	FrontNews_list01['FrontNews_list01-1409034107496_init'] = function(){
		var showDegree=0;
		var showQuestionTag=0;
		var showMessageCount=0;
		var otherShowDegree=0;
		var otherShowMessageCount=0;
