$(document).ready(function(){
	navtest(1,"/images/images/navlih.jpg") /*1,2,3*/
})
function navtest(type,bg){
	if(type==0 || type==1){
		$("#nav5 ul li").prepend('<div class="nlt"></div><div class="nlb"></div>')}
	if(type==0 || type==2){
		$("#nav5 ul li").prepend('<div class="nll"></div><div class="nlr"></div>')}
	if(type==3){
		$("#nav5 ul li").prepend('<div class="nlba"></div>')}
	$("#nav5 ul li").css({"background-image":"url("+bg+")"})
	$("#nav5 ul li").mouseover(function(){
		if(type==0 || type==1){
		$(this).children(".nlt").stop().animate({top:-20},100)
		$(this).children(".nlb").stop().animate({bottom:-20},100)}
		if(type==0 || type==2){
		$(this).children(".nll").stop().animate({width:0},100)
		$(this).children(".nlr").stop().animate({width:0},100)}
		if(type==3){
		$(this).children(".nlba").stop().animate({height:0},100)}
	}).mouseout(function(){
		if(type==0 || type==1){
		$(this).children(".nlt").stop().animate({top:0},100)
		$(this).children(".nlb").stop().animate({bottom:0},100)}
		if(type==0 || type==2){
		$(this).children(".nll").stop().animate({width:48},100)
		$(this).children(".nlr").stop().animate({width:48},100)}
		if(type==3){
		$(this).children(".nlba").stop().animate({height:39},100)}
	})
}
