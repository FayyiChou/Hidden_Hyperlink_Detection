<script>
    $(function () {
        $('img[cid]').mouseenter(function () {
            i = this;
            var k = setTimeout(function () {
                var showjob = $('#showjob');
                var cominfo = $('#cominfo');
                showjob.css("top", $(i).offset().top - 2).css("left", $(i).offset().left - 2).css("width", $(i).width() + 2).html("").append($(i).parent().clone());
                $.ajax({ url: "managead/html/cj/" + i.getAttribute("cid") + ".htm", dataType: "text", cache: !0, success: function (data) {
                    showjob.append(data).show();
                    if (showjob.width() < 200) { showjob.find('.cominfo').hide(); showjob.removeClass('ppx2'); }
                    else { showjob.addClass('ppx2'); $('#cominfo').css("width", $(i).width() + 2 - 164); }
                }
                })
            }, 500);
            $("img[cid]").mouseleave(function () {
                clearTimeout(k)
            })
        });
        $('#showjob').mouseleave(function () { $(this).hide(); });
    });</script>

