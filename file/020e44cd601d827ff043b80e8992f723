   function chgSearch(a)
	{
		var objs=document.getElementById('s_nav').getElementsByTagName('span');
		for(var i=0;i<objs.length;i++) objs[i].className='';
		objs[a].className='hover';
		var arr_ktp=["all","pos","com"];
		document.getElementById("ktp").value=arr_ktp[a];
	}
