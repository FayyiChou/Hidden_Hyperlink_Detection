        <div class="rl-list" id="rl-otherlist" style="display: block; ">
           <ul style="margin:0;">
                         </ul>
        </div>

        <script>
            //判断一个标签是否包含另外一个标签
            if (!document.all) {
                HTMLElement.prototype.contains = function(obj) {
                    return this.compareDocumentPosition(obj) & 16;
                }
            }

            (function() {
                var getBox = document.getElementById("rl-list");
                var getLi = getBox.getElementsByTagName("li");
                for (var i = 0; i < getLi.length; i++) {
                    getLi[i].onmouseover = function() {
                        this.style.backgroundColor = "#f3f3f3";
                        for (var j = 0; j < this.getElementsByTagName("a").length; j++) {
                            if (this.getElementsByTagName("a")[j].className == "btn") {
                                this.getElementsByTagName("a")[j].style.visibility = "visible";
                            }
                        }
                    }
                    getLi[i].onmouseout = function(event) {
                        event = event || window.event;
                        var toElement = event.relatedTarget || event.toElement;
                        if (!this.contains(toElement)) {
                            this.style.backgroundColor = "";
                            for (var j = 0; j < this.getElementsByTagName("a").length; j++) {
                                if (this.getElementsByTagName("a")[j].className == "btn") {
                                    this.getElementsByTagName("a")[j].style.visibility = "hidden";
                                }
                            }
                        }

                    }
                }
                
                var showIndex = 0;
                if (showIndex == 1 || showIndex == 2)
                {
                    $("rl-list").style.display = "block";
                    $("rl-otherlist").style.display = "none";
                }
                else
                {
                    $("rl-btn").style.display = "none";
                    $("rl-listbtn").className = "selected";
                    $("rl-list").style.display = "none";
                    $("rl-otherlist").style.display = "block";
                }
            })();
            function ChangeXuanshangTab(index) {
                if (index == 0) {
                    $("rl-list").style.display = "block";
                    $("rl-otherlist").style.display = "none";
                    $("rl-btn").className = "selected";
                    $("rl-listbtn").className = "";
                }
                else {
                    $("rl-list").style.display = "none";
                    $("rl-otherlist").style.display = "block";
                    $("rl-btn").className = "";
                    $("rl-listbtn").className = "selected";
                }
            }
        </script>

