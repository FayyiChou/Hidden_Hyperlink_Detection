    <script type="text/javascript">
        (function () {
            if (!window.TingBookUrl) return;
            
            for (var idx = 0; idx < window.TingBookUrl.length; idx++) {
                var book = window.TingBookUrl[idx];
                if ("1004066138" == book.BookId) {
                    var span = document.getElementById("spantingbook");
                    if(span){                   
                        var a = span.getElementsByTagName("a");
                        if (a && a.length>0) {
                            a[0].href = book.ChannelId;
                            span.style.display = "block";}
                    }
                    break;
                }
            }
        })();
    </script>
