    <script>
        (function() {
            if (location.hash != null && location.hash == "#bshd") ShowBook.SwitchHD($('hdtabs05'), 'inquiry'); //如果是参加调查,则默认切换到调查页面
            else if (ShowBook.BookInfo.IsHaveMonthTicket == "0") ShowBook.SwitchHD($('hdtabs02'), 'award'); //如果作品没月票，默认显示打赏页面

            /*大神之光称号显示*/
            var count;
            if (typeof (ShowBook.BookInfo.AuthorTitleCount) != undefined)
                count = parseInt(ShowBook.BookInfo.AuthorTitleCount)
            if (count >= 0) {
                var _at = document.getElementById("authortitleCount");
                if(count == 0)
                    _at.innerHTML = "<img src='/Images/deity_00.png' width='39' height='18' alt='大神之光' title='大神之光' style='vertical-align:middle;' />";
                else if (count > 0 && count <= 100)
                    _at.innerHTML = "<img src='/Images/deity_01.png' width='39' height='18' alt='大神之光' title='大神之光一级' style='vertical-align:middle;' />(" + count + ")";
                else if (count >= 101 && count <= 1000)
                    _at.innerHTML = "<img src='/Images/deity_02.png' width='39' height='18' alt='大神之光' title='大神之光二级' style='vertical-align:middle;' />(" + count + ")";
                else if (count >= 1001 && count <= 10000)
                    _at.innerHTML = "<img src='/Images/deity_03.png' width='39' height='18' alt='大神之光' title='大神之光三级' style='vertical-align:middle;' />(" + count + ")";
                else if (count > 10000)
                    _at.innerHTML = "<img src='/Images/deity_04.png' width='39' height='18' alt='大神之光' title='大神之光四级' style='vertical-align:middle;' />(" + count + ")";
                _at.href = "http://me.qdmm.com/Dszg/DszgDetail.aspx?authorId=" + ShowBook.BookInfo.authorId;
                _at.style.display = "";
            }
        })();
    </script>
