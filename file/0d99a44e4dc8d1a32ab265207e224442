<script type="text/javascript">
    (function(){
        var GetCookieAllyes = function (cookieName) {
            var cookieString = document.cookie;
            var start = cookieString.indexOf(cookieName + '=');
            if (start == -1) return null;
            start += cookieName.length + 1;
            var end = cookieString.indexOf(';', start);
            if (end == -1) return unescape(cookieString.substring(start));
            return unescape(cookieString.substring(start, end));
        };
        var us = GetCookieAllyes("us");
        if (!us) {
            document.write("<script type='text/javascript' src='http://wwwploy.qidian.com/RemoteWebService/IGetUserSign.aspx'></scr"+"ipt>");
        }
    })();
</script><script type="text/javascript" src="http://wwwploy.qidian.com/RemoteWebService/IGetUserSign.aspx"></script>





