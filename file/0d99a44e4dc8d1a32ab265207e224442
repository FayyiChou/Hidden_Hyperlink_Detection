<script>
var qdns = 'MMFloatNavjs'; /*  namespace  */
(function(win) {  
    if (!win[qdns])
        var F = { Extend: function(dest, source, replace) { for (prop in source) { if (replace == false && dest[prop] != null) continue; dest[prop] = source[prop]; }; return dest; } };
    win[qdns] = F;
})(window);

(function(){
    var AttachEvent = CommonMM.Conponent.AttachEvent;
    var Funcs = {
       	    fshow : function(){$(this).className = "fhshow";},
	        fhidden : function(){$(this).className = "";},
	        fn_off : $("fh_off"),
	        bindone : function(){
	            AttachEvent($("fh_off"),"click",this.backone,null);
	        },
	        backone : function(){
	            $("fh_on").style.display="block";
	            $("fh_on").style.width="129px";
	            $("fh_close").style.display="block";
	            $("fh_off").className="";
	        },
	        bindli:function(){
	            var liobj = $("menuli").getElementsByTagName("li");
	            var linum = $("menuli").getElementsByTagName("li").length;
	        },
	        backtwo:function(e){
	            var  e   =   window.event || e; 
	            var obj = e.srcElement ? e.srcElement : e.target;	       
	            var cateid =obj.getAttribute("name");
	            if(cateid.length<5){
	                Funcs.Showclass(cateid);
	            }	        
	        },
	        Showclass:function(cateid){
                var _div = document.createElement('div');
                $("fm_board").innerHTML ="";
                _div.innerHTML ='<div id="pop_Class" class="pop_Main"><iframe id="pop_frame_x22" frameborder="0" width="590px" height="358px" scrolling="no" haslayout="-1" src="/Controls/floatNav.aspx?categoryId='+cateid+'"></iframe></div>';
                $("fm_board").appendChild(_div);
                $("fm_board").style.display="block";       
            },       
            Oninit:function(){
                var categroy = CommonMM.Url.catecollect;
	            var str = new StringBuilder();
			    for(var c = 0;c<categroy.length;c++){
	                str.append("<li name='"+categroy[c].categoryid+"' onclick='MMFloatNavjs.Showclass("+categroy[c].categoryid+")' title='"+categroy[c].categoryname+"'>"+categroy[c].categoryname+"</li>");
	            }
	            str.append("<a href='http://wanben.qdmm.com/' target='_blank' title='精品完本'><li name='http://wanben.qdmm.com/'>精品完本</li></a>");
	            $("menuli").innerHTML = str.toString();
                if(document.body.clientWidth-990>0){
                    $("float-block").style.left=(document.body.clientWidth-990)/2+"px";
                }
                else{
	            $("float-block").style.left = "0px";	
                }
                $("fh_off").className="float-handle fhshow";
                this.bindone(); 

                AttachEvent($("fh_close"),"click",function(){
 		                    $("fh_on").style.display="none";
 		                    $("fh_close").style.display="none";
 		                    $("fh_off").className="float-handle fhshow";
 		                    $("fm_board").style.display="none";
 		                    $("fm_board").innerHTML="";
                     },null);
             }
    };
     window[qdns].Extend(window[qdns], Funcs, false); /* Extend Functions */
    })();

          
window.onresize = function(){
    if(document.body.clientWidth-990>0){
        $("float-block").style.left=(document.body.clientWidth-990)/2+"px";
    }
    else{
	$("float-block").style.left = "0px";	
    }
}
var pageHandleFloat = MMFloatNavjs;
pageHandleFloat.Oninit();
</script>
   
 
           
