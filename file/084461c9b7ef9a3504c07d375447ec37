var curmu=0;

function SelNav(obj)

{

 if(obj<$("#navul li").length && obj>=0){ curmu=obj; }

 $("#navul li").eq(curmu).attr("class","curnav");

}

 $("#navul li").bind("mouseover",function(){

 $("#navul li").attr("class","");

 $(this).attr("class","curnav");

 });

 $("#navul li").bind("mouseout",function(){

 $("#navul li").attr("class","");

 $("#navul li").eq(curmu).attr("class","curnav");

 });

 

 

</script>



