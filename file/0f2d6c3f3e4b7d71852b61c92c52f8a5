//                    break;
                case 2:
                    menu2.style.background = "url(images2/tpxw_bg.jpg)";
                    menu3.style.background = "";
                    menu4.style.background = "";
                    var item = menu2.getElementsByTagName("td");
                    item[1].className = "style_red12";
                    var item = menu3.getElementsByTagName("td");
                    item[1].className = "style_yellow";
                    var item = menu4.getElementsByTagName("td");
                    item[1].className = "style_yellow";
                    cd2.style.display = "";
                    cd3.style.display = "none";
                    cd4.style.display = "none";
                    break;
                case 3:
                    menu2.style.background = "none";
                    menu3.style.background = "url(images2/tpxw_bg.jpg)";
                    menu4.style.background = "";
                    var item = menu2.getElementsByTagName("td");
                    item[1].className = "style_yellow";
                    var item = menu3.getElementsByTagName("td");
                    item[1].className = "style_red12";
                    var item = menu4.getElementsByTagName("td");
                    item[1].className = "style_yellow";
                    cd2.style.display = "none";
                    cd3.style.display = "";
                    cd4.style.display = "none";
                    break;
                case 4:
                    menu2.style.background = "none";
                    menu3.style.background = "";
                    menu4.style.background = "url(images2/tpxw_bg.jpg)";
                    var item = menu2.getElementsByTagName("td");
                    item[1].className = "style_yellow";
                    var item = menu3.getElementsByTagName("td");
                    item[1].className = "style_yellow";
                    var item = menu4.getElementsByTagName("td");
                    item[1].className = "style_red12";
                    cd2.style.display = "none";
                    cd3.style.display = "none";
                    cd4.style.display = "";
                    break;

            }
        }
    </script>

