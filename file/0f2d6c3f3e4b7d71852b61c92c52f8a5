    <script language="javascript" type="text/javascript">
    function addFavorite(url, title) {
        if (document.all)
            window.external.AddFavorite(url, title);
        else if (window.sidebar)
            window.sidebar.addPanel(title, url, "");
    }
    function changecolor(id)
    {
        var ids=document.getElementById(id);
        //alert(id);
        ids.style.backgroundImage="url(images/dhuse.jpg)";
        ids.style.backgroundRepeat="repeat-x";
        ids.style.color="#ffffff";
        ids.style.fontWeight="bold";
    }
    function changecolor2(id)
    {
        var ids=document.getElementById(id);
        //alert(id);
        ids.style.backgroundImage="";
        ids.style.fontWeight="";
    }
    function changesid(id,sid)
    {
        var ids=document.getElementById(id);
        //alert(id);
        ids.style.backgroundImage="url(images/dhuse.jpg)";
        ids.style.backgroundRepeat="repeat-x";
        ids.style.color="#ffffff";
        ids.style.fontWeight="bold";
        
        var ids2=document.getElementById(sid);
        ids2.style.color="#ffffff";
        //alert(sid);
    }
    </script>

