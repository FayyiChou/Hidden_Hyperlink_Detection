                case 1:
                    menu1.style.display = "";
                    menu2.style.display = "none";
                    menu3.style.display = "none";
                    break;
                case 2:
                    menu1.style.display = "none";
                    menu2.style.display = "";
                    menu3.style.display = "none";
                    break;
                case 3:
                    menu1.style.display = "none";
                    menu2.style.display = "none";
                    menu3.style.display = "";
                    break;

            }
        }
