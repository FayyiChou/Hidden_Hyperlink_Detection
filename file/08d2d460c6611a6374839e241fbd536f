            <script language=javascript>
 
      function form_submit(){
        if (document.form_order.orderID.value.length==0){
			alert("友情提示：请输入您的订单号。");
			document.form_order.orderID.focus();
			return false;
			}
		if (fucPWDchk(form_order.orderID.value)==0 ){
 		         alert("友情提示：订单号应该是数字！"); 
 		       	 document.form_order.orderID.focus(); 
 		         return(false); 
 	    	  }
		if (document.form_order.customerName.value.length==0){
			   alert("友情提示：请输入收货人姓名。");
			   document.form_order.customerName.focus();
				return false;
			}
      }
	   function fucPWDchk(str){ 
        var strSource ="0123456789."; 
        var ch; 
        var i; 
        var temp; 
        for (i=0;i<=(str.length-1);i++) { 
            ch = str.charAt(i); 
            temp = strSource.indexOf(ch); 
            if (temp==-1){ 
               return 0; 
            } 
        } 
       if (strSource.indexOf(ch)==-1) { 
            return 0; 
       }else{ 
            return 1; 
       } 
     } 
                </script>
