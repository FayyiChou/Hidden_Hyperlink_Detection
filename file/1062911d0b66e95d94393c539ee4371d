<script type="text/javascript">
	$(function(){
		$(".nav ul li a").eq(2).click(function(){
			if($(".productList").css("display")=="none")
			{	$(".nav ul li a").removeClass("cur");
				$(".nav ul li a").eq(2).addClass("cur");
				$(".productList").slideDown();
			}
			else
			{
				$(".nav ul li a").eq(2).removeClass("cur");
				$(".productList").slideUp();
			}
		})
	})
</script>
  



