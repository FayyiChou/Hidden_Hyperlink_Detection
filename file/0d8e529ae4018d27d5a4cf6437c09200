            var isExpert = 1;
            $(document).ready(function () {
                var autoObj = document.getElementById("autoNum");
                if (autoObj) {
                    var value = parseInt(autoObj.innerHTML);
                    function autoUpdateTime(){
                        value--;
                        if(value>-1)
                            autoObj.innerHTML=value+"";
                        setTimeout(autoUpdateTime, 1000);
                    }
                    autoUpdateTime();
                }
            });
        </script>





    
