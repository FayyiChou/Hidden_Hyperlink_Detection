<a id="gotop" class="gotop" href="#"><img default_size="90,90" orgin_url="//imgv3.tvsou.com/m/images/gotop.png" width="0" height="0" src="//imgv3.tvsou.com/m/images/gotop.png"></a>
<script>
var imageManager = function() {
    var default_image = '//imgv3.tvsou.com/m/images/t.png';    var _scale = 1;

    function initImage() {
        this.setAttribute('default_size', this.width+','+this.height);
        if (this.getAttribute('fitwidth'))
            this.setAttribute('width', '100%');
        else if (this.getAttribute('fitheight'))
            this.setAttribute('height', '100%');
        imageManager.fixImageSize(this);
    }

    function imageLoadedError() {
        this.onerror = null;
        this.src = default_image;
    }

    return {
        init: function(scale) {
            var w, h, src;
            var imgs = document.getElementsByTagName('img');
            var len = imgs.length;

            _scale = scale;
            for (var i = 0; i < len; i ++) {
                if (! imgs[i].onerror)
                    imgs[i].onerror = imageLoadedError;
                imageManager.fixImageSize(imgs[i]);
                src = imgs[i].getAttribute('orgin_url');
                if (src)
                    imgs[i].src = src;
            }
        },
        fixImageSize: function(img) {
            var size = img.getAttribute('default_size');
            if (size) {
                var w, h, dw, dh;
                w = img.getAttribute('width');
                h = img.getAttribute('height');
                size = size.split(',');
                dw = parseInt(size[0]);
                dh = parseInt(size[1]);
                if (!w || w.indexOf('%') < 0) {
                    if (!h || h.indexOf('%') < 0) {
                        img.width = parseInt(dw * _scale);
                        img.height = parseInt(dh * _scale);
                    } else {
                        img.width = parseInt(dw * img.parentNode.clientHeight / dh);
                    }
                } else {
                    if (!h || h.indexOf('%') < 0) {
                        img.height = parseInt(dh * img.parentNode.clientWidth / dw);
                    } else {
                        //
                    }
                }
            } else if (img.onload != initImage) {
                img.onload = initImage;
            }
        },
        updateImages: function(scale) {
            var imgs = document.getElementsByTagName('img');
            var len = imgs.length;

            _scale = scale;
            for (var i = 0; i < len; i ++) {
                imageManager.fixImageSize(imgs[i]);
            }
        }
    }
}();
imageManager.init(_initialScale);

(function() {
    if (document.getElementById('gotop')) {
        var showGoTop = false;
        window.addEventListener('scroll', function() {
            var b = false;
            if (document.documentElement) {
                b = document.documentElement.scrollTop > 100;
            } 
            if (document.body) {
                b = b || document.body.scrollTop > 100;
            }
            if (b != showGoTop) {
                showGoTop = b;
                document.getElementById('gotop').style.display = (showGoTop) ? 'block' : 'none';
            }
        });
    }
}());
</script>

