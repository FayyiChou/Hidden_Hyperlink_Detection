<script>
    var count, width, height, currentIndex = -1, DEMO, timeout = 5000;
    function cover_resize() {
        count = 3;
        width = $(window).width();
        height = $(window).height();
    }
    function setFoucs() {
        currentIndex = currentIndex >= 3 ? 0 : currentIndex;
        $('.image-pos-area > div').removeClass('on').eq(currentIndex).addClass('on');
        $('.image-area').stop().animate({'marginLeft': 0 - width * currentIndex}, 800);
        $('#cover-curr-index').val(currentIndex);
    }
    function autoScroll() {
        currentIndex++;
        setFoucs();
    }
    function StopScroll() {
        clearInterval(DEMO);
    }
    $(document).ready(function() {
        $(".image-area .image").hover(function () {
            StopScroll();
        }, function () {
            DEMO = setInterval(autoScroll, timeout);
        });
        $(".btn.icon.cover").hover(function () {
            StopScroll();
        }, function () {
            DEMO = setInterval(autoScroll, timeout);
        });

        $('.image-pos-area > div').hover(function() {
            currentIndex = $(this).index();
            setFoucs();
            StopScroll();
        }, function() {
            DEMO = setInterval(autoScroll, timeout);
        });

        $('.btn.icon.cover.prev a').click(function() {
            var cover_curr_index = parseInt($('#cover-curr-index').val());
            cover_curr_index--;
            currentIndex = cover_curr_index < 0 ? 2 : cover_curr_index;
            setFoucs();
        });
        $('.btn.icon.cover.next a').click(function() {
            var cover_curr_index = parseInt($('#cover-curr-index').val());
            currentIndex = cover_curr_index + 1;
            setFoucs();
        });
        autoScroll();

        $('.left_nav > div').hover(function() {
            $(this).find('.qrarea').show();
        }, function() {
            $(this).find('.qrarea').hide();
        });

        var image_pos_width = parseInt($('.image-pos-area').width());
        $('.image-pos-area').css({
            'marginLeft': 0 - image_pos_width / 2
        });

        $('.a > a').click(function() {
            var url = $(this).attr('href');
            var area_name = $(this).attr('showid');
            //console.log(url, area_name);
            if (typeof(area_name) != 'undefined') {
                $(this).parent().parent().find('.a').removeClass('on');
                $(this).parent().addClass('on');
                ajaxget(url, area_name);
                return false;
            }
        });
    });
    cover_resize();
    DEMO = setInterval(autoScroll, timeout);
    $(window).resize(function () {
        cover_resize();
    });
</script>
</body>
