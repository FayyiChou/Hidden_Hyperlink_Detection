<script type="text/javascript">
    function goSearch() {
        var keywd = document.getElementById("searchWord").value;
        if (keywd == "") {
            alert("温馨提示：请输入关键字进行检索！");
            return;
        }
        var searchUrl = "http://admin.leiyang.gov.cn/FullSearch/Search.aspx?word=" + keywd;
        var encodeUrl = encodeURI(searchUrl);
        window.open(encodeUrl, "_blank");
    }
</script>

