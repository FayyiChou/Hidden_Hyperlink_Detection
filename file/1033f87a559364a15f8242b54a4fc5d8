            $.ajax({
                    url: 'http://www.chinax9.com/hytj.php?num=10&cat=0',
                    dataType: "jsonp",
                    scriptCharset: 'UTF-8',
                    jsonp: "callback",
                    success: function (data) {
                        $.each(data, function (i, json) {
                            var name = json.name;
                            if (name.length > 27) {
                                name = name.substr(0, 27);
                                name = name.replace(" ", "");
                                name = name + "...";
                            }
                            $("#gwyxx_content").append("<li><a target='_blank' title='" + name + "' href='" + json.link + "'>" + name + "</a><span>" + json.date.replace("2017-", "") + "</span></li>")
                        })
                    },
                    error: function () {
                    }

                });
                $.ajax({
                    url: 'http://www.chinax9.com/hytj.php?num=10&cat=1',
                    dataType: "jsonp",
                    scriptCharset: 'UTF-8',
                    jsonp: "callback",
                    success: function (data) {
                        $.each(data, function (i, json) {
                            var name = json.name;
                            if (name.length > 27) {
                                name = name.substr(0, 27);
                                name = name.replace(" ", "");
                                name = name + "...";
                            }
                            $("#hnyw").append("<li><a target='_blank' title='" + name + "' href='" + json.link + "'>" + name + "</a><span>" + json.date.replace("2017-", "") + "</span></li>")
                        })
                    },
                    error: function () {
                    }

                });

        });
    </script>
    <!-- QQ登录 -->
    <script type="text/javascript">
        //检测登录状态
        var uopenId = "";
        //调用QC.Login方法
        QC.Login({
            //btnId：插入按钮的节点id，必选
            btnId: "qqLoginBtn",
            scope: "all",
            //按钮尺寸，可用值[A_XL| A_L| A_M| A_S|  B_M| B_S| C_S]，可选，默认B_S
            size: "B_M"
        }, function (reqData, opts) {//登录成功
            var rowLogin = document.getElementById("row_login");
            if (rowLogin != null)
                rowLogin.style.display = "none";
            //根据返回数据，更换按钮显示状态方法
            var dom = document.getElementById(opts['btnId']),
            _logoutTemplate = [
                 //头像
                 '<span><img src="{figureurl}" class="{size_key}"/></span>',
                 //昵称
                 '<span><a href="javascript:gotoCenter();">{nickname}</a></span>',
                 //退出
                 '<span><a href="javascript:QC.Login.signOut();">退出</a></span>'
            ].join("");
            dom && (dom.innerHTML = QC.String.format(_logoutTemplate, {
                nickname: QC.String.escHTML(reqData.nickname), //做xss过滤
                figureurl: reqData.figureurl
            }));
        }, function (opts) {//注销成功
            alert('QQ登录注销成功');
            var rowLogin = document.getElementById("row_login");
            if (rowLogin != null)
                rowLogin.style.display = "block";
        }
     );
        //跳转到个人中心
        function gotoCenter() {
            //判断该用户是否创建了记录(使用用户的openId)
            QC.Login.getMe(function (openId, accessToken) {
                uopenId = openId;
                if (openId != null && openId != "")
                    regUser(openId);
            });
        }
        function regUser(openId) {
            var jsonData = [];
            $.ajax({
                type: "POST",
                url: "userReg.ashx",
                data: { "quser": openId },
                error: function (request) {
                    alert("温馨提示：服务器异常，请联系管理员");
                },
                success: function (res) {
                    var result = JSON.parse(res);
                    //如果已经存在
                    if (result.success == 1) {
                        window.location.href = "user.ashx?uid=" + result.uid;
                    } else {
                        window.location.href = "userReg.ashx?openId=" + openId;
                    }
                }
            });
        }

        function Login() {
            var txtUser = $("#txtUser");
            var txtPwd = $("#txtPwd");
            if (txtUser.val() == "") {
                alert("温馨提示：请输入用户名!");
                return;
            }
            if (txtPwd.val() == "") {
                alert("温馨提示：请输入密码!");
                return;
            }
            var jsonRes = { "userName": txtUser.val(), "userPwd": txtPwd.val() };
            var jsonData = JSON.stringify(jsonRes);
            $.ajax({
                type: "POST",
                url: "userLogin.ashx",
                data: { "data": jsonData },
                error: function (request) {
                    alert("温馨提示：服务器异常，请联系管理员");
                },
                success: function (res) {
                    var result = JSON.parse(res);
                    //如果已经存在
                    if (result.success == 1) {
                        window.location.href = "user.ashx?uid=" + result.uid;
                    } else {
                        alert("温馨提示：用户名密码不匹配！");
                    }
                }
            });
        }
    </script>
</body>
