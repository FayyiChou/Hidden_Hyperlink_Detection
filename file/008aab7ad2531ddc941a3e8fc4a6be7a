        //广告变暗效果

        $(function () {
            $(".index_showcase div").hover(
                function () {
                    $(".index_showcase div img").stop();
                    $(this).find("img").fadeTo("fast", 1);
                    $(this).siblings().find("img").fadeTo("fast", 0.5);
                },
                function () {
                    $(".index_showcase div img").stop();
                    $(this).find("img").fadeTo("fast", 0.5);
                    $(this).siblings().find("img").fadeTo("fast", 1);
                }
            );
            $(".index_showcase").mouseleave(function () { $(".index_showcase div img").stop().fadeTo("fast", 1); });
        })
    </script>

