    <script type="text/javascript">
        //大广告图
        $(function () {
            var tabindex = 0;
            var tabcount = $(".bigad .item").length;
            for (var i = 0; i < tabcount; i++) {
                $("<span></span>").text(i + 1).appendTo(".bigad .nav")
            }
            var tabcontrol = $(".bigad .nav span");
            var tabitems = $(".bigad .item");
            function showTab(i) {
                tabitems.stop();
                tabitems.eq(i).fadeIn(500).siblings(".item").fadeOut(500);
                tabcontrol.eq(i).addClass("cur").siblings().removeClass("cur");
            }
            tabcontrol.hover(
            function () {
                clearInterval(mytabTime);
                tabindex = tabcontrol.index(this);
                showTab(tabindex);
            },
            function () {
                mytabTime = setInterval(function () {
                    showTab(tabindex)
                    tabindex++;
                    if (tabindex == tabcount) { tabindex = 0; }
                }, 3000);
            });
            var mytabTime = setInterval(function () {
                showTab(tabindex)
                tabindex++;
                if (tabindex == tabcount) { tabindex = 0; }
            }, 3000);
            showTab(0);
        })
    </script>
    
