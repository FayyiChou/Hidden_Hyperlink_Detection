<script type="text/javascript">
    var context = '';
    userSettings.thumbnailsPerPage = 60;
    userSettings.previewVisible = false;
    userSettings.layoutImagesType = 'brief';

    $(document).ready(function() {

        
        
        
        
        userAllBasket.updateImageOperationsBasket(userAllBasket.cookieBasketTemp);
        
        

        // автоподстановка набираемого слова в строке поиска.

        $("#search-query").autocomplete({
            source : context + '/searchsuggestion.html', delay:10, minChars:2, matchSubset:1, autoFill:true, matchContains:1, cacheLength:10, selectFirst:true, maxItemsToShow:10});

        $(".menu-trigger").bind("mouseenter mouseleave", function(event) {
            var submenu = $("table.sub-menu", this);
            var submenuLabel = $("ul.sub-menu-label", this);
            if (event.type == 'mouseenter') {
                $(".menu-label", this).removeClass('menu-item-dsbld');
                $(".menu-label", this).addClass('menu-item-enbld');
                $(".menu-label", this).removeClass('menu-item-dsbld-bottom');
                $(".menu-label", this).addClass('menu-item-enbld-bottom');
                submenu.show();
                submenuLabel.show();
                var elem = $(this);
                var menuWidth = elem.width();
                var submenuWidth = $("ul.sub-menu li", this).width();
                if (submenuWidth < menuWidth && submenu.is(':visible')) {
                    $("ul.sub-menu li", this).width(menuWidth);
                }

            }
            else {
                $(".menu-label", this).removeClass('menu-item-enbld');
                $(".menu-label", this).addClass('menu-item-dsbld');
                submenu.hide();
                submenuLabel.hide();
            }
        });

        $(".custom-select").linkselect();


        /* Эффекты с формой в корзине */
        var triggers = $(".modalInput").overlay({

            // some mask tweaks suitable for modal dialogs
            mask: {
                color: '#000000',
                opacity: 0.4
            },

            closeOnClick: false
        });

        /* Подсветка главного меню

        var url = window.location.href;
        var page = new Array("collections", "creative", "editorial");
        var i, razdel;
        for (i=0;i<3;i++) {
            razdel = url.indexOf(page[i]);
            if(razdel > 0) {
                $("#"+page[i]).removeClass("collections-list-wrapper-li");
                $("#"+page[i]).addClass("collections-list-wrapper-li-selected");
            }
        }
        */



    });

</script>



