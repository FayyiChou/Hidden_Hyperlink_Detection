<script type="text/javascript">


    function onClickSearchBar() {

        var search_value = document.getElementById('search-query').value;
        if(search_value == "Поиск на английском языке даcт лучший результат")
        {
          document.getElementById('search-query').style.color = 'black';
          return '';
        }
        else
        {
          return search_value;
        }
    }
    </script>

