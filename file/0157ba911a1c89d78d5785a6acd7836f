	ymaps.ready(init);

	function init () {
		var myMap = new ymaps.Map('probkimap', {
				center: [54.624626,39.747508],
				zoom: 13
			});
		myMap.controls.add('smallZoomControl');

		// Создадим провайдер пробок "Сейчас" с включенным слоем инфоточек.
		var actualProvider = new ymaps.traffic.provider.Actual({}, { infoLayerShown: true });
		// И затем добавим его на карту.
		actualProvider.setMap(myMap);
	}
	</script>
	<script type="text/javascript">
	function selcity(city) {
		$.cookie("selectcity", city, {
			expires: 31,
			path: "/",
			domain: "www.taynavkusa.ru",
			secure: false
		});

		if (city != "" && city != 'www.taynavkusa.ru')
		{
			selcity('');
			setTimeout(function() { window.location = "http://" + city + "#TVSelectCity"; }, 100);
		}
		return false;
	}
	</script>

