<script type="text/javascript">
var blockCart = 1;
var selectOsnova = $('[data-remodal-id=modal]').remodal();
function CART() {
	$('.parametry').click(function(e){
		e.preventDefault();

		var ob = $(this),
			id = ob.attr('idf'),
			dsize = ob.attr('dsize'),
			param = ob.attr('ke'),
			cena = ob.attr('value'),
			ves = ob.text(),
			sp = $('#coststocart'+id),
			sp2 = $('#coststocart2'+id);

		$('.parametry'+id).removeClass('active');
		ob.addClass('active');
		$('#sizedish'+id).html(dsize);
		sp2.text(cena);
		sp.attr('value',cena);
		sp.attr('param',param);
		sp.attr('descr',ves);

		return false;
	});

	$("button.submit").click(function(e) {
		e.preventDefault();
		var ob = $(this),
			id = ob.attr('idf'),
			is_modal = ob.attr('data-remodal-target'),
			el = $('#coststocart'+id),
			kol = el.attr('kol'),
			param = el.attr('param');

		if ( is_modal == 'modal' )
		{
			// Выбор основы
			if ( blockCart == 1 )
			{
				blockCart = 2;

				$.ajax({
					type: "POST",
					dataType: "json",
					url: "/ajax/showOsnova",
					data: { good_id: id },
					cache: false,
					success: function(data) {

						$("#osnovaList").html("");
						$("#product_id").val("");
						$.each( data, function( key, val ) {

							$('<li><label><input type="radio" value="' + val['id'] + '" name="osnova" ' + val['checked'] + ' /> ' + val['title'] + ' – ' + val['dop_price'] + ' руб.</label></li>').appendTo("#osnovaList");
							$("#product_id").val(id);

						});

					}
				});
				selectOsnova.open();
				blockCart = 1;

			}
		}
		else
		{
			// Прямое добавление в корзину
			if ( blockCart == 1 )
			{
				blockCart = 2;

				$.post('/basket/add', {addkol:kol,product_id:id,addparam:param}, function(data){
					totalshow(data);

					// Инфо сообщение
					jNotify(
						'Блюдо добавлено в корзину',
						{
							autoHide : true,
							TimeShown : 1000,
							HorizontalPosition : 'center',
						}
					);

					blockCart = 1;
				},'html');
			}

		}
	});
}

$(document).on('confirmation', '.remodal', function () {
	var id = $("#product_id").val(),
		el = $('#coststocart'+id),
		kol = el.attr('kol'),
		param = el.attr('param');

	$.post('/basket/add', {addkol:kol,product_id:id,addparam:param,osnova_id:$("#osnovaList").find("input[name='osnova']:checked").val()}, function(data){
		totalshow(data);
		selectOsnova.close();

		// Инфо сообщение
		jNotify(
			'Блюдо добавлено в корзину',
			{
				autoHide : true,
				TimeShown : 1000,
				HorizontalPosition : 'center',
			}
		);

	},'html');
});

function totalshow(total) {
	if (total != 0)
	{
		$('#cart .korzina').html(total + ' р.');
	}
	else
	{
		$('#cart .korzina').html('Мой заказ');
	}
}
// Увеличение/уменьшение количества товара в корзине на 1 единицу
function intobasket(product_id, quantity) {
	var total = parseInt($('#quantity'+product_id).val());

	if (total >= 1)
	{
		if (quantity == 'increase')
		{
			// Увеличение количества товара на 1 единицу
			total = total+1;
		}
		else if (quantity == 'decrease' && total > 1)
		{
			// Уменьшение количества товара на 1 единицу
			total = total-1;
		}

		$('#quantity'+product_id).val(total);
		$('#coststocart'+product_id).attr('kol', total);
	}
}

// Инфо заказа
function orderdetails(product_id) {
	$("#cart_summary tr.table_order").hide();
	$("#order" + product_id).slideToggle("slow");
}

function showTimeRegion()
{
	$.ajax({
		url: "/ajax/showTimeDelivery",
		cache: false,
		success: function(data) {
			get = data.split('|');

			if ( $("#kitchen_center").hasClass(get[0]) == false )
			{
				$("#kitchen_center").removeClass().addClass('raion ' + get[0]);
				$(".loaded_center").html(get[1]);
				$(".descr_center").html(get[2]);
				$("#kitchen_center").flip('toggle');
			}

			if ( $("#kitchen_kanishego").hasClass(get[3]) == false )
			{
				$("#kitchen_kanishego").removeClass().addClass('raion ' + get[3]);
				$(".loaded_kanishego").html(get[4]);
				$(".descr_kanishego").html(get[5]);
				$("#kitchen_kanishego").flip('toggle');
			}

			if ( $("#kitchen_dashki").hasClass(get[6]) == false )
			{
				$("#kitchen_dashki").removeClass().addClass('raion ' + get[6]);
				$(".loaded_dashki").html(get[7]);
				$(".descr_dashki").html(get[8]);
				$("#kitchen_dashki").flip('toggle');
			}

		}
	});
}
$(document).ready(function() {

	CART();

	$('#cart').click(function(){
		window.location = "/basket";
	});

	$("#phone").inputmask("(999) 999-9999");
	$("#phone_checkorder").inputmask("(999) 999-9999");

	$(".selectcity").fancybox({
		'width' : '400px',
		'height' : '250px',
		'padding': '20px',
		'type': 'inline',
		'centerOnScroll': true,
		'autoCenter' : true,
		afterClose : function() {
			selcity('www.taynavkusa.ru');
			return;
		}
	});

	if ($.cookie("selectcity") == undefined)
	{
		$(".selectcity").click();
	}
	else
	{
		if ($.cookie("selectcity").length > 0 && $.cookie("selectcity") != 'www.taynavkusa.ru')
		{
			selcity('');
			var url_city = "http://" + $.cookie("selectcity");
			window.location = url_city;
		}
	}

	var a = window.location.hash,
		b = a.indexOf("codesms"),
		c = a.indexOf("codepass");
	if (a && b > 0) {
		$("#general").hide();
		$("#gencode").fadeIn();
		$("#act").val(2);
		$("#smscode").focus();
	}
	if (a && c > 0) {
		$("#general").hide();
		$("#genpass").fadeIn();
		$("#act").val(3);
		$("#pass").focus();
	}

	$('#login').submit(function() {
		if ($("#act").val() == 1)
		{
			$.post('/cabinet/auth/1', { phone: $("#phone").val() }, function(data, textStatus) {
				data_arr = data.split("|");

				if (textStatus == "success" && data_arr[0] == 1) {
					// Если код == 1, то выводить ошибку
					if ($("#error").size() == 0 && data_arr[1] != '')
					{
						jError(
							data_arr[1],
							{
								autoHide : false,
								HorizontalPosition : 'center',
							}
						);
					}
				}
				else if (textStatus == "success" && data_arr[0] == 3)
				{
					// Если код == 3, то информационное сообщение
					if ($("#info").size() == 0 && data_arr[1] != '')
					{
						jNotify(
							data_arr[1],
							{
								autoHide : true, // added in v2.0
								TimeShown : 10000,
								HorizontalPosition : 'center',
							}
						);
					}
				}
				else if (textStatus == "success" && data_arr[0] == 2) {
					$("#general").hide();
					$("#gencode").fadeIn();
					window.location.hash = "codesms";
					$("#act").val(2);
					$("#smscode").focus();
				}
				else if (textStatus == "success" && data_arr[0] == 4) {
					$("#general").hide();
					$("#genpass").fadeIn();
					window.location.hash = "codepass";
					$("#act").val(3);
					$("#pass").focus();
				}
				else if (textStatus == "success" && data_arr[0] == 5) {
					$("#general").hide();
					$("#gencode").fadeIn();
					window.location.hash = "codesms";
					$("#act").val(2);
					$("#smscode").focus();
				}

			});
		}
		else if ($("#act").val() == 2)
		{
			$.post('/cabinet/auth/2', { smscode: $("#smscode").val() }, function(data, textStatus) {
				data_arr = data.split("|");

				if (textStatus == "success" && data_arr[0] == 4) {
					window.location = "/cabinet/pass";
				}
				else if (textStatus == "success" && data_arr[0] == 1) {

					$.get('/cron/countdown.php', { id: data_arr[2] }, function(data) {});

					// Если код == 1, то выводить ошибку
					if ($("#error").size() == 0 && data_arr[1] != '')
					{
						jError(
							data_arr[1],
							{
								autoHide : false,
								HorizontalPosition : 'center',
							}
						);
					}
				}
				else if (textStatus == "success" && data_arr[0] == 3)
				{
					// Если код == 3, то информационное сообщение
					if ($("#info").size() == 0 && data_arr[1] != '')
					{
						jNotify(
							data_arr[1],
							{
								autoHide : true, // added in v2.0
								TimeShown : 10000,
								HorizontalPosition : 'center',
							}
						);
					}
				}
				else if (textStatus == "success" && data_arr[0] == 2) {
					window.location = "/cabinet";
				}
				else if (textStatus == "success" && data_arr[0] == 5) {
					window.location = "/cabinet/pass";
				}
			});
		}
		else if ($("#act").val() == 3)
		{
			$.post('/cabinet/auth/3', { pass: $("#pass").val() }, function(data, textStatus) {
				data_arr = data.split("|");

				if (textStatus == "success" && data_arr[0] == 1) {

					$.get('/cron/countdown.php', { id: data_arr[2] }, function(data) {});

					// Если код == 1, то выводить ошибку
					if ($("#error").size() == 0 && data_arr[1] != '')
					{
						jError(
							data_arr[1],
							{
								autoHide : false,
								HorizontalPosition : 'center',
								clickOverlay: true
							}
						);
					}
				}
				else if (textStatus == "success" && data_arr[0] == 3)
				{
					// Если код == 3, то информационное сообщение
					if ($("#info").size() == 0 && data_arr[1] != '')
					{
						jNotify(
							data_arr[1],
							{
								autoHide : true, // added in v2.0
								TimeShown : 10000,
								HorizontalPosition : 'center',
							}
						);
					}
				}
				else if (textStatus == "success" && data_arr[0] == 2) {
					window.location = "/cabinet";
				}
			});
		}
		return false;
	});

	$("#forgot").live('click', function(){
		$.post('/cabinet/forgot', { phone: $("#phone").val() }, function(data, textStatus) {
			data_arr = data.split("|");

			if (textStatus == "success" && data_arr[0] == 1) {
				// Если код == 1, то выводить ошибку
				if ($("#error").size() == 0 && data_arr[1] != '')
				{
					jError(
						data_arr[1],
						{
							autoHide : true, // added in v2.0
							TimeShown : 10000,
							HorizontalPosition : 'center',
						}
					);
				}
			}
			else if (textStatus == "success" && data_arr[0] == 3)
			{
				// Если код == 3, то информационное сообщение
				if ($("#info").size() == 0 && data_arr[1] != '')
				{
					jNotify(
						data_arr[1],
						{
							autoHide : true, // added in v2.0
							TimeShown : 10000,
							HorizontalPosition : 'center',
						}
					);
				}
			}
			else if (textStatus == "success" && data_arr[0] == 2) {
				$("#general, #genpass").hide();
				$("#gencode").fadeIn();
				window.location.hash = "codesms";
				$("#act").val(2);
				$("#smscode").focus();
			}

		});

	});

	$('.promo .slides').cycle({
		fx: 'fade',
		speed: 700,
		timeout: 5000,
		slideExpr: 'section',
		next: '.promo .btn-promo-prev',
		prev: '.promo .btn-promo-next',
		pager: '.promo .pager'
	});

	$("#carousel-new").carouFredSel({
		items	: 3,
		scroll	: {
			items			: 1,
			duration		: 500,
			timeoutDuration	: 5000
		},
		auto	: {
			items 			: 1,
			duration		: 500,
			timeoutDuration	: 5000,
			pauseOnHover	: "immediate"
		},
		prev	: "#carousel_prev-new",
		next	: "#carousel_next-new"
	});
	$("#carousel-pop").carouFredSel({
		items	: 3,
		scroll	: {
			items			: 1,
			duration		: 500,
			timeoutDuration	: 5000
		},
		auto	: {
			items 			: 1,
			duration		: 500,
			timeoutDuration	: 5000,
			pauseOnHover	: "immediate"
		},
		prev	: "#carousel_prev-pop",
		next	: "#carousel_next-pop"
	});
	$('nav#left-menu a').toggle(function(){
		if ($(this).attr('class')=='level1')
		{
			window.location.href = $(this).attr('href');
		}
		else
		{
			if ($(this).parent().parent().attr('class')=='sub-menu')
			{
				window.location.href = $(this).attr('href');
			}
			else
			{
				$('nav#left-menu a').closest('li').removeClass("active");
				$('nav#left-menu a').closest('li').find('ul:first').slideUp('fast');

				$(this).closest('li').addClass("active");
				$(this).closest('li').find('ul:first').slideDown();
			}
		}
	},
	function(){
		$(this).closest('li').find('ul:first').slideUp();
	});

	$('a.selected').parent().parent().css('display', 'block');
	$('a.selected').parent().parent().parent().parent().css('display', 'block');

	$('#fancybox').fancybox();
	$("#settings").hoverIntent({
		over: function(){
			$("#profile-hover").fadeIn('fast');
		},
		out: function(){
			$("#profile-hover").fadeOut('fast');
		}
	});

	$("#profile-info").slideDown();	

	$("ul.tabs-login").idTabs();

	$('#responses_button').fancybox({
		'maxWidth' : 630,
		'maxHeight' : 670,
		'padding': '20',
		'type': 'iframe',
		'centerOnScroll': true
	});

	$(".openmaps").fancybox({
		maxWidth	: 600,
		maxHeight	: 700,
		fitToView	: false,
		width		: '100%',
		height		: '100%',
		autoSize	: false,
		closeClick	: false,
		openEffect	: 'none',
		closeEffect	: 'none'
	});

	$(".info_order").fancybox({
		'width' : '400px',
		'height' : '250px',
		'padding': '20px',
		'type': 'inline',
		'centerOnScroll': true,
		'autoCenter' : true,
	});
	$("#kitchen_center, #kitchen_kanishego, #kitchen_dashki").flip({
		trigger: 'manual',
		reverse: true
	});
	setInterval('showTimeRegion()', 60000);

	// Проверка статуса заказа
	$("#checkorder").submit(function(){
		$.post('/checkorder', { phone_checkorder: $("#phone_checkorder").val() }, function(data, textStatus) {
			data_arr = data.split("|");

			if (textStatus == "success" && data_arr[0] == 1) {
				// Если код == 1, то выводить ошибку
				if ($("#error").size() == 0 && data_arr[1] != '')
				{
					jError(
						data_arr[1],
						{
							autoHide : true, // added in v2.0
							TimeShown : 10000,
							HorizontalPosition : 'center',
						}
					);
				}
			}
			else if (textStatus == "success" && data_arr[0] == 3)
			{
				// Если код == 3, то информационное сообщение
				if ($("#info").size() == 0 && data_arr[1] != '')
				{
					jNotify(
						data_arr[1],
						{
							autoHide : true, // added in v2.0
							TimeShown : 10000,
							HorizontalPosition : 'center',
						}
					);
				}
			}
			else if (textStatus == "success" && data == '') {
				window.location = "/checkorder/orders";
			}

		});

	});
});
</script>

