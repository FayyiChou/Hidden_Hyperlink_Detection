				<script type="text/javascript">
                    function switchTab2(m){
                        for(var x = 1; x <= 3; x++){
                            document.getElementById("tab2_" + x).className = "";
                            document.getElementById("tab2_con_" + x).style.display = "none";
                        }
                        document.getElementById("tab2_" + m).className = "on";
                        document.getElementById("tab2_con_" + m).style.display = "block";
                    }
                    $(document).ready(function() {
                        $("div.newsbox a[href$='.pdf']").each(function(){
                            $(this).fancybox({
                                'transitionIn':'elastic',
                                'transitionOut':'elastic',
                                'titlePosition':'over',

                                'titleFormat':function(title, currentArray, currentIndex, currentOpts) {
                                    return '<span id="fancybox-title-over">信息 ' + (currentIndex + 1) + ' / ' + currentArray.length + (title.length ? ' &nbsp; ' + title : '') + '</span>';
                                }
                            });
                        });

                    });
                </script>
