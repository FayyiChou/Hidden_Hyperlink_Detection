                    function switchTab3(n){
                        for(var i = 1; i <=2; i++){
                            document.getElementById("tab3_" + i).className = "";
                            document.getElementById("tab3_con_" + i).style.display = "none";
                        }
                        document.getElementById("tab3_" + n).className = "on";
                        document.getElementById("tab3_con_" + n).style.display = "block";
                    }
