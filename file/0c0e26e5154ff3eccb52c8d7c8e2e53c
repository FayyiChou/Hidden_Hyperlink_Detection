    <script>
        var grade = -1;
        var UserGrade = -2; //年级
        var region = -1; //用户权限 0:student ;1:teacher; 2:school;
        var isLogin = 0; //是否登录
        var isshiyanqu = 2; //是否是实验区 1:实验区，2：非实验区
        var step1 = 0, step2 = 0;
        var prvCode = -1;
        var cityId = -1;
        var countryId = -1;
        var comefrom = -1;
        var ipArea = "";
        var cityname = "";
        var classroom = -1;
        var schoolid = -1;
        var recordSort = 1;
        var chooseList = [];
        var comefromcss = "http://file.safetree.com.cn/special/90/push.css"; //微信单独样式
        $(function () {
            WebServes.checkUserLoginNoLayer(function (json) {
                //已登录
                if (json != null && json.userid > -1) {
                    isLogin = 1;
                    comefrom = json.comefrom;
                    prvCode = json.prvid;
                    cityId = json.cityid;
                    countryId = json.coutryid;
                    grade = json.grade;
                    schoolid = json.schoolid;
                    classroom = json.classroom;
                    switch (json.region) {//获取用户权限
                        case 0:
                            //student
                            region = 0;
                            $("#work2").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                            $("#btn1").show();
                            $("#btn2").show();
                            break;
                        case 9:
                        case 10:
                            //teacher
                            region = 1;
                            $("#work1").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                            break;
                        case 20:
                        case 21:
                            //school
                            region = 2;
                            $("#work1").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_nwc.png'>");
                            break;
                        default:
                            //others
                            region = 3;
                            break;
                    }
                    var registerUrl = getCookie("ServerSide"); //所属站点
                    //非实验区 没有获取到cookies,调用方法去主库获取
                    if (registerUrl == "-1" || registerUrl == null) {
                        isshiyanqu = 2;
                    } else {
                        isshiyanqu = 1;
                    }
                    ShowDCWJ(json.truename, cityId, json.region, json.SchoolProperty, json.LengthOfSchooling, grade);
                    showFinishStatus();
                }
                weixin(comefrom, function (a, b, c) {
                    comefrom = a, cityname = b, ipArea = c;
                    $("#weixin_cityname").append("教育百师通" + cityname);
                    if (comefrom >= 0) {
                        $("#wxbox-img1").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                        $("#wxbox-img2").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                        $("#wxbox-img4").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();")
                    }
                });

                $(".message-ul").each(function () {
                    var _this = this;
                    var t2 = 0;
                    t2 = setInterval(function () {
                        var b1 = $(_this).find('li:first');
                        var b2 = $(_this).find('li:last');
                        $(b1).insertAfter($(b2));
                    }, 3000);
                });
            });

            //点击完成情况按钮按钮
            $("#FinishSituation").click(function () {
                if (region > 0) {
                    window.location.href = "/2016FirstClass_Execution_school.html";
                } else {
                    window.location.href = "/2016FirstClass_Execution_family.html";
                }
            });
            loadQuestion(0, 7, 1, false);
        });


        //加载问卷、测试题
        function loadQuestion(index, size, k, must) {
            if (index > 0) {
                if (isLogin == 0) {
                    special.showLogin();
                    return;
                }
                if (region != 0) {
                    alert("只有学生才能参与活动进行答题！");
                    return;
                }
            }
            if (must) {
                if (!questionMust()) {
                    return;
                }
            }
            if (index == 8) {
                location.href = "#top";
            }
            var html = k > 0 && index == 0 ? "" : "";
            var startIndex = index;
            var endIndex = k > 0 ? index + size : index - size;
            var i = 0;
            if (k > 0) {
                for (i = 0; i < _QuestionList.length; i++) {
                    html += _QuestionList[i].html;
                }
            }
            html += '<dl><a href="javascript:;" class="tj_btn" onclick="submitQ()" id="tijiao1">提交</a><a href="javascript:;" class="tj_btnno" style="display:none" id="tijiao2" >正在提交...</a><a href="javascript:;" class="tj_btnno"  style="display:none" id="tijiao3">已提交</a></dl>';
            $('#questionlist').html(html);
            if (html.indexOf("text") >= 0) {
                lowIE10();
            }
            isShowTip = false;
            for (i = startIndex; i < endIndex && i < _QuestionList.length; i++) {
                var c = chooseList[i];
                if (c != undefined && c.N != "") {
                    if (_QuestionList[i].type == "checkbox") {
                        var aList = c.N.split(',');
                        for (var j = 0; j < aList.length; j++) {
                            if (aList[j] == 1) {
                                $('#radio_' + (i + 1) + '_' + (j + 1)).click();
                                $('#Q' + (c.ID) + ' :radio[value="' + (j + 1) + '"]').click();
                            }
                        }
                    } else {
                        $('#radio_' + (i + 1) + '_' + c.N).click();
                    }
                    if (c.M.trim() != "") {
                        $('#Q' + (c.ID) + ' :text').val(c.M);
                    }
                }
            }
            isShowTip = true;
            //radioclick();
            qitaTxtInput();
        }

        //判断是否有漏题
        function questionMust() {
            var qNum = 0;
            var aNum = 0;
            $('.test_question dl').each(function () {
                if ($(this).is(":visible")) {
                    if ($(this).find('[type="radio"]').length > 0 || $(this).find('[type="checkbox"]').length > 0) {
                        qNum++;
                    }
                    $(this).find('[type="radio"]').each(function () {
                        if ($(this).attr('checked') != undefined) {
                            aNum++;
                            return false;
                        }
                    });
                    $(this).find('[type="checkbox"]').each(function () {
                        if ($(this).attr('checked') != undefined) {
                            aNum++;
                            return false;
                        }
                    });
                }
            });
            if (qNum != aNum) {
                alert("每一道题都需要选择答案！");
                return false;
            }
            return true;
        }

        function choose(index, $this, showQid) {
            if (isLogin == 0) {
                special.showLogin();
                return;
            }
            if (region != 0) {
                alert("只有学生才能参与活动进行答题！");
                return;
            }
            if (isShowTip) {
                var u = getCookie("UserID");
                if (getCookie(u + "_2016fc2")) {
                    alert('您已参加过问卷调查，不可再次参与!');
                    return;
                }
            }
            setTimeout(function () {
                var checkedStr = "";
                var mStr = "";
                $('#Q' + index + " input:checkbox").each(function () {
                    if ($(this).attr("checked")) {
                        if ($(this).val() == "9" && index == 5) {
                            $("#ThemeTxt").show();
                            $("#NS").show();
                        }
                        checkedStr += "1,";
                        var text = $(this).parent().find(":text");
                        if (text.length > 0) {
                            mStr = text.val().trim();
                        }
                    } else {
                        checkedStr += "0,";
                        mStr = "";
                        if ($(this).val() == "9" && index == 5) {
                            $("#ThemeTxt").val('');
                            $("#ThemeTxt").hide();
                            $("#NS").hide();
                        }
                    }
                });
                if (checkedStr.length > 0) {
                    checkedStr = checkedStr.substring(0, checkedStr.length - 1);
                }
                if ($('#Q' + index + " input:radio:checked").length > 0) {
                    checkedStr = $('#Q' + index + " input:radio:checked").val();
                }
                chooseList[index - 1] = { ID: index, N: checkedStr, M: mStr }
                if (index == 6 && chooseList.length == 6) {
                    chooseList[index] = { ID: index + 1, N: '0', M: '' }
                }
            }, 100);
        }

        function qitaTxtInput() {
            $('#ThemeTxt').keyup(function () {
                var dlId = $(this).attr('dlId');
                if (!$(this).parent("").find(":checkbox").attr("checked")) {
                    chooseList[dlId - 1] = { ID: dlId, N: '', M: '' }
                    $(this).parent("").find(":checkbox").click();
                }
                chooseList[dlId - 1].M = $.trim($(this).val());
                if ($.trim($(this).val()).length < 30) {
                    $("#NS").html($.trim($(this).val()).length + "/30");
                } else {
                    $("#NS").html("<span style='color:red;'>" + $.trim($(this).val()).length + "</span>/30");
                }
            });

            $('#ThinkTxt').keyup(function () {
                var dlId = $(this).attr('dlId');
                if (!$(this).parent("").find(":checkbox").attr("checked")) {
                    chooseList[dlId - 1] = { ID: dlId, N: '', M: '' }
                    $(this).parent("").find(":checkbox").click();
                }
                chooseList[dlId - 1].M = $.trim($(this).val());
                chooseList[dlId - 1].N = "0";
                if ($.trim($(this).val()).length < 200) {
                    $("#TS").html($.trim($(this).val()).length + "/200");
                } else {
                    $("#TS").html("<span style='color:red;'>" + $.trim($(this).val()).length + "</span>/200");
                }
            });

        }

        function lowIE10() {
            //判断浏览器是否支持placeholder属性
            var supportPlaceholder = 'placeholder' in document.createElement('textarea');
            var placeholder = function (input) {
                var text = input.attr('placeholder'),
                    defaultValue = input.defaultValue;
                if (!defaultValue) {
                    input.val(text).addClass("phcolor");
                }
                input.focus(function () {
                    if (input.val() == text) {
                        $(this).val("");
                    }
                });
                input.blur(function () {
                    if (input.val() == "") {
                        $(this).val(text).addClass("phcolor");
                    }
                });
                //输入的字符不为灰色
                input.keydown(function () {
                    $(this).removeClass("phcolor");
                });
            };
            //当浏览器不支持placeholder属性时，调用placeholder函数
            if (!supportPlaceholder) {
                $('textarea').each(function () {
                    placeholder($(this));
                });
            }
        }

        //测试提交
        function submitQ() {
            var u = getCookie("UserID");
            if (!questionMust()) {
                return;
            }
            setCookie(u + "_2016fcTest", JSON.stringify(chooseList));
            if ($("#radio_5_9").attr("checked") == "checked" && $.trim($("#ThemeTxt").val()) == "") {
                alert("选择‘其他’必须填写内容才能提交");
                return;
            }
            $('#tijiao1').hide();
            $('#tijiao2').show();
            try {
                if (isshiyanqu == 1) {
                    SubmitTest(JSON.stringify(chooseList), function (sRes) {
                        if (sRes != null) {
                            if (sRes.Status == "True") {
                                setCookie(u + "_2016fc2", 1, 365);
                                $.post(Asmx + "/SubmitTest", { r: Math.random(), AnswerJson: JSON.stringify(chooseList), Prv: prvCode, City: cityId, County: countryId, School: schoolid, Grade: grade, Class: classroom, Comefrom: comefrom }, function (res) {
                                    if (res != null) {
                                        if (res.Status == "True") {
                                            $('#tijiao2').hide();
                                            $('#tijiao3').show();
                                            $('#btn2').attr('class', 'y-wc');
                                            $('#step2').html("已完成");
                                            setCookie(u + "_zk2016fc2", 1, 365);
                                            //提交成功 判断是否有错题记录 如果有：提示错题，否则：提示成功
                                            alert(res.Msg);
                                            if (getCookie(u + "_2016fc1") != "" && getCookie(u + "_2016fc1") != null && getCookie(u + "_2016fc2") != "" && getCookie(u + "_2016fc2") != null) {
                                                $("#work2").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                                            }
                                        } else {
                                            res.Msg.length > 0 && alert(res.Msg);
                                            $('#tijiao2').hide();
                                            $('#tijiao1').show();
                                        }
                                    }
                                }, "json");
                            } else {
                                $('#tijiao2').hide();
                                $('#tijiao1').show();
                                sRes.Msg.length > 0 && alert(sRes.Msg);
                            }
                        }
                    });
                } else {
                    $.post(Asmx + "/SubmitTest", { r: Math.random(), AnswerJson: JSON.stringify(chooseList), Prv: prvCode, City: cityId, County: countryId, School: schoolid, Grade: grade, Class: classroom, Comefrom: comefrom }, function (res) {
                        if (res != null) {
                            if (res.Status == "True") {
                                $('#tijiao2').hide();
                                $('#tijiao3').show();
                                //提交成功
                                $('#btn2').attr('class', 'y-wc');
                                $('#step2').html("已完成");
                                setCookie(u + "_2016fc2", 1, 365);
                                setCookie(u + "_zk2016fc2", 1, 365);
                                //提交成功 判断是否有错题记录 如果有：提示错题，否则：提示成功
                                alert(res.Msg);
                                if (getCookie(u + "_2016fc1") != "" && getCookie(u + "_2016fc1") != null && getCookie(u + "_2016fc2") != "" && getCookie(u + "_2016fc2") != null) {
                                    $("#work2").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                                }
                            } else {
                                res.Msg.length > 0 && alert(res.Msg);
                                $('#tijiao2').hide();
                                $('#tijiao1').show();
                            }
                        }
                    }, "json");
                }
            } catch (e) {
                alert("网络异常！请稍后尝试");
                $('#tijiao2').hide();
                $('#tijiao1').show();
            }

        }

        //判断完成状态
        function showFinishStatus() {
            var u = getCookie("UserID");
            if (isLogin) {  //登录状态
                if (region == 0) {
                    if (getCookie(u + "_2016fc1") == null || getCookie(u + "_2016fc2") == null) {
                        if (isshiyanqu == 1) { //实验区 
                            GetFinishStatus(function (fres) {
                                if (fres != null) {
                                    if (fres.StepOne == "True" && fres.StepTwo == "True") {
                                        $('#btn1').attr('class', 'y-wc');
                                        $('#step1').html('已完成');
                                        $('#btn2').attr('class', 'y-wc');
                                        $('#step2').html('已完成');
                                        setCookie(u + "_2016fc1", 1, 365);
                                        setCookie(u + "_2016fc2", 1, 365);
                                        $("#work2").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                                    }
                                    if (fres.StepOne == "True") {
                                        $('#btn1').attr('class', 'y-wc');
                                        $('#step1').html("已完成");
                                        setCookie(u + "_2016fc1", 1, 365);
                                    }
                                    if (fres.StepTwo == "True") {
                                        $('#btn2').attr('class', 'y-wc');
                                        $('#step2').html("已完成");
                                        setCookie(u + "_2016fc2", 1, 365);
                                    }
                                }
                            }, "json");
                        } else { //非实验区
                            $.getJSON(Asmx + "/GetFinishStatus?jsoncallback=?", { r: Math.random() }, function (res) {
                                if (res != null) {
                                    if (res.StepOne == "True" && res.StepTwo == "True") {
                                        $('#btn1').attr('class', 'y-wc');
                                        $('#step1').html('已完成');
                                        $('#btn2').attr('class', 'y-wc');
                                        $('#step2').html('已完成');
                                        setCookie(u + "_2016fc1", 1, 365);
                                        setCookie(u + "_2016fc2", 1, 365);
                                        setCookie(u + "_zk2016fc1", 1, 365);
                                        setCookie(u + "_zk2016fc2", 1, 365);
                                        $("#work2").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                                    }
                                    if (res.StepOne == "True") {
                                        $('#btn1').attr('class', 'y-wc');
                                        $('#step1').html("已完成");
                                        setCookie(u + "_2016fc1", 1, 365);
                                    }
                                    if (res.StepTwo == "True") {
                                        $('#btn2').attr('class', 'y-wc');
                                        $('#step2').html("已完成");
                                        setCookie(u + "_2016fc2", 1, 365);
                                    }
                                }
                            });
                        }
                    } else { //两个步骤都完成
                        $('#btn1').attr('class', 'y-wc');
                        $('#step1').html('已完成');
                        $('#btn2').attr('class', 'y-wc');
                        $('#step2').html('已完成');
                        $("#work2").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                    }
                } else {
                    if (getCookie(u + "_2016fc1") == null) {
                        if (isshiyanqu == 1) { //实验区
                            GetFinishStatus(function (fres) {
                                if (fres != null) {
                                    if (fres.StepOne == "True") {
                                        setCookie(u + "_2016fc1", 1, 365);
                                        $("#work1").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                                    }
                                }
                            }, "json");
                        } else { //非实验区
                            $.getJSON(Asmx + "/GetFinishStatus?jsoncallback=?", { r: Math.random() }, function (res) {
                                if (res != null) {
                                    if (res.StepOne == "True") {
                                        setCookie(u + "_zk2016fc1", 1, 365);
                                        $("#work1").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                                    }
                                }
                            });
                        }
                    } else {
                        $("#work1").html('<img src="http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png">');
                    }
                }
            }
        }

        //开学调查问卷弹层
        function ShowDCWJjjj(TrueName, CityID, Region, SchoolProperty, LengthOfSchool, Grade) {
            try {
                var DCWJCityAry = [370100, 370200, 320100, 440001, 150010, 20004, 330100, 510100, 120023, 440300]; //活动城市
                var NowDate = new Date(); //当前时间
                var EndDate = new Date('2016/09/28 23:59:59'); //结束时间
                var FinishStatus = getCookie("DCWJ2016") == getCookie("UserID"); //为True则表示肯定已经参与过了  False表示可能参与过也可能没参与过
                if (NowDate < EndDate && Region <= 10 && SchoolProperty == 1 && $.inArray(parseInt(CityID), DCWJCityAry) > -1 && !FinishStatus) {
                    if ((Region == 0 && ((LengthOfSchool == 2 && (Grade == 6 || Grade == 7)) || (LengthOfSchool != 2 && (Grade == 7 || Grade == 8)) || Grade == 1)) || (Region == 10 && Grade >= 1 && Grade <= 9)) {
                        $.getJSON(
                        "http://dcwj.safetree.com.cn/Questionnaire.asmx/QuestionnaireStatus?jsoncallback=?",
                        { r: Math.random() },
                        function (res) {
                            if (res != null && res != "" && res.Status == "False") {
                                $("#DCWJName").html(TrueName);
                                show("survey-modal");
                            }
                        })
                    }
                }
            } catch (e) {
                alert(e);
            }
        }
    </script>


