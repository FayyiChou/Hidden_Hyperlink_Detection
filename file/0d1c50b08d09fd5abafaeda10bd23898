if(typeof(Counter)!='undefined') document.write(s_counter(Counter)); 
function s_counter(p_id) {
   var l_path="http://www.fullyou.com.tw/img/counter/" 
   var l_length="6" 
   var str='',c,f 
   for(var i=0;i<p_id.length;i++) {
      c = p_id.charAt(i); 
      f = l_path+c+"_2"+".gif" 
      str+= "<img src='"+f+"' border=0>"
   }
   for(var i=l_length-p_id.length;i>0;i--) {
      f = l_path+"0_2"+".gif" 
      str= "<img src='"+f+"' border=0>"+str
   }
   return str 
}
