		else if(document.cookie.lastIndexOf(closeStr) > -1)

			tmpObj.MENU_OFF = 1

	}

	else {

		if(tmpObj.MENU_SWITCH_DEF == "OPEN") {

			tmpObj.MENU_OFF=0

