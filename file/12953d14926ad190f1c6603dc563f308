  <script type="text/javascript">
function search_chk(search_wd) {
	var search_wd = document.getElementById(search_wd);

	if (search_wd.value == '' || search_wd.value == '请输入要搜索的内容') {
		alert('请输入关键字!');
		return false;
	}
}

function clearvalue(id) {
	var search_wd = document.getElementById(id);
	search_wd.value = '';
}
</script>
    <form name="form2" method="post" action="Search.asp" onsubmit="return search_chk('search_wd')">
