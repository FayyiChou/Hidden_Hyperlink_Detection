$(document).ready(function($){

  function show(){

  $(".kfpopupdiv").show();
}

 setTimeout(show,5000);
  
  $("#kfpopupdiv_close").click(function(event) {
    $(this).parent().parent().css({"display":"none"});
    $("#kfpopupdiv").css({"display":"none"});
  });

    $("#kfpopupdiv_next").click(function(event) {
    $(this).parent().parent().css({"display":"none"});
    $("#kfpopupdiv").css({"display":"none"});
  });
  
  

  
});


</script> 
