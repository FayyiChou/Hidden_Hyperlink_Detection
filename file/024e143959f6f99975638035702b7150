		  var tid=-444,cid;
		  if(tid==9 || tid==10 || tid==11 || tid==12 || tid==13 || tid==22 || tid==23){
			  cid=1
		  }else if(tid==1 || tid==2 || tid==3 || tid==4 || tid==5 || tid==6 || tid==24){
			  cid=2
		  }else if(tid==7){
			  cid=3
		  }else if(tid==8){
			  cid=4
		  }else if(tid==27){
			  cid=5
		  }else{
			  cid=0
		  }
	  }
	  $(function(){
		  $(".menulistA li").removeClass("current").eq(cid).addClass("current")
		  if(cid<4){
			  $(".index-tags").hide().eq(cid).show();
		  }else{
			  $(".index-tags").hide().eq(0).show();
		  }
	  });
