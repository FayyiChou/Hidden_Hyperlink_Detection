  <div class="hover" style="padding:10px; border:1px solid #CCC; margin-left:8px;"><a href="privacy.asp" class="f_t_26_r">Privacy</a></div>
  <!--<div class="hover" style="padding:10px; border:1px solid #CCC;margin-left:8px;"><a href="#" class="f_t_26_r">Webmail  </a></div>-->
