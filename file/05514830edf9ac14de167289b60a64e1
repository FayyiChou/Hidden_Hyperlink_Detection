<div class="floor" floor="4" color="#e31939">
		<div class="floor-layout">
									<div class="floor-con">
				<div class="floor-title">
					<h2>
						<span> 4F </span>
						<a href="category-404-b0.html" target="_blank">监控、安防产品</a>
					</h2>
                    <ul class="floor-tabs-nav">
						<li class="floor-tabs-selected">
							<h3 style="border-color: #e31939 #e31939 #fff; color: #e31939;">精挑细选</h3>
						</li>
																		<li class="">
							<h3>监控设备</h3>
						</li>
												<li class="">
							<h3>监控配件</h3>
						</li>
												<li class="">
							<h3>监控线材</h3>
						</li>
											</ul>
				</div>
                <script type="text/javascript">
				$(function() {
					var sWidth = $("#focus_404").width(); //获取焦点图的宽度（显示面积）
					var len = $("#focus_404 ul li").length; //获取焦点图个数
					var index = 0;
					var picTimer;
				     //以下代码添加数字按钮和按钮后的半透明条，还有上一页、下一页两个按钮
					var btn = "<div class='btn'>";
				
					for(var i=0; i < len; i++) {
						btn += "<span></span>";
					}
					btn += "</div>";
					$("#focus_404").append(btn);
					$("#focus_404 .btnBg").css("opacity",0.5);
				
					//为小按钮添加鼠标滑入事件，以显示相应的内容
					$("#focus_404 .btn span").css("opacity",0.3).mouseover(function() {
						index = $("#focus_404 .btn span").index(this);
						showPics(index);
					}).eq(0).trigger("mouseover");
				
					//本例为左右滚动，即所有li元素都是在同一排向左浮动，所以这里需要计算出外围ul元素的宽度
					$("#focus_404 ul").css("width",sWidth * (len));
					
					//鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
					$("#focus_404").hover(function() {
						clearInterval(picTimer);
					},function() {
						picTimer = setInterval(function() {
							showPics(index);
							index++;
							if(index == len) {index = 0;}
						},3000); //此4000代表自动播放的间隔，单位：毫秒
					}).trigger("mouseleave");
					
					//显示图片函数，根据接收的index值显示相应的内容
					function showPics(index) { //普通切换
						var nowLeft = -index*sWidth; //根据index值计算ul元素的left值
						$("#focus_404 ul").stop(true,false).animate({"left":nowLeft},300);
						$("#focus_404 .btn span").stop(true,false).animate({"opacity":"0.3"},300).eq(index).stop(true,false).animate({"opacity":"0.7"},300); //为当前的按钮切换到选中的效果
					}
				});
				
