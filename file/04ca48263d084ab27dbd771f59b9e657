<script>
var speed=10//速度数值越大速度越慢
divid2.innerHTML=tdid.innerHTML
function Marquee(){
if(divid2.offsetWidth-divid.scrollLeft<=0)
divid.scrollLeft-=tdid.offsetWidth
else{
divid.scrollLeft++
}
}
var MyMar=setInterval(Marquee,speed)
divid.onmouseover=function() {clearInterval(MyMar)}
divid.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
</script>
