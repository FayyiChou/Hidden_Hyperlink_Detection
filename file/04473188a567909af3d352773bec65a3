    <script type="text/javascript">
        var screen = document.getElementById("splash");
//        window.localStorage.removeItem("pre_url");
//        console.log()
        var screen_date = window.localStorage.getItem("screen_date");
        var date = new Date();
        var now_date = date.getMonth()+ "_" + date.getDate();
//        console.log(now_date);
        if(getParameterByName("tks") && !getParameterByName('bm')){
            if(!screen_date || screen_date != now_date){
                screen.style.display = "block";
            }
                window.localStorage.setItem("screen_date", now_date);

        }
        var pass = document.getElementById("pass-word");
        document.getElementById("pass-word").addEventListener("click", function(){
            screen.style.display = "none";
        }, false);
        setTimeout(function(){
            screen.style.display = "none";
        }, 3000);
        setTimeout(function(){
            pass.style.display = "inline-block";
        }, 1000);
        function getParameterByName(name) {
            var match = RegExp('[?&]' + name + '=([^&]*)')
                .exec(window.location.search);
            return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
        }
    </script>
