	try{
	var aGet={};
		aGet.obj=document.getElementById('getMore');
		aGet.txt=document.getElementById('getList');
		aGet.cutNum=document.getElementById('cutNum');
		aGet.cut=1;
		aGet.url=document.location.href.replace('s.html','');
		aGet.obj.onclick=function(){
			aGet.cut+=1;
			aGet.urls=aGet.url+aGet.cut+'_s.html';
			var xhr = new XMLHttpRequest();
			xhr.onreadystatechange = function () {
				if (xhr.readyState == 4) {
					if (xhr.status == 200) {
						aGet.str=xhr.responseText;
						aGet.txt.innerHTML+=aGet.str;
						aGet.cutNum.innerHTML=aGet.cut;
					} else {
						alert('已经是最后一页了！');
					}
				}
			};
			xhr.open('get', aGet.urls+'?rand='+Math.random(), true);
			xhr.send(null);
			return false;
		}
	}catch(e){}
