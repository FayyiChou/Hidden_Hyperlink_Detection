<!-- <script src="/agrosalon_files/jCarouselLite/js/jcarousel.js" type="text/javascript"></script> -->
<!-- <script src="/agrosalon_files/jCarouselLite/changes.js" type="text/javascript"></script> -->
<!-- <script src="/agrosalon_files/flexslider/jquery.flexslider.js"></script> -->
<script type="text/javascript" src="/agrosalon_files/jquery.jcarousel.min.js"></script>

<script type="text/javascript">
// $(window).load(function(){
// 	$('.flexslider').flexslider({
// 		animation: "slide",
// 		slideshow: false,
// 		controlNav: false,
// 		itemWidth: 142,
// 		minItems: 2,
// 		prevText: "",
// 		nextText: "",
// 		maxItems: 10,
// 		controlsContainer: ".flexslider-container"
// 	});
// });

$(document).ready(function() { 
	$('#more').click( function(){ 
		$(this).css('display','none');
		$('.article_hidden').css('display','block');
		$('#hide_more').css('display','inline-block');
	});
	$('#hide_more').click( function(){ 
		$(this).css('display','none');
		$('.article_hidden').css('display','none');
		$('#more').css('display','inline-block');
	});

	// slider participants
	$('.members_slider').jcarousel({
		scroll: 1,
		wrap: 'circular',
		visible: 4
	})
	.jcarouselAutoscroll({'autostart': true, 'interval': 3000});

	$('.participants_gall .jcarousel-control-prev')
		.on('jcarouselcontrol:active', function () {
			$(this).removeClass('inactive');
		})
		.on('jcarouselcontrol:inactive', function () {
			$(this).addClass('inactive');
		})
		.jcarouselControl({
			target: '-=1'
		});

	$('.participants_gall .jcarousel-control-next')
		.on('jcarouselcontrol:active', function () {
			$(this).removeClass('inactive');
		})
		.on('jcarouselcontrol:inactive', function () {
			$(this).addClass('inactive');
		})
		.jcarouselControl({
			target: '+=1'
		});
});
</script>







