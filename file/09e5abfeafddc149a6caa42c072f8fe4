	<SCRIPT LANGUAGE="VBScript">
	Dim imgtop, imgleft
 
	tableindex = 0			    '
	mouseflag = False                   '
	offsetleft = 0			    '
	offsettop =  0			    '

	Sub initialimg()
		imgtop =  131	            ' 
		imgleft = 5	            ' 
		Document.All.fiximg.Style.position = "absolute"
		Document.All.fiximg.Style.postop =    imgtop
		Document.All.fiximg.Style.posleft =   imgleft
	End Sub

	Sub moveimg()
		Document.All.fiximg.Style.top = Document.Body.scrollTop + imgtop
		document.all.fiximg.Style.left = Document.Body.scrollLeft + imgleft
	End Sub

	Sub Document_onMouseMove()
		If mouseflag = True Then
			showtable(tableindex).Style.posLeft = Window.Event.X - offsetleft
			showtable(tableindex).Style.posTop = Window.Event.Y - offsettop
		End If
	End Sub

	Sub tableset(indexnumber)
		tableindex = indexnumber
		mouseflag = True		 '
		offsetleft = Window.Event.X - showtable(tableindex).Style.posLeft
		offsettop = Window.Event.Y - showtable(tableindex).Style.posTop
	End Sub

	Sub hiddentable(indexhidden)
		showtable(indexhidden).Style.Visibility = "hidden"
	End Sub
</SCRIPT> 



