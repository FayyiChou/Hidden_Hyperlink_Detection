		function calcchk() {
			if (!checkTel($("#mobile").val()) || !checkNull($("#mobile").val())) {
		        alert("请填写正确格式的手机号码！");
		        $("#mobile").focus();
		        return false;
	        }
	        return true
		}
	</script>
