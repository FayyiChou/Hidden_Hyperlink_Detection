$(function(){
    $("img").error(function () {  
        $(this).attr("src", "");//JSP页面  
    });  
	$(window).load(function() { 
		$('img').each(function() {
			if (!this.complete ) { 
				this.src = ''; 
			} 
		});
	});
})
