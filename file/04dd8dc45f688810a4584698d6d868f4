		function xf_checkform() {
			var title = $("#xf_title");
			var tel = $("#xf_tel");
			var address = $("#xf_address");

			if (!checkNull(title.val())) {
				alert("请输入您的姓名");
				title.focus();
				return false;
			}
			if (!checkkg(title.val())) {
				alert("姓名中不能有空格，请重新输入");
				title.focus();
				return false;
			}
			if (!checkNull(tel.val())) {
				alert("请输入您的号码");
				tel.focus();
				return false;
			}
			if (!checkTel(tel.val())) {
				alert("请输入正确格式的号码");
				tel.focus();
				return false;
