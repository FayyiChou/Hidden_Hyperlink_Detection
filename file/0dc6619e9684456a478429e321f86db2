<script>
 function hiddenDiv(id){
   id.style.display="none"; 
 }
 var flag=false;
 function imgchnage(){
    var fdimg=document.getElementById('fdimg');
	var fda=document.getElementById('fda');
    if(flag){
	  
	  fdimg.src="images/pf_img.jpg";
	  fda.href="news/news_list.asp?newstype=%C1%BD%D1%A7%D2%BB%D7%F6%BB%EE%B6%AF%D7%A8%C0%B8";
	  
	  flag=false;
	}else{
	  
	  flag=true;
	}
 }
 window.setInterval("imgchnage()",2000);
 
</script>
<SCRIPT LANGUAGE="JavaScript">
var xPos = 20;
var yPos = document.body.clientHeight;
var step = 1;
var delay = 30;
var height = 0;
var Hoffset = 0;
var Woffset = 0;
var yon = 0;
var xon = 0;
var pause = true;
var interval;
var obj=document.getElementById("ditan");
img.style.top = yPos;
function changePos() {
width = document.body.clientWidth;
height = document.body.clientHeight;
Hoffset = img.offsetHeight;
Woffset = img.offsetWidth;
img.style.left = xPos + document.body.scrollLeft;
img.style.top = yPos + document.body.scrollTop;
if (yon) {
yPos = yPos + step;
}
else {
yPos = yPos - step;
}
if (yPos < 0) {
yon = 1;
yPos = 0;
}
if (yPos >= (height - Hoffset)) {
yon = 0;
yPos = (height - Hoffset);
}
if (xon) {
xPos = xPos + step;
}
else {
xPos = xPos - step;
}
if (xPos < 0) {
xon = 1;
xPos = 0;
}
if (xPos >= (width - Woffset)) {
xon = 0;
xPos = (width - Woffset);
}
}
function www_helpor_net() {
img.visibility = "visible";
interval = setInterval('changePos()', delay);

<!-- 鼠标指向,停止-->

obj.onmouseover=function(){clearInterval(interval)} 
obj.onmouseout=function(){interval=setInterval("changePos()", delay)} 


}
www_helpor_net();
</script>
<!-- 鼠标指向,停止-->
<script> 
var x = 10,y = 60 
var xin = true, yin = true 
var step = 0.5 
var delay = 10 
var obj=document.getElementById("ad") 
function floatAD() { 
var L=T=0 
var R= document.body.clientWidth-obj.offsetWidth 
var B = document.body.clientHeight-obj.offsetHeight 
obj.style.left = x + document.body.scrollLeft 
obj.style.top = y + document.body.scrollTop 
x = x + step*(xin?1:-1) 
if (x < L) { xin = true; x = L} 
if (x > R){ xin = false; x = R} 
y = y + step*(yin?1:-1) 
if (y < T) { yin = true; y = T } 
if (y > B) { yin = false; y = B } 
} 
var itl= setInterval("floatAD()", delay) 
obj.onmouseover=function(){clearInterval(itl)} 
obj.onmouseout=function(){itl=setInterval("floatAD()", delay)} 
</script>

