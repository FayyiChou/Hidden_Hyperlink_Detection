					$(this).parents(".pic-box").find('.note-comment').css({
						top: e.pageY - 90 + 10,
						left: e.pageX - $(this).parents(".pic-box").offset().left +10
					});
				})
				$("#picture-pageshow .pic-box img").hover(function() {
					// Stuff to do when the mouse enters the element;
					$(this).parents(".pic-box").find('.note-comment').css('visibility', 'visible');
				}, function() {
					// Stuff to do when the mouse leaves the element;
					$(this).parents(".pic-box").find('.note-comment').css('visibility', 'hidden');
				});
			</script>
		</div>
	</div>
<a href="javascript:void(0)" id="backtotop">返回顶部</a>
<script>
var timeId = null;
jQuery(".original").hover(function(){
	$(this).siblings(".mod-tips").show();
}, function(){
	var _self = $(this);
	timeId = setTimeout(function(){
		_self.siblings(".mod-tips").hide();
	}, 200);
});

jQuery(".comment .mod-tips").hover(function(){
	clearTimeout(timeId);
}, function(){
	$(this).hide();
});
jQuery(".root img").hover(function(){
	jQuery(this).siblings(".mod-tips").show();
}, function(){
	jQuery(this).siblings(".mod-tips").hide();
});

//回到顶部
LEGO.backToTop("#backtotop",{scrollEffect:false});
</script>
