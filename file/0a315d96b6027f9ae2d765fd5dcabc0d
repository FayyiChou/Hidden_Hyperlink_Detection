<script type='text/javascript'>
$(".list.c-more").each(function () {
	var _this = $(this);
	fnMore(0, _this);
	_this.find('li').on("click", function () {
		var index = $(this).index();
		fnMore(index, _this);
	});
});
function fnMore(i, obj) {
	obj.siblings('a.more').hide();
	obj.siblings('a.more').eq(i).show();
}

$(function(){
	$(".checkImage").each(function(){
		var obj = $(this);
		if($(this).attr("imageSrc") != "" && $(this).attr("imageWidth") != "" && $(this).attr("imageHeight") != ""){
		   $.ajax({
			   url: "/index/checkImage",
			   type: 'POST',
			   data:{
				   src:$(this).attr("imageSrc"),
				   width:$(this).attr("imageWidth"),
				   height:$(this).attr("imageHeight")
			   },
			   dataType: 'html',
			   success: function(result){
				   result = eval("("+result+")");
				   $(obj).attr("src",result);
			   }
		   });
		}
	});
})
</script>
