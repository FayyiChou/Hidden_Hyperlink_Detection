<script>
function menuAjax(ID,DEPTH_LEVEL,that){	
	if(that.hasClass("active")){
		that.parent().find("ul").removeClass("active");
		that.removeClass("active");
	}else{
		that.addClass("active");
		if(that.parent().find("ul").length > 0){
			that.parent().find("ul").addClass("active");
		}else{
			$("#mouse_loading_icon").show();
			$.ajax({
				  method: "POST",
				  url: "/",
				  data: { ksajax: "Y", ID: ID,DEPTH_LEVEL: DEPTH_LEVEL }
				})
				  .done(function( msg ) {
					  that.parent().append(msg);
					  //that.parent().find("ul a.menu.active").parent().find(".menualt").click();
					  $("#mouse_loading_icon").hide();					 
				 });

		}
		
		
	}
	
}
