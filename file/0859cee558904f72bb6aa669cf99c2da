<script src="template/comiis_lsxc/img/portal/jquery-1.7.2.min.js" type="text/javascript" type="text/javascript"></script>
<script src="template/comiis_lsxc/img/portal/jquery-ui.min.js" type="text/javascript" type="text/javascript"></script>
<script src="template/comiis_lsxc/img/portal/allinone_thumbnailsBanner.js" type="text/javascript" type="text/javascript"></script>
<script type="text/javascript">
var jQ = jQuery.noConflict();
jQ(function(){
jQ("#Banner_simple").allinone_thumbnailsBanner({skin:"cool",numberOfThumbsPerScreen:5,width:590,height:360,thumbsReflection:0,defaultEffect:"random"}),jQ("#con ul li").hover(function(){jQ(this).find("div").stop(!0,!0).animate({bottom:"0"})},function(){jQ(this).find("div").stop(!0,!0).animate({bottom:"-90px"})});
  function new_slider(slidercon,mainspeed,inspeed,outspeed){
this.slidercon = slidercon;
this.mainspeed = mainspeed;
this.inspeed = inspeed;
this.outspeed = outspeed;
var i = 1;
var listcon = '<li class="current">' + 1 + '</li>';
jQ(slidercon).append(
'<div id="slider_title"></div>'
);
jQ(slidercon).children("#slider_title").html(jQ(slidercon).children("ul:first").children("li:eq(0)").children("a").children("img").attr("title"));
jQ(slidercon).append(
'<ul id="slider_counter"></ul>'
);
for(var j = 2; j <= jQ(slidercon).children("ul:first").children("li").length; j ++){
listcon = listcon + "<li>" + j + "</li>";
};
jQ(slidercon).children("#slider_counter").html(listcon);
jQ(slidercon).children("#slider_counter").children("li").mouseover(function(){
var index = jQ.inArray(this,jQ(slidercon).children("#slider_counter").children("li"));
jQ(slidercon).children("#slider_counter").children("li").removeClass("current")
.eq(index).addClass("current");
jQ(slidercon).children("ul:first").children("li").fadeOut(outspeed)
.eq(index).fadeIn(inspeed);
jQ(slidercon).children("#slider_title").html(jQ(slidercon).children("ul:first").children("li:eq(" + parseInt(jQ(this).html()-1)  + ")").children("a").children("img").attr("title"));
i = jQ(this).index() + 1;
if(i == jQ(slidercon).children("ul:first").children("li").length){
i = 0;
};
});
function init(){
jQ(slidercon).children("ul:first").children("li:not(" + i + ")").fadeOut(outspeed);
jQ(slidercon).children("ul:first").children("li:eq(" + i + ")").fadeIn(inspeed);
jQ(slidercon).children("#slider_counter").children("li:not(" + i + ")").removeClass("current");
jQ(slidercon).children("#slider_counter").children("li:eq(" + i + ")").addClass("current");
jQ(slidercon).children("#slider_title").html(jQ(slidercon).children("ul:first").children("li:eq(" + i + ")").children("a").children("img").attr("title"));
i++;
if(i >= jQ(slidercon).children("ul:first").children("li").length){
i = 0;
};
};
var move = setInterval(init,mainspeed);
jQ(slidercon).mouseover(function(){
clearInterval(move);
});
jQ(slidercon).mouseout(function(){
move = setInterval(init,mainspeed);
});
};
  new_slider('#new_slider',3000,150,10);
  new_slider('#new_slider3',3000,150,10);
});
</script></div>


