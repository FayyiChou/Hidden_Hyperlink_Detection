      <script type="text/javascript">
    function changeStyle(ele){
            var liAry=document.getElementById("test2_li_now_").getElementsByTagName("li");
            var liLen=liAry.length;
            var liID=ele.id;
            for(var i=0;i<liLen;i++)
            {
                if(liAry[i].id==liID)
                {
                    liAry[i].className="now";
					
					if(liAry[i].id=='product'){
						document.getElementById('syss').action="/product/";
						document.getElementById('f').value="list";
					}
					if(liAry[i].id=='buy'){
						document.getElementById('syss').action="/buy/";
						document.getElementById('f').value="list";
					}
					
					if(liAry[i].id=='company'){
						document.getElementById('syss').action="/company/";
						document.getElementById('f').value="list";
					}
					if(liAry[i].id=='news'){
						document.getElementById('syss').action="/?f=cate&catid=0";
						document.getElementById('f').value="cate";
						document.getElementById('catid').value="0";
					}
					if(liAry[i].id=='brand'){
						document.getElementById('syss').action="/brand/";
						document.getElementById('f').value="list";
					}
					if(liAry[i].id=='zhanhui'){
						document.getElementById('syss').action="/?f=cate&catid=85";
						document.getElementById('f').value="cate";
						document.getElementById('catid').value="85";
					}
					
					
					
                }
                else
                {
                    liAry[i].className="";
                }
            }
        }
    </script> 
