  <script type="text/javascript">
        $(function() {
            var oBox = $(".scroll");
            var oLi = $(".scroll .haixin");
            var flag = true;
            var spd = 3000;
            function ani() {
                if (flag == false) { return false; }
                flag = false;
                oBox.animate({ "left": oBox.position().left - 313 }, 1000, function() {
                    $(this).css("left", "0px").append(oLi.eq(0));
                    t = setTimeout(ani, spd);
                    flag = true
                });
            }
            var t = setTimeout(ani, spd);
            $(".left_btn").click(function() {
                clearTimeout(t);
                ani();
            })
            $(".right_btn").click(function() {
                if (flag == false) { return false; }
                flag = false;
                clearTimeout(t);
                oBox.css("left", "-313px").prepend(oLi.eq(oLi.length - 1));
                oBox.animate({ "left": oBox.position().left + 313 }, 1000, function() {
                    t = setTimeout(ani, spd);
                    flag = true
                });
            })
        })
    </script> 
