        $(function () {

            hideul("c");
            hideul("a");



            $('.btnmorearea').toggle(function () {
                var ul = $(this).parent().prev(ul);
                var lis = ul.find('li').length;

                var m = parseInt(lis / 5);
                if (m % 5 != 0) m++;
                var h = m * 18;
                ul.animate({ height: '+' + h + 'px' }, 800);
                $(this).text('隐藏');
                $("#amore").removeClass("gengduo");
                $("#amore").addClass("yinchang");
            }, function () {
                var ul = $(this).parent().prev(ul);
                ul.animate({ height: '46px' }, 800);
                $(this).text('更多');
                $("#amore").removeClass("yinchang");
                $("#amore").addClass("gengduo");
            });
        });
</script> 
