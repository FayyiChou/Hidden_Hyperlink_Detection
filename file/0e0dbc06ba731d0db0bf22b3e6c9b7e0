       <script type="text/javascript">


        $(function () {

            $('.btnmore').toggle(function () {
                var ul = $(this).parent().prev(ul);
                var lis = ul.find('li').length;

                var m = parseInt(lis / 5);
                if (m % 5 != 0) m++;
                var h = m * 18;
                ul.animate({ height: '+' + h + 'px' }, 800);
                $(this).text('隐藏');
                $("#cmore").removeClass("gengduo");
                $("#cmore").addClass("yinchang");

                //$("p").addClass("selected");
            }, function () {
                var ul = $(this).parent().prev(ul);
                ul.animate({ height: '46px' }, 800);
                $(this).text('更多');

                $("#cmore").removeClass("yinchang");
                $("#cmore").addClass("gengduo");
            });
        });
</script> 
