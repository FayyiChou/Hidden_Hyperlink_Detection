        $.getJSON('/loupan_api/loupanCommonInfo', loupan_info, function(data){
            var activity_signup_count = 0;
            if(typeof data.data.egou_activity.activity_signup_count != undefined){
                activity_signup_count = data.data.egou_activity.activity_signup_count;
                $('#j_egou_activity_sign_num .sign_num').text(activity_signup_count);
            }
            $('#j_egou_activity_sign_num').show();
        });
        setTimeout(function(){
            $('#j_egou_activity_sign_num').show();
        }, 5000);
    });
