	<script language="JavaScript" TYPE="text/javascript">

		
			document.forms['whalogin'].wha_username.focus();
		

		$(function() {
		    $( "#dialog-error" ).dialog({
		      autoOpen: false,
		      resizable: false,
		      width: 370,
		      modal: true,
		      show: {effect: 'fade', duration: 75},
      		  hide: {effect: 'fade', duration: 75},
		      buttons: {
		        "Ok": function() {
		          $( this ).dialog( "close" );
		        }
		      }
		    });
		});

		function validateLogin() {
			var uname = document.forms['whalogin'].wha_username.value;
			var pword = document.forms['whalogin'].wha_password.value;
			uname = uname.replace('-','');
			var msg = "<div style='color:crimson;'><ol class='err'>";

			if (uname.length == 0) {
				msg = msg + "<li>Username is required.</li>";
			} else if (uname.length < 5) {
				msg = msg + "<li>Username must be at least 5 characters.</li>";
			}

			if (pword.length == 0) {
				msg = msg + "<li>Password is required.</li>";
			} else if (pword.length < 6) {
				msg = msg + "<li>Password must be at least 6 characters.</li>";
			}

			if (msg == "<div style='color:crimson;'><ol class='err'>") {
				return true;
			} else {
				msg = msg + "</ol><div style='text-align:left;margin:5px 0px 0px 0px;color:black;'>" + "Please correct the problem before submitting." + "</div>";
				document.getElementById('derror').innerHTML = msg;
				$('#dialog-error').dialog('open');
				return false;
			}
		}


	</script>
		
