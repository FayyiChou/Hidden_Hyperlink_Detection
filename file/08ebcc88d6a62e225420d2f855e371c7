<script type="text/javascript">
    var map = new BMap.Map('allmap');
    var poi = new BMap.Point(113.3385,23.099108);
    map.centerAndZoom(poi, 16);
    map.enableScrollWheelZoom();
    var content = '<div style="margin:0;line-height:20px;padding:2px;">' +
                    '<img src="/images/company.jpg" alt="广东江影电影器材设备有限公司" style="float:right;zoom:1;overflow:hidden;width:100px;height:100px;margin-left:3px;"/>' +
                    '广州市海珠区江海大道2号南方明珠花园201-206<br/>电话：020-89309654<br/>简介：广东江影影视传媒有限公司业务覆盖影视剧、新媒体剧、微电影微视频定制；宣传片、广告片策划创意制作、创意视频' +
                  '</div>';
    var searchInfoWindow = null;
	searchInfoWindow = new BMapLib.SearchInfoWindow(map, content, {
			title  : "广东江影影视传媒有限公司",
			width  : 290, 
			height : 105, 
			panel  : "panel",
			enableAutoPan : true,
			searchTypes   :[
				BMAPLIB_TAB_SEARCH, 
				BMAPLIB_TAB_TO_HERE, 
				BMAPLIB_TAB_FROM_HERE 
			]
		});
    var marker = new BMap.Marker(poi); 
    marker.enableDragging(); 
    marker.addEventListener("click", function(e){
	    searchInfoWindow.open(marker);
    });
    map.addOverlay(marker); 
	searchInfoWindow.open(marker);
</script>
