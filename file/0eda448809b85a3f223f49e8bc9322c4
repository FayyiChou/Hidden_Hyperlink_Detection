<script language="javascript">
  function switchmodTag(modtag,modcontent,modk) {
    for(i=1; i <11; i++) {
      if (i==modk) {
        document.getElementById(modtag+i).className="sec1";document.getElementById(modcontent+i).className="block";}
      else {
        document.getElementById(modtag+i).className="sec2";document.getElementById(modcontent+i).className="unblock";}
    }
  }
</script>
