  <script type="text/javascript">
function nTabs(tabObj,obj){
 var tabList = document.getElementById(tabObj).getElementsByTagName("dd");
   for(i=0; i <tabList.length; i++)
     {
  if (tabList[i].id == obj.id)
   {
    document.getElementById(tabObj+"_Content"+i).style.display = "block";document.getElementById(tabObj+"_Title"+i).className = "sec3";
     }else{
      document.getElementById(tabObj+"_Content"+i).style.display = "none";document.getElementById(tabObj+"_Title"+i).className = "sec4";
      }
    } 
}
</script>
