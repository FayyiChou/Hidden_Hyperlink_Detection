    <script>

        var timeout;
        $(document).ready(function () {
            $(".rank li").each(function (index) {
                $(this).mouseover(function () {
                    timeout = setTimeout(function () {
                        $(".news .n2 ul.date").removeClass("con-box-2");
                        $(".rank li.hover").removeClass("hover");
                        $("div ul.date:eq(" + index + ")").addClass("con-box-2");
                        $(".rank li").eq(index).addClass("hover");
                    },
                    150);
                    $(this).mouseout(function () {
                        clearTimeout(timeout);
                    });
                });
            });
        });

        /*领号切换*/
        var open_mub, set_box
        $(".hotnew ul li").hover(function () {
            open_mub = $(this).index()
            set_box = $(this).parent().parent().index()
            $(".hotnew ul li").removeClass("open")
            $(this).addClass("open")
        })

