			$(function(){
				var b = $(".indexbr").height() + $("#header").height();
				var h = $(".tab-text").offset().top - $(window).scrollTop();
				var w = ($(window).width()-1200)/2;
				var t = h - b
				console.log(w)
				if (w < 114){
					$(".tab-text").hide();
					$(".tab-wechat .bar-icon").mouseenter(function () {
						$(".tab-text").show();
					});
					$(".tab-wechat .bar-icon").mouseleave(function () {
						$(".tab-text").hide();
					});
				}else {
					$(".tab-text").show();
				}

			});
		</script>
