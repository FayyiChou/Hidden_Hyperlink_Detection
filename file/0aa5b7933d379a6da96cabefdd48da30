<script>



        /*左侧导航*/
        var nav_productCategories = $("#nav_productCategories").find("dl").length;
        if (!nav_productCategories) {
            $('#nav_productCategories').append("<div style='width:100%; text-align: center;'>暂无报价信息！</div>");
        }
        $(".paihao_list").each(function () {
            $(this).children("dd").last().css("border", "0");
        });
        $(".Lf_nav2 .paihao_list dt").live('click', function () {
            if (!$(this).hasClass("h2_hover")) {
                $(".paihao_list dd").hide();
                $(this).parents("dl").find("dd").show();
                $(".paihao_list dt").removeClass("h2_hover");
                $(this).addClass("h2_hover");
            } else {
                $(".paihao_list dd").hide();
                $(this).removeClass("h2_hover");
            }
        });


    /*产品分类*/
        $(".Lf_nav1 .paihao_list dt").live('click', function () {
            if (!$(this).hasClass("h2_hover")) {
                $(".Lf_nav1 .paihao_list dt").removeClass("h2_hover");
                $(this).addClass("h2_hover");
                $(".paihao_list dd").hide();
                var _this = $(this);
                var tid = $(this).attr("t");
                if (_this.next("dd").html() == null) {
                    $.getJSON(shopDomain + "/ajax/GetShopCustomClassification?callback=?", {
                        id: tid
                    },
                        function (data) {
                            $.each(data, function (i) {
                                _this.parent("dl").append('<dd style="display:block"><a href="http://www.21cp.com/details/cpc139392/' + this.type + '/' + this.productId + '.htm">' + this.title + '</a></dd>')
                            });
                        }
                    );
                } else {
                    _this.parent("dl").find("dd").show();
                }
            } else {
                $(this).removeClass("h2_hover");
                $(".paihao_list dd").hide();
            }
    })



    //右侧浮窗
    $(".Right_Win").live("mouseenter", function () {
        $(this).addClass("Right_Win_select");
    });
    $(".Right_Win_select").live("mouseleave", function () {
        $(this).removeClass("Right_Win_select");
    });

    /*二维码*/
    function qrimg() {
        var qrimg = $(".Right_Win_ws .Right_Win_M");
        var surl = $(".Right_Win_ws .Right_Win_tit").attr("data-url");
        if (surl == null || surl == "" || surl == undefined) {
            $(".Right_Win_ws").remove();
        } else {
            qrimg.qrcode({ width: 100, render: (!-[1, ] ? "table" : "canvas"), height: 100, text: surl });
        }
    }
    qrimg()


    //加载在线客服
    
    if (!GAL_CurrentTheme.CustomerServiceJson) {
        $(".Right_Win_kf").hide();
    } else {
        var KF_a = "";
        var KF_array = GAL_CurrentTheme.CustomerServiceJson.split(",");
        var kf_open = 0;
        $.each(KF_array, function (i,t) {
            if (i == 0 || i == 3 || i == 6 || i == 9) {
                if (this == "已开启") {
                    KF_a += KF_array[i] + "," + KF_array[i + 1] + "," + KF_array[i + 2]+ ",";
                    kf_open++;
                };
            }
        })
        if (KF_array.length < 1 || kf_open < 1) {
            $(".Right_Win_kf").hide();
        } else {
            for (var i = 0; i < kf_open; i++) {
                $(".Right_Win_kf .Right_Win_M").append('<p class="Right_Win_M_kfP"><a target="_blank" href="http://wpa.qq.com/msgrd?v=3&uin=' + KF_a.split(",")[i * 3 + 2] + '&site=qq&menu=yes">' + KF_a.split(",")[i * 3 + 1] + '</a></p>')
            }
        }
    }
</script>
