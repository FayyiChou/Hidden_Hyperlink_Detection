<script type="text/javascript">
    (function () {
        var bannerInfo = [{"class":"11_1","note":"客户的成功才是我们真正的成功！"},{"class":"11_2","note":"客户的成功才是我们真正的成功！"},{"class":"11_3","note":"客户的成功才是我们真正的成功！"},{"class":"11_4","note":"客户的成功才是我们真正的成功！"}],
       toServerUrl = function(u){
           return  ('http://img.21cp.cc/s' + u).replace(/\/uploadfiles/gi,'') ;                
       };
        if(GAL_CurrentTheme.Theme_Id != GAL_PreviewId  && GAL_PreviewId != 0){
            bannerInfo = [];
        }
        if(bannerInfo.length == 1 && GAL_CurrentTheme.Theme_Id == 17){
            $("html").attr('id','theme_' + bannerInfo[0]['class']);            
        }else if(bannerInfo.length){
            $.each(bannerInfo,function(index,val){   
                if(val['class'] && val['class'].substring(0,1)!= '_'){
                    var Ms = (val.data_style||",,,,").split(","); 
                    Ms[2] == "1"  ? Ms[2] ="bold" :"normal";
                    Ms[3] == "1"  ? Ms[3] ="underline" :"none";
                    var notes1 ="", notes2 ="";
                    val.note == undefined ? note1 = "客户的成功是我们真正的成功!!" : note1 = val.note;
                    val.data_con == undefined  ? note2 = "客户的成功是我们真正的成功!~" : note2 = val.data_con;
                    $(".vip_banner .triggers").append(' <li class="' + (index == 0?"current":"") + '">'+ (index  + 1 )+'</li>');
                    $(".vip_banner .slider").append('<li class="vipbanner_' + val['class'] + '"><span><p style="font-family:&prime;微软雅黑&prime; font-size: 26px;">'+ note1 +'</p><p style="font-family:&prime; '+ Ms[0] +'&prime;; font-size: '+ Ms[1] +'px; font-weight:  '+ Ms[2] +'; text-decoration:  '+ Ms[3] +'; color: #'+ Ms[4] +';">'+ note2 +'</p></span></li>');       
                    if (val.data_url) {
                        var surl = val.data_url.substring(0,7);
                        surl != "http://" ? surl = "http://"+val.data_url :  surl = val.data_url;
                        $(".vipbanner_" + val['class']).css( "cursor","pointer");
                        $(".vipbanner_" + val['class']).live("click",function(){
                            window.open(surl);
                        })
                    }
                }else{
                    $(".vip_banner .triggers").append(' <li class="' + (index == 0?"current":"") + '">'+ (index  + 1 )+'</li>');
                    $(".vip_banner .slider").append('<li style="background:url('+ toServerUrl(val.img)+ ') no-repeat center center;"><span></span></li>');
                }
            });
        }else{           
            if(shopBannerData && shopBannerData.length){
                $.each(shopBannerData,function(index,val){
                    if(val.tempId == (GAL_PreviewId || GAL_CurrentTheme.Theme_Id)){
                        for(var i = 0; i <4 ; i++){
                            $(".vip_banner .triggers").append(' <li class="' + (i == 0?"current":"") + '"></li>');
                            $(".vip_banner .slider").append('<li class="vipbanner_' + val.classNames[i] + '"></li>');

                        }
                    }
                });
            }
        }
        
        //图片滚动
        $('.indexSlider1').slider({ timer: 3000 });
        //网站标题
        if ($("#FF_title_HI") !=  undefined) {
            var tit_css = $("#FF_title_HI").val();
            var tit_css_array = (tit_css||",,,,").split(",");
            tit_css_array[2] == "1" ? tit_css_array[2] = "bold" : tit_css_array[2] = "normal" ;
            tit_css_array[3] == "1" ? tit_css_array[3] = "underline" : tit_css_array[3] = "none" ;
            $(".header h1").css({"font-family":tit_css_array[0]||"宋体","font-size":tit_css_array[1]||"26px","font-weight":tit_css_array[2],"text-decoration":tit_css_array[3],"color":"#"+tit_css_array[4]});
        }

    })();
