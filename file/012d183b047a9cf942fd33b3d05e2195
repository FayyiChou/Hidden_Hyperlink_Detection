<script type="text/javascript">
$(document).ready(function(){
    improve_tips.show_tips();
    $('.tb-list tbody tr').stripe();
    if ('undefined' != typeof(config)  && config.unread_count && config.unread_count > 0)
    {
        $('#unread-count').html('(' + config.unread_count + ')').show();
    }
    $('#main').height($('#rightmain').height());
    $('#footer').show();
    $('#rightmain').resize(function(){
        $('#main').height($('#rightmain').height());
    });

    $('img').each(function(){
        var img = $(this);
        var def = img.attr('def');
        //console.log(def);
        if (def && def.length > 0)
        {
            img.bind('error', function(){
                var src = '//static.rong360.com/img/' + def;
                img.attr('src', src);
                img.unbind('error');
            });
        }
    });

    
    /** online servie */
    $("#floatShow").bind("mouseover",function(){
        $("#onlineService").animate({
            width:"180px", opacity:"show"
        }, "normal" ,function(){
        });
        
        $("#floatShow").hide();
        $("#floatHide").show();
    });
    
    $("#floatHide").bind("mouseout",function(){
        $("#onlineService").animate({
            width:"0", opacity:"hide"
        }, "normal" ,function(){
        });
       
        $("#floatShow").show();
        $("#floatHide").hide();
    });

});
</script>

<script>
$(document).ready(function(){
    $('#floatShow').bind('mouseover', function(){
        var img = document.createElement('img');
        img.src = get_ra() + '&ext=' + encodeURIComponent('&action=woyaozixun');
    });
    var img = document.createElement('img');
    img.src = get_ra();
});
</script>
<script src="/static/js/ra.js"></script>
<script>
function get_ra()
{
    var ra_pid = "e87e0cb46fa2b4e55e01601248caf0fc";
    if ($('#ra_pid').length > 0)
    {
        ra_pid = $('#ra_pid').val();
    }
    var url = "%2Fbd%2Flogin.html";
    var rf = encodeURIComponent(document.referrer);
    var host = "https%3A%2F%2Fbd.rong360.com";
    var banker_id = '';
    var abclass = '1491066767_2'.split('_');
    if(abclass.length > 1)
    {
        abclass = abclass[1];
    }
    else
    {
        abclass = abclass[0];
    }

    var ext_str = '';
    
    var page_name = '';
                page_name += '&page_name=bd_login';
                return '//g.rong360.com/u.gif?rid=' + ra_gc('RONGID') + '&access_id=' + 'b6fmn6o5sn2hb3pcdmm4rtrug0' + '&ca=site/login&tpl=/www/bd/view/login.tpl&url=' + url + '&rf=' + rf + '&host=' + host + '&city=&pid=' + ra_pid + '&uf=o_0660113dd045ae4b94d48527d9583010&abclass=' + abclass + '&ga_csr=&ga_ccn=&ga_cmd=&ga_ctr=&rtm=' + ra_gc('__rtm') + '&ext=' + encodeURIComponent(ext_str) + '&page_name=' + encodeURIComponent(page_name) + '&banker_id=' + banker_id + '&channel=bd';
}
</script>

<script type="text/javascript">

jQuery(function(){
        	$(".two-h2").addClass("high");
    	$(".one").hide();
        $(".one-h2").on("click",function(e){
		var timestamp = new Date().getTime();
        $('[name="verifyPic1"]').attr('src','https://www.rong360.com/vcode/create?type=bd_reg&t='+timestamp);
        $(".one-h2").addClass("high");
        $(".two-h2").removeClass("high");
        $(".one").show();
        $(".two").hide();
    });
    $(".two-h2").on("click",function(e){
		var timestamp = new Date().getTime();
		$('[name="verifyPic2"]').attr('src','https://www.rong360.com/vcode/create?type=bd_login&t='+timestamp);
        $(".one-h2").removeClass("high");
        $(".two-h2").addClass("high");
        $(".one").hide();
        $(".two").show();
    });
    });
</script>


