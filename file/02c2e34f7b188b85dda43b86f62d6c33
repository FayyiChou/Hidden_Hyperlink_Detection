 <SCRIPT>
 var demo = document.getElementById('demo');
 var demo1 = document.getElementById('demo1');
 var t=demo.scrollWidth
 demo1.innerHTML+=demo1.innerHTML
 function doMarquee()
 {
  demo.scrollLeft=demo.scrollLeft<demo.scrollWidth-demo.offsetWidth?demo.scrollLeft+1:t-demo.offsetWidth
 }
 function doscroll()
 {
  sc=setInterval(doMarquee,50)
 }
 function stopscroll()
 {
  clearInterval(sc)
 }
 doscroll()
</SCRIPT><script language="javascript" src="http://code.54kefu.net/kefu/js/107/486507.js" charset="utf-8"></script>
