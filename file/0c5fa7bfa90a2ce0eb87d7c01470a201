<script type="text/javascript">var this_url =window.location.hostname;$(function(){$(".regTitle a").click(function(){var index=$(this).index();$(this).addClass("on").siblings().removeClass("on");$(".regList").eq(index).show().siblings().hide();$('.usercenter_input span').css({right:'-50px',opacity:'0'
});})
$("#subregphone").click(function(){var read_true =$('#read_true').attr('checktype');if(read_true !='true'){$('#read_eorr').show();return false;}
var yzm =$('#yzm').val();var phone =$('#phone').val();var password =$('#password').val();if($("#origin_showsend .geetest_challenge").val()){var geetest_challenge=$("#origin_showsend .geetest_challenge").val();var geetest_validate=$("#origin_showsend .geetest_validate").val();var geetest_seccode=$("#origin_showsend .geetest_seccode").val();}else{var geetest_challenge=$("#origin_reguser .geetest_challenge").val();var geetest_validate=$("#origin_reguser .geetest_validate").val();var geetest_seccode=$("#origin_reguser .geetest_seccode").val();}
var next =0;$("#regphone span").each(function(){if($(this).attr('style') =='right: 10px; opacity: 1;'){next+=1;}
});if(next>0){return false;}
if(yzm.length <6){alert('验证码不能少于6位！');return false;}
$.ajax({url:"http://passport.mfniu.com/Api_useract/user_register",dataType:'jsonp',data:{"username":phone,"password":password,"repassword":password,"type":1,"geetest_challenge":geetest_challenge,"geetest_validate":geetest_validate,"geetest_seccode":geetest_seccode,"source":101,code:yzm,'this_url':this_url},jsonp:'callback',success:function(json){if(json.Status==1){window.location.href='http://passport.mfniu.com/index';}else{alert(json.Msg);};}
});});geet(function (reguser_Obj) {reguser_Obj.bindOn(".reguser");reguser_Obj.appendTo(".reguser");reguser_Obj.onSuccess(geetestSuccess);});geet(function (reguserRel_Obj) {reguserRel_Obj.bindOn(".reguser_rel");reguserRel_Obj.appendTo(".reguser_rel");reguserRel_Obj.onSuccess(sendsmsrel);});$("#read_true").click(function(){var read_true =$('#read_true').attr('checktype');if(read_true =='true'){$('#read_true').attr('checktype','false');$('#read_eorr').show();}else{$('#read_true').attr('checktype','true');$('#read_eorr').hide();}
})
});function geetestSuccess(){var read_true =$('#read_true').attr('checktype');if(read_true !='true'){$('#read_eorr').show();return false;}
var reg_type =$('.on').text();if(reg_type =='用户名注册'){var username =$('#username').val();var userpassword =$('#userpassword').val();var reuserpassword =$('#reuserpassword').val();var geetest_challenge=$("#origin_reguser .geetest_challenge").val();var geetest_validate=$("#origin_reguser .geetest_validate").val();var geetest_seccode=$("#origin_reguser .geetest_seccode").val();var next =0;$("#regusername span").each(function(){if($(this).attr('style') =='right: 10px; opacity: 1;'){next+=1;}
});if(next>0){return false;}else{$.ajax({url:"http://passport.mfniu.com/Api_useract/user_register",dataType:'jsonp',data:{"username":username,"password":userpassword,"repassword":reuserpassword,"type":2,"geetest_challenge":geetest_challenge,"geetest_validate":geetest_validate,"geetest_seccode":geetest_seccode,"source":101,'this_url':this_url},jsonp:'callback',success:function(json){if(json.Status==1){window.location.href='http://passport.mfniu.com/index';}else{alert(json.Msg);};}
});}
}else if(reg_type =='手机号注册'){var next =0;$("#regphone span").each(function(){if($(this).attr('style') =='right: 10px; opacity: 1;'){next+=1;}
});if(next>0){return false;}else{sendsms();}
}else{alert('注册类型错误！');}
}
function sendsms(){var regmobile =$('#phone').val();var LOGOIN_login_reg=/^[0-9]{11,11}$/;var geetest_challenge=$("#origin_reguser .geetest_challenge").val();var geetest_validate=$("#origin_reguser .geetest_validate").val();var geetest_seccode=$("#origin_reguser .geetest_seccode").val();if(regmobile=="")
{rz_text('.regmobile_s','手机号不能为空！');visible_text_true('.regmobile_s');return false;};if(!LOGOIN_login_reg.test(regmobile))
{rz_text('.regmobile_s','手机号格式不正确！');visible_text_true('.regmobile_s');return false;};$.ajax({url:"http://passport.mfniu.com/Api_useract/send_sms",dataType:'jsonp',data:{"username":regmobile,"type":'register',"geetest_challenge":geetest_challenge,"geetest_validate":geetest_validate,"geetest_seccode":geetest_seccode,'this_url':this_url},jsonp:'callback',success:function(json){if(json.Status ==1 &&json.ErrorCode !=18){$('.tj_phonebox').show();$('#showsend').hide();$('#nosend').show();$('#nosend').css("background","#bbb");$('#nosend').text("获取（"+60+"s）");countdown =60;var interval =setInterval(function () {countdown--;if (countdown <=0) {clearInterval(interval);$('#nosend').hide();$('#showsend').show();} else {$('#nosend').text("获取（"+countdown+"s）");};},1000);}else{alert(json.Msg);}
}
});}
function sendsmsrel(){var regmobile =$('#phone').val();var LOGOIN_login_reg=/^[0-9]{11,11}$/;var geetest_challenge=$("#origin_showsend .geetest_challenge").val();var geetest_validate=$("#origin_showsend .geetest_validate").val();var geetest_seccode=$("#origin_showsend .geetest_seccode").val();if(regmobile=="")
{rz_text('.regmobile_s','手机号不能为空！');visible_text_true('.regmobile_s');return false;};if(!LOGOIN_login_reg.test(regmobile))
{rz_text('.regmobile_s','手机号格式不正确！');visible_text_true('.regmobile_s');return false;};$.ajax({url:"http://passport.mfniu.com/Api_useract/send_sms",dataType:'jsonp',data:{"username":regmobile,"type":'register',"geetest_challenge":geetest_challenge,"geetest_validate":geetest_validate,"geetest_seccode":geetest_seccode,'this_url':this_url},jsonp:'callback',success:function(json){if(json.Status ==1 &&json.ErrorCode !=18){$('.tj_phonebox').show();$('#showsend').hide();$('#nosend').show();$('#nosend').css("background","#bbb");$('#nosend').text("获取（"+60+"s）");countdown =60;var interval =setInterval(function () {countdown--;if (countdown <=0) {clearInterval(interval);$('#nosend').hide();$('#showsend').show();} else {$('#nosend').text("获取（"+countdown+"s）");};},1000);}else{alert(json.Msg);}
}
});}
