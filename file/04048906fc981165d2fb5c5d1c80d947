    <script language="JavaScript" type="text/javascript">
        lastScrollY = 0;
        suspendcode = "";
        suspendcode2 = "";
        function heartBeat() {
            var diffY;
            if (document.documentElement && document.documentElement.scrollTop)
                diffY = document.documentElement.scrollTop;
            else if (document.body)
                diffY = document.body.scrollTop
            else
            { /*Netscape stuff*/ }
            percent = .1 * (diffY - lastScrollY);
            if (percent > 0) percent = Math.ceil(percent);
            else percent = Math.floor(percent);
            document.getElementById("Scrollid").style.top = parseInt(document.getElementById("Scrollid").style.top) + percent + "px";
            document.getElementById("Scrollid2").style.top = parseInt(document.getElementById("Scrollid2").style.top) + percent + "px";

            lastScrollY = lastScrollY + percent;
        }
        suspendcode += "<DIV id=\"Scrollid\" style='right:0px;width:88px;PosITION:absolute;TOP:200px;FILTER: alpha(opacity=85);'>";
        //        suspendcode += "  <div align=center>";
        //        suspendcode += "      <img src='upload/20160714203225.jpg' border=0  />";
        //        suspendcode += "      <br/>";
        //        suspendcode += "      <img src='images/close2.gif' border=0 onclick='closeScroll();' style='cursor:pointer;' title='鐐瑰嚮鍏抽棴' />";
        //        suspendcode += "  </div>";
        suspendcode += "</div>";


        suspendcode2 += "<DIV id=\"Scrollid2\" style='left:0px;width:88px;PosITION:absolute;TOP:200px;FILTER: alpha(opacity=85);'>";
        suspendcode2 += "  <div align=center>";
        suspendcode2 += "      <img src='upload/20160714203225.jpg' border=0  />";
        suspendcode2 += "      <br/>";
        suspendcode2 += "      <img src='images/close2.gif' border=0 onclick='closeScroll();' style='cursor:pointer;' title='鐐瑰嚮鍏抽棴' />";
        suspendcode2 += "  </div>";
        suspendcode2 += "</div>";
        document.write(suspendcode);
        document.write(suspendcode2);
        window.setInterval("heartBeat()", 1);
        function closeScroll() {
            document.getElementById("Scrollid").style.display = 'none';
            document.getElementById("Scrollid2").style.display = 'none';
        }
    </script>
</body>
