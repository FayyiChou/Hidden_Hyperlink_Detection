        <script type="text/javascript">

            $("#searchbox").click(function(){
                var content = $("#content").val();
                self.location =
                        "http://b2b.lenovo.com.cn/"+'Search/index/content/'+content;
            });

            $(function(){
                document.onkeydown = function(e){
                    var ev = document.all ?
                            window.event : e;
                    if(ev.keyCode==13) {
                        var content =
                                $("#content").val();
                        self.location =
                                "http://b2b.lenovo.com.cn/"+'Search/index/content/'+content;

                    }
                }
            });
		
		


		/* 导航切换 */

		/*
		e : 触发元素
		content : 切换内容
		method : 切换方法
		*/
		function navTab(e,content,method,animate){
			var jNav = $(e);
			jNav.on(method,function(){
				var _this = $(this);
				_this.parent().siblings().removeClass("active")
				_this.parent().addClass("active");
				if(_this.next(content).length > 0){
					if(animate == "show"){    
						_this.parent().siblings().removeClass("active").children(content).hide();
						_this.next(content).show();
					}else{
						_this.parent().siblings().removeClass("active").children(content).slideUp();
						_this.next(content).slideDown();
					}
				}
				
				
				
				
			});
		}



		navTab(".nav > li > .nav-tit",".nav-content-box01","mouseenter","slideup");
		navTab(".nav-li01 > .nav-txt01",".nav-content-box02","mouseenter","show");
		navTab(".nav-li02 > .nav-txt02",".nav-content-box03","mouseenter","show");


		// 鼠标放置一级导航执行动作
		$(".nav > li > .nav-tit").on("mouseenter",function(){

			// 导航添加css样式
			if( $(this).next(".nav-content-box01").length > 0 ){
				$(".headerBox").addClass("nav-hover");
			}else{
				return false;
			}

			// 设置元素距离顶部距离
			$(".nav-content-box01").css({"top":$(this).parent().height()})

			// 判断元素是否存在
			if($(".nav-bg").length > 0 ){

			}else{
				// 页面添加元素
				$("body").append("<div class='nav-bg'></div>");
			}
			
		});


		/*// 鼠标放置二级导航执行动作
		$(".nav-li01 > .nav-txt01").on("mouseenter",function(){
			var _this = $(this);
			// 获取一级导航高度
			var navH = _this.parent().parent().height(); 
			// 设置二级导航高度
			_this.next(".nav-content-box02").height(navH)

		});

		// 鼠标放置三级导航执行动作
		$(".nav-li02 > .nav-txt02").on("mouseenter",function(){
			var _this = $(this);
			// 获取2级导航高度
			var navH = _this.parent().parent().height()-2; 
			// 设置3级导航高度
			_this.next(".nav-content-box03").height(navH)
		});*/

		// 鼠标离开导航执行动作
		// 鼠标离开导航执行动作
		$(".nav li.n-li").on("mouseleave",function(){
			// 导航去掉在最上级
			$(".headerBox").removeClass("nav-hover");
			// 删除黑色背景颜色
			$(".nav-bg").remove();

			// 副导航隐藏
			$(".nav-content-box").hide();

			// 导航去掉active样式
			$(".nav-01 > .n-li").removeClass("active");

		})



	</script>
        
