    <script>
    $(function() {
        var suspension = $("#suspension"),
            suspension_top = $("#top1"),
            JWindowwidth = $(window).width();        
            s_t_value = suspension_top.length > 0 ? 50 : "";
        if ($(window).scrollTop() >= s_t_value) {
            suspension.fadeIn();
        } else {
            suspension.fadeOut();
        }
        if(JWindowwidth > 1366){
            suspension.addClass("slide-big")
        }else{
            suspension.addClass("slide-small")
        }
		
		// 右侧悬浮移上去效果
        var jSlideSmall = $(".slide-small-1"),
        	jSlideLinkA = jSlideSmall.find(".slide-link-a a");
        jSlideLinkA.on("mouseenter",function(){
        	var _this = $(this);
        	_this.parent().parent().addClass("active");

    		_this.parent().next().find("a").stop().animate({"margin-left":"120px"},400)

        	_this.parent().next().show()
        		.find("a").eq(_this.index()).stop().animate({"margin-left":"0"},400);
        	
        });

        // 鼠标移开效果
        jSlideSmall.on("mouseleave",function(){
        	var _this = $(this);
        	_this.find(".slide-link-active-a a").stop().animate({"margin-left":"120px"},300,function(){
        		_this.removeClass("active");
        		_this.find(".slide-link-active-a").hide();
        	})
        })
		
		
        $(window).scroll(function() {
            if ($(this).scrollTop() >= s_t_value) {
                suspension.fadeIn()
            } else {
                suspension.fadeOut();
            }
        });

        $(".animate1").on('click', function() {
            datalink($(this))
        });

        function datalink(e) {
            var id;
            var top;
            id = $(e).attr('data-href');
            top = $("#" + id).offset().top;
            $('body,html').stop().animate({
                "scrollTop": top
            },
            600);
        }
    });
</script>

</div>




