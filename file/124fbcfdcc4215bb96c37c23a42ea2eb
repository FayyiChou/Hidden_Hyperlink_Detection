<script>
var con_pf=new Array(4);
queryPF(productid,-56,1588.0,'con_one_1',true);
con_pf[0]=1;
function loadMiniPf(count,range){
	if(con_pf[count-1]!=1){
		queryPF(productid,range,1588.0,'con_one_'+count,false);
		con_pf[count-1]=1;
	}
}
var con_same_pf=new Array(sameListLength);
var con_like_pf=new Array(likeListLength);
function loadSamePf(count,pid,price){
	for(var i=0;i<sameListLength;i++){
		jQuery('#sameRange'+i).hide();
	}
	for(var i=0;i<likeListLength;i++){
		jQuery('#likeRange'+i).hide();
	}
	if(con_same_pf[count-1]!=1){
		querySLPF(pid,-56,price,'sameRange'+count);
		con_same_pf[count-1]=1;
	}
	jQuery('#sameRange'+count).show();
}
function loadlikePf(count,pid,price){
	for(var i=0;i<sameListLength;i++){
		jQuery('#sameRange'+i).hide();
	}
	for(var i=0;i<likeListLength;i++){
		jQuery('#likeRange'+i).hide();
	}
	if(con_like_pf[count-1]!=1){
		querySLPF(pid,-56,price,'likeRange'+count);
		con_like_pf[count-1]=1;
	}
	jQuery('#likeRange'+count).show();
}
