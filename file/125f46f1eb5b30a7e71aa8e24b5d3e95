function changePage(pn)
{
	if (pn > pageTotal) {
		alert('超出最大页码范围');
		return;
	}
	var url = '/184';
	if (pn == 1){
		url += '_1.html';
	}else{
		url += '_' + pn + '.html';
	}
	location.href = url;
}
</script> 
