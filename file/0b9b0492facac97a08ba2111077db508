<script>
$(document).ready(function(){
	$(window).scroll(function(){
		nowtop = parseInt($(document).scrollTop());	
		$('#cleft_box').css('top', nowtop + 135 + 'px')
	})
	$('#cleft_box').hover(function(){
		$(this).css('width','158px');
	},function(){
		$(this).css('width','30px');
	})	
	$('#toTop').click(function(){
		$(document).scrollTop(0);	
	})
})
</script>
