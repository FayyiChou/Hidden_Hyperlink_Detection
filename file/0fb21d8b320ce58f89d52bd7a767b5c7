        }
        //知识产权变更信息
        function onChangeLore(name,uuid){
            var d = dialog({
                id:"seeMovablelog",
                title: '知识产权出质详细信息',
                url: '/business/LookAlterIntRightsInfo.jspx?tMRegNo='+(encodeURIComponent(encodeURIComponent(name)))+'&mainId='+uuid,
                width:950
            });
            d.showModal();
        }
        //知识产权注销信息
        function onExitLore(name,uuid){
            var d = dialog({
                id:"seeMovablelog",
                title: '知识产权出质详细信息',
                url: '/business/CancelIntRightsInfo.jspx?tMRegNo='+(encodeURIComponent(encodeURIComponent(name)))+'&mainId='+uuid,
                width:950
            });
            d.showModal();
        }
        //知识产权无效及其它信息
        function onInvalidLore(name,uuid){
            var d = dialog({
                id:"seeMovablelog",
                title: '知识产权出质详细信息',
                url: '/business/OtherNullIntRightsInfo.jspx?tMRegNo='+(encodeURIComponent(encodeURIComponent(name)))+'&mainId='+uuid,
                width:950
            });
            d.showModal();
        }


      </script>
