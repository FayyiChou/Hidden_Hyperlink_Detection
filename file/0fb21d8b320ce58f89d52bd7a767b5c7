    //显示电子执照
    function showZhiZhao(obj){
        if(obj=="show"){
            $("#u3538").css("visibility","visible").css("z-index","1006");
            $("#u3541").css("visibility","visible").css("display","block");
        }else{
            $("#u3538").css("visibility","hidden").css("z-index","1006");
            $("#u3541").css("visibility","hidden").css("display","block");
            $("#u3510").css("visibility","hidden");
            $("#u3510_state0").css("visibility","hidden");
            $("#u3510_state1").css("visibility","hidden").css("z-index","1006");
        }
    }
    //发送报告
    function showMail(obj){
        if(obj=="hide"){
            $("#u3510").css("visibility","hidden");
            $("#u3510_state0").css("visibility","hidden");
            $("#u3541").css("visibility","hidden").css("display","block");
        }else{
            $("#u3510").css("visibility","visible").css("z-index","1006");
            $("#u3541").css("visibility","visible").css("display","block");
            $("#u3510_state0").css("visibility","visible");
        }
    }
    
    //发送报告 邮件
    function onSendMail(){
        var mail =$("#email_input").val();
        var reg = /^(\w-*\.*)+@(\w-?)+(\.\w{2,})+$/;
        if(reg.test(mail)){
        	//自定义遮罩层隐藏
        	 showZhiZhao("hide");
        	 valiateForAction=2;
        	//弹出验证码
        	 $("#GJGS_u3396").click();
        	 
        	
        	/* $("#sendEmail-2-2").attr("onclick",""); //设置发送事件失效
            $.post("/company/sendEmail.jspx",{"id":"","email":mail},function(data){
                showZhiZhao("hide");
                $("#u3517_input").val("");
                alert("发送成功");
                $("#sendEmail-2-2").attr("onclick","onSendMail()"); //设置发送事件
            }); */
        }else if(mail!=null&&mail!=""){
        	var d = dialog({
			    title: '提示',
			    content: "请输入正确的邮箱格式！",
			    okValue: '确定',
			    ok: function () {}
			}).width(320);
		    d.showModal();
            /* alert("请输入正确的邮箱格式"); */
        }else{
        	var d = dialog({
			    title: '提示',
			    content: "请输入邮箱！",
			    okValue: '确定',
			    ok: function () {}
			}).width(320);
		    d.showModal();
            /* alert("请输入邮箱"); */
        }

    }
    
    
    
    

