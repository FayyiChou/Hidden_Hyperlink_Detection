<!-- 收集访问信息脚本 -->
<script type="text/javascript">
//定义加载文件的函数
var loadJS = function(url,callback){  
	var head = document.getElementsByTagName('head')[0],
    script = document.createElement('script');    
	script.src = url;   
	script.type = "text/javascript";   
	head.appendChild(script);
	
    script.onload = script.onreadystatechange = function(){
	      if ((!this.readyState) || this.readyState == "complete" || this.readyState == "loaded" ){ 
	            callback();      
	      }else{          
	    	  // alert("can not load the js file")     
	      }   
    }
 }

//获取sessionid
function getSessionId(){
	var c_name = 'JSESSIONID';
	if(document.cookie.length>0){
	   c_start=document.cookie.indexOf(c_name + "=")
	   if(c_start!=-1){ 
	     c_start=c_start + c_name.length+1 
	     c_end=document.cookie.indexOf(";",c_start)
	     if(c_end==-1) c_end=document.cookie.length
	     return unescape(document.cookie.substring(c_start,c_end));
	   }
	}
}
//收集信息	 
$(window).load(function() {
	 $.get("/ifCollectInfo.jspx",function(data){
		 if(data=="1"){
				loadJS("http://fwtj.gsxt.gov.cn/js/logcollector.js?nodenum=410000",function(){
					sessionId_gsxt = getSessionId(); // 正常情况JS可以拿到sessionId,但经过测试发现有些web服务器，js拿不到sessionId，这种情况需要手动对sessionId进行赋值，如sessionId_gsxt=
					t1Collect_gsxt(); // 收集访问信息，每个页面都需要调用该方法
					t2Collect_gsxt(''); // 企业详情页收集被访问的企业名称，当用户进入企业详情页时，调用该方法，并将企业完整名称作为该方法的入参
		        });
		 }
	 })
})
</script>









<script type="text/javascript">
     //返回到中部
     function  srcollToMiddle(){
    	 $("html,body").animate({scrollTop: $("#guidedivall").offset().top}, 1000);
     }


    var myurl = window.location.href;
    function onGuideMouseOut(obj){
        if($(obj).attr("class").indexOf("guideselected")<0){
            $(obj).find("span").attr("style","");
        }
    }
    //n代表的是当前的是第几页；totalPage代表的是总页数
    function goPage1(flag, n, totalPage, order) {
        var isNot = true;
        if (order == 0) {
            if (n > totalPage) {
                isNot = false;
            }
        } else if (order == 1) {
            if (n == 1) {
                isNot = false;
            }
        } else if (order == 2) {
            if ((n + 1) > totalPage) {
                isNot = false;
            }
        }
        var uuId = document.getElementById("uuId").value;
        var mainId = document.getElementById("mainId").value;
        var request = new ajax.Request();
        if (flag != null && flag == 0 && isNot) { //股东及出资信息
            request.loadTextByGet("/business/QueryInvList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId, refreshInvList);
        } else if (flag != null && flag == 1 && isNot) {//变更信息
            request.loadTextByGet("/business/QueryAltList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId, refreshAltList);
        } else if (flag != null && flag == 2) { //经营异常信息
            request.loadTextByGet("/business/QueryExcList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId + "&ran=" + Math.random(), refreshExcList);
        } else if (flag != null && flag == 3) { //严重违法失信信息
            request.loadTextByGet("/business/QuerySerillList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId, refreshSerillList);
        } else if (flag != null && flag == 4) { //抽查检查信息
            request.loadTextByGet("/business/QuerySpotCheckList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId, refreshSpotCheckList);
        } else if (flag != null && flag == 5) { //动产抵押信息
            request.loadTextByGet("/business/QueryMortList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId + "&ran=" + Math.random(), refreshMortList);
        } else if (flag != null && flag == 6) { //股权出质登记信息
            request.loadTextByGet("/business/QueryPledgeList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId + "&ran=" + Math.random(), refreshPledgeList);
        } else if (flag != null && flag == 7) { //行政处罚
            request.loadTextByGet("/business/QueryPunList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId + "&ran=" + Math.random(), refreshPunList);
        } else if (flag != null && flag == 8) {  //行政许可  licenseRegDiv2
            request.loadTextByGet("/business/QueryLicRegList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId + "&ran=" + Math.random(), refreshLicRegList);
        } else if (flag != null && flag == 9) { //司法协助
            request.loadTextByGet("/business/QueryInfoList.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId + "&ran=" + Math.random(), refreshInfoList);
        } else if(flag != null && flag == 10){
            request.loadTextByGet("/investorCurrentPage.jspx?random="+Math.random()+"&pno=" + n + "&order=" + order + "&mainId="+mainId, refreshInvListInv);
        } else if(flag != null && flag == 11){
            request.loadTextByGet("/equityTransferCurrentPage.jspx?random="+Math.random()+"&pno=" + n + "&order=" + order + "&mainId="+mainId, refreshInvListEquity);
        } else if(flag != null && flag == 12){
            request.loadTextByGet("/licenseRegCurrentPage.jspx?random="+Math.random()+"&pno=" + n + "&order=" + order +"&mainId="+mainId, refreshInvListLicense);
        }else if(flag != null && flag == 13){
            request.loadTextByGet("/IntRightsPage.jspx?random="+Math.random()+"&pno=" + n + "&order=" + order + "&mainId="+mainId, refreshInvListIntRig);
        }else if(flag != null && flag == 14){//即时信息-行政处罚信息
            request.loadTextByGet("/PunishPage.jspx?random="+Math.random()+"&pno=" + n + "&order=" + order + "&mainId="+mainId, refreshInvListPunish);
        }else if(flag != null && flag == 15){
            request.loadTextByGet("/business/BrandInfoPage.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId, refreshInvListBrand);
        } else if (flag != null && flag == 16){//即时信息-股东及出资信息
            request.loadTextByGet("/business/invInfoPage.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId + "&ran=" + Math.random(), refreshInvListInvInfo);
        } else if(flag != null && flag == 17){ // 知识产权分页 工商公示
            request.loadTextByGet("/business/IntellectualProperty.jspx?pno=" + n + "&order=" + order + "&mainId=" + uuId, refreshIntellectualProperty);
        }
    }
     //知识产权详细信息页面  工商公示
     function seeIntellAppendix(intellId){
        var d = dialog({
            id:"seeIntellAppendixlog",
            title: '知识产权出质详细信息',
            url: '/business/intellectualPropertyDetail.jspx?intellId=' + intellId,
            width:950
        });
        d.showModal();
    }
    function seeMovable(id) {
        var d = dialog({
            id:"seeMovablelog",
            title: '动产抵押详细信息',
            url: '/business/mortInfoDetail.jspx?id=' + id,
            width:950
        });
        d.showModal();
    }
    function seeJudicial(invIdentID, frozState, id, firstFrozId, entId) {
        var d = dialog({
            id:"seeJudiciallog",
            title: '司法股权冻结详细信息',
            url: '/business/infoNoticeJudicialAssDetail.jspx?invIdentID=' + invIdentID + "&frozState=" + frozState + "&freezeId=" + id + "&firstFrozId=" + firstFrozId + "&mainId=" + entId,
            width:950
        });
        d.showModal();
    }

    function seeJudicialinfo(id) {
        var d = dialog({
            id:"seeJudicialinfolog",
            title: '司法股东变更详细信息',
            url: '/business/shareholderChangeDeatil.jspx?shareholderChangeId=' + id,
            width:950
        });
        d.showModal();
    }
    function seeInvest(id, typeName) {
        var d = dialog({
            id:"seeInvestlog",
            title: typeName + '及出资详细信息',
