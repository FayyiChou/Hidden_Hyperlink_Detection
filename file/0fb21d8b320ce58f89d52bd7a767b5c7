//股权变更信息
         function getGQBGPageNo(no,totalPage){
         var uuId = document.getElementById("uuId").value;
         var mainId = document.getElementById("mainId").value;
         var GQBGhnstr="";
         var GQBGPage="";
         var number = no -1;
         var first = "首页";
         var pre = "上一页";
         var alt = "下一页";
         var last = "尾页";
          $.ajax({
                url:"/business/pageGQBGltInfo.jspx?mainId" + mainId,
              data:{"currentPage":no,"mainId":mainId},
                type:'post',
                async:false,
                dataType:'json',   //ajax 返回 文件 类型,
                success: function(data) {
                    var length = data.objlist.length;
                    $("#GQBGinfoData tr").remove();

                    for(var i = 0 ;i<length;i++){
                        if((i%2)==0){
                            GQBGhnstr+="<tr style='background-color:#F9F9F9;'>";
                        }else{
                            GQBGhnstr+="<tr style='background-color:#F9F9F9;'>";
                        }
                        $('#GQBGinfoData').append(GQBGhnstr + '<td width="9%" style="text-align:center;">'
                                + (number * 5 + 1 + i)
                                +'</td><td width="23%" style="text-align:center;">'
                                +data.objlist[i]['preInvName']
                                +'</td><td width="18%" style="text-align:center;">'
                                +data.objlist[i]['preTransAmPr'] + '%'
                                +'</td><td width="18%" style="text-align:center;">'
                                +data.objlist[i]['altTransAmPr'] + '%'
                                +'</td><td width="18%" style="text-align:center;">'
                                +dataToStrChinese(data.objlist[i]['altDate'])
                                +'</td><td width="14%" style="text-align:center;">'
                                +dataToStrChinese(data.objlist[i]['createdate'])
                                +'</td></tr>')
                    }


                    $("#GQBGfenye a").remove();
                    if(no > 1){
                        GQBGPage = "<li><a href='javascript:getGQBGPageNo( 1 ,"+ totalPage + ")'>首页</a></li>";
                        GQBGPage+= "<li><a href='javascript:getGQBGPageNo("+ (no-1) +" ,"+ totalPage + ")'><img class='imgfenye' src='\\images\\left.png'/>&nbsp;上一页</a></li>";
                    }else{
                        GQBGPage = "<li><a disabled='disabled' href='javascript:getGQBGPageNo( 1 ,"+ totalPage + ")'>首页</a></li>";
                        GQBGPage+= "<li><a disabled='disabled' href='javascript:getGQBGPageNo(1 ,"+ totalPage + ")'><img class='imgfenye' src='\\images\\left.png'/>&nbsp;上一页</a></li>";
                    }
                    if(no > 4 && totalPage-no<1){
                        GQBGPage+= "<li><a href='javascript:getGQBGPageNo("+ (no-4) +" ,"+ totalPage + ")'>" +(no-4)+"</a></li>";
                    }
                    if(no > 3 && totalPage-no<2){
                        GQBGPage+= "<li><a href='javascript:getGQBGPageNo("+ (no-3) +" ,"+ totalPage + ")'>" +(no-3)+"</a></li>";
                    }
                    if(no > 2){
                        GQBGPage+= "<li><a href='javascript:getGQBGPageNo("+ (no-2) +" ,"+ totalPage + ")'>" +(no-2)+"</a></li>";
                    }
                    if(no > 1){
                        GQBGPage+= "<li><a href='javascript:getGQBGPageNo("+ (no-1) +" ,"+ totalPage + ")'>" +(no-1)+"</a></li>";
                    }
                        GQBGPage+= "<li><a class='fenye-bj' style='color:#FFF;' href='javascript:getGQBGPageNo("+ no +" ,"+ totalPage + ")'>" +no+"</a></li>";
                    if(totalPage-no>0){
                        GQBGPage+= "<li><a  href='javascript:getGQBGPageNo("+ (no+1) +" ,"+ totalPage + ")'>" +(no+1)+"</a></li>";
                    }
                    if(totalPage-no>1){
                        GQBGPage+= "<li><a  href='javascript:getGQBGPageNo("+ (no+2) +" ,"+ totalPage + ")'>" +(no+2)+"</a></li>";
                    }
                    if(totalPage-no>2 && no < 3){
                        GQBGPage+= "<li><a  href='javascript:getGQBGPageNo("+ (no+3) +" ,"+ totalPage + ")'>" +(no+3)+"</a></li>";
                    }
                    if(totalPage-no>3 && no < 2){
                        GQBGPage+= "<li><a  href='javascript:getGQBGPageNo("+ (no+4) +" ,"+ totalPage + ")'>" +(no+4)+"</a></li>";
                    }
                    if(totalPage-no>0){
                        GQBGPage+= "<li><a href='javascript:getGQBGPageNo("+ (no+1) +" ,"+ totalPage + ")'>下一页&nbsp;<img class='imgfenye' src='\\images\\right.png'/></a></li>";
                        GQBGPage += "<li><a href='javascript:getGQBGPageNo( "+totalPage+" ,"+ totalPage + ")'>末页</a></li>";
                    }else{
                        GQBGPage+= "<li><a disabled='disabled' href='javascript:getGQBGPageNo("+totalPage+" ,"+ totalPage + ")'>下一页&nbsp;<img class='imgfenye' src='\\images\\right.png'/></a></li>";
                        GQBGPage+= "<li><a disabled='disabled' href='javascript:getGQBGPageNo( "+totalPage+" ,"+ totalPage + ")'>末页</a></li>";
                    }
                 $('#GQBGfenye').append(GQBGPage);


                }
            });
    }

    </script>

