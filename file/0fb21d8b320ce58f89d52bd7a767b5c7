      <script type="text/javascript">
          $(function(){
              loadHeadHtml();
              loadEndHtml();
              loadGuideHtml();
              loadBasicHtml();

              guideToDetail("YYZZ", document.getElementById("uuId").value);
              
              //添加手机浏览器自适应
              aduptPhone();
              //手机分享时修改title
              if ((navigator.userAgent.match(/(iPhone|iPod|Android|ios|iPad)/i)))
              {    //手机端
              	 $(document).attr("title","国家企业信用信息公示系统");
           
              }
          })

            //添加手机浏览器自适应
	     function  aduptPhone(){
		       $.get("/ifAduptPhone.jspx",function(data){
		       	 //初始將a.html include div#iframe
		       	data=JSON.parse(data)
		       	//alert(data.result);
		       	 if(data.result){
		       		$("head").append("<meta http-equiv='X-UA-Compatible' content='IE=edge'>"+
		       			  "<meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>");
		       	 }
		   	　});
	        }
          //加载头部
          function  loadHeadHtml(){
              $.get("/company/head.jspx",function(data){
                  //初始將a.html include div#iframe
                  $("#headdiv").html(data);
              });
          }
          //加载底部
          function  loadEndHtml(){
              $.get("/company/end.jspx",function(data){
                  //初始將a.html include div#iframe
                  $("#enddiv").html(data);
              });
          }

          //加载导航栏
          function loadGuideHtml() {
              var idStr=$("#uuId").val();
              var jyzk="";
              $.get("/company/guide.jspx?id="+idStr+"&jyzk="+jyzk, function(data) {
                  $("#guideItemsDiv").html(data);
              });
          }
          //加载企业基本信息
          function loadBasicHtml() {
              var idStr=$("#uuId").val();
              $.get("/company/basic.jspx?id="+idStr, function(data) {
                  $("#companysimpleinfo").html(data);
              });
          }
          //加载导航详细
          function loadDetailHtml() {
              var id="123";
              $.get("/company/detail.jspx?id="+id, function(data) {
                  $("#guideItemsDiv").html(data);
              });
          }

          //导航到详细内容
        function guideToDetail(str, id) {
            if(str=="QYNBXX"){//年报信息
                $.ajax({
                    type: "post",
                    url:"/yearExm/checkYearExmNum.jspx",
                    data:"uuId="+id,
                    dataType: "json",
                    success:function (data) {
                        data = eval('(' + data + ')');
                        if (data.success == false) {
                            alert("该企业暂无年度报告信息！");
                        } else {
                            $.get("/yearExm/" + str + ".jspx?ram="+Math.random()+"&id=" + id, function(data) {
                                $("#guideDetailDiv").html(data);
                            });
                        }
                    }, async:true
                });

            } else if (str == "QYJS") {
                $.get("/company/guide/qiyejishixinxi.jspx?id=" + id+"&random="+Math.random(), function(data) {
                    $("#guideDetailDiv").html(data);
                });
            } else if (str == "YYZZ") {
                document.title = '登记备案信息';
                $.get("/business/JCXX.jspx?id=" + id+"&date="+new Date(), function(data) {
                    $("#guideDetailDiv").html(data);
                    adapt_jyfw_height();  // 调整经营范围显示格式
                });
            } else{
                if("XZXK" == str){
                    document.title = '行政许可信息';
                }else if("XZCF" == str){
                    document.title = '行政处罚信息';
                }else if("JYYC" == str){

                    document.title = '列入经营异常名录信息';
                }else{
                    document.title = '列入严重违法失信企业名单（黑名单）信息';
                }
                $.get("/business/" + str + ".jspx?id=" + id, function(data) {
                    $("#guideDetailDiv").html(data);
                });
            }
        }

        //行政许可信息
        function xzxkList(pageno,id){
            var hnstr="";
            $.ajax({
                type:"post",
                url :"/business/licenseRegCurrentPage.jspx?pageNo="+pageno+"&mainId="+id+"&t="+new Date(),
                dataType : "json",
                async : true,
                error : function(XMLHttpRequest, textStatus, errorThrown) {
                    alert("加载出错！");
                },
                success : function(data) {
                    $("#xzxkListTable").html("");
                    $("#xzxkPage").html("");
                    $.each(data.list,function(i,item){
                        if((i%2)==0){
                            hnstr+="<tr style='background-color:#F9F9F9;'>";
                        }else{
                            hnstr+="<tr style='background-color:#F9F9F9;'>";
                        }
                        hnstr+="<td width='49px'>"+(((data.pageNo-1)*5)+(i+1))+"</td>";
                        hnstr+="<td width='140px'>"+item.licNO+"</td>";
                        hnstr+="<td width='142px'>"+item.licName+"</td>";
                        hnstr+="<td width='124px'>"+(item.valFrom==null?"":new Date(item.valFrom).pattern("yyyy年MM月dd日"))+"</td>";
                        hnstr+="<td width='124px'>"+(item.valTo==null?"":new Date(item.valTo).pattern("yyyy年MM月dd日"))+"</td>";
                        hnstr+="<td width='186px'>"+(item.licAnth==null?"":item.licAnth)+"</td>";
                        hnstr+="<td width='161px'>"+(item.licItem==null?"":item.licItem)+"</td>";
                        var typeStr="";
                        if(item.type==3 || item.type==4 || item.type==5){
                           typeStr="无效";
                        }else{
                           typeStr="有效";
                        }
                        hnstr+="<td width='69px'>"+typeStr+"</td>";
                        if(item.type==5 || item.type==4 || item.type==3 || item.type==2){
                            hnstr+="<td width='63px'><a class=\"channelone\" href=\"javascript:seeAtiveLicensingInfo("+item.type+",'"+item.licNO+"')\" >查看</a></td>";
                        }else{
                            hnstr+="<td width='63px'>无</td>";
                        }
                        hnstr+="</tr>";
                    });
                    var id=$("#xzxkid").val();
                    var pagestr="<li>共查询到&nbsp;"+data.totalcount+"&nbsp;条信息&nbsp;&nbsp;</li>";
                    pagestr+="<li>共&nbsp;"+data.tatalpageno+"&nbsp;页</li>";
                    if(data.pageNo>1){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"javascript:xzxkList(1,'"+id+"')\">首页</a></li>";
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"javascript:xzxkList("+(data.pageNo-1)+",'"+id+"')\"><img class='imgfenye' src='\\images\\left.png'/>&nbsp;上一页</a></li>";
                    }else{
                        pagestr+="<li><a href='javascript:void(0)' disabled='disabled' onclick=\"javascript:xzxkList(1,'"+id+"')\">首页</a></li>";
                        pagestr+="<li><a href='javascript:void(0)' disabled='disabled' onclick=\"javascript:xzxkList(1,'"+id+"')\"><img class='imgfenye' src='\\images\\left.png'/>&nbsp;上一页</a></li>";
                    }
                    if(data.tatalpageno - data.pageNo ==0 && data.tatalpageno>=5){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo-4)+",'"+id+"');\">"+(data.pageNo-4)+"</a></li>";
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo-3)+",'"+id+"');\">"+(data.pageNo-3)+"</a></li>";
                    }
                    if(data.tatalpageno - data.pageNo ==1 && data.tatalpageno>=5){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo-3)+",'"+id+"');\">"+(data.pageNo-3)+"</a></li>";
                    }
                    if(data.pageNo>3 && data.tatalpageno<5){pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo-3)+",'"+id+"');\">"+(data.pageNo-3)+"</a></li>";}
                    if(data.pageNo>2){pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo-2)+",'"+id+"');\">"+(data.pageNo-2)+"</a></li>";}
                    if(data.pageNo>1){pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo-1)+",'"+id+"');\">"+(data.pageNo-1)+"</a></li>";}
                    pagestr+="<li><a  class='fenye-bj' style='color:#FFF;'>"+data.pageNo+"</a></li>";
                    if((data.tatalpageno-data.pageNo)>0){pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo+1)+",'"+id+"');\">"+(data.pageNo+1)+"</a></li>";}
                    if((data.tatalpageno-data.pageNo)>1){pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo+2)+",'"+id+"');\">"+(data.pageNo+2)+"</a></li>";}
                    if((data.tatalpageno-data.pageNo)>2 && data.tatalpageno<5){pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo+3)+",'"+id+"');\">"+(data.pageNo+3)+"</a></li>";}
                    if(data.tatalpageno>=5 && data.pageNo ==2){pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo+3)+",'"+id+"');\">"+(data.pageNo+3)+"</a></li>";}
                    if(data.tatalpageno>=5 && data.pageNo ==1){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo+3)+",'"+id+"');\">"+(data.pageNo+3)+"</a></li>";
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo+4)+",'"+id+"');\">"+(data.pageNo+4)+"</a></li>";
                    }
                    if((data.tatalpageno-data.pageNo)>0){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+(data.pageNo+1)+",'"+id+"');\">下一页&nbsp;<img class='imgfenye' src='\\images\\right.png'/></a></li>";
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"xzxkList("+data.tatalpageno+",'"+id+"');\">末页</a></li>";
                    }else{
                        pagestr+="<li><a href='javascript:void(0)' disabled='disabled' onclick=\"xzxkList("+data.tatalpageno+",'"+id+"');\">下一页&nbsp;<img class='imgfenye' src='\\images\\right.png'/></a></li>";
                        pagestr+="<li><a href='javascript:void(0)' disabled='disabled' onclick=\"xzxkList("+data.tatalpageno+",'"+id+"');\">末页</a></li>";
                    }
                    $("#xzxkListTable").html(hnstr);
                    $("#xzxkPage").html(pagestr);
                }
            });
        }
        //行政许可详情
        function seeAtiveLicensingInfo(type,licno){
            var id=$("#xzxkid").val();
            licno = encodeURIComponent(encodeURIComponent(licno));
            var url="";
            var title="";
            if(type=="2"){
                //变更
                title="行政许可详细信息";
                url="/business/AlterInfoLicenseReg.jspx?mainId="+id+"&type="+type+"&licNO="+licno;
            }else if(type=="3"){
                //注销
                title="行政许可详细信息";
                url="/business/CancelInfoLicenseReg.jspx?mainId="+id+"&type="+type+"&licNO="+licno;
            }else if(type=="5"){
                //其它无效
                title="行政许可详细信息";
                url="/business/OtherNullInfoLicenseReg.jspx?mainId="+id+"&type="+type+"&licNO="+licno;
            }else if(type=="4"){
                //吊销
                title="行政许可详细信息";
                url="/business/RevLicenseReg.jspx?mainId="+id+"&type="+type+"&licNO="+licno;
            }
            var d = dialog({
                id:"seeMovablelog",
                title: title,
                url: url,
                width:950
            });
            d.showModal();
        }

        //知识产权信息ajax分页
        function loreList(pageno,id){
            var hnstr="";
            //location="/lore/ajax/list.jspx?nowPageNum="+pageno+"&mpid="+id;
            $.ajax({
                type:"post",
                url :"/lore/ajax/list.jspx?nowPageNum="+pageno+"&mpid="+id+"&t="+new Date(),
                dataType : "json",
                async : true,
                error : function(XMLHttpRequest, textStatus, errorThrown) {
                    alert("加载出错！");
                },
                success : function(data) {
                    $("#zscqListtable").html("");
                    $("#detailsPage").html("");
                    var obj=eval("("+data+")");
                    $.each($.parseJSON(data).obj,function(i,item){
                        if((i%2)==0){
                            hnstr+="<tr style='background-color:#F9F9F9;'>";
                        }else{
                            hnstr+="<tr style='background-color:#F9F9F9;'>";
                        }
                        hnstr+="<td width='44px'>"+((($.parseJSON(data).data.page.nowPageNum-1)*5)+(i+1))+"</td>";
                        hnstr+="<td width='192px'>"+(item.tmRegNo==null?"":item.tmRegNo)+"</td>";
                        hnstr+="<td width='95px'>"+(item.tmName==null?"":item.tmName)+"</td>";
                        hnstr+="<td width='65px'>"+(item.cogGoS==null?"":item.cogGoS)+"</td>";
                        hnstr+="<td width='97px'>"+(item.pledgor==null?"":item.pledgor)+"</td>";
                        hnstr+="<td width='90px'>"+(item.ompOrg==null?"":item.ompOrg)+"</td>";
                        hnstr+="<td width='240px'>"+(item.pleRegPerFrom==null?"":new Date(item.pleRegPerFrom).pattern("yyyy年MM月dd日"))+"-"+(item.pleRegPerTo==null?"":new Date(item.pleRegPerTo).pattern("yyyy年MM月dd日"))+"</td>";
                        var typeStr="";
                        if (item.type == 3 || item.type == 4) {
                            typeStr = "无效";
                        } else {
                            typeStr = "有效";
                        }
                        hnstr+="<td width='50px'>"+typeStr+"</td>";
                        hnstr+="<td width='125px'>"+(item.createDate==null?"":new Date(item.createDate).pattern("yyyy年MM月dd日"))+"</td>";
                        if(item.type==2){
                           hnstr+="<td width='51px'><a class=\"channelone\" href=\"javascript:onChangeLore('"+item.tmRegNo+"','"+$.parseJSON(data).data.mainbody.uuid+"')\" >查看</a></td>";
                        }else if(item.type==3){
                           hnstr+="<td width='51px'><a class=\"channelone\" href=\"javascript:onExitLore('"+item.tmRegNo+"','"+$.parseJSON(data).data.mainbody.uuid+"')\" >查看</a></td>";
                        }else if(item.type==4){
                           hnstr+="<td width='51px'><a class=\"channelone\" href=\"javascript:onInvalidLore('"+item.tmRegNo+"','"+$.parseJSON(data).data.mainbody.uuid+"')\" >查看</a></td>";
                        }else{
                           hnstr+="<td width='51px'>无</td>"
                        }
                        hnstr+="</tr>";
                    });
                    var id=$("#zscqid").val();
                    var pagestr="<li>共查询到&nbsp;"+$.parseJSON(data).data.page.toteCount+"&nbsp;条信息&nbsp;&nbsp;</li>";
                    pagestr+="<li>共&nbsp;"+$.parseJSON(data).data.page.totePage+"&nbsp;页</li>";
                    if($.parseJSON(data).data.page.nowPageNum>1){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"javascript:loreList(1,'"+id+"')\">首页</a></li>";
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"javascript:loreList("+($.parseJSON(data).data.page.nowPageNum-1)+",'"+id+"')\"><img class='imgfenye' src='\\images\\left.png'/>&nbsp;上一页</a></li>";
                    }else{
                        pagestr+="<li><a href='javascript:void(0)' disabled='disabled' onclick=\"javascript:loreList(1,'"+id+"')\">首页</a></li>";
                        pagestr+="<li><a href='javascript:void(0)' disabled='disabled' onclick=\"javascript:loreList(1,'"+id+"')\"><img class='imgfenye' src='\\images\\left.png'/>&nbsp;上一页</a></li>";
                    }
                    if(($.parseJSON(data).data.page.totePage-$.parseJSON(data).data.page.nowPageNum) ==0 && $.parseJSON(data).data.page.totePage>=5){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum-4)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum-4)+"</a></li>";
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum-3)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum-3)+"</a></li>";
                    }
                    if(($.parseJSON(data).data.page.totePage-$.parseJSON(data).data.page.nowPageNum) ==1 && $.parseJSON(data).data.page.totePage>=5){
                        pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum-3)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum-3)+"</a></li>";
                    }
                    if($.parseJSON(data).data.page.nowPageNum>3 && $.parseJSON(data).data.page.totePage<5){pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum-3)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum-3)+"</a></li>";}
                    if($.parseJSON(data).data.page.nowPageNum>2){pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum-2)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum-2)+"</a></li>";}
                    if($.parseJSON(data).data.page.nowPageNum>1){pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum-1)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum-1)+"</a></li>";}
                    pagestr+="<li><a  class='fenye-bj' style='color:#FFF;'>"+$.parseJSON(data).data.page.nowPageNum+"</a></li>";
                    if(($.parseJSON(data).data.page.totePage-$.parseJSON(data).data.page.nowPageNum)>0){pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum+1)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum+1)+"</a></li>";}
                    if(($.parseJSON(data).data.page.totePage-$.parseJSON(data).data.page.nowPageNum)>1){pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum+2)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum+2)+"</a></li>";}
                    if(($.parseJSON(data).data.page.totePage-$.parseJSON(data).data.page.nowPageNum)>2 && $.parseJSON(data).data.page.totePage<5){pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum+3)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum+3)+"</a></li>";}
                    if($.parseJSON(data).data.page.totePage>=5 && $.parseJSON(data).data.page.nowPageNum ==2){pagestr+="<li><a href='javascript:void(0)' onclick=\"loreList("+($.parseJSON(data).data.page.nowPageNum+3)+",'"+id+"');\">"+($.parseJSON(data).data.page.nowPageNum+3)+"</a></li>";}
