function searchbox2012() {
  var searchbox = document.getElementById('inputbox2012');
  if ((searchbox.value != searchbox.defaultValue) && (searchbox.value != '')) {
    window.open('http://search.12371.cn/index.php?qtext=' + encodeURIComponent(searchbox.value));
  }
}
function getParameter(args) {
  var url = window.location.href;
  var paraString = url.substring(url.indexOf('?') + 1, url.length).split('&');
  var paraObj = {};
  for (var i = 0; j = paraString[i]; i++) {
    paraObj[j.substring(0, j.indexOf('=')).toLowerCase()] = j.substring(j.indexOf("=") + 1, j.length);
  }
  var returnValue = paraObj[args.toLowerCase()];
  if (typeof(returnValue) == 'undefined') {
    return "";
  } else {
    return returnValue;
  }
}
function getCookie(objName) {
  var arrStr = document.cookie.split("; ");
  for (var i = 0; i < arrStr.length; i++) {
    var temp = arrStr[i].split("=");
    if (temp[0] == objName) return unescape(temp[1]);
  }
}
