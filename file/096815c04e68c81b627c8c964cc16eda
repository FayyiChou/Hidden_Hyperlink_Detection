//日历
var now = new Date();
function RunTime() {
  var weeks = new Array('星期日', '星期一', '星期二', '星期三', '星期四', '星期五', '星期六');
  var year = now.getFullYear();
  var month = now.getMonth() + 1;
  var date = now.getDate();
  if (month < 10) {
    month = '0' + month;
  }
  if (date < 10) {
    date = '0' + date;
  }
  return year + '年' + month + '月' + date + '日 ' + weeks[now.getDay()];
}
