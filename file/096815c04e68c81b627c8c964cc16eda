<script type="text/javascript">
    /* {vtype:'vtype',vid:2,vtitle:'vtitle',vpic:'vpic',vurl:'vurl',vnums:10,vdesc:'vdesc',vspeaker:'vspeaker',vtime:12345678987,uid:1,r:Math.ceil(Math.random()*10000)}

     */
function shoucang(vtype,vid,vtitle,vpic,vurl,vnums,vdesc,vspeaker,vtime,objid) {//收藏
    var bbsUrl = "http://bbs.12371.cn/api/drefer.php";
    var bbs_re_url = bbsUrl+"?drefer="+encodeURIComponent(window.location.href);

    $.ajax({
        url:'http://bbs.12371.cn/api/api_check_login.php?r='+Math.ceil(Math.random()*10000),
        dataType:"jsonp",
        jsonp:"jsonpcallback",
        success:function(data){
            if(data['uid'] > 0){
                $.ajax({
                    url:'http://bbs.12371.cn/api/store/store.php',
                    dataType:"jsonp",
                    jsonp:"jsonpcallback",
                    data:{vtype:vtype,vid:vid,vtitle:vtitle,vpic:vpic,vurl:vurl,vnums:vnums,vdesc:vdesc,vspeaker:vspeaker,vtime:vtime,uid:data['uid'],r:Math.ceil(Math.random()*10000)},
                    success:function(sdata){
                        showMsgBox(objid,sdata.status);
                    }
                });
            } else {//用户没有登录				
                window.location.href="http://passport.12371.cn/loginForm?service="+encodeURIComponent(bbs_re_url);
            }
        }
    });
}

function shoucangnum(vid) {//收藏数
    $.ajax({
        url:'http://bbs.12371.cn/api/store/get_storenum.php?vid='+vid+'&r='+Math.ceil(Math.random()*10000),
        dataType:"jsonp",
        jsonp:"jsonpcallback",
        success:function(data){
            document.getElementById('shouchangnum').innerHTML="收藏("+data.num+")";
        }
    });
}

function showMsgBox(objid,status){
    var theobj=document.getElementById(objid);
    switch(status){
        case 1:
        　　     theobj.style.backgroundImage='url(http://r.img.cctvpic.com/dystudy/contentstyle/article/style/img/icon_02.jpg)';
            theobj.innerHTML='收藏成功！';
  　　   break;
　　   case 2:
    　　    theobj.style.backgroundImage='url(http://r.img.cctvpic.com/dystudy/contentstyle/article/style/img/icon_01.jpg)';
        theobj.innerHTML='请勿重复收藏！';
 　　    break;
　　   case 3:
    　　     theobj.style.backgroundImage='url(http://r.img.cctvpic.com/dystudy/contentstyle/article/style/img/icon_03.jpg)';
        theobj.innerHTML='收藏失败，请稍候再试！';
　　     break;
        default:
            return;
    }
    theobj.style.display='block';
    var t=setTimeout(function(){theobj.style.display='none'},2000);
}

