function hideSelect(e) {
  var evn = kEvent1.getTarget(e);
  if (evn.parentNode) {
    if (evn.parentNode.parentNode) {
      if (evn.parentNode.parentNode != document) {
        if (evn.parentNode.getAttribute("id") == "goto_btn" || evn.parentNode.getAttribute("id") == "goto_btn1" || evn.parentNode.getAttribute("id") == "goto_btn21" || evn.parentNode.getAttribute("id") == "goto_btn3" || evn.parentNode.getAttribute("id") == "goto_btn4" || evn.parentNode.getAttribute("id") == "goto_btn5" || evn.getAttribute("id") == "goto_btn" || evn.getAttribute("id") == "goto_btn1" || evn.getAttribute("id") == "goto_btn21" || evn.getAttribute("id") == "goto_btn3" || evn.getAttribute("id") == "goto_btn4" || evn.getAttribute("id") == "goto_btn5") {
          return;
        }
      }
    }
  }
