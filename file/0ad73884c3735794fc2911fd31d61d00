	$(this).find(".no_block").show();
	$(this).addClass("mary");
	$(this).siblings(".swiper-slide ul li").find(".no_block").hide();
	$(this).siblings(".swiper-slide ul li").removeClass("mary");
	$(this).siblings(".swiper-slide ul li").css("width","94px");
	$(this).siblings(".swiper-slide ul li").css("height","94px");
	$(this).siblings(".swiper-slide ul li").css("background","#fff");
	$(this).siblings(".swiper-slide ul li").css("margin-right","45px");
	$(this).siblings(".swiper-slide ul li").css("margin-left","0px");
	$(this).siblings(".swiper-slide ul li").css("margin-top","0px");
	$(this).siblings(".swiper-slide ul li").css("padding-top","0");
	$(this).siblings(".swiper-slide ul li").find(".slpic").css("width","84px");
	$(this).siblings(".swiper-slide ul li").find(".slpic").css("height","84px");	
});
</script>
