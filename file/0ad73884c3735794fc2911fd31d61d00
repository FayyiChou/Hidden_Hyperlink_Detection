  function getjiuyeinfo()
 {
	  $.get("/getyuancheng.php", { "act":"jiuyenum"},
    function(req) {
       var data=eval("(" + req + ")");
	   var num=data['num'];
	   var yxnum=data['yxnum'];
	    var exnum=data['exnum'];
		$("#jiuyenum").html(num);
		$("#jiuyeyxnum").html(yxnum);
		$("#jiuyeexnum").html(exnum);
    }) 
 }
	  setTimeout("getjiuyeinfo()",1000);	
	$("#qu_tiwen").click(function()
	{
		var qu_title=$.trim($("#qu_title").val());
		var qu_memo=$.trim($("#qu_memo").val());
		if(qu_memo=="")
		{
			alert('请填写内容');
			return false;
		}
				$.ajax({			  
			 type:"POST",
			 url:"/plus/question_save.php",
			  cache:'false',
			 data:{title:qu_title,msg:qu_memo},
			 success:function(data,st){
			 alert(data);
			 $("#qu_memo").val('');
			  $("#qu_title").val('');
			$("#wytw").modal("hide")
				 }
			 });
	})
