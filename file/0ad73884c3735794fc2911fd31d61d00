  <!--最新就业文字-->
<script>
function zxjyNew(){
  var showHeight = 496;
  var displayContent = $('#colee1');
  var displayLists = $('#colee1 li');
  var totalHeight = parseInt(displayContent.css('height'));
  var cellHeight = 60;
  var addNum = Math.ceil(showHeight / cellHeight);
  for (var i = 0; i < addNum; i++){
    var list = displayLists.eq(i % displayLists.length).clone();
    displayContent.append(list);
  }
  var aswitch = 1;
  displayContent.mouseover(function(){
      aswitch = 0;
  });
  displayContent.mouseout(function(){
    aswitch = 1;
  });

  var step = 1;
  var offsetTop = 0;
    
  function displayAnimate(){
    if(offsetTop - step < - totalHeight) {
        offsetTop = 0;
        displayContent.css('margin-top', '0px');
    }
    if (aswitch == 1) {
      offsetTop = offsetTop - step;
    }
    displayContent.animate({marginTop: offsetTop}, 30, displayAnimate);
  }

    displayAnimate();
}
  
</script>
 <script src="http://img.lanou3g.com/Scripts/page/index.js"></script>