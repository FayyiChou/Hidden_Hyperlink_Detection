<script>
$(document).ready(function(e) {
	$(".tab li").hover(function(){
		//改样式
		$(this).addClass("hive").siblings().removeClass("hive");
		//切换出
		var tt=$(this).attr("tt");
		$("#qh").children("."+tt).show().siblings().hide();
		});
});
</script>
