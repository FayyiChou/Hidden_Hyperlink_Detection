	var t = 10;
	function load_down(){
		document.getElementById("seconds").innerHTML=t;
		t = t - 1;
		if(t<=0){
			$('#loadtime').hide();
			$('#combtn').css('display','inline-block');
		}else{
			setTimeout("load_down()",1000);
		}
	}
	$(function(){
		$('.v_2-box,.v-box').mouseenter(function(){
			$(this).siblings().removeClass('on');
			$(this).addClass('on');
		});

		load_down();
	})
