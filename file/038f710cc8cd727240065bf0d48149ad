	function delmsg(tag){
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			$('.navbar-right li:eq(1)').addClass('open');
			return false;
		}
		layer.confirm('确定要删除吗?', function(index){
			var c1=layer.load();
			$(tag).addClass('disabled');
			var id=$(tag).attr('mid');
			$.post('ajax.php',{action:'delmsg',id:id},function(data){
				layer.close(c1);
				var res=eval('('+data+')');
				if(res.success){
					layer.msg('删除成功！');
					window.location.reload();
				}
			});
			layer.close(index);
		});
	}
