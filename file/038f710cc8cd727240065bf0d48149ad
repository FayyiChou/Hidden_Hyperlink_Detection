			},
			error:function(){
			}
		});
	}

	function com_down(file_id,verycode,event){

		var c1=layer.load();
		$.ajax({
			type : 'post',
			url : 'ajax.php',
			data : 'action=load_down_addr_com&file_id='+file_id+'&verycode='+verycode,
			dataType : 'text',
			success:function(msg){
				layer.close(c1);
				var arr = msg.split('|');
				if(arr[0] == 'true'){
					down_process2(file_id,event);
					window.location.href=arr[1];
					layer.closeAll();
					layer.msg('跳转下载链接中，请稍等...');
				}else{
					layer.msg(msg,function(){});
				}
			},
			error:function(){
			}
		});
	}
	function ckver(tag,file_id,event) {
		var vecd=$(tag).prev().val();//验证码
		var ftag=$(tag).prev().attr('ftag');//来自哪里

		if(ftag=='vip'){
			vip_down(file_id,vecd,event);
			return false;
		}else if(ftag=='svip'){
			svip_down(file_id,vecd,event);
			return false;
		}else if(ftag=='com'){
			com_down(file_id,vecd,event);
			return false;
		}
	}
	function vip_downvip_down(tab,file_id) {
		var het='230px';
		if('0'=='0'){
			het='480px';
		}
		var tabg=$('#very_btn');
		tabg.val('');
		tabg.prev().attr('src','imagecode.php?t='+Date.parse(new Date()));
		tabg.attr('ftag',tab);
		$('#vecysmt').attr('fid',file_id);

		layer.open({
			type: 1,
			shadeClose:true,
			title:'下载验证',
			area: ['400px', het],
			content:$('#vecytable')
		});
	}
	function save_as(file_id){

		if(!ckust()){
			return false;
		}
		$.ajax({
			type : 'get',
			url : 'ajax.php',
			data : 'action=save_as&file_id='+file_id+'&t='+Math.random(),
			dataType : 'text',
			success:function(msg){
				if(msg == 'true'){
					ctsuccess("另存文件操作成功。");
				}else if(msg =='ufile'){
					cterror("此文件已在您的网盘中，不需另存。");
				}else{
					cterror("另存文件操作失败，请重试。");
				}
			},
			error:function(){
			}

		});
	}
	function favorite_as(user_id){
		$.ajax({
