var speed2=30
getObject('demo_wb2').innerHTML=getObject('demo_wb1').innerHTML
function Marquee2(){
if(getObject('demo_wb2').offsetTop-getObject('demo_wb').scrollTop<=0)
getObject('demo_wb').scrollTop-=getObject('demo_wb1').offsetHeight
else{
getObject('demo_wb').scrollTop++
}
}
