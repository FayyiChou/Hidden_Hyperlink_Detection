                        <script type="text/javascript">
function doChangeHtalk1(idx) {
	for(i=1 ; i<=2 ; i++) {
		getObject('mtab10' + i).style.display = "block" ;
		getObject('mtab10' + i + "v").style.display = "none" ;
		getObject('table_contents' + i).style.display="none" ;
	}
	getObject('mtab10' + idx).style.display = "none" ;
	getObject('mtab10' + idx + "v").style.display = "block" ;
	getObject('table_contents' + idx).style.display="block" ;
}


                        </script>

