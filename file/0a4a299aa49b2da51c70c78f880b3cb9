				 $(".layui-layer-content").css("font-size","16px");
				 $(".layui-layer-content a").each(function(index,_a){
					$(_a).attr("target","_blank");
					$(_a).css("color","#0080ff");
					$(_a).mouseover(function(){
						 $(_a).css("text-decoration","underline");
					}); 
			     });
			 }
	     }
