            <script>

                function searchclick() {
                    var _searchkey = document.getElementById("searchkey");
                    var keys = _searchkey.value;
                    if (keys == "" || keys == "搜索") {
                        alert("请输入搜索内容！");
                        _searchkey.focus();
                        return false;
                    }
                    window.location = 'news/search-1-' + keys + '.html';
                    return true;
                }
            </script>

