            <script type="text/javascript">
                $(document).ready(function () {
                    var iconImgList = ['images/kv/point.png', 'images/kv/point.png', 'images/kv/point.png', 'images/kv/point.png', 'images/kv/point.png'];
                    var iconImg_overList = ['images/kv/overpoint.png', 'images/kv/overpoint.png', 'images/kv/overpoint.png', 'images/kv/overpoint.png', 'images/kv/overpoint.png'];
                    $('#slideArea_kv .slideContinar').cycle({  //turnUp   //fade  //uncover
                        fx: 'fade',  //blindX     * blindY    * blindZ    * cover    * curtainX    * curtainY    * fade    * fadeZoom    * growX    * growY    * none   * scrollUp    * scrollDown    * scrollLeft    * scrollRight    * scrollHorz    * scrollVert    * shuffle    * slideX    * lideY   * toss    * turnUp    * turnDown    * turnLeft    * turnRight    * uncover    * wipe    * zoom*/
                        timeout: 5000,
                        pager: '#slideTitleNavContainer_kv ',
                        pagerAnchorBuilder: pagerFactory,
                        before: function (currSlideElement, nextSlideElement, options, forwardFlag) {

                            var nextIndex = $(nextSlideElement).attr("index");
                            var slidnavtitleArray = $("#slideArea_kv .slidnavtitle");

                            var newTop = 0;
                            var newLeft = 0;

                            if (slidnavtitleArray.length == 0) {
                                //newLeft=$(".slidnavtitle").eq(0).position().left;
                            } else {
                                var nextSlidnavtitle = $($("#slideArea_kv .slidnavtitle")[nextIndex]);
                                newTop = nextSlidnavtitle.position().top;
                                newLeft = nextSlidnavtitle.position().left;

                            }

                            //alert(newLeft)

                            $("#slideArea_kv .slideTitleFloat").css({
                                "display": "block",
                                "background": "url('" + iconImg_overList[nextIndex] + "') no-repeat",
                                top: newTop, left: newLeft
                            });

                        }
                    });


                    function pagerFactory(idx, slide) {
                        var s = idx > 5 ? ' style="display:none"' : '';
                        return '<div ' + s + ' class="slidnavtitle"  style="background:url(' + iconImgList[idx] + ') no-repeat;">&nbsp;</div>';
                    };



                    $("#btnPrev").hover(function () {
                        $(this).css("background-image", "url('images/kv/button_left_over.png')");
                    }, function () {
                        $(this).css("background-image", "url('images/kv/button_left.png')");
                    })


                    $("#btnNext").hover(function () {
                        $(this).css("background-image", "url('images/kv/button_right_over.png')");
                    }, function () {
                        $(this).css("background-image", "url('images/kv/button_right.png')");
                    })

                });
            </script>

