    <script type="text/javascript">
        function InitSearch() {
            var F_Search = document.all["txtSearch"].value;
            if (F_Search == "请输入搜索内容")
                document.all["txtSearch"].value = "";
        }
        function DataSearch() {
            var F_Search = document.all["txtSearch"].value;
            if (F_Search == null || F_Search == "" || F_Search == "请输入搜索内容") {
                alert("请输入需要搜索的关键字！");
                document.all["txtSearch"].focus();
                return;
            }
            document.all["btnSearch"].click();
        }
        function LinkOpen(F_linkUrl, F_WinTitle, F_Win_Width, F_Win_Height) {
            var WinLeft = (window.screen.availWidth - F_Win_Width) / 2;
            var WinTop = (window.screen.availHeight - F_Win_Height) / 2;
            window.open(F_linkUrl, F_WinTitle, "top=" + WinTop + ",left=" + WinLeft + ",width=" + F_Win_Width + ",height=" + F_Win_Height + ",toolbar=no,resizable=yes,scrollbars=yes,status=no,menu=yes");
        }
        function DataInfo_Look(F_LinkUrl, F_MEMBER_ID) {
            LinkOpen(F_LinkUrl, "专家信息" + F_MEMBER_ID, 800, 600);
        }
        function ChangeTab(F_TabNo) {
            var Tab1 = document.getElementById("Tab_1");
            var Tab2 = document.getElementById("Tab_2");
            var Tab3 = document.getElementById("Tab_3");
            if (F_TabNo == 1) {
                Tab1.setAttribute("class", "b2");
                Tab2.setAttribute("class", "wf");
                Tab3.setAttribute("class", "wf");
                List_19_1.style.display = "block";
                List_19_2.style.display = "block";
                List_19_3.style.display = "none";
                List_19_4.style.display = "none";
                List_19_5.style.display = "none";
                List_19_6.style.display = "none";
            }
            if (F_TabNo == 2) {
                Tab1.setAttribute("class", "wf");
                Tab2.setAttribute("class", "b2");
                Tab3.setAttribute("class", "wf");
                List_19_1.style.display = "none";
                List_19_2.style.display = "none";
                List_19_3.style.display = "block";
                List_19_4.style.display = "block";
                List_19_5.style.display = "none";
                List_19_6.style.display = "none";
            }
            if (F_TabNo == 3) {
                Tab1.setAttribute("class", "wf");
                Tab2.setAttribute("class", "wf");
                Tab3.setAttribute("class", "b2");
                List_19_1.style.display = "none";
                List_19_2.style.display = "none";
                List_19_3.style.display = "none";
                List_19_4.style.display = "none";
                List_19_5.style.display = "block";
                List_19_6.style.display = "block";
            }

        }
    </script>
