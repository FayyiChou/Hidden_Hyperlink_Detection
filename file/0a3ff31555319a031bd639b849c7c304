<script>

function getUrlParam(name)
{
var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
var r = window.location.search.substr(1).match(reg);  //匹配目标参数
if (r!=null) return unescape(r[2]); return null; //返回参数值
} 


function gettReferrerParam(name,referrer)
{
var reg = new RegExp("(\\?|&)"+ name +"=([^&]*)(&|$)"); //构造一个含有目标参数的正则表达式对象
var r = referrer.match(reg);  //匹配目标参数
if (r!=null) return decodeURI(r[2]); return null; //返回参数值
} 



function SetCookie(name,value)
{
    var Days = 30; 
    var exp  = new Date();    //new Date("December 31, 9998");
    exp.setTime(exp.getTime() + Days*24*60*60*1000);
    document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();
}
function getCookie(name)
{
    var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
     if(arr != null) return unescape(arr[2]); return null;

}
function delCookie(name)
{
    var exp = new Date();
    exp.setTime(exp.getTime() - 1);
    var cval=getCookie(name);
    if(cval!=null) document.cookie= name + "="+cval+";expires="+exp.toGMTString();
}

function getReferrer() {
    var referrer = '';
    
    try {
        referrer = window.top.document.referrer;
    } catch(e) {
        if(window.parent) {
            try {
                referrer = window.parent.document.referrer;
            } catch(e2) {
                referrer = '';
            }
        }
    }
    if(referrer === '') {
        referrer = document.referrer;
    }
    return referrer;
};

//跟踪百度推广
var referrer=getReferrer();
if(isPC()){
	agent='PC'
}
else{
	agent='无线'
}
try
{
   if(referrer.indexOf('baidu.php')>0 && getCookie('laiyuan')==null){
	   keyword=gettReferrerParam('word',referrer);
	   if(keyword===null){
		   keyword=gettReferrerParam('wd',referrer);
	   }
	   kid=getUrlParam('kid');
	   if(kid===null){
		   kid=''
	   }
	   SetCookie('kid',kid);
	   SetCookie('keyword',keyword);
	   SetCookie('laiyuan',agent+'百度推广');
   }
   else if(referrer.indexOf('baidu.com')>0  && getCookie('laiyuan')==null){
	   SetCookie('laiyuan',agent+'百度搜索');
	   SetCookie('keyword','null');
	   SetCookie('kid','null');
   }
   else if(referrer.indexOf('so.com')>0 && getCookie('laiyuan')==null){
	   keyword=gettReferrerParam('q',referrer);
	   kid=getUrlParam('kid');
	   if(kid===null){
		   kid=''
	   }
	   SetCookie('kid',kid);
	   SetCookie('keyword',keyword);
	   SetCookie('laiyuan',agent+'360推广');
   }
   else if(  getCookie('laiyuan')==null){
	   SetCookie('laiyuan',agent+'其他');
	   SetCookie('keyword','null');
	   SetCookie('kid','null');
	   
   }
}
catch(err)
{
  
}

var ucode=getUrlParam('u');

if(getCookie('ucode')==null && ucode!=null){
	SetCookie('ucode',ucode);
}


var ec = new evercookie({
	/* Options */
	    	baseurl: '/Public/static/evercookie', 
    	asseturi: '/assets', 
    	phpuri: '/php',
    	history: false
});
//ec.set('uid', val);

ec.get("ucode",function(best){
		if(typeof(best)=='undefined'){ //没有cookie记录
			if(ucode!=null){  //用户通过邀请链接进来
				delCookie('ucode');
				ec.set('ucode', ucode);
			}
					}
		else{
			//填写邀请码
			//alert(best)
			if(best.indexOf("-")==-1 && ucode!=null){ //上次仅仅记录了此浏览器登录过的用户名
				ec.set('ucode', ucode+"-"+best);
				$("#ucode").val(ucode+"-"+best);
			}
			else{
				$("#ucode").val(best);
			}
			
		}

},0);


 //暂时关闭eveecookie

</script>
</div>


