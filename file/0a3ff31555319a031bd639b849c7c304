<script>

function isPC() {
    for (var userAgent = navigator.userAgent,
    		userAgents = new Array("Android", "iPhone", "SymbianOS", "Windows Phone", "iPad", "iPod"), n = !0, i = 0; i < userAgents.length; i++) if (userAgent.indexOf(userAgents[i]) > 0) {
        n = !1;
        break
    }
    return n
}

$(function(){
	
	if(!isPC()){
		$('.downloadbtn').click(function(){
			alert("亲，请使用电脑下载我们的客户端！我们的客户端暂时只支持在电脑上运行！");
			return false;
		});
	}
	
	$('#close_im').bind('click',function(){
		$('#main-im').css("height","0");
		$('#im_main').hide();
		$('#open_im').show();
	});
	$('#open_im').bind('click',function(e){
		$('#main-im').css("height","272");
		$('#im_main').show();
		$(this).hide();
	});
	$('.go-top').bind('click',function(){
		$(window).scrollTop(0);
	});
	$(".weixing-container").bind('mouseenter',function(){
		$('.weixing-show').show();
	})
	$(".weixing-container").bind('mouseleave',function(){        
		$('.weixing-show').hide();
	});
});
$('#im_main').show();
//$('#open_im').show();

</script>



