<script>
(function(){
	/*var menu=$('#ctrlMenu li');
	var menuURL = $.cookie('menuURL');
	if(menuURL){
		var li = menu.find('a[href="'+menuURL+'"]').parent();
	}
	menu.click(function(){
		var url = $(this).find('a').attr('href');
		$.cookie('menuURL',url);
	})*/
	var currentURL = location.href;
	
	var topMenu=$('#ctrlTopMenu li');
	topMenu.each(function(){
		var url = $(this).find('a').attr('href');
		var start=currentURL.indexOf(url);
		if(start>-1){
			topMenu.removeClass('navhor');
			$(this).addClass('navhor');
		}
	});
	
	var menu=$('#ctrlMenu li');
	menu.each(function(){
		var url = $(this).find('a').attr('href');
		var start=currentURL.indexOf(url);
		if(start>-1){
			$(this).addClass('current');
		}
	});
})();
</script>


