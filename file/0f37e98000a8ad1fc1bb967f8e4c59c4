<script>
$(function(){	
	/*粮食业务tabs*/
	var lsywTab = $('.tabLsywMenu span');
	var lsywContent = $('.tabLsyw .tabContent');
	lsywTab.click(function(){
		var href = $(this).attr('data-href');
		location.href=href;
	});
	lsywTab.mouseover(function(){
		lsywTab.removeClass('title04H');
		$(this).addClass('title04H');
		var index = $(this).index();
		lsywContent.removeClass('tabActive');
		lsywContent.eq(index).addClass('tabActive');
	});
	
	/*在线办事tabs*/
	var ZxbsTab = $('.tabZxbsMenu span');
	var ZxbsContent = $('.tabZxbs .tabContent');
	ZxbsTab.click(function(){
		var href = $(this).attr('data-href');
		location.href=href;
	});
	ZxbsTab.mouseover(function(){
		ZxbsTab.removeClass('title03H');
		$(this).addClass('title03H');
		var index = $(this).index();
		ZxbsContent.removeClass('tabActive');
		ZxbsContent.eq(index).addClass('tabActive');
	});
})
</script>
