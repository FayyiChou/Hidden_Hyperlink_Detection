//郭磊添加的活动计时js,活动结束后可取消
<!--
getActiveTime();
var $hdId;
var $hdtime=0;
var $hdhour=0;
var $hdmin=0;
var $hdsec=0;
var $hdstatus="END";
//改变购物车数量的方法
function getActiveTime(){
		xmlHttp=GetXmlHttpObject();
		if(xmlHttp==null){
			alert('浏览器不支持验证！！！');
		}else{
			var forwardUrl="/core_rayi/getservertime.php";
			forwardUrl=forwardUrl+"?tid="+Math.random();
			xmlHttp.open("GET",forwardUrl,true);
			xmlHttp.onreadystatechange=getActiveTimeBack;
			xmlHttp.send(null);
		}

}

function getActiveTimeBack(){ 
  if (xmlHttp.readyState==4){ 
  	if(xmlHttp.status == 200){
		var backStr=xmlHttp.responseText;
		var hdMain=document.getElementById('hdMain');
		if(backStr.substring(0,2)=='OK'){
			hdMain.style.display='block';
			clearInterval($hdId);
			var backArray=backStr.split("[|]");
			$hdtime=parseInt(backArray[1]);
			$hdstatus="OK";
			$hdId=setInterval(runhd,1000)
			
		}
		if(backStr.substring(0,4)=='WAIT'){
			hdMain.style.display='block';
			clearInterval($hdId);
			var backArray=backStr.split("[|]");
			$hdtime=parseInt(backArray[1]);
			$hdstatus="WAIT";
			$hdId=setInterval(runhd,1000)
		}
		if(backStr.substring(0,3)=='END'){
			
			hdMain.style.display='none';
			$hdtime=0;
			$hdhour=0;
			$hdmin=0;
			$hdsec=0;
			$hdstatus="END";
			clearInterval($hdId);
		}
		
	}
  }
}

function runhd(){
//alert($hdtime);
	
	var hdhour=document.getElementById('hdhour');
	var hdmin=document.getElementById('hdmin');
	var hdsec=document.getElementById('hdsec');
	var hdbgImg=document.getElementById('hdbgImg');
	var hdMain=document.getElementById('hdMain');
	if($hdstatus!='END'){
		if($hdtime<=0){
			getActiveTime();
		}
		if($hdstatus=='OK'){
			hdbgImg.style.background="url("http://www.rayi.cn/images/pop_ad/jiushihui2_02.gif")";
		}
		$hdhour=parseInt($hdtime/3600);
		$hdmin=parseInt(($hdtime-($hdhour*3600))/60);
		$hdsec=parseInt($hdtime-($hdhour*3600)-($hdmin*60));
		if($hdhour<10)
			hdhour.innerHTML="0"+$hdhour;
		else
			hdhour.innerHTML=$hdhour+"";
		if($hdmin<10)
			hdmin.innerHTML="0"+$hdmin;
		else
			hdmin.innerHTML=$hdmin+"";
		if($hdsec<10)
			hdsec.innerHTML="0"+$hdsec;
		else
			hdsec.innerHTML=$hdsec+"";
		
	}
	if($hdstatus=='END'){
		hdhour.innerHTML="00";
		hdmin.innerHTML="00";
		dsec.innerHTML="00";
		clearInterval($hdId);
		hdMain.style.display='none';
	}
	$hdtime=$hdtime-1;
	//alert($hdtime+"");

}

//获取xmlHttpRequest对象的方法
function GetXmlHttpObject(){
  var xmlHttp=null;
  try{
    // Firefox, Opera 8.0+, Safari
    xmlHttp=new XMLHttpRequest();
  }
  catch (e){
    // Internet Explorer
