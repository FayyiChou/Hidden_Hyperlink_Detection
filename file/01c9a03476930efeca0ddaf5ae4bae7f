 getdigest();
function getdigest(){
	     
		 xmlHttp=DigestGetXmlHttpObject();
		 if(xmlHttp==null){
		 	alert('浏览器不支持验证！！！');
		 }else{
			var checkurl="/core_rayi/getdigest.php";
			checkurl=checkurl+"?tmp="+Math.random();
			//alert(checkurl);
			
			xmlHttp.open("GET",checkurl,true);
			xmlHttp.onreadystatechange=getdigest_callback;
			xmlHttp.send(null);
		}
	
}
function DigestGetXmlHttpObject(){
  var xmlHttp=null;
  try{
    // Firefox, Opera 8.0+, Safari
    xmlHttp=new XMLHttpRequest();
  }
  catch (e){
    // Internet Explorer
   	try{
      xmlHttp=new ActiveXObject("Msxml2.XMLHTTP");
    }
    catch (e){
      xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");
    }
  }
  return xmlHttp;
}

function getdigest_callback(){ 
  if (xmlHttp.readyState==4){ 
  	if(xmlHttp.status == 200){
      var rbn=xmlHttp.responseText;
	  var bbshot=document.getElementById("bbshot");
	  //alert(bbshot.innerHTML);
	  bbshot.innerHTML=rbn;
      //alert(rbn);
    }
  	
  }
}
