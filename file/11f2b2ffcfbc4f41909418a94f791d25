<SCRIPT language=javascript type=text/javascript>
		function checkForm()
		{
			var objSearchText=document.getElementById("ctl03_ctl00_txtSearchText");
			if(objSearchText.value=="")			
			{
				alert("请输入您要查询的关键字！");
				window.event.returnValue = false;
				objSearchText.focus();
				return false;
			}
			var objSearchOption=document.getElementById("ctl03_ctl00_ddlSearchOption");
			if(objSearchOption.selectedIndex==0)			
			{
				alert("请选择搜索选项！");
				window.event.returnValue = false;
				objSearchOption.focus();
				return false;
			}

			var url=String(objSearchOption.options[objSearchOption.selectedIndex].value);
			
			if(url.indexOf("?")!=-1)
			{
				url+="&q="+objSearchText.value;
			}
			else
			{
				url+="?q="+objSearchText.value;
			}
			
			//alert(url);
			location.href=url;
		}
	</SCRIPT>
