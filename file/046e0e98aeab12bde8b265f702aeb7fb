<script type="text/javascript">
function autoTop(){
var bodyTop = document.documentElement.scrollTop;
document.getElementById("hd_ad").style.top = (bodyTop + 50) + "px";
document.getElementById("erwei").style.top = bodyTop + "px";
document.getElementById("erweima").style.top = (bodyTop + 250) + "px";
document.getElementById("erweima2").style.top = (bodyTop + 340) + "px";
}
setInterval("autoTop()","500");
</script>
