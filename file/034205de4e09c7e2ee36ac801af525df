        <div class="calss-list" style="display:none;">
            <script type="text/javascript">
                var allfldic = { "1": "icon-ciqi", "5": "icon-yuqi", "6": "icon-zhubao", "3": "icon-qianbi", "2": "icon-muqi", "4": "icon-tongqi", "68": "icon-zhongbiao", "7": "icon-zihua", "78": "icon-wenxianshuji", "8": "icon-zaxiang"};
                var dhhtml = "";
                var onefllist = new Array();
                var onefltitle = new Array();
                var onum = 0;
                for (var i = 0; i < subType.length; i++) {
                    if (subType[i][0] == "0" && subType[i][2] != "119" && subType[i][2] != "120")
                    { onefllist[onum] = subType[i][2]; onefltitle[onum] = subType[i][1]; onum++; }
                }
                for (var i = 0; i < onefllist.length; i++) {
                    dhhtml = dhhtml + "<div class='item'><div class='cathook'>";
                    dhhtml = dhhtml + "<i class='iconfont ";
                    dhhtml = dhhtml + allfldic[onefllist[i]];
                    dhhtml = dhhtml + " icon-pict'></i><a href='http://www.cang.com/new/list" + onefllist[i] + ",_s_0_0_0_0_0_2_0_4_t_1.html' target='_blank'><span>" + onefltitle[i] + "</span><i class='iconfont icon-youjiantou cat-ico'></i></a></div>";
                    dhhtml = dhhtml + "<div class='item-list'><div class='list-bd'><dl><dt>热门分类</dt><dd>";
                    for (var k = 0; k < subType.length; k++) {
                        if (onefllist[i] == subType[k][0]) {
                            dhhtml = dhhtml + "<a href='http://www.cang.com/new/list" + subType[k][3] + "_s_0_0_0_0_0_2_0_4_t_1.html' target='_blank'>" + subType[k][1] + "</a> ";
                        }
                    }
                    dhhtml = dhhtml + "</dd></dl>";
                    for (var n = 0; n < subAttibuteB.length; n++) {
                        if (subAttibuteB[n][1] == onefltitle[i]) {
                            for (var l = 0; l < subAttibute.length; l++) {
                                if (subAttibute[l][0] == subAttibuteB[n][2]) {
                                    if (subAttibute[l][1] == "品牌") continue;
                                    dhhtml = dhhtml + "<dl><dt>" + subAttibute[l][1] + " </dt><dd>";
                                    for (var m = 0; m < subAttibute.length; m++) {
                                        if (subAttibute[l][2] == subAttibute[m][0]) {
                                            dhhtml = dhhtml + "<a href='http://www.cang.com/new/list" + onefllist[i] + ",_" + subAttibute[m][2] + "s_0_0_0_0_0_2_0_4_t_1.html' target='_blank'>" + subAttibute[m][1] + "</a> ";
                                        }
                                    }
                                    dhhtml = dhhtml + "</dd></dl>";
                                }
                            }
                        }
                    }
                    dhhtml = dhhtml + "</div></div></div>";
                }
                document.write(dhhtml);
                //二级分类切换块
                if ($(".all-class .calss-list").is(':hidden')) {
                    $(".all-class").hover(function () {
                        $('.all-class .calss-list').css('display', 'block');
                        $(".all-class .a-title .ticon").css('display', 'none');
                    }, function () {
                        $('.all-class .calss-list').css('display', 'none');
                        $(".all-class .a-title .ticon").css('display', '');
                    })
                }
                $('.all-sort-list .item').hover(function () {
                    var eq = $('.all-sort-list .item').index(this),
                        h = $('.all-sort-list').offset().top,
                        s = $(window).scrollTop(),
                        i = $(this).offset().top,
                        ri = $(this).position().top,
                        t = $(this).height(),
                        item = $(this).children('.item-list').height(),
                        sort = $('.all-sort-list').height();
                    if (item < (ri + $(this).height())) {
                        $(this).children('.item-list').css({ "top": (ri - item + t + 4) + "px" });
                    }
                    $(this).addClass('hover');
                    $(this).children('.item-list').css('display', 'block');
                }, function () {
                    $(this).removeClass('hover');
                    $(this).children('.item-list').css('display', 'none');
                });
                </script><div class="item"><div class="cathook"><i class="iconfont icon-ciqi icon-pict"></i><a href="http://www.cang.com/new/list1,_s_0_0_0_0_0_2_0_4_t_1.html" target="_blank"><span>瓷器</span><i class="iconfont icon-youjiantou cat-ico"></i></a></div><div class="item-list"><div class="list-bd"><dl><dt>热门分类</dt><dd><a href="http://www.cang.com/new/list1,9,_s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">高古</a> <a href="http://www.cang.com/new/list1,10,_s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">明清</a> <a href="http://www.cang.com/new/list1,11,_s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">民国</a> <a href="http://www.cang.com/new/list1,12,_s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">其它</a> </dd></dl><dl><dt>年代 </dt><dd><a href="http://www.cang.com/new/list1,_163s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">夏商周</a> <a href="http://www.cang.com/new/list1,_164s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">春秋战国</a> <a href="http://www.cang.com/new/list1,_165s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">汉</a> <a href="http://www.cang.com/new/list1,_166s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">三国</a> <a href="http://www.cang.com/new/list1,_167s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">晋代</a> <a href="http://www.cang.com/new/list1,_168s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">南北朝</a> <a href="http://www.cang.com/new/list1,_169s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">隋唐</a> <a href="http://www.cang.com/new/list1,_170s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">宋代</a> <a href="http://www.cang.com/new/list1,_171s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">辽金</a> <a href="http://www.cang.com/new/list1,_172s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">元代</a> <a href="http://www.cang.com/new/list1,_173s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">明早期</a> <a href="http://www.cang.com/new/list1,_174s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">明空白期</a> <a href="http://www.cang.com/new/list1,_175s_0_0_0_0_0_2_0_4_t_1.html" target="_blank">明中期</a> <a href="http://www.cang.com/new/lis