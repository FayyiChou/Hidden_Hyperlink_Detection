<script type="text/javascript">
    function DrawImg(boxWidth, boxHeight) {
        var imgWidth = $("#imgLogo").innerWidth();
        var imgHeight = $("#imgLogo").innerHeight();
        if (imgWidth > 261 || imgHeight > 174) {
            //比较imgBox的长宽比与img的长宽比大小
            if ((boxWidth / boxHeight) >= (imgWidth / imgHeight)) {
                //重新设置img的width和height
                $("#imgLogo").width((boxHeight * imgWidth) / imgHeight);
                $("#imgLogo").height(boxHeight);
            }
            else {
                //重新设置img的width和height
                $("#imgLogo").width(boxWidth);
                $("#imgLogo").height((boxWidth * imgHeight) / imgWidth);
            }
        }
    }
    DrawImg(226, 196);
</script>


