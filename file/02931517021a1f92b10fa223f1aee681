    var wpId;
    var encryptSellerId = 'hi1BE8lcJLQh/AO63V283wX+AqAqeTO3';
    var loginId = '';
    var encryptCurrentLoginId 
    if(loginId == null || loginId ==""){
        encryptCurrentLoginId = "";
    } else {
        encryptCurrentLoginId = 'SAcRo2A5c64=';
    }
    //var e2 = 't5upIH14jNVGlfCMiDi1OhfBpz27z7rf';
    $(function(){
        $.ajax({
            url:"http://wp.5173.com/getWpInfoBySellerId",
            //data:{sellerId:'US14092653099191-03EE'},  US12091063810001-0104
            data:{'userId':encryptCurrentLoginId, 'sellerId' : encryptSellerId},
            cache: false,
            type:'GET',
            dataType: 'jsonp',
            jsonp: "callback",
            jsonpCallback: 'getWpInfoBySellerId',
            success:function(res){
                if(res.IsSucceed == "1"){
                    $("#dlWP").show();
                    $("#btnWP").show();
                    var result = res.Data;
                    wpId = result.WpId;
                    if(result.Authentication=="2")
                    {
                        $("#rowAuth").show();
                    } else {
                        $("#rowAuth").hide();
                    } 
                    if(result.IsCollected =="1"){
                        $("#aBookMark").val('已收藏');
                        $("#aBookMark").attr('disabled', 'disabled');
                        $("#aBookMark").css("color",'gray');
                        $("#aBookMark").css('cursor','default');
                    }

                    var url = "http://wp.5173.com/store/" + wpId;
                    $("#ltWpName").attr('href', url);
                    $("#ltWpName").html(result.Name);
                
                    $("#aEnter").click(function(){
                        window.location = url;
                    })
                    //收藏店铺
                    $("#aBookMark").click(function(){
                        var loginId ='';
                        var currentUrl = escape(window.location.href);
                        if (loginId == null || loginId =="") {  
                            window.location.href = "https://passport.5173.com/?returnUrl="+currentUrl;
                            return;
                        }
                        $.ajax({
                            url:"http://wp.5173.com/setWpCollection",
                            data:{userId:encryptCurrentLoginId, wpId:wpId },
                            type:'POST',
                            dataType:'jsonp',
                            jsonp:'callback',
                            jsonpCallback:'setWpCollection',
                            success:function(dataBookMark){
                                if(dataBookMark.IsSucceed="1"){
                                    $("#aBookMark").val('已收藏');
                                    $("#aBookMark").attr('disabled', 'disabled');
                                    $("#aBookMark").css("color",'gray');
                                    $("#aBookMark").css('cursor','default');
                                    alert('收藏成功！');
                                    //$("#aBookMark").removeAttr('href');
                                    //$("#aBookMark").removeAttr('onclick');
                                }
                                else 
                                    alert('收藏失败！');
                            },
                            error :function(XMLHttpRequest){
                                console.log('error');
                                console.log(XMLHttpRequest.error);
                            }
                        })
                    })
                    
                } else {
                    $("#dlWP").hide();
                    $("#btnWP").hide();
                }
               
            },
            error :function(XMLHttpRequest){
                console.log('error');
                console.log(XMLHttpRequest.error);
            }
        });

        
    })
    
</script>


<script type="text/javascript">
    function AddBizOfferFavorite() {
        var url = document.location.href;
        var bizTitle = document.getElementById('hTitle').innerText;
        try {
            window.external.addFavorite(url, bizTitle);
        }
        catch (e) {
            try {
                window.sidebar.addPanel(bizTitle, url, "");
            }
            catch (e) {
            }
        }
    }

    function CheckCurrent(btnBuy) {
        btnBuy.style.visibility = "hidden";
        return true;
    }

    function DeliveryVisit() {
        var url = "/businesspartner/ui5914/purchaseorder.aspx?BizOfferId=DB145-20170330-70709941&TradingServiceType=Escort";
        window.open(url);
    }


    function AddShoppingCart() {

        var getUrl = "ShoppingCart.axd?methodname=ADDGOODS2CART&servicetype=1&offerid=DB145-20170330-70709941";

        $.ajax({
            url: getUrl,
            type: 'POST',
            error: function () { },
            success: function (data) { CallBack_ShoppingCart(data); }
        });
    }
    function CallBack_ShoppingCart(data) {
        try {
            if (data != null && data != "") {
                var result = data.split("|");
                if (result[0] == "0") {
                    Cart_Succeed_Show(result[1], result[2]);
                } else if (result[0] == "-1") {
                    Cart_Exists_Show(result[1]);
                } else if (result[0] == "-2") {
                    Cart_UpperLimit_Show(result[1], result[2]);
                }
                else if (result[0] == "2" || result[0] == "4") {
                    window.location.href = "https://passport.5173.com/?returnUrl=http%3A%2F%2Fdanbao.5173.com%2Fbrowse%2FGoodsDetails.aspx%3FTradingServiceType%3DEscort%26Id%3DDB145-20170330-70709941%26IsCart%3D1";
                    return;
                }
                else {
                    alert("很抱歉，系统繁忙，请稍后再试…");
                }
} else {
    alert("很抱歉，系统繁忙，请稍后再试…");
}
} catch (e) {
    alert("很抱歉，系统繁忙，请稍后再试…");
}
    $("lblAddGoods2CartBackReslut").text(data);
}

function Cart_Succeed_Show(category, slimitDays) {
    Cart_Urchin_Run_ShowLayer("购物车加入成功", category);
    $("#p_cart_succeed_info").html("购物车中的" + category + "商品将为您保存" + slimitDays + "天。");
    jQuery.LAYER.show({ id: 'layer_cart_succeed' });
}
function Cart_Exists_Show(category) {
    Cart_Urchin_Run_ShowLayer("购物车重复添加", category);
    jQuery.LAYER.show({ id: 'layer_cart_exists' });
}
function Cart_UpperLimit_Show(category, limitCount) {
    Cart_Urchin_Run_ShowLayer("购物车已满", category);
    $("#span_cart_upperlimit_limitcount").html(limitCount);
    jQuery.LAYER.show({ id: 'layer_cart_upperlimit' });
}
function Cart_Urchin_Run_ShowLayer(addType, category) {
    try {
        // 类目
        var runtype = "%e8%b4%ad%e7%89%a9%e8%bd%a6";
        var runcategory = encodeURIComponent(addType + "-" + category).toLocaleLowerCase();
        var runname = encodeURIComponent("未登录").toLocaleLowerCase();
        __utmTrackEvent(runtype, runcategory, runname);
    } catch (e) { }
}
function Cart_Urchin_Run_CloseLayer(typeid) {
    try {
        switch (typeid) {
            case 1: // the goods add succeed layer click '我知道了'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%8a%a0%e5%85%a5%e6%88%90%e5%8a%9f', '%e5%86%8d%e9%80%9b%e9%80%9b');
                return;
            case 2: // the goods add succeed layer click '查看购物车'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%8a%a0%e5%85%a5%e6%88%90%e5%8a%9f', '%e6%9f%a5%e7%9c%8b%e8%b4%ad%e7%89%a9%e8%bd%a6');
                return;
            case 3: // the goods add succeed layer click colose button
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%8a%a0%e5%85%a5%e6%88%90%e5%8a%9f', '%c3%97');
                return;
            case 4: // the cart is full click '我知道了'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%b7%b2%e6%bb%a1', '%e5%86%8d%e9%80%9b%e9%80%9b');
                return;
            case 5: // the cart is full click '查看购物车'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%b7%b2%e6%bb%a1', '%e6%9f%a5%e7%9c%8b%e8%b4%ad%e7%89%a9%e8%bd%a6');
                return;
            case 6: // the cart is full click colose button
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e5%b7%b2%e6%bb%a1', '%c3%97');
                return;
            case 7: // the goods already exist click '我知道了'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e9%87%8d%e5%a4%8d%e6%b7%bb%e5%8a%a0', '%e5%86%8d%e9%80%9b%e9%80%9b');
                return;
            case 8: // the goods already exist click '查看购物车'
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e9%87%8d%e5%a4%8d%e6%b7%bb%e5%8a%a0', '%e6%9f%a5%e7%9c%8b%e8%b4%ad%e7%89%a9%e8%bd%a6');
                return;
            case 9: // the goods already exist click colose button
                __utmTrackEvent('%e8%b4%ad%e7%89%a9%e8%bd%a6', '%e8%b4%ad%e7%89%a9%e8%bd%a6%e9%87%8d%e5%a4%8d%e6%b7%bb%e5%8a%a0', '%c3%97');
                return;
        }
    } catch (e) { }
}
</script>

