<script type="text/javascript">
    $("#liAppraise").hide();
    if ("1" == "1") {
        $("#liShangpin_btn_equipment").hide();
        $("#liZhinan_btn_equipment").addClass("dd_on");
    }

    var fhcjzn = {
        'btn': ['liShangpin_btn_equipment', 'liZhinan_btn_equipment'],
        'liShangpin_btn_equipment': ['GoodsDescription1_spshow', 'GoodsDescription1_phBuyModeInfo_Equipment', 'hBuyManual', 'divZhinan_cnt'],
        'liZhinan_btn_equipment': ['divZhinan_cnt']
    };
    jQuery(function () {
        jQuery.each(fhcjzn.btn, function (_k, _v) {
            jQuery('#' + _v).bind('click', function () {
                jQuery.each(fhcjzn.btn, function (_k, _v) {
                    jQuery('#' + _v).attr('class', '');
                });
                jQuery.each(fhcjzn.liShangpin_btn_equipment, function (_k, _v) {
                    jQuery('#' + _v).hide();
                });
                jQuery(this).attr('class', 'dd_on');
                jQuery.each(fhcjzn[jQuery(this).attr('id')], function (_k, _v) {
                    jQuery('#' + _v).show();
                });
            });
        });
        $("#lidd_l_btn_equipment").click(function () { $("#liShangpin_btn_equipment").attr('class', 'dd_on'); $("#GoodsDescription1_spshow").show(); $("#GoodsDescription1_hBuyManual").show(); });
        $("#lidd_r_btn_equipment").click(function () { $("#liZhinan_btn_equipment").attr('class', 'dd_on'); $("#GoodsDescription1_spshow").hide(); $("#GoodsDescription1_hBuyManual").hide(); });
        $("#liAppraise").click(function () { $("#divBizInfo").addClass('UED_hide'); $("#divAppraise").removeClass('UED_hide').addClass("buy-pj"); });
        $("#liShangpin_btn_equipment").click(function () { $("#divBizInfo").removeClass('UED_hide'); $("#divAppraise").addClass('UED_hide'); });
        $("#liZhinan_btn_equipment").click(function () { $("#divBizInfo").removeClass('UED_hide'); $("#divAppraise").addClass('UED_hide'); });
    });
</script>
<script type="text/javascript">
    function DrawEQImg(boxWidth, boxHeight, i) {
        var imgWidth = $("#imgBig_" + i).innerWidth();
        var imgHeight = $("#imgBig_" + i).innerHeight();
        if (imgWidth > 380 || imgHeight > 380) {
            //比较imgBox的长宽比与img的长宽比大小
            if ((boxWidth / boxHeight) >= (imgWidth / imgHeight)) {
                //重新设置img的width和height
                $("#imgBig_" + i).width((boxHeight * imgWidth) / imgHeight);
                $("#imgBig_" + i).height(boxHeight);
            }
            else {
                //重新设置img的width和height
                $("#imgBig_" + i).width(boxWidth);
                $("#imgBig_" + i).height((boxWidth * imgHeight) / imgWidth);
            }
        }
    }
</script>
<script type="text/javascript">
    $(".sp_pic p").hover(
    function () {
        $(this).parents(".sp_pic").addClass("pic_h");
    }, function () {
        $(this).parents(".sp_pic").removeClass("pic_h");
    });

    $("#dtImg_1").attr("style", "display:''");

    $(function () {
        $("#ddImg_5").addClass("d_r");
        DrawEQImg(380, 380, 1);
    });
    $("#ddImg_1").addClass("i_on");

    // 更改装备上传图片的选中样式
    function ChangeClass(obj) {
        var divimg = $('#dlShowGameImg');
        if (divimg.length > 0) {

            var ddlist = divimg.find('dd');
            if (ddlist.length > 0) {
                var imgid = obj;

                for (var i = 1; i <= ddlist.length; i++) {
                    if (imgid == "ddImg_" + i) {
                        $("#ddImg_" + i).addClass("i_on");
                        document.getElementById("dtImg_" + i).style.display = "";

                        DrawEQImg(380, 380, i);
                    }
                    else {
                        $("#ddImg_" + i).removeClass();
                        document.getElementById("dtImg_" + i).style.display = "none";
                    }
                }
                if ($("#ddImg_5") != null) {
                    $("#ddImg_5").addClass("d_r");
                }
            }
        }
    }
</script>

