	<script type="text/javascript">
		(function($) {
		    "use strict";
			
			$(document).ready(function() {
				simple_carousel_bullets_cfs();
			});
			
			$(window).load(function() {
				$('.simple_carousel_bullets_init_26248').trigger('destroy');
				simple_carousel_bullets_cfs();
			});
			
			$(window).resize(function() {
				$('.simple_carousel_bullets_init_26248').trigger('destroy');
				simple_carousel_bullets_cfs();
			});
			
			var items = 1;
			
			function simple_carousel_bullets_cfs(){
				$('.simple_carousel_bullets_init_26248').carouFredSel({
					scroll   : {
					    items: items,
						fx : "direct",
				        duration : 800,                         
				        pauseOnHover : true
				    },
				    debug: false,
				    auto: {
				        play:false,
				        timeoutDuration: 5000
				    },
				    swipe: { 
					    onTouch: true 
					},
					responsive: true,
				    width: "100%",
				    items: {
				        visible: {
				            min:1,
				            max:4				        },
				    },
				    pagination: {
						container: function() {
				            return $(this).closest('.simple_carousel_with_bullets').find('.simple-carousel-bullets');
				        }
					},
				});
				
				items = $('.simple_carousel_bullets_init_26248').triggerHandler("currentVisible");
			};
		})(jQuery);
	</script>
