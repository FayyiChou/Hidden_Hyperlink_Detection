 $(document).ready(function() {
 		
    $('#date_start,#date_end').TN_date();
    $('#dp1401595967068').TN_date();
  });

  if (0 + 0 + 0 + 0 + 0 + 0 == 0) $('#iarspf').addClass('yes');

  $('.content3').each(function() {

    if ($(this).height() >= 19 * 2) {

      $(this).css({
        'height': '19px',
        'overflow': 'hidden'
      }).after('<li class="moreE">展开</li>');

      if ($(this).find(".yes").offset().top > $(this).offset().top) {
        $(this).removeAttr('style').parent().find('.moreE').removeClass().addClass('moreT').html('收起');
      }
    }
  });
  $('.moreE,.moreT').click(function() {
    if ($(this).attr('class') == 'moreE') {
      $(this).removeClass().addClass('moreT').html('收起').parent().find('.content3').removeAttr('style');
    } else {
      $(this).removeClass().addClass('moreE').html('展开').parent().find('.content3').css({
        'height': '19px',
        'overflow': 'hidden'
      });
    }
  });

  function ListUrl(id) {
    var url = location.href;
    var tt = url.split('?');
    if (typeof(tt[1]) == 'undefined') {
      url = '/add/l.php?tid=393';
    } else {
      url = location.href;
    }
    var a = id.split('_');

    var tmp;

    var s = url.indexOf("?");
    var Par = s == -1 ? '&': '&' + url.substring(s + 1) + '&';

    for (var i = 0; i < a.length; i += 2) {
      tmp = Par.match(new RegExp("\&" + a[i] + "\=-?(\\d+)?", 'i'));
      if (!tmp) {
        Par += a[i] + '=' + a[i + 1] + '&'
      } else {
        Par = Par.replace(tmp[0] + '&', '&' + a[i] + '=' + a[i + 1] + '&');
      }
    }

    Par = Par.substring(1, Par.length - 1);

    if (s == -1) {
      url += '?' + Par;
    } else {
      url = url.substring(0, s + 1) + Par;
    }

    location = url;

  }

  String.prototype.Trim = function() {

    return this.replace(/^\s+/g, "").replace(/\s+$/g, "");
  }

  if (getQueryString('id') == '0' || getQueryString('id') == null) {
    $('#id_0').addClass('yes');
  } else {
    $('.content0 a').removeClass('yes');
    $('#id_' + getQueryString('id')).addClass('yes');
  }

  if (getQueryString('id') == '0' || getQueryString('id') == null) {
    $('#id_0').addClass('yes');
  } else {
    $('.content0 a').removeClass('yes');
    $('#id_' + getQueryString('id')).addClass('yes');
  }

  if ((getQueryString('p1') == '0' && getQueryString('p2') == '0') || (getQueryString('p1') == null && getQueryString('p2') == null)) {
    $('#p1_0_p2_0').addClass('yes');
  } else {
    $('.content1 a').removeClass('yes');
    $('#p1_' + getQueryString('p1') + '_p2_' + getQueryString('p2')).addClass('yes');
  }

  if (getQueryString('xj') == '0' || getQueryString('xj') == null) {
    $('#xj_0').addClass('yes');
  } else {
    $('.content2 a').removeClass('yes');
    $('#xj_' + getQueryString('xj')).addClass('yes');
  }

  if (getQueryString('b') == '0' || getQueryString('b') == null) {
    $('#b_0').addClass('yes');
  } else {
    $('.content3 a').removeClass('yes');
    $('#b_' + getQueryString('b')).addClass('yes');
  }

  if (getQueryString('ts0') == '1') {
    $('#ts0_1').attr('checked', 'checked');
  }

  if (getQueryString('ts1') == '1') {
    $('#ts1_1').attr('checked', 'checked');
  }

  if (getQueryString('ts2') == '1') {
    $('#ts2_1').attr('checked', 'checked');
  }

  if (getQueryString('ts3') == '1') {
    $('#ts3_1').attr('checked', 'checked');
  }

  if (getQueryString('ts4') == '1') {
    $('#ts4_1').attr('checked', 'checked');
  }

  if (getQueryString('ts5') == '1') {
    $('#ts5_1').attr('checked', 'checked');
  }

  if (getQueryString('ts6') == '1') {
    $('#ts6_1').attr('checked', 'checked');
  }

  if (getQueryString('o') == '0' || getQueryString('o') == null) {
    $('#o_0').addClass('yes');
  } else {
    $('.content5 a').removeClass('yes');
    $('#o_' + getQueryString('o')).addClass('yes');
  }

  function Search(t, c) {
    var b, d1, d2, u = $(t).attr('url'),
    a = c.split(',');
    u = u + $('#cid').val() + '&';
    //u = u + '&';
    if ($('#cid').val() == '0' || $('#cid').val() == '') {
      alert('入住城市必须选择');
      $("input[name='CityName']").focus();

      return false;
    }

    for (var i = 0; i < a.length; i++) {
      d1 = a[i];

      if (d1 == 'hoteljiage') {
        var p = escape($('#' + d1).val()).split('_');
        u = u + p[0] + '=' + p[1] + '&' + p[2] + '=' + p[3] + '&';
      }

      else {

        b = escape($('#' + d1).val());

        if (b != $('#' + d2).attr('default')) u += (d1 + '=' + b + '&')
      }
    }

    b = u.substring(u.length - 1);
    if (b == '&' || b == '?') {
      u = u.substring(0, u.length - 1)
    }
    location = u;
    return false
  }

  function fxid(id, aid) {
    var id = aid + '_roomid_' + id;
    if ($('#' + id).css('display') == 'none') {
      document.getElementById(id).style.display = 'block'
    } else {
      document.getElementById(id).style.display = 'none'
    }

  }

  function getQueryString(name) {
    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
    var r = window.location.search.substr(1).match(reg);
    if (r != null) return unescape(r[2]);
    return null;
  }
</script>