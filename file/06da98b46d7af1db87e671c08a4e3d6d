	<script>
function addClass(obj, cls) {
	if (!this.hasClass(obj, cls)) obj.className += " " + cls;
}

function removeClass(obj, cls) {
	if (hasClass(obj, cls)) {
		var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)');
		obj.className = obj.className.replace(reg, ' ');
	}
}
function hasClass(obj, cls) {
		return obj.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)'));
}

function get_date(){
	var myDate = new Date();
    var year=myDate.getFullYear();   //获取完整的年份(4位,1970-????)
    var month=myDate.getMonth()+1;      //获取当前月份(0-11,0代表1月)
    var day=myDate.getDate();       //获取当前日(1-31)
	if(month<10){
	  month="0"+month;
	}
	if(day<10){
	  day="0"+day;
	}
	 document.getElementById("sea_likedata_1").value=year+"-"+month+"-"+day;
	 document.getElementById("sea_likedata_2").value=year+"-"+month+"-"+day;
	 document.getElementById("sea_likedata_3").value=year+"-"+month+"-"+day;
}
window.onload=get_date; 
function show_area(i){
  switch(i){
     case 1:
	 document.getElementById("show_table_1").style.display = "block";
	 document.getElementById("show_table_2").style.display = "none";
	 document.getElementById("show_table_3").style.display = "none";
	 document.getElementById("show_table_4").style.display = "none";
	 document.getElementById("show_table_5").style.display = "none";	
     removeClass(document.getElementById("dfn_tdy"),"oncursor");
     removeClass(document.getElementById("dfn_zty"),"oncursor");
	 addClass(document.getElementById("dfn_zyx"),"oncursor");
	break;
	 case 2:
	  document.getElementById("show_table_2").style.display = "block";
	  document.getElementById("show_table_1").style.display = "none";
	  document.getElementById("show_table_3").style.display = "none";
	  document.getElementById("show_table_4").style.display = "none";
	  document.getElementById("show_table_5").style.display = "none";
	  removeClass(document.getElementById("dfn_zyx"),"oncursor");
      removeClass(document.getElementById("dfn_zty"),"oncursor");
	  addClass(document.getElementById("dfn_tdy"),"oncursor");
	break;
	 case 3:
	  document.getElementById("show_table_3").style.display = "block";
	  document.getElementById("show_table_1").style.display = "none";
	  document.getElementById("show_table_2").style.display = "none";
	  document.getElementById("show_table_4").style.display = "none";
	  document.getElementById("show_table_5").style.display = "none";
	  removeClass(document.getElementById("dfn_zyx"),"oncursor");
      removeClass(document.getElementById("dfn_tdy"),"oncursor");
	  addClass(document.getElementById("dfn_zty"),"oncursor");
	break;
	 case 4:
	  document.getElementById("show_table_4").style.display = "block";
	  document.getElementById("searchRoad").style.display = "none";
	  document.getElementById("show_table_5").style.display = "none";
	  document.getElementById("back_2").setAttribute("class", "oncursor"); 
	  document.getElementById("back_1").setAttribute("class", ""); 
	  document.getElementById("back_3").setAttribute("class", ""); 
	  
	break;
	 case 5:
	  document.getElementById("show_table_5").style.display = "block";
	  document.getElementById("searchRoad").style.display = "none";
	  document.getElementById("show_table_4").style.display = "none";
	  document.getElementById("back_3").setAttribute("class", "oncursor"); 
	  document.getElementById("back_1").setAttribute("class", ""); 
	  document.getElementById("back_2").setAttribute("class", ""); 
	break;
	case 6:
	  document.getElementById("searchRoad").style.display = "block";
	  document.getElementById("show_table_5").style.display = "none";
	  document.getElementById("show_table_4").style.display = "none";
	  document.getElementById("back_1").setAttribute("class", "oncursor"); 
	  document.getElementById("back_3").setAttribute("class", ""); 
	  document.getElementById("back_2").setAttribute("class", ""); 
	break;
  }
}
function get_mudi_di(element){
    var val=element.innerHTML;
    document.getElementsByName("mdd")["0"].options[0].innerHTML=val;
    document.getElementsByName("mdd")["0"].options[0].value=val;
	document.getElementsByName("mdd")["1"].options[0].innerHTML=val;
    document.getElementsByName("mdd")["1"].options[0].value=val;
	document.getElementsByName("mdd")["2"].options[0].innerHTML=val;
    document.getElementsByName("mdd")["2"].options[0].value=val;
	document.getElementById("zyx_mmd").style.display = "none";
	document.getElementById("tdy_mmd").style.display = "none";
	document.getElementById("zty_mmd").style.display = "none";
	document.getElementById("ddfw_mmd").style.display = "none";
}
function show_mmd(i){
     if(i==1){
	     document.getElementById("zyx_mmd").style.display = "block";
	 }else if(i==2){
	     document.getElementById("tdy_mmd").style.display = "block";
	 }else if(i==3){
	     document.getElementById("zty_mmd").style.display = "block";
	 }else if(i==4){
	     document.getElementById("ddfw_mmd").style.display = "block";
	 }
}
function checkcity(){
  var city=document.hotelform.scity.value;
  if(city==""){
    alert('入住城市不能为空！');
	return false;
  }
}
