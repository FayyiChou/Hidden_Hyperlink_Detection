<script type="text/javascript"> 
function $(obj){
	return document.getElementById(obj);
}

var speed=20 
var d=0;
$('marquee_body').innerHTML+=$('marquee_body').innerHTML 

function Marquee(){
	if(d==1)
		if($('marquee').scrollLeft==0){ 
			$('marquee').scrollLeft=$('marquee_body').scrollWidth/2;
		}else{ 
			$('marquee').scrollLeft-- 		
		}
	else{
		if($('marquee').scrollLeft>=$('marquee_body').scrollWidth/2){ 
			
			$('marquee').scrollLeft=0 

		}else{ 
			$('marquee').scrollLeft++ 		
		}
	}
	
		
} 
function chgmar(str){
	d=str;
}
var MyMar=setInterval(Marquee,speed) 
$('marquee').onmouseover=function() {clearInterval(MyMar)} 
$('marquee').onmouseout=function() {MyMar=setInterval(Marquee,speed)} 
</script><!--bottom-->
