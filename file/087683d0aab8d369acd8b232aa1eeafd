    $("#sign_icon").click(function(){
    	if((isSignin == 'true' || isSignin == true ) ||(emptyUser == 'true' || emptyUser == true )){
    		Stats.cpb("id","indexSignIconToWeixinSignin");
    	    var to_sign_url = "/h5/weixin/signin?"+gp + "&fr="+encodeURIComponent(window.location.href);
    	    location.href = to_sign_url;
    	}else{
    		 Stats.cpb("id","indexSignIconSignin");
