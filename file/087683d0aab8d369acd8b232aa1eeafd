    		 $("#sign_icon").html("<img src='/h5/images/signed.png' alt='搜豆' class='soudou'>");
    		 isSignin = true ;
    	}
    });
    
    var mask = $(".wenxin-signin-mask");
    $(".signin-popup-close").click(function(){
        mask.hide();
    });
    
</script><script type="text/javascript">
        $(document).ready(function(){
            $(window).bind("scroll", $.debounce(500,function(){
                if (document.body.scrollTop > $(window).height()*0.5) {
                    $('#gotop').show();
                } else {
                    $('#gotop').hide();
                }
            }));
            $('#gotop').on('click',function(){
                window.scrollTo(0,0);
            });
            var tab_bar = $("#app_tab_bar");
            if(tab_bar.length>0){
                var pathName = window.location.pathname;
                if(pathName=="/h5/index"){
                    tab_bar.show();
                    $(tab_bar.find("li")[0]).siblings().removeClass("tb-selected");
                    $(tab_bar.find("li")[0]).addClass("tb-selected")
                }else if(pathName=="/h5/sort"){
                    tab_bar.show();
                    $(tab_bar.find("li")[1]).siblings().removeClass("tb-selected");
                    $(tab_bar.find("li")[1]).addClass("tb-selected")
                }else if(pathName=="/h5/ranking"){
                    tab_bar.show();
                    $(tab_bar.find("li")[2]).siblings().removeClass("tb-selected");
                    $(tab_bar.find("li")[2]).addClass("tb-selected")
                }else if(pathName=="/h5/shelf"){
                    tab_bar.show();
                    $(tab_bar.find("li")[3]).siblings().removeClass("tb-selected");
                    $(tab_bar.find("li")[3]).addClass("tb-selected")
                }else{
                    $(".app-main").css("bottom","0");
                }
            }
        })
    </script><script>
