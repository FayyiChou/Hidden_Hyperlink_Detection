<SCRIPT language=javascript>


















































































































function checkform()
	{
	  var sw=document.all.searchwd.value;
	   if(sw=="")
	      {
		    alert("请输入检索词！");
	        return false;
	      }
       else
       {
	  while(sw.charAt(0)==" ")
	  {
	  sw=sw.substring(1,sw.length);
	  }
	  while(sw.charAt(sw.length-1)==" ")
	  {
	  sw=sw.substring(0,sw.length-1);
	  }
	   while(sw.indexOf("  ")>-1)
	   {
	   sw=sw.replace("  "," ");
	   }
       while(sw.indexOf(" ")>-1)
	   {
	   sw=sw.replace(" ","*");
	   }
	   document.all.searchword.value=sw;
	   return true;
	   }
     }
</SCRIPT>



