<script type="text/javascript">
jQuery.extend({
		loginFun:function(){
			var userName = $("input[name='userName']").val();
			var password = $("input[name='password']").val();
			if(userName==null || userName==''){
				alert("请输入登录名!");
				return false;
			}
			if(!/^[A-Za-z0-9]{6,30}$/.test(userName)){	
				alert("登录名长度必须是6-30位数字或字母");
				return false;
			}
			
			if(password==null || password==''){
				alert("请输入密码!");
				return false;
			}
			
			if(!/^[A-Za-z0-9]{6,30}$/.test(password)){	
				alert("密码长度必须是6-30位数字或字母");
				return false;
			}
			return true;
		},
		doLogin:function(){
			//登录次数
			var times = $("input[name='loginTimes']").val();
			if(times >= 3){
				$("#logImg").hide();
				$("#myLog").show();
				var userName = $("input[name='userName']").val();
				var password = $("input[name='password']").val();
				var dialog = $.dialog({
					title:"输入验证码",
					width:550,
					height:320,
					id:"loginDialog",
					lock:true,
					opacity:0.4
				});
				dialog.content("<html><head>"+
						"<script type=\"text/javascript\" src=\"http://www.yunlongkm.com:80/front/js/jquery.form.js\"><\/script>"+
						"<link href=\"http://www.yunlongkm.com:80/front/css/body.css\" rel=\"stylesheet\" type=\"text/css\"/>"+
						"<script type=\"text/javascript\">"+
						"$(document).ready(function(){"+
						//更换验证码
						"$(\"#changeImg\").click(function(){"+
						"$(this).attr(\"src\",\"frontCheckCode.htm?d=\"+new Date());"+
						"});$(\"#timeLoginForm\").ajaxForm({"+
								"url : \"frontLogin.htm\","+
								"success : function(data) {"+
								"var code = data.code;"+
								"if (code == 10000) {"+
									"window.location.href = \"http://www.yunlongkm.com:80/front/main.htm\";"+
								"}else{"+
									"$(\"input[name='loginTimes']\").val(parseInt(data.loginTimes)+1);$(\"input[name='CSRFToken']\").val(data.token);alert(data.mess);"+
									"$(\"#changeImg\").attr(\"src\",\"frontCheckCode.htm?d=\"+new Date());"+
								"}"+
								"}"+
						     "});"+
						     "$(\"#loginBtn1\").click(function(){"+
						     	"var code = $(\"input[name='checkCode']\").val();"+
						     	"if(code==undefined || code==\"\"){"+
						     		"alert(\"请输入验证码!\");"+
						     		"return false;"+
						     	"}"+
						     	"$(\"#timeLoginForm\").submit();"+
						     "});});"+
						     "<\/script></head>"+
				"<body><form method=\"post\" id=\"timeLoginForm\">"+
				"<input type=\"hidden\" name=\"loginTimes\" value='"+times+"' />"+
				"<input type=\"hidden\" name=\"userName\" value='"+userName+"' />"+
				"<input type=\"hidden\" name=\"password\" value='"+password+"' />"+
				
				"<div class=\"tanchuang\" style=\"position:absolute;top:30px;left:5px;\" width=\"98%\">"+
				"<ul class=\"tc_ul\">"+
				"<li class=\"tc_li\"><label>输入验证码：</label><input id=\"checkCode\" name=\"checkCode\" maxlength=\"4\" type=\"text\" class=\"text\" /> <img id=\"changeImg\" style=\"cursor: pointer;\" title=\"看不清点我\" width=\"80\" height=\"30\" src=\"frontCheckCode.htm\" /></li>"+
				"<li class=\"tc_li\"><label></label><a id=\"loginBtn1\" name=\"loginBtn1\" href=\"javascript:void(0);\" class=\"tc_tijiao\">确认登录</a></li>"+
				"</ul>"+
			"</form></body></html>");
			}else{
				if($.loginFun()){
					$("#loginForm").submit();
				}
			}
		}
	});
	$(document).ready(function(){
		//更换验证码
		$("#changeImg").click(function(){
			$(this).attr("src","frontCheckCode.htm?d="+new Date());
		});
		$("#userName").val("输入用户名");
		$("#userName").focus(function(){
			var valss=$(this).val();
			if(valss=="输入用户名"){
				$("#userName").val("");
			}
		});
		$("#userName").blur(function(){
			var valss=$(this).val();
			if(valss==""){
				$("#userName").val("输入用户名");
			}
		});
		$("#loginForm").ajaxForm({
			url : "frontLogin.htm",
			type:"post",
			success : function(data) {
				var code = data.code;
				if (code == 10000) {
					window.location.href = "http://www.yunlongkm.com:80/front/main.htm";
				}else{
					$("#logImg").hide();
					$("#myLog").show();
					if(data.mess!=undefined){
						$("input[name='loginTimes']").val(parseInt(data.loginTimes)+1);
						alert(data.mess);
						$("#changeImg").attr("src","frontCheckCode.htm?d="+new Date());
					}
				}
			}
	    });
		//登录
		$("input[name='loginBtn']").click(function() {
			if($.loginFun()){
				$("#logImg").show();
				$("#myLog").hide();
				$.doLogin();
			}
		});
		//键盘事件
		document.onkeydown = function(event){
			var e = event||window.event||arguments.callee.caller.arguments[0];
			if(e && e.keyCode==13){
				if($.loginFun()){
					$.doLogin();
				}
			}
		};
	});
</script>
