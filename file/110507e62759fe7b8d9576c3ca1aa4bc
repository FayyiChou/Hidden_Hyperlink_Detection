                        <script>
                            var ii = -1;
                            var speed = 1;
                            var fangxiang = 1;

                            var pic = document.getElementById('pic');
                            var pic1 = document.getElementById('pic1');
                            var pic2 = document.getElementById('pic2');

                            pic2.innerHTML = pic1.innerHTML
                            function andyzsy() {
                                if (ii == 1) return
                                for (i = 1; i <= speed; i++) {
                                    if (fangxiang == "2") {
                                        pic.scrollLeft = pic.scrollLeft - 2;
                                        if (pic.scrollLeft == "0")
                                            pic.scrollLeft = pic1.offsetWidth
                                    }
                                    else {
                                        pic.scrollLeft = pic.scrollLeft + 2;

                                        if (pic.scrollLeft >= pic1.offsetWidth)
                                            pic.scrollLeft = 0
                                    }
                                }
                            }
                            function setspeed(x) {
                                speed = x;
                            }
                            function setfangxiang(x) {
                                fangxiang = x;
                            }
                            setInterval(andyzsy, 30)
                        </script>
