<!--上面专家的--> 
<script type="text/javascript">
 function cxzj(wrapera,preva,nexta,imga,speeda,ora)
 { 
  var wrapera = $(wrapera);
  var preva = $(preva);
  var nexta = $(nexta);
  var imga = $(imga).find('.cxlbtp');
  var wa = imga.find('.expert_b').outerWidth(true);
  var sa = speeda;
  nexta.click(function()
       {
        imga.animate({'margin-left':-wa},function()
                  {
                   imga.find('.expert_b').eq(0).appendTo(imga);
                   imga.css({'margin-left':0});
                   });
        });
  preva.click(function()
       {
        imga.find('.expert_b:last').prependTo(imga);
        imga.css({'margin-left':-wa});
        imga.animate({'margin-left':0});
        });
  if (ora == true)
  {
   ads = setInterval(function() { nexta.click();},sa*1000);
   wrapera.hover(function(){clearInterval(ads);},function(){ads = setInterval(function() { nexta.click();},sa*1000);});

  }
 }
 cxzj('.cxzj','.preva','.nexta','.cxzj_1',5,true);// true为自动播放，不加此参数或false就默认不自动
</script> 



</body>
</html>
