<script type="text/javascript">
    var BitNav_current_url = window.location.href;
    var BitNav_vc_url="http://vc.yiche.com/";
    var BitNav_List_len = document.getElementById("BitCommonNav_List").getElementsByTagName("li").length;
    for (var i = 0; i < BitNav_List_len; i++) {
        var BitNav_current_li = document.getElementById("BitCommonNav_List").getElementsByTagName("li")[i];
        var BitNav_url = BitNav_current_li.getElementsByTagName("a")[0].getAttribute("href");
        if (BitNav_url) {
            var reg = new RegExp(BitNav_url.split("?")[0]);
            var reg_vc=new RegExp(BitNav_vc_url);
	    if (reg.test(BitNav_current_url)) {
                BitNav_current_li.className = "active"; 
            }else if(reg_vc.test(BitNav_current_url)){
              document.getElementById("bitnav_video").className = "active"; 
            }
        } 
    }
</script>
 
