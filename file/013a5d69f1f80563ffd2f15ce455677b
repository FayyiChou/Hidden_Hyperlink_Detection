<script>

    window.soyoung = {
        staticBasePath : "html/",
        baseApiPath : "/",
        basePath : {
            imgPath: "//mstatic.soyoung.com/m/static/img/",
            oldImgPath: "//mstatic.soyoung.com/"
        }
    };
    var valiTpl = '1';
    if (valiTpl == 1) {
      soyoung.user = {"uid":0};
      soyoung.ua = {"app":"wap","os":"other"};
    }

    
        seajs.use("nine");
    

    //xysource
    (function(){
        var xysourceParam = location.href.match(/xysource=(\d*)/);
        var xysourceValue;
        xysourceParam && (xysourceValue = xysourceParam[1]);
        if(xysourceValue==1357){
            $('#ad_box_a').hide();
            $('.ad_box').hide();
            $('.drift_pop').hide();
        }
        if(xysourceValue > 0){
            var tagA = $("body").find("a");
            for(var i = 0; i < tagA.length; i ++){
                var tmp_href = tagA[i].href;
                if(tmp_href === 'undefined' || tmp_href === ''){
                    continue;
                }
                tmp_href = tmp_href.toLowerCase();

                if(tmp_href.indexOf('javascript') != -1 || tmp_href.indexOf('xysource') != -1){
                    continue;
                }
                if(tmp_href.indexOf('?') != -1){
                    tagA[i].href = tagA[i].href + '&xysource=' + xysourceValue;
                }else {
                    tagA[i].href = tagA[i].href + '?xysource=' + xysourceValue;
                }
            }
        }
    })();
    </script>
