  var xyz_is_w1200 = 0;
  function xyz_set_width() {
    var xyz_w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    if(xyz_w >= 1200) {
      if(xyz_is_w1200 != 1) {
        document.body.className = (document.body.className.replace("xyz_w960","").replace("xyz_w1200","") + " xyz_w1200").replace("  "," ");
