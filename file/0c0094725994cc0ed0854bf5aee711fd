<script language="javascript" type="text/javascript">
var myDate = new Date();
var myHours = myDate.getHours();
var writeText = "<div id=\"zzjs_net_1\" style='left:2px;position:absolute;top:460px;'><a href='#' onclick='javascript:openZxkf();'><img src='/casServer/web/activity/zxkf/images/woyaoliuyan.jpg' ></img></a></div>"
