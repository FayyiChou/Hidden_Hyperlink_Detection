<script type="text/javascript">
var typeId = "1";
var resetPasswordURL ="https://www.chinalife.com.cn/online/web/user/reset/resetPassword.jsp";
function changeItem(url,obj){
	$(".zz-main-top").find("[changeItems='']").each(function(){
		if($(this).attr("id") != $(obj).attr("id")){
			$(this).attr("class",$(this).attr("id"));
		}else{
			$(obj).attr("class",$(obj).attr("id")+ "checked");
		}
	});
	$("#userPersonalDiv").hide();
 	$("#mainFrame").attr("src",url);
 	$("#mainFrame").show()
}

function changeItemCas(divID,hiddenDiv,obj){
$("#mainFrame").hide();
	$("#userPersonalDiv").show()
		$(".zz-main-top").find("[changeItems='']").each(function(){
			if($(this).attr("id") != $(obj).attr("id")){
				$(this).attr("class",$(this).attr("id"));
			}else{
				$(obj).attr("class",$(obj).attr("id")+ "checked");
			}
		});
		if(divID=='userPersonalDiv'){
			$("#userNameTitle").html("用户名/身份证/个人计划证明号/邮箱/鹤卡号");
			typeId  ="1";
			$("#userAccount").attr("regexp","^(([a-zA-Z_]{1}(\w){5,22})|([0-9a-zA-Z_][-_\.0-9a-zA-Z-]{0,63}@([0-9a-zA-Z][0-9a-zA-Z-]*\.)+[a-zA-Z]{2,4})|([0-9a-zA-z]{19}[0-9]{1})|([0-9]{14}[Xx0-9])|([0-9]{17}[Xx0-9]))$");
			resetPasswordURL  = "https://www.chinalife.com.cn/online/web/user/reset/resetPassword.jsp";
		}else{
				$("#userNameTitle").html("用户名");
				typeId ="5";
				$("#userAccount").attr("regexp","^[^\*]*$");
				resetPasswordURL = "https://www.chinalife.com.cn/online/web/user/reset/enterpriseResetPassword.jsp";
				forgetPasswordURL ="";
 		}
}

var ifm=document.getElementById("mainFrame");
function iFrameHeight(){
	var subWeb = document.frames ? document.frames["mainFrame"].document : ifm.contentDocument; 
	if(ifm != null && subWeb != null) {
		ifm.height = document.frames ? subWeb.body.scrollHeight  : subWeb.body.offsetHeight ;
		ifm.width = subWeb.body.scrollWidth;
	}
}
function toRegister(){
	var type = $("#type").val(); 
	window.location.href='https://www.chinalife.com.cn/online/web/user/register/index.jsp?type='+type;
}
