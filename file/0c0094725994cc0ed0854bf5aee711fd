<script>
//init validate kuang
var check = new SFEC();

$(document).ready(function(){
	check.setForm("fm1");
	$("#userAccount").attr("bind","focus");
	$("#userAccount").attr("cType","notEmpty|regexp|moblieNotFind|moblieAccountError|moblieNotUniqueAndAuthenticError|moblieNotUniqueAndNotAuthenticError");//
	$("#userAccount").attr("tipid","userAccount_tip");
	$("#userAccount").attr("tipmsg","请正确填写登录账号");
	$("#userAccount").attr("errmsg","请输入登录账号|登录账号格式不正确|您输入的帐号信息有误|该手机号未经校验，请使身份证号等其他方式登录|您的手机号存在重复用户，请使身份证号等其他方式登录|该手机号未经校验，请使身份证号等其他方式登录");//
	
	//图片验证码校验
	$("#rand").attr("bind","focus");
	$("#rand").attr("cType","scope|pictureActiveCodeError");
	$("#rand").attr("tipid","rand_tip");
	$("#rand").attr("tipmsg","请正确填写验证码");
	$("#rand").attr("errmsg","验证码格式不正确|验证码错误");
	
	//校验手机号唯一性
	check.addRule('moblieNotFind',function (obj){
		if(!checkMobilePhone($("#userAccount").val())){
			return true;
		} else {
			return checkMobileNotFind($("#userAccount").val()+"#"+typeId);
		}
	 });	
	check.addRule('moblieAccountError',function (obj){
		if(!checkMobilePhone($("#userAccount").val())){
			return true;
		} else {
			return checkMobileNotRepeat($("#userAccount").val()+"#"+typeId);
		}
	 });
	check.addRule('moblieNotUniqueAndAuthenticError',function (obj){
		if(!checkMobilePhone($("#userAccount").val())){
			return true;
		} else {
			return checkMobileNotUnqiueAndAuthentic($("#userAccount").val()+"#"+typeId);
		}
	 });
	check.addRule('moblieNotUniqueAndNotAuthenticError',function (obj){
		if(!checkMobilePhone($("#userAccount").val())){
			return true;
		} else {
			return checkMobileNotUnqiueAndNotAuthentic($("#userAccount").val()+"#"+typeId);
		}
	 });
	check.addRule('pictureActiveCodeError',function (obj){
		return checkedPictureActiveCode();
	 });
})

 /**ajax验证手机号是否存在**/
 function checkMobileNotFind(mobileNum) {
	 	var dataFlag = false;
	 	$.ajax({
	 		url : "/casServer/mobileLoginValidate",
			cache : false,
			async : false,// 同步请求
	 		dataType : "json",
			data : {"flag":"1","mobileNum" : mobileNum},
			type : "POST",
			success : function(json) {
				mobileNumTotal=json.mobileNumTotal;
			}
		});
		if(mobileNumTotal==null || mobileNumTotal==undefined || mobileNumTotal=="" || mobileNumTotal==0){
			dataFlag = false;
		}else{
			dataFlag = true;
		}
		return dataFlag;
 }
 
/**ajax验证手机号唯一且有验真**/
function checkMobileNotRepeat(mobileNum) {
	 	var dataFlag = false;
	 	$.ajax({
	 		url : "/casServer/mobileLoginValidate",
			cache : false,
			async : false,// 同步请求
	 		dataType : "json",
			data : {"flag":"2","mobileNum" : mobileNum},
			type : "POST",
			success : function(json) {
				mobileNumTotal=json.mobileNumTotal;
				checkMobile = json.checkMobile;
				unCheckMobile = json.unCheckMobile;
			}
		});
		if(mobileNumTotal==1 && unCheckMobile==1){
			dataFlag = false;
		}else{
			dataFlag = true;
		}
		return dataFlag;
}

/**ajax验证手机号不唯一且有验真**/
function checkMobileNotUnqiueAndAuthentic(mobileNum) {
	 	var dataFlag = false;
	 	$.ajax({
	 		url : "/casServer/mobileLoginValidate",
			cache : false,
			async : false,// 同步请求
	 		dataType : "json",
			data : {"flag":"2","mobileNum" : mobileNum},
			type : "POST",
			success : function(json) {
				mobileNumTotal=json.mobileNumTotal;
				checkMobile = json.checkMobile;
				unCheckMobile = json.unCheckMobile;
			}
		});
		if(mobileNumTotal>1 && checkMobile>=1){
			dataFlag = false;
		}else{
			dataFlag = true;
		}
		return dataFlag;
}
/**ajax验证手机号不唯一且无验真**/
function checkMobileNotUnqiueAndNotAuthentic(mobileNum) {
	 	var dataFlag = false;
	 	$.ajax({
	 		url : "/casServer/mobileLoginValidate",
			cache : false,
			async : false,// 同步请求
	 		dataType : "json",
			data : {"flag":"2","mobileNum" : mobileNum},
			type : "POST",
			success : function(json) {
				mobileNumTotal=json.mobileNumTotal;
				checkMobile = json.checkMobile;
				unCheckMobile = json.unCheckMobile;
			}
		});
		if(mobileNumTotal>1 && checkMobile==0){
			dataFlag = false;
		}else{
			dataFlag = true;
		}
		return dataFlag;
}
/**ajax验证验证码**/
function checkedPictureActiveCode() {
	var dataFlag = false;
	$.ajax({
		type : "POST",
		async : false,
		url : "/casServer/mobileLoginValidate",
		dataType : "json",
		data : {"flag":"3" ,"checkCode":$("#rand").val()},
		success : function(json){
			if($.trim(json.checkFlag) == "1") {
				dataFlag = true;
			} else {
				dataFlag = false;
			}
		}
	});
	return dataFlag;
}

function checkMobilePhone(mobileNum){
 	var dataFlag = false;
	var pattern = /^[1-9]{1}[0-9]{10}$/;
	if (pattern.test(mobileNum)) {
		dataFlag = true;
	} else {
		dataFlag = false;
	}
	return dataFlag;
}

function addRules(){
	//帐号校验
	$("#userAccount").attr("bind","focus");
	$("#userAccount").attr("cType","notEmpty|regexp|moblieNotFind|moblieAccountError|moblieNotUniqueAndAuthenticError|moblieNotUniqueAndNotAuthenticError");//
	$("#userAccount").attr("tipid","userAccount_tip");
	$("#userAccount").attr("tipmsg","请正确填写登录账号");
	$("#userAccount").attr("errmsg","请输入登录账号|登录账号格式不正确|您输入的帐号信息有误|该手机号未经校验，请使身份证号等其他方式登录|您的手机号存在重复用户，请使身份证号等其他方式登录|该手机号未经校验，请使身份证号等其他方式登录");//
	//密码校验
	$("#pwd").attr("bind","focus");
	$("#pwd").attr("cType","notEmpty");
	$("#pwd").attr("tipmsg","请正确填写密码");
	$("#pwd").attr("tipid","pwd_tip");
	$("#pwd").attr("errmsg","请输入密码");		
	//图片验证码校验
	$("#rand").attr("bind","focus");
	$("#rand").attr("cType","scope|pictureActiveCodeError");
	$("#rand").attr("tipid","rand_tip");
	$("#rand").attr("tipmsg","请正确填写验证码");
	$("#rand").attr("errmsg","验证码格式不正确|验证码错误");
	
	check.restart();
}

function removeRules(){
	//移除帐号校验
	$("#userAccount").removeAttr("bind");
	$("#userAccount").removeAttr("cType");
	$("#userAccount").removeAttr("tipmsg");
	$("#userAccount").removeAttr("tipid");
	$("#userAccount").removeAttr("errmsg");
	//移除密码校验
	$("#pwd").removeAttr("bind");
	$("#pwd").removeAttr("cType");
	$("#pwd").removeAttr("tipmsg");
	$("#pwd").removeAttr("tipid");
	$("#pwd").removeAttr("errmsg");	
	//移除验证码校验
	$("#rand").removeAttr("bind");
	$("#rand").removeAttr("cType");
	$("#rand").removeAttr("tipmsg");
	$("#rand").removeAttr("tipid");
	$("#rand").removeAttr("errmsg");
	
	check.restart();
}
function doResetPwd(){
	window.open(resetPasswordURL,"重置密码" ,"");
}

/**submit*/
function login(formId){
	check.start();
	addRules();
	if(check.result(true)){
		var newUserAccount = $.trim($("#userAccount").val())+"#"+typeId;
		$("#"+formId+" #username").val(newUserAccount);
		document.getElementById("btn_submitId").disabled = "disabled";
		return true;
	}else{
		if(check.checkResult.rand_tip){
			var phoneNotFind = checkMobileNotFind($("#userAccount").val()+"#"+typeId);
		    var phoneNumNotUnqiueAndAuthenticFlag = checkMobileNotUnqiueAndAuthentic($("#userAccount").val()+"#"+typeId);
		    var phoneNumNotUnqiueAndNotAuthenticFlag = checkMobileNotUnqiueAndNotAuthentic($("#userAccount").val()+"#"+typeId);
		    if(phoneNotFind){
			    if(!phoneNumNotUnqiueAndAuthenticFlag || !phoneNumNotUnqiueAndNotAuthenticFlag){
// 					$("#userAccount").removeClass("fail_style");
					$("#userAccount_tip").html("");
					window.location.href = "https://www.chinalife.com.cn/online/web/user/login/modifymobile.jsp";
			    }
		    }
		} else {
			$("#userAccount_tip").html("");
		}
		removeRules();
		return false;
	}
}

/**update rand*/
function changeImage(){
	$("#randImge").attr("src","/casServer/web/user/login/image.jsp?id="+Math.random(100)*10);
}

/**get error msg html*/
function getErrorMsg(msg){
	var failHtml = "<table class='cudTs3'><tbody><tr><td class='topL'></td><td></td><td class='topR'></td></tr><tr><td></td>"+
	"<td class='tdCon'>"+msg+"</td><td></td></tr><tr><td class='botL'></td><td></td><td class='botR'></td></tr></tbody></table>"
	return failHtml;
}

function mobliePhoneLogin(){
	var action  = $("#fm1").attr("action");
	var params = action.indexOf("?")!= -1 ? "?mobileLogin=1234&"+action.substring(action.indexOf("?")+1) : "?mobileLogin=1234";
	window.location.href = "/casServer/mobileLogin"+params;
}
