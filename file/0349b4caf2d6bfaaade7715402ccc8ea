var t1 = n1 = 0, count1;
function newsAuto(){	
	count1=jQuery("#nt_list div.ntc").length;
	jQuery("#nt_list div.ntc:not(:first-child)").hide();
	jQuery("#nt_point li").click(function() {
		var i = jQuery(this).attr("val") - 1;
		n1 = i;
		if (i >= count1) return;
		jQuery("#nt_list div.ntc").filter(":visible").fadeOut(1).parent().children().eq(i).fadeIn(1);
		jQuery(this).toggleClass("nton");
		jQuery(this).siblings().removeAttr("class");
		clearInterval(t1);
	});
	t1 = setInterval("showAuto1()", 4000);
	jQuery("#nt").hover(function(){clearInterval(t1);}, function(){t1 = setInterval("showAuto1()", 4000);});
}
function showAuto1(){
	n1 = n1 >=(count1 - 1) ? 0 : ++n1;
	jQuery("#nt_point li").eq(n1).trigger('click');
}
</script>
