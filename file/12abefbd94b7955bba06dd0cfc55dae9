<script>
    //微信分享
    $(function(){
        var url = window.location.href;
        if(url.indexOf("pingce.jd.com") > 0){
            url = url.replace("pingce.jd.com", "pingce.m.jd.com");
        }
        var str = toUtf8(url);
        $('#code').qrcode({
            //render: "table",
            width: 120,
            height:120,
            text: str
        });

        $("#shareToWeixin").click(function(){
            $('#shareCode').show();
        });

        $('#shareCode .close').on('click',function(){
            $(this).parent().hide();
        });
    })
    function toUtf8(str) {
        var out, i, len, c;
        out = "";
        len = str.length;
        for(i = 0; i < len; i++) {
            c = str.charCodeAt(i);
            if ((c >= 0x0001) && (c <= 0x007F)) {
                out += str.charAt(i);
            } else if (c > 0x07FF) {
                out += String.fromCharCode(0xE0 | ((c >> 12) & 0x0F));
                out += String.fromCharCode(0x80 | ((c >>  6) & 0x3F));
                out += String.fromCharCode(0x80 | ((c >>  0) & 0x3F));
            } else {
                out += String.fromCharCode(0xC0 | ((c >>  6) & 0x1F));
                out += String.fromCharCode(0x80 | ((c >>  0) & 0x3F));
            }
        }
        return out;
    }

</script>

<script type="text/javascript">
    (function() {
        $.trim = function(str) {
            if (str === undefined || str === null) {
                return "";
            }
            return str.replace(/^\s/, '').replace(/\s$/, '');
        }
        jQuery = $;
    })("hack")
</script>
