<script type="text/javascript" src="/statics/js/jquery.js"></script>
<script>
$(function(){
	var zzsc = 0; // 默认值为0，二级菜单向下滑动显示；值为1，则二级菜单向上滑动显示
	if(zzsc ==0){
		$('.zzsc li').hover(function(){
			$('.second',this).css('top','52px').show();
		},function(){
			$('.second',this).hide();
		});
	}else if(zzsc ==1){
		$('.zzsc li').hover(function(){
			$('.second',this).css('bottom','52px').show();
		},function(){
			$('.second',this).hide();
		});
	}
});
</script>
