        function CheckForm() {
            var a = window.document.getElementById("SelContent").value;
            var b = window.document.getElementById("Select").value;
            if (a == "") {
                window.alert("请填写搜索内容");
                return false;
            }
            else {

                window.open("/HomePage/SS.aspx?a=" + encodeURIComponent(a) + "&b=" + b);
            }
        }

        window.onload = function () {//加载
            document.getElementById('Load_time').innerHTML = '今天是：2017年02月18日（农历 正月廿二）';
        }
