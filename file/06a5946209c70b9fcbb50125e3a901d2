<script type="text/javascript">
    $(document).click( function(e){
        var tjTarget = $(e.target);
        if(tjTarget.is('img') || tjTarget.is('a'))
        {
            for(var i=0;i<5;i++)
            {
                if(tjTarget.data('tj'))
                {
                    clickCount(tjTarget.data('tj'));
                    break;
                }
                else
                {
                    tjTarget = tjTarget.parent();
                }
            }
        }
    });

    // 顶部搜索
    $(function() {
        $(document).on('focus','.input_box .txt',function(){
            if ($(this).val() == '搜索一下') {
                $(this).val('');
            }
            $(this).addClass('txt_active');
        })
        $(document).on('blur','.input_box .txt',function(){
            if ($(this).val() == '') {
                $(this).val('搜索一下');
                $(this).removeClass('txt_active');
            }
        })

        var headHtml1 = '<a href="http://licai.2345.com/finance/?newso" target="_blank" data-tj="tj_search_ad1">月入5000巧理财</a><a href="http://tools.2345.com/kuaidi.htm" target="_blank" data-tj="tj_search_ad2">快递查询</a>';
        var headHtml2 = '<a href="http://lvyou.2345.com/jipiao.html?news" target="_blank" data-tj="tj_search_ad3">99元特价机票</a><a href="http://licai.2345.com/?news" target="_blank" data-tj="tj_search_ad4">最赚理财</a>';
        var newsNum = 0;

        setInterval(function() {
            newsNum++;
            if (newsNum % 2 == 0) {
                $('#top_r').html(headHtml2);
            } else {
                $('#top_r').html(headHtml1);
            }
        }, 10000)
    })
</script>

