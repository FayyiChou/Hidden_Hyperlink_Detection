        <script type="text/javascript">
            function submitOnClick(){
                $('.value').find('input').each(function(){
                    if ($(this).hasClass('has-error')){
                        $(this).removeClass('has-error');
                        $(this).val("");
                    }
                });
                reachGoalZide();
            }

            function closeDialog(){
                var dlg = $(".dialog-overlay");
                dlg.hide();
            }

            $(document).ready(function(){

                $('.bxslider').bxSlider({
                    auto: true,
                    speed: 900,
                    pause: 6000,
                    autoControls: false,
                    controls: false,
                    pagerCustom: '#bx-pager',
                    autoHover: true
                });

                $('#on-line a, #add-region').click(function(){
                    var dlg = $(".dialog-overlay");
                    dlg.load("/popup", function(){
                        dlg.show();
                        initDialog(dlg);
                        $('.dialog').find('.field').first().find('input').focus();
                    });
                });

                function initDialog(dlg){
                    var dlg_window = $('.dialog-container');
                    dlg_window.css('margin-top', Math.floor(($(window).height() - dlg_window.height()) / 2)+'px');
                    var div = $('.dialog-overlay');

                    $(".dialog-overlay form").ajaxForm({
                        target: dlg,
                        complete: function (){ initDialog(dlg); }
                    });

                    $('.form-close a').click(function(){
                        closeDialog();
                    });

                    var form = $('.dialog');
                    form.find('input').focus(function(){
                        if ($(this).hasClass('has-error')){
                            $(this).removeClass('has-error');
                            $(this).val("");
                        }
                        $(this).addClass('active');
                    });

                    form.find('input').focusout(function(){
                       $(this).removeClass('active');
                    });
                    form.find('textarea').focus(function(){
                        $(this).addClass('active');
                    });
                    form.find('textarea').focusout(function(){
                        $(this).removeClass('active');
                    });

                    var values = $('.value');
                    values.each(function(){
                        if ($(this).children('ul').find('li').length){
                            $(this).children('input').addClass('has-error');
                            $(this).children('input').val($(this).children('ul').find('li').first().text().trim());
                        }
                    })
                }
            });
        </script>

        
