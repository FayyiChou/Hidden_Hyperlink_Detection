  var speed=30
  demox2.innerHTML=demox1.innerHTML
  function Marquee(){
  if(demox2.offsetWidth-demox.scrollLeft<=0)
  demox.scrollLeft-=demox1.offsetWidth
  else{
  demox.scrollLeft++
  }
  }
  var MyMar=setInterval(Marquee,speed)
