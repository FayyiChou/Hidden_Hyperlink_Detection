      function tx_name(s)
      {
            if (s==1) {
                if (document.getElementById('tx_name').value=='Имя...') {
                document.getElementById('tx_name').value=''
                }
            } else {
                if (document.getElementById('tx_name').value=='') {
                document.getElementById('tx_name').value='Имя...'
                }
            }
      }

      function tx_comment(s)
      {
            if (s==1) {
                if (document.getElementById('tx_comment').value=='Текст сообщения...') {
                document.getElementById('tx_comment').value=''
                }
            } else {
                if (document.getElementById('tx_comment').value=='') {
                document.getElementById('tx_comment').value='Текст сообщения...'
                }
            }
      }

  </script>
