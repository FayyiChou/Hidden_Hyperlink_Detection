		function changeShen(obj,city){
			var aTmp='';
			if(!$(city))
				return;
			if(!obj)
				return;
			city = $(city);
			var index = parseInt(obj.value);
			if(!index)
				index = 0;
			city.length = aCity[index].length;
			for(var i=0;i<city.length;i++)
			{
				city.options[i].value = i;
				aTmp=aCity[index][i].split('|');
				city.options[i].text  = aTmp[0];
			}
			city.options[0].selected=true;

			aTmp=aCity[index][0].split('|');
			jq('#hidden1')[0].value=aTmp[1];
		};
		changeShen($('shen'),'city');
		function changeCity(){
			var shen_index=$('shen').value;
			var city_index=$('city').value;
			var aTmp=aCity[shen_index][city_index].split('|');
			jq('#hidden1')[0].value=aTmp[1];
		};
