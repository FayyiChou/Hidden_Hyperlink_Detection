//编辑推荐检索
var getTeamlist = function(word,chn,prepage,page,order){
	var flag = word;
//	word = word.replace(/&/g," ");
//
//	if(word == null  || word == 'undefined'){
//        word="njtype='1799' and cctype='1451'";  //默认设置一年级上册相关内容
//      }
	word="njtype='1799' and cctype='1451' and doctitle!='banner' and doctitle!='创编团队' and doctitle!='我要投稿' and doctitle!='我有疑问'";
	
	//alert("/was5/web/search?channelid="+chn+"&prepage="+prepage+"&orderby="+order+"&searchword="+word+"&page="+page);
	
	if(order != null && order != ""){
		order = "-"+order;
	}
	$.ajax({
		url:"/was5/web/search?channelid="+chn+"&prepage="+prepage+"&orderby="+order+"&searchword="+word+"&page="+page, 
//		error: function(){},
		async:true,
		timeout: 300,
		success:function(html){
                        //alert(html);
			$("#bjtjul").html(html);
			$("#bjtjul li a").each(function(index,element){
			    $(this).attr("href",$(this).attr("href")+"?"+flag);
			});
		},
		error:function(){
		}
	});
};
function nextPage(){
	page++;
	getTeamlist(word,chn,prepage,page,"weight");
}
function previousPage(){
	if(page==1){
		return;
	}
	page--;
	getTeamlist(word,chn,prepage,page,"weight");
}
//切换
function tab(flag){
    if(flag=="1"){
    	$("#djdzk").css("display","none");
		$("#bjtj").css("display","none");
		$(".xw_rjdtLeft").attr("style","float:none;margin:0 auto;");
		$(".xw_sanXianA").attr("onclick","tab(2)");
    }else if(flag=="2"){
    	$("#djdzk").css("display","block");
		$("#bjtj").css("display","block");
		$(".xw_rjdtLeft").attr("style","");
		$(".xw_sanXianA").attr("onclick","tab(1)");
    }
}

function addfavorite(sTitle,sURL)
{
	try
	{
	window.external.addFavorite(sURL, sTitle);
	}
	catch (e)
	{
	try
	{
	window.sidebar.addPanel(sTitle, sURL, "");
	}
	catch (e)
	{
	   if(isFirefox=navigator.userAgent.indexOf("Firefox")>0){  
	        return;  
	   }  
		alert("加入收藏失败，请使用Ctrl+D进行添加");
	}
	}
} 

function change_size(element,size){
     var current = parseInt(element.css('font-size'));
     if(size == "smaller"){
         var newSize = current - 1;
     }else if(size == "bigger"){
         var newSize = current + 1;
    }    
     element.css('font-size',newSize+'px');
 }

$(function(){

    if($.trim($("#downtext").text())=="点击下载："){
       $("#down").css("display","none");
    }

	$(".contee img").each(function(index, element) {
	    $(this).parent("p").css("text-align","center");
	});
	
	$(".contee embed").each(function(index, element) {
	    $(this).parent("p").css("text-align","center");
	});
	
	$(".contee object").each(function(index, element) {
	    $(this).parent("p").css("text-align","center");
	});
	
	$(".share5").click(function(){
	    change_size($(".contee"),"smaller");    
	    change_size($(".contee *"),"smaller");    
	});
	$(".share4").click(function(){
	     change_size($(".contee"),"bigger");    
	     change_size($(".contee *"),"bigger");    
	});

 
//微信鼠标移入移除效果
	$(".xw_wxA").mouseover(function(){
		$("#xw_xwSaoSao").css("display","block");
	});
	$(".xw_wxA").mouseout(function(){
		$("#xw_xwSaoSao").css("display","none");
	});
	
});

//PDF插件
function showPDF(path1, path2) {
	$.fancybox.open({
		href : path1 + 'images/pdfjs/web/viewer.html?file=../../..' + path2,
		type : 'iframe',
		padding : 0,
		width : '100%',
		height : '100%'
	});
}
