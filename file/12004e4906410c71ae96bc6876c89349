<script>
//导航条url判断后加active
$(function(){
	
	var urlkey = window.location.host;
	localStorage.setItem("urlkey", urlkey); //设置键的值 
	
	var url1 = window.location.pathname;
	
	var url2 = url1.replace("/xxyw","");
	var url = url2.replace("/xiaoyu","");
	
	if(url=="/tbjx/"){
		$("#tbjx").attr("class","active");
	}else if(url=="/tbxx/"){
		$("#tbxx").attr("class","active");
	}else if(url=="/tbzy/"){
		$("#tbzy").attr("class","active");
	}
});

  function js_method(urlpath){
	  
	  var url = localStorage.getItem("urlkey"); //获取键的值 

	  if(url=="www.pep.com.cn"){
		  window.location.href = "http://"+url+"/xiaoyu/"+urlpath+"/";
	  }else{
		  window.location.href = "http://"+url+"/"+urlpath+"/";
	  }
  }



$(document).ready(function() {

$(".wx_link").hover(
	function(){
		$(".wx_con").show();
	},
	function(){
		$(".wx_con").hide();
	}
)

$(".navbar-droplist").mouseenter(function(){
$(".main1_bg").show();
})
$(".main1_bg").mouseleave(function(){
$(".main1_bg").hide();
})
}); 
</script>
