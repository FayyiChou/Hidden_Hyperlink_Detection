<SCRIPT type=text/javascript>
    function movePiao() {
        var piao=document.getElementById('shengming'); 
        if (window.innerHeight) {
            pos=window.pageYOffset;
        } else if (document.documentElement){ 
            pos=document.documentElement.scrollTop;
        } else if (document.body) {
            pos=document.body.scrollTop;
        }
        piaoTop=pos - (-30);
        var res=new Array;
        res[0]=piaoTop.toString();
        res[1]="px";
        piao.style.top=res.join('');
    }
    window.onload=movePiao;
   // window.onscroll=movePiao;
</SCRIPT>



