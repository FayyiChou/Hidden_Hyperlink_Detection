            function _getsalary() {
                var s = '2001-4000元/月';
                var g = new RegExp('\\d+', 'g');
                if (g.test(s)) {
                    var t = g.exec(s);
                    var a = [];
                    while (t) {
                        a.push(Number(t[0]));
                        t = g.exec(s);
                    }
                    return a.sort(function(x, y) { return x - y; }).reverse()[0];
                } else {
                    return s;
                }
            }
			
			function _getindustry(){
			    var s = '<a target="_blank" href="http://jobs.zhaopin.com/baoding/in140000/">房地产/建筑/建材/工程</a>';
			    var g = new RegExp('<\/?a[^>]*>', 'g');
				s = s.replace(g,'');
				return s;
			}

            function _getproductid() {
                var g = new RegExp('\\d+');
                return g.exec('http://jobs.zhaopin.com/551710220250241.htm')[0];
            }

            function _getcomplogo() {
                var logo = '';
                if (logo.replace(/\s+/g, '').length == 0) {
                    logo = 'http://img01.zhaopin.cn/2014/rd2/img/default360.jpg';
                }
                return logo;
            }

