            _mvq.push(['$addGoods', /*分类id*/ '', /*品牌id*/ '', /*商品名称*/ '司机', /*商品ID*/_getproductid(), /*商品售价*/ _getsalary(), /*00商品图片url*/ _getcomplogo(), /*分类名*/ _getindustry(), /*品牌名*/ '', /*商品库存状态1或是0*/ '', /*网络价*/ '', /*收藏人数*/ '', /*商品下架时间*/ '']);
            _mvq.push(['$addPricing', /*价格描述*/ _getsalary()]);
