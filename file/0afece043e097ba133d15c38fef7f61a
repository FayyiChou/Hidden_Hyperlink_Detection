<script type="text/javascript">

$(".ibanner").slide({mainCell:".bd ul",effect:"fold",autoPlay:true,pnLoop:false,delayTime:600,interTime:5000});

function changeBanner(){

	var ww = $(window).width();

	if(ww<1100){ ww=1100; }

	$(".ibanner .bd, .ibanner .bd ul, .ibanner .bd li").width(ww);

}

$(window).resize(function(){ changeBanner(); });

$(window).scroll(function(){ changeBanner(); });



