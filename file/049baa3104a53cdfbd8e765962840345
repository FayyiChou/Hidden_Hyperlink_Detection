                        $(".I4_2_2 hd").css("position","relative");
                        var tag_num=$(".I4_2_1 ul li").length;
                        $(".next").click(function(){
                            var active_idx=$(".I4_2_1 ul li.mon5").index();
                            var active_now=active_idx;
                            if(active_idx<(tag_num-1)){
                                active_idx++;
