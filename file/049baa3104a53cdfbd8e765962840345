                                active_idx=0;
                            }
                            change(active_idx,active_now);
                        })
                        $(".prev").click(function(){
                            var active_idx=$(".I4_2_1 ul li.mon5").index();
                            var active_now=active_idx;
                            if(active_idx>0){
                                active_idx--;
                            }else{
                                active_idx=tag_num-1;
                            }
                            change(active_idx,active_now);
                        })
                        $(".I4_2_1 ul li").click(function(){
                            var active_now=$(".I4_2_1 ul li.mon5").index();
                            var active_idx=$(this).index();
                            if(active_idx!=active_now){
                                change(active_idx,active_now);
                            }
                        }).mouseover(function(){
                            var active_now=$(".I4_2_1 ul li.mon5").index();
                            var active_idx=$(this).index();
                            if(active_idx!=active_now){
