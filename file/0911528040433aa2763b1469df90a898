	//判断是否经纪人
	function checkBroker(tel,hid) {
		$.ajax({
			url:'/comm/checkBroker.htm?infoId=' + hid + "&tel=" + tel + "&t=" + new Date(),
			type:"post",
			success:function(data) {
				if (data != '' && data.length < 30) { 
					var cityName = data.split(',')[0];
					var count = data.split(',')[1];
					isBroker(count);
				}
			}
		})
	}
	//显示房源信息真实度
	function isBroker(count) {
		count = parseInt(count);
		var brokerYes = 70;
		var brokerNo = 30;
		if (count <= brokerNo) {
			$('#isBroker').show();
			$('#believe').removeClass().addClass('red');
			$('#fytips').html("<img src='/images/v2.0/icon_fd2.gif' >&nbsp;&nbsp;房源智能分析系统检测此房源为 <b>中介房源</b>");
		} else if (count >= brokerNo && count < brokerYes) {
			$('#believe').removeClass().addClass('blue');
			$('#fytips').html("<img src='/images/v2.0/icon_fd2.gif' >&nbsp;&nbsp;房源智能分析系统检测此房源为 <B>不确定房源</b>");
		} else if (count >= brokerYes) {
			$('#believe').removeClass().addClass('green');
			$('#fytips').html("<img src='/images/v2.0/icon_fd2.gif' >&nbsp;&nbsp;房源智能分析系统检测此房源为 <B>真实个人房源</b>");
		}
		$('#progress').progressBar(count,{showText:false});
		$('#bcount').html(count + "%");
		$('#barDiv').show();
		$('#barLoadDiv').hide();
	}
	$(document).ready(function(){
		$('#info_url').html('登录查看来源详细');
		$("#info_url").click(function(){
			slogin();
		});
	});
	
	
	function judge(hid){
        showIframe('/info/judge.htm?id='+hid+"&t="+new Date(),"中介判断工具",200,400);
	}
	

   $(function(){
	/** 登录 **/
	var phoneMsg = '请输入您要查询的手机号码';
	//账号输入框得到焦点 失去焦点事件
	$('#phone').val(phoneMsg);
	$('#phone').addClass('color_1');
	$('#phone').focus(function() {
		var phone = $(this).val();
		if(phone == '' || phone == phoneMsg)
			$(this).val('');
		$(this).removeClass('color_1');
	});
	$("#phone").blur(function(){
		var phone = $(this).val();
		if(phone == '') {
			$(this).val(phoneMsg);
			$(this).addClass('color_1');
		}
	});
	$('#phone').focus();
	})

	
	//cookie 浏览记录
	$(function(){
		var His_hid = "15969506";
		var His_hname = "连云港市新浦区光辉路25号楼三单元602室二手房出售朝阳二手房(个人)";
		var His_houseType_name = "fangwu";
		var canAdd = false;
		
		//取得cookie
		var hisHouse = $.cookie("hisHouse");
		var len=0;
	 
		if(hisHouse){
			hisHouse = eval('('+hisHouse+')');
			len = hisHouse.length;
		}
		//已存在的id
		var dupId = len + 1;
		if('null'!=  His_hid  && His_hid.length > 0){//查看
			canAdd = true;
			//该房源是否在cookie中 (先删除然后添加)
			$(hisHouse).each(function(i){
				if(this.houseId==His_hid){
					dupId = i;
				}
			})
		}
		if(canAdd){
			var temp="[";
			var startNum=0;
			if(len > 10) startNum = 1;
			
			for(var m=startNum;m<len;m++){
				if(m!=dupId)
					temp += "{\"houseId\":\""+hisHouse[m].houseId+"\",\"housename\":\""+hisHouse[m].housename+"\",\"houseTypeName\":\""+hisHouse[m].houseTypeName+"\"},"; 
			}
			
			temp +="{\"houseId\":\""+His_hid+"\",\"housename\":\""+His_hname+"\",\"houseTypeName\":\""+His_houseType_name+"\"}]";
			$.cookie("hisHouse",temp,{expires:30,path:"/"});
		}
	})	
	
	
	//房源删除
	function houseinfo_del(hid){
	   if( !hid || hid == 0 ){
	       alert("房源参数无效");
	       return;
	   }
	   $("#houseInfoId").val(hid);
	   $("#house_del").dialog({
	     title:'房源删除',
	     modal:true,
	     width:230,
	     height:100
	   });
	}
    function houseinfoDel(){
      var operatePwd = $("#operatePwd").val();
      if( $.trim(operatePwd) == "" ){
        alert("请输入房源删除口令");
        $("#operatePwd").focus();
        return;
      }
      var hid = $("#houseInfoId").val();
      if( hid == 0 ){
           alert("房源参数无效");
	       return;
      }
	  if(confirm("确定删除吗？")){
			$.ajax({
				type:"get",
		    	url:"/info/houseInfoDel.htm?hid=" + hid +"&operatePwd=" + operatePwd,
		    	dataType: "json",
		    	cache:false,
		    	success:function(json) {
		    		 alert(json.msg);
		    		 window.location.reload();
				},
				error: function(XmlHttpRequest) {
		            alert("发送信息错误！！请重试...");
		            return false;
		        }
			})			
		}
	}
</script>

</body></html>