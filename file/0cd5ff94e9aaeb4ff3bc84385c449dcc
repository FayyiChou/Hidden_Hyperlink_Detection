    <script type="text/javascript">
	$('#nsearch').on('click', function(){
	  	var ntradenum = $("#ntradenum").val();
		if(isNaN(ntradenum) || ntradenum==""){
			layer.alert('请输入贸易总金额', {icon: 5}); 
			return false;
		}
		$("#nsearchrezult").css('display','block');
		var js_jsb = parseFloat(ntradenum)*6.4*0.0011*10000;
		var js_hxt = parseFloat(ntradenum)*6.4*1.1*0.0006*10000;
		var js_ddr = parseFloat(ntradenum)*0.8*6.4*10000;
		$("#js_jsb").html(js_jsb.toFixed(2));
		$("#js_hxt").html(js_hxt.toFixed(2));
		$("#js_ddr").html(js_ddr.toFixed(2));
	});
	</script>
