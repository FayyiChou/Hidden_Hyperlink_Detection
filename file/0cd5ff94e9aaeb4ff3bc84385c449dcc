<script>
$(window).scroll(function () {
	if($(document).scrollTop()>=30){		
		$('div#t_menu').addClass('settopmenu');
		$('div#t_menu').css('position','fixed');
	}else{
		$('div#t_menu').addClass('settopmenunormal');
		$('div#t_menu').css('position','relative');
	}
});
</script>

