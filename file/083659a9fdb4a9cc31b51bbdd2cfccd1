        // 下载跳转
    $('.btn-ios-download').click(function() {
        window.open("https://itunes.apple.com/cn/app/id416048305?mt=8&uo=4");
    })
    $('.pop-btn').click(function() {
        location.href = "http://xiuxiu.android.dl.meitu.com/xiuxiu.apk";
        $('.mask').css('display', 'none');
        $('.android-pop').css('display', 'none');
    })
    $('.pop-btns').click(function() {
        location.href = "http://xiuxiu.dl.meitu.com/MTXX4.8.1_20151113_18.29.apk";
        $('.mask').css('display', 'none');
        $('.android-pop').css('display', 'none');
    })
    $('.btn-ipad-download').click(function() {
        window.open("https://itunes.apple.com/app/id463422433?mt=8");
    })
    $('.btn-wp-download').click(function() {
            window.open("http://windowsphone.com/s?appid=fd7fcb4d-122c-45cd-952a-efb3a1293963");
        })
        // 安卓下载
    $('.btn-android-download').click(function() {
        $('.mask').css('display', 'block');
        $('.android-pop').css('display', 'block');
    })
    $('.pop-close').click(function() {
            $('.mask').css('display', 'none');
            $('.android-pop').css('display', 'none');
        })
        /*
         * 第二屏图片切换功能
         * author: zenggx
         */
        // Ie浏览器兼容
    if (navigator.userAgent.indexOf('MSIE') >= 0) {
        $('.sec2-pic>li').css({
            'display': 'none',
            'opacity': '1'
        });
        $('.sec2-pic').children('li:first').css('display', 'block');
    }
    $('.sec2-tip>a').click(function() {
        $(this).addClass('sec2-tip-selected').siblings('a').removeClass('sec2-tip-selected');
        var index = $(this).index();
        var ele = $('.sec2-pic>li').eq(index);
        if (navigator.userAgent.indexOf('MSIE') >= 0) {
            ele.css('display', 'block').siblings().css('display', 'none');
        }
        $('.section2-con').children('.sec2-word').eq(index).fadeIn(200).siblings('.sec2-word').fadeOut(200);
        $('.sec2-pic>li').removeAttr('class');

        ele.siblings().addClass('translate_third');
        ele.addClass('translate_first').css('z-index', '6');
        ele.prev().removeAttr('class').addClass('translate_fourth');
        ele.next().removeAttr('class').addClass('translate_second');

        if (index == 0) {
            ele.parent().children('li:last').removeAttr('class').addClass('translate_fourth');
        } else if (index == 3) {
            ele.parent().children('li:first').removeAttr('class').addClass('translate_second');
        }
    })

    /*
     * 第四屏图片切换功能
     * author: zenggx
     */
    $('.img-select').hover(function() {
        $(this).addClass('img-selected').siblings('.img-select').removeClass('img-selected');
        $('.img-tab').eq($(this).index()).addClass('img-show').siblings('.img-tab').removeClass('img-show');
    })

    /*
     * 首屏轮播
     * author: zenggx
     */
    slide('#imgWrap', '#imgs', '.img-slide');

    function slide(slideimgWrap, imgview, imgslide) {
        var len = $(imgslide).length,
            imgWidth = $(imgslide).width(),
            timerL = null,
            timerAuto = null,
            nowNum = 0;

        function autoMove() {
            nowNum++;
            if (nowNum == len) {
                nowNum = 0;
            };
            timerL = setInterval(move, 20);
        }
        timerAuto = setInterval(autoMove, 3000);

        function move() {
            if (timerL) {
                clearTimeout(timerL);
            }
            clearInterval(timerAuto);
            $(imgview).stop().animate({
                'scrollLeft': imgWidth * nowNum
            }, 300)
            timerAuto = setInterval(autoMove, 3000);
        }
    }

    /*
     * 返回顶部
     * element-返回顶部的标签,criticalValue-  表示显示/隐藏返回顶部功能的临界值,time-返回顶部耗用的时间
     * author: zenggx
     */
    function returnTop(element, criticalValue, time) {
        var booleans = 1;
        $(window).scroll(function() {
            if ($(document).scrollTop() >= criticalValue) { // 当可滚动距离超过一定范围的时候
                $(element).stop(true, true).fadeIn();
                if (booleans == 1) {
                    $(element).click(function() {
                        booleans = 0;
                        $('html,body').animate({
                            'scrollTop': '0'
                        }, time, function() {
                            $('html,body').stop(true, true);
                            booleans = 1;
                        })
                    })
                };
            } else {
                $(element).stop(true, true).fadeOut();
            };
        })
    }
    returnTop('#goTop', 600, 300);

    // 二维码
    $('.fixscan').hover(function() {
        $('.fixscan-scale').css('display', 'block');
    }, function() {
        $('.fixscan-scale').css('display', 'none');
    })
    </script>
