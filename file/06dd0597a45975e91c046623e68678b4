				$("#favorite").val('收藏（' + data.num + '）');
			} else if (data.status == 2) {
				alert('您已收藏过该内容了！');
			} else {
				alert('请登录');
			}
		});
	}

	function vote(id) {
		$.getJSON(
				'http://www.cb.com.cn/index.php?m=mood&c=index&a=post&id=27-1153668-1&k='
						+ id + '&' + Math.random() + '&callback=?', function(
						data) {
					if (data.status == 1) {
						if (data.id == 1) {
							$('#mood1').val('顶（' + data.num + '）');
						} else if (data.id == 2) {
							$('#mood2').val('踩（' + data.num + '）');
						}
					} else {
						alert(data.data);
					}
				});
	}

	
