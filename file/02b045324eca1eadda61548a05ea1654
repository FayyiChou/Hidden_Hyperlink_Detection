            arry[1] = ["pc","pad","/smb","aio.","workstation","notebook"];
            arry[2] = ["digital","dv","dc","gps","mid","/ebook"];
            arry[3] = ["chuwei","tv.","air","hdplayer","dh","icebox","washer","ac","projector","life"];
            arry[4] = ["diy","cpu","/mb.","lcd.","speaker","power","input","vga.","storage"];
            arry[5] = ["oa","server","dev","net","cloud","soft","enterprise","cio"];
            arry[6] = ["comic","game.","wcg","gameonline","/shouyou"];
            arry[7] = ["wearable"];
            for(var i=0; i<arry.length; i++){
                for(var j=0; j<arry[i].length; j++){
                    var index1 = url.indexOf(arry[i][j]);
                    if(index1>0 && index1<25){
                        var str = arry[i][0];
                        navLi[i].className = str+" show";
                        navLi[i].firstChild.className = "show";
                    }
                }
            }
        </script>
<div class="ch_nav">
