})(); */
var _hmt = _hmt || [];
(function() {
  var hm = document.createElement("script");
  hm.src = "//hm.baidu.com/hm.js?731d207dd3fb613008738a2a7521ece7";
  var s = document.getElementsByTagName("script")[0]; 
  s.parentNode.insertBefore(hm, s);
})();

function toOnlineService(obj){
	$.ajax({
		url: "/onlineService",
		method:"POST",
		dataType:"json",
		async:false,
		cache: false,
		success: function(data, textStatus, jqXHR){
           var url1 =data.result;
           obj.href=""+url1+"";
		}
	})
}
</script>

