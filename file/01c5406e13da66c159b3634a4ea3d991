<script>

var top = document.getElementById('chapter');

$('.mylink').click(function (e) {
    e.preventDefault(); //阻止默认动作：跳转到指定的锚点
    $('body').animate({
//        scrollTop: ' 0px'
        scrollTop: $('#chapter').offset().top
    }, 500);
});

