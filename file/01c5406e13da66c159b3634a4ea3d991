<script>
   /*  var mySwiper = new Swiper('.swiper-container', {
        direction: 'horizontal',
        autoplay: 5000,//自动切换时间
        loop: true,//循环
        pagination: '.swiper-pagination',
        paginationClickable: true
    }); */
    $(function(){
    	$("#pageTitle_p").text("产品中心");
	    jQuery.ajax({
	    	url: "/web/getRollPic",
	    	type:"POST",
	    	dataType:"json",
	    	data:{"picPosition":"productIndex"},
	    	async:false,
	    	success: function(data, textStatus, jqXHR){
	    		var rollPicList = data.rollPicList;
	    		for(var i = 0;i < rollPicList.length;i++){
	    			var  imgUrl = rollPicList[i].picUrl;
	    			var target = "_blank"; 
	    			if(imgUrl == null || typeof(imgUrl) == "undefined" || imgUrl==""||imgUrl=="#"){
	    				imgUrl = "javascript:void(0);";
	    				target = "";
	    			}else{
	    				imgUrl = imgUrl;
	    				target = "_blank";
	    			}
	
	    			var img_htmlStr = 
	    				" <div  class='swiper-slide'> " +
	                    "<a href='"+imgUrl+"' target='"+target+"'><img  style='width: 100%' class='img-responsive' class='img-responsive' src='/GetImage?bt=rollPicImage&fn="+rollPicList[i].picName+"' title='"+rollPicList[i].picTitle+"'/></a>" +
	                    "</div>";
	    			$(".swiper-wrapper").append(img_htmlStr);
	    		}
	    		//初使化滚动图片的方法
	    		var mySwiper = new Swiper('.swiper-container', {
	    		    direction: 'horizontal',
	    		    autoplay: 5000,//自动切换时间
	    		    loop: true,//循环
	    		    pagination: '.swiper-pagination',
	    		    paginationClickable: true,
	    		    autoplayDisableOnInteraction : false
	    		});
	    	}
	    })
    })
	
	function getProductListByColumn(columnId){
    	jQuery.ajax({
	    	url: "/product/getProductListByColumn",
	    	type:"POST",
	    	dataType:"json",
	    	data:{"columnId":columnId},
	    	async:false,
	    	success: function(data, textStatus, jqXHR){
	    		$("#accident_panel_"+columnId).html("");
	    		if(data.length>0){
		    		for(var i = 0;i < data.length;i++){
		    			var lihtml="<a href='/product/toMProductDetail/"+data[i].productId+"'>";
						lihtml+="<div style='position: relative;margin-top:2%;border-bottom:1px solid #d7d3d0>";
						lihtml+="<div class='product01'>";
						lihtml+="<img style='width: 100%;height:auto;' src='/styles/m/img/product_center/wap_cpzx_fl_1_1_L.png'>";
						lihtml+="</div>";
						lihtml+="<p class='product_p2' style='display: block;margin: 5% 10% 10% 10%;font-size: 1.85rem'>";
						lihtml+=data[i].productName;
						lihtml+="</p>";
						lihtml+="<div class='c1'></div>";
						//lihtml+="<span class='product_span4' style='right: 3%;bottom:9%;position: absolute;display:inline-block;'>￥"+data[i].price+"元起</span>";
						lihtml+="</div>";
						lihtml+="</a>";
						$("#accident_panel_"+columnId).append(lihtml);
					}
	    		}else{
	    			$("#accident_panel_"+columnId).html("<span class='center_span'>暂无产品</span>");
		    	}
	    	}
	    })
    }

	function getProductListByInsuranceType(insuranceType){
		jQuery.ajax({
	    	url: "/product/getProductListByInsuranceType",
	    	type:"POST",
	    	dataType:"json",
	    	data:{"insuranceType":insuranceType},
	    	async:false,
	    	success: function(data, textStatus, jqXHR){
	    		$("#insuranceType_panel_"+insuranceType).html("");
