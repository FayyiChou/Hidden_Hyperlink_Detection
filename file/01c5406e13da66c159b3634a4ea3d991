<script>
	function showInfo(info,ms){
		 $("#fluid_id").show(1000);
		 $("#fluid_value").text(info);
		 if(typeof(ms) == "undefined" || ms == null || ms == ""){
			ms = 3000;
		 }
		 setTimeout(hideInfo,ms);
	}
	function hideInfo(){
		$("#fluid_id").hide(1000);
		$("#fluid_value").text("");
	}
</script>
