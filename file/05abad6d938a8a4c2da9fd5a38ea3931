<script>
  var mySwiper = new Swiper('.swiper-container',{
    pagination: '.pagination',
   autoplay:3000,
    loop:true,
    grabCursor: true,
    paginationClickable: true
  })
  jQuery('.arrow-left').on('click', function(e){
    e.preventDefault()
    mySwiper.swipePrev()
  })
  jQuery('.arrow-right').on('click', function(e){
    e.preventDefault()
    mySwiper.swipeNext()
  })
