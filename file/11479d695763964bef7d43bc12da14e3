<script type="text/javascript" src="/resources/scripts/jquery/jquery-1.11.2.min.js"></script>
<script type="text/javascript" src="/resources/scripts/swfobject.js"></script>

<script>
    jQuery(document).ready(function () {
        var attributes = {};
        var params = {
            wmode: "transparent"
        };
        //地市地图
        swfobject.embedSWF("/resources/images/map.swf", "swf_map", "208", "160", "9.0.0", "/resources/scripts/expressInstall.swf", {}, params, attributes);
        $('#appSelect').change(function () {
            var p = $(this).children('option:selected').val();
            if (p.length > 2)
                window.location.href = p;
        });

        var selectx=document.getElementById("year");
        for (i=0;i<selectx.length;i++) {
            if(selectx.options[i].value == "") {
                selectx.options[i].selected = true;
                break;
            }
        }

        $.get("/hebi/online?sessionId=2d49591280c6464097d20c64cf133151",function(data){
            $("#online").html(data);
        });
        //屏蔽IE执行误差
        window.onerror = function () {
            return true
        };
    });
</script>


