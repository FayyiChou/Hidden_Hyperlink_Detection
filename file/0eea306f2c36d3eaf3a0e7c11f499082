var speed=20
dmt2.innerHTML=dmt1.innerHTML
function leftMarquee(){
if(dmt2.offsetWidth-dmt.scrollLeft<=0)
dmt.scrollLeft-=dmt1.offsetWidth
else{
dmt.scrollLeft++
}
}
var LeftMar=setInterval(leftMarquee,speed)
dmt.onmouseover=function() {clearInterval(LeftMar)}
dmt.onmouseout=function() {LeftMar=setInterval(leftMarquee,speed)}
