                    cache: false,
                    dataType: 'JSON',
                    data: {
                        ac: 'getsign'
                    },
                    success: function (result) {
                        //判断是否是可签到用户组
                        if (result.gstatus == 0) {
                            setcookie('sign_close', 1);
                            jQuery('#qd-sign, #sign-ad').css('display', 'none');
                            tice.css('backgroundPosition', '-5px -1114px').html('没有签到权限');
                        } else {
                            //0 未签到  1已签到
                            if (getcookie('sign_close') != 1 && signin != 0) {
                                jQuery('#sign-ad,#nosign').show();
                                jQuery(".unlogin-sign").hide();
                            }
                            if (result.daystatus == 0) {
                                S_status = 1;
                                tice.html('立刻签到');
                                if (result.times) {
                                    G_time = result.times;
                                    jQuery('.sign-body li:lt(' + result.times + ')').addClass('qd-icon');
                                    day.html(result.deldays);
                                    G_deldays = result.deldays;
                                } else {
                                    day.html(30);
                                }
                            } else {
                                tice.html('今日已签').css('backgroundPosition', '-5px -1114px');
                                jQuery('.sign-body li:lt(' + result.times + ')').addClass('qd-icon');
                                b.html(_html);
                                result.type == 'A' ? c.html("您已获得" + result.snum + "个金币") : c.html("您已获得" + result.snum + "天VIP");
                            }
                        }
                    }
                });
            }
