<script>
$(window).load(function(){
    var h=Math.floor($('.pickup__slide .slide__body').height()/2);
/*
    $('.pickup__slide .slide__arrow__prev').css({top:h});
    $('.pickup__slide .slide__arrow__next').css({top:h});
*/
});



$(window).load(function(){
/*
    $('.pickup__slide__item').masonry({
        itemSelector:'.xpress__item',
        isAnimated:false,
        gutter:20,
        columnWidth:235
    });
*/
});



$(function(){
    var hover=[function(){
        var o=$(this).find('.more-overlay');
        o.stop(true,true).fadeIn(500);
        $(this).find('.xpress__item__title,.xpress__item__cat').stop(true,true).animate({opacity:0},500);
//         o.prevAll().fadeTo(500,0.5);
    },function(){
        var o=$(this).find('.more-overlay');
        o.stop(true,true).fadeOut(500);
        $(this).find('.xpress__item__title,.xpress__item__cat').stop(true,true).animate({opacity:1},500);
//         o.prevAll().fadeTo(500,1);
    }];

    if(!isMobile()) $('.xpress__item .xpress__item__link').off('hover').hover.apply($('.xpress__item .xpress__item__link'),hover);
});



$(function(){
    $('.showcase__slide__item').hover(function(){
        var o=$(this).find('.more-overlay');
        o.stop(true,true).fadeIn(500);
        $(this).find('.slide__item__title').stop(true,true).animate({opacity:0},500);
//         o.prevAll().fadeTo(500,0.5);
    },function(){
        var o=$(this).find('.more-overlay');
        o.stop(true,true).fadeOut(500,function(){});
        $(this).find('.slide__item__title').stop(true,true).animate({opacity:1},500);
//         o.prevAll().fadeTo(500,1);
    });
});

var o={
    slide:'.slide__item',
    infinite:true,
    slidesToShow:3,
    arrows:false,
    dots:false,
    variableWidth:true,
    responsive: [
        {
            breakpoint:680,
            settings: {
                slidesToShow:1,
                slidesToScroll:1,
                infinite:true,
                dots:false
            }
        }
    ]
};

$(window).bind('load resize',function(){
    var o=$('.cover__slide .slide__item img');
    if($(window).width()>680){
        o.removeAttr('style');
        o.css({visibility:'visible'});
        return;
    }
    o.width($(window).width());
});

$(window).load(function(){
    $('.cover__slide .slide__item img').css({visibility:'visible'});
    var t=$('<div class="slide__indicator"></div>').appendTo('.cover');
    $('.cover__slide__item').not('.slick-cloned').each(function(){
        t.append('<a href="#">&nbsp;</a>');
    });
    t=t.find('a');
    t.eq(0).addClass('on');

    var s=$('.cover__slide .slide__body').slick($.extend({},o,{
        slidesToShow:1,
        autoplay:true,
        centerMode:true,
        speed:1000,
        autoplaySpeed:10000
    }));

    $('.cover__slide__arrow__prev').click(function(){
        s.slick('slickPrev');
        return false;
    });
    $('.cover__slide__arrow__next').click(function(){
        s.slick('slickNext');
        return false;
    });

    t.click(function(){
        s.slick('slickGoTo',t.index(this));
        return false;
    });
    s.on('beforeChange',function(e,f,c,n){
        t.removeClass('on').eq(n).addClass('on');
    });
});


$(function(){
    $('.cover__slide').hover(function(){
        $('.cover__slide__arrow__prev span,.cover__slide__arrow__next span').animate({opacity:1},300);
    },function(){
        $('.cover__slide__arrow__prev span,.cover__slide__arrow__next span').animate({opacity:0},300);
    });
});
$(window).on('load resize',function(){
    $('.cover__slide').height('auto');
});



$(window).load(function(){
    var s=$('.pickup__slide .slide__body').slick($.extend({},o,{
        responsive:[
            {
                breakpoint:640,
                settings: {
                    centerMode:true
                }
            }
        ]
    }));
    $('.pickup__arrow__prev').click(function(){
        s.slick('slickPrev');
        return false;
    });
    $('.pickup__arrow__next').click(function(){
        s.slick('slickNext');
        return false;
    });
    var c=function(){
        if($(window).width()<640) {
            $('.pickup__slide .slide__body').css({marginLeft:0});
            return;
        }
        $('.pickup__slide .slide__body').css({marginLeft:-((1020-($(window).width()%1020))/2)});
    };
    $(window).resize(c);
    c();
});

$(function(){
    var s=$('.showcase__slide').css({position:'relative'});
    var d=20000;
    var loop=function(t){
        if(!t) t=d;
        s.animate({left:-250},t,'linear',function(){
            s.find('.slide__item').eq(0).appendTo(s);
            s.css({left:0});
            loop();
        });
    };
    var p;
    s.hover(function(){
        s.queue([]);
        s.stop();
        p=s.position().left;
    },function(){
        loop(Math.floor(d-((p/-250)*d)));
    })
    var m=s.find('.slide__item');
    s.width(m.length*m.width());
    loop();
});
</script>



