	switchMode:"click",
	
	clickPanel:".navigate",
	
	clickTag:"li",
	
	addStyle:"current",
	
	showContent:".content01"
	});
	
$('.r_zhuti01').slide();


$('.pindaozhishi').hover(function(){
	$(this).children('.searchleftul').show();
	},function(){
		$(this).children('.searchleftul').hide();
		})
$('.searchleftul').children('li').children('a').click(function(){
	 $('.pindaozhishi').find('span').children('a').html($(this).html());
	 return false;
	}).parent('.searchleftul').hide();
	
//搜索的js效果	
$('.pindaozhishi').hover(function(){
	$(this).children('.searchleftul').show();
	},function(){
		$(this).children('.searchleftul').hide();
		})
$('.searchleftul').children('li').children('a').click(function(){
	 $('.pindaozhishi').find('span').children('a').html($(this).html());
	 $('.searchleftul').hide()
	 $("#channel").val($(this).attr("id"));
});
//鼠标放到视频效果
$('.picInderA').hover(function(){
	$(this).children('.coverDiv').show();
	},function(){
		$(this).children('.coverDiv').hide();
		})
});

