<script type="text/javascript" src="http://static.chaoxing.com/www/foot/wzstudy_foot.js" charset="utf-8"></script>
<script type="text/javascript" src="/template/wzstudy/js/bannerFadeIn.js"></script>
<script type="text/javascript" src="/template/wzstudy/js/register.js"></script>
