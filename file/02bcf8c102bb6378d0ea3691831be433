<script type="text/javascript">
	$('.reosd').click(function(){
		$('.logorespnd').hide();
	});
//首页左侧分类
function index_left_class(cname,cname1,cname2){
	$.ajax({
	    url:"http://video.wzstudy.cn/getWzcdClassifybytype.shtml?callback=?",
	    dataType: 'jsonp',
	    type: 'post', 
	    data : {},
	    success:function(data){
		var datas=data;
		
	 	if(datas.length > 0){
				var dataHtml='';
				var dataHtml1='';
				var dataHtml2='';
				if(datas[0].listByPersion!=null){
					for(var key in datas[0].listByPersion){
						dataHtml+='<a href="'+key+'">'+datas[0].listByPersion[key]+'</a>'
					}
					$("#"+cname).html(dataHtml);
				}else{
					$("#"+cname).html('<li><span>没有数据...</span></li>');
				}
				if(datas[0].listByKnowlageSys!=null){
					for(var key in datas[0].listByKnowlageSys){
						dataHtml1+='<a href="'+key+'">'+datas[0].listByKnowlageSys[key]+'</a>'
					}
					$("#"+cname1).html(dataHtml1);
				}else{
	 				$("#"+cname1).html('<li><span>没有数据...</span></li>');
				}
				if(datas[0].listAnimation!=null){
					for(var key in datas[0].listAnimation){
						dataHtml2+='<a href="'+key+'">'+datas[0].listAnimation[key]+'</a>'
					}
					$("#"+cname2).html(dataHtml2);
				}else{
	 				$("#"+cname2).html('<li><span>没有数据...</span></li>');
				}
	 		}else{
	 			$("#"+cname).html('<li><span>没有数据...</span></li>');
	 			$("#"+cname1).html('<li><span>没有数据...</span></li>');
	 			$("#"+cname2).html('<li><span>没有数据...</span></li>');	
	 		}
		}
	}); 
}
//首页最新资讯新闻
function index_recommendNew(name,count,nid){
	$.ajax({
	    url:"/ajaxnewslist.js",
	    dataType: 'json',
	    type: 'post', 
	    data : {
	        "pageCount" : count,
	        "name":name
	         },
	     success:function(datas){
	        if(datas.length > 0){
				var dataHtml='';
				for(var i=0; i<datas.length; i++){
					dataHtml +='<li><span>'+datas[i].createDate+'</span><b></b><a title="'+datas[i].title+'" href="http://www.wzstudy.cn/newArticle_'+datas[i].id+'.html">'+stringlength(datas[i].title,17)+'</a></li>';
				}
				$("#"+nid).html(dataHtml);
				//$("#more"+nid).attr("href","http://wzstudy.chaoxing.com/newsList_"+datas[0].cid+".html");
				return false;
			}
		 $("#"+nid).html('<li><span>没有数据...</span></li>');	
		}
	}); 
}
//最新视频(手动推荐)
function index_newVideos(count,title,cname){
	$.ajax({
	    url:"http://video.wzstudy.cn/getInfoByTag.jsonp?callback=?",
	    dataType: 'jsonp',
	    type: 'post', 
	    data : {
	        "count":count,
	        "start":1,
	        "tagName":title
	         },
	    success:function(data){
	    var datas=data.objModules;
		
	 	if(datas.length > 0){
				var dataHtml='';
				var dataHtml1='';
				var schoolname='';
				var introduction='';
				var teachername='';
				var imgpath='';
				if(datas[0].siteSeries[0].series.is2b==1){imgpath=datas[0].siteSeries[0].series.videoImg;}
				else if(datas[0].siteSeries[0].series.is2b==0){imgpath='http://video.image.ssreader.cn/'+datas[0].siteSeries[0].series.imgPath2;}
				if(datas[0].siteSeries[0].schools.length!=0){schoolname=datas[0].siteSeries[0].schools[0].name;}
				if(datas[0].siteSeries[0].teachers.length!=0){teachername='主讲人：'+datas[0].siteSeries[0].teachers[0].teacherName;}
				if(datas[0].siteSeries[0].series.introduction!=''||datas[0].siteSeries[0].series.introduction!=null){introduction=stringlength(datas[0].siteSeries[0].series.introduction,35);}
				dataHtml+='<div class="picDiv01"><a href="http://video.wzstudy.cn/serie_'+datas[0].siteSeries[0].series.seriesNo+'.shtml">';
                dataHtml+='<img width="291" height="207" title="'+datas[0].siteSeries[0].series.seriesName+'" onerror="src=\'/template/wzstudy/images/no_series.jpg\'" src="'+imgpath+'" alt="'+datas[0].siteSeries[0].series.seriesName+'"/></a></div>';
                dataHtml+='<h3><span class="colo1" title="'+schoolname+'">'+stringlength(schoolname,4)+'</span><span title="'+teachername+'">'+teachername+'</span><a class="colo" href="http://video.wzstudy.cn/serie_'+datas[0].siteSeries[0].series.seriesNo+'.shtml" title="'+datas[0].siteSeries[0].series.seriesName+'"><b></b>'+stringlength(datas[0].siteSeries[0].series.seriesName,6)+'</a></h3>';
                dataHtml+='<p>'+introduction+'&nbsp;&nbsp;<a href="http://video.wzstudy.cn/serie_'+datas[0].siteSeries[0].series.seriesNo+'.shtml" target="_blank" >更多</a></p>';
				for(i=1;i<datas.length;i++){
					if(datas[i].siteSeries[0].series.is2b==1){imgpath=datas[i].siteSeries[0].series.videoImg;}
					else if(datas[i].siteSeries[0].series.is2b==0){imgpath='http://video.image.ssreader.cn/'+datas[i].siteSeries[0].series.imgPath2;}
					if(datas[i].siteSeries[0].schools.length!=0){schoolname=datas[i].siteSeries[0].schools[0].name;}
					if(datas[i].siteSeries[0].teachers.length!=0){teachername=datas[i].siteSeries[0].teachers[0].teacherName;}
					dataHtml1+='<li><a class="picInderA" href="http://video.wzstudy.cn/serie_'+datas[i].siteSeries[0].series.seriesNo+'.shtml"><img width="137" height="93" onerror="src=\'/template/wzstudy/images/no_series.jpg\'" src="'+imgpath+'" title="'+datas[i].siteSeries[0].series.seriesName+'" alt="'+datas[i].siteSeries[0].series.seriesName+'" /><div class="coverDiv"></div></a>';
                    dataHtml1+='<h3><a class="colo" href="http://video.wzstudy.cn/serie_'+datas[i].siteSeries[0].series.seriesNo+'.shtml" title="'+datas[i].siteSeries[0].series.seriesName+'">'+stringlength(datas[i].siteSeries[0].series.seriesName,6)+'</a></h3>';
                    dataHtml1+='<p><span title="'+teachername+'">'+teachername+'</span>&nbsp;<span title="'+schoolname+'">'+stringlength(schoolname,5)+'</span></p></li>';
				}
				$("#left_"+cname).html(dataHtml);
				$("#right_"+cname).html(dataHtml1);
	 		}else{
	 			$("#left_"+cname).html('<li><span>没有数据...</span></li>');
	 			$("#right_"+cname).html('<li><span>没有数据...</span></li>');	
	 		}
		}
	}); 
}
