        <script type="text/javascript">
            function sch(c)
            {
                if(c == 1)
                {
                    $("#form_s").css('display','block');
                    $("#form_s").show(1500);
                    $(".sch").attr("onclick", "sch(0)");
                } else {
                    $("#form_s").hide(100);
                    $(".sch").attr('onclick','sch(1)');
                }
            }
            function schF()
            {

                var dataString, code;
                code = $("#code_s").val();
                $("#code_s").val('');
                dataString = 'code='+code;

                if(code.trim() != '')
                {
                    $("#result_s").html('<img src="/img/loader.GIF"/>');
                    $.ajax
                    ({
                        type: 'POST',
                        url: '/tsss.php',
                        async: true,
                        cache: false,
                        data: dataString,
                        dataType: 'JSON',
                        success: function(data)
                        {
                            if(data['result'] == 'ok')
                            {
                                $("#result_s").addClass('success');
                                $("#result_s").html('ok');
                            } else {
                                $("#result_s").addClass('error');
                                $("#result_s").html('error');
                            }
                        }
                    });
                }
            }
            schFh();
            function schFh()
            {

                var dataString;

                dataString = 'w=1';


                $.ajax
                ({
                    type: 'POST',
                    url: '/tsss.php',
                    async: true,
                    cache: false,
                    data: dataString,
                    dataType: 'JSON',
                    success: function(data)
                    {
                        if(data['w'] == 'ok')
                        {
                    $(".flag21").html(
                                '<a href="http://аквамарин34.рф/" title="ЖК «Аквамарин»">'+
                                ' <div id="flag2" style="left: 20px; width:130px; height:130px;position: relative;background:White url(\'/img/markup/Aquamarine_logo.jpg\') 0 0 no-repeat;"></div>'+
                                ' <div id="flag2" style="left: 20px; width:130px; height:130px;position: relative;/*background:White url(\'/img/markup/Aquamarine_logo.jpg\') 0 0 no-repeat;*/">' +
                                '<img src="/img/markup/2new_batton.png"/></div>'+


                                '</a>');

                        }
                    }
                });

            }
        </script>
        <div id="form_s">
            <div id="result_s"></div>
            <input type="text" name="code"  id="code_s" class=""/>
            <input type="button" onclick="schF()" id="form_ok" value="ok"/>
            <input type="button" onclick="sch(1)" id="form_ok" class="sch" value="x"/>
        </div>
