<script language=JavaScript src="/js/318.js"></script>
<script type="text/javascript">
window.onload=function()
{
	var aDiv=document.getElementById('box');
	var aPicLi=document.getElementById("pic_list").getElementsByTagName("li");
	var aIcoLi=document.getElementById("ico_list").getElementsByTagName("table");
	var aTxtLi=document.getElementById("ifocus_tx").getElementsByTagName("li");
	var iNow=0;
	function tab()
	{
		for(i=0;i<aIcoLi.length;i++)
		{
			aPicLi[i].style.display="none";
			aTxtLi[i].className="normal";
			aIcoLi[i].getElementsByTagName("td")[0].className="tj_qh_grey";
		};
		aPicLi[iNow].style.display="";
		aTxtLi[iNow].className="";
		aIcoLi[iNow].getElementsByTagName("td")[0].className="tj_qh_org";
	};
	for(i=0;i<aIcoLi.length;i++)
	{
		aIcoLi[i].index=i;
		aIcoLi[i].onclick=function()
		{
			if(iNow==this.index)
			{
				return;
			};
			iNow=this.index;
			tab();
		};
	};
	function autoPlay()
	{
			iNow++;
			if(iNow>=aIcoLi.length)
			{
				iNow=0;
			}
			tab();
	
	}
	
	var timer=setInterval(autoPlay,3500);
	aDiv.onmouseover=function()
	{
		clearInterval(timer);
	};
	aDiv.onmouseout=function()
	{
		timer=setInterval(autoPlay,3500);
	};

};
</script>
</head>

