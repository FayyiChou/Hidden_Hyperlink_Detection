    <script type="text/javascript">
        $(document).ready(function () {
            //登录状态
            $(function () {
                if ('False' == 'True') {
                    $(".login-before").hide();
                    $(".login-after").show();
                } else {
                    $(".login-before").show();
                    $(".login-after").hide();
                }
            })
            //顶部滚动效果
            $(window).scroll(function () {
                var topBk = $(window).scrollTop();
                if (topBk < 330) {
                    $(".header").css({ backgroundPositionY: topBk - (0.15 * topBk) });
                } else {
                    $(".header").css({ backgroundPositionY: 0 });
                };
                /*if (topBk > 320) {
                    $(".nav-bar").css({ position: "fixed", top: 0, left: 0, borderRadius: 0, zIndex: 999, width: "100%" });
                    $(".nav-list-box").css({ width: "10%" });
                } else {
                    $(".nav-bar").css({ position: "absolute", bottom: 0, left: 0, top: "auto", borderRadius: "4px 4px 0 0", width: 980 });
                    $(".nav-list-box").css({ width: "10%" });
                }*/
            });

            //导航栏展开
            $(".nav-list-box").mouseenter(function () {
                $(this).find(".nav-title").css({ backgroundColor: "#fafafa" });
                $(this).find(".nav-list").slideDown(100);
            }).mouseleave(function () {
                $(this).find(".nav-title").css({ backgroundColor: "transparent" });
                $(this).find(".nav-list").slideUp(100);
            });
            $(".nav-list li:last-child").css({ border: "none" });

            $(".nav-list li").mouseenter(function () {
                $(this).css({ backgroundColor: "#fafafa" });
                $(this).find("p").css({ color: "#339933" });
            }).mouseleave(function () {
                $(this).css({ backgroundColor: "transparent" });
                $(this).find("p").css({ color: "#eee" });
            });
        })
    </script>
