    <script type="text/javascript">
        $(document).ready(function () {
            //新闻图片切换
            $(".news-picture a").hide().first().show();
            var ss = 0;
            function picSwitch() {
                if (ss < 4) {
                    ss = ss + 1;
                    $(".news-picture a").hide().eq(ss).fadeIn(900);
                    $(".pic-pages li").removeClass("current").eq(ss).addClass("current");
                } else {
                    ss = 0;
                    $(".news-picture a").hide().first().fadeIn(900);
                    $(".pic-pages li").removeClass("current").eq(ss).addClass("current");
                }
            };
            setInterval(picSwitch, 4000);
            //手动切换
            $(".pic-pages li").bind("click", function () {
                ss = $(this).index();
                $(".news-picture a").hide().eq(ss).fadeIn(900);
                $(".pic-pages li").removeClass("current").eq(ss).addClass("current");
            });
            //飘窗
            var x = 400, y = 100;
            var xin = true, yin = true;
            var step = 1;
            var delay = 10;
            var itl = null;
            var obj = document.getElementById("temp-img");
            function float() {
                var L = T = 0;
                var R = document.documentElement.clientWidth - obj.offsetWidth;
                var B = document.documentElement.clientHeight - obj.offsetHeight;
                obj.style.left = x + document.body.scrollLeft + "px";
                obj.style.top = y + document.body.scrollTop + "px";
                x = x + step * (xin ? 1 : -1);
                if (x < L) { xin = true; x = L };
                if (x > R) { xin = false; x = R };
                y = y + step * (yin ? 1 : -1);
                if (y < T) { yin = true; y = T };
                if (y > B) { yin = false; y = B };
            }
            itl = setInterval(float, delay)
            obj.onmouseover = function () { clearInterval(itl) }
            obj.onmouseout = function () { itl = setInterval(float, delay) }

            $("#temp-img span").bind("click", function () {
                $("#temp-img").remove();
            });
        });
        
    </script>
</head>
