<script language="JavaScript" type="text/javascript">
// JavaScript Document

var speed=20;
document.getElementById("demo7").innerHTML=document.getElementById("demo6").innerHTML
function Marquee(){
if(document.getElementById("demo7").offsetWidth-document.getElementById("demo5").scrollLeft<=0)
document.getElementById("demo5").scrollLeft-=document.getElementById("demo6").offsetWidth
else{
document.getElementById("demo5").scrollLeft++;
}
}
var MyMar=setInterval(Marquee,speed)
document.getElementById("demo5").onmouseover=function() {clearInterval(MyMar)}
document.getElementById("demo5").onmouseout=function() {MyMar=setInterval(Marquee,speed)}
</script>          
