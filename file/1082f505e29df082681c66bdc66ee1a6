<script language="JavaScript" type="text/javascript">
// JavaScript Document

var speed=20;
document.getElementById("demo17").innerHTML=document.getElementById("demo16").innerHTML
function Marquee(){
if(document.getElementById("demo17").offsetWidth-document.getElementById("demo15").scrollLeft<=0)
document.getElementById("demo15").scrollLeft-=document.getElementById("demo16").offsetWidth
else{
document.getElementById("demo15").scrollLeft++;
}
}
var MyMar=setInterval(Marquee,speed)
document.getElementById("demo15").onmouseover=function() {clearInterval(MyMar)}
document.getElementById("demo15").onmouseout=function() {MyMar=setInterval(Marquee,speed)}
</script>  
     </div>
