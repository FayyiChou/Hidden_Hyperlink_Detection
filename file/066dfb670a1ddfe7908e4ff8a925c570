    $('.head_nav li').each(function(){
        $(this).hover(function(){
            $(this).addClass('on').siblings().removeClass('on');
        });
    });
    $(".nav ul li").live("mouseover",function(){
        $(this).children("dl").show();
    })
    $(".nav ul li").live("mouseout",function(){
        $(this).children("dl").hide();
    })
</script>
<script type="text/javascript">
    $('#closeul').each(function(){
        $(this).click(function(){
            $('#head_nav').css('width',"613px");
        });
    });
    var clickNumber = 2;
    $('#closeul').click(function(){
        if(clickNumber %2==0){
            $('#closeul').each(function(){
                $(this).click(function(){
                    $('#head_nav').css('width',"368px");
                });
            });
        }
        else{
            $('#closeul').each(function(){
                $(this).click(function(){
                    $('#head_nav').css('width',"613px");
                });
            });
        }
        clickNumber ++;
    })
</script>
