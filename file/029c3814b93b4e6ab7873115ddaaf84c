    _ub.city = $("#cityName").val(); //所在城市（中文）
    _ub.biz = 'j'; //固定
    _ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
    _ub.collect(0, { 'vwg.page': 'jrcalculatorzxdk' });
</script>
