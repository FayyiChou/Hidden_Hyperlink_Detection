        function resizeImg(obj){
            var height = obj.height,
                width = obj.width,
                sw=240,
                new_w=width,
                new_h=height;
            var img = new Image();
            img.src = obj.src;
            if(new_w > sw){
                obj.width = sw;
                new_h = parseInt(img.height*(sw/img.width));
                obj.height = new_h;
            }
        }
