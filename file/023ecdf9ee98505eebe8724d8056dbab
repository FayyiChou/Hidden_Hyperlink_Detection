	if(window.frames.length != parent.frames.length){
	$(window.parent.document).find("img").each(function(){
			if(null!=this.src && ''!=this.src && 'undefined'!=this.src){
				if(this.src.indexOf("xm_wsbs_10.png")>-1){
					$("#xm_p").css("display","block");
