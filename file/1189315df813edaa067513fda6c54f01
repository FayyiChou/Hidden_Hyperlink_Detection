    <script src="/Resource/Javascript/jquery.mx.lay_1.0.js?v=20160919"></script>
    <script type="text/javascript">
        if (top.location != this.location) {
            top.location.replace('/login.aspx');
        }
        var PageUI = {
            DomEle: { panel: $(".login-panel"), account: $("#txtAccount"), password: $("#txtPassword"), isAutoLogin: $("#chkAutoLogin"), isAccount: $("#chkAccount"), checkCode: $("#txtCheckCode") },

            Logined: function () {
                //location.href = "Index.aspx";
                if (PageUI.DomEle.account.val().length <= 0) {
                    MXNET_UI_LAY.Tips("请输入帐号", PageUI.DomEle.account)
                    PageUI.DomEle.account.focus();
                    return;
                }
                if (PageUI.DomEle.password.val().length <= 0) {
                    MXNET_UI_LAY.Tips("请输入密码", PageUI.DomEle.password)
                    PageUI.DomEle.password.focus();
                    return;
                }
                if (PageUI.DomEle.checkCode.val().length <= 0) {
                    MXNET_UI_LAY.Tips("请输入验证码", PageUI.DomEle.checkCode)
                    PageUI.DomEle.checkCode.focus();
                    return;
                }
                PageUI.LoginBtn(1);
                var mod = {
                    account: PageUI.DomEle.account.val(),
                    password: PageUI.DomEle.password.val(),
                    checkCode: PageUI.DomEle.checkCode.val(),
                    isAutoLogin: PageUI.DomEle.isAutoLogin[0].checked,
                    isAccount: PageUI.DomEle.isAccount[0].checked,
                    supplierGuid: "",
                    isPopupAgreement: true,
                    isJCCustomer: false,
                    jcUserGuid: ""
                }
                AjaxPost($.toJSON(mod), "/DataService/Login.asmx/SignIn", function (d) {
                    var json = d.d;
                    if (json.status) {
                        //判断是否弹出网络协议
                        if (json.msg == "OpenAgreement") {
                            MXNET_UI_LAY.NetworkProtocol(function () {
                                var mod2 = {
                                    account: PageUI.DomEle.account.val(),
                                    password: PageUI.DomEle.password.val(),
                                    checkCode: PageUI.DomEle.checkCode.val(),
                                    isAutoLogin: PageUI.DomEle.isAutoLogin[0].checked,
                                    supplierGuid: "",
                                    isPopupAgreement: false,
                                    isJCCustomer: false,
                                    jcUserGuid: ""
                                }
                                //更新门店网络协议点击
                                AjaxPost("", "/DataService/Login.asmx/UpdateIsClickProtocol", function (d) {
                                    var json = d.d;
                                    if (!json.status) {
                                        PageUI.LoginBtn(2);
                                        MXNET_UI_LAY.ShowMsg(json.msg, MXNET_UI_LAY.MsgType.MsgTypeErr);
                                        return;
                                    }
                                });
                                AjaxPost($.toJSON(mod2), "/DataService/Login.asmx/SignIn", function (d) {
                                    var json = d.d;
                                    if (json.status) {
                                        location.href = json.msg;
                                    } else {
                                        PageUI.LoginBtn(2);
                                        $("#imgCheckCode").attr("src", "/webcomm/verification_code.aspx?sn=" + Math.random());
                                        MXNET_UI_LAY.ShowMsg(json.msg, MXNET_UI_LAY.MsgType.MsgTypeErr);
                                    }
                                });
                            });
                        }
                        else {
                            location.href = json.msg;
                        }
                    } else {
                        PageUI.LoginBtn(2);
                        $("#imgCheckCode").attr("src", "/webcomm/verification_code.aspx?sn=" + Math.random());
                        MXNET_UI_LAY.ShowMsg(json.msg, MXNET_UI_LAY.MsgType.MsgTypeErr);
                    }
                });
            },
            LoginBtn: function (type) {
                if (type == "1") {
                    $(".login-button").unbind("click");
                    $(".login-button").attr("class", "login-buttonB");
                }
                else {
                    $(".login-buttonB").click(PageUI.Logined);
                    $(".login-buttonB").attr("class", "login-button");
                }
            },
            InitLayout: function () {

                var windowHeight = $(window).height() < 583 ? 583 : $(window).height();
                var wrapHeight = windowHeight - $(".header").outerHeight() - $(".footer").outerHeight();
                $(".body-wrap").css({ height: wrapHeight + "px" });
                $(".panel-box-shadow").css({ top: ((wrapHeight - $(".panel-box-shadow").outerHeight()) / 2) + "px" });
                $(".panel-box").css({ top: ((wrapHeight - $(".panel-box-shadow").outerHeight()) / 2) + "px" });
                $(".i8").css({ top: ((wrapHeight - $(".i8").outerHeight()) / 2) + "px" });
            },
            Init: function () {
                if (window.location.href.indexOf("www") > 0) {
                    location.href = "http://fxs.888ly.cn";
                }
                PageUI.CheckCodeRefresh();
                $(".login-button").click(PageUI.Logined);
                PageUI.DomEle.account.keydown(function (e) { if (e.keyCode == "13") { PageUI.Logined(); } });
                PageUI.DomEle.password.keydown(function (e) { if (e.keyCode == "13") { PageUI.Logined(); } });
                PageUI.DomEle.checkCode.keydown(function (e) { if (e.keyCode == "13") { PageUI.Logined(); } });
                PageUI.InitLayout();
                $(window).resize(function () { PageUI.InitLayout(); });
                //this.CheckBrowser();
                //用户名获得焦点
                PageUI.DomEle.account.focus();
            },
            CheckCodeRefresh: function () {
                //$("#imgCheckCode").each(function () { this.src = "/webcomm/verification_code.aspx?sn=" + Math.random(); });
                $("#imgCheckCode").click(function () { this.src = "/webcomm/verification_code.aspx?sn=" + Math.random(); });
            },
            CheckBrowser: function () {
                var userAgent = navigator.userAgent.toLowerCase();
                browser = {
                    version: (userAgent.match(/.+(?:rv|it|ra|ie|chrome)[\/: ]([\d.]+)/) || [0, '0'])[1],
                    safari: /webkit/.test(userAgent),
                    opera: /opera/.test(userAgent),
                    msie: /msie/.test(userAgent) && !/opera/.test(userAgent),
                    mozilla: /mozilla/.test(userAgent) && !/(compatible|webkit)/.test(userAgent),
                    chrome: /chrome/.test(userAgent)
                }
                //alert(userAgent);
                //alert(browser.version);
                if (!(browser.msie && parseInt(browser.version) > 8 || browser.mozilla && parseInt(browser.version) > 34 || browser.chrome && parseInt(browser.version) > 30)) {
                    this.SetAlert();
                }
            },
            SetAlert: function (strBrowse) {
                $('body').empty();
                var strPanel = '<div id="ie-alert-overlay"></div>';
                strPanel += '<div id="ie-alert-panel">';
                strPanel += '    <span>很抱歉，你的浏览器版本过低！</span>';
                strPanel += '    <p style="word-break:break-all;">为了得到更好的用户体验效果，我们建议你点击以下图标下载安装最新版本的Web浏览器。</p>';
                strPanel += '    <div class="browser">';
                strPanel += '        <ul>';
                strPanel += '          <li><a class="chrome" href="http://pan.baidu.com/s/1gdyBLJ1" target="_blank"></a></li>';
                strPanel += '          <li><a class="firefox" href="http://download.firefox.com.cn/releases/stub/official/zh-CN/Firefox-latest.exe" target="_blank"></a></li>';
                strPanel += '          <li><a class="ie9" href="http://windows.microsoft.com/zh-cn/internet-explorer/download-ie" target="_blank"></a></li>';
                //strPanel+='            <li><a class="safari" href="http://www.apple.com/safari/download/" target="_blank"></a></li>';
                //strPanel+='            <li><a class="opera" href="http://www.opera.com/download/" target="_blank"></a></li>';
                strPanel += '            <ul>';
                strPanel += '    </div>';
                strPanel += '</div>';
                $('body').append(strPanel);
            }
        };
        PageUI.Init();

        function popUp() {
            if (!$("#popupDiv").length) {
                /*弹出友情提醒*/
                var popupDiv = $("<div class='popup-div' id='popupDiv'></div>");
                /*img的box*/
                var imgBox = $("<div class='img-box'></div>");
                popupDiv.append(imgBox);
                /*关闭按钮*/
                imgBox.append("<div class='button popup-close' id='popupClose'></div>");
                /*不在提醒按钮*/
                imgBox.append("<div class='button no-longer-tip' id='popupNoLonger'></div>");
                /*append*/
                $("body").append(popupDiv);
                /*动画*/
                popupDiv.find(".img-box").animate({ "top": "0" }, 300);
                /*绑定事件*/
                $("body").on("click", "#popupClose", function () {
                    popupDiv.find(".img-box").animate({ "top": "-400px" }, 300);
                });
                $("body").on("click", "#popupNoLonger", function () {
                    popupDiv.find(".img-box").animate({ "top": "-400px" }, 300);
                    /*并且设置cookie值，不再出现*/
                    document.cookie += "canpop=false";
                });
            }
        }
        /*判断cookie*/
        //var can = !/^canpop=(\S*)$/.exec(document.cookie) || /^canpop=(\S*)$/.exec(document.cookie)[1] !== "false";
        //if (can) {
        //    popUp();
        //}


        $(".wGif").on("mouseover", function () {
            $(this).children(".wx_qr").show();
            return false;
        });
        $(".wGif").on("mouseout", function () {
            $(this).children(".wx_qr").hide();
            return false;
        });
    </script>
