<script>
$(function(){
	var id = 'demo_6b8268b2aea8c743f066d9a8f5d056f6',
		$demo = $('#'+id),
		$ul = $demo.find('ul'),
		$demowidth = $demo.width(),
		$demoheight = $demo.height();
		piclist = [{"FILE":"bbx\/upimages\/0.jpg","URL":"","TEXT":"","GO":1},{"FILE":"bbx\/upimages\/1.jpg","URL":"","TEXT":"","GO":2},{"FILE":"bbx\/upimages\/2.jpg","URL":"","TEXT":"","GO":3}],
		direction = 'RightToLeft',
		pages = [{"pic_num":"1","format":"1"},{"pic_num":"1","format":"1"},{"pic_num":"1","format":"1"}];
		
	$(window).resize(function(event){
		$demowidth = $demo.width();
		$demoheight = $demo.height();
	});
	
	for(var i in pages){
		var $li, pic,
			page = pages[i];
		if(typeof(pages[i])!="object") continue;

		$li = $('<li>');
		for(var j=0;j<page.pic_num;j++){
			pic = piclist.shift();
			if(!pic) pic = {};
			var html = '<div>';
			
			if(pic.URL) html += '<a href="'+pic.URL+'" target="_blank">';
			
			html += '<img src="'+pic.FILE+'" title="'+pic.TEXT+'">';
			
			if(pic.URL) html += '</a>';
		
			html += '</div>';
			$li.append(html);
		}

		switch(page.format){
			case 'rl2'://左右
				$('div', $li).css({
					width : '50%',
					height : $demoheight
				});
				$('img', $li).css({height:$demoheight});
				break;
			case 'ud2'://上下
				$('img', $li).css({
					height : $demoheight/2
				});
				break;
			case 'ud_2_1'://上2下1
			case 'ud_3_1'://上3下1
				$('img', $li).css({height:$demoheight/2});
				var $tmp = $('div:not(:last)', $li);
				$tmp.css({
					width : (100/$tmp.length)+'%'
				});
				break;
			case 'ud_1_2'://上1下2
			case 'ud_1_3'://上1下3
				$('img', $li).css({height:$demoheight/2});
				var $tmp = $('div:not(:first)', $li);
				$tmp.css({
					width : (100/$tmp.length)+'%'
				});
				break;
			case 'rl_1_2'://右1左2
			case 'rl_1_3'://右1左3
				$('div', $li).css({
					float : 'none',
					width : '50%'
				}).last().css({
					top : '0px',	
					right : '0px',
					position : 'absolute' 
				}).find('img').css({height : $demoheight});
				var $tmp = $('img:not(:last)', $li);
				$tmp.css({height : $demoheight/$tmp.length});
				break;
			case 'rl_2_1'://右2左1
			case 'rl_3_1'://右3左1
				$('div', $li).css({width:'50%'});
				$('img:first', $li).css({height:$demoheight});
				var $tmp = $('img:not(:first)', $li);
				$tmp.css({height:$demoheight/$tmp.length});
				break;
			case 'ud_3_0'://从上到下
			case 'ud_4_0'://从上到下
				var $tmp = $('img', $li);
				$tmp.css({
					height : $demoheight/$tmp.length
				});
				break;
			case 'rl_3_0'://从左到右
			case 'rl_4_0'://从左到右
				$tmp =$('div', $li);
				$tmp.css({width:(100/$tmp.length)+'%'});
				$('img', $li).css({height:$demoheight});
				break;
			case 'ud_2_2'://上2下2
				$('div', $li).css({width:'50%'});
				$('img', $li).css({height:($demoheight/2)});
				break;
			case 1:
			default:
				$('img', $li).css({
					height : $demoheight
				});
		}
		
		$ul.append($li);
	}
	var len = $('li', $ul).length; //获取焦点图个数
	$ul.width(((len+1)*100)+'%');
	$('li', $ul).width(100/(len+1)+'%');

	//以下代码添加数字按钮和按钮后的半透明长条
	var btn = "<div class='btnBg'><\/div><div class='test_btn'>";
	for(var i=0; i < len; i++){
		btn += "<span>" + (i+1) + "<\/span>";
	}
	btn += "<\/div>"
	$("#focus_44500").append(btn);

  //鼠标滑入某li中的某div里，调整其同辈div元素的透明度，由于li的背景为黑色，所以会有变暗的效果
	$("#focus_44500 ul li div").hover(function(){
		$(this).siblings().css("opacity",0.5);
	},function() {
		$("#focus_44500 ul li div").css("opacity",1);
	});

	var index = 0;
	//为数字按钮添加鼠标滑入事件，以显示相应的内容
	$("#focus_44500 .test_btn span").mouseenter(function(){
		index = $("#focus_44500 .test_btn span").index(this);
		showPics_44500(index);
	}).eq(0).trigger("mouseenter");

	//鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
	$("#focus_44500").hover(function(){
		clearInterval(window.picTimer_44500);
	},function(){
    window.picTimer_44500 && clearInterval(picTimer_44500);
		window.picTimer_44500 = setInterval(function(){
			if(index == len){ //如果索引值等于li元素个数，说明最后一张图播放完毕，接下来要显示第一张图，即调用showFirPic()，然后将索引值清零
				showFirPic_44500();
				index = 0;
			}else{ //如果索引值不等于li元素个数，按普通状态切换，调用showPics()
				showPics_44500(index);
			}
			index++;
		},3000); //此3000代表自动播放的间隔，单位：毫秒
	}).trigger("mouseleave");

	//显示图片函数，根据接收的index值显示相应的内容
	function showPics_44500(index){  //普通切换
		//alert("directionA:"+direction);
		if(direction=="LeftToRight" || direction=="RightToLeft"){
			//alert("A");
			var nowLeft = -index*$demowidth; //根据index值计算ul元素的left值
		}else{
			//if(direction=="TopToBottom"){//上到下 bottom-
				var nowLeft = -index*$demoheight;
			//}else{//下到上 top-
			//	var nowLeft = -index*$demoheight;
			//}
		}
		$("#focus_44500 ul").stop(true,false).animate({"left":nowLeft},500); //通过animate()调整ul元素滚动到计算出的position
		$("#focus_44500 .test_btn span").removeClass("on").eq(index).addClass("on"); //为当前的按钮切换到选中的效果
	}

	function showFirPic_44500(){ //最后一张图自动切换到第一张图时专用
		$("#focus_44500 ul").append($("#focus_44500 ul li:first").clone());
		//alert("directionB:"+direction);
		if(direction=="LeftToRight" || direction=="RightToLeft"){
			var nowLeft = -len*$demowidth; //通过li元素个数计算ul元素的left值，也就是最后一个li元素的右边
		}else{
			//if(direction=="TopToBottom"){
				var nowLeft = -len*$demoheight;
			//}else{
			//	var nowLeft = -len*$demoheight;
			//}
		}
		$("#focus_44500 ul").stop(true,false).animate({"left":nowLeft},500,function(){
			//通过callback，在动画结束后把ul元素重新定位到起点，然后删除最后一个复制过去的元素
			$("#focus_44500 ul").css("left","0");
			$("#focus_44500 ul li:last").remove();
		}); 
		$("#focus_44500 .test_btn span").removeClass("on").eq(0).addClass("on"); //为第一个按钮添加选中的效果
	}
	
});
</script>
