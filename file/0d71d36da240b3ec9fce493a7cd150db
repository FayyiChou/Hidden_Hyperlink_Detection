<script language="javascript">
function change(type,content,nums,id,css1,css2)
{
	for(var i=0;i<nums;i++)
	{
		if(i==id)
		{
			document.getElementById(type+i).className=css1;
			document.getElementById(content+i).style.display="";
		}
		else
		{
			document.getElementById(type+i).className=css2;
			document.getElementById(content+i).style.display="none";
		}
	}
}
</script>
<script type='text/javascript'>
			$(function() {
				jdt_lie_w=($("#liangfajdt ul").width()-500)/($("#liangfajdt ul li").length-1);
				jdt_lie_w=Math.ceil(jdt_lie_w);
				jdt_lie_sed=$("#liangfajdt ul").width()-jdt_lie_w*($("#liangfajdt ul li").length-1);
				$("#liangfajdt ul li").css("width",Number(jdt_lie_w)-1);
				//$("#liangfajdt ul li.selected").css("width",jdt_lie_sed);
				
				$('.kwicks').kwicks({
					minSize : Number(jdt_lie_w)-1,
					spacing : 1,
					behavior: 'slideshow'
				});
			});
			
			
			var jq=jQuery;
			function fnHover(div){
			var obj=jq("#"+div),
				lis=obj.find("li");
			lis.each(function(inx,li){
				li=jq(li);
				var text=jq(li.find(".text")[0]),
					height=text.height();
					newheight=li.height()-height;
				//text.css("padding-top",newheight/2+"px");
				li.find(".box").css("height","0px");
				li.hover(function(){
					li.find(".box").animate({height:66},200);
				},function(){
					li.find(".box").animate({height:"0"},200);
					});
			});
		}
		fnHover("hover01");
</script>
		  <script>
		//单个栏目的焦点图
		var ulnum=new Array();
		var ulwidth=new Array();
		var zt_clearinter=new Array();
		var gdnum=new Array();
		function clearztgd(xuhao){
			clearInterval(zt_clearinter[xuhao]);
		}

		function zt_left_gongdong(obj_gd,xuhao){
			if($("#"+obj_gd+" .waibao").is(":animated")){}
			else{
				gdnum[xuhao]++;		
				if(gdnum[xuhao]==ulnum[xuhao]+1){
					$("#"+obj_gd+" .waibao").css("left",0);
					gdnum[xuhao]=1;
				}		
				if(gdnum[xuhao]!=ulnum[xuhao]){
					$("#"+obj_gd+" .sz1").html(gdnum[xuhao]+1);
					$("#"+obj_gd+" .huodian ul li:eq("+gdnum[xuhao]+")").addClass("on").siblings().removeClass("on");
					$("#"+obj_gd+" .wire_ul li:eq("+gdnum[xuhao]+")").addClass("on").siblings().removeClass("on");
					
					
				}else{
					$("#"+obj_gd+" .sz1").html(1);
					$("#"+obj_gd+" .huodian ul li:eq(0)").addClass("on").siblings().removeClass("on");
					$("#"+obj_gd+" .wire_ul li:eq(0)").addClass("on").siblings().removeClass("on");
					
				}
				
				
				$("#"+obj_gd+" .waibao").animate({left:-($("#"+obj_gd+" .waibao ul").width()*gdnum[xuhao])},500);
			}
		}

		function zt_right_gongdong(obj_gd,xuhao){	
			if($("#"+obj_gd+" .waibao").is(":animated")){}
			else{
				gdnum[xuhao]--;
				if(gdnum[xuhao]==-1){
					$("#"+obj_gd+" .waibao").css("left",-$("#"+obj_gd+" .zt_yuanlai").width());
					gdnum[xuhao]=ulnum[xuhao]-1;
				}
				$("#"+obj_gd+" .sz1").html(gdnum[xuhao]+1);
				
				$("#"+obj_gd+" .waibao").animate({left:-($("#"+obj_gd+" .waibao ul").width()*gdnum[xuhao]), opacity:1},500);
				
				
				$("#"+obj_gd+" .huodian ul li:eq("+gdnum[xuhao]+")").addClass("on").siblings().removeClass("on");
				$("#"+obj_gd+" ul.wire_ul li:eq("+gdnum[xuhao]+")").addClass("on").siblings().removeClass("on");
				
				
			}
		}


		function gdztfun(obj_gd,pd,xuhao){
			gdnum[xuhao]=0;
			ulnum[xuhao]=$("#"+obj_gd+" .waibao ul").length;
			ulwidth[xuhao]=$("#"+obj_gd+" .waibao ul").width();
			
			//加红点
			for(i=0;i<ulnum[xuhao];i++){
				$("#"+obj_gd+" .huodian ul").append("<li></li>");
			}	
			$("#"+obj_gd+" .huodian ul li:eq(0)").addClass("on");
			$("#"+obj_gd+" .huodian").css("width",ulnum[xuhao]*21);
			$("#"+obj_gd+" .sz2").html(ulnum[xuhao]);	
		    $("#"+obj_gd+" .waibao").width(ulnum[xuhao]*ulwidth[xuhao]*2);
			$("#"+obj_gd+" .zt_zenjia").html($("#"+obj_gd+" .zt_yuanlai").html());
			
			
			$("#"+obj_gd).find(".fs_jdtgd").live("mouseover",function(){clearztgd(xuhao)});
			$("#"+obj_gd+" .fsj_l").mouseover(function(){clearztgd(xuhao)}).click(function(){zt_right_gongdong(obj_gd,xuhao);});
			$("#"+obj_gd+" .fsj_r").mouseover(function(){clearztgd(xuhao)}).click(function(){zt_left_gongdong(obj_gd,xuhao);});
			if(pd){
				zt_clearinter[xuhao]=setInterval(function(){zt_left_gongdong(obj_gd,xuhao);},4000);
				$("#"+obj_gd+" .fsj_l").mouseout(function(){zt_clearinter[xuhao]=setInterval(function(){zt_left_gongdong(obj_gd,xuhao);},4000);});
				$("#"+obj_gd+" .fsj_r").mouseout(function(){zt_clearinter[xuhao]=setInterval(function(){zt_left_gongdong(obj_gd,xuhao);},4000);});
				$("#"+obj_gd).find(".fs_jdtgd").live("mouseout",function(){zt_clearinter[xuhao]=setInterval(function(){zt_left_gongdong(obj_gd,xuhao);},4000);});
			}
		}

		//滚动下面的点事件
		function dianhuadongfun(obj_gd,pd,xuhao){
			$("#"+obj_gd+" .huodian li").live("mouseover",function(){
				num=$(this).index();
				gdnum[xuhao]=num;
				if(pd){clearInterval(zt_clearinter[xuhao]);}
				$("#"+obj_gd+" .waibao").animate({left:-($("#"+obj_gd+" .waibao ul").width()*num)},500);	
				$("#"+obj_gd+" .huodian ul li:eq("+num+")").addClass("on").siblings().removeClass("on");
			})
			if(pd){
				$("#"+obj_gd+" .huodian li").live("mouseout",function(){
					num=$(this).index();
					gdnum[xuhao]=num;
					zt_clearinter[xuhao]=setInterval(function(){zt_left_gongdong(obj_gd,xuhao);},4000);
				})
			}
		}
		$(document).ready(function() {
			
			//点事件	

			gdztfun("mr_move",true,2);	

			
		});


		  </script>
</body>

</html>



