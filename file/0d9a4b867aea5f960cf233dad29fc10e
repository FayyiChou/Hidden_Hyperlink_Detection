    <script type="text/javascript">
    function IsMobile() {
        var reg = "(nokia|iphone|ipad|android|motorola|^mot\-|softbank|foma|docomo|kddi|up\.browser|up\.link|";
        reg += "htc|dopod|blazer|netfront|helio|hosin|huawei|novarra|CoolPad|webos|techfaith|palmsource|";
        reg += "blackberry|alcatel|amoi|ktouch|nexian|samsung|^sam\-|s[cg]h|^lge|ericsson|philips|sagem|wellcom|bunjalloo|maui|";
        reg += "symbian|smartphone|midp|wap|phone|windows ce|iemobile|^spice|^bird|^zte\-|longcos|pantech|gionee|^sie\-|portalmmm|";
        reg += "jig\s browser|hiptop|^ucweb|^benq|haier|^lct|zunewp7|opera\s*mobi|opera\*mini|320x320|240x320|176x220";
        reg += ")";
        reg = new RegExp(reg, 'i');
        return reg.test(navigator.userAgent.toLowerCase());
    }
    if (IsMobile()) {
        window.location.href = "http://m.ka.tgbus.com/4712/";
    }

    $(function () {
    
var model_Description_json=[];
 function  model_Description_json_chis(key,value) //声明对象
     {
         this.key = key;
         this.value=  value;
     }


   var r, re;  
   var s = $("#tpc2_01").html();
   re = /(http|https|ftp)\:\/\/([a-zA-Z0-9\.\-]+(\:[a-zA-Z0-9\.&amp;%\$\-]+)*@)?((25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9])\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[0-9])|([a-zA-Z0-9\-]+\.)*[a-zA-Z0-9\-]+\.[a-zA-Z]{2,4})(\:[0-9]+)?(\/[^/<][a-zA-Z0-9\.\,\?\'\\/\+&amp;%\$#\=~_\-@]*)*/g;  
     r = s.match(re) 
   for(var i=0;i<r.length;i++){
    for(var j = i + 1;j<r.length;j++){
        if(r[i].length<r[j].length){
            var tmp = r[i];
            r[i] = r[j];
            r[j] = tmp;
        }
    }
}

for(var i=0;i<r.length;i++){
	var id_now = Date.now().toString();
    $("#tpc2_01").html(($("#tpc2_01").html().replace(r[i],id_now.toString())));
	model_Description_json[i]= new model_Description_json_chis(id_now,r[i]);
	for(var y=0;y<100;y++){}
}
$("#tpc2_01").html($("#tpc2_01").html().replace(location.protocol+'//'+location.host+location.pathname,""));



var show_numid_bool=false;
var show_numid_bool_url='';
function show_numid(numid)
{
	show_numid_bool=false;
	show_numid_bool_url='';
	$.each(model_Description_json,function(i,attrib){  
	if(numid==$(this)[0].key)
	{
		show_numid_bool=true;
		show_numid_bool_url=$(this)[0].value;
		return false;
	}
	}); 
	return show_numid_bool
}



var each_str =["href","src"];
 
for(var i=0;i<each_str.length;i++)
{
    $.each(model_Description_json,function(y,attrib){  
            $('['+each_str[i]+'="'+attrib.key+'"]').each(function(){
                $(this).attr(each_str[i],attrib.value);
            });
          $('['+each_str[i]+'="'+location.protocol+'//'+location.host+location.pathname+attrib.key+'"]').each(function(){
                $(this).attr(each_str[i],attrib.value);
            });
	});
}

$.each(model_Description_json,function(i,attrib){
	
		$("#tpc2_01").html(
		
		$("#tpc2_01").html().replace($(this)[0].key,"<a href='" + $(this)[0].value +"' target='_blank' rel='nofollow'>" + $(this)[0].value + "</a>")
		)
		});
	
	
    });
