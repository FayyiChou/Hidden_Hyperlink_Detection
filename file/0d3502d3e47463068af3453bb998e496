<script type="text/javascript">
	$("#aLeftFloatTools_Show").click(function(){
		$('#divLeftFloatToolsView').animate({width:'show',opacity:'show'},100,function(){$('#divLeftFloatToolsView').show();});
		$('#aLeftFloatTools_Show').hide();
		$('#aLeftFloatTools_Hide').show();
	});
	$("#aLeftFloatTools_Hide").click(function(){
		$('#divLeftFloatToolsView').animate({width:'hide', opacity:'hide'},100,function(){$('#divLeftFloatToolsView').hide();});
		$('#aLeftFloatTools_Show').show();
		$('#aLeftFloatTools_Hide').hide();
	});
	$("#aRightFloatTools_Show").click(function(){
		$('#divRightFloatToolsView').animate({width:'show',opacity:'show'},100,function(){$('#divRightFloatToolsView').show();});
		$('#aRightFloatTools_Show').hide();
		$('#aRightFloatTools_Hide').show();
	});
	$("#aRightFloatTools_Hide").click(function(){
		$('#divRightFloatToolsView').animate({width:'hide', opacity:'hide'},100,function(){$('#divRightFloatToolsView').hide();});
		$('#aRightFloatTools_Show').show();
		$('#aRightFloatTools_Hide').hide();
	});
</script>
