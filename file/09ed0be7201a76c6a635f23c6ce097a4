<script>
    $(function () {
        $("#btn_baojia").click(function () {
            
            var mj = $("#mj").val();
            if (mj == "")
            {
                alert("请输入面积！");
                return false;
            }
			if(isNaN(mj)){
 				 alert("房屋面积只能输入数字");
				 return false;
			}
            var tel = $("#tel").val();
            if (tel == "") {
                alert("请输入手机号！");
                return false;
            }
			if (!$("#tel").val().match(/^(((13[0-9]{1})|(15[0-9]{1})|(17[0-9]{1})|(18[0-9]{1}))+\d{8})$/)) { 
				alert("手机号码格式不正确！");
				return false;
			} 
            var model=new Object;
            model.tel = tel;
            model.mj = mj;
           
            $.post("/Message/InserBaoJiaMessage", model, function (data) {
                if (data["Result"] == true) {
                    $("#mj").val("");
                    $("#tel").val("");
                    
                    var total = parseInt(mj * $("#sel_taocan").val() * 1.13);
                    $("#msgprice").val(total);
                }
                else
                {
                    alert("报价失败");
                }
            });
        })
    });
</script>


