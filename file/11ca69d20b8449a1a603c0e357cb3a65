    <script type="text/javascript" language="javascript">
		function checkApply() {
			var relation = $('#relation').val().length;
			if (relation == 0) {
				alert("请选择与被纪念人关系！");
				return false;
			}
			var explen = $('#explanation').val().length;
			if (explen == 0) {
				alert("请填写申请说明！");
				return false;
			} else if (explen < 10) {
				alert("申请说明不能少于10个汉字！");
				return false;
			}
			return true;
		}

