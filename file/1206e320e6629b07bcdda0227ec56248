    <script type="text/javascript">
        function query_by_name(){
            var query_name = $("#query_name").val();
            if(query_name.length==0){
                $("#query_name").focus();
                return false;
            }
            $.layer({
            	type: 2,
                shadeClose: true,
                title: ['中国正面管教协会会员', 'background:#ED8216;color:#FFF;'],
                closeBtn: [0, true],
                shade: [0.8, '#000'],
                border: [0],
                offset: ['',''],
                area: ['520px', '300px'],
                iframe: {src: '/index.php?m=home&c=index&a=queryer&type=name&val='+query_name}
            });
        }
        function query_by_code(){
            var query_code = $("#query_code").val();
            if(query_code.length==0){
                $("#query_code").focus();
                return false;
            }
            $.layer({
            	type: 2,
                shadeClose: true,
                title: ['中国正面管教协会会员', 'background:#ED8216;color:#FFF;'],
                closeBtn: [0, true],
                shade: [0.8, '#000'],
                border: [0],
                offset: ['',''],
                area: ['520px', '300px'],
                iframe: {src: '/index.php?m=home&c=index&a=queryer&type=code&val='+query_code}
            });
        }
    </script>
