<script language="javascript">
var imgs=document.getElementsByName("scrollImg");
var nowImg=0;  
var numStr="<table border='0px solid white' cellspacing='2px' cellpadding='0px'><tr>";
for(var i=0;i<=imgs.length-1; i++){
if(nowImg == i)
{
numStr+="<td  align='center' style='width:30px; background:rgb(160,160,160)' onclick='changeImg("+i+")'><font style='cursor:pointer' color='white' align='center'>"+(i+1)+"</font></td>";
}
else
{
	numStr+="<td align='center' style='width:30px;  background:rgb(160,160,160)' onclick='changeImg("+i+")'><font style='cursor:pointer' color='white' align='center'>"+(i+1)+"</font></td>";

}

}
numStr+="</tr></table>";

document.getElementById("nums").innerHTML=numStr;
var frame=document.getElementById("fram");
var imgW=document.getElementById("img1").width;  

function showOne(){
	frame.scrollLeft=nowImg*imgW;
	document.getElementById("nums").style.left=nowImg*imgW+210+"px";
	if(nowImg==imgs.length-1){
		nowImg=0;	
	}else
	{
		nowImg++;	
	}
}

var id=window.setInterval(showOne,5000);
function overFrame(){
	window.clearInterval(id);	
}
function outFrame(){
		id=window.setInterval(showOne,5000);
}


function changeImg(x){
		nowImg=x;
		showOne();
}


var yd = document.getElementById("frame");





</script>




