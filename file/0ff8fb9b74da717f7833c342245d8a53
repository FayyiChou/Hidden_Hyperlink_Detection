<script type="text/javascript">
var naver = window.naver || {};
 	naver.dict = naver.dict || {};
 	naver.dict.endic = naver.dict.endic || {};
 	naver.dict.endic.surveyPopup = (function(window, $, undefined) {
 		if (!$) {
 	        throw  new Error("it depends on 'jindo',please check");
 	    }
 
 		var popupDiv = jindo.$("surveyPopupDiv");
 		var now = new Date();
 		var timeZone = 9;
 		var now = new Date();
 		var utcTime = now.getTime() + now.getTimezoneOffset() * 60000;
 		now = new Date(utcTime + 3600000 * timeZone);
  		
  		var eventStartDate = new Date('2017/02/22 00:00:00');
  		var eventEndDate = new Date('2017/02/23 23:59:59');
  		var skinCookie = $Cookie().get("DICT_LOCALE2", "ko");
  		
  		var init = function(){
  			if (skinCookie == "en" && now.getTime() >= eventStartDate.getTime() && now.getTime() <= eventEndDate.getTime()){
 				var cookieValue = $Cookie().get("ENDIC_SURVEY_POPUP", "N");
  				if (cookieValue == "Y") {
  					$Element(popupDiv).hide();
  				} else {
 					$Element(popupDiv).show();
 				}
 			} else {
 				$Element(popupDiv).hide();
 			}
  		}
  		
  		var hidePopupToday = function(){
        	$Cookie().set("ENDIC_SURVEY_POPUP", "Y", 1);
  			$Element(popupDiv).hide();
  		}
  		
  		var hidePopupForever = function(){
 			$Cookie().set("ENDIC_SURVEY_POPUP", "Y", 365);
  			$Element(popupDiv).hide();
  		}
  		
  		var goToSurveyPage = function() {
  			hidePopupToday();
  			window.open("https://form.office.naver.com/form/responseView.cmd?formkey=ODk4NjAzNWEtZTMyYy00YjA2LTllM2QtMjJhMmJjZjliNDUx&sourceId=urlshare&lang=en");
  		}
  		// export modules
 		return {
 			init: init,
 			hidePopupToday: hidePopupToday,
 			hidePopupForever: hidePopupForever,
 			goToSurveyPage: goToSurveyPage
 		}
 		// end
 	}(this, this.jindo));
 	naver.dict.endic.surveyPopup.init();
</script>
