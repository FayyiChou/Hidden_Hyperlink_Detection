<script type="text/javascript">
//더보기 BFCache
var $ListContainer = jQuery("#homeCards");
$ListContainer.addClass("NO_TAP_HIGHLIGHT");
$ListContainer.on("click", document.body, function(e){
	if(jQuery(e.target).closest('div.lang_select_wrap').length<=0){
		jQuery.persist("NEWMAIN",{
			"info" : $ListContainer.html(),
			"scrollTop" : document.body.scrollTop
		});
	}
});

var cacheData = jQuery.persist("NEWMAIN");
if(cacheData) {
	$ListContainer.html(cacheData.info);
	window.scrollTo(0, cacheData.scrollTop);
	if($('moreCardBtn').style.display == "none"){
			initTodayQuizFlicking();
	}
}

function initTodayQuizFlicking(){
	if(jindo.$('mflickTodayQuiz') != null){		
		var todayQuizAjaxData = [
	        "<div class=\"word_area\">"
			+"<strong class=\"tit_confuse\">Today's quiz</strong>"
			+"<div class=\"versus\">reproduction</div>"
			+"<a onclick=\"nclk(this, 'cqu.entry', '', 1)\" href=\"http://m.wordbook.naver.com/endic/today/quiz.nhn?index=0&targetDate=2017.04.01\" class=\"btn_quiz\">Quiz</a></div>"
	        ,
	        "<div class=\"word_area\">"
			+"<strong class=\"tit_confuse\">Today's quiz</strong>"
			+"<div class=\"versus\">civility</div>"
			+"<a onclick=\"nclk(this, 'cqu.entry', '', 1)\" href=\"http://m.wordbook.naver.com/endic/today/quiz.nhn?index=1&targetDate=2017.04.01\" class=\"btn_quiz\">Quiz</a></div>"
	        ,
	        "<div class=\"word_area\">"
			+"<strong class=\"tit_confuse\">Today's quiz</strong>"
			+"<div class=\"versus\">casualty</div>"
			+"<a onclick=\"nclk(this, 'cqu.entry', '', 1)\" href=\"http://m.wordbook.naver.com/endic/today/quiz.nhn?index=2&targetDate=2017.04.01\" class=\"btn_quiz\">Quiz</a></div>"
	        ,
	        "<div class=\"word_area\">"
			+"<strong class=\"tit_confuse\">Today's quiz</strong>"
			+"<div class=\"versus\">prospective</div>"
			+"<a onclick=\"nclk(this, 'cqu.entry', '', 1)\" href=\"http://m.wordbook.naver.com/endic/today/quiz.nhn?index=3&targetDate=2017.04.01\" class=\"btn_quiz\">Quiz</a></div>"
	        ,
	        "<div class=\"word_area\">"
			+"<strong class=\"tit_confuse\">Today's quiz</strong>"
			+"<div class=\"versus\">come out</div>"
			+"<a onclick=\"nclk(this, 'cqu.entry', '', 1)\" href=\"http://m.wordbook.naver.com/endic/today/quiz.nhn?index=4&targetDate=2017.04.01\" class=\"btn_quiz\">Quiz</a></div>"
	        
			];
		
		$Element('mflickTodayQuiz').html("<div class=\"flick-container\"></div>");
		var oFlickingTodayQuiz = new jindo.m.CircularFlicking(jindo.$('mflickTodayQuiz'),{
			nDuration : 200,
			bUseCircular : false,
		    nTotalContents : todayQuizAjaxData.length //전체 아이템 개수
		});
		
		var cardLoca = jindo.$('todayQuizCard');
		var pagingBarListTodayQuiz = jindo.$$('div.paging a', cardLoca);
		
		$Fn(function(e){
			var clickIndex = $Element(e.currentElement).html();
			
			oFlickingTodayQuiz.setContentIndex(clickIndex);
			$ElementList(pagingBarListTodayQuiz).removeClass("on");
			$Element(pagingBarListTodayQuiz[clickIndex]).addClass("on");
		}, this).attach(pagingBarListTodayQuiz, "click");
		
		oFlickingTodayQuiz.attach({
		    'afterFlicking' : function(oCustomeEvt){
		    	$ElementList(pagingBarListTodayQuiz).removeClass("on");
				$Element(pagingBarListTodayQuiz[oCustomeEvt.nContentIndex]).addClass("on");
				if(oCustomeEvt.bLeft){
		            //왼쪽으로 움직였을 경우 오른쪽 panel만 업데이트
		            this.getRightPanelElement().html(todayQuizAjaxData[oCustomeEvt.nContentRightIndex]);
		        }else{
		            //오른쪽으로 움직였을 경우 왼쪽 panel만 업데이트
		            this.getLeftPanelElement().html(todayQuizAjaxData[oCustomeEvt.nContentLeftIndex]);
		        }
		    },
		    'move' : function(oCustomeEvt){
				//setContentIndex로 플리킹 효과가 아닌 전체 panel의 정보가 바뀔경우   3개 panel의 모든 정보를 바꾼다.
				var welCurrent = this.getPanelElement();
				var welLeft = this.getLeftPanelElement();
				var welRight = this.getRightPanelElement();
	
				welCurrent.html(todayQuizAjaxData[oCustomeEvt.nContentIndex]);
				welLeft.html(todayQuizAjaxData[oCustomeEvt.nContentLeftIndex]);
				welRight.html(todayQuizAjaxData[oCustomeEvt.nContentRightIndex]);
			}
		});
		
		$A(pagingBarListTodayQuiz).forEach(function(item){
			if($Element(item).hasClass("on")){oFlickingTodayQuiz.setContentIndex($Element(item).html());}
		}, this);
		$Element(cardLoca).show();
	}
}

function initConfoundQuizFlicking(){
	if(jindo.$('mflickConfoundQuiz') != null){
	
		var confoundQuizAjaxData = [
        "<div class=\"word_area\">"
        +"<strong class=\"tit_confuse\">Confused words <span class=\"count\"><strong>0</strong>/10</span></strong>"
        +"<div class=\"versus\">metal<span class=\"vs\"> vs </span>mortal</div>"
        +"<a href=\"http://wquiz.naver.com/bundle/quiz.dict?service=endic&dic_type=koko&quizId=9c01ec02909b439784cdb1681e03c8fb&group_id=11\" onClick=\"nclk(this, 'ceq.quiz', '', 1);\" class=\"btn_quiz\">Quiz</a>"
        +"</div>",
        "<div class=\"word_area\">"
        +"<strong class=\"tit_confuse\">Misspelled words <span class=\"count\"><strong>0</strong>/10</span></strong>"
        +"<div class=\"versus\">cemetery<span class=\"vs\"> vs </span>cemetary</div>"
        +"<a href=\"http://wquiz.naver.com/bundle/quiz.dict?service=endic&dic_type=koko&quizId=1ff1f83ee1a0427b9934c7e34b718b92&group_id=12\" onClick=\"nclk(this, 'ceq.quiz', '', 1);\" class=\"btn_quiz\">Quiz</a>"
        +"</div>",
        "<div class=\"word_area\">"
        +"<strong class=\"tit_confuse\">Synonym Quiz</strong>"
        +"<div class=\"versus\">'오래된' '나이가 든'의 뜻이 아닌 것은?</div>"
        +"<a href=\"http://wquiz.naver.com/quiz.dict?service=endic&dic_type=koko&quizId=f857c460c3e347348966df95cb44f79c\" onClick=\"nclk(this, 'ceq.quiz', '', 1);\" class=\"btn_quiz\">Quiz</a>"
        +"</div>"
        ];
		
		$Element('mflickConfoundQuiz').html("<div class=\"flick-container\"></div>");
		var oFlickingConfoundQuiz = new jindo.m.CircularFlicking(jindo.$('mflickConfoundQuiz'),{
			nDuration : 200,
			bUseCircular : false,
		    nTotalContents : confoundQuizAjaxData.length //전체 아이템 개수
		});
		
		var cardLoca = jindo.$('confoundQuizCard');
		var pagingBarListConfoundQuiz = jindo.$$('div.paging a', cardLoca);
		
		$Fn(function(e){
			var clickIndex = $Element(e.currentElement).html();
			oFlickingConfoundQuiz.setContentIndex(clickIndex);
			$ElementList(pagingBarListConfoundQuiz).removeClass("on");
			$Element(pagingBarListConfoundQuiz[clickIndex]).addClass("on");
		}, this).attach(pagingBarListConfoundQuiz, "click");
		
		oFlickingConfoundQuiz.attach({
		    'afterFlicking' : function(oCustomeEvt){
		    	$ElementList(pagingBarListConfoundQuiz).removeClass("on");
				$Element(pagingBarListConfoundQuiz[oCustomeEvt.nPanelIndex]).addClass("on");
				if(oCustomeEvt.bLeft){
		            //왼쪽으로 움직였을 경우 오른쪽 panel만 업데이트
		            this.getRightPanelElement().html(confoundQuizAjaxData[oCustomeEvt.nContentRightIndex]);
		        }else{
		            //오른쪽으로 움직였을 경우 왼쪽 panel만 업데이트
		            this.getLeftPanelElement().html(confoundQuizAjaxData[oCustomeEvt.nContentLeftIndex]);
		        }
		    },
		    'move' : function(oCustomeEvt){
				//setContentIndex로 플리킹 효과가 아닌 전체 panel의 정보가 바뀔경우   3개 panel의 모든 정보를 바꾼다.
				var welCurrent = this.getPanelElement();
				var welLeft = this.getLeftPanelElement();
				var welRight = this.getRightPanelElement();
	
				welCurrent.html(confoundQuizAjaxData[oCustomeEvt.nContentIndex]);
				welLeft.html(confoundQuizAjaxData[oCustomeEvt.nContentLeftIndex]);
				welRight.html(confoundQuizAjaxData[oCustomeEvt.nContentRightIndex]);
			}
		});
		
		$A(pagingBarListConfoundQuiz).forEach(function(item){
			if($Element(item).hasClass("on")){oFlickingConfoundQuiz.setContentIndex($Element(item).html());}
		}, this);
		$Element(cardLoca).show();
	}
}

