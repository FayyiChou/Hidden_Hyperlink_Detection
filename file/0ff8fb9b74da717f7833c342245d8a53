<script type="text/javascript">
window.onscroll = function () { 
	// iOS 사전앱일 경우 검색창 상단고정 않함.
	if (navigator.userAgent.indexOf("naverdicapp") > -1 && (navigator.userAgent.indexOf("iPad") > -1 || navigator.userAgent.indexOf("iPhone") > -1)){
		stopSearchFollow = true;
	} else if ($Element("nDrawer").css("display") == "none") { // 햄버거 메뉴 닫긴 경우
		stopSearchFollow = false;
	}
	
	if(!stopSearchFollow){
		var a = document.documentElement.scrollTop==0? document.body.clientHeight : document.documentElement.clientHeight;
		var b = document.documentElement.scrollTop==0? document.body.scrollTop : document.documentElement.scrollTop;
		var c = document.documentElement.scrollTop==0? document.body.scrollHeight : document.documentElement.scrollHeight;
		if (b > 45 && c-(a+b) > 200) {
			$Element($("header")).addClass("is_fixed");
			$Element("wrap").css("padding-top", "48px");
			if($("main_input")){
	        	$("main_input").blur();
			}
			if($("footerToTop")){$Element($("footerToTop")).show();}
		}
		else { 
			if(c-(a+b) > 200){
				$Element($("header")).removeClass("is_fixed");
				$Element("wrap").css("padding-top", "");
			}
			if($("footerToTop")){$Element($("footerToTop")).hide();}
		}
	}else{
		if($Element($("header")).hasClass("is_fixed")){
			$Element($("header")).removeClass("is_fixed");
		}
	}
}
</script>

<script type="text/javascript">
var cLocaleCokie = "";
//쿠키를 받는다 .
var dCookie = document.cookie;
if (dCookie.length >= 1){
	var rReg = new RegExp('\\bDICT_LOCALE2=([^;]*)(;|$)');
	var tCokie = dCookie.match(rReg);
	if(tCokie!=null){
		cLocaleCokie = tCokie && tCokie[1];
		if(cLocaleCokie == "ko"){
			cLocaleCokie="";
		}else if(cLocaleCokie == "zh_CN"){
			cLocaleCokie="zhcn";
		}
	}
}

if(cLocaleCokie == ""){
	cLocaleCokie = window.i18n("locale.locale","ko");
	if(cLocaleCokie == "ko"){
		cLocaleCokie="";
	}else if(cLocaleCokie == "zh_CN"){
		cLocaleCokie="zhcn";
	}else if(cLocaleCokie == "in"){
		cLocaleCokie="id";
	}
}

if(cLocaleCokie != ""){
	cLocaleCokie="_"+cLocaleCokie;
}

//Global 변수 선언
var nsc = "Mendic.home2"+cLocaleCokie;
var ccsrv="cc.naver.com";
var g_query="";
var pcEndicUrl = "http://endic.naver.com";
var wordbookUrl = "http://wordbook.naver.com";
var addWordbookUrl = "http://m.wordbook.naver.com/endic/setting/my.nhn";
var userId = "";

var mobile = {
	isMobile:true
}

if (!!nhn && !!nhn.MobilePronListening) {
	nhn.MobilePronListening.init();
}
</script>
