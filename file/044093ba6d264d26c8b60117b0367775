                        </table></div><SCRIPT language=javascript type=text/javascript>
	var marqueesHeight6 = 305;
	var stopscroll6 = false;
	GetObj("scrollBlk5").scrollTop = false;
	with(GetObj("scrollBlk5")){
		style.width = "215px";
		style.height = marqueesHeight6 + "px";
		style.overflow = "hidden";
		noWrap = true;
		onmouseover = new Function("stopscroll6=true");
		onmouseout=new Function("stopscroll6=false"); 
	}
	preTop6=0;
	currentTop6=164;
	stoptime6=0;
	GetObj("scrollBlk5").innerHTML+=GetObj("scrollBlk5").innerHTML;

	function initSrolltext(){
		GetObj("scrollBlk5").scrollTop=0;
		setInterval("scrollUp6()",85);
	}

	initSrolltext();

	function scrollUp6(){
		if(stopscroll6==true) return;
		currentTop6+=1;
		if(currentTop6==25)
		{
			stoptime6+=1;
			currentTop6-=1;
			if(stoptime6==40)
			{
				currentTop6=0;
				stoptime6=0;
			}
		}
		else {
			preTop6=scrollBlk5.scrollTop; 
			scrollBlk5.scrollTop+=1;
			if(preTop6==scrollBlk5.scrollTop){ 
				scrollBlk5.scrollTop=24; 
				scrollBlk5.scrollTop+=1; 
			}
		}
	}

	function GetObj(objName){
		if(document.getElementById){
			return eval('document.getElementById("' + objName + '")');
		}else{
			return eval('document.all.' + objName);
		}
	}
	    </SCRIPT>
						
						
						
						
