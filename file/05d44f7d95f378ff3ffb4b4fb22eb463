     //   $('.techinfo').fadeOut(500)
    //}
	)

    $('.techinfoLink').click(function () {
        $('.techinfo').fadeOut(500)
    }
    //,function(){
    //   $('.techinfo').fadeOut(500)
    //}
	)
});
</script>

