<script>
$(document).ready(function(){

	// hide #back-top first
 //   $("#back-top").hide();
//    $("#back-bottom").hide();
	
	// fade in #back-top
	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 00) {
                $('#back-bottom').fadeOut();
			} else {
                $('#back-bottom').fadeIn();
			}
		});

		// scroll body to 0px on click
		$('#back-top a').click(function () {
			$('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});
});

// fade in #back-bottom
$(function () {
//    $(window).scroll(function () {
//        if ($(this).scrollTop() >= 00) {
//            $('#back-bottom').fadeIn();
//        } else {
//            $('#back-bottom').fadeOut();
//        }
//    });

    // scroll body to 0px on click
    $('#back-bottom a').click(function () {
        $('body,html').animate({
            scrollTop: $('#about').offset().top
        }, 800);
        return false;
    });
});

});
</script>


