            <script language="javascript">
            function loadVms(userId){
				var url = "http://vms.info.hc360.com/vms/shopvmsinfo/shopInfoAction?callback=?";
				jQuery.ajax({
					type:"GET",
					url:url,
					dataType:"jsonp",
					contentType: "application/x-www-form-urlencoded; charset=gbk",
					timeout:3000,
					scriptCharset:'utf-8',		
					data:{
						"userId":userId
					},
					success:function(result){
						if(result.infoState != "0"){
							var returnList = new Array();
							returnList = result.returnVlaue;
							var listFirstValue = returnList[0];
							//显示十大勋章
							if(listFirstValue.medalState == "1"){
								var medalStr = '<img src="http://style.org.hc360.cn/images/detail/mysite/public/xuanp3/top10logo.png" onmouseover="toolTip('+"'"+listFirstValue.medalInfo+"'"+',this)" onmouseout="toolTip()" >';
								jQuery("#top10logo").html(medalStr);
							}
							//显示参选信息
							if(result.infoState == "1" ||result.infoState == "2"){
								var infoListStr = "";
								for(var i=0; i<returnList.length; i++){
									reValue = returnList[i];
									infoListStr = infoListStr + "<dl class='myTop10Item'><dt>" +reValue.vmsIntro+ "</dt><dd><a onclick = 'HC.UBA.sendUserlogsElement(\"UserBehavior_top10_abovepic\")' href='" +reValue.voteUrl+ "' target='_blank'><img src='"+reValue.logoUrl+"'/></a><a onclick = 'HC.UBA.sendUserlogsElement(\"UserBehavior_top10_belowtitle\")' href='" +reValue.voteUrl+ "'class='nameLink' target='_blank'>"+reValue.actorName+"</a><span>投票数：<b>"+reValue.voteSum+"</b></span></dd></dl>";
								}
								jQuery(".myTop10Cont").html(infoListStr);
								jQuery(".myTop10Box").css({display:"block"});
							}
						}
					}
				});
			}
