<script>
$(function(){
    $('#fabiao_pinlun').submit(function(){
                    var options = {
                        type:"POST",url: "/pinglun/save_pinglun.html",cache:'false',//dataType:'json',
                        success: function(msg)
                        {
                            if(msg.error<0)
                            {
                                alert(msg.content);
                                if(msg.error==-2)
                                {
                                    location.href=msg.url
                                }
                            }else
                            {
                                // alert(msg.html);
                                $("#cmt-list").append(msg.html)
                            }
                            
                            return false;
                        }
                    };
                    $(this).ajaxSubmit(options);
                    return false;
                })  
});
$(function(){
    var pageCount = 1;//总页数
    var regExp = /_ueditor_page_break_tag_/;//根据某处字符来分页
    var saveContent;//用于保存分页数据
    var content, pageList;//保存全局ID

    $(document).ready(function(){
        UeInitialize("#artibody","#page");
    });
    UeInitialize = function (id,page) {
        var cTxt = $(id).html();
        content = $(id);
        pageList = $(page);
        if (cTxt != null && regExp.test(cTxt)) {

            saveContent = cTxt.split(regExp);

            pageCount = saveContent.length;

        }
        window.UePageContent(1);
    };

    UePageContent = function (pageIndex) {
        if (pageIndex >= 1 && pageIndex <= pageCount && saveContent != null && saveContent.length >= 0) {
            var pageHtml = pageList;
            if ((parseInt(pageIndex) - 1) <= saveContent.length) {
                content.html(saveContent[parseInt(pageIndex) - 1]);
            }

            pageHtml.html("");
            var innht ='';
            for(var i =1;i<=pageCount;i++){
                if(i==pageIndex){
                 innht +="<a target='_self' href='javascript:UePageContent("+i+")' style='color:red'>【"+i+"】</a>";
                }else{
                    innht +="<a target='_self' href='javascript:UePageContent("+i+")'>【"+i+"】</a>";
                }
            }
            /*var innHtml = "页数：" + pageIndex + "/" + pageCount;
            innHtml += "<a target='_self' href='javascript:UePageContent(1)'>首页</a>";
            if (pageIndex > 1) {
                innHtml += "<a target='_self' href='javascript:UePageContent(" + (parseInt(pageIndex) - 1) + ")'>上一页</a>";
            }
            if (pageIndex < pageCount) {
                innHtml += "<a target='_self' href='javascript:UePageContent(" + (parseInt(pageIndex) + 1) + ")'>下一页</a>";
            }
            innHtml += "<a target='_self' href='javascript:UePageContent(" + pageCount + ")'>末页</a>";*/
            pageHtml.html(innht);
            //pageHtml.html(innHtml);
        }
    };

})
</script>



</body></html>