<div class="wrap"><iframe id="frameID" name="frameID" onload="Javascript:SetWinHeight(this)"  marginwidth="0" framespacing="0" marginheight="0" frameborder="0" width="100%" height="965px" src="sy.html" allowtransparency="true" scrolling="no"></iframe>
 <script type="text/javascript">
    function reinitIframe(){
    var iframe = document.getElementById("frameID");
    try{
    var bHeight = iframe.contentWindow.document.body.scrollHeight;
    var dHeight = iframe.contentWindow.document.documentElement.scrollHeight;
    var height = Math.max(bHeight, dHeight);
    iframe.height =  height;
    }catch (ex){}
    }
    window.setInterval("reinitIframe()", 200);
    </script>  </div>
