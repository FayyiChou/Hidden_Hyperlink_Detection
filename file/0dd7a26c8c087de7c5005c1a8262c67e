<script type="text/javascript">
	var Mtime;
	if (typeof szzbAffiches == "undefined") {
		szzbAffiches = "";
	}
	if (szzbAffiches != "" && szzbAffiches.size != 0) {
		var date = szzbAffiches[0][5];
		Mtime = date;
	}else{
		var today = new Date();
		var day = today.getDate();
		var month = today.getMonth() + 1;
		var year = today.getFullYear();
		month = month < 10 ? "0" + month : month;
		day = day < 10 ? "0" + day : day;
		Mtime = year + "-" + month + "-" + day;
	}

	if (document.all) {
		window.attachEvent('onload', StockList);
	} else {
		window.addEventListener('load', StockList, false);
	}
	function StockList() {
		this.stockCode = document.getElementById('stockCode');
		if (this.stockCode) {
			var suggestUrl = "";
			var suggestStock = new Suggest(suggestUrl);
			suggestStock.bind(this.stockCode);
		}
	}
	function setPIH() {
		var iframe = document.getElementById("i_nr");
		try {
			var bHeight = iframe.contentWindow.document.body.scrollHeight;
			var dHeight = iframe.contentWindow.document.documentElement.scrollHeight;
			var height = Math.max(bHeight, dHeight);
			iframe.height = height;
		} catch (ex) {
		}

	}
	window.setInterval("setPIH()", 100);
</script>

<script>
	function getSecode(){
		var paramName = "secode=";
		var req = location.href;
		var secode;
		var start = req.indexOf(paramName);
		if(start>0){
			var end = req.indexOf("&", start);
			end = end == -1 ? req.length : end;
			secode = req.substring(start+paramName.length, end);
			return secode;
		}
		return "";
	}
	
	var secode = getSecode();
	if(secode != ''){
		document.write("<script type='text/javascript' src=/disclosure/fulltext/stocks/szse/gsgg1y/"+secode+".js><\/script>");
	}
</script>
