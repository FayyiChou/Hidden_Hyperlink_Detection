	<script language="javascript"> 	
		function mouseOver(id){	
			var arrays = ['www.szse.cn','disclosure','marketdata','rule','investor','research','aboutus'];	
			for(var i=0;i<arrays.length;i++){	
				if(id==arrays[i]){	
					document.getElementById(id).className="ind";	
				}	
			}	
		}	
		function mouseOut(id){	
			var linkUrl = GetPageName(window.location.href);	
			if(linkUrl.indexOf(id)>-1 && id != "www.szse.cn"){	
				document.getElementById(id).className="ind";	
			}else{	
				document.getElementById(id).className="";	
			}	
		}	
	</script>	


