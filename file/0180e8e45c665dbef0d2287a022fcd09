<script type="text/javascript">
    var currentNav = $('#nav .ce');
    if ($(currentNav).length == 0) {
        $('#nav .index').addClass("home");
    }
    else {
        $('#nav .ce').addClass("home");
    }
    $(".xiaoxue_downlist").mouseenter(function () {
        $(this).addClass("hover");
        $(this).find("ul").show();
    }).mouseleave(function () {
        $(this).removeClass("hover");
        $(this).find("ul").hide();
    })

    $(function(){
        //搜索框样式变化
        $("#search").mouseenter(function () {
            $(this).addClass("over");
        }).mouseleave(function () {
            $("#search").removeClass("over");
        });
        $("#search_input").blur(function () {
            $("#search").removeClass("over");
            var value = $('#search_input').val();
            if (value) {return;}
            $('#search .keywords-wrap').show();
        });
        
        $('#search_input').focus(function(e){
            $('#search .keywords-wrap').hide();
        });

        if ($('.keywords-wrap').length) {
            $('.keywords-wrap').on('click', '.item', function(e){
                if (typeof (window.ht) !== 'undefined') {
                    var index = $(this).index() + 1,
                        lang = getLangs();
                    window.ht.sendCustomEvent('hotkeywords', '{"index": "' + index + '", "Lang": "' + lang + '"}');
                }
            });
        }

        $('.nav_wrapper').on('mouseover', '.nav_li', function (){
            $(this).addClass('cur_li');
            // $('.nav_wrapper .nav_li').removeClass('cur_li').filter($(this)).addClass('cur_li');
        }).on('mouseout', '.nav_li', function (){
            $(this).removeClass('cur_li')
        })
    });
</script>
        

