     <script language="javascript" type="text/javascript">
$(function(){
	inirili();	
})
function inirili()
{
	itemid=$("#itemid").val();
	nian=$("#nian").val();
	yue=$("#yue").val();
	$("#nowstr").html(nian + " 年 " + yue + " 月 ");
	$.get("_rl.php?act=addcart&yy=" + nian + "&mm=" + yue + "&id=" + itemid + "&t="+new Date().getTime(),function(data){
		$("#rilicontent").html(data)
	});
}
function rilijia()
{
	itemid=$("#itemid").val();
	nian=$("#nian").val();
	yue=$("#yue").val();
	yue=parseInt(yue)+1;
	if(yue==13)
	{
		yue=1;
		nian=parseInt(nian)+1;		
	}
	$("#nian").val(nian)
	$("#yue").val(yue)
	$("#nowstr").html(nian + " 年 " + yue + " 月 ");
	$.get("_rl.php?act=addcart&yy=" + nian + "&mm=" + yue + "&id=" + itemid + "&t="+new Date().getTime(),function(data){
		$("#rilicontent").html(data)
	});
}
function rilijian()
{
	itemid=$("#itemid").val();
	nian=$("#nian").val();
	yue=$("#yue").val();
	yue=parseInt(yue)-1;
	if(yue==0)
	{
		yue=12;
		nian=parseInt(nian)-1;		
	}
	$("#nian").val(nian)
	$("#yue").val(yue)
	$("#nowstr").html(nian + " 年 " + yue + " 月 ");
	$.get("_rl.php?act=addcart&yy=" + nian + "&mm=" + yue + "&id=" + itemid + "&t="+new Date().getTime(),function(data){
		$("#rilicontent").html(data)
	});
}
</script> 
