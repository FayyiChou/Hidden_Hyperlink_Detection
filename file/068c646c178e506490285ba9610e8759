<script type="text/javascript">
    $(function () {
        var winhight = $(window).height() - 300; //当前窗口高度
        $('.sidenav').css('top', winhight / 2 + 'px');
        $(window).scroll(function () {
            var nowtop = parseInt($(window).scrollTop());
            $('.sidenav').css('top', nowtop + winhight / 2 + 'px');
        })
        $("#backtop").click(function () {
            $('body,html').animate({ scrollTop: 0 }, 1000);
            return false;
        });

        /* 产品搜索框焦点 */
        $(".s_input").focus(function () {
            if ($(".s_input").val().replace(/^\s+|\s+$/g, '') == '请输入产品名称') {
                $(".s_input").val('');
            }
        });
        $(".s_input").blur(function () {
            if ($(".s_input").val().replace(/^\s+|\s+$/g, '') == '') {
                $(".s_input").val('请输入产品名称');
            }
        });
    });

    function getsearch() {
        var keyword = document.getElementById("keyword").value;
        if (keyword != "" && keyword != "请输入产品名称") {
            window.location.href = "/search-1-" + keyword;
        }
    }
    $(document).keydown(function (e) {
        if (e.keyCode == 13) {
            getsearch();
        }
    });
</script>

