        //选项卡
        function setTab(name, cursel, n, start) {
            for (i = start; i < n; i++) {
                var menu = document.getElementById(name + i);

                var con = document.getElementById("con_" + name + "_" + i);
                if (i < 5) {
                    menu.firstChild.className = i == cursel ? "iboxmt_hover" : "";
                }
                else {
                    menu.className = i == cursel ? "pdtbg" : "";
                }
                con.style.display = i == cursel ? "block" : "none";
            }
        }
        $(function () {
            jQuery(".m_menu ul li").hover(function () {
                jQuery(this).children("div").slideDown(200);
                jQuery(this).children(".m_menu ul li a:first").attr("class", "m_fixed")
            }, function () {
                jQuery(this).children("div").slideUp(100);
                jQuery(this).children(".m_menu ul li a").attr("class", "")
            });
            $("#botton-scroll").jCarouselLite({
                btnNext: ".og_next",
                btnPrev: ".og_prev",
                visible: 5,
                scroll: 2
            });
            $(".picbox li a").fancybox();
            $(".picbox li a").fancybox();
        });

    </script>
