        $(function() {
            $("#headermorelink").click(function(e) {
                if ($("#morelinklist").is(":hidden")) {
                    $("#morelinklist").show();
                    return;
                }
                $("#morelinklist").hide();
            })
            $(document).click(function(event) {
                if (event.target && (event.target.id == "headermorelink" || $(event.target).parent().attr("id") == "headermorelink"))
                    return;
                $("#morelinklist").hide();
            })
        })
