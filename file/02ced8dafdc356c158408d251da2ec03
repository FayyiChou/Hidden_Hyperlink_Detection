<script>
    var currentDatabase = 'WF_HY'
    var SDomain = 'http://s.g.wanfangdata.com.cn/';
    var CommunityDomain = 'http://social.wanfangdata.com.cn/';
    var lastSearchTip = "在3,263,176篇论文中检索";
    var LibrarianDomain = "http://librarian.wanfangdata.com.cn/";
    var IsticDomain = 'http://istic.wanfangdata.com.cn/';
    var wfCookieName = 'WFKS.Auth';

    function getFromParam() {
        if (currentDatabase == "WF_QK")
            return "c.Periodical";
        if (currentDatabase == "WF_XW")
            return "c.Thesis";
        if (currentDatabase == "WF_HY")
            return "c.Conference";
        if (currentDatabase == "Cecdb")
            return "c.Institution";
        if (currentDatabase == "LocalChronicleItem")
            return "c.LocalChronicle";
        return "c." + currentDatabase;
    }
    function topSearch(e) {
        if ('True' == 'True') {
            return search(e, getFromParam());
        }
        return search(e);
    }
    $(function () {
        $.watermark.options.useNative = false; //不用placeholder
        TwoButton(currentDatabase);

        $("#queryBox").watermark(lastSearchTip);
        $("#queryBox").keypress(function (e) {
            if (e.which == 13)
                return topSearch(e);
        });

        $("#searchButton, #searchMagazine").click(function (event) {
            return topSearch(event);
        });
        try {
            autocomplete('http://trend.wanfangdata.com.cn/AutoComplete');
            $("#queryBox").bind("autocompleteselect", function (event, ui) {
                $("#queryBox").val(ui.item.value);
                topSearch(event)
            });
        } catch (ex) { }
        exportInit();
    });
</script>
            
