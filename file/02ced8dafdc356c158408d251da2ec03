<script type="text/javascript">
    $.cachedScript = function (url, callback) {
        try {
            var options = {
                dataType: "script",
                cache: true,
                url: url,
                success: callback
            }
            $.ajax(options);
        } catch (ex) { console.log('动态加载Script失败：' + ex) }
    }
    function getCookie(name) {
        var cookieArray = document.cookie.split("; ");
        var val = null;
        $(cookieArray).each(function () {
            var arr = this.split("=");
            if ($.trim(arr[0]) == name) { val = decodeURIComponent(arr[1]); }
        });
        return val;
    }
    function GetAuthContext() {
        var context = getCookie('WFKS.Auth');
        return eval('(' + context + ')');
    }

    function GetDisplayName(context, accountType, key) {
        for (var i = 0; i < context.Data.length; i++) {
            if (context.Data[i].Key == (accountType + "." + key + ".DisplayName"))
                return decodeURIComponent(context.Data[i].Value);
        }
        return key;
    }
    function getNotificationInfo(accountIdContainer) {
        var href = 'http://social.wanfangdata.com.cn/Blog/Comment.aspx';
        var cls = 'mailimg';
        try {
            $.getJSON('http://social.wanfangdata.com.cn/Services/NotificationService.ashx?callback=?', null,
                function (json) {
                    if (json != null && json.length > 0) {
                        var url = "";
                        switch (json[0].Type) {
                            case "Comment": url = "Comment.aspx"; break;
                            case "Forward": url = "Forward.aspx"; break;
                            case "Chat": url = "ChatHistory.aspx"; break;
                            default: url = "Fans.aspx"; break;
                        }
                        href = 'http://social.wanfangdata.com.cn/Blog/' + url;
                        cls = 'mailinfor';
                    }
                    accountIdContainer.prepend('<a id="notification" class=' + cls + ' href=' + href + '></a>|')
                });
        } catch (ex) {
        }
    }
    function GetAccountIdObj(accountIdStr) {
        var accountId = { AccountType: '', Key: '' },
        index = accountIdStr.indexOf(".");
        if (index === -1) {
            accountId.AccountType = accountIdStr;
        } else {
            accountId.AccountType = accountIdStr.substring(0, index);
            accountId.Key = accountIdStr.substring(index + 1);
        }
        return accountId;
    }
    function refreshloginStatus() {
        var accountIdContainer = $('#accountIdContainer').html('');
        var register = '<a href="http://login.wanfangdata.com.cn/Register/index"><t>注册</t></a>| ';
        var charge = '<a href="http://tran.wanfangdata.com.cn/Charge.aspx"><t>充值</t></a>| ';
        var guide = '<a href="http://g.wanfangdata.com.cn/NewGuide" target="_blank"><t>帮助</t></a>|<a href="http://www.sojump.com/jq/6334684,i,t.aspx" target="_blank" style="color:red">满意度调查</a>|';
        var home = '<a style="color:#5471AC;text-decoration:none;" target="_blank" href="http://www.wanfang.com.cn"><t>公司首页</t><img align="absmiddle" src="http://g.wanfangdata.com.cn/share/Images/blog_link.gif" style="margin-left:2px;"></a>';
        var loginurl = 'http://login.wanfangdata.com.cn/Login.aspx';
        var location = document.location.href.toLocaleLowerCase();
        if (location.indexOf('http://login.wanfangdata.com.cn/'.toLocaleLowerCase()) == -1 || location.indexOf('returnurl') == -1) {
            loginurl += '?ReturnUrl=' + encodeURIComponent(document.location.href);
        }
        var login = '<a id="login" href="' + loginurl + '"><t>登录</t></a>| '

        var authContext = GetAuthContext();
        if (authContext == null || authContext.Context.AccountIds.length == 0) {
            accountIdContainer.append(login).append(register).append(charge).append(guide).append(home);
            return;
        }

        var showNotify = false,
	    showOrder = false,
        personId = null,
        magazineId = null,
        researcherId = null,
        groups = [],
        orderUrl='';
        $(authContext.Context.AccountIds).each(function () {
            var _this = GetAccountIdObj(this);
            if (_this.AccountType == "Person") {
                personId = _this.Key;
            }
            if (_this.AccountType == "Researcher" || _this.AccountType == "VerifiedUser") {
                researcherId = _this.Key;
                showNotify = true;
            }
            if (_this.AccountType == "MagazineInstitution") {
                magazineId = _this.Key;
                showNotify = true;
            }
            if (_this.AccountType == "Group") {
                var displayName = GetDisplayName(authContext.Context, _this.AccountType, _this.Key);
                groups.push({ Key: _this.Key, Name: displayName });
            }
            if (_this.AccountType == "IsticTimeLimit" || _this.AccountType == "IsticBalanceLimit") {
                showOrder = true;
                orderUrl='http://istic.wanfangdata.com.cn/Order';
            }
        });
        //以下按显示顺序添加
        if (showNotify) {
            getNotificationInfo(accountIdContainer);
        }
        if (personId != null) {
            var space = '';
            if (personId == magazineId) {
                var displayName = GetDisplayName(authContext.Context, 'MagazineInstitution', magazineId)
                space = '(<a id="MagazinespaceLink" href="http://social.wanfangdata.com.cn/Blog/Blog.aspx?Id=' + encodeURIComponent(displayName) + '">机构空间</a>)';
            } else if (personId == researcherId) {
                space = '(<a id="personspace" href="http://social.wanfangdata.com.cn/Home.aspx?Id=' + encodeURIComponent(personId) + '">个人空间</a>)';
                //显示弹出框
                $.cachedScript('http://g.wanfangdata.com.cn/share/Script/correlateScholarPopup-1.1.js', function () {
                    if ($.isFunction(showCorrelateScholar)) { showCorrelateScholar(personId, 'http://social.wanfangdata.com.cn/'); }
                });
            }
            accountIdContainer.append('<a href="http://g.wanfangdata.com.cn/UserService" target="_self" id="personAccountKey">' + personId + '</a>' + space+'|');
        }
        if (groups.length == 1) {
            accountIdContainer.append('<a href="http://g.wanfangdata.com.cn/Organization?userId=' + groups[0].Key + '" id="groupAccountKey">' + groups[0].Name + '服务站</a> ');
	    if (showOrder) {
            accountIdContainer.append('(<a href="'+orderUrl+'" target="_blank">查询订单</a>)| ');
            }else{
            accountIdContainer.append('| ');
            }
        }
        if (groups.length > 1) {
            var clickCount = 0;
            var options = $('<select id="groupAccountList"></select>').click(function () {
                if (clickCount++ > 0) {
                    document.location = 'http://g.wanfangdata.com.cn/Organization?userId=' + options.val();
                }
            }).focus(function () {
                clickCount = 0;
            });
            $.each(groups, function () {
                options.append($('<option value=' + this.Key + '>' + this.Name + '</option>'));
            });
            accountIdContainer.append(options);
	    if (showOrder) {
            accountIdContainer.append('(<a href="'+orderUrl+'" target="_blank">查询订单</a>)| ');
            }else{
            accountIdContainer.append('| ');
            }
        }
        accountIdContainer.append(login).append('<a href="http://login.wanfangdata.com.cn/Logout.aspx" target="_self"><t>退出</t></a>| ');

        if (personId != null) {
            accountIdContainer.append(charge)
        }
        accountIdContainer.append(guide).append(home);
    }
    try {
        refreshloginStatus();
    } catch (ex) { }
</script>
