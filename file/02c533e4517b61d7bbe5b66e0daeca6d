  if ($('.mod-page-switcher').length == 0) {
      var eye = '<br/><a href="http://www.17173.com/" target=_blank><img src="http://i2.17173.itc.cn/2011/news/main/eye02.gif"></a>  <a href="http://' + window.location.host + '">返回专区首页>></a>   <a href="http://www.17173.com/?pos_ref=zq_b">返回17173首页>></a>';
      if ($('.content :last').is('center')) {
        $('.content :last').after(eye);
      } else {
        if ($('.content p:last:has(img)').length != 0) {
          if ($('.content p:last').text() == '') {
              eye = '<br/>' + eye;
          } else {
              if ($('.content p:last').find('img').parent().is('a')) {
                  $('.content p:last').find('img').parent().after('<br/>');
              } else {
                  $('.content p:last').find('img').after('<br/>');
              }
          }
        }
        $('.content p:last').append(eye);
      }
  }
