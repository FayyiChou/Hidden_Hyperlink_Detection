var   MyMar1=setInterval(Marquee,speed);
document.getElementById("qiye").onmouseover=function(){clearInterval(MyMar1)};
document.getElementById("qiye").onmouseout=function(){MyMar1=setInterval(Marquee,speed)};
</script>
