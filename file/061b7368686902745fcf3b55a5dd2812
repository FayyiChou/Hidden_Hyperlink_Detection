//鼠标经过、离开区域上时，显示、隐藏城市
	$(".activi_dj_n").hover(function(){
		$(".activi_dj_n").addClass("borcss");
		$(".activi_dj_p").show();
	},function(){
		$(".activi_dj_n").removeClass("borcss");
		$(".activi_dj_p").hide();
	});
	$(".activi_dj_p").hover(function(){
		$(".activi_dj_n").addClass("bgs");
		$(".activi_dj_p").show();
	},function(){
		$(".activi_dj_n").removeClass("bgs");
		$(".activi_dj_p").hide();
	});
</script>
