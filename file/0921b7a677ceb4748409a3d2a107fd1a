		$(function(){
			$(".lhtop a").mouseenter(function(){
				$(".lhtop a").removeClass("on");
				$(this).addClass("on");
				var t=$(this).index();
				$(".lhctn ul").hide();
				$(".lhctn ul").eq(t).show();
			})
		})
