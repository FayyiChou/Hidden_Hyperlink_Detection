          document.getElementById('popular-index-'+popularIndex).style.backgroundImage='url(image/popular-index-bg-active.gif)';
		  document.getElementById('popular-index-'+popularIndex).style.color='#fff';
          document.getElementById('popular-pic').src=populars[popularIndex].image;
		  document.getElementById('popular-href').setAttribute("href",populars[popularIndex].href);
		  //alert(document.getElementById('popular-href').getAttribute("href"));
		  document.getElementById('popular-href').href = populars[popularIndex].href;
		  document.getElementById('popular-href').target = "_blank";
          document.getElementById('popular-title').innerHTML = "<a href="+populars[popularIndex].href+" style='color:#FFF' target='_blank'>"+populars[popularIndex].title+"</a>";
          popularIndex++;
          if(con){
            setTimeout('showPopular('+popularIndex+')',popularDelayTime);
          }
        }
        showPopular(true);
