        function showPopular(con){
          if(popularIndex==populars.length){
            popularIndex = 0;
          }
          for(var i=0;i<populars.length;i++) {
            document.getElementById('popular-index-'+i).style.backgroundImage='url(image/popular-index-bg-inactive.gif)';
			document.getElementById('popular-index-'+i).style.color='#333';
