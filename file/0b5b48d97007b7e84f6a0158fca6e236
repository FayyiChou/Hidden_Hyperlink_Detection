    $(function () {
        var xuanzeCount = $('.xuanze').length;//大于2，就说明有多个规格选项  
        if (xuanzeCount <= 2) { 
            $("#sel_color li").removeClass("can-not").each(function () {
                var $thisc = $(this);
                var valc = $thisc.children("span").text();
                var okcolors = specFiter_One(valc);
                if (!okcolors) {
                    $thisc.addClass("can-not");
                }
            });
            $("#sel_size li").removeClass("can-not").each(function () {
                var $thisc = $(this);
                var valc = $thisc.children("span").text();
                var oksizes = specFiter_One(valc);
                if (!oksizes) {
                    $thisc.addClass("can-not");
                }
            });
        }
