<script>
document.write('<div id=img999 style="z-index: 2000; left: 2px; width: 59px; position: absolute; top: 43px; height1: 61px; visibility: visible;">');
document.write('<a  target="_blank"><img src="dbsource/4814199/19297499.jpg" border=0></a></div>');
var xPos1 = 0;
var yPos1 = 100; 
var step1 = 1;
var delay1 = 30; 
var height1 = 0;
var Hoffset1 = 0;
var Woffset1 = 0;
var yon1 = 0;
var xon1 = 0;
var pause1 = true;
var interval;
var c = 1;
img999.style.top = yPos1;
function changePos1() 
{
width = document.body.clientWidth;
height1 = 800;
Hoffset1 = img999.offsetheight;
Woffset1 = img999.offsetWidth;
img999.style.left = xPos1 + document.body.scrollLeft +"px";
img999.style.top = yPos1 + document.body.scrollTop + "px";
if (yon1) 
{
yPos1 = yPos1 + step1;
}
else 
{yPos1 = yPos1 - step1;}
if (yPos1 < 0) 
{yon1 = 1;yPos1 = 0;}
if (yPos1 >= (height1)) 
{yon1 = 0;yPos1 = (height1);}
if (xon1) 
{xPos1 = xPos1 + step1;}
else 
{xPos1 = xPos1 - step1;}
if (xPos1 < 0) 
{xon1 = 1;xPos1 = 0;}
if (xPos1 >= (width - Woffset1)) 
{xon1 = 0;xPos1 = (width - Woffset1); }
}
function start1()
{
img999.visibility = "visible";
interval = setInterval('changePos1()', delay1);
}
function pause_resume1() 
{
if(pause1) 
{
clearInterval(interval);
pause1 = false;}
else 
{
interval = setInterval('changePos1()',delay1);
pause1 = true; 
}
}
start1();
</script>
