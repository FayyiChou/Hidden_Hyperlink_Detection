<script>
$(".loadImg").scrollLoading();
$(function(){
	$(".QR").hover(function(){
		$(this).find(".QRcode").fadeIn(100);
		$(this).find("a").css("color","#ff0000");	
	},function(){
		$(this).find(".QRcode").fadeOut(100);
		$(this).find("a").css("color","#5f5c5c");			
	})	
	$(".parents").hover(function(){
		$(this).find(".children").fadeIn(100);
		$(this).find("a").css("color","#ff0000");	
	},function(){
		$(this).find(".children").fadeOut(100);
		$(this).find("a").css("color","#5f5c5c");			
	})
})

$(function(){
	$('#close').click(function(){
		$(this).hide();
		$('#adZhe').hide();
		$('#ad1').hide();
		$('#yuan').show();	
		
	})	
	$('#qad').click(function(){
		$(this).hide();
		$('#adZhe').show();
		$('#ad1').show();
		$('#close').show();	
		
	})
})
</script> 

