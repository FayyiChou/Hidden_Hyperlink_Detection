		<script language='javascript'>
		function validate(frm) {
		var err = "";
		if (isEmpty(frm.LogonName.value)) { 
			err += "User ID\n"
		}
		if (isEmpty(frm.Password.value)) { 
			err += "Password\n"
		}
		if (!isEmpty(err)) { 
			err = "Error!\nThe following required fields are empty or invalid:\n\n" + err;
			alert(err);
			return false;
		} else {
			return true;
		}
		}
		</script>              	
