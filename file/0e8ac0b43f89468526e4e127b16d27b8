<script>
  var speed_linkpic=10//速度数值越大速度越慢
  demo5.innerHTML=demo4.innerHTML
  function Marquee_linkpic(){
  if(demo5.offsetWidth-demo3.scrollLeft<=0)
  demo3.scrollLeft-=demo4.offsetWidth
  else{
  demo3.scrollLeft++
  }
  }
  var MyMar_linkpic=setInterval(Marquee_linkpic,speed_linkpic)
  demo3.onmouseover=function() {clearInterval(MyMar_linkpic)}
  demo3.onmouseout=function() {MyMar_linkpic=setInterval(Marquee_linkpic,speed_linkpic)}
          </script> 
