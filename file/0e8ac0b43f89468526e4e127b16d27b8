<script type="text/javascript">	
var titles = "";
var imgs = "";
var urls = "";
function addtitle(title){
	if(titles!=""){
		titles+="|";
	}
	titles+=title;
}

function addphotourl(photourl){
	if(imgs!=""){
		imgs+="|";
	}
	imgs+=photourl;
}

function addlink(link){
	if(urls!=""){
		urls+="|";
	}
	urls+=link;
}


addtitle("凤台一中2016年全国中小学骨干…");
addphotourl("/news/UploadFiles_9772/201608/2016080410391383.jpg");
addlink("/news/ShowArticle.asp?ArticleID=837");

addtitle("县委李大松书记调研我校新校区…");
addphotourl("/news/UploadFiles_9772/201607/2016072617045384.jpg");
addlink("/news/ShowArticle.asp?ArticleID=832");

addtitle("凤台一中召开2016年第一次教职…");
addphotourl("/news/UploadFiles_9772/201607/2016071316341796.jpg");
addlink("/news/ShowArticle.asp?ArticleID=825");

addtitle("凤台一中西校区“直饮水系统”…");
addphotourl("/news/UploadFiles_9772/201607/2016071311155069.jpg");
addlink("/news/ShowArticle.asp?ArticleID=824");

addtitle("凤台一中北师大骨干教师研修班…");
addphotourl("/news/UploadFiles_9772/201607/2016070515044211.jpg");
addlink("/news/ShowArticle.asp?ArticleID=815");

					var speed = 4000;					
					var sohuFlash2 = new sohuFlash("/images/focus0414a.swf","flashcontent01","275","205","8","#ffffff");
sohuFlash2.addParam("quality", "medium");
sohuFlash2.addParam("wmode", "opaque");	
sohuFlash2.addVariable("speed",speed);
						sohuFlash2.addVariable("p",imgs);	
						sohuFlash2.addVariable("l",urls);
						sohuFlash2.addVariable("icon",titles);
						sohuFlash2.write("flashcontent01");
					</script></td>
