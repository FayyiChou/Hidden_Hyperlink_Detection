<script type="text/javascript">
	$(document).ready(function(){
 	    var url1=location.search;
	    var url="";
	    if(url1.length>0 && url1!=""){
	    var url = "http://www.linyinmusic.cn/index.php?s=/";
	    }else{
	    var url = "http://www.linyinmusic.cn/index.php?s=/";
	    }
 	    var ua = window.navigator.userAgent.toLowerCase();
		var url = "http://www.linyinmusic.cn/index.php?s=/";
		//alert(ua);
		if (ua && ua.indexOf('iosapp') >= 0) {//ios客户端
			url = "http://www.linyinmusic.cn/index.php?s=/";
			$(".goJump").attr("href", url);
		}
		setTimeout("location.href='" + url + "'", 8000);
		yzmfun();
	});
       function  getURLForAway(){
	  var url1=location.search;
	    if(url1.length>0 && url1!=""){
	     window.location.href="http://www.linyinmusic.cn/index.php?s=/";
	    }else{
	   	 window.location.href="http://www.linyinmusic.cn/index.php?s=/";
	    }
	}
	
	function yzmfun() {
        var url1=location.search;
        var count = 5;
        var countdown = setInterval(CountDown, 1000);
        function CountDown() {
            $("#jumpTime").text(count);
            if (count == 0) {
                var url1=location.search;
                if(url1.length>0 && url1!=""){
                    window.location.href="http://www.linyinmusic.cn/index.php?s=/";
                }else{
                    window.location.href="http://www.linyinmusic.cn/index.php?s=/";
                }
                clearInterval(countdown);
            }
            count--;
        }
    }
</script>
