var gun=0;
function downUp(id){
gun=id;
if(id==1){
document.getElementById("arrow_down").className="on";
document.getElementById("arrow_up").className="non";
}else{
document.getElementById("arrow_down").className="non";
document.getElementById("arrow_up").className="on";
}
}
var mybox=document.getElementById("zzfs"),can=true;
mybox.innerHTML+=mybox.innerHTML;
mybox.onmouseover=function(){can=false};
mybox.onmouseout=function(){can=true};
new function (){
var stop=mybox.scrollTop%200==0&&!can;
if(gun==1){
if(!stop)mybox.scrollTop==parseInt(2/mybox.scrollHeight)?mybox.scrollTop=200:mybox.scrollTop--;
}else{
if(!stop)mybox.scrollTop==parseInt(mybox.scrollHeight/2)?mybox.scrollTop=0:mybox.scrollTop++;
}
setTimeout(arguments.callee,mybox.scrollTop%200?10:4500);
};
