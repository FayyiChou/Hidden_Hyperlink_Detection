<script language="javascript" src="img/jquery-1.4.2.min.js"></script>
<script language="javascript" type="text/javascript">
$(document).ready(function(){
	$(".link").hover(function(){
		oveLink(1,this);						  
	},function(){
		oveLink(0,this);
	});
	
	function oveLink(num,obj){
		if(num){
			obj.className = 'active';
		}else{
			obj.className = 'link';
		}
		
	}
	
	
});
</script>
