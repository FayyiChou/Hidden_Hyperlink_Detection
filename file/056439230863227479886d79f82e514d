    <script>
    $(function () {
        setRenderFullScreen($('#view_banner_32_843'));
        var bannerPostion_view_banner_32_843 = $('#view_banner_32_843').css('position');
        var $banner_view_banner_32_843 = $('#view_banner_32_843');
        var bannerTop_view_banner_32_843 = $banner_view_banner_32_843.position().top;
        var bannderBottom_view_banner_32_843=$banner_view_banner_32_843.css('bottom');
        if (bannerPostion_view_banner_32_843 === 'fixed' &&
            bannerTop_view_banner_32_843 == 0 &&
            bannderBottom_view_banner_32_843 === 'auto') {
            $('#fullScreen_view_banner_32_843').css('position', 'fixed');
        } else {
            if (bannerPostion_view_banner_32_843 === 'fixed' &&bannerTop_view_banner_32_843 !== 0 && bannderBottom_view_banner_32_843 === '0px') {
                $('#fullScreen_view_banner_32_843').css({'position':'fixed','top':'auto','bottom':'0px'});
            }
        }
    });
</script>


