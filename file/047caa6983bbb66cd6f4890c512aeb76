<script>
    function calogin() {
        var top, left, Width, Height;
        left = 0;
        top = 0;
        Width = window.screen.width - 6;
        Height = window.screen.height - 70;
        var win = window.open("https://ca.hngp.gov.cn", "CZTMainWindow", "left=" + left + ",top=" + top + ",width=" + Width + ",height=" + Height + ",toolbar=no,location=no,directories=no,status=no,fullscreen=0,menubar=no,scrollbars=no, resizable=yes");
        win.focus();
    }
</script>
