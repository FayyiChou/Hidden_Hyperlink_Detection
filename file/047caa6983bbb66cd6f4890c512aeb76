        //漂浮窗口广告
        $("div[id^='AD2']").each(function () {
            var gg = $(this);
            $(this).css("display", "block");
            new AdMove($(this).attr("id")).Run();
            $(this).find(".adClose").click(function () {
                gg.hide();
            });
        });

        //边栏浮动
        var opt_banner = {
            top: 300
        };

