            $(this).scrollBox(opt_banner);
            //$(this).find(".adClose").click(function () {
            //    gg.hide();
            //});
        });
	//通知
        var tzgg = $("div.tzgg");
        var tzgg_close = $(".tzgg_close");

        var window_w = $(window).width();
        if (window_w > 700) {
            tzgg.show();
        }
        $(window).scroll(function () {
            var scrollTop = $(window).scrollTop();
            tzgg.stop().animate({top: scrollTop + 340});
        });
        //tzgg.scrollBox(opt_banner);
        tzgg_close.click(function () {
            $(".tzgg").hide();
            return false;
        });


//地市地图
        swfobject.embedSWF("/resources/images/map.swf", "swf_map", "208", "160", "9.0.0", "/resources/scripts/expressInstall.swf", {}, params, attributes);

        $.get("/henan/online?sessionId=0c344d21af8943d3aa2e8785f81ccf47", function (data) {
            $("#online").html(data);
        });
        $('#appSelect').change(function () {
            var p = $(this).children('option:selected').val();
            if (p.length > 2)
                window.location.href = p;
        });
        //屏蔽IE bug
        window.onerror = function () {
            return true
        };

    });
