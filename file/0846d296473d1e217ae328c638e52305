function checkvl(str){
	if(str==1 || str==2){
		document.getElementById('ja').style.display='';
		document.getElementById('jb').style.display='none';
		document.getElementById('jc').style.display='none';
	}
	if(str==3){
		document.getElementById('ja').style.display='none';
		document.getElementById('jb').style.display='';
		document.getElementById('jc').style.display='none';
	}
	if(str==4){
		document.getElementById('ja').style.display='none';
		document.getElementById('jb').style.display='none';
		document.getElementById('jc').style.display='';
	}
}
