<script type="text/javascript">
	$(function(){
		if( $.browser.msie && $.browser.version == 6 ){
			$('#online-service').css( 'position', 'absolute' );
			setInterval( function(){
				var wst = $(window).scrollTop() + 100;
				$('#online-service').css( 'top', wst + 'px' );
			}, 20);
		}
	})
</script>
<!--banner-->
<script language="javascript" type="text/javascript">
			var timeout         = 3000;
			var closetimer		= 0;
			var bannerList = [];
			
			$(function(){
				// 分类菜单效果
				$('.modelMenuFirst').hover(function(){
					$(this).addClass("modelMenuFirst-hover");
				}, function(){
					$(this).removeClass("modelMenuFirst-hover");
				});
				
				$('#Category').on("click", ".modelMenuFirst", function(){
					$('.modelMenuDiv').slideUp('fast');
					if ( $(this).hasClass('modelMenuFirst-check') ){
						$(this).removeClass('modelMenuFirst-check');			
					} else {
						$('.modelMenuFirst').removeClass('modelMenuFirst-check');
						$(this).addClass('modelMenuFirst-check');
						$(this).next('div').slideDown('fast');
					}
				});
				
				//banner start		
				closetimer = window.setTimeout(display, timeout);
				$("#BannerBox").mouseover(function(){
					window.clearTimeout(closetimer);
				}).mouseleave(function(){
					closetimer = setTimeout(display, timeout);
				});
				$("#bannerLinkImg").mouseover(function(){
					window.clearTimeout(closetimer);
				}).mouseleave(function(){
					closetimer = setTimeout(display, timeout);
				});	
				initBannerNav();
				display();
				$("#bannerLinkImg").show();
				//banner end
			})
			
			
			function bannerShow(id)
			{
				$(".Bannernav").removeClass("BannernavHover");
				$("#Bannernav" + id + "").addClass("BannernavHover");
				for (var i = 0; i < bannerList.length; ++i) {
					var banner = bannerList[i];
					if (banner.id == id) {
						$("#bannerLinkImg").attr("src",banner.imgUrl);
						$("#bannerLinkImg").show();
						$("#bannerLink").attr("href", banner.linkUrl);
						$("#bannerLink").attr("title", banner.title);
					}
				}
				
				window.clearTimeout(closetimer);
				closetimer = window.setTimeout(display, timeout);
			}
			
			var bannerIndex = 0;
			function display()
			{
				var c = 0;
				bannerIndex++;
				if(bannerIndex>c)bannerIndex=1;
				bannerShow(bannerIndex);
			}
			function initBannerNav(){
				var c = 0;
				if(c<=1){
					return ;//只有一个  没必要显示导航了
				}	
				for( var i=c; i > 0; i-- ){
					 var bannernav = $('<div id="Bannernav'+ i +'" class="Bannernav" onClick="bannerShow('+ i +')"></div>');
					 if(1 == i){
						 bannernav.addClass('BannernavHover');
					 }
					 $('.banner_nav').append(bannernav);
				}		
			}
				
</script>
</html>
