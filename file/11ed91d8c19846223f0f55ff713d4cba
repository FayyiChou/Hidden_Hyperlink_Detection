$(document).ready(function(){
	$("#ssoLBtn").click(function(){
		$(this).addClass("selt c4");
		$("#ydLBtn").removeClass("selt c4");
		$("#ssoLoginFrame").show();
		$("#ydLoginFrame").hide();
		
	});

	$("#ydLBtn").click(function(){
		$(this).addClass("selt c4");
		$("#ssoLBtn").removeClass("selt c4");
		$("#ydLoginFrame").show();
		$("#ssoLoginFrame").hide();
		createFTUCtrl();
		
	});
	
});
