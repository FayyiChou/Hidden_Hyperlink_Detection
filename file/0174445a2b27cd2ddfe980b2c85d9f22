<script src="http://ivy.pconline.com.cn/adpuba/show3?id=pc.luntan.nr.network.gcbtwz."></script>
<script>
(function(){
	var shuffle = function(v){
		for(var j, x, i = v.length; i; j = parseInt(Math.random() * i), x = v[--i], v[i] = v[j], v[j] = x);
		return v;
	};
	var inArray = function(arr, value){
	    for (var i = 0; i < arr.length; i++) {
	        if (arr[i] === value) {
	            return true;
	        }
	    }
	    return false;
	}
	var randomItem = function(arr,flength){
		var rlt = Math.floor(Math.random()*flength);
		if(inArray(arr,rlt)){
			return randomItem(arr,flength);
		}else{
			return rlt;
		}
	}
	var randomFloor = function(num,flength){
		var rlt = [];
		for (var i = 0; i < num; i++) {
			rlt.push(randomItem(rlt,flength));
		};
		return rlt;
	};
	var insertAd = function(ad,floor){
		var adHTML = toolJs.domGenerator(''+ad.src);
		pc.insertAfter(pc.getElem(".floorname",floor),adHTML);
	}
	var floors = [];
	pc.each(pc.getElems(".floor"),function(floor){
		if(!pc.hasClass(floor,"floor-lock") && !pc.hasClass(floor,"top-reply")){
			floors.push(floor);
		}
	})
	if(floors.length > 0){
		var floorNum = floors.length;
		var adList = getLocationAd('pc.luntan.nr.network.gcbtwz.',10);
		var adNum = adList.length;
		if(adNum > 0){
			if(adNum >= floorNum){
				adList = shuffle(adList);
				pc.each(floors,function(floor,index){
					var adFloor = adList[index];
					insertAd(adFloor,floor);
				})
			}else{
				var adFloor = randomFloor(adNum,floorNum);
				pc.each(adFloor,function(value,index){
					var floor = floors[value];
					var adFloor = adList[index];
					insertAd(adFloor,floor);
				})
			}
		}
	}
})();
</script>

