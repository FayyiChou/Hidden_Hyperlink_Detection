<script type="text/javascript">
function log_check()
{
 if(form1.u.value=="")
 {
  alert("您还没有输入账号！ ");
  form1.u.focus();
  return false;
}
 if(form1.p.value=="")
 {
  alert("您还没有输入密码！ ");
  form1.p.focus();
  return false;
 }
}
</script>
