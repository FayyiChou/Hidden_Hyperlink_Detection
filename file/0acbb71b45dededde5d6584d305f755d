			<script>
				var loginsubmitflag = 0;
				
				function forgetpwdcallback(data){
					$.unblockUI();
					var html = "";
					if(data.ret == "err"){
						html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' /> "+LABEL_systemerr+"</h1></div>";
					}else if(data.ret == "ok"){
						html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' />"+DLG_forgetpwdok+"</h1></div>";					    
					}else{
						html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' /> "+DLG_forgetpwdcon+"</h1></div>";					    
					}
					$.blockUI({ message: html});
					setTimeout($.unblockUI, 3000);
				}
				
				function forgetpwd(){
					var html = '<div id="global_topdiv" class="dlgtip"><div class="tipdiv_title" ><h4>'+DLG_forgetpwd+'</h4></div>\
					<p style="margin:10px;">'+ DLG_forgetpwdtip +' </p><br><div style="maigin:10px auto;margin-left:20px;width:330px!important;">\
					<div stlyle="width:100%;"><div style="width:100px;text-align:right;display:inline-block;">用户名:&nbsp;&nbsp;</div><input type=text class="normalinput" id="pwdusername"></div><br><div stlyle="width:100%;"><div style="width:100px;text-align:right;display:inline-block;">Email:&nbsp;&nbsp;</div><input type=text class="normalinput" id="pwdemail"></div><br><div style="width:100px;text-align:right;display:inline-block;">验证码:&nbsp;&nbsp;</div><input type="text" style="width:80px"  name="Checknum" id="pwdChecknum"  tip="看不清楚？点击图片，换一张">&nbsp;&nbsp;<img id="cheimg" onclick=changImg("cheimg") style="vertical-align:bottom" src ="http://www.okkouyu.com/okApp/checkcode.action" width="90" height="35" title="看不清楚？点击图片，换一张"></div>\
					</div><br/><div class="global_topdiv_bottom"><span class="50span"><a class="normalbtn" id="submitbtn">'+LABEL_submit+'</a></span><span class="50span"><a class="normalbtn" id="cancelbtn">'+LABEL_cancel+'</a></span></div></div>';
					
					$.blockUI({ message: html,css: { border: 'none', width:'400px', height:'300px', cursor:'normal' }});
					
					
					$("#submitbtn").click(function(){
						var pwdusername = $("#pwdusername").val();
						var pwdemail = $("#pwdemail").val();
						var checknum = $("#pwdChecknum").val();
						console.log(pwdusername + ":" + pwdemail +":"+ checknum);
						if(pwdusername.length < 1 || pwdemail.length < 1 || checknum.length != 4){
							$.unblockUI();
						    var html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' />"+DLG_forgetpwdtip+"</h1></div>";
						    $.blockUI({ message: html});
						    setTimeout($.unblockUI, 3000);
							return;	
						}
						GLOBAL_rpcUrl = "/okApp";
						var rpcCall = $.extend(RpcCall,{
					 		async: true,
					 		action: 'login.action',
					 		method: 'RestorePwd',
					 		params: [pwdusername, checknum, pwdemail],
					 		beforeLoad: function(){
							},
					 		processRequestData: function(data){
					 			forgetpwdcallback(data);
					 		},
					 		 processRequestError: function(data){
						    	console.log(data);
						    },
						});
					    rpcCall.request();
					    $.unblockUI();
					    var html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' /> Just a moment...</h1></div>";
					    $.blockUI({ message: html});
					});
					
					$("#cancelbtn").click(function(){
						$.unblockUI();
					});
				}
				
				
				function changImg(id){
					document.getElementById(id).src ='http://www.okkouyu.com/okApp/checkcode.action?r=' + Math.random();
				}
function loginsubmit(){
	var html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' />"+LABEl_logining+"</h1></div>";
	$.blockUI({ message: html});
	GLOBAL_rpcUrl = "/okApp";
	var rpcCall = $.extend(RpcCall,{
		 		async: true,
		 		action: 'login.action',
		 		method: 'Login',
		 		params: [$("#username").val(), $.md5($("#pwd").val()), $("#Checknum").val()],
		 		beforeLoad: function(){
				},
		 		processRequestData: function(data){
		 			if(data.ret == "pwd"){
		         		$("#pwd").attr("class","tooltipinputerr");
		          		$("#pwd").val("");
		          		$("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_pwderr+'</h1>');
			        }else if(data.ret == "checknum"){
				        $("#Checknum").attr("class","tooltipinputerr");
				        $("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_checknumerr+'</h1>');
			        }else if(data.ret == "userlock"){
				     	 $("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_userlock+'</h1>');
			        }else if(data.ret == "ok"){
				    	$("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_loginok+'</h1>');
						
						setTimeout(function(){
							$.unblockUI();
							window.location = 'http://www.okkouyu.com/okApp/uc/doing.action';
						}, 3000);
				        return;
			        }else{
				        $("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEL_systemerr+'</h1>');
			        }
			         setTimeout(function(){
						$.unblockUI();
					 }, 3000);
		 		},
		 		 processRequestError: function(data){
			    	console.log(data);
			    },
			});
	rpcCall.request();
}


function loginclick(){
	$("#regdlg").empty();
	$("#regdlg").css("left",-400);	
	if($(".logindlgtitle").length==0){
		var html = '<div class="logindlgtitle"><a class="logindlgclose" href="javascript:closeclick();"></a></div><div class="logindlgcontent">';
		html += '<form name="loginform">	<ul class="loginul">';
		html += '<li><div class="opinfo"><label>用户名:&nbsp;&nbsp;</label></div><input  type="text"  name="username" id="username" tip="用户名" check="2" ></li>';
		html += '<li><div class="opinfo">密码:&nbsp;&nbsp;</div><input type="password"  name="pwd" id="pwd" tip="请输入5-20个字母开头，后带字母、数字的密码(必输入项)" check="2" ></li>';
		html += '<li><div class="opinfo">验证码:&nbsp;&nbsp;</div><input type="text" style="width:80px"  name="Checknum" id="Checknum"  tip="看不清楚？点击图片，换一张">&nbsp;&nbsp;<img id="cheimg" onclick=changImg("cheimg") style="vertical-align:bottom" src ="http://www.okkouyu.com/okApp/checkcode.action" width="90" height="35" title="看不清楚？点击图片，换一张">';
		html += '</li><li style="margin-bottom:10px;"><a id="loginsubmit" onclick="loginsubmit();" class="normalbtn" style="width:80px;color:#FFF;">提交</a>&nbsp;&nbsp;<a onclick="forgetpwd()">忘记密码?</a> &nbsp;&nbsp;</li></ul></form></div>';
		

		
		$("#logindlg").html(html);
		
		setUserName("username");  
		setPasswordCheck("pwd");
		setVerifyCodeCheck("Checknum");

		$('select[tip],select[check],input[tip],input[check],textarea[tip],textarea[check]').tooltip({formname:document.login1form, btnid:"loginsubmit"});
	}
	var left = $("#loginactid").offset().left;
	var top = $("#loginactid").offset().top;
	var width = $("#loginactid").width();

	$(".logindlgtitle").height($("#loginactid").height() + 1);
	left = left + width;
	$("#logindlg").css("z-index",101);
	$("#logindlg").animate({'left': left +'px', 'top': top+"px", 'opacity':'1'}, "slow");
	//$("#hide_div").animate({'left': '0px','opacity':'1'},"slow");
}
function closeclick(){
	$("#logindlg").stop(true).animate({'left':'-400px','opacity':'0'},800);
}


function regsubmit(){
	if($(".tooltipinputerr").length > 0){
		return;	
	}
	var html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' />"+LABEl_reging+"</h1></div>";
	$.blockUI({ message: html});
	/*
	var mobile = $("#user_mobile").val();
	if(mobile == null || mobile.length < 11){
		html = "<div id='global_topdiv' class='blocktip'><h1><img src='/okApp/images/busy.gif' />"+LABEl_mobilecon+"</h1></div>";
		$.blockUI({ message: html});
		setTimeout(function(){
			$.unblockUI();
	 	}, 3000);
		return;
	}
	*/
	
	GLOBAL_rpcUrl = "/okApp";
	var rpcCall = $.extend(RpcCall,{
		 		async: true,
		 		action: 'login.action',
		 		method: 'Reg',
		 		params: [$('input[name="roleid"]:checked').val(), $("#username").val(), $.md5($("#password").val()), $.md5($("#check_passwd").val()), $("#Checknum").val(), $("#email").val()],
		 		beforeLoad: function(){
				},
		 		processRequestData: function(data){
		 			if(data.ret == "pwd"){
		         		$("#pwd").attr("class","tooltipinputerr");
		          		$("#pwd").val("");
		          		$("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_pwdcon+'</h1>');
			        }else if(data.ret == "checknum"){
				        $("#Checknum").attr("class","tooltipinputerr");
				        $("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_checknumerr+'</h1>');
			        }else if(data.ret == "usercon"){
				        $("#username").attr("class","tooltipinputerr");
				        $("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_usercon+'</h1>');
			        }else if(data.ret == "mailcon"){
				        $("#email").attr("class","tooltipinputerr");
				     	 $("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_mailcon+'</h1>');
			        }else if(data.ret == "ok"){
				    	$("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEl_regok+'</h1>');
						
						setTimeout(function(){
							$.unblockUI();
							window.location = 'http://www.okkouyu.com/okApp';
						}, 3000);
				        return;
			        }else{
				        $("#global_topdiv").html('<h1><img src="/okApp/images/busy.gif" />'+LABEL_systemerr+'</h1>');
			        }
			         setTimeout(function(){
						$.unblockUI();
					 }, 3000);
		 		},
		 		 processRequestError: function(data){
			    	console.log(data);
			    },
			});
	rpcCall.request();
}

var regdlgsubmitflag = 0;
function regdlgclick(){
	$("#logindlg").empty();
	$("#logindlg").css("left",-400);
	if($(".regdlgtitle").length==0){
		var html = '<div class="regdlgtitle"><a class="regdlgclose" href="javascript:regdlgcloseclick();"></a></div>';
		
		 html += '<div class="regdlgcontent">	<form name="regdlgform1">';
		 html += '<ul class="regdlgul"><li><div STYLE="WIDTH:100%;TEXT-ALIGN:CENTER;">';
         html += '<input  type="radio" class="normalradio" id="roleid1" name="roleid" checked="checked" value="1"> <label for="roleid1">学生</label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;';
         html += '<input  type="radio" class="normalradio" id="roleid2"  name="roleid"   value="2"> <label for="roleid2">老师</label></div></li>';
         html += '<li><div class="opinfo">用户名:&nbsp;&nbsp;</div><input  type="text"  name="username" id="username" tip="请输入5-20个字母开头，后带字母、数字的密码(必输入项)" check="2"></li>';
         html += '<li><div class="opinfo">密码:&nbsp;&nbsp;</div><input type="password"  name="password" id="password" tip="请输入5-20个字母开头，后带字母、数字的密码(必输入项)" check="2"></li>';
         html += '<li><div class="opinfo">重复密码:&nbsp;&nbsp;</div><input type="password"  name="check_passwd" id="check_passwd" compare="password" tip="重复密码" check="2"></li>';
         html += '<li><div class="opinfo">email:&nbsp;&nbsp;</div><input type="text"  name="email" id="email" tip="请输入正确的邮件地址" check="2"></li>';
         html += '<li><div class="opinfo">验证码:&nbsp;&nbsp;</div><input type="text" style="width:80px"  name="Checknum" id="Checknum" tip="看不清楚？点击图片，换一张">&nbsp;&nbsp;<img id="cheimg" onclick=changImg("cheimg") style="vertical-align:bottom" src ="http://www.okkouyu.com/okApp/checkcode.action" width="90" height="35" title="看不清楚？点击图片，换一张">';
		 html += '</li><li style="margin-bottom:10px;"><a id="regdlgformsubmit" onclick="regsubmit();" class="normalbtn" style="width:80px;color:#FFF;">提交</a></li>';
         html += '</ul></form></div>';

		$("#regdlg").html(html);

		setUserName("username");
	    setPasswordCheck("password");
	    setPasswordCheck("check_passwd");	
	    setEmailCheck("email");  

	
		setVerifyCodeCheck("Checknum");
	
		$('select[tip],select[check],input[tip],input[check],textarea[tip],textarea[check]').tooltip({formname:document.regdlgform, btnid:"regdlgforsubmit"});		
	}
	
	var left = $("#regactid").offset().left;
	var top = $("#regactid").offset().top;
	var width = $("#regactid").width();
	$("#regdlg").css("z-index",101);
	$(".logindlgtitle").height($("#regactid").height() + 1);
	left = left + width;

	$("#regdlg").animate({'left': left +'px', 'top': top+"px", 'opacity':'1'}, "slow");
	//$("#hide_div").animate({'left': '0px','opacity':'1'},"slow");
}
function regdlgcloseclick(){
	$("#regdlg").stop(true).animate({'left':'-400px','opacity':'0'},800);
}

document.onkeydown = function(e) {
     e = e || window.event;
      if (e.keyCode == 13) {
  		if($("#loginsubmit").length>0){
  			$("#loginsubmit").trigger('click');
  		}
  		if($("#loginsubmit").length>0){
  			$("#regdlgformsubmit").trigger('click');
  		}
      }
 }

</script>
