function chglang(lang){
	GLOBAL_rpcUrl = "/okApp";

	var rpcCall = $.extend(RpcCall,{
 		async: true,
 		action: 'login.action',
 		method: 'SetLang',
 		params: [lang],
 		beforeLoad: function(){
		},
 		processRequestData: function(data){
	 		window.location.reload();
 		},
 		 processRequestError: function(data){
	    	console.log(data);
	    },
	});
    rpcCall.request();
}
var classroomurl = "http://www.okkouyu.com/okApp";
