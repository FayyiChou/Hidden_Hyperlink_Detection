    <script type="text/javascript">
        $(function () {
	

		    $('#ctl00_cphContent_btnLogin').click(function () {
		        var str = "";
		        if (getVal('ctl00_cphContent_txtAccount') == "") {
		            
                    str += '- 帐号\n';
                     
				}
			    if (getVal('ctl00_cphContent_txtPWD') == "") {
			        str += '- 密码\n';
				}
			    if (str != "") {
			        alert('请输入下列栏位：\n' + str);
				    return false;
				}
			});
        });

        
        $(document).ready(function () {
            if (window.location.hash != "")
            {
                switch (window.location.hash)
                {
                    case "#pane_login":
                        {
                            $(".full-li").removeClass("active");
                            $(".tab-pane").removeClass("active");

                            $("#tab_login").addClass("active");
                            $("#pane_login").addClass("active");

                        } break;
                    case "#pane_learn":
                        {
                            $(".full-li").removeClass("active");
                            $(".tab-pane").removeClass("active");

                            $("#tab_learn").addClass("active");
                            $("#pane_learn").addClass("active");

                        } break;
                    case "#pane_exam":
                        {
                            $(".full-li").removeClass("active");
                            $(".tab-pane").removeClass("active");

                            $("#tab_exam").addClass("active");
                            $("#pane_exam").addClass("active");

                        } break;
                }
            }
        });
          
    </script>

