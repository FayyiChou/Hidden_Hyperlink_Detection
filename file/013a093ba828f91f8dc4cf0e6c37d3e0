<script>
$(window).scroll(function(){

var xh = $(window).scrollTop();
//alert (xh);
if (xh > 300) {
$(".ms1").hide();
$(".header_box").show();
$("#mdgov_enterpriseNavigationWrap").addClass("ms3");
$(".header-navbar").hide();
$(".search_box_lj").hide();

}
else{

  $(".ms1").show();
  $(".header_box").hide();
  $(".header-navbar").show();
  $("#mdgov_enterpriseNavigationWrap").removeClass("ms3");
$(".search_box_lj").show();

}
})

</script>
