<script src="/dianying/Content/vender/jquery.bxslider.min.js"></script>
<script type="text/javascript">
    (function ($) {
        'use strict';
        $(function () {
            var movieSeo = {
                tabChange: function (tabButton, tabContent) {
                    var $tabButton = tabButton,
                        $tabContent = tabContent;

                    $tabButton.click(function () {
                        var _this = $(this);
                        $tabButton.removeClass('active');
                        _this.addClass('active');

                        var index = _this.index();
                        $tabContent.hide();
                        $tabContent.eq(index).show();
                    });
                }
            };
            movieSeo.tabChange($('.slider_title li'), $('.slider ul'));
            movieSeo.tabChange($('.same_movie_head li'), $('.same_movie_content ul'));
            movieSeo.tabChange($('.rate_tab li'), $('.list_rate ul'));

            if ($('.intro_box').height() < 64) {
                $('.show_more_info').hide();
            }
            $('.show_more_info').click(function () {
                $('.movie_info_content .intro').css('height', 'auto');
                $(this).hide();
            })
        });
    })(window.$);
</script>

