<script>
    //获取pass环境版本
    function GetPassHostVersion() {
        var passname = 'pass',
            hostname = location.hostname;

        if (/local|beta|dev|qa|(\w+)2/.test(hostname)) {
            passname = 'qapass';
        } else if (/yz/.test(hostname)) {
            passname = 'yzpass';
        }

        return passname;
    }

    if (typeof HJPassport !== 'undefined') {
        HJPassport.init({
            API_SLD: 'https://' + GetPassHostVersion(),  //配置接口host的头 （0.1.3接入）
            source: 'menghu' //这里根据每个业务线不同
        });
        //login button click
        $('.fastLogin').bind('click', function() {
            HJPassport.show('login');
        });
        //register button click
        $('.fastRegister').bind('click', function() {
            HJPassport.show('register');
        });
    }
</script>


