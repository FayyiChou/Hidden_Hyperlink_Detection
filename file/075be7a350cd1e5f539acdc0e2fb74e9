<script type="text/javascript">
var Pt12345list ;
var PtList = avalon.define({
    $id : "pt12345" ,
    list : [],error :false
});
require(["tools","list","objectModel","SuperSlide"],function(tools ,list ,ObjModel ,SuperSlide){
	tools.tab([
		{ name : "Wsdc" , index : 0 },
		{ name : "Bszn" , index : 0 }
	]);
	var FjdzList = [
{
			vmId:'list_143',
			Mode:"advice",
			listArgs:{
				chnlIds :'143'
			},
			num:6
		}
	];
	for(var i=0;i<FjdzList.length;i++){
		list(FjdzList[i]);
	}
	Pt12345list = ObjModel({
        sysUrl : "/ptapp/searchByDept.htm" ,
        listArgs:{
            deptId : "39163&currPage=1&pageSize=5"
        }
    });
    Pt12345list.getList( function(msg){
        if( msg.retcode == "0"){
            PtList.error = false;
            PtList.list = msg.data.slice(0,5);
        }else{
            PtList.error = '获取数据失败';
        }
    });
	jQuery(".focusBox1").slide({titCell:".num1",mainCell:".pic1",effect:"fade",delayTime:500,autoPlay:true,trigger:"mouseover",autoPage:"<li><a></a><span></span></li>",
		startFun:function(i){
			jQuery(".focusBox1 .txt1 li").eq(i).fadeIn().siblings().fadeOut();
		}
	});
	jQuery(".focusBox2").slide({titCell:".num2",mainCell:".pic2",effect:"fade",delayTime:500,autoPlay:true,trigger:"mouseover",autoPage:"<li><a>$</a><span></span></li>"});
	jQuery(".picScroll_left").slide({mainCell:".bd ul",effect:"topMarquee",autoPlay:true,vis:4,interTime:30});
});
avalon.filters.getDateAsString = function( str , format  ){
	var d = new Date( str );
	return avalon.filters.date( d , format )
}
</script>
