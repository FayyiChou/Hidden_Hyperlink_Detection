<script type="text/javascript">
require(["jquery"],function(){
(function(){
var title = "";
if( "" != "" ){
	title = "";
}else if( "" != "" ){
	title = "";
}else{
	title = "莆田市教育局";
}

	$.ajax({
		url: "/fjdzapp/pv/?op=view&chnlid=&siteid=25&docid=&title="+ encodeURIComponent( title ),
		dataType:"jsonp",
		success: function( data ){
			if( !data.error ){
				$("#pvCount").html( data.pvt );
				$("#siteCount").html( data.pvst );				
			}
		}
	});

})();
});
</script>