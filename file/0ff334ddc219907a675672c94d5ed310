function hideBox(id){
document.getElementById(id).style.display="none";
}

function searchCK(){
    if (document.getElementById("key").value =="")
      {
        alert("请输入检索内容!");
        return false;
      }else{
		  var url = "./jsjgnew/";
		  //alert(url);
		 window.location.href = url+"?word="+document.getElementById("key").value;
	  }
}

