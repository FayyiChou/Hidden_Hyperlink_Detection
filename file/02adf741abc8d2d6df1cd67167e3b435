	function huoquyaowei(){
		//alert('获取');
		var xingbie=document.getElementById('xingbie').value;
		var gaodu=document.getElementById('tuotuoyaowei').style.top;
	    gaodu=gaodu.replace("px"," ");  
		//alert(gaodu);
		gaodu=250-gaodu;
		
		//成人32-52 儿童22-32
		//成人男
		if(xingbie=='1'){
		
		gaodu=gaodu/12.5;
		gaodu=gaodu+32;
		gaodu=Math.ceil(gaodu);
		document.getElementById('yaoweishuzi').innerHTML=gaodu;
		document.getElementById('shuitongyao').style.display='block';
		
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		
		if(30<gaodu && gaodu<34){
			//document.getElementById('shuitongyao').src = '/Public/qimg/shuitongyao/shuitongyao01.png';	
			document.getElementById('shuitongyao').innerHTML= "<img  src='/Public/qimg/shuitongyao/shuitongyao01_nan.png' width='100%' height='100%' />";//
			
			//alert('150');
			//alert('来了');
		}
		if(34<gaodu && gaodu<36){
			//document.getElementById('shuitongyao').src = '/Public/qimg/shuitongyao/shuitongyao02.png';	
			
