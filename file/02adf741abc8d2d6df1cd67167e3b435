 //体重
 function huoqutizhong(){
		//alert('获取');
		var xingbie=document.getElementById('xingbie').value;
		var gaodu=document.getElementById('tuotuotizhong').style.top;
	    gaodu=gaodu.replace("px"," ");  
		//alert(gaodu);
		gaodu=250-gaodu;
		//成人体重
		if(xingbie=='1' || xingbie=='2'){
		gaodu=gaodu/4;
		gaodu=gaodu-1+40;
		gaodu=Math.ceil(gaodu);
		
		document.getElementById('tizhongshuzi').innerHTML=gaodu;
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		if(35<gaodu && gaodu<=45){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='45%';
			//alert('150');
			
		}
		if(45<gaodu && gaodu<=50){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='50%';
			//alert('150');
		}
		if(50<gaodu && gaodu<=55){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='55%';
		}
		if(55<gaodu && gaodu<=60){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='60%';
		}
		if(60<gaodu && gaodu<=65){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='65%';
			//alert('150');
		}
		if(65<gaodu && gaodu<=70){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='70%';
			//alert('150');
		}
		if(70<gaodu && gaodu<=75){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='75%';
			//alert('150');
		}
		if(75<gaodu && gaodu<=80){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='80%';
			//alert('150');
		}
		if(80<gaodu && gaodu<=85){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='85%';
			//alert('150');
		}
		if(85<gaodu && gaodu<=90){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='90%';
			//alert('150');
		}
		if(90<gaodu && gaodu<=95){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='95%';
			//alert('150');
		}
		if(95<gaodu && gaodu<=105){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='100%';
			//alert('150');
		}
		
		}
		//童
		if(xingbie=='3' || xingbie=='4'){
			gaodu=gaodu/6;
		gaodu=gaodu-1+20;
		gaodu=Math.ceil(gaodu);
		
		document.getElementById('tizhongshuzi').innerHTML=gaodu;
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		if(15<gaodu && gaodu<=25){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='65%';
			//alert('150');
			
		}
		if(25<gaodu && gaodu<=30){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='70%';
			//alert('150');
		}
		if(30<gaodu && gaodu<=35){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='75%';
		}
		if(35<gaodu && gaodu<=40){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='80%';
		}
		if(40<gaodu && gaodu<=45){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='85%';
			//alert('150');
		}
		if(45<gaodu && gaodu<=50){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='90%';
			//alert('150');
		}
		if(50<gaodu && gaodu<=55){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='95%';
			//alert('150');
		}
		if(55<gaodu && gaodu<=75){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='100%';
			//alert('150');
		}
			
		}
	}
	function tizhongKai(){
		document.getElementById('shangchuantouxiang').style.display = "none";
			document.getElementById('new_xiangqing_zuocebiaotidakuang').style.width = "225px";
			document.getElementById('moteluotizuidahua').style.display='block';
			document.getElementById('xuanhaocaidankuang').style.display='block';
			
			
		document.getElementById('beijing').style.display='none';
		document.getElementById('touxiang').style.display='none';
		document.getElementById('shengao').style.display='none';
		document.getElementById('tizhong').style.display='block';	
		document.getElementById('jiankuan').style.display='none';
		document.getElementById('yaowei').style.display='none';
