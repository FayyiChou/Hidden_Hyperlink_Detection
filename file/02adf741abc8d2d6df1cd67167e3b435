	//试穿
	function shichuan(a){
		//alert(a);//yipai01id
		var xingbie=document.getElementById('xingbie').value;
		if(a=='1101'){
		  var goodsid=document.getElementById('yipai01id').value;	
		}
		if(a=='1102'){
		  var goodsid=document.getElementById('yipai02id').value;	
		}
		if(a=='1103'){
		  var goodsid=document.getElementById('yipai03id').value;	
		}
		if(a=='2201'){
		  var goodsid=document.getElementById('erpai01id').value;	
		}
		if(a=='2202'){
		  var goodsid=document.getElementById('erpai02id').value;	
		}
		if(a=='2203'){
		  var goodsid=document.getElementById('erpai03id').value;	
		}
		if(a=='3301'){
		  var goodsid=document.getElementById('sanpai01id').value;	
		}
		if(a=='3302'){
		  var goodsid=document.getElementById('sanpai02id').value;	
		}
		if(a=='3303'){
		  var goodsid=document.getElementById('sanpai03id').value;	
		}
		//alert(goodsid);
		
		$.ajax({
            type: "POST",
            url: "/index.php?m=Index&a=newxiangqingshichuanAjax",
            data: {
             
			 'goodsid01':goodsid,
			 'xingbie01':xingbie,
     },
             dataType: "json",
             timeout: 15000,
             success: function(data){
                 if(data.message){
                  return false;
                  }
				  
				  if(data=='buneng'){
                  return false;
                  }
				  //穿衣服
				  //模特是正面的，矫正，拿到模特头像$data['shouye_pc_motetou']
				  if(data.shouye_pc_motetou){
					  var  motetou=data.shouye_pc_motetou;
				  }else{
					  var  motetou='nan1'; 
				  }
				  if(motetou=='nan1'){
					document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nanmo01.png";  
				  }
				  if(motetou=='nan2'){
					document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nanmo02.png";  
				  }
				  if(motetou=='nan3'){
					document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nanmo03.png";  
				  }
				  if(motetou=='nan4'){
					document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nanmo04.png";  
				  }
				  if(motetou=='nan5'){
					document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nanmo05.png";  
				  }
				  if(motetou=='nan6'){
					document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nanmo06.png";  
				  }
				  //模特校正完成
				  
				  
				  //console.log(data);
				  var zongjianshu=[];

				  
				  if(xingbie=='1'){
					//给模特穿上
				      
				      if(data.nan_xiangqing101_motetu){
					 
					  //zongjianshu.push('101');
					  document.getElementById('motetu101').style.display="block";
					  document.getElementById('motetu101').innerHTML= "<img  src='/Public/Uploads/"+data.nan_xiangqing101_motetu+"' width='100%' height='100%' />";
					 
				       }
					   
				      if(data.nan_xiangqing104_motetu){
					  //zongjianshu.push('104');
					  document.getElementById('motetu104').style.display="block";
					  document.getElementById('motetu104').innerHTML= "<img  src='/Public/Uploads/"+data.nan_xiangqing104_motetu+"' width='100%' height='100%' />";//
				       }
					   
				      if(data.nan_xiangqing105_motetu){
					 // zongjianshu.push('105');
					  document.getElementById('motetu105').style.display="block";
					  document.getElementById('motetu105').innerHTML= "<img  src='/Public/Uploads/"+data.nan_xiangqing105_motetu+"' width='100%' height='100%' />";//
				       }
					   
				      if(data.nan_xiangqing201_motetu){
					  //zongjianshu.push('201');
					  document.getElementById('motetu201').style.display="block";
					  document.getElementById('motetu201').innerHTML= "<img  src='/Public/Uploads/"+data.nan_xiangqing201_motetu+"' width='100%' height='100%' />";//
				       }
					   
				      if(data.nan_xiangqing301_motetu){
					 // zongjianshu.push('301');
					  document.getElementById('motetu301').style.display="block";
					  document.getElementById('motetu301').innerHTML= "<img  src='/Public/Uploads/"+data.nan_xiangqing301_motetu+"' width='100%' height='100%' />";//					  	
				       }
					   //穿上的给加上，限时总清单 
					  //右边要价格 主图或者颜色图
					  //要价格
				     //alert(zongjianshu[0])
					 if(zongjianshu[0]=='101'){
						 document.getElementById('qingdan01').style.display="block";
						 document.getElementById('qingdanid01').value=data.nan_xiangqing101_id;
						 document.getElementById('qingdantu01').innerHTML="<a  href='index.php?m=Index&a=Goodsxiangqing&id=" +data.nan_xiangqing101_id+ "&did="+data.nan_xiangqing101_userid+"' target='_blank'><img  src='/PUBLIC/Uploads/" +data.nan_xiangqing101_yansetu+ "' width='100%' height='100%' /></a>";
						
						 document.getElementById('qingdanjia01').innerHTML=data.nan_xiangqing101_jiage;
						 
						 
					 }
					 if(zongjianshu[0]=='104'){
						 document.getElementById('qingdan01').style.display="block";
						 document.getElementById('qingdanid01').value=data.nan_xiangqing104_id;
						 document.getElementById('qingdantu01').innerHTML="<a  href='index.php?m=Index&a=Goodsxiangqing&id=" +data.nan_xiangqing104_id+ "&did="+data.nan_xiangqing104_userid+"' target='_blank'><img  src='/PUBLIC/Uploads/" +data.nan_xiangqing104_yansetu+ "' width='100%' height='100%' /></a>";
						
						 document.getElementById('qingdanjia01').innerHTML=data.nan_xiangqing104_jiage;
						 
					 }
					 if(zongjianshu[0]=='105'){
						 document.getElementById('qingdan01').style.display="block";
						 document.getElementById('qingdanid01').value=data.nan_xiangqing105_id;
						 document.getElementById('qingdantu01').innerHTML="<a  href='index.php?m=Index&a=Goodsxiangqing&id=" +data.nan_xiangqing105_id+ "&did="+data.nan_xiangqing105_userid+"' target='_blank'><img  src='/PUBLIC/Uploads/" +data.nan_xiangqing105_yansetu+ "' width='100%' height='100%' /></a>";
						
						 document.getElementById('qingdanjia01').innerHTML=data.nan_xiangqing105_jiage;
						 
					 }
					 if(zongjianshu[0]=='201'){
