	   function shengaoKai(){
		document.getElementById('shangchuantouxiang').style.display = "none";
			document.getElementById('new_xiangqing_zuocebiaotidakuang').style.width = "225px";
			document.getElementById('moteluotizuidahua').style.display='block';
			document.getElementById('xuanhaocaidankuang').style.display='block';
			
			
		document.getElementById('beijing').style.display='none';
		document.getElementById('touxiang').style.display='none';
		document.getElementById('shengao').style.display='block';
		document.getElementById('tizhong').style.display='none';	
		document.getElementById('jiankuan').style.display='none';
		document.getElementById('yaowei').style.display='none';
		document.getElementById('shengaotubiao').src='../../../PUBLIC/qimg/new_xiangqing_mote06_02.png';
		document.getElementById('tizhongtubiao').src='../../../PUBLIC/qimg/new_xiangqing_mote07.png';
		document.getElementById('jiankuantubiao').src='../../../PUBLIC/qimg/new_xiangqing_mote08.png';
		document.getElementById('yaoweitubiao').src='../../../PUBLIC/qimg/new_xiangqing_mote09.png';
		}
		
		
		$(function(){
			$('.box-1 dl').each(function(){
				$(this).dragging({
					//dragging//拖拽属性
					move : 'y',
					//拖拽属性坐标，横向或纵向
					//randomPosition : true
					//位置随动//为false的时候就是你的拖动在哪儿刷新之后都会带初始位置,为true的时候就是在随机任意的地方
					randomPosition : false
				});
			});
					

		});

  
	function huoqushengao(){
		//alert('获取');
		var xingbie=document.getElementById('xingbie').value;
		var gaodu=document.getElementById('tuotuoshengao').style.top;
	    gaodu=gaodu.replace("px"," ");  
		//alert(gaodu);
		gaodu=250-gaodu;
		
		if(xingbie=='1' || xingbie=='2'){
		gaodu=gaodu/5;
		gaodu=gaodu-1+141;
		gaodu=Math.ceil(gaodu);
		
		//console.log(gaodu);
		
		document.getElementById('shengaoshuzi').innerHTML=gaodu;
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		if(140<=gaodu && gaodu<=145){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='82%';
			//alert('150');
			
		}
		if(145<gaodu && gaodu<=150){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='84%';
			//alert('150');
			
		}
		if(150<gaodu && gaodu<=155){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='86%';
			//alert('150');
			
		}
		if(155<gaodu && gaodu<=160){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='88%';
			//alert('150');
			
		}
		if(160<gaodu && gaodu<=165){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='90%';
			//alert('150');
			
		}
		if(165<gaodu && gaodu<=170){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='92%';
			//alert('150');
			
		}
		if(170<gaodu && gaodu<=175){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='94%';
			//alert('150');
			
		}
		if(175<gaodu && gaodu<=180){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='96%';
			//alert('150');
			
		}
		if(180<gaodu && gaodu<=185){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='98%';
			//alert('150');
			
		}
		if(185<gaodu && gaodu<=190){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='100%';
			//alert('150');
			
		}
		}
		//童装身高
		if(xingbie=='3' || xingbie=='4'){
	    //document.getElementByName('box-1').style.height='262px';
		var gaodu=document.getElementById('tuotuoshengao').style.top;
		
	    gaodu=gaodu.replace("px"," ");  
		//alert(gaodu);
		gaodu=240-gaodu;	
		gaodu=gaodu/3;
		gaodu=gaodu-1+61;
		gaodu=Math.ceil(gaodu);
		
		//console.log(gaodu);
		
		document.getElementById('shengaoshuzi').innerHTML=gaodu;
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		if(40<=gaodu && gaodu<=65){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='70%';
			//alert('150');
			
		}
		if(65<gaodu && gaodu<=70){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='72%';
			//alert('150');
			
		}
		if(70<gaodu && gaodu<=75){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='74%';
			//alert('150');
			
		}
		if(75<gaodu && gaodu<=80){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='76%';
			//alert('150');
			
		}
		if(80<gaodu && gaodu<=85){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='78%';
			//alert('150');
			
		}
		if(85<gaodu && gaodu<=90){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='80%';
			//alert('150');
			
		}
		if(90<gaodu && gaodu<=95){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='82%';
			//alert('150');
			
		}
		if(95<gaodu && gaodu<=100){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='84%';
			//alert('150');
			
		}
		if(100<gaodu && gaodu<=105){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='86%';
			//alert('150');
			
		}
		if(105<gaodu && gaodu<=110){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='88%';
			//alert('150');
			
		}
		if(110<gaodu && gaodu<=115){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='90%';
			//alert('150');
			
		}
		if(115<gaodu && gaodu<=120){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='92%';
			//alert('150');
			
		}
		if(120<gaodu && gaodu<=125){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='94%';
			//alert('150');
			
		}
		if(125<gaodu && gaodu<=130){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='96%';
			//alert('150');
			
		}
		if(130<gaodu && gaodu<=135){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='98%';
			//alert('150');
			
		}
		if(135<gaodu && gaodu<=150){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='100%';
			//alert('150');
			
		}
		}
		
		
	}
