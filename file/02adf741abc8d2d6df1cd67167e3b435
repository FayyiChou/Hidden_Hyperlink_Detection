<!--NEW_mote开始-->
<script type="text/javascript">
function huanmote(a){
	    //alert('换模特头像');	//给后台存起来
		//alert(a);
		$.ajax({
            type: "POST",
            url: "/index.php?m=Index&a=PcshouyehuanmoteAjax",
            data: {
             'dangqianmote':a,
     },
             dataType: "json",
             timeout: 15000,
             success: function(data){
                 if(data.message){
                  alert(data.message);
                  return false;
                  }
				//alert(a);
		               if(a=='nan1'){
			
		               document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nanmo01.png";	
		                }
		                if(a=='nan2'){
		                document.getElementById('nuomotebenshen').src = '/PUBLIC/sjing/sj_nanmo02.png';	
		               }
		                if(a=='nan3'){
		                document.getElementById('nuomotebenshen').src = "/Public/sjing/sj_nanmo03.png";	
		               }
		                if(a=='nan4'){
		                document.getElementById('nuomotebenshen').src = "/Public/sjing/sj_nanmo04.png";	
		                }
		                if(a=='nan5'){
		                 document.getElementById('nuomotebenshen').src = "/Public/sjing/sj_nanmo05.png";	
		                }
		                if(a=='nan6'){
		                  document.getElementById('nuomotebenshen').src = "/Public/sjing/sj_nanmo06.png";	
		                }
		                if(a=='nv1'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvmo01.png";	
		                 }
		                if(a=='nv2'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvmo02.png";	
		                }
		                if(a=='nv3'){
			
		                document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvmo03.png";	
		               }
		                if(a=='nv4'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvmo04.png";	
		                }
		                if(a=='nv5'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvmo05.png";	
		               }
		                if(a=='nv6'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvmo06.png";	
		               }
					   //男童
					   if(a=='nantong1'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nantongmo01.png";	
		                 }
		                if(a=='nantong2'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nantongmo02.png";	
		                }
		                if(a=='nantong3'){
			
		                document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nantongmo03.png";	
		               }
		                if(a=='nantong4'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nantongmo04.png";	
		                }
		                if(a=='nantong5'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nantongmo05.png";	
		               }
		                if(a=='nantong6'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nantongmo06.png";	
		               }
					   //女童
					   if(a=='nvtong1'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvtongmo01.png";	
		                 }
		                if(a=='nvtong2'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvtongmo02.png";	
		                }
		                if(a=='nvtong3'){
			
		                document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvtongmo03.png";	
		               }
		                if(a=='nvtong4'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvtongmo04.png";	
		                }
		                if(a=='nvtong5'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvtongmo05.png";	
		               }
		                if(a=='nvtong6'){
			
		                 document.getElementById('nuomotebenshen').src = "/PUBLIC/sjing/sj_nvtongmo06.png";	
		               }
			       }
                 });
	}
