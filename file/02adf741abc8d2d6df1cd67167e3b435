	//清单删除
	function qindanshanchu(a){
		//alert('清单删除');
		//alert(a);
		if(a=='1'){
		  var goodsid=document.getElementById('qingdanid01').value;
		}
		if(a=='2'){
		  var goodsid=document.getElementById('qingdanid02').value;
		}
		if(a=='3'){
		  var goodsid=document.getElementById('qingdanid03').value;
		}
		if(a=='4'){
		  var goodsid=document.getElementById('qingdanid04').value;
		}
		if(a=='5'){
		  var goodsid=document.getElementById('qingdanid05').value;
		}
		//alert(goodsid);
		$.ajax({
            type: "POST",
            url: "/index.php?m=Index&a=XiangqingmoteshanchuAjax",
            data: {
             'goodsid01':goodsid,
     },
             dataType: "json",
             timeout: 15000,
             success: function(data){
                 if(data.message){
                  alert(data.message);
                  return false;
                  }
				  
				 // alert(data);
				 if(a=='1'){
				 document.getElementById('qingdan01').style.display='none';
				 }
				 if(a=='2'){
				 document.getElementById('qingdan02').style.display='none'; 
				 }
				 if(a=='3'){
				 document.getElementById('qingdan03').style.display='none'; 
				 }
				 if(a=='4'){
				 document.getElementById('qingdan04').style.display='none'; 
				 }
				 if(a=='5'){
				 document.getElementById('qingdan05').style.display='none'; 
				 }
				 if(data==101){
				    document.getElementById('motetu101').style.display="none";
				    document.getElementById('motetu101').innerHTML= " "; 	 
				 }
				 if(data==104){
				    document.getElementById('motetu104').style.display="none";
				    document.getElementById('motetu104').innerHTML= " "; 	 
				 }
				 if(data==105){
				    document.getElementById('motetu105').style.display="none";
				    document.getElementById('motetu105').innerHTML= " "; 	 
				 }
				 if(data==201){
				    document.getElementById('motetu201').style.display="none";
				    document.getElementById('motetu201').innerHTML= " "; 	 
				 }
				 if(data==301){
				    document.getElementById('motetu301').style.display="none";
				    document.getElementById('motetu301').innerHTML= " "; 	 
				 }
      
      }
      })
		
		}   
    //提交
    function tijiao(){

	window.parent.location.href="/index.php?m=Index&a=XiangqingxuanzechimaCunchu";//跳转
 
  }
