					  document.getElementById('yipai03name').innerHTML= data.shuju[2].type_name;
					  document.getElementById('yipai03jiage').innerHTML="¥"+data.shuju[2].goods_jiage+"元";
					  document.getElementById('yipai01id').value=data.shuju[2].goods_id;
					  }  
					
				  }
				 //2pai
				 if(paishu=='2'){

					 var page=document.getElementById('erpaipage').value=newpage;//当前页 
					 
					 
					 if(data.shuju[0]==undefined){
						 document.getElementById('erpai01tu').innerHTML= "<img src='/Public/qimg/new_xiangqing_mote32.png' width='100%' height='100%' />";//第一个 
						 document.getElementById('erpai01name').innerHTML= ' ';
						 document.getElementById('erpai01jiage').innerHTML= ' ';
						 document.getElementById('erpai01id').value= ' ';
						 
					 }else{
					     document.getElementById('erpai01tu').innerHTML= "<img   onclick='shichuan(2201)' src='/Public/Uploads/"+data.shuju[0].goods_yansetu+"' width='100%' height='100%' />";
					     document.getElementById('erpai01name').innerHTML= data.shuju[0].type_name;
						 document.getElementById('erpai01jiage').innerHTML="¥"+data.shuju[0].goods_jiage+"元";
						 document.getElementById('erpai01id').value=data.shuju[0].goods_id;
					  }
					  
					  
					  
