	  document.getElementById('nv').style.background = "";
	  document.getElementById('tong').style.background = "";
	  document.getElementById('xie').style.background = "";
	  document.getElementById('bao').style.background = "";
	  document.getElementById('muyin').style.background = "#F2F2F2";
	  document.getElementById('wanju').style.background = "";
	  document.getElementById('peishi').style.background = "";
	  }
	  //
	  if(a=='wanju'){
      document.getElementById('index_m02az03_nanxuan').style.display = "none";
	  document.getElementById('index_m02az03_nv').style.display = "none";
	  document.getElementById('index_m02az03_tong').style.display = "none";
	  document.getElementById('index_m02az03_xie').style.display = "none";
