			document.getElementById('shuitongyao').innerHTML= "<img  src='/Public/qimg/shuitongyao/shuitongyao03_nvtong.png' width='100%' height='100%' />";//	
		}
		if(28<gaodu && gaodu<30){
			document.getElementById('shuitongyao').innerHTML= "<img  src='/Public/qimg/shuitongyao/shuitongyao04_nvtong.png' width='100%' height='100%' />";//		
		}
		if(30<gaodu && gaodu<36){
			document.getElementById('shuitongyao').innerHTML= "<img  src='/Public/qimg/shuitongyao/shuitongyao05_nvtong.png' width='100%' height='100%' />";//		
			//alert('150')
		}

		}
	   
		
	}
	</script>
<!--华子-->

<script type="text/javascript">
//试穿
 function indexshichuan(a){
	// alert(a);
	 
	 var a02=a;
	 $.ajax({
            type: "POST",
            url: "/index.php?m=Index&a=IndexmoteshichuanAjax",
			
            data: {
             'xing':a02,
     },
             dataType: "json",
             timeout: 15000,
             success: function(data){
                 if(data.message){
                  //alert(data.message);
                  return false;
                  }
				  
				 
				  
				   
				   
				  if(data.yipai==11){
					  //试穿和旋转的上衣层级都是105定死了的
					 document.getElementById('motetu105').style.display="block";
					 var shichuan011=document.getElementById('shichuan11').value;
					// alert(shichuan011);
					 document.getElementById('motetu105').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan011+'" width="100%" height="100%" />';  
				  }
				  if(data.yipai==12){
					  document.getElementById('motetu105').style.display="block";
					  var shichuan012=document.getElementById('shichuan12').value;
					// alert(shichuan012);
					 document.getElementById('motetu105').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan012+'" width="100%" height="100%" />';
					 
				  }
				  if(data.yipai==13){
					  document.getElementById('motetu105').style.display="block";
					 var shichuan013=document.getElementById('shichuan13').value;
					// alert(shichuan013);
					 document.getElementById('motetu105').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan013+'" width="100%" height="100%" />'; 
				  }
				  
				  if(data.erpai==21){
					 document.getElementById('motetu201').style.display="block";
					var shichuan021=document.getElementById('shichuan21').value;
					// alert(shichuan021);
					 document.getElementById('motetu201').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan021+'" width="100%" height="100%" />'; 
				  }
				  if(data.erpai==22){
					  document.getElementById('motetu201').style.display="block";
					 var shichuan022=document.getElementById('shichuan22').value;
					 //alert(shichuan011);
					 document.getElementById('motetu201').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan022+'" width="100%" height="100%" />';
				  }
				  if(data.erpai==23){
					  document.getElementById('motetu201').style.display="block";
					 var shichuan023=document.getElementById('shichuan23').value;
					 //alert(shichuan011);
					 document.getElementById('motetu201').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan023+'" width="100%" height="100%" />'; 
				  }
				  
				  if(data.sanpai==31){
					 document.getElementById('motetu301').style.display="block";
					var shichuan031=document.getElementById('shichuan31').value;
					 //alert(shichuan011);
					 document.getElementById('motetu301').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan031+'" width="100%" height="100%" />';
				  }
				  if(data.sanpai==32){
					  document.getElementById('motetu301').style.display="block";
					var shichuan032=document.getElementById('shichuan32').value;
					 //alert(shichuan011);
					 document.getElementById('motetu301').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan032+'" width="100%" height="100%" />';
				  }
				  if(data.sanpai==33){
					  document.getElementById('motetu301').style.display="block";
					var shichuan033=document.getElementById('shichuan33').value;
					 //alert(shichuan011);
					 document.getElementById('motetu301').innerHTML= '<img  src="/Public/shouyesc_pc/'+shichuan033+'" width="100%" height="100%" />'; 
				  }
      
      }
      })
  
	 
 }

