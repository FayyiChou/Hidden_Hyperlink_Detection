			if(yipaicengji==104){
			   document.getElementById('motetu104').style.display="block";
			   document.getElementById('motetu104').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimgh+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu104').style.display="none";	
			}
			
			if(yipaicengji==105){
			   document.getElementById('motetu105').style.display="block";
			   document.getElementById('motetu105').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimgh+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu105').style.display="none";	
			}
			
			if(erpaiimgh){
			   document.getElementById('motetu201').style.display="block";
			   document.getElementById('motetu201').innerHTML= "<img  src='/Public/shouyesc_pc/"+erpaiimgh+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu201').style.display="none";	
			}
			
			if(sanpaiimgh){
			   document.getElementById('motetu301').style.display="block";
			   document.getElementById('motetu301').innerHTML= "<img  src='/Public/shouyesc_pc/"+sanpaiimgh+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu301').style.display="none";	
			}
			
		 }
		  //
		if(mote_zhuangtai=='hou'){
			 if(yipaicengji==101){
			   document.getElementById('motetu101').style.display="block";
			   document.getElementById('motetu101').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimgz+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu101').style.display="none";	
			}
			if(yipaicengji==104){
			   document.getElementById('motetu104').style.display="block";
			   document.getElementById('motetu104').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimgz+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu104').style.display="none";	
			}
			
			if(yipaicengji==105){
			   document.getElementById('motetu105').style.display="block";
			   document.getElementById('motetu105').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimgz+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu105').style.display="none";	
			}
			
			if(erpaiimgz){
			   document.getElementById('motetu201').style.display="block";
			   document.getElementById('motetu201').innerHTML= "<img  src='/Public/shouyesc_pc/"+erpaiimgz+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu201').style.display="none";	
			}
			
			if(sanpaiimgz){
			   document.getElementById('motetu301').style.display="block";
			   document.getElementById('motetu301').innerHTML= "<img  src='/Public/shouyesc_pc/"+sanpaiimgz+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu301').style.display="none";	
			}
			
		 }
		 ///
		 if(mote_zhuangtai=='zuo'){
			  if(yipaicengji==101){
			   document.getElementById('motetu101').style.display="block";
			   document.getElementById('motetu101').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimg+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu101').style.display="none";	
			}
			if(yipaicengji==104){
			   document.getElementById('motetu104').style.display="block";
			   document.getElementById('motetu104').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimg+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu104').style.display="none";	
			}
			
			if(yipaicengji==105){
			   document.getElementById('motetu105').style.display="block";
			   document.getElementById('motetu105').innerHTML= "<img  src='/Public/shouyesc_pc/"+yipaiimg+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu105').style.display="none";	
			}
			
			if(erpaiimg){
			   document.getElementById('motetu201').style.display="block";
			   document.getElementById('motetu201').innerHTML= "<img  src='/Public/shouyesc_pc/"+erpaiimg+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu201').style.display="none";	
			}
			
			if(sanpaiimg){
			   document.getElementById('motetu301').style.display="block";
			   document.getElementById('motetu301').innerHTML= "<img  src='/Public/shouyesc_pc/"+sanpaiimg+"' width='100%' height='100%' />";
			}else{
			document.getElementById('motetu301').style.display="none";	
			}
			
		 }
			   
			   
			   
			
			   }
	      });
	   
   }
   
   //翻页
   function shang(a){
	   //111一排上一页 11111一排下一页
	  var xingbie=document.getElementById('xingbie').value;//性别
	  if(a=='111'){
		  //alert('一排');
		  //取当前页
		  var page=document.getElementById('yipaipage').value;//当前页 
		  var zongpage=document.getElementById('yipaizongpage').value;//当前页
	  }
	  if(a=='222'){
		  //alert('二排');
		  var page=document.getElementById('erpaipage').value;//当前页 
		  var zongpage=document.getElementById('erpaizongpage').value;//当前页
	  }
	  if(a=='333'){
		  //alert('三排');
		  var page=document.getElementById('sanpaipage').value;//当前页 
		  var zongpage=document.getElementById('sanpaizongpage').value;//当前页
	  }
	  var xingbie=document.getElementById('xingbie').value;//性别
	  if(a=='11111'){
		  //alert('一排');
		  //取当前页
		  var page=document.getElementById('yipaipage').value;//当前页
		  var zongpage=document.getElementById('yipaizongpage').value;//当前页 
	  }
	  if(a=='22222'){
		  //alert('二排');
		  var page=document.getElementById('erpaipage').value;//当前页 
		  var zongpage=document.getElementById('erpaizongpage').value;//当前页
	  }
	  if(a=='33333'){
		  //alert('三排');
		  var page=document.getElementById('sanpaipage').value;//当前页 
		  var zongpage=document.getElementById('sanpaizongpage').value;//当前页
	  }
	  
	  //alert(a); 
	  //alert(page); 
	  //alert(xingbie); 
	  if(a=='111' || a=='222' || a=='333'){
		if(page<2){
		 alert('已是第一页');
		 return false;	
		}else{
			newpage=page-1;
		}
	  }
	  
	  if(a=='11111' || a=='22222' || a=='33333'){
		if(page<zongpage){
		 newpage=page-1+2;
		}else{
		
		alert('已是最后一页');//计算总页数	
		 return false;	
		}
	  }
	//开工继续
	 
	$.ajax({
            type: "POST",
            url: "/index.php?m=Index&a=XiangqingfanyeAjax",
            data: {
             'newpage01':newpage,
			 'xingbie01':xingbie,
			 'a01':a,
     },
             dataType: "json",
             timeout: 15000,
             success: function(data){
                 if(data.message){
                  //alert(data.message);
                  return false;
                  }
				  
				  var paishu=data.paishu;
				  
				 
				  //alert(data.shuju[1]['goods_yansetu']);
				  //一排
				  if(paishu=='1'){

					 var page=document.getElementById('yipaipage').value=newpage;//当前页 
					 
					 
					 if(data.shuju[0]==undefined){
						 document.getElementById('yipai01tu').innerHTML= "<img src='/Public/qimg/new_xiangqing_mote32.png' width='100%' height='100%' />";//第一个 
						 document.getElementById('yipai01name').innerHTML= ' ';
						 document.getElementById('yipai01jiage').innerHTML= ' ';
						 document.getElementById('yipai01id').value= ' ';
						 
					 }else{
					     document.getElementById('yipai01tu').innerHTML= "<img   onclick='shichuan(1101)' src='/Public/Uploads/"+data.shuju[0].goods_yansetu+"' width='100%' height='100%' />";
					     document.getElementById('yipai01name').innerHTML= data.shuju[0].type_name;
						 document.getElementById('yipai01jiage').innerHTML="¥"+data.shuju[0].goods_jiage+"元";
						 document.getElementById('yipai01id').value=data.shuju[0].goods_id;
					  }
					  
					  
					  
					  if(data.shuju[1]==undefined){
						  document.getElementById('yipai02tu').innerHTML= "<img src='/Public/qimg/new_xiangqing_mote32.png' width='100%' height='100%' />";//第一个
						  document.getElementById('yipai02name').innerHTML= ""; 
						  document.getElementById('yipai02jiage').innerHTML= "";
						  document.getElementById('yipai02id').value= ' ';
					  }else{
					  document.getElementById('yipai02tu').innerHTML= "<img  onclick='shichuan(1102)'  onclick='shichuan()' src='/Public/Uploads/"+data.shuju[1].goods_yansetu+"' width='100%' height='100%' />";//第一个
					  document.getElementById('yipai02name').innerHTML= data.shuju[1].type_name;
					  document.getElementById('yipai02jiage').innerHTML= "¥"+data.shuju[1].goods_jiage+"元";
					  document.getElementById('yipai01id').value=data.shuju[1].goods_id;
					  }
					  
					  
					  
					  
