	 function huoqujiankuan(){
		//alert('获取')
		var xingbie=document.getElementById('xingbie').value;
		var gaodu=document.getElementById('tuotuojiankuan').style.top;
	    gaodu=gaodu.replace("px"," ");  
		//alert(gaodu);
		gaodu=250-gaodu;
		
		//成人男42-52
		if(xingbie=='1'){
		gaodu=gaodu/25;
		gaodu=gaodu+40;
		gaodu=Math.ceil(gaodu);
		
		
		document.getElementById('jiankuanshuzi').innerHTML=gaodu;
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		if(36<gaodu && gaodu<=40){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='50%';
			//alert('150');	
		}
		if(40<gaodu && gaodu<=42){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='60%';
			//alert('150');
		}
		if(42<gaodu && gaodu<=44){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='70%';
		}
		if(44<gaodu && gaodu<=46){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='80%';
		}
		if(46<gaodu && gaodu<=48){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='90%';
			//alert('150');
		}
		if(48<gaodu && gaodu<=50){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='100%';
			//alert('150');
		}
		
		}
		//成人女35-45
		if(xingbie=='2'){
		gaodu=gaodu/25;
		gaodu=gaodu+35;
		gaodu=Math.ceil(gaodu);
		
		
		document.getElementById('jiankuanshuzi').innerHTML=gaodu;
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		if(30<gaodu && gaodu<=35){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='60%';
			//alert('150');	
		}
		if(35<gaodu && gaodu<=37){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='70%';
			//alert('150');
		}
		if(37<gaodu && gaodu<=39){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='80%';
		}
		if(41<gaodu && gaodu<=43){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='90%';
		}
		if(43<gaodu && gaodu<=47){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='100%';
			//alert('150');
		}
		
		
		}
		//童25-35
		
		if(xingbie=='3' || xingbie=='4'){
		gaodu=gaodu/25;
		gaodu=gaodu+25;
		gaodu=Math.ceil(gaodu);
		
		
		document.getElementById('jiankuanshuzi').innerHTML=gaodu;
		//document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.height='50%';
		
		if(20<gaodu && gaodu<=25){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='40%';
			//alert('150');	
		}
		if(25<gaodu && gaodu<=27){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='50%';
			//alert('150');
		}
		if(27<gaodu && gaodu<=29){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='60%';
		}
		if(29<gaodu && gaodu<=31){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='70%';
		}
		if(31<gaodu && gaodu<=33){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='80%';
			//alert('150');
		}
		if(33<gaodu && gaodu<=35){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='90%';
			//alert('150');
		}
		if(35<gaodu && gaodu<=37){
			document.getElementById('new_xiangqing_mote02zz0301_luomokuang').style.width='100%';
			//alert('150');
		}
		}
	}
