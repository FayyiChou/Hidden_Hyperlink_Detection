		var $v = jQuery.noConflict();
		$v(function(){
			$v( '.tm_item' ).hover(
				function(){
					var options;
					$v( '.tm_subitem', this ).toggle( 'blind', options, 100 );
					$v( '.tm_link', this ).toggleClass( 'sel', 0 );
				}
			);
			$v( '.super_button_div' ).hover(
				function(){
					var options;
					$v( '.sb_subm', this ).toggle( 'blind', options, 100 );
					$v( '.super_button_2', this ).toggleClass( 'sel', 0 );
				}
			);
		});
	</script>

	<script type="text/javascript">
		$v(function(){
			$v("#toTop").hide().removeAttr("href");
			if($v(window).scrollTop()!="0"){
				$v("#toTop").fadeIn("slow")
			}
			var scrollDiv=$v("#toTop");
			$v(window).scroll(function(){
				if($v(window).scrollTop()=="0"){$v(scrollDiv).fadeOut("slow")}else{$v(scrollDiv).fadeIn("slow")}
			});
			$v("#toTop").click(function(){
				$v("html, body").animate({scrollTop:0},"slow")
			})
		});
	</script>



