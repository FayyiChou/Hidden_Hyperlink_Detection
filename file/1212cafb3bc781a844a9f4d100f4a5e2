       <script type="text/javascript">
//公益广告的滚动

var speed=30
var colee_left26=document.getElementById("colee_left26");
var colee_left16=document.getElementById("colee_left16");
var colee_left6=document.getElementById("colee_left6");
colee_left26.innerHTML=colee_left16.innerHTML
function Marquee36(){
if(colee_left26.offsetWidth-colee_left6.scrollLeft<=0)
colee_left6.scrollLeft-=colee_left16.offsetWidth
else{
colee_left6.scrollLeft++
}
}
var MyMar36=setInterval(Marquee36,speed)
colee_left6.onmouseover=function() {clearInterval(MyMar36)}
colee_left6.onmouseout=function() {MyMar36=setInterval(Marquee36,speed)}
</script>  
