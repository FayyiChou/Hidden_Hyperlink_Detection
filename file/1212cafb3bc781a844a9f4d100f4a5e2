<script type="text/javascript">
function carddddddw(ff,idNameeeeeew,idNameeeeeew1,cssNameeeeeew) {   
 for(gg=0;gg<=5;gg++)   {    
  if(gg==ff)        {          
   document.getElementById(idNameeeeeew+ff).style.display="";
    //document.getElementById(idNameeeeeew1+ff).style.display="";                  
     document.getElementById(cssNameeeeeew+ff).className="cardoooooow1";     
      }        else        {               
     document.getElementById(idNameeeeeew+gg).style.display="none";  
      //document.getElementById(idNameeeeeew1+gg).style.display="none";             
       document.getElementById(cssNameeeeeew+gg).className="cardttttttw1";       
        }   }    }  
        </script>
