<script src="/template/sdmos/11/images/sjsxl/tcck.js"></script>
<script>
 
 $(document).ready(function(){
  divcenter();
  $("#img").show();
  $("#close").bind( "click", handle);
  $("#pic").bind("click", picshow);
 });
 function handle(){
  $("#img").hide();
  $("#zhezhao").hide();
 }
 function picshow(){
  divcenter();
    $("#img").show();
    $("#zhezhao").show();
 }
 function divcenter()
    {
    var divId=$("#img");
 divId.css("position","absolute");
    divId.css("top", ( $(window).height() - divId.height() ) / 2+$(window).scrollTop() + "px");
    divId.css("left", ( $(window).width() - divId.width() ) / 2+$(window).scrollLeft() + "px");
 divId.css("z-index",15000);
    }
</script>
<style>
 .zhezhao
 {
  width:100%;
  height:100%;
  background-color: #F4F4F4;
  filter:alpha(opacity=50);
  -moz-opacity:0.3;
  opacity:0.3;
  position:absolute;
  left:0px;
  top:0px;  
  z-index:1000;
 }
</style>
