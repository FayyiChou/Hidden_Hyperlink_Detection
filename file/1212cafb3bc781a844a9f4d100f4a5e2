//公益广告的滚动
var speed=30
var colee_left27=document.getElementById("colee_left27");
var colee_left17=document.getElementById("colee_left17");
var colee_left7=document.getElementById("colee_left7");
colee_left27.innerHTML=colee_left17.innerHTML
function Marquee37(){
if(colee_left27.offsetWidth-colee_left7.scrollLeft<=0)
colee_left7.scrollLeft-=colee_left17.offsetWidth
else{
colee_left7.scrollLeft++
}
}
var MyMar37=setInterval(Marquee37,speed)
colee_left7.onmouseover=function() {clearInterval(MyMar37)}
colee_left7.onmouseout=function() {MyMar37=setInterval(Marquee37,speed)}
