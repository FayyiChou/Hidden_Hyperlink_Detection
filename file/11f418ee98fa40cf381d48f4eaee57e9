    setInterval(function(){
            $('#showSl').find('dt:first').fadeOut(1000,function(){
                $(this).clone().appendTo('#showSl');
                $('#showSl').find('dt:last').show();
                $(this).remove();
            }); 
        },3000);
</script> 
