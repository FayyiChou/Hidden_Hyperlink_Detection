function switchModule(m)
{
   var tli = document.getElementById("module").getElementsByTagName("li");
   
   for(i=0;i<tli.length;i++){
  tli[i].className=i==m?"focus":"";
 }
}
function searchModule()
{
    var cli = document.getElementById("module").getElementsByTagName("li");
       for(i=0;i<cli.length;i++){
        if(cli[i].className=="focus"){
		var keywords = encodeURI(document.getElementById("searchcontent").value);
            switch(i){
                case 0:
                window.open("http://www.sosw.net/sell/search.php?kw="+keywords);
                break;
                case 1:
                window.open("http://www.sosw.net/mall/search.php?kw="+keywords);
                break;
                case 2:
                window.open("http://www.sosw.net/buy/search.php?kw="+keywords);
                break;
                case 3:
				this.location.href="http://baike.sosw.net/index.php?search-fulltext-title-"+keywords+"--all-0-within-time-desc-1";
                break;
            }
            }
        }
        }
</script> 
