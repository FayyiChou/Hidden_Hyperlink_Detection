<script type="text/javascript">


    $(document).ready(function(){
        $(".closskf").click(function(){
            $("#advisoryTB").hide();
            $(".closeStyle").show();
            $backToTopFun();
        });

        $(".closeStyle").live("click",function(){
            $("#advisoryTB").show();
            $(".closeStyle").hide();
            $backToTopFun();
        },function(){});

        var $backToTopEle = $(".backToTop");
        var $backToTopFun = function() {
            var st = $(document).scrollTop(), winh = document.documentElement.clientHeight;
            //IE6下的定位
            if (!window.XMLHttpRequest) {
                $backToTopEle.css("top", st + winh - 400);
            }
            $(".backToTop").css("top", (document.documentElement.clientHeight - $backToTopEle.css("height").split("px")[0])/2);
        };
        $(window).bind("scroll", $backToTopFun);
        $(function() { $backToTopFun(); });
    })
</script></body>
