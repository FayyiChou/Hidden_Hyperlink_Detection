<script type="text/javascript">
var showad = true;	
var MinScreenW = 1280;

var y=0;
function dlScroll(){
if (window.screen.width > 1280){
    per=Math.floor(.1*((document.documentElement&&document.documentElement.scrollTop||document.body&&document.body.scrollTop)-y));
    var sTop=parseInt(document.getElementById("ad3").style.top)+per+"px";
    document.getElementById("ad3").style.top=sTop;
    document.getElementById("ad4").style.top=sTop;
    y+=per;
}else{
    per=Math.floor(.1*((document.documentElement&&document.documentElement.scrollTop||document.body&&document.body.scrollTop)-y));
    var sTop=parseInt(document.getElementById("ad1").style.top)+per+"px";
    document.getElementById("ad1").style.top=sTop;
    document.getElementById("ad2").style.top=sTop;
    y+=per;
	}
}

function scall(){
if(!showad){return;}
// setTimeout(tips_pop, 3000);
//setTimeout(duilianclose,10000);
if (window.screen.width<MinScreenW){
//alert("临时提示：\n\n显示器分辨率宽度小于"+MinScreenW+",不显示广告");
showad = false;
 setTimeout(duilianclose, 1500);

return;
}
if (window.screen.width >1280){

document.getElementById("ad3").style.display="block";
document.getElementById("ad4").style.display="block";

}else{

document.getElementById("ad1").style.display="block";
document.getElementById("ad2").style.display="block";
}
}
window.setInterval(dlScroll,1);
window.onload=scall;
function duilianclose(){
document.getElementById("ad1").style.display="none";
document.getElementById("ad2").style.display="none";
document.getElementById("ad3").style.display="none";
document.getElementById("ad4").style.display="none";
//var MsgPop=document.getElementById("haoetv");
  // MsgPop.style.display="none";

}
function duilian(){
  var dl1=document.getElementById("ad1");
   dl1.style.display="none";//显示隐藏的窗口
   var dl2=document.getElementById("ad2");
   dl2.style.display="none";//显示隐藏的窗口
    var dl3=document.getElementById("ad3");
   dl3.style.display="none";//显示隐藏的窗口
   var dl4=document.getElementById("ad4");
   dl4.style.display="none";//显示隐藏的窗口
   }
function trimLR(str) 
{ 
return str.replace(/(^\s*)|(\s*$)/g, ""); 
}


	function yjform(){
	var SQLKeyword,SQL;
		if(document.yjzzform.searchword2.value==""
		&&document.yjzzform.searchword3.value=="")
	      {
		  alert("输入关键词！！");
		document.yjzzform.searchword2.focus();
		return false;
	     }
       if(document.yjzzform.searchword3.value!=""&&document.yjzzform.searchword2.value==""){
	   SQLKeyword="TITLE=%" + trimLR(document.yjzzform.searchword3.value) +"%";
	   }else if(document.yjzzform.searchword2.value!=""&&document.yjzzform.searchword3.value==""){
	   SQLKeyword="name=%" + trimLR(document.yjzzform.searchword2.value) +"%";
	   }else if(document.yjzzform.searchword2.value!=""&&document.yjzzform.searchword3.value!=""){
	  SQLKeyword="TITLE =\%" + trimLR(document.yjzzform.searchword3.value) + "\% and name =%" + trimLR(document.yjzzform.searchword2.value)+"%";
	   }
	   
	  
	   document.yjzzform.searchword.value=SQLKeyword;
        document.yjzzform.submit();
	}

function bjform(){
	
        document.bjzzform.submit();
	}

function jjform(){
	
        document.jjzzform.submit();
	}

</script> 



