<script type="text/javascript">
    //<![CDATA[

    window.onload = function onLoaded() {
        MM_preloadImages('images/newPic/loading.gif', 'images/pic/ajax-loader.gif');
        reSizeTab();
    }

    function WebForm_CallbackComplete() {
        for (var i = 0; i < __pendingCallbacks.length; i++) {
            var callbackObject = __pendingCallbacks[i];
            if (callbackObject && callbackObject.xmlRequest && (callbackObject.xmlRequest.readyState == 4)) {
                WebForm_ExecuteCallback(callbackObject);
                if (__pendingCallbacks[i] && !__pendingCallbacks[i].async) {
                    __synchronousCallBackIndex = -1;
                }
                __pendingCallbacks[i] = null;
                var callbackFrameID = "__CALLBACKFRAME" + i;
                var xmlRequestFrame = document.getElementById(callbackFrameID);
                if (xmlRequestFrame) {
                    xmlRequestFrame.parentNode.removeChild(xmlRequestFrame);
                }
            }
        }
    } 

    var txtemail = get_element_id('WebChat1__ctl0_txtEmail');
    var txtNickName = get_element_id('WebChat1__ctl0_txtNickName');
    var txtPhone = get_element_id('WebChat1__ctl0_txtPhone');
    var pContent = null;

