<script type="text/javascript">
    //<![CDATA[
function setPic(img,ht){
    try{   
      wf = img.offsetWidth;
      hf = img.offsetHeight;

      if (hf > ht) {
          img.height = ht;
          img.width = Number(ht * wf / hf);
      }
      else{
              img.width   =   wf;   
              img.height   =  hf;   
      }
    }catch(err){}
  }

//]]>      
