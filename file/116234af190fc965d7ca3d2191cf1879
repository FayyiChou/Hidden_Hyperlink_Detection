    get_element_id("txtInput").onfocus = function() { chatWinInputOnfocus(); stopTitleMsg(); };

    function _365webcall_isInstalled() {
        if (winType == 2) {
            var isT = true;

            try {
                window.parent._365webcall_frameDomain._365webcall_domain();
            }
            catch (err) {
                isT = false;
            }
            return isT;
        }
        else {
            if (_365webcall_bho_installed == 1)
                return true;
            else
                return false;
        }
    }

    function _365webcall_setBHOInstalled() {
        _365webcall_bho_installed = 1;
    }

    function Capture() {
        if (hold) {
            SayMsg(GetShowText("holdBackHintText"), "", "s");
            return;
        }

        if (typeof (captureParam) == "undefined")
            return;

        if (captureParam.length == 0) {
            alert(GetShowText("capture_alt"));
            return;
        }
        
        if (!WebCaptureInstalled) {
            if (checkWebCaptureLoadTime()) {
                var src = "CaptureHelp_en.htm";

                if (ChatLanguage == "0" || ChatLanguage == "1")
                    src = "CaptureHelp.htm";
                window.open(src);
            }
            return;
        }

        divDoCaptureFromIframe("1");
    }

    var panelInfo1 = get_element_id("panelInfo1");

    if (panelInfo1 != null) {
        setAttributeValue(panelInfo1, "defaultVal", get_width(panelInfo1) + "px");
        panelInfo1.style.width = 0;
    }

    var panelInfo2 = get_element_id("panelInfo2");

    if (panelInfo2 != null) {
        setAttributeValue(panelInfo2, "defaultVal", get_width(panelInfo2) + "px");
        panelInfo2.style.width = 0;
    }

    reSizeTab();
    window.setTimeout(reShowPanel, 1000);
    window.setTimeout(correctPNG, 500);

    var page_loader = get_element_id('page_loader');

    if (page_loader != null)
        page_loader.parentNode.removeChild(page_loader);

    //        page_loader.style.display = "none";

    //]]>      
  </script>

    

