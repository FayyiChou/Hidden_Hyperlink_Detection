<script>
var bodyfrm = ( document.compatMode.toLowerCase()=="css1compat" ) ? document.documentElement : document.body;
var adst = document.getElementById("ShowAD").style;
	adst.top = ( bodyfrm.scrollTop + 170) + "px";
	adst.left = ( bodyfrm.clientWidth -125 ) + "px";
function moveR() {
	adst.top = ( bodyfrm.scrollTop + 170) + "px";
	adst.left = ( bodyfrm.scrollLeft + bodyfrm.clientWidth - 125 ) + "px";
}
setInterval("moveR();", 80);


var adst1 = document.getElementById("ShowAD1").style;
	adst1.top = ( bodyfrm.scrollTop + 170) + "px";
	adst1.right = ( bodyfrm.clientWidth -125 ) + "px";
function moveR1() {
	adst1.top = ( bodyfrm.scrollTop + 170) + "px";
	adst1.right = ( bodyfrm.scrollLeft + bodyfrm.clientWidth - 125 ) + "px";
}
setInterval("moveR1();", 80);


