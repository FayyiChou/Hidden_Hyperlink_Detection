<script type="text/javascript">

	var jsLoadErrorDisplayed = false;
	
	function displayJSLoadError(errorType) {
		if (jsLoadErrorDisplayed) {
			return;
		}
		jsLoadErrorDisplayed = true;

		// Get the server date
		var serverTime = '2016-10-11';

		// Calculate the client date
		var dateObject = new Date();
		var date = dateObject.getDate() + "";
		if (date.length == 1) {
			date = '0' + date;
		}
		var month = (dateObject.getMonth()+1) + "";
		if (month.length == 1) {
			month = '0' + month;
		}
		var year = dateObject.getFullYear();

		// Find the correct error message
		var errorMessage = '';

		// Specific error
		if (errorType) {
			// Specific error
			errorMessage =
				'<p>Det ser ut til å ha oppstått en %errorType%-feil.</p>'.replace('%errorType%', errorType);
		} else if (serverTime !== year + '-' + month + '-' + date) {
			// The dates don't match
			errorMessage =
				'<p>Problemet ser ut til å være at datoen på datamaskinen din er feil innstilt. </p>' +
				'<p>Ifølge våre serverne våre er datoen i dag <strong>11-10-2016</strong>, mens din datamskin ser ut til å ha datoen <strong>' + date + '-' + month + '-' + year + '</strong></p>' +
				'<p>Lukk nettleseren, sett datoen på datamaskinen til <strong>11-10-2016</strong> og prøv å gå inn igjen på MatteMestern.no.</p>';
		} else {
			// Generic error
			errorMessage =
				'<p>Det ser ut til å ha oppstått en sertifikatfeil.</p>' +
				'<p><a href="//files.mattemestern.no">Klikk her for å prøve å løse problemet.</a></p>';
		}

		// Print error to browser
		document.body.style.fontSize = '1em';
		document.body.style.background = 'none';
		document.body.innerHTML = '<div style="font-family: georgia, serif; max-width: 65em; margin: 7em auto; box-shadow: 0 .1em 0.2em rgba(0, 0, 0, .3); padding: 1em 2em 1.5em; background: #f5f5f5;">' +
			'<h1 style="font-size: 2em">Det er litt problemer med å hente MatteMestern.no...</h1>' +
			errorMessage +
			'<p>Hvis problemet fortsetter, bør du ta kontakt med support på telefon +45 88 82 66 77 eller e-post <a href="mailto: support@mattemestern.no">support@mattemestern.no</a>.</p>' +
			'</div><script type="text/javascript">' +
			'try {NREUM.noticeError(new Error(\'JS load error. May be certificate error.\', \'JSLoadError\'));}catch(er){}' +
			'<'+'/script>';
	}

