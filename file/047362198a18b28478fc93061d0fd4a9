<Script language=javascript>
function on_Stat_Submit(){
var hadsel;
hadsel = false;
for(var i=0;i<Stat_Form.DOC_ARCH_ID.length;i++){
	if (Stat_Form.DOC_ARCH_ID[i].checked){
		hadsel = true;
	}
}
	if (!hadsel){
		alert("请选择一项问卷答题！");	
		return false;
	}
	Stat_Window = window.open("","Stat_Window","top=150, left=200,height=220,width=500,resizable=no,scrollbars=yes,status=no,toolbar=no,menubar=no,location=no");
	Stat_Window.focus();
}
function openStat_Window(){
var Stat_Window;
	Stat_Window = window.open("/Stat/Stat_Submit.asp?SYS_ID=9","Stat_Window","top=150, left=200,height=220,width=500,resizable=no,scrollbars=yes,status=no,toolbar=no,menubar=no,location=no");
	Stat_Window.focus();
}
</Script>
