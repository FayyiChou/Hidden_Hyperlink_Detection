<script type="text/javascript">
var search_key_tag = $("#search_key_tag").val();
function search_tag_submit(){
	var search_type = $("#search_type").val();
	var keyWord=$('#'+search_key_tag).val();
	keyWord=$.trim(keyWord);
	if(keyWord==''){
		alert('请填写关键词！');
		return false;
	}else{
        		if(search_type==11){
			$('#navtype').val('doc');
		}
		if(search_type==14){
			$('#navtype').val('ser');
		}
		if(search_type==10){
			$('#navtype').val('pro');
		}
		if(search_type==4){
			$('#navtype').val('blog');
		}
		if(search_type==7){
			$('#navtype').val('bbs');
		}
		if(search_type==32){
			$('#navtype').val('zd');
		}
		if(search_type==31){
			$('#navtype').val('baike');
		}
		if(search_type==15){
          $('#navtype').val('item');
        }
        		$('#search_tag_form').submit();
	}
}
</script>            
    
