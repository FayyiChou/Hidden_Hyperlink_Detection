<script type="text/javascript">

    function toFourYearPate() {

        window.open("/html/htmlController/toFourYearPage.do");
    }


    var error = "";
    var cusName = "";
    /**
     * 检测登陆账号是否存在
     */
    function checkLogin() {
        document.getElementById("login-find-error").style.display = "none";
        document.getElementById("login-pass-error").style.display = "none";
        var cusName = $("#cusName").val();
        if (cusName.length != 0) {
            $.get("/customer/loginController/checkCusName.do", {mobile: cusName}, function (res) {
                if (res == "ok") {
                    document.getElementById("login-find-error").style.display = "block";
                    $("#login-null-error").css("display", "none");
                    $('#cusName').val("");
                    $('#cusName').focus();
                }
            });
        }

    }


    /**
     * 检查登陆信息返回结果（是否登陆成功）
     */
    function checkError() {
        document.getElementById("login-pass-error").style.display = "none";
        document.getElementById("login-find-error").style.display = "none";

        if (error == "error") {
            document.getElementById("login-pass-error").style.display = "block";
            $("#cusName").val(cusName);
            $("#passWord").val("");
            return false;
        }
        return true;
    }

    function login(e) {

        document.onkeydown = function (event) {
            e = event ? event : (window.event ? window.event : null);
            if (e.keyCode == 13) {
                checkSubmit();
            }
        }
    }

    function checkSubmit() {
        var cusName = $("#cusName").val();
        if (cusName.length == "") {

            $("#login-null-error").css("display", "block");
            $("#login-find-error").css("display", "none");
            return false;
        } else {
            $("#login-null-error").css("display", "none");

        }
        var passWord = $("#passWord").val();
        $("#passWord").val(hex_md5(passWord));
        $("#formLogin").submit();

    }
    $(function () {
        $("#cusName").blur(function () {
            $("#login-null-error").css("display", "none");
        })
    })

    function loginOut() {
//        alert("安全退出");
        $.get("http://wechat.gsjyb.com/scanLoginController/loginOut.do");
    }
    //增值判断添加字符串颜色

    $(".zengzhi").find(".pd_c").each(function(){
        if($(this).text().indexOf("-")!=-1){
            $(this).css("color","#16E03C")
        }else{
            $(this).css("color","#b8243e")
        }
    })

    function fundDetails(fundCode) {
        var mydate = new Date();
        var year = mydate.getFullYear();
        var month = mydate.getMonth() - 2;
        var quarter = 0;
        if (month >= 1 && month <= 3) {
            quarter = 1;
        } else if (month >= 4 && month <= 6) {
            quarter = 2;
        } else if (month >= 7 && month <= 9) {
            quarter = 3;
        } else if (month <= 0) {
            year -= 1;
            quarter = 4;
        }
        window.location.href = "/fund/FundController/fundDetails.do?fundCode=" + fundCode + "&year=" + year + "&quarter=" + quarter;
    }

    window.onload = function () {
        checkError();
        loginOut();
    }
</script>

</body>
