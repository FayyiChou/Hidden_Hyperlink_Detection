    (function(){
        $('.button01').click(function(){
            var key = jQuery.trim($('#key01').val());
            if(141849 == 603837 || 141849 == 607468){
                var url = "//mall.jd.com/view_search-" + 448158 + '-' + 141849 + '-' + 138919 + '-0-0-0-0-1-1-60.html';
            } else {
                var url = "//mall.jd.com/advance_search-" + 448158 + '-' + 141849 + '-' + 138919 + '-0-0-0-1-1-60.html';
            }


            var key = encodeURIComponent(encodeURIComponent(key));

            if(key!='') {
                url += '?keyword=' + key;
            }
            location.href = url;
        });
    })();
