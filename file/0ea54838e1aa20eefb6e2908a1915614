 (function(){
function $(id,tag){
 var re=(id&&typeof id!="string") ? id : document.getElementById(id);
 if(!re){return null;}
 if(!tag){
return re;
 }else{
return re.getElementsByTagName(tag);
 }}
function setWidth(id,img,left,right){
  if(!id && !img){return;}
  var oBox = $(id),oImg = $(img),
 oLeft = $(left)||null,
 oRight = $(right)||null;
 
  if(oImg.complete == true || oImg.readyState == 'complete' || oImg.readyState =='loaded'){
 set()
  }else{
 Event.add(oImg,'load',set);
  }
  function set(){
 oBox.style.width = oImg.offsetWidth+'px';
 oBox.style.height = oImg.offsetHeight + 'px';
 if(oLeft){
  oLeft.style.width = oImg.offsetWidth/2+'px';
  oLeft.style.height = oImg.offsetHeight+'px';
 }
 if(oRight){
  oRight.style.width = oImg.offsetWidth/2+'px';
  oRight.style.height = oImg.offsetHeight+'px';
 }
  }
 
 }
 setWidth('pbbImg_box','pbbImg_img','pbbImglf_prev','pbbImglf_next');
 })();
 </script>
