dataType:'xml',
})
.success(function(s) {
popup.open(s.lastChild.firstChild.nodeValue);
evalscript(s.lastChild.firstChild.nodeValue);
})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
}); 
return false;
});
</script> 
    <a href="javascript:;" title="返回顶部" class="scrolltop bottom"></a> 
