$("#mm-blocker").click(function(){
$("html").removeClass();
$(".viewShare").attr("class","viewShare mm-menu mm-offcanvas mm-bottom mm-front mm-autoheight");
});
$('.jiathis_button_weixin').click(function(){
$(".popMask").show();
$("#mm-blocker").trigger("click");
return false;
});
