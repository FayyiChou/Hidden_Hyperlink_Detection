  <script type="text/javascript">jQuery(document).ready(function () {
  var _navPop=$('.navPop_logined');
  $('.nav_logined').on('mouseenter',function(){
    _navPop.fadeIn(100);
  }).on('mouseleave',function(){
    _navPop.css('display','none');
  })

  var _items=$('.header .itemsBox').children('.item');
    var _pageStr="<li class='active'></li>";
    var _pageli=$('.pagelis');
    console.log(_pageli);
    var n=0;
    for(var i=0,len=_items.length;i<len-1;i++){
      _pageStr=_pageStr+"<li></li>";
    }
    _pageli.html(_pageStr);
    var n1=_pageli.children('li').length;
    _pageli.children('li').on('click',function(){
      if(!$(this).hasClass('active')){
        var index=$(this).index();
        $(this).addClass('active').siblings().removeClass('active');
        _items.css('display','none').eq(index).fadeIn(400);
      }
    });

    function scroll(){
      n=n<(n1-1)?n+1:0;
      _pageli.children('li').eq(n).trigger('click');
    }

    var t=setInterval(scroll,4000);

    $('.itemsBox').on('mouseenter',function(){
      clearInterval(t);
    }).on('mouseleave',function(){
      t=setInterval(scroll,4000);
    })

    $('.searchIcon').on('click', function () {
      $('#header-search').submit();
    });

            // image lazyload
            $("img.lazy").lazyload({
                threshold : 200
            });


            // 收藏
            $('#add-favorite').on('click', function () {
                var url = window.href,
                    title = '勤书网';
                try {
                    window.external.AddFavorite(url, title);
                } catch(e) {
                    try {
                        window.sidebar.addPanel(title, url, "");
                    } catch(e){
                        alert("抱歉，您所使用的浏览器无法完成此操作。加入收藏失败，请使用Ctrl+D进行添加");
                    }
                }
            });
});</script>   
 </body>
</html>