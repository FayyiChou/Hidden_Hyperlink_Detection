<script type="text/javascript">
seajs.use(['jquery'], function ($) {
    //返回顶部
    $(".yskj_con .a4").click(function () {
        $("html,body").animate({
            scrollTop: 0
        }, 500);
        return false;
    })
    $(".yskj_con .a3").click(function () {
        if (!$(this).hasClass("on")) {
            $(this).addClass("on");
            $('.yskj_con .pic').animate({
                'width': '80'
            }, 200)
        } else {
            $(this).removeClass("on");
            $('.yskj_con .pic').animate({
                'width': '0'
            }, 200)
        }
    })
})
