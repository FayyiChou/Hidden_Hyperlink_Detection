<script type="text/javascript">
    seajs.use(['jquery'], function ($) {
        $(function () {
            $('.p_o_left li').hover(function () {
                var pos_l = $(this).position();
                var list_r = $(this).find('.list_sub');
                var list_r_top;
                if (list_r.height() >= pos_l.top) {
                    list_r_top = pos_l.top;
                } else {
                    list_r_top = list_r.height() - $(this).height();
                }
                list_r.css({
                    'top': -(list_r_top) + 'px'
                });
            });
            $('.p_o_left li').hover(function () {
                $(this).addClass('fff');
                $(this).find('.list_sub').show();
            }, function () {
                $(this).removeClass('fff');
                $(this).find('.list_sub').hide();
            });

            $('.nav_main .h1').hover(function () {
                $(this).addClass('fff');
                $('.p_o_left').show();
            }, function () {
                $(this).removeClass('fff');
                $('.p_o_left').hide();
            })
        })
    })
</script>
