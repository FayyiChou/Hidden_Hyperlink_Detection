<script>
/*跳转Url model:0 菜单 model:1价格筛选 model:2 分页筛选*/
function goToUrl(btype, stype, postype, efftype, pricearea, brandid, seriesid, productid, orderby, page, model){
	var myUrl = '';
	switch(model){
		case 0 : 
			myUrl = "/list-"+btype+"-"+stype+"-"+postype+"-"+efftype+"-"+pricearea+"-"+brandid+"-"+seriesid+"-"+productid+"-"+orderby+"-1.html#xuanze";
			break;
		case 1 : 
			myUrl = "/list-"+btype+"-"+stype+"-"+postype+"-"+efftype+"-"+pricearea+"-"+brandid+"-"+seriesid+"-"+productid+"-"+orderby+"-1.html#paixu";
			break;
		case 2 : 
			myUrl = "/list-"+btype+"-"+stype+"-"+postype+"-"+efftype+"-"+pricearea+"-"+brandid+"-"+seriesid+"-"+productid+"-"+orderby+"-"+page+".html#paixu";
			break;
		default:
			myUrl = "";
	}
	if(model == 1 && pricearea == '0_0'){
		alert('请输入价格区间');
		return false;
	}
	if(model == 2 && page <= 1){
		alert('请输入大于1的页码');
		return false;
	}
	location.href = myUrl;
}
</script>
<script language="javascript">  
var flag;  //全局变量用于标识是否延时执行keyup事件
function myFunc1(){
	//不管存不存在flag这个延时执行函数，先清除
	clearTimeout(flag);
	//延时500ms执行请求事件，如果感觉时间长了，就用合适的时间
	//只要有输入则不执行keyup事件
	flag = setTimeout(function(){
		myFunc2();
	}, 500);
}
function myFunc2(){
	$(".pinpai li").each(function(){
		 //取得标签的文本
		 $(this).show();
		 var t = $(this).text();
		 $(this).find("a").html(t);
		 //取得需要查出的关键字，我们这里假定是多关键字以","间隔
		 var array = ($("#qbrandName").val()).split(",");
		 //开始用关键字遍历标签文本
		 for(var i=0;i<array.length;i++){
			 //判断标签是否包含关键字
			 if(t.indexOf(array[i])>-1){
				 //定义正则表达式对象  array[i]是关键字   "g"是指全局范围
				 var a = new RegExp(array[i],"g")
				 //对标签文本进行全局替换，包含关键字的位置替换为加红字span对象
				 t = t.replace(a,("<b style='color:red'>" + array[i] + "</b>"));
				 //将替换完的文本对象赋给此对象中A标签对象的html值中
				 $(this).find("a").html(t);
			 }else{
				$(this).hide();
			 }
		 }
	});
}
$("#qbrandName").keyup(function(){
	myFunc1();
});
</script>

