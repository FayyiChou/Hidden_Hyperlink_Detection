    function getCaptcha(){
        var nc = new noCaptcha();
        var nc_appkey = 'QNYX';  // 应用标识,不可更改
        var nc_scene = 'login';  //场景,不可更改
        var nc_token = [nc_appkey, (new Date()).getTime(), Math.random()].join(':');
        var nc_option = {
            renderTo: '#dom_id',
            appkey: nc_appkey,
            scene: nc_scene,
            token: nc_token,
            callback: function (data) {
                document.getElementById('csessionid').value = data.csessionid;
                document.getElementById('sig').value = data.sig;
                document.getElementById('token').value = nc_token;
                document.getElementById('scene').value = nc_scene;
            }
        };
        nc.init(nc_option);
    }
    formset({
        "id":"user_login_modal",
        "url":"user_loginaction",
        "rule":{
            "nameNormal":{
                required:true,
                ismobile:true
            },
            "pwdNormal":{
                required:true,
                minlength:6
            }
        },

        "messages":{
            "nameNormal":{
                required:"请输入手机号",
                ismobile:"请输入正确格式的手机号"
            },
            "pwdNormal":{
                required:"请输入密码",
                minlength:"密码最少6位"
            }
        },
        "sucfunc":function(){
            $('#loginModalClose').click();
            location.reload();
        },
        "falfunc":function(rs){
            faldia({'content':'登录失败：'+rs.msg});
            getCaptcha();
            document.getElementById('csessionid').value = '';
            document.getElementById('sig').value = '';
            document.getElementById('token').value = '';
            document.getElementById('scene').value = '';
        }
    });
