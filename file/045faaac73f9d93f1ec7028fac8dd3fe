	$(function(){
		
		/*引导注册文字变化*/
		function barAnimate(){
			$('#V3_Foot_An1').removeClass('bounceIn');
			$('#V3_Foot_An2').removeClass('bounceIn');
			$('#V3_Foot_An3').removeClass('bounceIn');
			$('#V3_Foot_An4').removeClass('bounceIn');
			
			$('#V3_Foot_An4').hide();
			$('#V3_Foot_An1').show();
			$('#V3_Foot_An1').addClass('bounceIn');
			setTimeout(function(){
				$('#V3_Foot_An1').hide();
				$('#V3_Foot_An2').show();
				$('#V3_Foot_An2').addClass('bounceIn');
				setTimeout(function(){
					$('#V3_Foot_An2').hide();
					$('#V3_Foot_An3').show();
					$('#V3_Foot_An3').addClass('bounceIn');
					setTimeout(function(){
						$('#V3_Foot_An3').hide();
						$('#V3_Foot_An4').show();
						$('#V3_Foot_An4').addClass('bounceIn');
					},3000);
				},3000);
			},3000);
		}
		barAnimate();
		setInterval(barAnimate,12000);
		
