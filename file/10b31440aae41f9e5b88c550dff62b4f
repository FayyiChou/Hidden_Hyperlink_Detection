	scrollPic_03.speed          = 10; //移动速度(单位毫秒，越小越快)
	scrollPic_03.space          = 10; //每次移动像素(单位px，越大越快)
	scrollPic_03.autoPlay       = true; //自动播放
	scrollPic_03.autoPlayTime   = 3; //自动播放间隔时间(秒)
	
	scrollPic_03.initialize(); //初始化
							
		//--><!]]>
	</SCRIPT>
	</DIV></DIV></DIV></DIV>
  </td>
</tr>
</table></div></td>
      </tr>
    </table>
    </td>
  </tr>
