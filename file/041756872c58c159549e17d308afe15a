arr[7]=["http://www.lztv.tv/topic_images/1008.jpg", "#"];

setInterval(changeImg,timeInterval);
function changeImg()
{
    var image=document.getElementById("showpic");
    if (curIndex==arr.length-1) 
    {
        curIndex=0;
    }
    else
    {
        curIndex+=1;
    }
    image.src=arr[curIndex][0];
    image.parentNode.href = arr[curIndex][1];
}
