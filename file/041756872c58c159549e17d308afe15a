<script language =javascript >
 var curIndex=0;
 //时间间隔 单位毫秒
 var timeInterval=3000;
 var arr=new Array();
//arr[0]=["http://www.lztv.tv/topic_images/zgdx.jpg", "#"];
arr[1]=["http://www.lztv.tv/topic_images/hhzd.jpg", "http://www.lztv.tv/view/view_74709.html"];
arr[2]=["http://www.lztv.tv/topic_images/hlzq.jpg", "http://www.lztv.tv/news/news_523_0.html"];
