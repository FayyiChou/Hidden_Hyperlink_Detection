<script>
    $(function(){
        $(".submenu").hover(function(){
            $(this).addClass("on");
            $(this).find(".box").show();
        },function(){
            $(this).removeClass("on");
            $(this).find(".box").hide();
        });

        $(".yx-rotaion").yx_rotaion({
            auto:true,
            title:false,
            width:'1008px',
            height:'428px'
        });

        $('.turn').hover(function() {
            var id = $(this).attr('rel');
            $('#' + id).show().siblings('.info_con').hide();
        });

        $("#ijzbox li").hover(function(){
            $("#ijzbox li").find(".img").hide();
            $("#ijzbox li").find(".txt").show();
            $(this).find(".img").show();
            $(this).find(".txt").hide();
        });

        $(".videoBox .item").hover(function(){
            $(this).find(".txtbox").stop();
            $(this).find(".playicon").hide();
            $(this).find(".txtbox").animate({top:"-45px"},500);
        },function(){
            $(this).find(".txtbox").stop();
            $(this).find(".playicon").show();
            $(this).find(".txtbox").animate({top:"0px"},500);
        });

    });
</script>

