function caluSubTop(count){
	var top =  jQuery("#submenu_id").css("top");
   
	if(count==1) {
		jQuery("#submenu_id").css("top","31px");
	}else{ 
		 
		var val = 31 +((count-1)*31);
		jQuery("#submenu_id").css("top",val+"px");
	}  
}

var to;
function mouseOut(){  
	flag = true;
	to = window.setTimeout("hiddenAll()",1000);
}

function hiddenAll(){
 
	if(flag == null){
		return;
		window.clearTimeout(to);
	}
	document.getElementById('rightmenu_id').style.display='none';
	document.getElementById('submenu_id').style.display='none';
}

function subOver(){
	flag = null;
}

function allOver(){
	flag = null;//clear out flag
	document.getElementById('rightmenu_id').style.display='block';
}

function allout(){
	flag = true;
	window.setTimeout("hiddenAll()",1000);   
}
