     <script type="text/javascript">
     function HiddenStudyDiv(){
        if(isHidden)
     		document.getElementById("map_div").style.display= "none";
      	document.getElementById("studymap").Reset();
     }
     
	 //flash鼠标移出后调用的js，进行延时关闭层
     function HandleMapDiv(){
    	 setout();
     }
     var isHidden = true;
     var hiddentuu;
     function setnew(){
     	 isHidden = false;
     	 window.clearTimeout(hiddentuu);
     }
     function setout(){
     	 isHidden = true;
     	 window.clearTimeout(hiddentuu);
     	 hiddentuu = window.setTimeout('HiddenStudyDiv()',2000);
     }

     function onSubmitStudySit(){
       
    	 var x=document.getElementById("siteForm"); 
		 x.method="post";
		 x.target="_blank";
		 x.action='/assist/studysite.action';
		 x.submit();
     }
     function ShowCity(data){
          
         	//var datas = data.split('|');
         	var cityname = data;//datas[0];
         	var x = 214;
         	var y = 28;
     
         	var city = getCity(cityname);
         	var count= getMenuCount(cityname);
         	document.getElementById('cityName_id').innerHTML=city;
         	document.getElementById('map_div').style.left=(x-5)+"px";
         	document.getElementById('map_div').style.top=(y)+"px";
     
         	jQuery("#map_div").css("border-color",getBorderColor(cityname)); 
         	getStudySite(city)
          
		    document.getElementById("menuLiId").value="menuLi"+count; 
 
		    document.getElementById("cityNameId").value=city; 
         	
         	document.getElementById("map_div").style.display= "block";
         	hiddentuu = window.setTimeout('HiddenStudyDiv()',3000);
	 }

     function getCity(city){
 		if(city=="sz"){
 			return "苏州市";
 		}else if(city=="nt"){
 			return "南通市";
 		}else if(city=="yc"){
 			return "盐城市";
 		}else if(city=="xz"){
 			return "徐州市";
 		}else if(city=="lyg"){
 			return "连云港市";
 		}else if(city=="sq"){
 			return "宿迁市";
 		}else if(city=="ha"){
 			return "淮安市";
 		}else if(city=="nj"){
 			return "南京市";
 		}else if(city=="cz"){
 			return "常州市";
 		}else if(city=="wz"){
 			return "无锡市";
 		}else if(city=="zj"){
 			return "镇江市";
 		}else if(city=="tz"){
 			return "泰州市";
 		}else if(city=="yz"){
 			return "扬州市";
 		}else{
 			return "";
 		}
 	}

	 function getMenuCount(city){
		if(city=="sz"){
			return 5;
		}else if(city=="nt"){
			return 6;
		}else if(city=="yc"){
			return 9;
		}else if(city=="xz"){
			return 3;
		}else if(city=="lyg"){
			return 7;
		}else if(city=="sq"){
			return 13;
		}else if(city=="ha"){
			return 8;
		}else if(city=="nj"){
			return 1;
		}else if(city=="cz"){
			return 4;
		}else if(city=="wx"){
			return 2;
		}else if(city=="zj"){
			return 11;
		}else if(city=="tz"){
			return 12;
		}else if(city=="yz"){
			return 10;
		}else{
			return 0;
		}
	 }
	 function getBorderColor(city){
			if(city=="sz"){
				return "#ffcc66";
			}else if(city=="nt"){
				return "#7a97cc";
			}else if(city=="yc"){
				return "#f5ce86";
			}else if(city=="xz"){
				return "#f18953";
			}else if(city=="lyg"){
				return "#5e96d5";
			}else if(city=="sq"){
				return "#bfb198";
			}else if(city=="ha"){
				return "#ee6b29";
			}else if(city=="nj"){
				return "#fad130";
			}else if(city=="cz"){
				return "#66cc33";
			}else if(city=="wx"){
				return "#de6872";
			}else if(city=="zj"){
				return "#53c0f4";
			}else if(city=="tz"){
				return "#da505a";
			}else if(city=="yz"){
				return "#73a5b0";
			}else{
				return "";
			}
		}
     </script>
      
