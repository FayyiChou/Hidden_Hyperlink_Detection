<script>
function onLoadSearch(){
	if(document.getElementById('headerIndexSelect').value=="2")
		document.getElementById("basicHeaderSearchForm").action="/news/article!search.action";
}
var curMenuName = null;
//用于栏目管理 的级联查询取数据
$(document).ready(function() {
	$.ajax({
		url:"http://www.js-study.cn/course/study/msgajax.excsec",
		type:"post",
		success: function(html){
			if(html.msgJson){
				var data =  eval('('+html.msgJson+ ')');
				if(data.count){
					$("#msgJson").html(data.count);
				}
			}
		  }
	});
	
	//根据一级栏目获得二级栏目
	$('a[name="one_menu"]').bind({
		mouseover:function() {
			curmenu = this;
			hideOthers();
			var submenu_id = this.id+"_sub";
			$('#'+submenu_id).show();
			this.className="nav_hover";
		},
		mouseout:function() {
			if(event.toElement && this.contains && !this.contains(event.toElement) && !$(event.toElement).parent().is(".sub_nav"))
			{
				this.className = "";
				var objid = this.id;
				var submenu_id = objid+"_sub";
				$('#'+submenu_id).hide();
				$('#'+objid).attr('className','');
			}
		}
	});
	$('.sub_nav').mouseout(function() {
		if(event.toElement && this.contains && !this.contains(event.toElement))
		{
			var cur = $(this);
			var submenu_id = cur.attr("id");
			$('#'+submenu_id).hide();   
			$('#'+submenu_id.substring(0, submenu_id.indexOf("_"))).attr('className','');
			displayCurMenu();
		}
	});
	$('.menu_bg').mouseout(function() {
		if(event.toElement && this.contains && !this.contains(event.toElement) && !$(event.toElement).parent().is(".sub_nav"))
		{
			displayCurMenu();
		}
	});
	$('#studyManage').bind({
		mouseover:function() {
			$('#studyManage').show();
		},
		mouseout:function() {
			$('#studyManage').hide();
		}
	});
	curMenuName = $('meta[name="menu"]').attr("content")
	displayCurMenu();
	handleclick(curMenuName);

	//获得是否显示学习圈
	$.post("http://www.js-study.cn/course/course/metadata.excsec",function(data) {
			var val = data.value;
			if(val=="1"){
				$("#bbsHref").show();
			}else if(val=="2")
				$("#bbsHref").hide();
		});
})

function displayCurMenu(){
	$('#'+curMenuName).attr('className','nav_selected');
}

//先隐藏其他的
function hideOthers(){
	$('#jydt_sub').hide();
	$('#scenter_sub').hide();
	$('#sjts_sub').hide();
	$('#help_sub').hide();
	$('#index').attr('className','');
	$('#aboutme').attr('className','');
	$('#jydt').attr('className','');
	$('#sjts').attr('className','');		
	$('#scenter').attr('className','');
	$('#hdjl').attr('className','');
	$('#help').attr('className','');		
}

function hiddenMenu(objid){
	var submenu_id = objid+"_sub";
	$('#'+submenu_id).hide();
	$('#'+objid).attr('className','');
}

function handleover(parentMenuid){	
	
	$('#'+parentMenuid+'_sub').show();
	$('#'+parentMenuid).attr('className','nav_selected');
}

function handleclick(parentMenuid){	
	$('#'+parentMenuid+'_sub').hide();
	$('#'+parentMenuid).attr('className','nav_selected');
}

function switchStudyManage(){
	var displayAttr = $('#studyManage').css("display");
	if(displayAttr == 'block')
		$('#studyManage').hide();
	else
		$('#studyManage').show();
}

function getTwoCats(id,url){
	$("#ta_c_id").empty();
	$.post(url,{"id":id},function(data) {
		 
		if(data != null){
			var objVal = data.cats;
			var courseCats = eval(objVal);
	 
			if(courseCats.length == 0) {  
				document.getElementById('submenu_id').style.display="none";  
				return;
			} 
			$("#ta_c_id").empty();
			
			for(var i = 0 ;i < courseCats.length ; i++) {
				$("#ta_c_id").append(" <li><a href='http://www.js-study.cn/course/course/course.excsec?oneCatId="+id+"&twoCatId="+courseCats[i].value+"'>"+courseCats[i].label+"</a></li> ");
			}
			jQuery("#submenu_id").css("border","1px solid #61befb");
		}
	}); 
} 
 
//首页中关键字查询 
function onFindByKey(formId,selectId,textId,hidId,flagId){  
	var selIndex=$("#"+selectId).get(0).selectedIndex;
	
	if(0==selIndex){ 
		$('#'+flagId).removeAttr("disabled");
		$("#"+hidId).attr("name","courseIndexQueryVO.name");
		$("#"+formId).attr("action","http://www.js-study.cn/course/course/course.excsec");
	}else if(1==selIndex){ 
		$('#'+flagId).attr('disabled',"true"); 
		$("#"+hidId).attr("name","articleIndexQueryVO.title"); 
		$("#"+formId).attr("action","/news/article!search.action");
	} 
	
	var textVal = $("#"+textId).val();
	if(null!=textVal && ""!=textVal) 
		$("#"+hidId).val(textVal);
	 
	$("#"+formId).submit();
} 
 
//学习网站地图
function getStudySite(city){
		$.post("./news/relatedmap!getStudySites.excsec",{"city":city},function(data) {
			if(data!=null){
				
				var studySite = eval(data.maps);
				document.getElementById("oneMapStu").innerHTML="";
				document.getElementById("twoMapStu").innerHTML="";
				if(studySite == null) {
					document.getElementById("map_count").innerHTML="0";
					return;
				}
				if(studySite.length<=5){
		
					for(var i = 0 ;i < studySite.length ; i++) {
						$("#oneMapStu").append("<li onmouseover='setnew()'><a target='_blank' href='"+studySite[i].value+"' target='_blank'>"+studySite[i].label.substring()+"</a></li>");
					}
				}else{
					for(var i = 0 ;i < 5 ; i++) {
						$("#oneMapStu").append("<li onmouseover='setnew()'><a target='_blank' href='"+studySite[i].value+"' target='_blank'>"+studySite[i].label+"</a></li>");
					}
					for(var i = 5 ;i < studySite.length ; i++) {
						$("#twoMapStu").append("<li onmouseover='setnew()'><a target='_blank' href='"+studySite[i].value+"' target='_blank'>"+studySite[i].label+"</a></li>");
					}					
				}
				var count =  eval('('+data.mapsCount+')') ;
	    		document.getElementById("map_count").innerHTML=count.value;
			}
		}); 
	}
if(window.addEventListener) { FixPrototypeForGecko(); }  
 
  function  FixPrototypeForGecko()
  {
      HTMLElement.prototype.__defineGetter__("runtimeStyle",element_prototype_get_runtimeStyle);
      window.constructor.prototype.__defineGetter__("event",window_prototype_get_event);
      Event.prototype.__defineGetter__("srcElement",event_prototype_get_srcElement);
      Event.prototype.__defineGetter__("fromElement",  element_prototype_get_fromElement);
      Event.prototype.__defineGetter__("toElement", element_prototype_get_toElement);
 
  }  
 
  function  element_prototype_get_runtimeStyle() { return  this.style; }
  function  window_prototype_get_event() { return  SearchEvent(); }
  function  event_prototype_get_srcElement() { return  this.target; }  
 
  function element_prototype_get_fromElement() {
      var node;
      if(this.type == "mouseover") node = this.relatedTarget;
      else if (this.type == "mouseout") node = this.target;
      if(!node) return;
      while (node.nodeType != 1)
          node = node.parentNode;
      return node;
  }
 
  function  element_prototype_get_toElement() {
          var node;
          if(this.type == "mouseout")  node = this.relatedTarget;
          else if (this.type == "mouseover") node = this.target;
          if(!node) return;
          while (node.nodeType != 1)
             node = node.parentNode;
          return node;
  }
 
  function  SearchEvent()
  {
      if(document.all) return  window.event;  
 
      func = SearchEvent.caller;  
 
      while(func!=null){
          var  arg0=func.arguments[0];  
 
          if(arg0 instanceof Event) {
              return  arg0;
          }
         func=func.caller;
      }
      return   null;
  }
