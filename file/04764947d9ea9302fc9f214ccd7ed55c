function displaySubDiv(catId,count){
	document.getElementById('submenu_id').style.border='1px';
	document.getElementById('submenu_id').style.display='block';
    getTwoCats(catId,'http://www.js-study.cn/course/course/relatedcoursecat.excsec');
 
