<script type="text/javascript">
//根据一级栏目获得二级栏目
var curMenuNameCat = null;
var didplayCurMenuCat = true;
 
var curMenuNameCatMore = null;
var didplayCurMenuCatMore = true;
 
$(document).ready(function() {
	
//上面三个一级课程菜单的onmouseover,onmouseout事件操作
	//根据一级栏目获得二级栏目
	$('a[name="one_cat"]').bind({
		mouseover:function() {
			curMenuNameCat = this;
			hideOthersCat();
		 
			var submenu_id = this.id+"_sub"; 
			var subId = this.id;

			if(subId=="onecat"){
				if(true){
					$('#'+submenu_id).show();
					this.className="hover";
					didplayCurMenuCat = false;
				}else{
					this.className="hoverAll";
				}
			}else if(subId=="threecat"){
				if(true){
					$('#'+submenu_id).show();
					this.className="hover";
					didplayCurMenuCat = false;
				}else{
					this.className="hoverAll";
				}
			} else if(subId=="fivecat"){
				if(true){
					$('#'+submenu_id).show();
					this.className="hover";
					didplayCurMenuCat = false;
				}else{
					this.className="hoverAll";
				}
			} 
		},
		mouseout:function() {
			this.className = "";
			hiddenMenuCat(this.id);
		}
	}); 

	$('a[name="two_cat"]').bind({
			mouseover:function() {
			curMenuNameCatMore = this;
			hideOthersCat();

			var submenu_id = this.id+"_sub";
			var subId = this.id;

			if(subId=="twocat"){
				if(true){
					$('#'+submenu_id).show();
					this.className="hover2";
					didplayCurMenuCatMore = false;
				}else{
					this.className="hoverAll2";
				}
			}else if(subId=="fourcat"){
				if(true){
					$('#'+submenu_id).show();
					this.className="hover2";
					didplayCurMenuCatMore = false;
				}else{
					this.className="hoverAll2";
				}
			} else if(subId=="sixcat"){
				if(true){ 
					$('#'+submenu_id).show();
					this.className="hover2";
					didplayCurMenuCatMore = false;
				}else{ 
					this.className="hoverAll2";
				}
			} 
		},
		mouseout:function() {
			this.className = "";
			hiddenMenuCatMore(this.id);
		}
	}); 
})

//先隐藏其他的(上面三个菜单)...................
function hideOthersCat(){
	$('#onecat_sub').hide();
	$('#twocat_sub').hide();
	$('#threecat_sub').hide();
	$('#fourcat_sub').hide();
	$('#fivecat_sub').hide();
	$('#sixcat_sub').hide();
	$('#onecat').attr('className','');
	$('#twocat').attr('className','');
	$('#threecat').attr('className','');
	$('#fourcat').attr('className','');
	$('#fivecat').attr('className','');
	$('#sixcat').attr('className','');
}

function hiddenMenuCat(objid){
	var submenu_id = objid+"_sub";
	$('#'+submenu_id).hide();
	$('#'+objid).attr('className','');
	didplayCurMenuCat = true;
	timeoutDidplayCurMenuCat();
}

function timeoutDidplayCurMenuCat(){
	window.setTimeout("disPlayCurMenuCat()",500);
}

function disPlayCurMenuCat(){
	if(!didplayCurMenuCat)return;
	$('#'+curMenuNameCat).attr('className','hover');
}

function handleoverCat(parentMenuid){	
	$('#'+parentMenuid+'_sub').show();
	$('#'+parentMenuid).attr('className','hover');
	didplayCurMenuCat = false;
}

//

function hiddenMenuCatMore(objid){
	var submenu_id = objid+"_sub";
	$('#'+submenu_id).hide();
	$('#'+objid).attr('className','');
	didplayCurMenuCatMore = true;
	timeoutDidplayCurMenuCatMore();
}

function timeoutDidplayCurMenuCatMore(){
	window.setTimeout("disPlayCurMenuCatMore()",500);
}

function disPlayCurMenuCatMore(){
	if(!didplayCurMenuCatMore)return;
	$('#'+curMenuNameCatMore).attr('className','hover2');
}

function handleoverCatMore(parentMenuid){	
	$('#'+parentMenuid+'_sub').show();
	$('#'+parentMenuid).attr('className','hover2');
	didplayCurMenuCatMore = false;
}
 
</script>
     

