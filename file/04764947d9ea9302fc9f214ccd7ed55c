<script src="/course/js/jquery.js" type="text/javascript"></script> 
 
<style>
.courseteach_list .hoverAll{
	background:#fff;
     border:1px solid #61befb;
     height: 32px;
	line-height: 32px;
	width:98px;
	text-align: center;
	font-weight:bold;
	text-decoration:none;
	cursor:pointer;
	margin-bottom:5px;
	color:#017CC8;
	display:block; 
	}

.courseteach_list2 .hoverAll2{
	background:#fff;
     border:1px solid #61befb;
     height: 32px;
	line-height: 32px;
	width:98px;
	text-align: center;
	font-weight:bold;
	text-decoration:none;
	cursor:pointer;
	margin-bottom:5px;
	color:#017CC8;
	display:block; 
	}	
