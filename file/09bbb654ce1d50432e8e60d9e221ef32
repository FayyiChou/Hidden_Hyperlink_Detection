<script type="text/javascript">
    var showDialogJspIndex=0;
    $(function () {
        loginSelectEnrollmentYearLoad();
        //placeholder修复ie不支持
        JPlaceHolder.init(); 
        //密码显示隐藏
        $(".show-pw-btn").click(function () {
            if (!$(this).hasClass("blur")) {
                $(this).addClass("blur");
                var pwval = $(this).siblings(".pw-ipt-hide").attr("value");
                $(this).siblings(".pw-ipt-show").attr("value", pwval).show();
                $(this).siblings(".pw-ipt-hide").hide();
            } else {
                $(this).removeClass("blur");
                var pwval = $(this).siblings(".pw-ipt-show").attr("value");
                $(this).siblings(".pw-ipt-hide").attr("value", pwval).show();
                $(this).siblings(".pw-ipt-show").hide();
            }
        })
        //登录注册切换
        $(".lg-rg-switch-tab").delegate("span", "click", function () {
            var $this = $(this);
            if (!$this.hasClass("cur")) {
                $this.siblings("span").removeClass("cur");
                $this.addClass("cur");
                $(".lg-rg-content-wrap").hide();
                $(".lg-rg-content-wrap").eq($this.index()).show();
                userLoginNew.clearLayerHtml();
            }
        })


        //错误提示
//		$(".lg-rg-operate-btn").on("click",function(){
//			$(".form-ipt-error").addClass("is-visible");
//			$(".misc-supplement-wrap").slideDown();
//		})

    })
    /**
     * 加载入学年份
     */
    function loginSelectEnrollmentYearLoad() {
        var year = (new Date()).getFullYear();
        var yearStr = "<option  value='-1' selected='selected'  >入学年份</option> " +
                "<option value='" + year + "'>" + year + "</option>" +
                "<option value='" + (year - 1) + "'>" + (year - 1) + "</option>" +
                "<option value='" + (year - 2) + "'>" + (year - 2) + "</option>" +
                "<option value='" + (year - 3) + "'>" + (year - 3) + "</option>" +
                "<option value='" + (year - 4) + "'>" + (year - 4) + "</option>" +
                "<option value='" + (year - 5) + "'>" + (year - 5) + "</option>" +
                "<option value='" + (year - 6) + "'>" + (year - 6) + "</option>" +
                "<option value='" + (year - 7) + "'>" + (year - 7) + "</option>" +
                "<option value='" + (year - 8) + "'>" + (year - 8) + "</option>" +
                "<option value='" + (year - 9) + "'>" + (year - 9) + "</option>";
        $("#loginSelectEnrollmentYear").html(yearStr);
        $("#registerSelectEnrollmentYear").html(yearStr);
        $("#completeSelectEnrollmentYear").html(yearStr);

    }

    //判断浏览器是否支持 placeholder属性
    var JPlaceHolder = {
	    //检测
	    _check : function(){
	        return 'placeholder' in document.createElement('input');
	    },
	    //初始化
	    init : function(){
	        if(!this._check()){
	            this.fix();
	        }
	    },
	    //修复
    	fix : function(){
	        $(':input[placeholder]').not(".hidden").each(function(index, element) {
	            var self = $(this), txt = self.attr('placeholder');
	            var pos = self.position(), h = self.outerHeight(true), paddingleft = self.css('padding-left'),fontsize = self.css('font-size');
	            var holder = $('<span></span>').text(txt).addClass("placeholder").css({position:'absolute', left:"0px", top:"0px", height:h+'px', lineHeight:h+'px', paddingLeft:paddingleft, color:'#999999',fontSize:fontsize}).appendTo(self.parent());
	            if(!self.val()){
	            	holder.show();
	            }else{
	            	holder.hide();
	            }
	            self.focusin(function(e) {
	            }).focusout(function(e) {
	                if(!self.val()){
	                    holder.show();
	                }
	            });
	            holder.click(function(e) {
	                self.focus();
	            });
                self.keyup(function(){
                	holder.hide();
                });
	        });
	    }
	};
</script>



