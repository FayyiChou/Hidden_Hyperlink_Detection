	function switchingBackground(){
	        var urlImage = "http://image.zhihuishu.com/testzhs/myuni/home/201703/018a0f2acaf04bed969a9a85ddd65f37.jpg";
	        if(urlImage =="" || urlImage ==null ) {
	        	var arr = ['https://image.zhihuishu.com/testzhs/ablecommons/demo/201702/e389898488d343588a6fdf8f8e0b576d.jpg','https://image.zhihuishu.com/testzhs/ablecommons/demo/201702/69d9ca75524949afa34dec811c7f9d89.jpg','https://image.zhihuishu.com/testzhs/ablecommons/demo/201702/ece175747d0f45df997879a2fd275e60.jpg'];
				var a = parseInt((Math.random()*3));
				urlImage =arr[a];
	        }
			
			$('.homepage').css({
			   backgroundImage: "url('" + urlImage + "')",
			});
   
	}
switchingBackground();

</script>



