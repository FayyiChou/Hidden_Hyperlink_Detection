<script type="text/javascript">
    var hSchoolName = "长春工程学院";
    var hSchoolCode = "ccit";
    var hSchoolId = "396";
    var headType = "0";
    var isComplete="";
    var uSchoolId="";
    var uCollegeId="";
    var uGrade="";
    var uCode="";
    var uRealName="";
    var uId="";
    $(document).ready(function () {
        $(".schIndexInner_box .menu-search-btn").bind("click", function () {
            if (!$(this).hasClass("blur")) {
                $(this).addClass("blur");
                $(".zhs-mooc-logo").fadeOut(300);
                $(".menu-search-wrap").fadeIn(300);
                $(".menu-search-ipt input").focus();
            } else {
                $(this).removeClass("blur");
                $(".zhs-mooc-logo").fadeIn(300);
                $(".menu-search-wrap").fadeOut(300);
            }
        });
    });
    //重写js
    if (typeof String.prototype.startsWith != 'function') {
        String.prototype.startsWith = function (prefix) {
            return this.slice(0, prefix.length) === prefix;
        };
    }
    if (typeof String.prototype.endsWith != 'function') {
        String.prototype.endsWith = function (suffix) {
            return this.indexOf(suffix, this.length - suffix.length) !== -1;
        };
    }


</script>


