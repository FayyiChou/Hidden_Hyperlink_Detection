yn=1&rand="+Math.random();createscript(a,e,null,t,"utf-8")}function createscript(e,a,t,n,i){var i=i||"utf-8";window[e]=function(n){t?a.call(t,n):a(n);try{r.removeChild(l),window[e]=null}catch(i){}};var r=document.getElementsByTagName("head")[0],l=document.createElement("script");l.charset=i,l.src=n,r.appendChild(l)}function showlogin(){$("#username_div").val(""),$("#password_div").val(""),$("#hide_msg").show()}function CheckLogin(e){var a=(new Date).getTime();a="callback_"+a;var t="http://user.tgbus.com/api/action.ashx?callback="+a+"&t=uinfo&rand="+Math.random();createscript(a,e,null,t,"utf-8")}function IsLogin(e){(new Date).getTime();e?"success"==e.msg&&($("#u_info").html("您好,<a href='http://user.tgbus.com' target='_blank'>"+e.data.username+"</a> | <a href='javascript:;' onclick='logout();'>退出</a>").show(),$("#hide_msg").hide(),$("#denglu").hide(),$("#zhuce").hide(),$("#bdhide").hide()):CheckLogin(IsLogin)}$(document).ready(function(){IsLogin()})</script>
