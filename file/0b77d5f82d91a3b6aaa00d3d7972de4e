<script type="text/javascript">
$(document).ready(function(e) {

});

function newsJoin(){
	mailAlan = $('#email_newsletter').val();

	var atpos=mailAlan.indexOf("@");
	var dotpos=mailAlan.lastIndexOf(".");	

	if(mailAlan == ''){
		alert('Please fill e-mail blank.');
	}else if(atpos<1 || dotpos<atpos+2 || dotpos+2>=mailAlan.length){
		alert("Not a valid e-mail address.");
	}else{
		$('#emailNAlan').html('<p style="text-align:right; margin-top:10px;">Waiting...</p>');
		$.post("newsJoin.php",{'email':mailAlan},function(result){
			$('#emailNAlan').html('<p style="text-align:right; margin-top:10px;">'+result+'</p>');
		});
	}
	return false;
}
</script>
