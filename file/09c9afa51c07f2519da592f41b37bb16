function checkByteLength(str,minlen,maxlen) {

	if (str == null) return false;

	var l = str.length;

	var blen = 0;

	for(i=0; i<l; i++) {

		if ((str.charCodeAt(i) & 0xff00) != 0) {

			blen ++;

		}

		blen ++;

	}

	if (blen > maxlen || blen < minlen) {

		return false;

	}

	return true;

}

function validateEmail(obj){

	var str = obj.value;

	

	if(!checkByteLength(str,1,50)) return 1;

	var patn = /^[_a-zA-Z0-9\-]+(\.[_a-zA-Z0-9\-]*)*@[a-zA-Z0-9\-]+([\.][a-zA-Z0-9\-]+)+$/;

	if(!patn.test(str)){

		return 1;//incorrect format

	}

	return 0;	

}

function check_askform(obj)

{

	hidDiv("mysubmitbutton2") ;

	var leftChars = obj.qtitle.value.length;

	if(leftChars < 1)

	{

	      alert("问题不够详细，请补充");

	      obj.qtitle.focus();

		  showDiv("mysubmitbutton2") ;

	      return false;    

	}

	if(obj.qcontent.value == '') {   

	      alert("请填写问题内容");

	      obj.qcontent.focus();

		  showDiv("mysubmitbutton2") ;

	      return false;    

	}

	if(obj.qcontent.value.length >500) 

	{   

	      alert("问题描述过长，字数限定在500字节以内");

	      obj.qcontent.focus();

		  showDiv("mysubmitbutton2") ;

	      return false;    

	}

	

	if(obj.qtitle.value =="" || obj.qtitle.value.length ==0)

	{

		alert("请正确写下您的问题！");obj.qtitle.focus();showDiv("mysubmitbutton2") ; return false;

	}    

	if(obj.country.value==''){

		alert("请选择地区");obj.province.focus();showDiv("mysubmitbutton2") ; return false;

	}



	if (obj.email.value && validateEmail(obj.email)) {alert("email填写有误？");obj.email.focus();showDiv("mysubmitbutton2") ;return false};

	if ($('#tel').val() == '') {
		pubmobile.showMsg(pubmobile.remindid, '手机号未填写', 1);
		showDiv("mysubmitbutton2");
		return false;
	}
	if (!pubmobile.checkTel()) {
		alert('请先验证您的手机号');
		$('#tel').focus();
		showDiv("mysubmitbutton2");
		return false;
	}

	//$("#mysubmitbutton2").attr("disabled",'disabled') ;

	

	//TimeDo(2,'submit1');

	//if (!formSubDelay(30,'askquestime')) return false;

	obj.submit();

	$.cookie('set_ask_cookie','sn_qid_20110707');

}

function TimeDo(sec,ele){

	var wait = sec * 1000;

	document.getElementById(ele).disabled = true;

	window.setTimeout("document.getElementById('"+ele+"').disabled = false;", wait);

}



function formSubDelay(sec,ele){

	Today = new Date();

  	var NowHour = Today.getHours();

  	var NowMinute = Today.getMinutes();

  	var NowSecond = Today.getSeconds();

  	var mysec = (NowHour*3600)+(NowMinute*60)+NowSecond;

  	if ((mysec-document.getElementById(ele).value)>sec){

  		document.getElementById(ele).value = mysec;

  		return true;

  	}else{

  		alert('请不要在短时间内提交问题！'); 

  		return false;

  	}

}

function check_myscore()

{

	var givescore=Number(document.askform.givescore.value);

	var myscore=Number('20');

	if(givescore > myscore)

	{

		alert('抱歉，悬赏不能高于您目前的总积分！');

		document.askform.givescore.options[0].selected = true;

	}

	else

	{

		document.getElementById("scoretip").innerHTML = "";

	}

}

function checkQues(){

	if(document.getElementById('word').value=='请输入您的问题，律师将为你一一解答')

	document.getElementById('word').value="";

	document.wordform.action="ask.php";

	document.wordform.submit();

}

function clean(){

	var isinput = '';

	if(isinput==''){

		isinput ='Y';

		document.getElementById('word').value='';

	}

}

function hidDiv(divId){

	document.getElementById(divId).style.display="none";

}



function showDiv(divId){

	document.getElementById(divId).style.display="block";

}





