<script>
	var authPopup = null;
	var regPopup = null;
	var fogPopup=null;
	function onRegisterClick()
	{
		if (authPopup)
			authPopup.close();
		openRegisterPopup();

		return false;
	}

//îòêðûâàåò  âîññòàíîâëåíèå ïàðîëÿ
function onfogClick()
	{
		if (authPopup)
			authPopup.close();
		openfogPopup();

		return false;
	}



	function openAuthorizePopup()
	{
		if (authPopup == null)
			authPopup = BX.PopupWindowManager.create("AuthorizePopup", null,
			{
				autoHide: true,
				//	zIndex: 0,
				offsetLeft: 0,
				offsetTop: 0,
				overlay : true,
				draggable: {restrict:true},
				closeByEsc: true,
				closeIcon: { right : "15px", top : "10px", width: "16px", height: "16px" },
				content: '<div style="width:400px;height:400px; text-align: center;"><span style="position:absolute;left:50%; top:50%"><img src="/bitrix/templates/latanya/components/bitrix/system.auth.form/latanya_fieldreg/images/wait.gif"/></span></div>',
				events:
				{
					onAfterPopupShow: function()
					{
						BX.ajax.post
						(
							'/bitrix/templates/latanya/components/bitrix/system.auth.form/latanya_fieldreg/ajax.php',
							{
								type: 'auth',
								backurl: '/',
								forgotPassUrl: '/auth?forgot_password=yes'
							},
							BX.delegate(function(result)
								{
									this.setContent(result);
								},
								this
							)
						);
					}
				}
			});

		authPopup.show();
	}

	function openRegisterPopup()
	{
		if (regPopup == null)
			regPopup = BX.PopupWindowManager.create("RegisterPopup", null,
			{
				autoHide: true,
				//	zIndex: 0,
				offsetLeft: 0,
				offsetTop: 0,
				overlay : true,
				draggable: {restrict:true},
				closeByEsc: true,
				closeIcon: { right : "15px", top : "10px", width: "16px", height: "16px" },
				content: '<div style="width:400px;height:400px; text-align: center;"><span style="position:absolute;left:50%; top:50%"><img src="/bitrix/templates/latanya/components/bitrix/system.auth.form/latanya_fieldreg/images/wait.gif"/></span></div>',
				events:
				{
					onAfterPopupShow: function()
					{
						BX.ajax.post
						(
							'/bitrix/templates/latanya/components/bitrix/system.auth.form/latanya_fieldreg/ajax.php',
							{
								type: 'reg',
								backurl: '/',
								forgotPassUrl: '/auth?forgot_password=yes'
							},
							BX.delegate
							(
								function(result)
								{
									this.setContent(result);
								},
								this
							)
						);
					}
				}
			});

		regPopup.show();
	}
	//äëÿ îêíà âîññòàíîâëåíèÿ ïàðîëÿ
	function openfogPopup()
	{
		if (fogPopup == null)
			fogPopup = BX.PopupWindowManager.create("fogPopup", null,
			{
				autoHide: true,
				//	zIndex: 0,
				offsetLeft: 0,
				offsetTop: 0,
				overlay : true,
				draggable: {restrict:true},
				closeByEsc: true,
				closeIcon: { right : "15px", top : "10px", width: "16px", height: "16px" },
				content: '<div style="width:400px;height:400px; text-align: center;"><span style="position:absolute;left:50%; top:50%"><img src="/bitrix/templates/latanya/components/bitrix/system.auth.form/latanya_fieldreg/images/wait.gif"/></span></div>',
				events:
				{
					onAfterPopupShow: function()
					{
						BX.ajax.post
						(
							'/bitrix/templates/latanya/components/bitrix/system.auth.form/latanya_fieldreg/ajax.php',
							{
								type: 'fog',
								backurl: '/',
								forgotPassUrl: '/auth?forgot_password=yes'
							},
							BX.delegate
							(
								function(result)
								{
									this.setContent(result);
								},
								this
							)
						);
					}
				}
			});

		fogPopup.show();
	}

