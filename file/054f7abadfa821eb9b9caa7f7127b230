<script type="text/javascript">
//êèñëîòíûé òåêñò
$(document).ready(function (){
	$('.container').mouseover(function (){
		var o=$(this).find('.text-acid');
		if (o.hasClass('on')) return;
		o.slideDown();
		o.addClass('on');
	});
	$('.text-acid > button').click(function (){
		var o=$(this.parentNode);
		o.slideUp();
		setTimeout(function (){
			o.removeClass('on');
		},300);
	});
});
</script>
	

<script type="text/javascript">
 
$(function() {
 
$(window).scroll(function() {
 
if($(this).scrollTop() != 0) {
 
$('#toTop').fadeIn();
 
} else {
 
$('#toTop').fadeOut();
 
}
 
});
 
$('#toTop').click(function() {
 
$('body,html').animate({scrollTop:0},800);
 
});
 
});
 
</script>


