<script type="text/javascript">
    $(function(){
        $(".solution_scroll_outer a.left_arrow").click();
    });
    var timer2 = '';
    var delay2 = 5000;//延时
    function play3(type){
        clearTimeout(timer2);
        type = (type == "pre" || type == "next")? type : "pre";
        iscroll(type,'#solution_scroll','');
        if(type == "pre"){
            timer2 = setTimeout(function(){ $(".solution_scroll_outer a.left_arrow").click();},delay2);
        }else{
            timer2 = setTimeout(function(){ $(".solution_scroll_outer a.right_arrow").click();},delay2);
        }
    }

  
</script>
