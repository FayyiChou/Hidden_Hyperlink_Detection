<script>
    $('input[type="checkbox"]').click(function(){
        if ($(this).parent(".mei4").attr("id")=="zhong2") {
            $(this).parent(".mei4").removeAttr("id");
        }else{
            $(this).parent('.mei4').attr('id','zhong2');
        }
    })
    $("input[type=checkbox]").click(function(){
        var value=$(this).val();
        if ($(this).is(":checked")) {
            $("input[title="+value+"]").parents(".massegexx").show();
            $('#wqError').hide();
        }else{
            $("input[title="+value+"]").parents(".massegexx").hide();
            $('#wqError').hide();
        }
    })
    // 信息填写提交
    //姓名验证
    var p=false,q=false,w=false;
    function nameValid() {
        var name= $("#name").val()
        if (name == "") {
            layer.msg('请填写真实姓名');
            return false;
        } else {
            return true;
        }
    }
    // 手机号验证
    function mobileValid() {
        var mobile= $("#mobile").val();
        var nuberal=/^[0-9]*$/;
        var qq = $('#QQ').val();
        var wx = $('#WX').val();
        if(mobile=='')
        {
            if (qq==""&&wx=="") {
                layer.msg('请填写联系信息');
                return false;
            }else{
                return false;
            }
        }
        else if(!nuberal.test(mobile))
        {
            layer.msg('请输入正确手机号');
            return false;
        }
        else{
            return true;
        }
    }
    //邮箱验证
    function qqValid(){
        var emailword=/^(\w)+(\.\w+)*@(\w)+((\.\w{2,3}){1,3})$/;
        var qq = $('#QQ').val();
        var wx = $('#WX').val();
        var phone = $('#mobile').val();
        if(qq=='')
        {
            if (phone==""&&wx=="") {
                layer.msg('请填写联系信息');
                return false;
            }else{
                return false;
            }
        }
        else if(!emailword.test(qq))
        {
            layer.msg('邮箱格式不对');
            return false;
        }
        else{
            return true;
        }
    }
    //微信
    function wxValid(){
        var wsword=/^[a-zA-Z\d_]{5,}$/;
        var qq = $('#QQ').val();
        var wx = $('#WX').val();
        var phone = $('#mobile').val();
        if(wx=='')
        {
            if (phone==""&&qq=="") {
                layer.msg('请填写联系信息');
                return false;
            }else{
                w=true;
                return false;
            }
        }
        else if(!wsword.test(wx))
        {
            layer.msg('微信号格式不对');
            return false;
        }
        else{
            w=true;
            return true;
        }
    }
    function wq(){
        var length = $('input[type=checkbox]:checked').length;
        if (length<=0) {
            layer.msg('请至少选择一种联系方式');
            return false;
        } else{
            return true;
        }
    }
    //点登录校验整个表单
    function validxx(){
        if(nameValid()&&wq()){
            if (mobileValid()||qqValid()||wxValid()) {
                return true;
            }
            return false;
        } else{
            return false;
        }
    }
    //热门学科--点击跳转
    $(".schoolMajor tr td").click(function(){
        $("input[name='field']").val($(this).attr("code"));
        $("#field").submit();
    })
</script>
</body>
</html>