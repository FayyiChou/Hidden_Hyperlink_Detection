    $(".Hnav2 a").click(function(){
        var ind=$(this).index();
        var go=$(this).attr("go");
        $(".go-c").attr("href",go);
        var textarea=$(".sl").eq(ind).find("textarea").text();
        if ($(this).attr("class")=="go-b") {
            return;
        }else if (textarea!=="") {
            $(".sl").eq(ind).html(textarea);
            jQuery(".team").slide({ titCell:".hd ul", mainCell:"#owl-demo", effect:"left", vis:"4", autoPlay:true,scroll:"4", autoPage:true, trigger:"click",interTime:"5000"});
            $(this).attr("id","haiOrange").siblings("a").attr("id","");
            $(".sl").eq(ind).slideDown().siblings(".sl").slideUp().find("span").removeClass("counter");
            $(".sl").eq(ind).find("span").addClass("counter");
            $('.counter').counterUp({
                delay: 10,
                time: 800,
                startVal:0,
            });
            $('.counter').unbind('counterUp')
        }
        else{
            $(this).attr("id","haiOrange").siblings("a").attr("id","");
            $(".sl").eq(ind).slideDown().siblings(".sl").slideUp().find("span").removeClass("counter");
            $(".sl").eq(ind).find("span").addClass("counter");
            $('.counter').counterUp({
                delay: 10,
                time: 800,
                startVal:0,
            });
            $('.counter').unbind('counterUp')
        }
    })
