<script type="text/javascript">
    $('.strategyM section .p2').dotdotdot();
    //计划出国时间
    function timeValid(){
        var time = $('#time').text();
        if(time=='计划出国时间')
        {
            $('#timeError').show();
            $('#timeErrorText').text('请选择计划出国时间');
            $('#time').parent("span").css("border-color","#ed503d");
            return false;
        }
        else{
            $('#timeError').hide();
            $('#time').parent("span").css("border-color","#e0e0e0");
            return true;
        }
    }
    //在读或最高学历
    function educationValid(){
        var education = $('#education').text();
        if(education=='在读或最高学历')
        {
            $('#educationError').show();
            $('#educationErrorText').text('请选择学历');
            $('#education').parent("span").css("border-color","#ed503d");
            return false;
        }
        else{
            $('#educationError').hide();
            $('#education').parent("span").css("border-color","#e0e0e0");
            return true;
        }
    }
    //就读专业
    function majorValid(){
        var major = $('#major').text();
        if(major=='就读专业')
        {
            $('#majorError').show();
            $('#majorErrorText').text('请选择就读专业');
            $('#major').parent("span").css("border-color","#ed503d");
            return false;
        }
        else{
            $('#majorError').hide();
            $('#major').parent("span").css("border-color","#e0e0e0");
            return true;
        }
    }
    //平时成绩
    $("#grade").on("keyup",function(){
        var numeral=/^\+?[1-9][0-9]*$/;
        var num=$(this).val();
        if (!numeral.test(num)) {
            $(this).val("");
        }else if (num>100) {
            $(this).val("");
        }
        $(this).siblings(".hint").find(".Error").hide();
        $('#grade').parent("div").css("border-color","#e0e0e0");
    })
    function gradeValid(){
        var grade = $('#grade').val();
        if(grade=='')
        {
            $('#gradeError').show();
            $('#gradeErrorText').text('请输入平时成绩');
            $('#grade').parent("div").css("border-color","#ed503d");
            return false;
        }
        else{
            $('#gradeError').hide();
            $('#grade').parent("div").css("border-color","#e0e0e0");
            return true;
        }
    }
    function valid(){
        if(timeValid()&&educationValid()&&majorValid()&&gradeValid()){
            $(".hidden1").val($("#time").text());
            $(".hidden2").val($("#education").text());
            $(".hidden3").val($("#major").text());
            $(".hidden4").val($("#grade").val());
            $(".fastApply").fadeIn();
            $(".Black").fadeIn();
            return false;
        }else{
            return false;
        }
    }
    $(".Black,.fastApply em").click(function(){
        $(".fastApply").fadeOut();
        $(".Black").fadeOut();
    })
    jQuery(".ad1").slide({ titCell:".hd ul", mainCell:".pj-Carousel-box", effect:"leftLoop",vis:"1", autoPlay:true, autoPage:true, trigger:"click",interTime:"5000",startFun:function( i,c,s ){
        var textarea=s.find("a").eq(i+1).find("textarea").text();
        var len=s.find("a").length;
        var textarea0=s.find("a").eq(len-1).find("textarea").text();
        if (textarea0!=="") {
            s.find("a").eq(len-1).html(textarea0);
            s.find("a").eq(0).html(s.find("a").eq(0).find("textarea").text());
        }
        if (textarea!=="") {
            s.find("a").eq(i+1).html(textarea);
            return;
        }else{
            return
        }
    }
    });
    //大图切换
    jQuery(".schoolS .smallScroll").slide({mainCell:".ulxx",delayTime:800,vis:4,effect:"left",autoPlay:true,pnLoop:true,autoPage:true,prevCell:".prev2",nextCell:".next2"});
    //小图左滚动切换
    jQuery(".schoolS").slide({ titCell:".smallImg li", mainCell:".bigImg", effect:"fold", autoPlay:true,pnLoop:true,delayTime:200});
    jQuery(".fullSlide").slide({ titCell:".hd ul", mainCell:".bd ul", effect:"leftLoop", vis:"1", autoPlay:true, autoPage:true, trigger:"click",interTime:"5000",startFun:function( i,c,s ){
        var textarea=s.find("span").eq(i+1).find("textarea").text();
        var len=s.find("span").length;
        var textarea0=s.find("span").eq(len-1).find("textarea").text();
        if (textarea0!=="") {
            s.find("span").eq(len-1).html(textarea0);
            s.find("span").eq(0).html(s.find("span").eq(0).find("textarea").text());
        }
        if (textarea!=="") {
            s.find("span").eq(i+1).html(textarea);
            return;
        }else{
            return;
        }
    }
    });
    //下拉框
    $(".formSelect").click(function () {
        var display = $(this).find('ul').css("display");
        if (display == "none") {
            $(this).find('ul').css("display", "block");
            $(this).css("border-color","#43a0ff");
            return false;
        } else {
            $(this).find('ul').css("display", "none");
            $(this).css("border-color","#e0e0e0");
            return false;
        }
    })
    $(document).on("mouseenter mouseleave", ".formSelect ul", function (even) {
        if (even.type == "mouseenter") {
            $(this).show();
            $(this).parent(".formSelect").css("border-color","#43a0ff");
        } else if (even.type == "mouseleave") {
            $(this).hide();
            $(this).parent(".formSelect").css("border-color","#e0e0e0");
        }

    })
    $(".formSelect ul li").click(function () {
        $('.formSelect ul li').removeClass("f1-color ie-color");
        $(this).addClass("f1-color ie-color");
        var text = $(this).text();
        $(this).parent('ul').css("display", "none");
        $(this).parents('.formSelect').css("border-color","#e0e0e0");
        $(this).parent().siblings('p').text(text);
        $(this).parents('.formSelect').siblings('input').val(text);
        $(this).parents('.formSelect').siblings('.hint').find(".Error").hide();
        return false;
    })
    window.onscroll = function () {
        $(".formSelect ul").css("display", "none");
    };
</script>
