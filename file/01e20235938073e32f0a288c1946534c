    <script type="text/javascript">
        var pindex = 1; //表示从第2页开始异步加载
        groupid = request("groupid");
        parentid = request("parentid");
        catalogid = request("catalogid");
        var kwd = request("kwd");
        var jLoading = $('#loading');
        var htmlstring = "";
        var $container = $('#container'), sTimer;
        $(function () {
            $container.imagesLoaded(function () {
                $container.masonry({
                    itemSelector: '.pic',
                    isFitWidth: true, // 适应宽度   Boolean
                    isResizableL: true, // 是否可调整大小 Boolean
                    isAnimated: false,
                    isRTL: false, //false为从左边到右边
                    gutterWidth: 0, //列的间隙
                    animationOptions: {
                        duration: 750,
                        easing: 'swing',
                        queue: false
                    },
                    gutterWidth: 14
                });
            });
            loadMore();
            $(window).scroll(function scrollHandler() {
                clearTimeout(sTimer);
                sTimer = setTimeout(function () {
                    if (window.loaded == 1) { $(window).unbind("scroll", scrollHandler); }
                    var c = document.documentElement.clientHeight || document.body.clientHeight, t = $(document).scrollTop();
                    if (t + c >= $container.offset().top - 100 + $container.height()) { ReadyLoad(htmlstring); }
                }, 100);
            });
        });
        function ReadyLoad(htmls) {
            $container.append(htmls).masonry("reload");
            jLoading.hide();
            loadMore();
        }

        function loadMore() {
            var html = "";
            pindex++;
            if (kwd == undefined || kwd.length <= 0) {
                kwd = $("#hiddensearchtag").val();
            }
            $.ajax({
                type: "POST",
                url: "/ashx/SimilarSearch.ashx",
                data: "&pageindex=" + pindex + "&pagesize=25&kwd=" + kwd,
                success: function (data) {
                    var table = eval("(" + data + ")");
                    for (var i = 0; i < table.TableInfo.length; i++) {
                        switch (table.TableInfo[i].Server) {
                            case "tk":
                                html += TkPicture(table.TableInfo[i]);
                                break;
                            case "mn":
                                html += MNPicture(table.TableInfo[i])
                                break;
                            default:
                                html += QJPicture(table.TableInfo[i]);
                                break;
                        }
                    }
                    if (html.length <= 0) {
                        htmlstring = "";
                    }
                    else {
                        htmlstring = html;
                    }
                    html = "";
                }
            });
        }
        function request(paras) {
            var url = location.href;
            var paraString = url.substring(url.indexOf("?") + 1, url.length).split("&");
            var paraObj = {}
            for (i = 0; j = paraString[i]; i++) {
                paraObj[j.substring(0, j.indexOf("=")).toLowerCase()] = j.substring(j.indexOf("=") + 1, j.length);
            }
            var returnValue = paraObj[paras.toLowerCase()];
            if (typeof (returnValue) == "undefined") {
                return "";
            } else {
                return returnValue;
            }
        }

    </script>
