    <script type="text/javascript">
        var imgchange = "";
        $(function () {
            imgchange = setInterval(reloadimg, 500);
        })
        function reloadimg() {
            if ($(".pic").css("position") == "absolute") {
                clearInterval(imgchange);
                $("#container img").each(function () {
                    if ($(this).attr("catalogid") != undefined)
                        $(this).attr("src", "http://pic250.quanjing.com/" + $(this).attr("catalogid") + "/" + $(this).attr("picid") + ".jpg");
                })
            }
        }
    </script>
