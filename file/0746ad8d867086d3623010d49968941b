        function doquery() {
            if (document.form1.idno.value.length == 0) {
                alert("请输入注册证件号");
                return;
            } else if (document.form1.phone.value.length == 0
                    && document.form1.email.value.length == 0) {
                alert("请输入注册时输入的手机号码或者邮件地址");
                return;
            }
            document.form1.submit();
        }
    </script>
