<script src="http://www.c-zgpr.com/static/20150505/js/index.js"></script>
<script>
    function _switch(hselector, bselector, selClass){
        var $h = $(hselector),
            $b = $(bselector);
        $h.on('click', function(event){
            var index = $(this).index();
            $h.removeClass(selClass).eq(index).addClass(selClass);
            $b.addClass('hidden').eq(index).removeClass('hidden');
            event.preventDefault();
        });
    }
    _switch('#zgcp .c_sortitems a', '#zgcp .c_con', 'c_sitem_cur');
    _switch('#slsc .c_sortitems a', '#slsc .i_con2', 'c_sitem_cur');
</script>



</body>
</html>