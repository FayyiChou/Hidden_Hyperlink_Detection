	scroll(".pufa dd",".pufa .prev",".pufa .next",".pufa",5,true);
	scroll(".ceb dd",".ceb .prev",".ceb .next",".ceb",5,true);
	scroll(".psbc dd",".psbc .prev",".psbc .next",".psbc",5,true);
	//bank
	$(".bank-name li").each(function(index) {
		$(this).mouseenter(function() {
			$(".bank-name li").removeClass("curr");
			$(this).addClass("curr");
			$(".bank-con:eq(" + index + ")").show().siblings().hide();
		});
	});
	//tab
	 $("div.tabIndex a").each(function(index) {
		$(this).mouseenter(function() {
			$(this).siblings().removeClass("curr");
			$(this).addClass("curr");
			$("div.tabCon:eq(" + index + ")").show().siblings().hide();
		});

	});
})
