                     btn_borderHoverColor:"#c60",btn_borderWidth:0}
	})
	
	//sroll
	scroll(".bank-nav",".bank-nav .prev",".bank-nav .next",".bank-name",3,false);
	$(".bank-name li").each(function(index) {
		$(this).mouseenter(function() {
			$(".bank-name li").removeClass("curr");
			$(this).addClass("curr");
			//$("div.recomCon li:eq(" + index + ")").show().siblings().hide();
		});
	});
	
	scroll(".boc dd",".boc .prev",".boc .next",".boc",5,true);
	scroll(".icbc dd",".icbc .prev",".icbc .next",".icbc",5,true);
	scroll(".ccb dd",".ccb .prev",".ccb .next",".ccb",5,true);
	scroll(".abc dd",".abc .prev",".abc .next",".abc",5,true);
	scroll(".comm dd",".comm .prev",".comm .next",".comm",5,true);
	scroll(".cmb dd",".cmb .prev",".cmb .next",".cmb",5,true);
