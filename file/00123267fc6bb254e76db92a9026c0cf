		<script language="JavaScript1.2">
			var speed_h=30//速度,垂直滚动,速度越大滚动越慢
                        demo4.innerHTML=demo3.innerHTML
			function Marquee_h(){
			if(demo4.offsetTop-demo_h.scrollTop<=0)
				demo_h.scrollTop-=demo3.offsetHeight
			else{
				demo_h.scrollTop++
				}
			}
			var MyMar_h=setInterval(Marquee_h,speed_h)
			demo_h.onmouseover=function() {clearInterval(MyMar_h)}
			demo_h.onmouseout=function() {MyMar_h=setInterval(Marquee_h,speed_h)}
		</script>


		
