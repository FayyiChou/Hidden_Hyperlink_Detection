    <script type="text/javascript">


        function Submit_Points_pl() {
            var Pointsresult = $("#Pointsresult").text();
            var demandID = $("#hid_demandID").val();
            if ($.trim(Pointsresult) == "请打分") {
                alert('请打分！');
                return false;
            }
            $.ajax({
                type: "post",
                url: "ajax/submitZWpoints.aspx",
                dataType: "text",
                data: { id: demandID, remark: escape(Pointsresult) },
                success: function(result) {
                    if (result == "success") {
                        alert('提交成功，请等待审核!');
                        $(".pf_tab").hide();
                        $(".pf_bg").hide();
                    }
                    else {
                        if (result == "notlogin") {
                            alert('请先登录!');
                            location.href = "http://passport.shangxueba.com/user/userlogin.aspx?url=http://www.shangxueba.com/zuowen/" + demandID + ".html";
                            return false;
                        }
                        if (result == "null") {
                            alert('请打分!');
                            return false;
                        }
                        else {
                            alert('提交失败,请核实后重新提交!');
                            return false;
                        }
                    }
                },
                error: function() { },
                cache: false
            });
        }

        function Submit_Lession_pl(vtype) {
            var remark = $("#txt_plRemark").val();
            if (vtype == 1) {
                remark = $("#txt_plRemark2").val();
            }
            var demandID = $("#hid_demandID").val();
            if ($.trim(remark) == "") {
                alert('请输入评论内容！');
                return false;
            }
            $.ajax({
                type: "post",
                url: "ajax/submitZWpl.aspx",
                dataType: "text",
                data: { id: demandID, type: vtype, remark: escape(remark) },
                success: function(result) {
                    if (result == "success") {
                        if (vtype == 0) {
                            alert('提交评论成功，请等待审核!');
                            $("#txt_plRemark").val("");
                        }
                        else {
                            alert('提交点评成功，请等待审核!');
                            $("#txt_plRemark2").val("");
                            $(".tc_bg").hide();
                            $(".dp_tc").hide();
                        }
                    }
                    else {
                        if (result == "notlogin") {
                            alert('请先登录!');
                            location.href = "http://passport.shangxueba.com/user/userlogin.aspx?url=http://www.shangxueba.com/zuowen/" + demandID + ".html";
                            return false;
                        }
                        if (result == "null") {
                            alert('请输入评论内容!');
                            return false;
                        }
                        else {
                            alert('提交失败,请核实后重新提交!');
                            return false;
                        }
                    }
                },
                error: function() { },
                cache: false
            });
        }

        $(function() {
            //加载更多评论信息
            $("#see_more").bind("click", function() {
                $.ajax({
                    type: "post",
                    url: "dplLoading.aspx",
                    dataType: "text",
                    data: { id: $("#hid_demandID").val(), page: parseInt($("#hide_plpage").val()) + 1 },
                    success: function(result) {
                        if (result == "end") {
                            $("#see_more").html("已经加载到最后一条了！");
                        }
                        else {
                            $("#morepl").append(result);
                            $("#hide_plpage").val(parseInt($("#hide_plpage").val()) + 1);
                        }
                    },
                    error: function() { },
                    cache: false
                });
            });

            $.ajax({
                type: "post",
                url: "http://www.shangxueba.com/zuowen/ajax/submitZWreadcount.aspx",
                dataType: "text",
                data: { id: $("#hid_demandID").val() },
                success: function(result) {
                    if (result == "success") {
                    }
                    else {
                        //alert("添加作文浏览数失败!");
                    }
                },
                error: function() { },
                cache: false
            });
        });
    </script>

