                weixinBox.css("position", "fixed");
                weixinBox.css("top", "41px");
            } else if (wst > bottom) {
                weixinBox.css("position", "fixed");
                weixinBox.css("top", "-" + (wst - bottom) + "px");
            }
        }
    });
