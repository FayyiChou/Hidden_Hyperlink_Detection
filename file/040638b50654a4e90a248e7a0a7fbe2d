    <script type="text/javascript">
        var leh1 = $(".main_l").offset().top;
        var leh2 = leh1 + $(".main_l").height();
        var leh3 = leh2 - $("#xh250").height() - 348;
        var rih1 = $("#fanwen_zuowen").offset().top;
        var rih2 = rih1 + $("#fanwen_zuowen").height() - 270;
        $(document).ready(function() {
            $(window).scroll(loadData);
        });
        function loadData() {
            var now = $(window).scrollTop();
            if (now >= rih2 && now <= leh3) {
                $("#xh250").addClass("main_r_fix");
                $("#xh250").css("position", "fixed");
                $("#xh250").css("top", "20px");
                $("#xh250").css("bottom", "auto");
            }
            else if (now >= leh3) {
                $("#xh250").css("position", "absolute");
                $("#xh250").css("top", leh3 - 402 + "px");
                $("#xh250").css("bottom", "auto");
            }
            else {
                $("#xh250").removeClass("main_r_fix");
                $("#xh250").css("position", "static");
                $("#xh250").css("top", "0px");
                $("#xh250").css("bottom", "auto");
            }
        }
    </script>

