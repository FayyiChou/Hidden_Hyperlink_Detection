<SCRIPT type=text/javascript>

$(function(){
	var size=$(".gonggao div").length;
	//自动轮播
	var i=0;
         $(".gonggao div").eq(0).css("display","block");
	var t=setInterval(move,3000);
	function move(){
		i++;
		if(i==size){
			i=0;
		}
				
		
		/* $(".num li").eq(i).addClass("active").siblings().removeClass("active");
		$(".img li").eq(i).fadeIn(300).siblings().fadeOut(300); */
		$(".gonggao div").eq(i).fadeIn(500).siblings().fadeOut(500);
	}
	
	//定时器的开始于结束
	$(".gonggao div").hover(function(){
		clearInterval(t)
	},function(){
		t=setInterval(move,3000);
	})
	
	
	
})
	
</SCRIPT>
