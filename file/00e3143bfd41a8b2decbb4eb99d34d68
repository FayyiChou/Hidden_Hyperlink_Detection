function check(){
	var keywords=$("#keywords").val().trim();
	if($("#keywords").val().trim()==""){
	alert("请输入关键字");
	return false;
	}
         if(/[<>"';()+@#\$%\^&\*]+/g.test(keywords)){
		
		alert("不要带有非法字符");
		return false
	}
document.getElementById('subform').submit();
}
