      <script type="text/javascript">
            $(document).ready(function(){
                  $('#tab-container ul.tabs li a').click(function(e) { e.preventDefault(); });
        
                $(".tab-contents").hide(); 
                $(".tab-contents:first").show();		
                $("#tab-container ul.tabs li a").click(function(){
                    
                    var activeTab = $(this).attr("href");			
                    $("#tab-container ul.tabs li a").removeClass("active");
                    $(this).addClass("active");
                    $(".tab-contents").hide(); 
                    $(activeTab).fadeIn(); 
                });
            });
        </script>

