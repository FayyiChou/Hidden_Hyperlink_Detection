		function show(tips,flag,url){ // 这个控制宁海新闻网微信图片效果
		var my_tips=document.getElementById("mytips");
		if(flag){
		my_tips.style.display="";
		if (url!=null){my_tips.innerHTML="<img src='"+url+"' align=left width=87>";}
		else{ my_tips.innerHTML=tips;}
		my_tips.style.left=event.clientX-20;
		my_tips.style.top=event.clientY+10;
		}else{
		my_tips.style.display="none";
		}
		}
