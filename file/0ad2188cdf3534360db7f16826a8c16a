        var index = $(this).index();
        var divs = $("#hotday-con > ul");
        $(this).parent().children("li").attr("class", "hand");//将所有选项置为未选中
        $(this).attr("class", "now hand"); //设置当前选中项为选中样式
        divs.hide();//隐藏所有选中项内容
        divs.eq(index).show(); //显示选中项对应内容
    }); 
    
    //视频信息内容显示
    $("#infomore").bind("click", function () {
        var height = $("#infobox").css("height");
        if (height == "111px") {
            $("#infobox").css("height","auto");
            $(this).css("background","url(http://www.hoolo.tv/t/2/4/images/infobottom2-1031.png) no-repeat 0 0");
        }else{
            $("#infobox").css("height","111px");
            $(this).css("background","url(http://www.hoolo.tv/t/2/4/images/infobottom-1031.png) no-repeat 0 0");
        }
    });

    //二维码内容显示
    $("#qr-btn").bind("click", function () {
        $("#qrbox").toggle();//隐藏显示二维码内容区域
        $("#qr-sj").toggleClass("box2");//切换小三角
    });
});
</script> 
  <script type="text/javascript">var video_id = $('#m2o_vodplayer').html();
var parameters = {
