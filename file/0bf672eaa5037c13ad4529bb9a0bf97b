//评分
function pf(id,name){
if(id && name){
cid = id;
}else{
cid = 1;
}

var saveid = GetCookie('pf');
if(saveid != null)
{
var saveids = saveid.split(',');
var hasid = false;
saveid = '';
j = 1;
for(i=saveids.length-1;i>=0;i--)
{
if(saveids[i]==cid && hasid) continue;
else {
if(saveids[i]==cid && !hasid) hasid = true;
saveid += (saveid=='' ? saveids[i] : ','+saveids[i]);
j++;
if(j==20 && hasid) break;
if(j==19 && !hasid) break;
}
}
if(hasid) {alert("您已经顶过该帖，请不要重复顶帖 ！"); return;}
else saveid += ','+cid;
SetCookie('pf',saveid,1);
}
else
{
SetCookie('pf',cid,1);
}

if(id && name){
new ajax().start('GET','/plus/vote_new.php?i='+id+'&vid=208336',true,null,null,null);
document.getElementById(name).innerHTML = parseInt(document.getElementById(name).innerHTML)+1;
}else{
option = parseInt(document.getElementById("option").innerHTML);
new ajax().start('GET','/plus/vote_new.php?i='+option+'&vid=208336',true,vote,null,null);
}
}
//获得评分
function vote(_xmlHttp){
arr = _xmlHttp.responseText.split('|');
document.getElementById('i6').innerHTML = arr[2];
document.getElementById('i7').innerHTML = arr[3];
document.getElementById('i8').innerHTML = arr[0];
if(arr[0] <=2){
document.getElementById('i5').innerHTML ='烂作';
}
else if(arr[0] <=4){
document.getElementById('i5').innerHTML ='一般';
}
else if(arr[0] <=6){
document.getElementById('i5').innerHTML ='尚佳';
}
else if(arr[0] <=8){
document.getElementById('i5').innerHTML ='大作';
}
else if(arr[0] <=10){
document.getElementById('i5').innerHTML ='神作';
}
document.getElementById('i9').innerHTML = arr[1];
//alert('总分:'+arr[0]+'评分数:'+arr[1]+'玩过:'+arr[2]+'想玩:'+arr[3]);
}
new ajax().start('GET','/plus/vote_new.php?vid=208336',true,vote,null,null);
