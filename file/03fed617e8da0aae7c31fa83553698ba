function checklength(theform,postmaxchars){
if(postmaxchars != 0){
message = '<br />系统限制最大字节数:'+postmaxchars+' 字节';
} else{
message = '';
}
showDlg({type:"warning",message:'您的信息已有字节数: <font color="red">'+strlen(theform.atc_content.value)+'</font> 字节'+message});
}
