    $('.lh_tit li').eq(0).addClass("cur");
    $("div.lh_cc").eq(0).show();
    $('.lh_tit li').hover(function () {
        var index = $(".lh_tit li").index(this);
        $(this).addClass("cur").siblings("li").removeClass("cur");
        $("#shkxs_news" + index).show().siblings("div.lh_cc").hide();
    });
</script>

         
            

