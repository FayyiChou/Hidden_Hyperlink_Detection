    var divlen=$(".vidnew-con>div").length-3;
    if(divlen<=0){
        divlen=1;
    }
    $(".public_pages").text(divlen);
    $("body").on("click", ".vid-prev", function () {
        var public_pageNo = $.trim($(".public_pageNo").text()) * 1;
        if (public_pageNo == 1) {
            return;
        } else {
            $(".public_pageNo").text(public_pageNo - 1);
            $(".vidnew-con").stop();
            var t = -1 * (public_pageNo - 2) * 100;
            $(".vidnew-con").stop();
            $(".vidnew-con").animate({top: t + "px"}, 500);
        }
    });

    $('.vidnew-con .vidnew-list').hover(function(){
        $(this).find('.vidnew-hf').css('background','#494a4c');
        $(this).find('.sku-play').css('background','#494a4c');
    },function(){
        $(this).find('.vidnew-hf').css('background','#3f4042');
        $(this).find('.sku-play').css('background','#3f4042');
    });
    $("body").on("click",".vid-next",function(){

        var public_pageNo = $.trim($(".public_pageNo").text()) * 1;
        var public_pages = $.trim($(".public_pages").text()) * 1;
        if (public_pageNo == public_pages) {
            return;
        } else {
            $(".public_pageNo").text(public_pageNo + 1);
            var t = -1 * public_pageNo * 100;
            $(".vidnew-con").stop();
            $(".vidnew-con").animate({top: t + "px"}, 500);
        }
    });
    var pIndex = listNum+1;
    if(pIndex>2 && divlen>1){
        //需要滑动的页数
        var slidePage=(pIndex-2)>(divlen-1)?(divlen-1):(pIndex-2);
        $(".public_pageNo").text(slidePage+1);
        var t = -1 * slidePage * 100;
        $(".vidnew-con").stop();
        $(".vidnew-con").animate({top: t + "px"}, 500);
    }
    var i = 0;
    setInterval(function () {
        $(".sku-one").animate({
            height: Math.round(Math.random() * 18 + 1) + "px"
        }, 350)
        $(".sku-two").animate({
            height: Math.round(Math.random() * 18 + 1) + "px"
        }, 350)
        $(".sku-three").animate({
            height: Math.round(Math.random() * 18 + 1) + "px"
        }, 350)
    }, 350);
    /*还原代码结束*/
</script>

