<script type="text/javascript">
    function downloadJSAtOnload() {
        var element = document.createElement("script");
        element.src = "http://dl.ntalker.com/js/xn6/ntkfstat.js?siteid=kf_9751";
        document.body.appendChild(element);
    }
    if (window.addEventListener)
        window.addEventListener("load", downloadJSAtOnload, false);
    else if (window.attachEvent)
        window.attachEvent("onload", downloadJSAtOnload);
    else window.onload = downloadJSAtOnload;
    /**
     * 在线咨询窗口显示，并进行销售推广记录
     */
    function xiaonengIm(categoryId, key) {
        var sid = settingid;
        if (sid != null && sid != "" && typeof sid != 'undefined') {
            NTKF.im_openInPageChat(sid);
            // 添加小能销售推广记录
            var url = path + "/sysKeyPointLog/saveXnSale";
            var datas = {"uuid": sid, "username": NTKF.user.id, "sku": categoryId, "et": "LIVE_SALES_IM"};
            $.post(url, datas, function (data) {
            });
        } else {
            xiaoneng(categoryId, key);
        }
    }

    function xiaoneng(sku, key) {
        var path = $("#path").val();//暂时这么写着
        if (!isNaN(sku)) {
            var cookieKey = sku + "_" + key;
            if (Cookies.get(cookieKey) == null) {
                var url = path + '/sysDict/xiaoneng';
                var dataes = {sku: sku};
                $.ajax({
                    type: "post",
                    url: url,
                    data: dataes,
                    async: false,
                    dataType: "json",
                    success: function (data) {
                        var val = data[key];
                        NTKF.im_openInPageChat(val);
                        Cookies.set(cookieKey, val, {path: "/"});
                    }
                });
                $.ajax({
                    type: "post",
                    url: path + "/sysKeyPointLog/saveXn/" + sku + "/" + key,
                    data: {uuid: NTKF.user.id},
                    async: false,
                    success: function (data) {

                    }
                });
            } else {
                var val = Cookies.get(cookieKey);
                NTKF.im_openInPageChat(val);
            }
        } else {
            NTKF.im_openInPageChat(sku);
        }
    }
</script>
