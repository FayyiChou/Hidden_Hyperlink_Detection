<script>
    var curPath="";
    var curSku="605";
    var curSkuName="银行从业资格证";
    var imgDomain="http://tu.duia.com/";
    var duiaDomain="http://www.duia.com/";
    var tikuDomain="http://tiku.duia.com/";
    var duibaDomain="http://bang.duia.com/";
    var downloadDomain="http://tu.duia.com/";
    var ssoDomain="http://sso.duia.com";

    var uuid=Cookies.get("client_id");
    if(uuid==null || uuid==undefined || uuid==""){
        uuid=Math.uuid().replace("-","").replace("-","").replace("-","").replace("-","");
        Cookies.set("client_id", uuid,{path:"/"});
    }
    
    function scriptFilterHtml2Java(content){
    	if(content==null || content=="" || content==undefined){
    		return content;
    	}
    	if(content.indexOf("&") != -1){
    		content = content.replace(new RegExp('&', 'g'), "&amp;");
    	}
    	if(content.indexOf("<") != -1){
    		content = content.replace(new RegExp('<', 'g'), "&lt;");
    	}
    	if(content.indexOf(">") != -1){
    		content = content.replace(new RegExp('>', 'g'), "&gt;");
    	}
    	if(content.indexOf('"') != -1){
    		content = content.replace(new RegExp('"', 'g'), "&quot;");
    	}
    	if(content.indexOf(' ') != -1){
    		content = content.replace(new RegExp(' ', 'g'), "&nbsp;");
    	}
    	
    	return content;
    }
    
    function scriptFilterJava2Html(content){
    	if(content==null || content=="" || content==undefined){
    		return content;
    	}
    	if(content.indexOf("&amp") != -1){
    		content = content.replace(new RegExp('&amp', 'g'), "&;");
    	}
    	if(content.indexOf("&lt;") != -1){
    		content = content.replace(new RegExp('&lt;', 'g'), "<");
    	}
    	if(content.indexOf("&gt;") != -1){
    		content = content.replace(new RegExp('&gt;', 'g'), ">");
    	}
    	if(content.indexOf('&quot;') != -1){
    		content = content.replace(new RegExp('&quot;', 'g'), '"');
    	}
    	if(content.indexOf("&nbsp;") != -1){
    		content = content.replace(new RegExp("&nbsp;", 'g'), " ");
    	}
    	
    	return content;
    }
    
</script>
            

