    function indexHover() {
        /*$(".search_area").mouseenter(function(){
            $(".index_search_key").show();
        });
        $(".index_search_key").mouseleave(function(){
            $(".index_search_key").hide();
        });*/
        $(".flcz_Qu ul li").mouseenter(function () {
            $(this).find(".cate_img").css({
                "transform": "scale(1.2)",
                "transition": "transform .8s"
            });
            $(this).find(".cate_box").css({
                "transition": "opacity,background 1s",
                "opacity": ".4",
                "background": "#141311"
            });
            $(this).find("h2").css({
                "margin": "70px 0 0 0",
                "transition":"all .5s"
            });
            $(this).find("p").css({
                "font-size": "16px",
                "transform": "scale(1)",
                "transition":"all .3s",
                "line-height":"24px",
                "max-height":"45px",
                "overflow":"hidden"
            });
        }).mouseleave(function () {
            $(this).find(".cate_img").css({
                "transform": "scale(1)",
                "transition": "transform .8s"
            });
            $(this).find(".cate_box").css({
                "transition": "opacity,background 1s",
                "opacity": "1",
                "background": "transparent"
            });
            $(this).find("h2").css({
                "margin": "87px 0 0 0",
                "transition":"all .5s"
            });
            $(this).find("p").css({
                "font-size": "0",
                "transform": "scale(0)",
                "transition":"all .3s"
            });
        });
    };
