    function channelManage() {
        $(".flcz_Ul li").each(function() {
            var index = $(this).index();
           if(index > 9 && index % 4 == 1){
               $(this).css("margin-right",0);
           }
        })
    }

    var sameCityTotalLength = $(".same_city_Li").length;
    var sameCityShowLength = 20;
    function getMoreLi() {
        var size = 0;
        $(".same_city_Li").each(function(){

            if(size >= 20){
                return false;
            }
            if($(this).attr("data-show") == "0"){
                size++;
                sameCityShowLength++;
                $(this).attr("data-show", "1");
                $(this).show();
            }
        });

        if(sameCityShowLength >= sameCityTotalLength){
            $(".moreList").show();
            $(".upMore").hide();
        }

    };

    function indexDoSearch() {
        var word = $('#index_search').val();
        doSearch(word);
    };

    $('#index_search').bind("keydown", function(e){

        var curKey = e.which;

        if(curKey == 13){
            indexDoSearch();
        }

    });

    $(".same_city_Li").each(function() { //金额的字号判断
        if($(this).find('.same_city_Li_join').children("b").length==0){
            var val = $(this).find('.same_city_Li_join').text();
            if(val.charAt(val.length-1) == '起'){
                $(this).find('.same_city_Li_join').html("<span>￥</span><b>"+val.substring(0,val.length-1)+"</b><span>起</span>");
            }
            if(val.charAt(val.length-1) != '起' && val.charAt(val.length-1) != '费'){
                $(this).find('.same_city_Li_join').html("<span>￥</span><b>"+val+"</b>")
            }
        }
    });

    var focusShop = {
        id: "",
        type: "",
        post: function (id, type) {
            if (!_user._login()) {
                _user._toLogin();
                return;
            }
            focusShop.id = id;
            focusShop.type = type;
            _$(_api3._shopAttent, "focus_type=" + focusShop.type + "&focus_shop_id=" + focusShop.id, focusShop.ok);
        },
        ok: function (json, code) {
            if (code != 200) {
                _toast._show("操作失败!");
                return;
            }
            if (json.is_update == "0") {
                if (focusShop.type == '1') {
                    $("#ponsor_info_list_" + focusShop.id + " p").html('<i class="blue">√</i>&nbsp已关注').removeClass("wei_gz").addClass("yi_gz").attr("onclick","focusShop.post(\'' + focusShop.id + '\',\'2\')");
                    $(".yi_gz").mouseover(function () {
                        $(this).addClass("hover2").html("取消关注").css({"background":"#ececec","color":"#999","border-color":"#ececec"});
                    }).mouseout(function () {
                        $(this).removeClass("hover2").html('<i class="blue">√</i>&nbsp已关注').css({"background":"#fff","color":"#999","border-color":"#ececec"});
                    });
                    /*$("#ponsor_info_list_" + focusShop.id).find(".btn_gz").prop('outerHTML', '<a class="btn_gz yi_gz" href="javascript:void(0)" onclick="focusShop.post(\'' + focusShop.id + '\',\'2\')" rel="nofollow">√&nbsp;已关注</a>');
                    var num = $("#sponsor_info_list_" + focusShop.id).find(".fans_num").html().replace("人关注", "") * 1 + 1;
                    $("#sponsor_info_list_" + focusShop.id).find(".fans_num").html(num + '人关注');*/
                } else {
                    $("#ponsor_info_list_" + focusShop.id + " p").html("+&nbsp;关注").removeClass("yi_gz").addClass("wei_gz").attr("onclick","focusShop.post(\'' + focusShop.id + '\',\'1\')");
                    $(".wei_gz").mouseover(function () {
                        $(this).addClass("hover1").html("+&nbsp;关注").css({"background":"#0099e9","color":"#fff","border-color":"#0099e9"});
                    }).mouseout(function () {
                        $(this).removeClass("hover1").html("+&nbsp;关注").css({"background":"#fff","color":"#999","border-color":"#ececec"});
                    });
                    /*$("#ponsor_info_list_" + focusShop.id).find(".btn_gz").prop('outerHTML', '<a class="btn_gz" href="javascript:void(0)" onclick="focusShop.post(\'' + focusShop.id + '\',\'1\')" rel="nofollow">+&nbsp;关注</a>');
                    var num = $("#sponsor_info_list_" + focusShop.id).find(".fans_num").html().replace("人关注", "") * 1 - 1;
                    $("#sponsor_info_list_" + focusShop.id).find(".fans_num").html(num + '人关注');*/
                }
            }
        }
    };


