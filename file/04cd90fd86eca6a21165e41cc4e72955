}
/*$("#pub_search").blur(function(){
    setTimeout(function(){
        $(".search_key").hide();
    }, 200);
});
$("#pub_search").focus(function(){
  $(".search_key").show();
});*/
$(".city_nav ul li").click(function(){
    var nav_index = $(this).index();
    $(this).addClass("thisOver").siblings().removeClass("thisOver");
    $("#city_style"+nav_index).addClass("thisOver").siblings().removeClass("thisOver");
});
/*$(".choose_city_li").on("click","a",function () {
    $(this).addClass("thisOver").parent().siblings().child("a").removeClass("thisOver");
})*/
$(".header_public_search").mouseover(function(){
  $(".search_key").show();
});
$(".header_public_search").mouseout(function(){
  $(".search_key").hide();
});
$(".city_box_list div ul li").click(function () {
    $(this).addClass("thisOver").siblings().removeClass("thisOver");
})
</script>
