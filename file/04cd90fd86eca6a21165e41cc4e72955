            $(".focusMap_k").css("width","1190px");
            $(".focusMap_k li").css("width","1190px");
        }else{
            $(".focusMap").css("width",_w + "px");
            $(".focusMap_k").css("width",_w + "px");
            $(".focusMap_k li").css("width",_w + "px");
        }
        if(_w*0.2604<30){
            $(".focusMap").css("height","30px");
            $(".focusMap_k").css("height","30px");
            $(".focusMap_k li").css("height","30px");
        }else {
            $(".focusMap").css("height",_w*0.2604+"px");
            $(".focusMap_k").css("height",_w*0.2604+"px");
            $(".focusMap_k li").css("height",_w*0.2604+"px");
        }
    });

    //主办方轮播
    $(".zbf_list").Xslider({//.zbf_list是要移动对象的外框,页面上所有绑定了类.zbf_scroll的对象都会有切换效果;
        unitdisplayed:6,//可视的单位个数   必需项;
        numtoMove:6,//要移动的单位个数    必需项;
        viewedSize:1190,//可视宽度或高度     不传入则查找要移动对象外层的宽或高度;
        scrollobj:".zbf_ul",//要移动的对象    不传入则查找.zbf_scroll下的ul;
        unitlen:198,//移动的单位宽或高度      不传入则查找li的尺寸;
        scrollobjSize:$(".zbf_li").width(),//移动最长宽或高（要移动对象的宽度或高度）   不传入则由li个数乘以unitlen计算;
        dir:"H",//水平移动还是垂直移动       默认H为水平移动，传入V则表示垂直移动（注意是大写字母）;
        loop:"",//cycle循环滚动            不传入则不循环滚动;
        speed:500, //滚动速度      默认为500;
        autoscroll:""//自动移动间隔时间（毫秒）     不传入则不会自动移动;
    });

    //首图轮播
    $(".focusMap").slide({ titCell:".num ul" , mainCell:".focusMap_k" , effect:"fold", autoPlay:true, delayTime:1000 ,interTime:4500, autoPage:true });


    var attentionGoBack ={
        ok : function (json, code) {
            if (code != 200) {
                return;
            };
            for (var i = 0; i < $(".zbf_list ul li").length; i++) {
                var dataId =  $(".zbf_list ul li").eq(i).find(".txt.txt2").attr('data-id');
                $(".zbf_list ul li").eq(i).find(".txt.txt2").html(json.userInfoCountList[i][dataId]+"场活动");
                var id =  $(".zbf_list ul li").eq(i).children('a').attr('id');
                if( json.shopFocusList[i][id] == "1" ){// 1 已关注  2 未关注
                    $(".zbf_list ul li").eq(i).find(".yi_gz").show();
                }else {
                    $(".zbf_list ul li").eq(i).find(".wei_gz").show();
                }
            };
        }
    };

