        <script type="text/javascript">
            // 开发判断是否登录
            function loginOrNot() {
                return false;
            }

            //发表回复
            var posting = false;
            function submitForm(pid, beReplyId) {
                if (posting) {
                    return false;
                }
                var $cmtEle = pid === 0 ? $('#JcmtCon') : $('#JcmtConPop');
                var $cmtCaptchaEle = pid === 0 ? $('#JcmtCaptcha') : $('#JcmtCaptchaPop');
                var $cmtSubmit = pid === 0 ? $('#JcmtSubmit') : $('#JcmtSubmitPop');
                if ($cmtEle.val() === "") {
                    return tip("请输入回复内容");
                }
                var l = $cmtEle.val().replace(/(^\s*)/g, '').length;
                var topicContentMinLength = 1 * '5';
                var topicContentMaxLength = 1 * '150';
                if (topicContentMinLength > l) {
                    return tip("帖子字数不能小于" + topicContentMinLength);
                }
                if (topicContentMaxLength < l) {
                    return tip("帖子字数不能大于" + topicContentMaxLength);
                }
                if (!isHidden($cmtCaptchaEle.parent().parent()) && $cmtCaptchaEle.val() === "") {
                    return tip("请输入验证码");
                }
                posting = true;
                $cmtSubmit.val("发表中");
                var message = $cmtEle.val();
        		$.ajax({
        			url : "http://g.pclady.com.cn/my/post/reply.do",
        			data : {
        				subjectId : 35279,
        				parentId : pid,
        				message : message,
        				beReplyId : beReplyId,
        				isWap : 1
        			},
        			type : "post",
        			success : function(data){
        				if(data.length < 100){
        					var jsonData = eval('(' + data + ')');
        					if(jsonData.statusCode == 300){
        						messageTip("回复失败");
        					}
        					return false;
        				}
        				//隐藏回复输入框
        				$("#JcmtConPop").val("");
        				$cmtSubmit.val("发表");
		                if (beReplyId == 0) {
		                	$("#postReplyBtn" + pid).click();
						} else {
							$("#childPostReplyBtn" + beReplyId).click();
						}        				
        				posting = false;
        				//不刷新插楼
        				$("#replyDiv" + pid).append(data);
        				//显示提示
        				var returnlevelId = $("#replyDiv" + pid).children(".insertReply").last().children("input[name='returnlevelId']").val();
        				var returncredits = $("#replyDiv" + pid).children(".insertReply").last().children("input[name='returncredits']").val();
        				getTipMessage(returnlevelId, returncredits);
        				return true;
        			}
        		});
            }
        
            function isHidden($ele){
                var _o = $ele[0].style.display;
                return _o == "none";
            }
        
            function tip(message){
                core.showTopTip(message);
                core.hideTopTip();
                return false;
            }
            
            (function(){
                var Jreply = document.getElementById('Jreply');
                if(!Jreply){
                    return false;
                }
                function matchSelector(ele, selector) {
                    if (selector.charAt(0) === "#") {
                        return ele.id === selector.slice(1);
                    }
                    if (selector.charAt(0) === ".") {
                        return (" " + ele.className + " ").indexOf(" " + selector.slice(1) + " ") != -1;
                    }
                    return ele.tagName.toLowerCase() === selector.toLowerCase();
                }
                Jreply.addEventListener('click', function(e) {
                    if (matchSelector(e.target, '.reply-btn')) {
                        if(!loginOrNot()){
                            //core.showFirstLoginWrap();
                        	messageTip("请先登录，登录了才能发表回复哦");
                        	return;
                        }
                    }
                });
            })();
            
            //组成回复成功提示语
            function getTipMessage(levelId, score) {
				if (levelId != '0') {
					messageTip("恭喜您等级升到" + levelId + "回复成功，获取学分" + score);
				} else if (score != '0') {
					messageTip("回复成功，获取学分" + score);
				}
			}
            
            //发表话题成功提示语
            function getSubjectTipMessage(desc, levelId, score) {
				if (levelId != '0') {
					messageTip("恭喜您等级升到" + levelId + "级" + desc + "成功，获取学分" + score);
				} else if (score != '0') {
					messageTip(desc + "成功，获取学分" + score);
				}		
			}            
        </script>
        <script type="text/javascript">
	    	function doLike(hasLike){
	    		//前端判断是否已经点赞过
	    		if(hasLike == 1){
	    			return messageTip("您已经喜欢过了哦");
	    		}
	    		var $txt=$('.eNub').text();
	    		var $parent=$(".acBtnMode");
	    		$.ajax({
	    	        url: "http://g.pclady.com.cn/my/post/doLike.do",
	    	        type: "POST",
	    	        data: {
	    	            subjectId : 35279,
	    	            isWap : 1
	    	        },
	    	        success: function (data) {
	    	        	json = eval('(' + data + ')');
	    	        	if (json.statusCode == 200) {
	    	    			var newLikeBtn = "<a href='javascript:void(0)' class='a-like' onclick='doLike(1)'>喜欢</a>";
	    	    			$("#likeBtn").children().remove();
	    	    			$("#likeBtn").html(newLikeBtn);
	    	    			messageTip(json.message);
	    	    			return true;
						} else {
							messageTip("您已经喜欢过了哦");
	    	    			return false;
						}
	    	        }
	    	    });
	    	}        	
        </script>
        
<script> 
/*视频兼容*/ 
(function(){var check={mobile:function(){var _this=this;var ua=navigator.userAgent.toLowerCase();var config={is_iphone:'iphone',is_android:'android',is_ipod:'ipod',is_ipad:'ipad',is_mobile:'mobile'};var result={mobile:false};for(i in config){if(ua.indexOf(config[i])!==-1){result[i]=true;result.mobile=true;}else{result[i]=false;};};return result;},loadJs:function(url){var head=document.getElementsByTagName("head")[0];var script=document.createElement("script");script.src=url
;script.onload=script.onreadystatechange=function(){var f=script.readyState;if(f&&f!="loaded"&&f!="complete")return;script.onload=script.onreadystatechange=null;head.removeChild(script);};head.appendChild(script);}};var link=location.href;if(link.indexOf('pclady.com.cn')!=-1){var check_result=check.mobile();if(check_result.mobile){check.loadJs('http://js.3conline.com/wap/pclady/2015/cms/covert.min.js');};};})(); 
</script>
	
