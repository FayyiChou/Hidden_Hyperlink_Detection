var oID = pattern[1].split("/")[1];
schemeUrl = "pcladyytbbrowser://app/type_id?type=" + strArr[i] + "&id=" + oID;
}
};
}
var downUrl = (APPTool.is);
APPTool.YTB.schemeInfo.schemeUrl = schemeUrl;
APPTool.bindDownBtn("downLoadBox",APPTool.YTB)
}else if(APPTool.isMdAPP()){
APPTool.showAPP("fb-mdktApp");
}else{
/*ab测试法随机露出悦选或摩登浮窗*/
function checkCookie() {
var showAB = Cookie.get("showAB");
var enterTime=new Date().getSeconds();
if (showAB != null) {
if(showAB >30){
APPTool.showAPP("fb-mdktApp");
APPTool.bindDownBtn("fb-mdktApp",APPTool.modernAPP);
}else{
APPTool.showAPP("fb-ios2017");
APPTool.bindDownBtn("fb-ios2017",APPTool.choiceAPP);
}
}else{
Cookie.set("showAB", enterTime, 30,"/");
if(enterTime >30){
APPTool.showAPP("fb-mdktApp");
APPTool.bindDownBtn("fb-mdktApp",APPTool.modernAPP);
}else{
APPTool.showAPP("fb-ios2017");
APPTool.bindDownBtn("fb-ios2017",APPTool.choiceAPP);
}
};
}
checkCookie();
}
