<script src="http://static.zhonghuasuan.com/javascript/common/jquery/jquery-1.9.1.min.js"></script>
<script src="http://static.zhonghuasuan.com/javascript/m/jquery.lazyload.min.js"></script>
   <script src="http://static.zhonghuasuan.com/javascript/m/goods-center.js"></script><script>

    /*=== load more ===*/
    var loadder = {
        loading:false,              // 请求ing...
        option:{
            loadTimes: 3,                   // 请求次数
            loadCount:10,                   // 请求条数
            parentid:0,
            listType:0,
            orderType:0,
        },
        loadingShow: function(){
            $('#load-more-text').addClass("hide");
            $('#loading').removeClass("hide"); 
        },
        url: '/home/lastpage',  // 请求地址
        load: function () {
            if (loadder.loading) return false;            // 正在请求就返回
            loadder.loading = true;                       // 锁定请求
            loadder.loadingShow();
            $.get(loadder.url, loadder.option , function (data) {
                if(data.replace(/(^\s*)|(\s*$)/g, "") == ""){ // 数据为空隐藏下一页按钮
                    $('#loading').hide();
                } else {
                    $("#load-more").replaceWith(data);
                    loadder.loading = false;               // 解锁请求
                    loadder.option.loadTimes++;
                }
            });
        }
    }
    $(function () {

        /*=== lazyload ===*/
        $('.goods-item-pic img').lazyload({ effect: "fadeIn" });

        /*=== laod more ===*/
        $(document).on('click','#load-more', loadder.load);

        /*=== nav active ===*/
       var navType = parseInt(getQueryString('listType')) || 0;
       $("#navigation a").eq(navType).addClass('active');

    });
    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return null;
    }
    
</script>
</body></html>