<script type="text/javascript">
function run(){
    var sec = $("#agree").attr('sec') - 1;

    if(sec < 1){
        $("#agree").remove();
        $("#checkbox").removeAttr("disabled");
        //$(".mysubmit").removeClass("disabled");
	   $(".mynext input").removeClass('mynonext').addClass('mynext');
        clearTimeout(interval);
    }else{
        $("#agree").html('('+ sec +')').attr('sec',sec);
    }
}
$("#checkbox").click(function(){
    if($(this).val() == 'true'){
        $("#submit").removeAttr("disabled");
    }else{
        $("#submit").attr("disabled",true);
    }
})
interval = setInterval(run, "1000");
</script>
<script>
    $(function(){
            //复选
        $(".radiospan").click(function(){
                if($(this).children('input').attr('checked')){
                    $(this).addClass("selradio");
                    $(this).children('input').attr("checked",true);
                }else{
                    $(this).children('input').attr("checked",false);
                    $(this).removeClass("selradio");
                }

            });
    })
</script>



