<script language="javascript" type="text/javascript">
	var siteid = "kf_9158";//企业号。目前只有生产的，直接写死：kf_9158
	var settingid = "";

	function openInPageChat(jnz){
		settingid = jnz;
		
		$.ajax({
			async:false,
			url : "http://lt.4007112366.com/kfpt-gwt/worktime/isWorkTime?jnzid=" + settingid,
			dataType : "jsonp",//数据类型为jsonp
			jsonp: "callback",//服务端用于接收callback调用的function名的参数
			success : function(data){
				var response = JSON.stringify(data);
				var array = response.split("\"isWorkTime\":\"")[1];
				var isWorkTime = array.split("\"")[0];
				if (isWorkTime != 'Y'){
					alert("亲，您好！工作日9:00-17:30，周末休息。请您在受理时间内咨询，谢谢您的支持和理解【税友】");
				}else{
					//知识库查询的维度值,需base64加密
					var json = '{"brand":"","custom1":"","location":"3100"}';
					var vid = Base64.encode(json);

					NTKF_PARAM = {
					  siteid:siteid,
					  settingid:settingid,
					  uid:"",//唯一标识，小能用来区分是哪个用户，一般传用户ID或手机号，可以传空，若传空为游客
					  uname:"",//与uid一起，标识用户的名称，uid为空，uname也传空	
					  erpparam:"sid=;sname=;vid=" + vid
					}

					var protocol = "https:" == location.protocol ? "https://" : "http://";

					$.get(protocol + 'dl.ntalker.com/js/xn6/ntkfstat.js?siteid=kf_9158', function () {
					  NTKF.im_openInPageChat(settingid)
					}, 'script');
				}
			},
			error:function(){
				//错误处理
			}
		});
	}
</script>

