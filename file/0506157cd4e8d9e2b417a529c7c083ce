<script type="text/javascript">
function kmss_onsubmit(){
	var loginInput = "请输入用户名和密码";
	if(document.forms[0].j_username.value==""){
		alert(loginInput);
		document.forms[0].j_username.focus();
		return false;
	}
	if(document.forms[0].j_password.value==""){
		alert(loginInput);
		document.forms[0].j_password.focus();
		return false;
	}
	
	document.getElementsByName("btn_submit")[0].disabled = true;
	//caojing 添加记住密码功能;
	SetPwdAndChk();
	encryptPassword();
	return true;
}

seajs.use(['lui/jquery'], function($) {
	window.onload = function(){
		var j_username = document.getElementsByName('j_username')[0].value;
		if(j_username==""){ 
			document.getElementsByName('j_username')[0].focus();
		}
		else{
			document.getElementsByName('j_password')[0].focus();
		}
		$(document).keypress(function(e) {
			//if (e.which == 13)
				//$("form").submit();
		});
		//var left = (screen.width-500)/2;
		//var top = (screen.height-450)/2;
		//window.showModalDialog('/ekp/loginHint.jsp', 'window', 'status:no;scroll:no;dialogWidth:500px;dialogHeight:450px');
	};
});
function encryptPassword(){ 
	document.forms[0].j_password.value = desEncrypt(document.forms[0].j_password.value);
}

function changetype(obj){
	var url = "";
	if(obj == "pc"){
		url = '/ekp/login.jsp';
	}
	if(obj == "pda"){
		url = '/ekp/third/pda/login.jsp';
	}
	url = Com_SetUrlParameter(url, "loginType", obj);
	location.href = url;
}
</script>
