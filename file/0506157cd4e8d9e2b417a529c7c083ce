<script language="javascript" type="text/javascript">
	function GetLastUser() {
		var id = "49BAC005-7D5B-4231-8CEA-16939BEACD67";
		var usr = GetCookie(id);
// 		alert("过期后usr："+usr);
		if (usr != null) {
			document.getElementsByName('j_username')[0].value = usr;
		} else {
			document.getElementsByName('j_username')[0].value = "";
		}
		GetPwdAndChk();
	}

	//点击登录出发客户端事件
	function SetPwdAndChk() {
		//取用户名
		var usr = document.getElementsByName('j_username')[0].value;
		//alert(usr);
		//将最后一个用户信息写入到Cookie
		SetLastUser(usr);
		//选中记住密码       
		if (document.getElementById('rememberPW').checked == true) {
			//取密码值 
			var pwd = document.getElementsByName('j_password')[0].value;
			var expdate = new Date();
			expdate.setTime(expdate.getTime() + 360 * (24 * 60 * 60 * 1000));
			//将用户名和密码写入到Cookie之前加密
			pwd = Base64.encode(pwd);
			SetCookie(usr, pwd, expdate);
		} else {
			//如果没有选中记住密码,则立即过期
			ResetCookie();
		}
	}

	//设置最后一个用户
	function SetLastUser(usr) {
		var id = "49BAC005-7D5B-4231-8CEA-16939BEACD67";
		var expdate = new Date();
		//当前时间加上360天的时间
		expdate.setTime(expdate.getTime() + 360 * (24 * 60 * 60 * 1000));
		SetCookie(id, usr, expdate);
	}

	//用户名失去焦点时调用该方法
	function GetPwdAndChk() {
		var usr = document.getElementsByName('j_username')[0].value;
		var pwd = GetCookie(usr);
		//密码解密
		if (pwd != null) {
			pwd = Base64.decode(pwd);
// 			alert("session过期后：-"+pwd+"-");
			document.getElementById('rememberPW').checked = true;
			document.getElementsByName('j_password')[0].value = pwd;
		} else {
			document.getElementById('rememberPW').checked = false;
			document.getElementsByName('j_password')[0].value = "";
		}
	}

	//取Cookie的值
	function GetCookie(name) {
		var arg = name + "=";
		var alen = arg.length;
		var clen = document.cookie.length;
		var i = 0;
		while (i < clen) {
			var j = i + alen;
			//alert(j);
			if (document.cookie.substring(i, j) == arg)
				return getCookieVal(j);
			i = document.cookie.indexOf(" ", i) + 1;
			if (i == 0)
				break;
		}
		return null;
	}

	//获取cookie值
	function getCookieVal(offset) {
		var endstr = document.cookie.indexOf(";", offset);
		if (endstr == -1)
			endstr = document.cookie.length;
		return unescape(document.cookie.substring(offset, endstr));

	}

	//写入到Cookie
	function SetCookie(name, value, expires) {
		var argv = SetCookie.arguments;
		//本例中length = 3
		var argc = SetCookie.arguments.length;

		var expires = (argc > 2) ? argv[2] : null;
		var path = (argc > 3) ? argv[3] : null;
		var domain = (argc > 4) ? argv[4] : null;
		var secure = (argc > 5) ? argv[5] : false;
		document.cookie = name
				+ "="
				+ escape(value)
				+ ((expires == null) ? "" : ("; expires=" + expires
						.toGMTString()))
				+ ((path == null) ? "" : ("; path=" + path))
				+ ((domain == null) ? "" : ("; domain=" + domain))
				+ ((secure == true) ? "; secure" : "");
	}
	//重置cookie
	function ResetCookie() {
		var usr = document.getElementsByName('j_username')[0].value;
		var expdate = new Date();
		SetCookie(usr, null, expdate);
	}
	
	//IE:如果浏览器中存在window.attachEvent函数则使用window.attachEvent函数,判断是否是IE还可以使用:if (document.all){//..}
	if(window.attachEvent) {
		window.attachEvent("onload", GetLastUser); 
	}else{//FireFox 
		window.addEventListener("onload", GetLastUser, false); 
	}  

</script>
