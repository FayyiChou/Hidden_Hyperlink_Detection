<script>
	window.onload=function(){
		function startMove(){
				var oBox=document.getElementById('demo'),
					speedX=3,
					speedY=3,
					timer=null;
				timer=setInterval(function(){
					var l=oBox.offsetLeft+speedX,
					t=oBox.offsetTop+speedY;
					if(l<0){
						l=0;
						speedX*=-1;
					}else if(l>document.documentElement.clientWidth-oBox.offsetWidth-2){
						l=document.documentElement.clientWidth-oBox.offsetWidth;
						speedX*=-1;
					}
					if(t<0){
						t=0;
					speedY*=-1;
					}else if(t>document.documentElement.clientHeight-oBox.offsetHeight-2){
						t=document.documentElement.clientHeight-oBox.offsetHeight;
						speedY*=-1;
				}
				oBox.style.left=l+'px';
				oBox.style.top=t+'px';
			},40)
			oBox.onmouseover=function(){
				clearInterval(timer);
				}
			oBox.onmouseout=function(){
				startMove();
				}
			var oClose=document.getElementById('close');
			var demo=document.getElementById('demo');
			oClose.onclick=function(){
				demo.style.display='none';
				
			}
					}
			startMove();
 videojs.options.flash.swf = "http://player.youku.com/player.php/sid/XODcxNDU3OTg4/v.swf";
	}
</script>
