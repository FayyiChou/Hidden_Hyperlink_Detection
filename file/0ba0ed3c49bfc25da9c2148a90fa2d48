  <script type="text/javascript">
 var winH = $(window).height();
    var hua = 1;
    var range = 50;  //距离底部距离
    var totalheight = 0; 
    var type = ('0'==""?0:'0');
    var url = '/Index/ajaxgetdata/typeurl/0/page/1';
    var page = 2;
    window.onscroll = function() {

        if (hua == 1) {
            var top = document.body.scrollTop || document.documentElement.scrollTop;
            totalheight = parseFloat(winH) + parseFloat(top);
            if (($(document).height() - range) <= totalheight) {
              hua = 0;
            $.post('/index/more_detail',{'type':type,'page':page},function (ret){
                if(ret.flag==1){
                    page++;
                    $("#aaa0").before(ret.data);
                    hua = 1;
                }else if(ret.flag==-1){
                    $("#aaa0").hide();
                    $(".nodata").show();
                }
            },'json');

            };



        };
    };
    document.getElementById('aaa0').onclick=function(){
        if(hua==1){
           hua = 0;
            $.post('/index/more_detail',{'type':type,'page':page},function (ret){
                if(ret.flag==1){
                    page++;
                    $("#aaa0").before(ret.data);
                    hua = 1;
                }else if(ret.flag==-1){
                    $("#aaa0").hide();
                    $(".nodata").show();
                }
            },'json');
        };
    };
	
