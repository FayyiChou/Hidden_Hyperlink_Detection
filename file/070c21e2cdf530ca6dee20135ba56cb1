<script>
function changeColumnName(){
	var a_url = window.location.href;
	var a_mark = a_url.split("/")[3];
	if(a_mark == "auto"){
		$(".column-name").attr("href","http://life.dayoo.com/auto/");
		$(".column-name").find("span:eq(0)").html("汽车");
		$(".column-name").find("span:eq(1)").html("auto.dayoo.com");
		return false;
	}
}
changeColumnName();
</script>



