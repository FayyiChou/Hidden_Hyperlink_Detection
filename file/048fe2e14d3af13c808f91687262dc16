	<script type="text/javascript">
		function onover_a(id){
			document.getElementById("li_a"+id).style.background = "url(/themes/default/css/images/pg_nav_onbg.png) no-repeat";
			document.getElementById("li_a"+id).style.color = "#148bb8";
			document.getElementById("nav0").style.background = "url(/themes/default/css/images/pg_nav_bg.png) no-repeat";
			document.getElementById("nav0_a").style.color = "#000";
		}
		function onout_a(id){
			document.getElementById("li_a"+id).style.background = "url(/themes/default/css/images/pg_nav_bg.png) no-repeat";
			document.getElementById("li_a"+id).style.color = "";
			document.getElementById("nav0").style.background = "url(/themes/default/css/images/pg_nav_onbg.png) no-repeat";
			document.getElementById("nav0_a").style.color = "#148bb8";
		}
		
	</script>
