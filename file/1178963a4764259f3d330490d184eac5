        <script>
        	$(function(){
				var no = 0;
				$(".bannerPrev").click(function(){
					no--;
					bannerEffect();	
				})
				$(".bannerNext").click(function(){
					no++;
					bannerEffect();	
				})
				function bannerEffect(){
					if(no<0){no = 3}
					if(no>3){no = 0}
					$(".banner li").eq(no).fadeIn(800).siblings().fadeOut(800);
				}
				var t= setInterval(function(){
					no++;
					if(no>3){no = 0}
					$(".banner li").eq(no).fadeIn(800).siblings().fadeOut(800);	
				},5000)
			})
        </script>
