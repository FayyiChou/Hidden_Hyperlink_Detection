<script>
$("img.lazy").lazyload({
	placeholder : "/statics/images/loading.jpg",
	effect      : "fadeIn",
	failurelimit : 30,
	width :300,
	height:180
});	
$(".searchText").focus(function(){
	if($(this).val()==this.defaultValue){
		$(this).val("")
		$(".searchInput").addClass("blur");
	}
})
$(".searchText").blur(function(){
	if($(this).val()==""){
		$(this).val("搜索特效");
		$(".searchInput").removeClass("blur");
	}
});
var ajaxUrl="/index.php?m=content&c=index&a=ajax_more&page=";//ajax请求连接地址
var pagecur=1;//当前页
$(".loadMore>a").click(function(){
	if($(".loadmore>a").text()=="没有更多了..."){
	    return false;
	}
	$.get(ajaxUrl+(pagecur+1), function(data){
		if(data==0){
			$(".loadmore>a").text("没有更多了...");
			return false;
		}
		$("#caseList").append(data);
	    pagecur++;
	});
}) 
</script>
