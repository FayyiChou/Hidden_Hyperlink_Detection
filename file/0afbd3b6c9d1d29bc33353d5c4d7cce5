   <script type="text/javascript">
    Navigation();
    setLoginStatus();
    function QQ_loginout() {
        QC.Login.signOut();
        $.ajax({
            type: "POST",
            url: "/Account/Logout",
            success: function (data) {
                if (data.code == "0") {
                    window.location.href = document.URL;
                }
                else {
                    artDialogMessage(data.message);
                }
            }
        })
        $('#dulogin_n').find('.dulogin_n_qq').hide();
        $('#dulogin_n').find('.dulogin_n_name').show();
        // window.location.href = window.location.href;
        return false;
    }
    //转到qq登陆页
    function QQ_login_page() {
        return window.open('https://graph.qq.com/oauth2.0/authorize?client_id=100356985&response_type=token&scope=all&redirect_uri=http://www.fx110.com/qqlogin/QQloginCallback.aspx?id=1', 'oauth2Login_10906', 'toolbar=no, menubar=no, scrollbars=no, status=no, location=yes, resizable=yes');
    }  
</script> 
   <script>
    (function () {
        var hearder_AD = $('.hearder_AD');
        setTimeout(
            function () {
                var ShowAry = ['/tradersdetail-', '/hkgold-', '/rights.', '/list-']; //
                var ShowAryIB = ['/ib-', '/view-'];
                var url = document.URL;
                var bol = true;
                var bolIb = true;
                if (hearder_AD) {
                    for (var z = 0; z < ShowAry.length; z++) {
                        if (url.indexOf(ShowAry[z]) >= 0) {
                            bol = false;
                        }
                    }
                    if (bol) {
                        for (var zz = 0; zz < ShowAryIB.length; zz++) {
                            if (url.indexOf(ShowAryIB[zz]) >= 0) {
                                bolIb = false;
                            }
                        }
                    }
                    else { hearder_AD.hide(); };
                    if (bolIb) { //主站顶通
                        $('.hearder_ADMain').show();
                        new AdvertisingSystem("172");
                    }
                    else { //IB顶通
                        $('.hearder_ADIb').show();
                        new AdvertisingSystem("173");
                    }
                }
            }, 100
            )
    })()

    $(function () {      
        var Popads = {
            time:0,
           set: function () {
               var that = this;
               that.time++;
               var eg = document.getElementById("EG0phD8f");
               if (eg) {eg.remove();return false;}
               else if(that.time >= 300){return false;}
              that.st = setTimeout(function () { that.set() },100)
           },
           setTime: function () {
               var that = this;
               that.st = setTimeout(function () { that.set() }, 1000);
           }
       }
       Popads.setTime()
   })
</script> 
