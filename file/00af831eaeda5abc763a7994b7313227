	$(function(){
		var s1 = $("body").height();
		$(".menu").css("height",s1)
		
		$(".caidan").click(function(){
			$(".menu").css("display","block");
		});
		$(".close_menu").click(function(){
			$(".menu").css("display","none");
		});
		
		$(".caidan").on("click", function(e){ 
				if($("table").is(":hidden")){ 
					$("table").show(); 
				}else{ 

					$("table").show(); 
				} 
				$(document).one("click", function(){ 
					$("table").hide(); 
					$(".menu").hide(); 
				});
				 e.stopPropagation(); 
			}); 
			$("table").on("click", function(e){
				 e.stopPropagation(); 
			});
	})
