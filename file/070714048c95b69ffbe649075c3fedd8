<script>
function searchfun()
{
   var kw = $.trim($("#textfield").val());
   if(kw=="" || kw=="请输入搜索关键词")
   {alert("请输入搜索关键词");return;}
   else{
     myForm.action='http://61.144.227.212/was5/web/search?searchword='+encodeURI($("#textfield").val());
     myForm.method='POST';
     myForm.submit();
   }
}
$("#submitbutton").click(function(){searchfun();});
$(".searchClose").click(function(){
	$(".mbSearchBoxPop").html($(".mbSearchBoxPop2").html());
});
var curl = location.href;
if(curl.indexOf("/2015ms/")>=0 || curl.indexOf("/zdyw/")>=0 ){
$("#ztzlbar").addClass("act");
}else if(curl.indexOf("/sbjxxgk/")>=0){
$("#zwgkbar").addClass("act");
}else if(curl.indexOf("/zxbs/")>=0){
$("#bsfwbar").addClass("act");
}else if(curl.indexOf("/gzfw/")>=0){
$("#hdjlbar").addClass("act");
}else{
}
</script>
