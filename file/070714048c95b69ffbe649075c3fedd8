<script type="text/javascript">
$(function(){
	if(navigator.appName == "Microsoft Internet Explorer") { 
		$("#pageforie").show();
		$("#pagefornotie").hide();
	} else{
		$("#pagefornotie").show();
		$("#pageforie").hide();
	}
	var countI=$(".ftdt-list li").length;
	var pagecount = 17;
	if(countI<20 || parseInt(pagecount)<=20){
		$(".loadMore").hide();;
	}
});
var current = 0;
var ext="";
var pagecount = 17;
if(parseInt(pagecount)>=300){
 pagecount = parseInt(300);
}
pagecount = parseInt(pagecount)/parseInt(20);
function next(){
  var url = "./data_23401.htm";
  current=parseInt(current)+parseInt(1);
	if(current<pagecount){
	   ext="_"+current;
	   url=url.substring(0,url.length-4)+ext+".htm";
	   //console.log(url);
	   $.ajax({
			url : url,
			success : function(data){
				var doms = $.parseHTML(data);
				$(".ftdt-list").append(data);
				//console.log("countItem="+$(doms).filter("li").length);
				var countItem=$(doms).filter("li").length;
				if(countItem<20){
					  $(".loadMore").text("");
				}else{
					  $(".loadMore").text("点击加载更多...");
				}
			 },
			 error : function(){
				  $(".loadMore").text("");
			 }
		 });
	}else if(parseInt(current)==parseInt(20)){
		 $(".loadMore").text("");  
	}else{

	}
}//next的结尾
</script>
</body></html>