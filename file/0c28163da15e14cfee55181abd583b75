<script>
			(function(){
				$.fn.lubo = function(h,f){
					var $box = $(this);
					var $li = $box.find("li");
					var $con = $box.find(h);
					var $tit = $box.find(f);
					var inx = 0;
					$li.each(function(index, element) {
						$(element).hover(function(){
							if(inx!=index){
								$tit.eq(inx).show();
								$tit.eq(index).hide();
								$con.eq(index).show();
								$con.eq(inx).hide();
								
								inx=index;	
							}
						},function(){});
					});	
				}
				$("#tab1").tab({blank:"http://www.18183.com/images/blank.gif"});
				$("#ot1").lubo(".gdcon",".tit_con1");
				$("#ot2").lubo(".gdcon",".tit_con1");
				$("#ot3").lubo(".gdcon",".tit_con1");
			})();
