<script type="text/javascript">
    function go(obj) {
        if (obj.options[obj.selectedIndex].value != "")
            window.open(obj.options[obj.selectedIndex].value);
    }
</script>



    <script type="text/javascript">
    jQuery(".home-res").slide({ titCell: ".hd ul", mainCell: ".bd ul", autoPage: true, effect: "left", autoPlay: false, scroll: 1, vis: 6, pnLoop: false });

    $(document).ready(function (e) {
        $("#banner").mslide();
        $(".bd ul li a img").each(function (index, element) {
            var t = $(this);
            var src = t.attr("data-src");
            var srch = t.attr("data-srch");
            t.attr("src", src);
            t.hover(function (e) {
                t.attr("src", srch);
            }, function (e) {
                t.attr("src", src);
            });
        });
    });

    function logonGbzx()
    {
        if ($("#UserName").val() == "")
        {
            $("#UserName").focus();
            return;
        }
        if ($("#Password").val() == "") {
            $("#Password").focus();
            return;
        }
        $("#formGbzx").submit();
    }
    </script>




