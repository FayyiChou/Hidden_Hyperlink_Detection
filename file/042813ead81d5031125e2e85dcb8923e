<script type="text/javascript">

$(function() {
	initCAPTCHASHR('#shrLoginForm', '#btn_sign_shr', '#embed-captcha-shr', '#wait-shr', '#notice-shr');
});
	// 用户注册
	function register() {
		window.open("http://sc.gsxt.gov.cn/notice/user/register?userType="
				+ $("#normalLoginForm [name=userType]").val());
	}
	
	function initCAPTCHASHR(formId, btnId, captchaContainerId, waitId, noticeId) {	
		var handlerEmbed = function (captchaObj) {
		    $(btnId).click(function (e) {
		    	if (!$(formId).validate().form()) {
		    		captchaObj.refresh();
		    		return false;
		    	}
		        var validate = captchaObj.getValidate();
		        if (!validate) {
		            $(noticeId)[0].className = "show";
		            setTimeout(function () {
		                $(noticeId)[0].className = "hide";
		            }, 2000);
		            e.preventDefault();
		        } else {
		        	$.ajax({
			            url: "pc-geetest/validate", // 进行二次验证
			            type: "post",
			            dataType: "json",
			            data: {
			                geetest_challenge: validate.geetest_challenge,
			                geetest_validate: validate.geetest_validate,
			                geetest_seccode: validate.geetest_seccode
			            },
			            success: function (data) {
			                if (data && (data.status === "success")) {
			                	$('#geetest_challengeshr').val(validate.geetest_challenge);
			                	$('#geetest_validateshr').val(validate.geetest_validate);
			                	$('#geetest_seccodeshr').val(validate.geetest_seccode);
			                	logon('#shrLoginForm', 'shr');
			                } else {
			                	window.top.dialogMessage("验证码校验失败！"); 
			                }
			                captchaObj.refresh();
			            }
			        });
		        }
		        return false;
		    });
		    // 将验证码加到id为captcha的元素里，同时会有三个input的值：geetest_challenge, geetest_validate, geetest_seccode
		    $(captchaContainerId).empty();
		    captchaObj.appendTo(captchaContainerId);
		    captchaObj.onReady(function () {
		        $(waitId)[0].className = "hide";
		    });
		    // 更多接口参考：http://www.geetest.com/install/sections/idx-client-sdk.html
		    //alert($("#embed-captcha").html());
		};
		$.ajax({
		    // 获取id，challenge，success（是否启用failback）
		    url: "pc-geetest/register?t=" + (new Date()).getTime(), // 加随机数防止缓存
		    type: "get",
		    dataType: "json",
		    success: function (data) {
		        // 使用initGeetest接口
		        // 参数1：配置参数
		        // 参数2：回调，回调的第一个参数验证码对象，之后可以使用它做appendTo之类的事件
		        initGeetest({
		            gt: data.gt,
		            challenge: data.challenge,
		            product: "float", // 产品形式，包括：float，embed，popup。注意只对PC版验证码有效
		            offline: !data.success // 表示用户后台检测极验服务器是否宕机，一般不需要关注
		            // 更多配置参数请参见：http://www.geetest.com/install/sections/idx-client-sdk.html#config
		        }, handlerEmbed);
		    }
		});
	}

</script></div>


<script type="text/javascript">
	$(function() {	
		// 重置登录计数
		login.count("reset");
		
		// 根据用户类型限定初始化页面显示
		 
			// 初始化登录标签页显示
			$(".login-popup .title-bar a[rel]").each(function() {
				if("0|1|2|6|8|0|1|8".indexOf($(this).attr("rel")) == -1) {
					$(this).remove();
				}
			});
			
			// 初始化登录方式显示
			if($(".login-popup .title-bar a[rel]").size() == 0) {
				$("[name=mode]").parent("li").remove();
				$("[rel^='login-']").remove();
			} else if("0|1|2|6|8|0|1|8".indexOf("0") == -1) {
				if("0|1|2|6|8|0|1|8".indexOf("9") == 0) {
					tab('shr','9');
					$("[name=mode]").parent("li").remove();
					$("[rel='login-ca']").remove();
					$("[rel='login-normal']").remove();
					return;
				}else {
					$("#mode-ca").parent("li").remove();
					$("[rel='login-ca']").remove();
				}
			}
		 
		// 默认初始化登录方式
		tab();
		
		$('#nav li a').click(function() {
			var isCurrent = $(this).hasClass("selected");
			if (!isCurrent) {
				$('#nav li a').removeClass("selected");
				$(this).addClass('selected');
			}
		});
		$('#nav li a').first().click();
		$('#nav').show();
	});

	// 登录
	function logon(form, opt) {
		// 校验表单
		if(!$(form).validate().form()) {
			return false;
		}
		switch(opt) {
			case "sms":
				// 企业联络员用户登陆
				parent.ajaxCommon({
					url: "user/verify_login_sc",
					data: $.extend({
						regNo: $(form).find("[name=regNo]").val(),
						password: hex_md5($(form).find("[name=password]").val()), // MD5加密后传参
						idCardType: $(form).find("[name=idCardType]").val(),//登录用户类型 sc随便写写
						geetest_challenge: $(form).find("[name=geetest_challenge]").val(),
						geetest_validate: $(form).find("[name=geetest_validate]").val(),
						geetest_seccode: $(form).find("[name=geetest_seccode]").val()
						//liaisonCertNo: $(form).find("[name=liaisonCertNo]").val(),
					}, global.token.data)
				}, function(data) {
					var map = JSON.parse(data);
					
					// 错误信息为空，则表示登录成功
					// 否则即登录失败
					if(typeof map.error == "undefined") {
						// 如果当前用户仍使用初始化密码，则跳转至密码修改页面
						// 否则将跳转至预设页面
						if(map.initPwd == "1") {
							parent.login.popupRedirect("http://sc.gsxt.gov.cn/notice/user/toUpdatePwd", "_self", {
								redirect: "http://sc.gsxt.gov.cn/notice/report/home"
							});
						}else { 
							parent.login.popupRedirect("http://sc.gsxt.gov.cn/notice/report/home", "_self", {
								loginId: map.loginId
							});
						} 
					} else {
						// 弹出错误信息
						window.top.dialogMessage(map.error);
					}
				});
				break;
			case "peReg":
				parent.ajaxCommon({
					url: "user/verify_login_sc_pe",
					data: $.extend({
						userType: "2",
						regNo: $(form).find("[name=regNo]").val(),
						/* password: hex_md5($(form).find("[name=password]").val()), // MD5加密后传参 */
						idCardType: $(form).find("[name=idCardType]").val(),
						idCardNo: $(form).find("[name=idCardNo]").val(),
						geetest_challenge: $(form).find("[name=geetest_challenge]").val(),
						geetest_validate: $(form).find("[name=geetest_validate]").val(),
						geetest_seccode: $(form).find("[name=geetest_seccode]").val()
					}, global.token.data)
				}, function(data) {
					var map = JSON.parse(data);
					
					// 错误信息为空，则表示登录成功
					// 否则即登录失败
					if(typeof map.error == "undefined") {
						// 如果当前用户仍使用初始化密码，则跳转至密码修改页面
						// 否则将跳转至预设页面
						/* if(map.initPwd == "1") {
							parent.login.popupRedirect("http://sc.gsxt.gov.cn/notice/user/toUpdatePwd", "_self", {
								redirect: "http://sc.gsxt.gov.cn/notice/report/home"
							});
						} else { */
							parent.login.popupRedirect("http://sc.gsxt.gov.cn/notice/report/home", "_self", {
								loginId: map.loginId
							});
						/* } */
					} else {
						// 弹出错误信息
						window.top.dialogMessage(map.error);
					
					}
				});
				break;
			case "normal":
				// 普通三种用户登陆
				// 后台根据userType, loginName, password三个属性判断是否存在该用户
				parent.ajaxCommon({
					url: "user/verify_login",
					data: $.extend({
						userType: $(form).find("[name=userType]").val(),
						loginName: $(form).find("[name=loginName]").val(),
						password: hex_md5($(form).find("[name=password]").val()), // MD5加密后传参
						idCardType: $(form).find("[name=idCardType]").val(),
						geetest_challenge: $(form).find("[name=geetest_challenge]").val(),
						geetest_validate: $(form).find("[name=geetest_validate]").val(),
						geetest_seccode: $(form).find("[name=geetest_seccode]").val()
					}, global.token.data)
				}, function(data) {
					var map = JSON.parse(data);
					
					// 错误信息为空，则表示登录成功
					// 否则即登录失败
					if(typeof map.error == "undefined") {
						// 如果当前用户仍使用初始化密码，则跳转至密码修改页面
						// 否则将跳转至预设页面
					/* 	if(map.initPwd == "1") {
							parent.login.popupRedirect("http://sc.gsxt.gov.cn/notice/user/toUpdatePwd", "_self", {
								redirect: "http://sc.gsxt.gov.cn/notice/report/home"
							});
						} else { */
							parent.login.popupRedirect("http://sc.gsxt.gov.cn/notice/report/home", "_self", {
								loginId: map.loginId
							});
						/* } */
					} else {
						// 弹出错误信息
						window.top.dialogMessage(map.error);
						
					}
				});
				break;
			case "shr":
				// 其他部门用户登陆
				// 后台根据userType, loginName, password三个属性判断是否存在该用户
				parent.ajaxCommon({
					url: "user/verify_login",
					data: $.extend({
						userType: $(form).find("[name=userType]").val(),
						loginName: $(form).find("[name=loginName]").val(),
						password: hex_md5($(form).find("[name=password]").val()), // MD5加密后传参
						idCardType: $(form).find("[name=idCardType]").val(),
						geetest_challenge: $(form).find("[name=geetest_challenge]").val(),
						geetest_validate: $(form).find("[name=geetest_validate]").val(),
						geetest_seccode: $(form).find("[name=geetest_seccode]").val()
					}, global.token.data)
				}, function(data) {
					var map = JSON.parse(data);
					
					// 错误信息为空，则表示登录成功
					// 否则即登录失败
					if(typeof map.error == "undefined") {
						parent.login.popupRedirect("http://sc.gsxt.gov.cn/notice/report/home", "_self", {
							loginId: map.loginId
						});
					} else {
						// 弹出错误信息
						window.top.dialogMessage(map.error);
						
					}
				});
				break;
			default:
				break;
		}
	}
	
