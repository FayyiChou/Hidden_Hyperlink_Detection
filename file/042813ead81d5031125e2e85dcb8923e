<script type="text/javascript">
$(function() {
	// 预设当前需显示的导航链接
	var navs = new Array();
	
		navs.push("94");
	

	// 初始化导航栏显示
	toggleNav(navs);
	
});

// 切换导航栏链接显示
function toggleNav(navs) {
	// 隐藏所有导航链接
	$(".inforFunction li[rel]").hide();
	// 显示预设导航链接
	for(var i in navs) {
		$(".inforFunction li[rel='nav-" + navs[i] + "']").show();
		//默认顶部显示不带菜单的蓝色部分
		if(navs[i]=='92'){
			$("#navv-" + navs[i] + "").show();
			$("#navv-91").hide();
		}
		
		/* if(navs[i]=='94'){
			$("#big_small").removeClass().addClass("logoC");
		} */

		//默认没有 退出登录栏
		if(navs[i]=='03'){
			$("#nav-" + navs[i]).show();
		}
	}
}

// 下载使用帮助
function help() {
	var iframe = $("iframe#download");
	
	if(iframe.size() == 0) {
		iframe = elem('<iframe src="javascript:" class="hide" id="download"></iframe>').appendTo("body");
	}
	
	iframe.attr("src", "http://sc.gsxt.gov.cn/notice/download/handbook?name=help.doc");
}

// 下载电子营业执照
function licenceEx() {
	var iframe = $("iframe#download");
	
	if(iframe.size() == 0) {
		iframe = elem('<iframe src="javascript:" class="hide" id="download"></iframe>').appendTo("body");
	}
	
	iframe.attr("src", "http://sc.gsxt.gov.cn/notice/download/licenceEx?entId="+'');
}

function toggle(targetid){
    if (document.getElementById){
        target=document.getElementById(targetid);
            if (target.style.display=="block"){
                target.style.display="none";
            } else {
                target.style.display="block";
            }
    }
}
</script>

