	// 切换登录方式
	function tab(opt, ut) {
		switch(opt) {
			case "sms":
				$("[rel^='login-']").hide().filter("[rel='login-" + opt + "']").show();
				$("[rel=loginName]").html("统一社会信用代码/注册号：");
				$("[name=loginName]").attr("label", "统一社会信用代码/注册号");
				break;
			case "peReg":
				$("[rel^='login-']").hide().filter("[rel='login-" + opt + "']").show();
				break;
			case "shr":
				$("[rel^='login-']").hide().filter("[rel='login-" + opt + "']").show();
				break;
			case "normal":
				$("[rel^='login-']").hide().filter("[rel='login-" + opt + "']").show();
				$("[rel=loginName]").html("证件号码：");
				$("[name=loginName]").attr("label", "证件号码");
				break;
			case "insp":
				$("[rel=loginName]").html("检验机构登录名：");
				$("[name=loginName]").attr("label", "检验机构登录名");
				$("[rel=login-normal]").show();
				$("#divRegist").hide();
				break;
			default:
				// 默认选择登录方式
				$(".login-popup .title-bar a[rel]").first().click();
				//$("[name=mode]").first().click();
				break;
		};
		// 设置用户登录类型
		if(typeof ut == "undefined") {
			$("[name=userType]").val($("[name=mode]:checked").val());
		} else {
			$("[name=userType], #mode-normal").val(ut);
		}
	}
</script>
</div></div>
	
	
