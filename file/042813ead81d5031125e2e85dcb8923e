<script type="text/javascript">
$(function() {
	initCAPTCHANORMAL('#normalLoginForm', '#btn_sign_normal', '#embed-captcha-normal', '#wait-normal', '#notice-normal');
});
	// 用户注册
	function register() {
		window.open("http://sc.gsxt.gov.cn/notice/user/register?userType="
				+ $("#normalLoginForm [name=userType]").val());
	}
	
	function initCAPTCHANORMAL(formId, btnId, captchaContainerId, waitId, noticeId) {	
		var handlerEmbed = function (captchaObj) {
		    $(btnId).click(function (e) {
		    	if (!$(formId).validate().form()) {
		    		captchaObj.refresh();
		    		return false;
		    	}
		        var validate = captchaObj.getValidate();
		        if (!validate) {
		            $(noticeId)[0].className = "show";
		            setTimeout(function () {
		                $(noticeId)[0].className = "hide";
		            }, 2000);
		            e.preventDefault();
		        } else {
		        	$.ajax({
			            url: "pc-geetest/validate", // 进行二次验证
			            type: "post",
			            dataType: "json",
			            data: {
			                geetest_challenge: validate.geetest_challenge,
			                geetest_validate: validate.geetest_validate,
			                geetest_seccode: validate.geetest_seccode
			            },
			            success: function (data) {
			                if (data && (data.status === "success")) {
			                	$('#geetest_challengeno').val(validate.geetest_challenge);
			                	$('#geetest_validateno').val(validate.geetest_validate);
			                	$('#geetest_seccodeno').val(validate.geetest_seccode);
			                	logon('#normalLoginForm', 'normal');
			                } else {
			                	window.top.dialogMessage("验证码校验失败！"); 
			                }
			                captchaObj.refresh();
			            }
			        });
		        }
		        return false;
		    });
		    // 将验证码加到id为captcha的元素里，同时会有三个input的值：geetest_challenge, geetest_validate, geetest_seccode
		    $(captchaContainerId).empty();
		    captchaObj.appendTo(captchaContainerId);
		    captchaObj.onReady(function () {
		        $(waitId)[0].className = "hide";
		    });
		    // 更多接口参考：http://www.geetest.com/install/sections/idx-client-sdk.html
		    //alert($("#embed-captcha").html());
		};
		$.ajax({
		    // 获取id，challenge，success（是否启用failback）
		    url: "pc-geetest/register?t=" + (new Date()).getTime(), // 加随机数防止缓存
		    type: "get",
		    dataType: "json",
		    success: function (data) {
		        // 使用initGeetest接口
		        // 参数1：配置参数
		        // 参数2：回调，回调的第一个参数验证码对象，之后可以使用它做appendTo之类的事件
		        initGeetest({
		            gt: data.gt,
		            challenge: data.challenge,
		            product: "float", // 产品形式，包括：float，embed，popup。注意只对PC版验证码有效
		            offline: !data.success // 表示用户后台检测极验服务器是否宕机，一般不需要关注
		            // 更多配置参数请参见：http://www.geetest.com/install/sections/idx-client-sdk.html#config
		        }, handlerEmbed);
		    }
		});
	}
