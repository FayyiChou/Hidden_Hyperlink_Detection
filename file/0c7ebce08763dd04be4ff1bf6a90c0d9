<!--JavaScript部分-->
            <SCRIPT language=javascript>
     function seceBoard(n)
  {
   var secTable = document.getElementById("seceTable"); 
	var td = secTable.getElementsByTagName("TD"); 
	
	for(var i= 0; i<td.length; i++){
		td[i].className = "sec1e";
	}    
    td[n].className = "sec2e";
	
    for(var k = 0; k<6; k++){
	document.getElementById('tblj_'+k).style.display = "none";		 
    }   
	document.getElementById('tblj_'+n).style.display = "block";
  }
      </SCRIPT>

