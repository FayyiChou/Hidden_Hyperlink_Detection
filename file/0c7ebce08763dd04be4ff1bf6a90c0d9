<SCRIPT language="JavaScript" type="text/javascript">


  function secBoard(n)
  {
    var secTable = document.getElementById("secTable"); 
	var mainTable = document.getElementById("mainTable"); 
	var td = secTable.getElementsByTagName("TD"); 
	var tbody = mainTable.getElementsByTagName("TBODY");
	for(var i= 0; i<td.length; i++){
		td[i].className = "sec1";
	}    
    td[n].className = "sec2";
	
    for(var k = 0; k<4; k++){
	document.getElementById('tb_'+k).style.display = "none";		
    }   
	document.getElementById('tb_'+n).style.display = "block";
  }
</SCRIPT>

