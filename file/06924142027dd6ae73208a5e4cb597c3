            function changetype() {
                    if($('searchtxt').value == '标题') {
                            $('searchname').value = 'subject';
                    }else if($('searchtxt').value == '内容') {
                            $('searchname').value = 'message';
                    }else if($('searchtxt').value == '作者') {
                            $('searchname').value = 'author';
                    }
            }
