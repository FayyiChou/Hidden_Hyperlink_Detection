<script type="text/javascript" language="javascript">

function changeLab(objId){
if(document.getElementById(objId)){
var obj=document.getElementById(objId);
var h3s=obj.getElementsByTagName("h3");
var uls=obj.getElementsByTagName("ul");

//var a_link = new Array;
//a_link[0] = "#";
//a_link[1] = "#";
//a_link[2] = "#";
//a_link[3] = "#";


seth3(h3s,uls,obj);
if(h3s.length>1){
for(var i=0,h3,ul;h3=h3s[i],ul=uls[i];i++){
h3.onmouseover=function(){

var a_counts=this.id.split("_");
//objs=document.getElementById(a_counts[1]);
emHid(h3s,uls);
this.className=this.className+" onthis";
strUlname=a_counts[0]+"_"+a_counts[1]+"_ul_"+a_counts[3];
thisUl=document.getElementById(strUlname);
//alert(thisUl.id);
thisUl.className="show";

}
/*
h3.ondblclick=function(){
var a_counts=this.id.split("_");
var q = a_counts[3];
var strUrl=a_link[q];
removeAttribute("href");
}
*/
}
}
}

}

function seth3(array_h3,array_ul,obj){
for(var i=0,h3,ul;h3=array_h3[i],ul=array_ul[i];i++){
h3.id=obj.id+"_h3_"+i;
ul.id=obj.id+"_ul_"+i;


//alert(ul.id);
h3.className="onthis p"+i
ul.className="show";
if (i!=0){
h3.className="p"+i;
ul.className="hid";

}
}
}

function emHid(array_h3,array_ul){

for(var i=0,ul,h3;ul=array_ul[i],h3=array_h3[i];i++){
ul.className="hid";
h3.className="p"+i;
}
}
