<script >  //案例切换按钮
	var caseListLen = $('.caseindexlist li').length;
	var caseMenuPointLen = Math.ceil(caseListLen/3);
	for(var i = 0;i < caseMenuPointLen;i++){
		$('.caseindexmenul').append('<a href="javascript:void(0);" class="png"></a>');
	}
	$('.caseindexmenul a').first().addClass('cur png');
	$('.caseindexmenul a').click(function(){
		var index = $(this).index();
		$(this).siblings().removeClass('cur');
		$(this).addClass('cur');
		var leftLen = 337*3*index;
		$('.caseindexlist ul').animate({marginLeft:-1*leftLen},2000);
		//clearInterval(casetimer);
		return false;
	});
	
	$('.caseindexmenul a').mouseover(function(){
		clearInterval(casetimer);
		return false;
	});
   //幻灯片自动换页 
     casetimer = setInterval(casetab,8000);
	 
    //当鼠标悬停到图片上时停止自动换页  
    caseindexlistbox.onmouseover = function(){  
        clearInterval(casetimer);
    }  
  
    //鼠标离开图片区域，重新开启自动换页  
    caseindexlistbox.onmouseout = function(){  
        casetimer = setInterval(casetab,8000);  
    }
	
  function casetab(){
		var nextObj = $('.caseindexmenul a.cur').next();
		if(nextObj.length == 0){
			nextObj = $('.caseindexmenul a').first();
		}
		nextObj.click();
	}
  </script>
