    <script>
        // 全局变量设置
        (function () {
            var modelString = "";
            saas.model = JSON.parse(Commons.DecodeToStr(modelString));
            
        })();

        $(function () {//关注功能
            saas.setHistory(saas.model);
            saas.AddAttention = function (productId) {
                checkLogin(function (isLogin, loginUrl) {
                    if (isLogin) {
                        saas.ajax("AddAttention/" + productId, {}, function (result) {
                            if (result.Code == 1) {
                                layer.alert("成功关注！", {
                                    title: false, icon: 6, cancel: function () {
                                        window.location.reload();
                                    }
                                }, function () {
                                    window.location.reload();
                                });
                            }
                        }, "/moyu/");
                    } else {
                        window.location.href = loginUrl;
                        window.event && (window.event.returnValue = false);
                    }
                }, Commons.currentUrl);
            };
            saas.DelAttention = function (productId) {
                saas.ajax("DelAttention/" + productId, {}, function (result) {
                    if (result.Code == 1) {
                        window.location.reload();
                    }
                }, "/moyu/");
            }

            saas.HasAttention = function (productId) {
                saas.ajax("HasAttention/" + productId, {}, function (result) {
                    if (result.Code == 1) {
                        $("#btnAttention").bind("click", function () {
                            saas.DelAttention(productId);
                        }).text("取消关注");
                    } else {
                        $("#btnAttention").bind("click", function () {
                            saas.AddAttention(productId);
                        }).text("关 注");
                    }
                }, "/moyu/", function (httprequest) { });
            }
            saas.HasAttention(163388);
        })

        $(function () {//详细页公共方法
            //span赋值
            saas.setModel = function (model) {
                $("#divMain span,em,p,a,i").each(function () {
                    var keys = $(this).data("model");
                    if (keys != null) {
                        if ($(this).data("bind") == null)//保存模板防止多次赋值
                            $(this).data("bind", $(this).html());
                        var arrKey = keys.split(',');
                        var bindStr = $(this).data("bind");
                        for (var i = 0; i < arrKey.length; i++) {
                            if (model != null) {
                                if (model[arrKey[i]] != null) {
                                    bindStr = bindStr.replace("{" + i + "}", model[arrKey[i]]);
                                }
                                else {
                                    bindStr = bindStr.replace("{" + i + "}", "");
                                }
                            }
                            else {
                                bindStr = "";
                            }
                        }
                        $(this).html(bindStr);
                    }
                })
            }
            //数字转图片
            saas.getNumberPic = function (number) {
                var html = "";
                var className = "";
                if (number == null)
                    number = 0;
                for (var i = 0; i < number.toString().length; i++) {
                    switch (Number(number.toString().charAt(i))) {
                        case 0:
                            className = "zero";
                            break;
                        case 1:
                            className = "one";
                            break;
                        case 2:
                            className = "two";
                            break;
                        case 3:
                            className = "three";
                            break;
                        case 4:
                            className = "four";
                            break;
                        case 5:
                            className = "five";
                            break;
                        case 6:
                            className = "six";
                            break;
                        case 7:
                            className = "seven";
                            break;
                        case 8:
                            className = "eight";
                            break;
                        case 9:
                            className = "nine";
                            break;
                    }
                    html = html + '<i class="' + className + '"></i>';
                }
                return html;
            }
        })
    </script>
    
    <script>
        // 页面类
        var GeneralPage = (function () {

            // 构造函数
            function GeneralPage() {
                /// <summary>构造方法</summary>
            }

            // 静态内部属性
            (function () {
            })();

            // 静态属性
            (function () {
                GeneralPage._ = {};
            })();

            // 静态方法
            (function () {
                // 页面初始化
                GeneralPage.init = function () {
                    // 显示[总览]tab选中
                    $("#navGeneral").addClass("on");

                    // 设置切导航行为
                    saas.toggleNav = function (obj, key) {
                        $(obj).parent().find("a").removeClass("on");
                        $(obj).addClass("on");
                        $(".main-right [id^=div]").hide();
                        $("#div" + key).show();
                    }

                    // 显示左边区域
                    var leftFrame = new LeftFrame();
                    leftFrame.show();

                    // 显示右边区域
                    var rightFrame = new RightFrame();
                    rightFrame.show();
                };
            })();

            return GeneralPage;
        })();

        // 左边区域类
        var LeftFrame = (function () {

            // 构造函数
            function LeftFrame() {
                /// <summary>构造方法</summary>
                this.template = $(".main-left>.templete").html();
                this.landTemplate = $("#landDetailtemplate").html();
            }

            // 渲染左边区域
            LeftFrame.prototype.show = function () {
                // 获取战斗力图片
                saas.model.Extend.base.battle_lev_pic = getBattlePic(saas.model.Extend.base.battle_lev);

                // 渲染html
                var html = TrimPath.parseTemplate(this.template).process(saas.model.Extend);
                $("#leftFrame").html(html);

                // 渲染摊位和地皮信息
                html = TrimPath.parseTemplate(this.landTemplate).process(saas.model.Extend);
                $("#tempDiv").prepend(html);

                // 绑定地皮信息显示
                bindLandBox();
            };

            // 控制地皮信息显示
            var bindLandBox = function () {
                var timeHander;
                $(".land-box").bind("mouseover", function (a) {
                    var landNo = $(this).data("landno");
                    var detailsBox = $("#" + landNo);

                    clearTimeout(timeHander);
                    $(".details-box-2").css("display", "none");
                    detailsBox.css("left", a.pageX + "px");
                    detailsBox.css("top", a.pageY + "px");
                    detailsBox.css("display", "block");
                });
                $(".land-box").bind("mouseout", function (a) {
                    var detailsBox = $(".details-box-2");

                    timeHander = setTimeout(function () {
                        detailsBox.css("display", "none");
                    }, 500);
                });
                $(".details-box-2").bind("mouseover", function (a) {
                    clearTimeout(timeHander);
                });
                $(".details-box-2").bind("mouseout", function (a) {
                    var detailsBox = $(".details-box-2");
                    timeHander = setTimeout(function () {
                        detailsBox.css("display", "none");
                    }, 500);
                });
            };

            // 获取战斗力图片
            var getBattlePic = function (number) {
                /// <summary>获取战斗力图片</summary>

                var html = "";
                var className = "";
                if (number == null)
                    number = 0;
                for (var i = 0; i < number.toString().length; i++) {
                    switch (Number(number.toString().charAt(i))) {
                        case 0:
                            className = "zero";
                            break;
                        case 1:
                            className = "one";
                            break;
                        case 2:
                            className = "two";
                            break;
                        case 3:
                            className = "three";
                            break;
                        case 4:
                            className = "four";
                            break;
                        case 5:
                            className = "five";
                            break;
                        case 6:
                            className = "six";
                            break;
                        case 7:
                            className = "seven";
                            break;
                        case 8:
                            className = "eight";
                            break;
                        case 9:
                            className = "nine";
                            break;
                    }
                    html = html + '<i class="' + className + '"></i>';
                }
                return html;
            };

            return LeftFrame;
        })();

        // 右边区域类
        var RightFrame = (function () {
            // 构造函数
            function RightFrame() {
                /// <summary>构造方法</summary>
                this.template = $(".main-left>.templete").html();
            }

            // 渲染左边区域
            RightFrame.prototype.show = function () {
                saas.addRow('Normal', 0, 6, saas.model.Extend.magic.normalList);
                saas.addRow('XP', 0, 6, saas.model.Extend.magic.xpList);
                saas.addRow('God', 0, 6, saas.model.Extend.magic.godList);
                saas.addRow('Social', 0, 6, saas.model.Extend.magic.socialList);
            };

            return RightFrame;
        })();

        // 默认调用
        (function () {
            GeneralPage.init();
        })();

    </script>

    <script type="text/javascript">
