<script type="text/plain" class="templete" id="pagerTemplete">
    {if totalPage > 0}
    {if prePage > 0}
    <a class="goto-page" href="javascript:void(0);" data-pageno="${prePage}">上一页</a>
    {else}
    <a class="disable" href="javascript:void(0);">上一页</a>
    {/if}
    {/if}
    {if minPageNo > 2}
    <a class="goto-page" href="javascript:void(0);" data-pageno="1">1</a>
    <span>...</span>
    {/if}
    {for pageNo in pageNos}
    {if curPageNo == pageNo}
    <a class="on goto-page" href="javascript:void(0);" data-pageno="${pageNo}">${pageNo}</a>
    {else}
    <a class="goto-page" href="javascript:void(0);" data-pageno="${pageNo}">${pageNo}</a>
    {/if}
    {/for}
    {if maxPageNo < totalPage}
    <span>...</span>
    <a class="goto-page" href="javascript:void(0);" data-pageno="${totalPage}">${totalPage}</a>
    {/if}
    {if totalPage > 0}
    {if nextPage <= totalPage}
    <a class="goto-page" href="javascript:void(0);" data-pageno="${nextPage}">下一页</a>
    {else}
    <a class="disable" href="javascript:void(0);">下一页</a>
    {/if}
    {/if}
</script>
