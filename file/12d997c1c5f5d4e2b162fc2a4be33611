<script type="text/javascript">

    //修改部分
    $(function(){
        //log.Show('sign2');
    });

    $('.sign2 .sign2-close').click(function(){
        log.Hide('sign2');
    });
	$('.yy').click(function(){
		log.Show('sign2');
	});
    window.onload = function(){
        var $li = $('.sytab li');
        var $ul = $('.sycon ul');
        $li.mouseover(function(){
            var $this = $(this);
            var $t = $this.index();
            $li.removeClass('current');
            $this.addClass('current');
            $ul.css('display','none');
            $ul.eq($t).css('display','block');
        })
    }
</script>
