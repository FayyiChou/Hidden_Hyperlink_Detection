                var _this = $(".n_msgs");
                navTimer[3] = window.setTimeout(function () {
                    _this.find(".msgs_box").stop(true, true).slideUp(150);
                }, 300);
                navTimer[33] = window.setTimeout(function () {
                    _this.removeClass("h");
                }, 450);
