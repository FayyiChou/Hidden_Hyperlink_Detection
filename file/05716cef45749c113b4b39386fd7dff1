function seckillState(){
var getTimestamp=new Date().getTime();
    var url = "seckill_list.php?act=index_seckill_state&date="+getTimestamp;
    var params = "what=1";
    $.ajax({
    type: "post",
    url: url,
    dataType: "json",
    data: params,
    success: function(msg){
    //$("#huichuan").html(msg.start_buy);
    //$("#state").html(msg.seckill_state);
    //$("#left-goods-num").html(msg.left_num);
    //$("#buy-goods-num").html(msg.buy_num);
    //$("#clock11").html(msg.clock);
    $("#seckill-state").html(msg.state_date);
    //$("#apply_state").html(msg.apply_state);
    }
  });
}
var timer = setInterval("seckillState()",1000);
</script>
