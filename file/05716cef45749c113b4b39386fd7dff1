	  <script>
      		$(window).scroll(function() {
      			var scroll_len = $(window).scrollTop();
      			if (scroll_len > 100) {
      				$('#jump').fadeIn();
      			} else {
      				$('#jump').fadeOut();
      			};
      		});
      		$(document).ready(function() {
                   //如果需要屏幕跳转滚动,将最下面的里的代码拷贝过来
            //计算屏幕的宽度
            if (!isPC()){
                $("#jump").css("left",1100);
            } else{
                var windowWidth = document.documentElement.clientWidth;//$(window).width();
                var whitespace = (windowWidth - 1000)/2;
                var showPlace = 1000 + whitespace + (whitespace-50)/2;
                $("#jump").css("left",showPlace);
                $("#slide_top").hide();
                if(windowWidth < 1200){
                $("#slide_top").hide();
                }else{
                $("#slide_top").show();
                }
            }
            var oTop = document.getElementById('jump');
           	var oTli = oTop.getElementsByTagName('li');
            var windowHeight = document.documentElement.clientHeight;
            var whiteTopspace = (windowHeight - oTli.length*50)/2;
            $("#jump").css("top",whiteTopspace);
              });
          </script>
