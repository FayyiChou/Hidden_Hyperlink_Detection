<script>
$(document).ready(function(e) {
    $("#categorys-2013 .mc ul li").hover(function(){
        $(this).find(".Anav-title").addClass("ailse-thisnav");
        $(this).find(".Anav-title").find("em").animate({margin:'-55px 10px 0 20px'},200);
        $(this).find(".i-mc").show();
        },
        function(){
        $(this).find(".Anav-title").removeClass("ailse-thisnav");
        $(this).find(".Anav-title").find("em").animate({margin:'0 10px 0 20px'},200);
       $(this).find(".i-mc").hide();
            }
        );
});
