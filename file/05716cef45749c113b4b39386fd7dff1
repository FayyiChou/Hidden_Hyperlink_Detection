<script>
function fastGrabState(){
    var getTimestamp=new Date().getTime();
    var url = "seckill_list.php?act=index_fast_grab_state&date="+getTimestamp;
    var params = "goods_id=";
    $.ajax({
    type: "post",
    url: url,
    dataType: "json",
    data: params,
    success: function(msg){
    //$("#huichuan").html(msg.start_buy);
    //$("#state").html(msg.seckill_state);
    //$("#left-goods-num").html(msg.left_num);
    //$("#buy-goods-num").html(msg.buy_num);
    //$("#clock11").html(msg.clock);
    $("#fast-grab-state").html(msg.state_date);
    //$("#apply_state").html(msg.apply_state);
    }
    });
}
setInterval("fastGrabState()",1000);
</script>
