	$(window).scroll(function(){
		if ($(window).scrollTop() > 300){
			$(".backpanel").fadeIn(500);
		}
		else
		{
			$(".backpanel").fadeOut(500);
		}
	});
	$(".backtop").click(function(){
		$('body,html').animate({scrollTop:0},500);
		return false;
	});
})
</script>
