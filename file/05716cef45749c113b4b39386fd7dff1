<script>
function oldWindow(url){
var timestamp = (new Date()).valueOf();
window.location.href=url+"timestamp="+timestamp;
}
function newWindow(url){
var timestamp = (new Date()).valueOf();
window.open(url+"timestamp="+timestamp);
}
</script>
