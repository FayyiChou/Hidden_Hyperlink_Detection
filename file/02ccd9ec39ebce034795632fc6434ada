<SCRIPT LANGUAGE="JavaScript">

function G(id){return document.getElementById(id);};
function Alltrim(s){return s.replace(/(^[s]*)|([s]*$)/g, "")};
function mapSearch(tp){
	var pars = "http://map.baidu.com/?";
	var parStr = "";
	var tmpC = "";
	switch(tp){
		case "poi":
			if(Alltrim(G("txtPoi").value) == ""){
				alert("请您输入查询字符串！");
				G("txtPoi").focus();
				return;
			}
			tmpC = Alltrim(G("txtcity1").value);
			if(tmpC == "城市名") tmpC = "";
			parStr = tmpC + " " + G("txtPoi").value;
			pars += "newmap=1&s="+encodeURIComponent("s&wd="+encodeURIComponent(parStr)+"&c=1");
			break;
		case "bus":
			if(Alltrim(G("txtBusSta").value) == ""){
				alert("请您输入出发地！");
				G("txtBusSta").focus();
				return;
			}
			if(Alltrim(G("txtBusEnd").value) == ""){
				alert("请您输入目的地！");
				G("txtBusEnd").focus();
				return;
			}
			tmpC = Alltrim(G("txtcity2").value);
			if(tmpC == "城市名") tmpC = "";
			parStr = "bt&c=1&sn=2$$$$$$"; 
			parStr += encodeURIComponent(Alltrim(tmpC + " " + G("txtBusSta").value));
			parStr += "$$&en=2$$$$$$";
			parStr += encodeURIComponent(Alltrim(tmpC + " " + G("txtBusEnd").value));
			parStr += "$$";
			pars += "newmap=1&s=" + encodeURIComponent(parStr);
			break;
		case "trave":
			if(Alltrim(G("txtTraveSta").value) == ""){
				alert("请您输入出发地！");
				G("txtTraveSta").focus();
				return;
			}
			if(Alltrim(G("txtTraveEnd").value) == ""){
				alert("请您输入目的地！");
				G("txtTraveEnd").focus();
				return;
			}
			tmpC = Alltrim(G("txtcity4").value);
			if(tmpC == "城市名") tmpC = "";
			parStr = "nav&c=1&sc=1&ec=1&sn=2$$$$$$"; 
			parStr += encodeURIComponent(Alltrim(tmpC + " " + G("txtTraveSta").value));
			parStr += "$$&en=2$$$$$$";
			parStr += encodeURIComponent(Alltrim(tmpC + " " + G("txtTraveEnd").value));
			parStr += "$$";
			pars += "newmap=1&s=" + encodeURIComponent(parStr);
			break;
		case "drive":
			tmpC = Alltrim(G("txtcity5").value);
			if(tmpC == "城市名") tmpC = "";
			if(Alltrim(G("txtDriveSta").value) == "" && tmpC == ""){
				alert("请您输入出发地！");
				G("txtcity5").focus();
				return;
			}
			parStr = "nav&c=1&sc=1&ec=1&sn=2$$$$$$"; 
			parStr += encodeURIComponent(Alltrim(G("txtcity5").value + " " + G("txtDriveSta").value));

			tmpC = Alltrim(G("txtcity6").value);
			if(tmpC == "城市名") tmpC = "";
			if(Alltrim(G("txtDriveEnd").value) == "" && tmpC == ""){
				alert("请您输入目的地！");
				G("txtcity6").focus();
				return;
			}			
			parStr += "$$&en=2$$$$$$";
			parStr += encodeURIComponent(Alltrim(tmpC+ " " + G("txtDriveEnd").value));
			parStr += "$$";
			pars += "newmap=1&s=" + encodeURIComponent(parStr);
			break;
	}
	//pars += "&newmap=1";
	window.open(pars,'_blank') 

}
