<script type="text/javascript">
    /*举报*/
    var report ='';
    GJ.use('js/util/modal/modal.js, js/util/modal/modal.css, js/util/ui_compatible/placeholder.js', function(Modal){

        var userId = "";
        if(userId != '') {
            //do report
            report = new Modal({
                $triggerEl: $('#task_report'),
                title     : '举报',
                content   : $('#report_content'),
                mask      : 'true',
                width     : 710,
                height    : 448
            });
        }

        $('#task_report').live('click', function() {
            if(userId == '') {
                $('.jz_lg').trigger('click');
                return false;
            } else {
                $('.js-modal').addClass('popup-report');

                $('.js-modal-title').text('举报');
                $('.win_reportTips').hide();
                $('.win_report').show();
                $('.jz-textarea').val('');
            }
        });

        $('.win_reportTips_btn').live('click', function(){
            $('.js-modal').removeClass('popup-report');
            $('.js-modal').removeClass('js-show');
            $('.js-modal-mask').removeClass('js-show');
            $('.win_reportTips').hide();
            $('.win_report').hide();
        });

        $('.js-modal-close').live('click', function(){
            $('.js-modal').removeClass('popup-report');
            $('.win_reportTips').css('display', 'none');
            $('.win_report').css('display', 'block');
        });

        //提交
        $('.btn_submit').live('click', function() {
            var container = $(this).parent();

            var typeVal = container.find('input:radio[name="report-type"]:checked').val();
            if (!typeVal) {
                container.find('#report_type_error').css('visibility','visible');
                return false;
            } else {
                container.find('#report_type_error').css('visibility','hidden');
            }

            var conVal = $.trim(container.find('textarea[name="report-con"]').val());
            var reportConPlaceholder = container.find('textarea[name="report-con"]').attr('placeholder');

            conVal = conVal.replace(reportConPlaceholder, '');
            conVal = $.trim(conVal.replace(reportConPlaceholder, ''));

            if(conVal == "") {
                container.find('#report_con_error').css('visibility','visible').text('请填写具体描述');
                return false;
            } else if (conVal.length > 100) {
                container.find('#report_con_error').css('visibility','visible').text('请具体描述字数100字以内');
                return false;
            } else {
                container.find('#report_con_error').css('visibility','hidden');
            }

            var postId = $(this).attr('data-pid');

            $.ajax({
                'type' : 'POST',
                'url'  : '/api/v1/client/report/',
                'data' : {post_id:postId,report_type:typeVal,report_con:conVal},
                'dataType' : 'json',
                'success' : function (json) {
                    var json = $.parseJSON(json[0]);

                    container.parent().find('.win_report').hide();
                    container.parent().parent().css('width', '480');
                    container.parent().parent().css('height', '142');
                    container.parent().css('height', '200');

                    if (json.err_code == 0) {
                        $('.jubao-box').html('<i class="jubao-ico"></i><span>我们已收到您的举报信息，查证后会进行处理。</span>');
                        $('.js-modal-title').text('举报成功');
                        container.parent().find('.win_reportTips').show();
                        container.parent().find('.win_reportTips').find('.reportTips_con').html("您的举报信息已提交成功，<br>我们将尽快处理！");
                    } else if(json.err_code == -8) {
                        $('.jubao-box').html('<i class="jubao-ico"></i><span>我们已收到您的举报信息，查证后会进行处理。</span>');
                        $('.js-modal-title').text('举报失败');
                        container.parent().find('.win_reportTips').show();
                        container.parent().find('.win_reportTips').find('.reportTips_con').html("您的举报次数过多，<br>已经举报过的帖子不能继续举报");
                    } else if(json.err_code == -9) {
                        $('.js-modal-title').text('举报失败');
                        container.parent().find('.win_reportTips').show();
                        container.parent().find('.win_reportTips').find('.reportTips_con').html("您的举报次数过多，<br>已经举报过的帖子不能继续举报");
                    } else {
                        $('.js-modal-title').text('举报失败');
                        container.parent().find('.win_reportTips').show();
                        container.parent().find('.win_reportTips').find('.reportTips_con').text('举报失败');
                    }
                },
                'error' : function (json)
                {
                    var jsonmsg = $.parseJSON(json.responseText);

                    if (-208 == jsonmsg.code) {
                        window.alert(jsonmsg.message);
                        window.location.reload();
                        return false;
                    } 
                    container.parent().find('.win_report').hide();
                    $('.js-modal').css('width', '480');
                    $('.js-modal').css('height', '142');
                    $('.js-modal-content').css('height', '200');

                    $('.js-modal-title').text('举报失败');
                    container.parent().find('.win_reportTips').show();
