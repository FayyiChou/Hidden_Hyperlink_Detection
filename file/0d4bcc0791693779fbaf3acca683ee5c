			function scroll(callback,time){
			        var scrollt1=null,scrollt2=null;
			        $(window).scroll(function(){
				        if(scrollt2==null)
				            scrollt2=setInterval(function(){
				                callback.call();
				            },time||100);
				
				        clearTimeout(scrollt1);
				        scrollt1=setTimeout(function(){
				            clearInterval(scrollt2);
				            scrollt2=null;
				        },time||100);
			        });
