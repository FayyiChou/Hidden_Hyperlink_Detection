<script type="text/javascript">
    var t = n = 0, count;
    $(document).ready(function() {
        count = $("#im1 div").length;
        $("#im1 div:not(:first-child)").hide();
        $("#im2 li").click(function() {
            var i = $("#im2 li").index(this); //获取Li元素内的值，即1，2，3，4

            n = i;
            if (i >= count) return;
            $("#im1 div").filter(":visible").fadeOut(1000).parent().children().eq(i).fadeIn(1000);
            $(this).toggleClass("lh");
            $(this).siblings().removeAttr("class");
        });
        //t = setInterval("showAuto()", 4000);
        setInterval('showAuto()', 10000);
    });

    function showAuto() {
       
        n = n >= (count - 1) ? 0 : ++n;
        $("#im2 li").eq(n).trigger('click');
        //setTimeout('showAuto();', 9000);
    }
</script>

