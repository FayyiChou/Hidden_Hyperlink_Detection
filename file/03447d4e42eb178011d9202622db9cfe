    <script type="text/javascript">
        addLoadEvent(function () {
        	document.getElementById('txtUserId').focus();
			
			if (!$mb.versions.iPad){
				document.getElementById('download').style.display = 'block';
			}
			//定义营销人企业号二维码显示位置
            var aShowHideQrCodeForBGYYXR = document.getElementById("aShowHideQrCodeForBGYYXR");
            var imgQrCodeForBGYYXR = document.getElementById("imgQrCodeForBGYYXR");

            var getCoords = function (el) {
                var box = el.getBoundingClientRect(),
                doc = el.ownerDocument,
                body = doc.body,
                html = doc.documentElement,
                clientTop = html.clientTop || body.clientTop || 0,
                clientLeft = html.clientLeft || body.clientLeft || 0,
                top = box.top + (self.pageYOffset || html.scrollTop || body.scrollTop) - clientTop,
                left = box.left + (self.pageXOffset || html.scrollLeft || body.scrollLeft) - clientLeft
                return { 'top': top, 'left': left };
            };

            var coords = getCoords(aShowHideQrCodeForBGYYXR);
            imgQrCodeForBGYYXR.style.left = (coords.left - 20) + "px";
            imgQrCodeForBGYYXR.style.top = (coords.top - 180) + "px";

            return;
        });
        document.forms[0].onsubmit = function () {
            if (!document.getElementById('txtUserId').value
        || !document.getElementById('txtUserPsw').value) {
                alert("请填写账号和密码！");
                return false;
            }
            return true;
        };
    </script>


