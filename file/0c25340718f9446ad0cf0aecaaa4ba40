function search(e) {
	var keywords = $("#searchInput").val();
	if(!keywords) {
		location.href = "http:\/\/ethanzhu.leanote.com\/search";
	} else {
		var tpl = '<form action="http:\/\/ethanzhu.leanote.com\/search" method="get">';
		tpl += '<input name="keywords" value="' + keywords + '" />';
		tpl += "</form";
		$(tpl).submit();
	}
}
</script> 
