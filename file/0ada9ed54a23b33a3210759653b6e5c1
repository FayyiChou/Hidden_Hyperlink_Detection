            //首页banner自动播放效果
            var i=0;
            function shuntime()
            {
                i++;
                if(i>=5){i=1}
                for (var s=1;s<=4; s++)
                {
                    $("#preview-slide-"+s).css("display","none");
                    $("#preview-slide-"+s).slideUp("slow");
                    $("#preview-thumbnail-im"+s).css("top","0px");
                }
                $("#preview-slide-"+i).slideDown("slow");
                $("#preview-thumbnail-im"+i).css("top","-10px");
            }
            
            $(document).ready(function() {
            var strats=setInterval('shuntime()',5000);
            $("#preview-content-head").hover(function() {
            clearInterval(strats);
            }, function() {
                strats=setInterval('shuntime()',5000);
            });
        });
        </SCRIPT>
