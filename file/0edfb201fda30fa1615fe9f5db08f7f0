<script type="text/javascript">
$.dialog.setConfig({'base':'http://www.huimg.cn/lib/dialog', skin:'xiaobaike'});
$(document).ready(function(){
cas_synlogin('http://passport.baike.com/loginInfo.jsp?requestType=statusAndTime', "http://passport.baike.com/login.do?site_ource=xiaobaike_page&service=http%3A%2F%2Fcaikuai.baike.com%2F%3Fautologin%3Dyes");
