$.getScript("http://www.huimg.cn/xiaobaike/js/prettify.js?20140603",function(){
if(typeof prettyPrint == 'function') {
$('head').append('<link href="http://www.huimg.cn/xiaobaike/css/prettify.css" type="text/css" rel="stylesheet" media="all"/>');
prettyPrint();
}
});
}
});
</script>

