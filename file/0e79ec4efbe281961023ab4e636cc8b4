var speed = 40;
var colee_upC = document.getElementById("colee_upC");
var colee_upB = document.getElementById("colee_upB");
var colee_upA = document.getElementById("colee_upA");
colee_upC.innerHTML = colee_upB.innerHTML;
function Marquee() {
	if (colee_upC.offsetTop - colee_upA.scrollTop <= 0) {
		colee_upA.scrollTop -= colee_upB.offsetHeight;
	} else {
		colee_upA.scrollTop++
	}
}
var MyMar = setInterval(Marquee, speed)
 colee_upA.onmouseover = function() {
	clearInterval(MyMar)
}
colee_upA.onmouseout = function() {
	MyMar = setInterval(Marquee, speed)
}</script></td>
