.backToTop { display: none; width: 50px; height: 50px; line-height: 1.2; color: #fff; font-size: 12px; text-align: center; position: fixed; _position: absolute; right: 10px; bottom: 50px; _bottom: "auto"; cursor: pointer; background-image: url(/Template/Default/Skin/lyzqc_web/img/btn_gototop01.png); background-repeat: no-repeat; background-position: left bottom; text-indent: -9999em; }
.backToTop:hover { background-image: url(/Template/Default/Skin/lyzqc_web/img/btn_gototop01.png); background-repeat: no-repeat; background-position: left top; }
</style>

  <!--返回到顶部 此标签不能放在其它容器内 建议放在body结束之前-->
<script type="text/javascript">
(function() {
    var $backToTopTxt = "返回顶部", $backToTopEle = $('<div class="backToTop"></div>').appendTo($("body"))
        .text($backToTopTxt).attr("title", $backToTopTxt).click(function() {
            $("html, body").animate({ scrollTop: 0 }, 120);
    }), $backToTopFun = function() {
        var st = $(document).scrollTop(), winh = $(window).height();
        (st > 0)? $backToTopEle.show(): $backToTopEle.hide();    
        //IE6下的定位
        if (!window.XMLHttpRequest) {
            $backToTopEle.css("top", st + winh - 166);    
        }
    };
    $(window).bind("scroll", $backToTopFun);
    $(function() { $backToTopFun(); });
})();</script>

