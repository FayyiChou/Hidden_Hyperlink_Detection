<script>
function turnPage(pageNum){
	var keyword = $("#keyword").val();
	window.location.href="/portal/courseNetwork/eryaCourse?page="+pageNum+"&keyword="+keyword;
}

function searchCourse() {
	var keyword = $("#keyword").val();
	var pageNum = "1";
	window.location.href="/portal/courseNetwork/eryaCourse?page="+pageNum+"&iserya=1&keyword="+keyword;
}
</script>


