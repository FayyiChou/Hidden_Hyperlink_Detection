				<script>
				//js获取总条数
                		function skip(){
                    		var category = document.location.href.split("/");
                    		var totalindex = $("#index").html().split("/")[1];
                    		var targetindex = $("#target").val();
							var url="";
                    		for(var i =0;i < category.length;i++)
                    		{
							if(i+1<category.length){
							    if(i>2){
								url=url+"/"+category[i];
									}
								}
                    		}
                    		 var ww = new Number(totalindex);
 							if(targetindex>0 && targetindex<=ww)
                			{    
                				window.location.href = /hbksy/url + "/" + targetindex + ".html";
                				return true;
                			}
                    		alert("页码错误！");
            				$("#target").val("");
                    	}
                		
                	
                    </script>
