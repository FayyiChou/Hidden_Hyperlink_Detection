            <script src="http://webres.61.com/common/js/jquery_171.js"></script>
            <script src="http://webres.61.com/topbar/js/topbarEvents.js"></script>
        <script type="text/javascript">
            var votes;
            var url=window.location.href;//获取完整URL
            var tmp= new Array();//临时变量，保存分割字符串
            tmp=url.split("/");//按照"/"分割
            var pp = tmp[tmp.length-1];//获取最后一部分，即文件名和参数
            tp=pp.split("?");//把参数和文件名分割开
            tmp=tp[0].split(".");//把参数和文件名分割开
            $(function(){
                //用户发表看法
                var $detimg = $("div.cont_det_com_tag"),$detbtns = $("a.cont_det_com_link");
                $.ajax({
                    url:'http://webevent.61.com/?c=hq_dressup&d=getartileId',
                    type:'GET',
                    dataType:'jsonp',
                    jsonp:'callback',
                    data:{'aid':tmp[0],'flag':0},
                    success:function(data)
                    {
                        if(data){
                            updetdate(0,data.agree);
                            updetdate(1,data.nothing);
                            updetdate(2,data.against);
                            $("#agr").html(data.agr);
                            $("#not").html(data.noth);
                            $("#aga").html(data.aga);
                        }
                    }
                });
                $detbtns.each(function(){
                    $(this).bind("click",function(e){
                        e.preventDefault();
                        votes = $(this).attr("data");
                        $.ajax({
                            url:'http://webevent.61.com/?c=hq_dressup&d=getartileId',
                            type:'GET',
                            dataType:'jsonp',
                            jsonp:'callback',
                            data:{'aid':tmp[0],'votes':votes,'flag':1},
                            success:function(data)
                            {
                                if(data){
                                    updetdate(0,data.agree);
                                    updetdate(1,data.nothing);
                                    updetdate(2,data.against);
                                    $("#agr").html(data.agr);
                                    $("#not").html(data.noth);
                                    $("#aga").html(data.aga);
                                }
                            }
                        });
                        $detbtns.unbind().css("cursor","default")
                    })
                })
                function updetdate(index,num){
                    var _thisdetimg = $detimg.eq(index);
                    //   var _thisnumobj = _thisdetimg.find("span.cont_det_com_num");
                    // var _thisnum = parseInt(_thisnumobj.text()) + 1;
                    var _thisdetimgH = Math.round(44 * num);
                    _thisdetimg.animate({
                        height: _thisdetimgH
                    },function(){
                        // _thisnumobj.text(_thisnum);
                    })
                }
            })
        </script>
    

