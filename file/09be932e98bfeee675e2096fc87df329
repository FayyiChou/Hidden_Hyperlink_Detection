function stl_tab_7(tabName, no){
	for ( i = 1; i <= 11; i++){
		var el = jQuery('#页签三_tabContent_' + i);
		var li = $('#页签三_tabHeader_' + i);
		if (i == no){
            try{
			    el.show();
            }catch(e){}
            li.removeClass('x44');
            li.addClass('x33');
		}else{
            try{
			    el.hide();
            }catch(e){}
            li.removeClass('x33');
            li.addClass('x44');
		}
	}
}
</SCRIPT>
