				var $tooltip = $('<div class="club-tip"><div class="club-tip-content"><p>车友会重装上线，赶快加入吧！</p><p><a href="#">我知道了</a></p></div><span class="club-tip-bottom club-tip-arrow"></span></div>');
				$('#carclub').after($tooltip);
				$('a', $tooltip).on('click', function () {
					$tooltip.remove();
					Z.setCookie('CYHTooltip', 1, 999);
					return false;
				});
			}
		})();
	});

