		var $js_dropdown = $('.js_dropdown');
		//当浏览器窗口大小改变时
		function bind() {
			$js_dropdown.mouseenter(function () {
				if ($js_dropdown.find('dl').hasClass('fn-hide')) {
					$js_dropdown.find('dl').removeClass('fn-hide');
					$js_dropdown.css('width', '169px');
				}
			});
			$js_dropdown.mouseleave(function () {
				$js_dropdown.find('dl').addClass('fn-hide');
				$js_dropdown.css('width', '31px');
			});
		}
		function setdrop() {
			if ($(window).width() < 1280) {
				$js_dropdown.addClass('age-item-small');
				$js_dropdown.find('dl').addClass('fn-hide');
				$js_dropdown.css('width', '31px');
				bind();
			} else {
				$js_dropdown.unbind('mouseenter');
				$js_dropdown.unbind('mouseleave');
				$js_dropdown.removeClass('age-item-small');
				$js_dropdown.find('dl').removeClass('fn-hide');
				$js_dropdown.css('width', '')
			}
		}
		$(window).resize(setdrop);
		setdrop();
	</script>
