			autohome.dealerAD = function () {
				if (autohome.clubInfo.IsBrandTypePage) {
					return;
				}
				var provinceid = parseInt(autohome.cookie.get('area'));
				provinceid = isNaN(provinceid) ? 110000 : provinceid;
				if (provinceid == 110199) provinceid = 110000;
				var cityid = parseInt(autohome.cookie.get('area'));
				cityid = isNaN(cityid) ? 110100 : cityid;
				if (cityid == 110199) cityid = 110100;
