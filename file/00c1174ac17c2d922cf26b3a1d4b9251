		$(function() {
			var lastGetCarOwnerCount = $.jStorage.get("GetCarOwnerCount");
			var now = new Date().getTime();
			if (!lastGetCarOwnerCount || !lastGetCarOwnerCount.count || !lastGetCarOwnerCount.timeStamp || lastGetCarOwnerCount.timeStamp + 20 * 1000 < now) {
				$.getJSON("/ajax/GetCarOwnerCount", null, function (result) {
					$("#carOwnerCount").text(result.data || 0);
					$.jStorage.set("GetCarOwnerCount", {timeStamp:now, count:result.data});
				});
			} else {
				$("#carOwnerCount").text(lastGetCarOwnerCount.count);
			}
		});
