	<script type="text/javascript">
	$(function () {
		//iframe高度自适应
		$('iframe.auto-height').on('load', function () {
			$(this).height($(this).contents().find("body").height());
		});
		//tab切换效果
		$("ul.tab>li>a").on("click", function () {
			var $this = $(this);
			if ($this.attr("target") !== "_blank") {
				$this.parents('li').addClass('cur').siblings().removeClass('cur');
			}
			var target = $this.attr('target') || '_self';
			if (target == "_blank" || target == "_parent" || target == "_top" || target == "_self") return true;
			if ($this.attr('docrossdomain') != "1") {
				var href = $this.attr('href');
				href += href.indexOf('?') > 0 ? "&" : "?";
				href += "domain=" + document.domain;
				$this.attr("href", href);
				$this.attr('docrossdomain', "1");
			}
			$('iframe#' + target).attr("src", $this.attr('href'));
			$("#subcontent").remove();
			$this.parents('ul.tab').next('.sort').hide();
			return false;
		});

		(function () {
			var chytip = Z.getCookie('CYHTooltip'); //获取自驾游读图模式提示状态
