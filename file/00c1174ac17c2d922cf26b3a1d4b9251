<script type="text/javascript">
	if (/ipad/.test(navigator.userAgent.toLowerCase())) {
		$(window).on('load', autohome.head.init);
	}

	// GetComplaintsUserCheck(int cType, int bbsid)
	function GetComplaintsUserCheck(ctype, loadUrl, bbsid) {
		if (!autohome.UserValidate()) return false;
		$.get("/ajax/GetComplaintsUserCheck2", { cType: ctype, bbsid: bbsid }, function (data) {
			var result = data;
			if (result == -1) {
				alert("未登录无法投诉！");
			} else if (result == 0) {
				window.location = loadUrl;
			} else {
				alert("您好，您的投诉已受理，" + result + "天内请勿重复投诉！");
			}
		});
	}
