		  document.writeln('<iframe src="'+F_Src+'" style="width:'+F_w+'px;height:'+F_h+'px;bottom:'+F_b+'px; right:'+F_b+'px;position:fixed; Z-INDEX:1;" id="'+Fid+'" frameborder="0" allowTransparency="true"  scrolling="no" name="hot"></iframe>');
