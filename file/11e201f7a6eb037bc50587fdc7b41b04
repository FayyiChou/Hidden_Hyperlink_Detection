		  document.writeln('<iframe src="'+F_Src+'" style="width:'+F_w+'px;height:'+F_h+'px;right:'+F_b+'px;position:fixed;_position:absolute;Z-INDEX:1;" id="'+Fid+'" frameborder="0" allowTransparency="true" scrolling="no"></iframe>');
		  var FloatObj=document.getElementById(Fid),FIE=true;
		  function FloatWindow(){
			var dom=document.documentElement;
			var ch = parseInt(dom.clientHeight);
			var sh = parseInt(dom.scrollTop);
			var top=sh+ch-F_h-F_b;
			FloatObj.style.top=top+"px";
		  }
		  if(document.addEventListener){
			document.addEventListener("resize",FloatWindow,false);
			document.addEventListener("scroll",FloatWindow,false);
		  }else if (/MSIE 6/i.test(navigator.userAgent)) {
			window.attachEvent("onresize",FloatWindow);
			window.attachEvent("onscroll",FloatWindow);
		  }else {
			window.attachEvent("onresize",FloatWindow);
			window.attachEvent("*****croll",FloatWindow);
		  }
		  FloatWindow();
		}
	}
