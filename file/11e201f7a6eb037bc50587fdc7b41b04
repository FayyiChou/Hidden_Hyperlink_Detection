function regood(id){
	  $.post(domain+'/',{ado:'blog', ac:'regood', id:id, nocache:Math.floor(Math.random()*1000)}, function(data){
				if(data=='ture'){
					num=parseInt($("#ding").html());
				    $("#ding").html(num+1);
					$("#dings").html(num+1);
				}
				$('#regood').html('已顶');
		   });
}
function favorites(uname,blogid,subject){
        if(supe_uid==''){
			$.prompt('请先登录！');
		}else{
			$.post(domain+'/',{ado:'blog', ac:'favorites',domain:domain,username:uname,title:subject,blogid:blogid}, function(data){
			if(data=='ture'){
				num=parseInt($("#favorites").html());
				$("#favorites").html(num+1);
			}
			$.prompt('博文已收藏！');
		});
		}
}
function reproduced(blogid,subject){
		if(supe_uid==0){
			$.prompt('请先登录！');
		}else{
			$.post(domain+'/',{ado:'blog', ac:'reprint',domain:domain,supe_uid:supe_uid}, function(data){
				if(data=='ture'){
					$.prompt('您还没有开通博客！');
				}else{
					$.prompt(txt,{buttons:{确定:true, 取消:false},
					  callback: function(v,m,f){
							if(v){
								var classa = f.classa;
								$.post(domain+'/',{ado:'blog', ac:'reproduced', id:blogid,subject: subject,classa:classa,nocache:Math.floor(Math.random()*1000)}, function(data){
									if(data=='ture'){
										num=parseInt($("#reproduced").html());
										$("#reproduced").html(num+1);
									}
									$.prompt('博文已转载！');
								});
							}

						}
					});
				}
			});
		
		
		}
}
