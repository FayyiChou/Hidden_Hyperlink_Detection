$(function(){
    $("#head_search").focus(function(){
        if($(this).val() == '搜索你感兴趣的内容'){
            $(this).val('');
        }
    });
    $("#head_search").blur(function(){
        if($(this).val() == ''){
            $(this).val('搜索你感兴趣的内容');
        }
    });
})
function headsearch(){
    var content = $("#head_search").val();
    if(content == '' || content == '搜索你感兴趣的内容'){
        return false;
    }else{
        var search_type = $("#head_search_type").val();
        if( search_type == 2){
            window.open('http://www.baidu.com/baidu?word='+content+'&ct=2097152&si=chinaiiss.com');
        }else if(search_type == 3){
            window.open('http://search.discuz.qq.com/f/search?q='+content+'&sId=8647558&ts=1370247409&mySign=d7f4c4ad&orderField=default&menu=1&rfh=1&qs=txt.shome.a');
        }else if(search_type == 4){
            window.open('http://www.baidu.com/baidu?word='+content+'&ct=2097152&si=blog.chinaiiss.com');
        }
        return false;
    }
    return false;
}
$(function(){
    var tim=null;
    $("#denglu").mouseover(function(){
        clearTimeout(tim);
        $(this).addClass('on_dl');
        $("#denglubox").show();
    });
    $("#denglu").mouseout(function(){
        tim = setTimeout("$('#denglu').removeClass('on_dl');$('#denglubox').hide();",500);
    });
    $("#denglubox").mouseover(function(){
        clearTimeout(tim);
    });
    $("#denglubox").mouseout(function(){
         tim = setTimeout("$('#denglu').removeClass('on_dl');$('#denglubox').hide();",500);
    });
    $("#username").focus(function(){
        if($(this).val() == '用户名'){
            $(this).val('');
        }
    })
    $("#username").blur(function(){
        if($(this).val() == ''){
            $(this).val('用户名');
        }
    })
    $("#password").focus(function(){
        if($(this).val() == '密码'){
            $(this).val('');
        }
        $(this).attr('type','password');
    })
    $("#password").blur(function(){
        if($(this).val() == ''){
            $(this).val('密码');
            $(this).attr('type','text');
        }
        
    })
    $("#denglubtn").click(function(){
        login('denglubox');
    })
});
</script>
