$(this).slideUp("fast");
});
$(".faceBtn").toggle(function(){
$(".faceBox").show();},
//$(".fastpost .textarea")[0].focus();
function(){
$(".faceBox").hide();}
//$(".fastpost  .textarea")[0].focus();
);
</script> 
