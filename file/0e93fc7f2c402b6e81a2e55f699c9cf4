<script type="text/javascript">
function setTab(name,num,n){
	for(i=1;i<=n;i++){
		var menu=document.getElementById(name+i);
		var con=document.getElementById(name+"_"+"con"+i);
		menu.className=i==num?"on":"";
  		con.style.display=i==num?"block":"none"; 
	}
}
</script>
