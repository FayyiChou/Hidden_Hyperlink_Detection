  <script>
  function formchk()
  {
	 userid = document.getElementById("userid").value;
	 pwd = document.getElementById("pwd").value;
	 validate = document.getElementById("validate").value;
	 userid = userid.replace(/\s/ig,"");
	 pwd = pwd.replace(/\s/ig,"");
	 validate = validate.replace(/\s/ig,"");
	 if(userid=="" || pwd == ""||validate=="")
	 {
		alert("请输入用户名或密码！");
		return false; 
	 }
	 return true;
  }
  
  $ = jQuery;
  function changeAuthCode() {
  	var num = 	new Date().getTime();
  	var rand = Math.round(Math.random() * 10000);
  	num = num + rand;
  	$('#ver_code').css('visibility','visible');
  	if ($("#vdimgck")[0]) {
  		$("#vdimgck")[0].src = "http://www.tanzhou.gov.cn/include/vdimgck.php?tag=" + num;
  	}
  	return false;	
  }
  </script>
