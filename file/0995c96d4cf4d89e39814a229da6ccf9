<script>
	$(function(){
		$('.tab-pannel .tab-pannel-title li a').mouseover(function(){
			$(this).addClass('hover');
			$(this).parent().parent().find('a').removeClass('hover');
			$(this).parent().parent().parent().find('.tab-pannel-connent').hide();
			//$(this).parent().parent().parent().find('.tab-pannel-connent').css('display','none');
			$(this).parent().parent().parent().find('.tab-pannel-connent').eq($('.tab-pannel .tab-pannel-title li a').index(this)).show();
		});
	});
</script>
