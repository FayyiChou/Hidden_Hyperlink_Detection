<script type="text/javascript">
        //pop
        $(".m-pop li").hover(function () {
            $(this).find(".m-pop-tk").removeClass("none");
        }, function () {
            $(this).find(".m-pop-tk").addClass("none");
        });

        $("#go-top").click(function () {
            $('html,body').animate({ 'scrollTop': 0 }, 500);
        });

        $(".m-pop .m-pop-sx").click(function () {
            var $mPop = $(this).parent(".m-pop").find("ul");
            if ($mPop.hasClass("none")) {
                $mPop.removeClass("none");
                $(this).html('-');
            } else {
                $mPop.addClass("none");
                $(this).html('+');
            }
        });

        // 导航吸顶
        $(window).scroll(function () {
            if ($(window).scrollTop() >= $(".g-s").outerHeight()) {
                $(".sub-nav").addClass("fixed-top");
                $(".main").addClass("pt80");
            } else {
                $(".sub-nav").removeClass("fixed-top");
                $(".main").removeClass("pt80");
            };
        });
        
        // 服务地区 serve-diqu-btn
        $(".serve-diqu-btn").click(function () {
            $("#serve-diqu").dialog("open");
        });
        $("#serve-diqu .i-me9").click(function () {
            $("#serve-diqu").dialog("close");
        });
        var none = '1' - 0;
        //‘咨询我’ 按钮
        var $bg = $('body>div.bg-black');
        var $telPn = $('div.consult-pop');
        $('div.consult-pop>a.i-me9').on('click', function() {
            $bg.hide();
            $(this).parent().hide();
        });

        $('p>a.e-btn-big').on('click', function() {
            if (none != '1') {
                $.alert('notice', '系统提示', '该律师不接受异地电话咨询', 3000);
                return;
            }
            $telPn.show();
            $bg.removeClass('none').show();
        });

        $('div.consult-pop>div>a.ls-tel').on('click', function() {
            $(this).children('span').hide();
            $(this).children('em').show();
        });

        //律师名片电话提示
        $(".zx-tips .i-per9").hover(function() {
            $(".zx-tips-pop").removeClass("none");
        }, function() { $(".zx-tips-pop").addClass("none"); });
        
    </script>
    
