    <script type="text/javascript">
        $('.praise').on('getstate', function(e, num) {
            $('#pnum').text(num);
        });
        $('.praise').on('praisechange', function(e, isParised) {
            var pnum = parseInt($.trim($('#pnum').text()));

            if (isParised) {
                $('#pnum').text(pnum + 1);
            } else {
                $('#pnum').text(pnum - 1);
            }
        });

        $(function() {
            var timer;
            $('.btn-share').hover(function() {
                clearTimeout(timer);

                if ($("#ctn-piaofu #bdshare_s").length < 1) {
                    $("#bdshare_s").appendTo("#ctn-piaofu");
                    $("#bdshare_l,ul.zxkf-tc").hover(function() {
                        clearTimeout(timer);
                    }, function() {
                        $(this).hide();
                    });
                }
                $("#bdshare_l").show().css({ "right": "100px", "top": "-272px" });
            }, function() {
                var that = this;
                timer = setTimeout(function() {
                    $("#bdshare_l").hide();
                }, 100);
            });
        });
    </script>



