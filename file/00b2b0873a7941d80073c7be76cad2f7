$(document).ready(function(){
	if($(".pages a").length){
		$('.m_page').css('display', 'none');
	}
	
	$(".mo").click(function(){
		if($(this).find("i").hasClass("more")){
			$(".lm_com").find("li:nth-child(n+13)").fadeIn();
			$(this).find("i").removeClass("more").addClass("less");
			$(this).find("a").removeClass("mo_wz").addClass("mo_wz1").text("收起");
		}else{
			$(".lm_com").find("li:nth-child(n+13)").hide();
			$(this).find("i").removeClass("less").addClass("more");
			$(this).find("a").removeClass("mo_wz1").addClass("mo_wz").text("查看更多");
		}
	});
	
	var oDiv = document.getElementsByClassName("artCont")[0],
		str = oDiv.innerHTML;
	oDiv.innerHTML = str.substr(0,1500) + "...";
	var oBtn = document.getElementsByClassName("readMore")[0],
		oA = oBtn.getElementsByTagName("a")[0],
		oI = oBtn.getElementsByTagName("i")[0],
		onoff = true;
	oBtn.onclick = function(){
		oDiv.innerHTML = str;
		$(".readMore").find("i").removeClass("readmo").addClass("remo");
		$(this).remove();
	}
});
