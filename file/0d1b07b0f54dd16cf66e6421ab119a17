	((function($){var ad=$(".adv[data-ad-pos='fixed']"),top=0,css={},scroll_top=0;css.marginTop=parseInt(ad.css("margin-top"));css.marginBottom=parseInt(ad.css("margin-bottom"));function fixedNav(){top=ad.offset().top-css.marginTop-css.marginBottom;$(window).scroll(function(){scroll_top=$("html").scrollTop()||$("body").scrollTop();if(scroll_top>=top){ad.css({position:"fixed",width:300,top:69});}else{ad.css({position:"static"});}});}
	$(window).load(function(){fixedNav();});})(jQuery));
</script>	</div>
	<ul class="clearfloat"></ul>
</div>
