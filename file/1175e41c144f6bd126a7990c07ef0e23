<script language="javascript" type="text/javascript">
    function DefaultButton(event, target) {
        if (event.keyCode == 13 && !(event.srcElement && (event.srcElement.tagName.toLowerCase() == "textarea"))) {
            var defaultButton = $(target);
            if (defaultButton && typeof (defaultButton.click) != "undefined") {
                defaultButton.click();
                event.cancelBubble = true;
                if (event.stopPropagation)
                    event.stopPropagation();
                return false;
            }
        }
        return true;
    }
    function OnSearchCheckAndSubmit() {
        var keyword = document.getElementById("keyword").value;
       // var reg = /^(\w|[\u4E00-\u9FA5])*$/;
        //if (!reg.test(keyword)) {
       //     document.getElementById("keyword").value = "";
        //    alert("只能输入英文数字汉字！注意不留空格");
        //    return false;
      //}
        if (keyword == '' || keyword == null) {
            alert("请填写您想搜索的关键词");
            return;
        }
        else {
            var fieldOptionSel = document.getElementById("fieldOption");
            var fieldOption = fieldOptionSel.options[fieldOptionSel.options.selectedIndex].value;
            if (fieldOption == "content") {
                window.open("http://www.cntaijiquan.com/search/s.aspx?c=&f=0&o=1&wd=" + escape(keyword) + "");
            }
            else if (fieldOption == "title") {
                var x = new AjaxRequest('XML', '');
                x.para = ['keyword=' + keyword];
                x.post('addkeyword', '/Ajax.aspx?HandlerName=BlogAjaxHandler', function (s) {
                });
                window.open("http://so.cntaijiquan.com/cse/search?q=" + keyword + "&s=12525413316480353993");
            }
            else {
                window.open("/search.aspx?searchtype=1&ModelId=1&Keyword=" + escape(keyword) + "&fieldOption=title");
            }
        }
    }


           
</script>
<script type="text/javascript" language="JavaScript" charset="UTF-8">
    document.onkeydown = function (event) {
        var e = event || window.event || arguments.callee.caller.arguments[0];
        if (e && e.keyCode == 13) { // enter 键
            OnSearchCheckAndSubmit();
        }
    }; 
