    document.getElementById("LoginStatus").innerHTML = document.getElementById("LoginStatus").innerHTML + LoginString;
}
function CheckIsLogin(){
    document.getElementById("UserStatus").innerHTML = "<img src=\"/Template/Info2014/skin/Images/loading.gif\" alt=\"\"/>";
    var x = new AjaxRequest('XML', 'status');
    x.para = [];
    x.post('logincheck', '/ajax.aspx', function(s){
        var xml = x.createXmlDom(s);
        var status = xml.getElementsByTagName("status")[0].firstChild.data;
        
        if (status == 'ok') {
            document.getElementById("UserStatus").style.display = "";
            document.getElementById("LoginStatus").style.display = "none";
            document.getElementById("UserStatus").innerHTML = GetUserInfo(xml);
        }
        else {
            document.getElementById("UserStatus").style.display = "none";
            document.getElementById("LoginStatus").style.display = "";
             }
    });
}

function loginout(){
    var x = new AjaxRequest('XML','');
    x.post('userlogout', '/ajax.aspx', function(s) {
    var xml = x.createXmlDom(s);
        var status = xml.getElementsByTagName("status")[0].firstChild.data;
        if (status == "ok") {
                if (xml.getElementsByTagName("API_Enable")[0].firstChild.data == "1") {
                    var LogOutString = xml.getElementsByTagName("LogOutString")[0].firstChild.data;
                    Loginin(LogOutString);
                }
         }
        document.getElementById("UserStatus").style.display = "none";
        document.getElementById("LoginStatus").style.display = "";
    });
}

function GetUserInfo(xml){
    var userInfo = "<span class=\"userinfo\">您好" + xml.getElementsByTagName("username")[0].firstChild.data + "，";
    userInfo = userInfo + "<a href=\"/User/Default.aspx\" linkOpenType=\"_top\">会员中心</a>&nbsp;|&nbsp;";
    userInfo = userInfo + "<a href=\"#\"  onclick=\"loginout()\" linkOpenType=\"_top\">退出登陆</a>";
    userInfo = userInfo + "</span>";
    return userInfo;
}
