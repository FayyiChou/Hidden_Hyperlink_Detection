    <script language="javascript" type="text/javascript">
        function AddFav() {
            var title = '';
            var url = window.location;
            showModalDialog("../my/AddFavorite.aspx?title=" + title + "&url=" + url, "", "");
        }
        function WinOpen(width, height) {
            var left = (screen.availWidth - width) / 2;
            var top = (screen.availHeight - height) / 2;
            var features = 'scrollbars=yes,width=' + width + ',height=' + height + ',left=' + left + ',top=' + top;
            var title = '';
            var url = window.location;
            window.open("../my/AddFavorite.aspx?title=" + title + "&url=" + url, 'DoteyMsg', features);
        } 
    </script>

