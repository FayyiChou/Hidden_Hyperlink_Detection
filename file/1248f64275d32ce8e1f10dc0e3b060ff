		    btnPAPlayStop_d2_1479_2a267_PSlider_.onclick=parPlaySlide_d2_1479_2a267_PSlider_;
        }
        
        function parNextSlide_d2_1479_2a267_PSlider_() //NEXT
        {
           
		    parSlideIndex_d2_1479_2a267_PSlider_%=5;
		    parViewSlide_d2_1479_2a267_PSlider_();
        }
        
        function parPrevSlide_d2_1479_2a267_PSlider_() //向前
        {
		    parSlideIndex_d2_1479_2a267_PSlider_-=2;
