        var parSliderTimeId_d2_1479_2a267_PSlider_;
        var sliderMode_d2_1479_2a267_PSlider_=5;
        var parSlideDelay_d2_1479_2a267_PSlider_=4;
        function parViewSlide_d2_1479_2a267_PSlider_() //初始化
        {
		    parSlideCount_d2_1479_2a267_PSlider_ = 0;
		    try
		    {
			    pa_d2_1479_2a267_PSlider_.filters[0].Stop();
                //如果当前动画为随机则触发随机切换的效果
                if(sliderMode_d2_1479_2a267_PSlider_==10)
                {
                   pa_d2_1479_2a267_PSlider_.style.filter=changeFilter();
                }           
