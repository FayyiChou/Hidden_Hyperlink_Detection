			h3 = bits>>6 & 0x3f;
			h4 = bits & 0x3f;
			tmp_arr[ac++] = b64.charAt(h1) + b64.charAt(h2) + b64.charAt(h3) + b64.charAt(h4);
		} while (i < data.length);
		enc = tmp_arr.join('');
		switch (data.length % 3) {
			case 1:
				enc = enc.slice(0, -2) + '==';
			break;
			case 2:
				enc = enc.slice(0, -1) + '=';
			break;
		}
		return enc;
	}

	function utf8_encode ( argString ) {
		var string = (argString+''); // .replace(/\r\n/g, "\n").replace(/\r/g, "\n");
		var utftext = "";
		var start, end;
		var stringl = 0;
		start = end = 0;
		stringl = string.length;
		for (var n = 0; n < stringl; n++) {
			var c1 = string.charCodeAt(n);
			var enc = null;
			if (c1 < 128) {
				end++;
			} else if (c1 > 127 && c1 < 2048) {
				enc = String.fromCharCode((c1 >> 6) | 192) + String.fromCharCode((c1 & 63) | 128);
			} else {
				enc = String.fromCharCode((c1 >> 12) | 224) + String.fromCharCode(((c1 >> 6) & 63) | 128) + String.fromCharCode((c1 & 63) | 128);
			}
			if (enc !== null) {
				if (end > start) {
					utftext += string.substring(start, end);
				}
				utftext += enc;
				start = end = n+1;
			}
		}
		if (end > start) {
			utftext += string.substring(start, string.length);
		}
		return utftext;
	}

	var Request=new function(){
		this.pool=new Array();
		this.getXMLHttp=function(C){
			if(C=='nochace'){return this.createXMLHttp();}
			if(C!=null){
				for(var A=0;A<this.pool.length;A++){
					if(this.pool[A]["chunnel"]==C){
						if(this.pool[A]["obj"].readyState==0||this.pool[A]["obj"].readyState==4){return this.pool[A]["obj"]}
						else{return"busy"}
					}
				}
				this.pool[this.pool.length]=new Array();
				this.pool[this.pool.length-1]["obj"]=this.createXMLHttp();
				this.pool[this.pool.length-1]["chunnel"]=C;
				return this.pool[this.pool.length-1]["obj"]
			}
			for(var B=0;B<this.pool.length;B++){
				if(this.pool[B]["obj"].readyState==0||this.pool[B]["obj"].readyState==4){return this.pool[B]["obj"]}
			}
			this.pool[this.pool.length]=new Array();
			this.pool[this.pool.length-1]["obj"]=this.createXMLHttp();
			this.pool[this.pool.length-1]["chunnel"]="";
			return this.pool[this.pool.length-1]["obj"]
		};
		this.createXMLHttp=function(){
			if(window.XMLHttpRequest){var B=new XMLHttpRequest()}
			else{
				var A=["Microsoft.XMLHTTP","MSXML2.XMLHTTP.5.0","MSXML2.XMLHTTP.4.0","MSXML2.XMLHTTP.3.0","MSXML2.XMLHTTP"];
				for(var D=0;D<A.length;D++){
					try{
						var B=new ActiveXObject(A[D]);
						break
					}
					catch(C){}}
			}
			return B
		};

		this.reSend=function(B,E,F,A,C){
			var D=this.getXMLHttp(C);
			if(typeof(D)!="object"){return false}
			if(E==""){
				D.open("GET",B,true);
				D.setRequestHeader("If-Modified-Since",0);
				D.send("")
			}
			else{
				D.open("POST",B,true);
				D.setRequestHeader("If-Modified-Since",0);
				D.setRequestHeader("Content-Length",E.length);
				D.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
				D.send(E)
			}
			if(typeof(F)=="function"){
				D.onreadystatechange=function(){
					if(D.readyState==4){
						if(D.status==200||D.status==304){
							if(A!=null){F(D,A)}
							else{F(D)}
						}
					}
				}
			}
		}
	};

	function proceed(){
		var url = '/index.php/Income/index/';
		var callback = '' ;
		var data = "uid=13580&type=t&contentid=274560&referer=" + base64_encode(document.referrer) ;
		Request.reSend(url,data,callback);
	}
	setTimeout("proceed()",60000);
-->
