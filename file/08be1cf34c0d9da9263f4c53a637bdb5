            var system_href = { win: false, mac: false, xll: false };

            //检测平台      

            var p_href = navigator.platform;

            system_href.win = p_href.indexOf("Win") == 0;

            system_href.mac = p_href.indexOf("Mac") == 0;

            system_href.xll = (p_href == "Xll") || (p_href.indexOf("Linux") == 0);

            //跳转语句      

            if (system_href.win || system_href.mac || system_href.xll) {

