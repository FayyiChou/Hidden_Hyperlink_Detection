<!-- jquery版本jquery-2.1.3.min.js
swiper版本swiper.jquery.min.js -->
<script type="text/javascript" src="//magicube.pconline.com.cn/enroll/_js_enroll_simple.jsp?enrollId=2403&amp;diy=1"></script>
<script type="text/javascript">
$(document).ready(function() {
	var mySwiper = new Swiper('.swiper-box1', {
		loop:true,
		prevButton:'.l-but1',
		nextButton:'.r-but1',
	});
	
	var mySwiper2 = new Swiper('.swiper-box2', {
		pagination : '.swiper-pagination',
		paginationElement : 'li',
		paginationClickable :true,
		onSlideChangeEnd: function(swiper){
   		$('.swiper-box2 .swiper-wrapper .swiper-slide').eq(swiper.previousIndex).trigger('click');
    }
	});

	var see_more=$('.more-see');
	var close_but=$('.close-but');
	var cover=$('.cover-box');
	var text_win=$('.text-window');
	close_but.on('click',  function(event) {
		event.preventDefault();
		cover.css('display','none');
		text_win.css('display','none');
	});
	see_more.on('click',  function(event) {
		event.preventDefault();
		cover.css('display','block');
		text_win.css('display','block');
	});

	var crow_but=$('.crow');
	crow_but.on('click', function(event) {
		event.preventDefault();
		$(this).siblings('.sn').trigger('click');
	});


	var mySwiper2 = new Swiper('#swiper-container2',{
			watchSlidesProgress : true,
			watchSlidesVisibility : true,
			prevButton:'.l-but2',
			nextButton:'.r-but2',
			slidesPerView : 2,
			spaceBetween : 5,
			freeMode : true,
			freeModeMomentumRatio : 3,
			onTap: function(){
				mySwiper3.slideTo( mySwiper2.clickedIndex)
				$('.now_first').removeClass('now_first')
			}
		})
	var mySwiper3 = new Swiper('#swiper-container3',{
		slidesOffsetBefore : 1,
	onSlideChangeStart: function(){
			updateNavPosition();
			$('.now_first').removeClass('now_first');
			}

		})

	function updateNavPosition(){
		$('#swiper-container2 .active-nav').removeClass('active-nav')
		var activeNav = $('#swiper-container2 .swiper-slide').eq(mySwiper3.activeIndex).addClass('active-nav');

		if (!activeNav.hasClass('swiper-slide-visible')) {

			if (activeNav.index()>mySwiper2.activeIndex) {

				var thumbsPerNav = Math.floor(mySwiper2.width/activeNav.width())-1
				mySwiper2.slideTo(activeNav.index()-thumbsPerNav)
			}
			else {

				mySwiper2.slideTo(activeNav.index())
			}	
		}
	}
	
	$('#enroll_memo1').change(function(event) {
		$('#enroll_memo3').find('option').remove();
		$('#enroll_memo3').append('<option>请选择</option>')
	});
});

$(".form-but").click(function(){
	//前台校验
	var name=$("#enroll_name").val();
	var phone = $("#enroll_phone").val();
	var province=$("#enroll_memo1").val();
	var city=$("#enroll_memo2").val();
	var dealer=$("#enroll_memo3").val();
	
	var Reg = /^[1][34578][0-9]{9}$/;
	if(!/^[\u4E00-\u9FA5]{1,}$/.test(name)){
		alert("请输入正确的中文名");
		return;
	}else if(!Reg.test(phone)){
		alert("请输入正确的电话号码");
		return;
	}else if(province==""){
		alert("请选择省份");
		return;
	}else if(city==""){
		alert("请选择城市");
		return;
	}else if(dealer==""){
		alert("请选择经销商");
		return;
	}
	
 	CubeEnroll.enroll2(2403,function(data){
		alert(data.msg);
	})
})



</script>
</body></html>