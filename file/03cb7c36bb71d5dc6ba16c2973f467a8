  <script type="text/javascript">
    // 百度地图API功能
    /*$('#classaddr').click(function(){
        var school_map = '';
        initMap('map', school_map);
    });*/

    $('input[name=gender]').change(function () {
        var start_place = $('#start_place').val();
        var end_place = $('#end_place').val();
        if (this.value == 2) {
            searchDrivingRoute(start_place, end_place);
        } else {
            searchTransitRoute(start_place, end_place);
        }
    });
    //查询路线
    function school_mapsearch() {
        var mapinfo = '{"start_place":"' + $('#start_place').val() + '","end_place":"' + $('#end_place').val() + '","type":"' + $('input[name=gender]:checked').val() + '"}';
        if ($('#start_place').val() != '' && $('#end_place').val() != '') {
            mapsearch(mapinfo);
        }
    }
</script> 
  <script>
    $(function (){
        var nav_tt = $(".co-nav").offset().top;
        console.log(nav_tt);
        $(window).scroll(function(){
                if($(window).scrollTop()>562) {
                    $(".co-nav").addClass("fixed");
                    $(".left-content-box").css("margin-top","60px");
                    $(".con-btn-act").show();
                }else{
                    $(".co-nav").removeClass("fixed");
                    $(".left-content-box").css("margin-top","0");
                    $(".con-btn-act").hide();
                }
            })
    })



    //导航条选项卡
    detail_tab('co-nav-each','detail-box','co-nav-active',584);
    //其他课程选项卡
    tab('other-tab-li','other-cou-ul','other-tab-li-active');
    //报名
    function now_sign(){
        $("#now-sign").hide().siblings('.con-btn-qr').show().css({
            'display':'inline-block'
        });
        $("#now-sign").closest('.sign-wrap').find('.sign-box').show();
    }
    $('#now-sign').click(function (){
        now_sign();
    });
    $("#now-sign-scroll").click(function(){
        $(window).scrollTop(0);
        now_sign();
    });

    $('.sign-box-close').click(function (){
        $(this).closest('.sign-box').hide();
        $(this).closest('.sign-wrap').find('.con-btn-sign').show();
        $(this).closest('.sign-wrap').find('.con-btn-qr').hide();
    });
    //二维码显示隐藏
    $('.co-code').mouseenter(function (){
        $(this).find('.code-show').show();
    }).mouseleave(function (){
        $(this).find('.code-show').hide();
    })
    //标签显示
    $('.common-label').mouseenter(function (){
        $(this).find('.sch-label-intro').show();
    }).mouseleave(function (){
        $(this).find('.sch-label-intro').hide();
    })

    $('#bdjr-check1').click(function(){
        if($(this).attr('data-checked')=='false'){
            $('#bdjr1').val(1);
            $(this).attr('data-checked','true');
            $(this).css('background-position','-23px 0');
        }else{
            $('#bdjr1').val(0);
            $(this).attr('data-checked','false');
            $(this).css('background-position','0 0');
        }
    })

</script> 
