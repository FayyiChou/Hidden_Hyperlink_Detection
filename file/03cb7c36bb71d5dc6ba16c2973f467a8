      <script type="text/javascript">
    $('#bdjr-check').click(function(){
        if($(this).attr('data-checked')=='false'){
            $('#bdjr').val(1);
            $(this).attr('data-checked','true');
            $(this).css('background-position','-23px 0');
        }else{
            $('#bdjr').val(0);
            $(this).attr('data-checked','false');
            $(this).css('background-position','0 0');
        }
    });
    $(".warm-tip-close").click(function(){
        $("#wrong-tip").html("");
        $(".sign-mask").hide();
        $(".warm-tip").hide();
    });
    $(".sign-mask").click(function(){
        $("#wrong-tip").html("");
        $(".sign-mask").hide();
        $(".warm-tip").hide();
    })

    $(".signup_div").find("input[type=text]").focus(function () {
        $(this).closest(".signup_form").children(".signup_div").css("border", "1px solid #e8e8e8");
        $(this).parent().css("border", "1px solid #ff5400");
    });
    $(".signup_div").find("textarea").focus(function () {
        $(this).closest(".signup_form").children(".signup_div").css("border", "1px solid #e8e8e8");
        $(this).parent().css("border", "1px solid #ff5400");
    });
</script> 
