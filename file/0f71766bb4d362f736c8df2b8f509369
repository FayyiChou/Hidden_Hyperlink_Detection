<script type="text/javascript">
function setTab(m,n){
 var tli=document.getElementById("menu"+m).getElementsByTagName("li");
 var mli=document.getElementById("main"+m).getElementsByTagName("li");
 for(i=0;i<tli.length;i++){
  tli[i].className=i==n?"hover":"";
  mli[i].style.display=i==n?"block":"none";
 }
}
//切换选项卡
//a当选ID 从0开始
//b总共多少个TAB
//titid TAB标题
//txtid TAB内容
function ShowTabs(a,b,titid,txtid) {
		var i;
		//alert(document.getElementById(titid + i).className);
        for (i = 0; i < b; i++) {
            if (i == a) {
               	document.getElementById(titid + i).className = "act";
				document.getElementById(txtid + i).style.display = "";
            }
            else {
               	document.getElementById(titid + i).className = "";
				document.getElementById(txtid + i).style.display = "none";
            }
        }
    }
    //]]>
</script>
