<!--浮动窗体开始-->
<!--对联广告的js代码-->
<script src="js/jquery.min.js" type="text/javascript"></script>
<script type="text/javascript">
var jq =jQuery.noConflict();

    var x = 50, y = 60
    var xin = true, yin = true
    var step = 1
    var delay = 10
    var obj;
    var itl;
    function float() {
        var L = T = 0
        var R = jq(window).width() - obj.width();
        var B = jq(window).height() - obj.height();
        obj.css("left", x + jq(window).scrollLeft());
        obj.css("top", y + jq(window).scrollTop());
        x = x + step * (xin ? 1 : -1);
        if (x < L) { xin = true; x = L }
        if (x > R) { xin = false; x = R }
        y = y + step * (yin ? 1 : -1);
        if (y < T) { yin = true; y = T }
        if (y > B) { yin = false; y = B }
    }
    window.onload = function () {
        obj = jq("#codefans_net"); 
        itl = setInterval("float()", delay);
        obj.mouseover(function () { clearInterval(itl); });
        obj.mouseout(function () { itl = setInterval("float()", delay); });
        jq("#pclose").click(function () { clearInterval(itl); obj.remove() });
    };
</script>
<!--对联广告的js代码结束-->

<!--对联广告开始-->
