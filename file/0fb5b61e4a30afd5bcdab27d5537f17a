				step = 705;
				$("#onTimeBoxLeftBtn").click(function(){
					this_step = parseInt($("#onTimeContainer #bar").css("left"))+step ;
					if(this_step>0){
						this_step = 0;
					}
					else{
					}
					$("#onTimeContainer #bar").animate({"left":(this_step)+"px"})
				})
				$("#onTimeBoxRightBtn").click(function(){
					this_step =parseInt($("#onTimeContainer #bar").css("left"))-step;
					var cp = $(".item:last").offset().left-$("#onTimeContainer").offset().left;
