<script>
//delay延迟时间（毫秒）
$(function() {
	$("input[name=q]").autocomplete({
		 source: "/search/v_ajax_list.jspx",
		 minLength: 2,
		 max:5,
		 delay:400,
		 autoFocus: true,
		 select: function(e, ui) {
			 $("input[name='q']").val(ui.item.value);
			 $("#searchForm").submit();
		 }
	});
});
function qkeypress(){
	var q=$("input[name=q]");
	if(q.val().trim()!=""){
		$("input[name=q]").autocomplete("enable");
	}else{
		$("input[name=q]").autocomplete("disable");
	}
}
function search(){
	var q= $("input[name='q']").val();
	if(q=="请输入关键字"){
		return alert("请输入关键字");
	}else{
		document.getElementById("searchForm").submit();
	}
}
function OpenLayer(){
	layer.open({
	    type : 1,
	    title : false,
	    border : false,
	    area : ['340px','340px'],
	    content: '<div style="height:300px;margin-top:30px;text-align:center"><img alt="" src="/fzmh/r/cms/www/default/image/wsbs/indexweixin.jpg"></div>', //捕获的元素
	});;
}
</script>
