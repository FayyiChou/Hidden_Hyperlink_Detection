<script type="text/javascript">
var myTimer = setInterval("Carousel()", 3000);  //专题焦点图片轮播
$(function() {
	Cms.siteFlow("", location.href, document.referrer);
	$.cookie("_site_id_cookie","1",{path: '' });
});

function MenuSwitch(titleClass,btnClass,curClass,textClass,tar,ClassName){
	$("."+ClassName+" ."+titleClass+" ."+btnClass).removeClass(curClass);
	$(tar).addClass(curClass);
	var divArr=$("."+ClassName+" ."+titleClass+" div");
	var textArr=$("."+ClassName+" ."+textClass);
	var m=0;
	for(var i=0;i<divArr.length;i++)
	{
		if($(divArr[i]).hasClass(curClass)){
			m=i;
		}
	}
	 $("."+ClassName+" ."+textClass).css("display","none");
	$(textArr[m]).css("display","block"); 
}

function MenuSwitch2(titleClass,btnClass,curClass,textClass,tar,ClassName){
	$("."+ClassName+" ."+titleClass+" ."+btnClass).removeClass(curClass);
	$(tar).addClass(curClass);
	var Arr = $("."+ClassName+" ."+titleClass+" ."+btnClass);
	var divImgTemp="";
	for(var m=0;m<Arr.length;m++){
		divImgTemp="";
		divImgTemp = $(Arr[m]).find("img").attr("src");
		divImgTemp=divImgTemp.replace("2.png","1.png");
		$(Arr[m]).find("img").attr("src",divImgTemp);
	}
	var imgSrc=$(tar).find("img").attr("src");	
	imgSrc=imgSrc.replace("1.png","2.png");
	$(tar).find("img").attr("src",imgSrc);
	var divArr=$("."+ClassName+" ."+titleClass+" div");
	var textArr=$("."+ClassName+" ."+textClass);
	var m=0;
	for(var i=0;i<divArr.length;i++)
	{
		if($(divArr[i]).hasClass(curClass)){
			m=i;
		}
	}
	 $("."+ClassName+" ."+textClass).css("display","none");
	$(textArr[m]).css("display","block"); 
}
	function turn_right(){
		if(!$(".fl-pic li:first").is(":animated")){ 
			 var moveLiWidth =  $(".fl-pic li:first img").css("width");
			 var nextImg=$(".tp_index img.cur").next();
			 var nextClass=nextImg.attr("class");
			 if (typeof(nextImg.attr("class")) == "undefined") { 
				 nextClass="a";
				}
			 $(".fl-pic li:first").after($(".fl-pic li."+nextClass));
			 var moveEle=$(".fl-pic li:first");
					$(".fl-pic li:first").animate({marginLeft:"-"+moveLiWidth},"slow",function(){ 
					$(".fl-pic li:last").after(moveEle);
					$(".fl-pic li:last").css("margin","0px");
				});
				var imgArr =$(".tp_index img");
				for(var m=0;m<=imgArr.size();m++){
					$(imgArr[m]).removeClass("cur");
					$(imgArr[m]).attr("src","/fzmh/r/cms/www/default/image/"+$(imgArr[m]).attr("class")+".png");
				}
				var  className=	$(".fl-pic li:first + li").attr("class");
				$(".tp_index ."+className).attr("src","/fzmh/r/cms/www/default/image/"+className+"_current.png");
				$(".tp_index ."+className).addClass("cur");
		} 
	}


function Carousel(){
	if(!$("#ztjd").hasClass("hold"))
	{
		turn_right();
	}	
}
function holdOn(){
	$("#ztjd").addClass("hold");
}
function relief(){
	$("#ztjd").removeClass("hold");
	clearInterval(myTimer);
	myTimer = setInterval("Carousel()", 3000); 
}
function turnCurrent(tar){
	var tpArr = $(".tp_index img");
	var position="";
	var className="";
	var flag=false;
	for(var i=0;i<tpArr.length;i++){
		if(tpArr[i]==tar){
			flag=true;
		}
		className=$(tpArr[i]).attr("class");
		if(className.indexOf("cur")>0){
			if(flag){
				position="pre";
			}else{
				position="after";
			}
		}
	}
	className=$(tar).attr("class");
	if(position=="pre"){
		if(!$(".fl-pic li:first").is(":animated")){ 
		var LiArr =	$(".fl-pic li[class='"+className+"']");
		var LiWidth=0;
		for(var m=0;m<LiArr.length;m++){
			LiWidth =LiWidth + parseInt($(LiArr[m]).css("width"));
		}
		$(".fl-pic li:first").before(LiArr);
		$(".fl-pic li:first").css("margin-left","-"+LiWidth +"px");
		$(".fl-pic li:first").animate({marginLeft:'0px'},"slow");
		var imgArr =$(".tp_index img");
		for(var m=0;m<=imgArr.size();m++){
			$(imgArr[m]).removeClass("cur");
			$(imgArr[m]).attr("src","/fzmh/r/cms/www/default/image/"+$(imgArr[m]).attr("class")+".png");
		}
		var  classNametemp=	$(".fl-pic li:first").attr("class");
		$(".tp_index ."+classNametemp).attr("src","/fzmh/r/cms/www/default/image/"+classNametemp+"_current.png");
		$(".tp_index ."+classNametemp).addClass("cur");
		}
	}else{
		if(!$(".fl-pic li:first").is(":animated")){ 
			var LiArr =	$(".fl-pic li[class='"+className+"']");
			var moveLiWidth=0;
			for(var m=0;m<LiArr.length;m++){
				moveLiWidth =moveLiWidth + parseInt($(LiArr[m]).css("width"));
			}
					$(".fl-pic li:first").after(LiArr);
					$(".fl-pic li:first").animate({marginLeft:"-"+moveLiWidth+"px"},"slow",function(){ 
					$(".fl-pic li:last").after($(".fl-pic li:first"));
					$(".fl-pic li:last").css("margin","0px");
				});
				var imgArr =$(".tp_index img");
				for(var m=0;m<=imgArr.size();m++){
					$(imgArr[m]).removeClass("cur");
					$(imgArr[m]).attr("src","/fzmh/r/cms/www/default/image/"+$(imgArr[m]).attr("class")+".png");
				}
				var  className=	$(".fl-pic li:first + li").attr("class");
				$(".tp_index ."+className).attr("src","/fzmh/r/cms/www/default/image/"+className+"_current.png");
				$(".tp_index ."+className).addClass("cur");
		} 
		
	}
	
}
function openXjcx(){
	var ywptObj=$("#xjcx_layer");
	 layerObj=layer.open({
	  type: 1,
	  move: false,
	  scrollbar: false,
	  title:'<span style=" color: #216DC5;font-weight:bold;">留言查询</span>',
	  skin: 'layui-layer-rim', //加上边框
	  area: ['750px', '330px'], //宽高
	  content: ywptObj
	});
}
function xjcxForm(){
	var letterNum =$("#xjcxForm input[name='letterNum']").val();
	var queryPword =$("#xjcxForm input[name='queryPword']").val();
	if(letterNum==""||letterNum=="留言编号"){
		alert("请输入留言编号！！");
		$("#xjcxForm input[name='letterNum']").focus();
		return false;
	}
	if(queryPword==""||queryPword=="查询密码"){
		alert("请输入查询密码！！");
		$("#xjcxForm input[name='queryPword']").focus();
		return false;
	}
	
	$.ajax({
        url: '/fz/xjcxForAjax.jspx',
        type: 'post',
        dataType: 'json',
        async:false,
        data: $("#xjcxForm").serialize(),
        success:  function (data) {
        	if(data.status==0) {
        		alert(data.message);
        		return false;
            } else if(data.status==1) {	
        		if(data.reply){
        			SubmitForm();
        		}else{
        			alert("暂未回复！！！！");
        		}
            	return false;
            }
        },
});
	
}
function SubmitForm(){
	$("#xjcxForm").submit();
	$("#xjcxForm input[name='letterNum']").val("");
	$("#xjcxForm input[name='queryPword']").val("");
}

</script>

</head>
