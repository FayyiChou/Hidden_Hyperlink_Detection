			 $("#searchForm").submit();
		 }
	});
});
function qkeypress(){
	var q=$("input[name=q]");
	if(q.val().trim()!=""){
		$("input[name=q]").autocomplete("enable");
	}else{
		$("input[name=q]搜索搜索").autocomplete("disable");
	}
}
function search(){
	document.getElementById("searchForm").submit();
}
