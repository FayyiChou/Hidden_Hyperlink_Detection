<section class="login-section new">
	<script type="text/javascript">
		en4.core.runonce.add(function () {
			var tabAutorisationSwitch = window.tabAutorisationSwitch = function (element, block, num) {
				if (element[0].tagName.toLowerCase() == 'a') {
					element = element[0];
				}
				var left = $j('.index_menu > li:last-child').offset().left - 41;
				$j('.login-section').css('left', left);
				if (num == 1) {
					$j('.landing_popup').css('display', 'block');
					$j('.login-section').css('display', 'block');
					$j('.close_tab').css('display', 'none');
				}
				var myContainer = element.getParent('.login-section-title').getParent();
				myContainer.getChildren('div:not(.login-section-title)').setStyle('display', 'none');
				myContainer.getElements('div > a').removeClass('active');
				element.get('class').split(' ').each(function (className) {
					className = className.trim();
					if (className.match(/^tab_[a-z]+$/)) {
						if (block == 'forgot') {
							className = 'tab_forgot';
						}
						myContainer.getChildren('div.' + className).setStyle('display', null);
						element.addClass('active');
					}
				});
			}
		});
		function recoverPass(element) {
			var container = element.getParent(),
					email = $j('#email-recovery-pass-popup').val();
			$j.post("/user/auth/forgot", {email: email});
			container.getChildren('div:not(.recover_password)').setStyle('display', 'none');
			element.getChildren('div:not(.recover_password)').setStyle('display', 'none');
			$j(element).find('.recover_password p').html(email);
			element.getChildren('div.recover_password').setStyle('display', 'block');
			$j('#submit-forgotten').hide();
			return false;
		}
		function closePopUp(element) {
			element.css('display', 'none');
			$j('.login-section').css('display', 'none');
			$j('.close_tab').css('display', 'inline-block');
		}
	</script>

	<div class="login-section-title">
		<a class="tab_registration" onclick="tabAutorisationSwitch($j(this));" href="javascript:void(0);">Регистрация</a>
		<a class="tab_enter active" onclick="tabAutorisationSwitch($j(this));" href="javascript:void(0);">Вход</a>
	</div>

