				<input id="reg_name_popup"   type="text" placeholder="Ваше имя" name="first_name" required pattern="^[a-zA-Zа-яА-ЯёЁ][a-zA-Zа-яА-ЯёЁ0-9-_\. ]{1,64}$" title="имя" aria-required="true" onchange="trimStr(this);"   />
			</div>

			<div class="field-box email">
				<input id="email-address"   type="text" placeholder="E-mail" name="email" required pattern="^[^\s]+.*@.+\.[A-Za-z0-9]{2,6}$" title="youmail@domain.ru" aria-required="true" onchange="trimStr(this);"   "/>

			</div>

