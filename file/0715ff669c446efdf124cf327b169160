<script>
	$j('.itp_search, .itp_s_block input[type="reset"]').on('click', function(e){
		var el = e.target || e.srcElement;
		if(el.className == this.className) {
			$j('.itp_s_block').slideToggle();
			$j('.itp_s_block input[type="text"]').focus();
		}
	});

	$j(document).on('click', function(e){
		var el = e.target || e.srcElement;
		if(el.className !== 'itp_search align_top' && !$j('a').is($j(el).closest('.itp_search'))){
			$j('.itp_s_block').slideUp("slow");
		}
	});

	$j('.pt_title a, .pt_title .close').on('click', function(e){
		$j('.pt_content').slideToggle();
		console.log($j($j(this).parent()).is('.pt_title.active'));
		if($j($j(this).parent()).is('.pt_title.active')){
			$j(this).parent().removeClass('active');
		} else {
			$j(this).parent().addClass('active');
		}
	});

	$j('.go_to_registration').on('click', function(){
		var scrollTop = $j('.itr_right').offset().top;
		var delay = 800;
		$j('body, html').animate({
			scrollTop: scrollTop
		}, delay);
		setTimeout(function(){
			$j('.itr_r_input[type="text"]').focus()
		}, delay);
	});

    $j('.pt_bottom_block a').on('click', function(){
        var scrollTop = $j('.index_top_reg').offset().top;
        var delay = 800;
        $j('body, html').animate({
            scrollTop: scrollTop
        }, delay);
        setTimeout(function(){
            $j('.itr_r_input[name="first_name"]').focus()
        }, delay);
    });

	var timeout = null, xhr = null;
	function renderProductList(page, type) {
		if (xhr && xhr.readyState != 4) {
			xhr.abort();
		}

		history.pushState({}, [], location.pathname + '?p=' + page);

		xhr = $j.ajax({
			method: "POST",
			url: "/detstrana/index/paginator/",
			data: {page: page, type: type, ajax: true}
		}).done(function(data) {
			if (!data.error) {
				$j('#blog_list').append(data.html);
				$j('#category_product_list_paginator').html(data.paginator);
			} else {
				console.log()
			}
		});

		return false;
	}
</script></body>
</html>