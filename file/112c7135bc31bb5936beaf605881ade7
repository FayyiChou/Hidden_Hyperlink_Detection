<script language="javascript">
var jq = jQuery.noConflict();
    var lastScrollY = 0;
    function heartBeat() {
        var diffY = jq(document).scrollTop();
        percent = .1 * (diffY - lastScrollY);
        if (percent > 0) {
            percent = Math.ceil(percent);
        } else {
            percent = Math.floor(percent);
        }
        document.getElementById("webpiao").style.top = parseInt(document.getElementById("webpiao").style.top) + percent + "px";
        lastScrollY += percent;
    }
    window.setInterval("heartBeat()", 1);
    jq(document).ready(function() {
        jq("#tabs_infos li").mouseover(function() {
            jq(this).siblings("li").removeClass("info_sel");
            jq(this).addClass("info_sel");
            jq(this).parent().siblings("div").addClass("tabs-hide");
            jq(this).parent().siblings($(this).children("a").attr("rel")).removeClass("tabs-hide");
        });
        jq(".webpiaos_1").click(function() {
            jq("#webpiao").css({ "width": "27px", "height": "114px" });
            jq(".webpiao_hide").hide();
            jq(".webpiao_show").show();
        });
        jq(".webpiaoh_1").click(function() {
        jq("#webpiao").css({ "width": "105px", "height": "auto" });
            jq(".webpiao_hide").show();
            jq(".webpiao_show").hide();
        });
    })
