<script type="text/javascript">
	function addJS(url){
		var script=document.createElement("script");  
		script.setAttribute("type", "text/javascript");  
		script.setAttribute("src", url);
		var heads = document.getElementsByTagName("head");
		if(heads.length) {
		    heads[0].appendChild(script);
		} else {  
		    document.documentElement.appendChild(script);
		}
	}
	function getQueryString(name) {
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i"); 
		var r = window.location.search.substr(1).match(reg); 
		if (r != null) return unescape(r[2]); return null;
	}
	if(getQueryString('utm_source') == "onStar"){
		window.localStorage.setItem("onStarToken", getQueryString('token'));
		window.localStorage.setItem("onStarAuth", getQueryString('auth'));
	}
	if(window.localStorage.getItem("DELIVERY_APP") || getQueryString('channel') == "Delivery"){
		addJS("SUPER_APP_FILE_LIB:///cordova/cordova.js");
		window.localStorage.setItem("DELIVERY_APP", "deliveryApp");
	}
</script>

