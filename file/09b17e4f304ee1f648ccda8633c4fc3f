<script>
	// Only used in config.js and require.config.js
	window.KFCMWOS_PATH = "/kfcmwos/";
	window.currProjectPath = 'https://m.4008823823.com.cn/kfcmwos/';
	window.KFCMWOS_HTTPS = '//m.4008823823.com.cn/kfcmwos/';
	window.dcsTrackingUrl = 'dt.hwwt8.com/dcsoye69k00000w4dihnwa22c_7u5p/dcs.gif?WT.branch=kfc_mwos_A';
	window.managementPublicityUrl = 'http://deliverycert.yumchina.com/yum-portal/disclosure/KFC_mobile_123';
	var imageResPath ='//imgm.4008823823.com.cn/kfcmwos/img/';
    var resPath = '//m.4008823823.com.cn/kfcmwos/';
    var _mvq = _mvq || [];
    _mvq.push(['$setAccount', 'm-243945-0']);
    _mvq.push(['$logConversion']);
    //google dsp
	dataLayer.push({'event': 'home_load'});
	//获取经纬度
	var dfrd = $.Deferred();
	var getPosition = function () {
		if (!navigator || !navigator.geolocation || !navigator.geolocation.getCurrentPosition) {
			dfrd.reject({
				code : -1,
				error : "browser do not support geo location"
			});
		} else {
			navigator.geolocation.getCurrentPosition(function(ret) {
				var trueLoc;
				if(typeof AlipayJSBridge !== 'undefined'){
					trueLoc = {
						lat: ret.coords.latitude,
						lng: ret.coords.longitude
					};
				}else{
					trueLoc =  transformFromWGSToGCJ(ret.coords.longitude,ret.coords.latitude);
				}
				dfrd.resolve({
					code : 0,
					error : "",
					latitude : trueLoc.lat,
					longitude : trueLoc.lng
				});
			}, function(ret) {
				dfrd.reject({
					code : ret.code,
					error : ret.message
				});
			}, {
				//开启高精度定位
				enableHighAccuracy : true,
				timeout : 5000,
				maximumAge : 300000
			});
		}
		return dfrd.promise();
	}
	var backCallBack = null;
	var isWeixin = function(){
		var ua = navigator.userAgent.toLowerCase();
		return ua.match(/MicroMessenger/i) == 'micromessenger';
	}
	//banner的点击
	function bannerClick(){
		$('#topCarrousel li span').click(function(){
			var $this = $(this);
			window._tag && _tag.dcsMultiTrack('wt.event','CTW_home_banner'+$this.parent().index(),'wt.msg',"首页,轮播BANNER"+$this.parent().index());
			//跳转分类
			if ($this.attr('relationType') == 1) {
				backCallBack && document.removeEventListener("backbutton", backCallBack, true);
				setTimeout(function(){
					location.href = window.currProjectPath + "menu.htm?classId=" + $this.attr('classId');
				},50);
			}
			//跳转外部链接
			else{
				//added by qianpeng @20161020 double 11 carnival
				if (typeof cordova !== 'undefined') {
					if("carnival" == $this.attr('linkedUrl')){
						if(isWeixin()){
							location.href = window.KFCMWOS_PATH + "imCouponing.do?carnival=1";
						}else{
							location.href = window.currProjectPath + "login?registerChannel=4";
						}
					}else{
						smartmobile.appmanager.startApp({
							url : $this.attr('linkedUrl'),
							title : $this.attr("titleCn") ? $this.attr("titleCn") : "活动"
						},function (scb) {},function (fcb) {});
					}
				}else{
					setTimeout(function(){
						if("carnival" == $this.attr('linkedUrl')){
							if(isWeixin()){
								location.href = window.KFCMWOS_PATH + "imCouponing.do?carnival=1";
							}else{
								location.href = window.currProjectPath + "login?registerChannel=4";
							}
						}else{
							location.href = $this.attr('linkedUrl');
						}
					},50);
				}
			}
		});
	}
	
	//返回设备类型
	function checkOS(ua){
		var rs = '';
		if(ua.indexOf('Android') > -1 || ua.indexOf('Linux') > -1 ){
			rs = 'android';
		}else if(ua.indexOf('iPhone') > -1 || ua.indexOf('iPad') > -1){
			rs = 'ios';
		}else if(ua.indexOf('Windows Phone') > -1){
			rs = 'windows phone';
		}
		return rs;
	}
	
	//首页轮播
	var carrousel = new $.am.Carrousel({
		id : 'carrouselDiv',
		height : null,
		autoSwitch : 2000,
		itemw : window.innerWidth - 25,
		homeFlag : true,
		onchange : function(i) {
			$('#topCarrousel li').attr('class','');
			$('#topCarrousel li:eq('+i+')').addClass('on');
			$('#topCarrousel li:eq('+(i+1)+')').addClass('after');
			if(i>0){
				$('#topCarrousel li:eq('+(i-1)+')').addClass('before');
			}
		}
	});
	$('#topCarrousel li:eq(0)').addClass('on');
	
	//操作城市banner
	//1、定位到城市，显示当前城市和全国
	//2、未定位到城市，显示全国和所有城市的
	var banners = $("#carrouselDiv ul");
	function switchBannerByCity(city){
		var tempBanners = banners.clone(true);
		if(city){
			var tempBannerLi = tempBanners.find("li");
			var hasCityBanner = false;
			if(tempBannerLi){
				for(var i = 0; i < tempBannerLi.length; i++){
					var citycodesSpan = $(tempBannerLi[i]).find("span").attr("citycodes");
					var cityselecttype = $(tempBannerLi[i]).find("span").attr("cityselecttype");
					if(citycodesSpan){
						//正选，无city-删除
						//反选，有city-删除
						if((citycodesSpan.indexOf(city) < 0 && cityselecttype == "true") || (citycodesSpan.indexOf(city) > -1 && cityselecttype == "false")){
							tempBanners.find("li span[citycodes='" + citycodesSpan + "']").parent().remove();
						}else{
							hasCityBanner = true;
						}
					}
				}
			}
		}
		$("#carrouselDiv ul").html(tempBanners.html());
		bannerClick();
		carrousel.refresh(0, true);
		$('#carrouselDiv').show();
	}
	
	//定位
	getPosition().done(function(ret){
		window.longitude = ret.longitude;
		window.latitude = ret.latitude;
		var ua = navigator.userAgent;
		var os = checkOS(ua);
		var cityCode = "";
		$.post(window.currProjectPath + "rest/misc/search/gpscity", {
			x: ret.longitude,
			y: ret.latitude,
			"meters": 100,
			"brand": 'kfc',
			"userAgent": ua,
			"os":  os,
			"relocate": "Y"
		}).done(function(data) {
			var cityName = "";
	 		if (data.cityInfo) {
				var gpsData = JSON.parse(data.cityInfo);
				var allCities = data.allCities;
				if(allCities){
					for(var i = 0; i < allCities.length; i++){
						if($.trim(allCities[i].name) == $.trim(gpsData.data.cityName)){
							cityCode = allCities[i].citycode;
							cityName = allCities[i].name;
							break;
						}
					}
				}
			}
	 		switchBannerByCity(cityCode);
	 	}).fail(function() {
	 		switchBannerByCity("");
	 	});
	}).fail(function(){
		switchBannerByCity("");
	});
</script>
