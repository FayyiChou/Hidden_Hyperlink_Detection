<script type="text/javascript">!function(){    (function () {
        var currentOverlay, currentButton;

        $('.button-area a').on('click', function (e) {
            if(!$(this).hasClass('btn-category')) {
                return true;
            }

            e.preventDefault();

            var $thisButton = $(this),
                $thisOverlay = $($thisButton.attr('data-associated-overlay'));

            if (currentOverlay && currentOverlay[0] != $thisOverlay[0]) {
                currentOverlay.hide();
            }

            if (currentButton && currentButton[0] != $thisButton[0]) {
                currentButton.removeClass('ui-state-active');
            }

            if ($thisButton.hasClass('ui-state-active')) {
                $thisButton.removeClass('ui-state-active');
                $thisOverlay.hide();
            } else {
                $thisButton.addClass('ui-state-active');
                $thisOverlay.show();
                currentButton = $thisButton;
                currentOverlay = $thisOverlay;
            }
        });

        // 点击背景直接关闭弹层（曹宇添加）
        $('.nav-overlay').click(function(e) {
            e.stopPropagation();
            // console.log($(e.target).hasClass('nav-overlay'));
            if($(e.target).hasClass('nav-overlay')) {
                currentButton.removeClass('ui-state-active');
                currentButton = null;
                currentOverlay.hide();
                currentOverlay = null;
            }
        });
    }());
    }();
!function(){        var resizeEventName = 'onorientationchange' in window ? 'orientationchange' : 'resize';
            //mainTabs = new $.hao123.Tabs('#main_tabs');

        (function () {
            var scrolls = [],
                    timer;

            function refreshScrolls() {
                $(scrolls).each(function (index, scroll) {
                    var wrapper = scroll.wrapper,
                            totalWidth = wrapper.clientWidth;

                    //$('>ul>li', wrapper).css('width', Math.floor(totalWidth * 0.28) + 'px');
                    $(wrapper).children('ul').children('li').css('width', Math.floor(totalWidth * 0.28) + 'px');
                    scroll.refresh();
                });
            }

            $(window).on('load', function () {
                $('#recommend .js-iscroll').each(function (index, item) {
                    var totalWidth = item.clientWidth;

                    //$('>ul>li', item).css('width', Math.floor(totalWidth * 0.28) + 'px');
                    $(item).children('ul').children('li').css('width', Math.floor(totalWidth * 0.28) + 'px');

                    scrolls.push(new IScroll(item, {
                        scrollbars: true,
                        fadeScrollbars: true,
                        scrollX: true,
                        scrollY: false,
                        eventPassthrough: true
                    }));
                });
            });

            $(window).on(resizeEventName, function () {
                clearTimeout(timer);
                timer = setTimeout(function () {
                    refreshScrolls();
                }, 500);
            });

            // mainTabs.on('activate', function () {
            //     if (this.activeIndex === 0) {
            //         clearTimeout(timer);
            //         timer = setTimeout(function () {
            //             refreshScrolls();
            //         }, 0);
            //     }
            // });
        }());
        }();</script></body></html>