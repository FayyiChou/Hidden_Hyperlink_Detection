    function showhidediv(id){
        if (id == 'msg_3') {
            if (msg_3.style.display=='none') {msg_4.style.display='none';msg_3.style.display='block';}
            return;
        }
        if(id=='msg_4'){
            if (msg_4.style.display=='none') {msg_3.style.display='none';msg_4.style.display='block';  }
            return;
        }
        if (id == 'msg_1') {
            if (msg_1.style.display=='none') {msg_2.style.display='none';msg_1.style.display='block';}
            return;
        }
        if(id=='msg_2'){
            if (msg_2.style.display=='none') {msg_1.style.display='none';msg_2.style.display='block';  }
            return;
        }
    }
