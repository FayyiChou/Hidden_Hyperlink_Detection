<script text='text/javascript' language='javascript'>
function resizeContainer()
{
        if(window.outerHeight)
        {
                cont = document.getElementById("container");
                cont.style.marginTop = (window.outerHeight/2) - cont.clientHeight;
        }
        else
        {
                cont = document.getElementById("container");
                cont.style.marginTop = (self.screen.availHeight/4) - cont.clientHeight;
        }
}
</script>
<title>Site Offline</title>
</head>
<body onload='resizeContainer();'>
