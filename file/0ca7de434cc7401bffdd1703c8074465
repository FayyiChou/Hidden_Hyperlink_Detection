<!--左侧导航位置固定效果-->
<script type="text/javascript">

    $(function(){
        //如果右边高度不够，不去做左边滚动效果
        //获取左边div的高度
        var left_height = parseInt($('#aboutNav').outerHeight(true));
        //var left_height = parseInt($('#aboutNav').height());
        //获取右边div的高度
        var right_height = parseInt($('#aboutRight').outerHeight());

        if( right_height < left_height ){
            //若右边div高度过低，展示不了左边链接，给右边一个最小高
            $('#aboutRight').css('min-height',left_height+'px');
        }else{
            leftScroll( );
        }
    });
    function leftScroll(){
        //左侧导航位置固定效果
        var t = null;
        $(document).scroll(function(){
            //显示当前滑块滑过的高度
            var hua_kuai = $(this).scrollTop();
            //获取左边div的高度
            var left_height = $('#aboutNav').outerHeight();
            //获取右边div的高度
            var right_height = $('#aboutRight').outerHeight();

            //获取左边导航相对于页面的高度
            var left_top = $("#aboutNav").position().top;

            var height = hua_kuai + left_top ;

            //$('#aboutNav a span').html(height);
            //如果左边的高度加上滑块滑过的距离大于右边的高度
            if( parseInt(hua_kuai+left_height) > parseInt(right_height) ){
                if(t == null) t=parseInt(height)-20;

                //超出的话，position改为相对的
                //top高度来自于滑块滑过的高度及距离顶部的高度
                $('#aboutNav').css({
                    'position':'absolute',
                    'top':t + 'px'
                });

            }else{
                //未超出的话，position改为相对的
                $('#aboutNav').css({
                    'position':'fixed',
                    'top':'156px'
                });
            }
        });
    }

</script>
