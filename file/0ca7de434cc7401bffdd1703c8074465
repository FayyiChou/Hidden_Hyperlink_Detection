<script>
	$(document).ready(function(){
		$('#closeBanner').click(function(){hideAd();});

		var bannerHeight = $('#indexBanner').height();
		var navStatus = -1; //status 0-不显示 1-显示 -1-需要重新处理
		var adEffect = (bannerHeight > 0 && $(window).scrollTop()>50) ? 1 : 0;

		var docScroll = $(window).scrollTop();/*当前页面位置*/
		setAdStatus(bannerHeight, docScroll);/*设置当前页面状态*/
		//showNav();
		topStatus();

		var initPos = docScroll;/*滚动前的页面位置*/
		$(window).scroll(function(){
			var docScroll = $(window).scrollTop();
			setAdStatus(bannerHeight, docScroll);/*设置滚动后页面状态*/
			topStatus();
			/*向上滚动时显示导航*/
			if(docScroll < initPos && docScroll<$(document).height()-$(window).height()) showNav();
			/*向下滚动并且不受广告影响时隐藏导航*/
			else if(docScroll > initPos && docScroll>0) hideNav();
			initPos = docScroll;
		});

		function topStatus(){
			var topTop = adEffect ? '50px' : '0px';
			var topPos = adEffect ? 'absolute' : 'fixed';
			if($('#Top').css('position') != topPos) $('#Top').css('position', topPos);
			if($('#Top').css('top')!=topTop) $('#Top').css('top', topTop);
		}
		function hideNav(){
			if(navStatus == 0) return false;
			var navTop = adEffect ? '85px' : '35px';
			var navPos = adEffect ? 'absolute' : 'fixed';
			var duration = navStatus==-1 ?0:70;
			if($('#Nav').css('position') != navPos) $('#Nav').css('position', navPos);
			/*导航状态需要重新设定时，撤销动画，防止抖动*/
			$('#Nav').animate({top:navTop}, duration);
			var tmp = bannerHeight + 80;
			$('body').animate({'margin-top':tmp+'px'}, duration);
			navStatus = 0;
		}
		function showNav(){
			if(navStatus == 1) return false;
			var navTop = adEffect ? '130px' : '80px';
			var navPos = adEffect ? 'absolute' : 'fixed';
			var duration = navStatus==-1 ?0:70;
			if($('#Nav').css('position') != navPos) $('#Nav').css('position', navPos);
			$('#Nav').animate({top:navTop}, duration);
			var tmp = bannerHeight + 80 + 45;
			$('body').animate({'margin-top':tmp+'px'}, duration);
			navStatus = 1;
		}

		/*设置当前页面状态, 判断是否受广告影响*/
		function setAdStatus(bannerHeight, docScroll){
			/*有广告并且页面仍能看到广告受广告影响*/
			if(bannerHeight > 0 && docScroll < bannerHeight){
				if(adEffect != 1){
					adEffect = 1;
					navStatus = -1;
				}
			}
			else{/*不受广告影响时*/
				if(adEffect != 0){
					adEffect = 0;
					navStatus = -1;
				}
			}
		}
		function hideAd(){
			$('#indexBanner').css('height', '0px');
			$('body').css('margin-top','125px');
			bannerHeight = initPos = docScroll = 0;
			setAdStatus(bannerHeight, docScroll);/*设置滚动后页面状态*/
			topStatus();
			showNav();
		}
	});
</script>

