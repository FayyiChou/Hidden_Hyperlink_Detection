	var isShowcatTitle = "false";
	var categoryName = "社保专区";
	addLoadEvent(onloadScrol6IJJEFMJCUFOQPCJN68BXSO2GUIHPBD5);
	function onloadScrol6IJJEFMJCUFOQPCJN68BXSO2GUIHPBD5(){
		if(null != isShowcatTitle && "true" == isShowcatTitle){
		    if(null == categoryName || "" == categoryName){
		    	window.document.title = "中山市地方税务局门户网站";
		    }else{
				window.document.title = "社保专区" + "_" + "中山市地方税务局门户网站";
			}
		}else{
			window.document.title = "社保专区_" + "中山市地方税务局门户网站";
		}
	}
	function addLoadEvent(fun){
		var oldonload = window.onload;
		if(typeof window.onload != 'function'){
			window.onload = fun;
		}else {
			window.onload = function(){
				if(oldonload){
					oldonload();
				}
				fun();
			}
		}
	}
