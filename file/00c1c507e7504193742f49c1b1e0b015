function searchSubmit() {
	var search = document.getElementById('searchPO1P5YI3E9997XHG8CUIVZEXG1HSR6W7');
	if (search.news.value == ""||search.news.value=="全文搜索") {
		alert("请输入查询关键字");
		search.news.select();
		search.news.focus();
		return;
	}
	search.keyword.value = search.news.value;
	searchPO1P5YI3E9997XHG8CUIVZEXG1HSR6W7.action = "/portal/cmsSearchV2?entryUrl=/fzgb/zs/";
	searchPO1P5YI3E9997XHG8CUIVZEXG1HSR6W7.target = "_blank";
	search.submit();
}
function clearFont() {
	var search = document.getElementById('searchPO1P5YI3E9997XHG8CUIVZEXG1HSR6W7');
	if (search.news.value=="全文搜索") {
		search.news.value="";
 	}
}
