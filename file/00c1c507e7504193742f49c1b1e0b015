    var xmlHttp;
    var str = "";
    var commonPath = '/skin/style/images-zs-00/common';
    var isNeedStatistic = 'Y';
    function Ajax() {
		var ajax = false;
		try {
			ajax = new ActiveXObject("Msxml2.XMLHTTP");
		} catch (e) {
			try {
				ajax = new ActiveXObject("Microsoft.XMLHTTP");
			} catch (E) {
				ajax = false;
			}
		}
		if (!ajax && typeof XMLHttpRequest != "undefined") {
			ajax = new XMLHttpRequest();
		}
		return ajax;
	}
    function getGiftFromServer() {
        var url = "/stat/statgather?sitename=904038009&accessType=site&accessId=0&return=S";
        xmlHttp=Ajax();
        xmlHttp.onreadystatechange = showGiftExt;
        if (url.indexOf('stat') > 1) {
        	url = url.substring(url.indexOf('stat') - 1);
        }
        xmlHttp.open("GET", url, true);
        xmlHttp.send(null);
    }
    function showGiftExt() {
        if (xmlHttp.readyState == 4 || xmlHttp.readyState == "complete") {
            var responseText = xmlHttp.responseText;
            var isNum = /^[0-9]$/;
            var isNumFlag = true;
            for (var i = 0; responseText != null && responseText != "" && i < responseText.length; i ++) {
                var numChar = responseText.charAt(i);
                if (!isNum.test(numChar)) {
                    isNumFlag = false;
                    break;
                }
            }
            if (isNumFlag) {
                str=responseText;
            }
            var strChange = str.toString();
            if(strChange != null){
	            var countString = "";
	            if(strChange.length%3 !=0){
	            	countString = strChange.substring(0,strChange.length%3) + ",";
	            }
	            for(var i=strChange.length%3; i<strChange.length;){
					if(i+3 !=strChange.length){
						countString += strChange.substring(i,i+3) + ",";
					}
					else{
						countString += strChange.substring(i,i+3);
					}
					i = i+3;
				}
			    if (document.getElementById("output") != null) {
	            	document.getElementById("output").innerHTML = countString;
	            }
	    	} else if (document.getElementById("output") != null) {
	            document.getElementById("output").innerHTML = str;
	        }
        }
    }
    if (isNeedStatistic == "Y") {
        getGiftFromServer();
    }
</script> <script type="text/javascript">
function fRemoveClass(oElement, sClassName){
	if(arguments.length == 1){
		var sClassName = oElement;
		var oElement = this;
	}
	var aClass = oElement.className.split(" ");
	var arr = [];
	for(var i=0,m=aClass.length;i<m;i++){
		if(aClass[i] != sClassName){
		  arr[arr.length] = aClass[i];
		}
	}
	oElement.className = arr.join(" ");
}
function fSetClass(oElement, sClassName){
	if(arguments.length == 1){
		var sClassName = oElement;
		var oElement = this;
	}
	oElement.className = sClassName;
}
if(screen.width <= 1260) {
	fRemoveClass(document.getElementById("footDiv"), "div-foot-01");
	fSetClass(document.getElementById("footDiv"), "div-foot-02");
}
window.onresize = function() {
	if(document.body.clientWidth <= 1260) {
		fRemoveClass(document.getElementById("footDiv"), "div-foot-01");
		fSetClass(document.getElementById("footDiv"), "div-foot-02");
	} else {
		fRemoveClass(document.getElementById("footDiv"), "div-foot-02");
		fSetClass(document.getElementById("footDiv"), "div-foot-01");
	}
	try {
		resetPis();
	} catch(e) {}
}
