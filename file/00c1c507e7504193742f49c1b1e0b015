<div id="pDWJ6XT57VCZFVD14P3ZYNK12D8N82XS7"> <script language="javascript" type="text/javascript">
			var xfPageNum=1;
			var blockNum=1;
 			function getElement(v){return document.getElementById(v);}
			function changePage(page)
				{
    				if(isNaN(page)){alert('请输入数字');return;}
    				if(page>parseInt(getElement('totalPage').value)||page<=0){alert('没有该页');return;}
    				xfPageNum = page;
    				if(xfPageNum==blockNum){
    				   alert("已经是当前页面");
    				   return;
    				}
    				if(getElement('DIV'+blockNum))getElement('DIV'+blockNum).style.display="none";
					if(getElement('DIV'+xfPageNum))getElement('DIV'+xfPageNum).style.display="block";
					blockNum=xfPageNum;
					getElement('nepage').value=parseInt(xfPageNum)+1;
					getElement('prePage').value=parseInt(xfPageNum)-1;
					getElement('CurpageLbl').innerHTML=xfPageNum;
				}
			function prePage()
				{
					var curr=getElement('prePage').value;
					if(parseInt(curr)<1){alert('已经是第一页');return;}
					changePage(curr);
				}
			function nextPage()
				{
					var ne=getElement('nepage').value;
					if(parseInt(ne)>parseInt(getElement('totalPage').value)){ne=getElement('totalPage').value;alert('已经是最后一页');return;}
					changePage(ne);
				}
