    <script>
        function changeImage(imgName) {
            image = document.getElementById('wibble');
            var imagesrc = image.src;
            var lastfound = imagesrc.lastIndexOf("/");
            alert('original image details ' + imagesrc + ' ' + lastfound + ' >>>>> ' + imagesrc.length + '<<<<' +  imagesrc.substring(lastfound + 1));
            var sliced = imagesrc.substring(lastfound + 1);
            alert('sliced - ' + sliced);
            var newstr = imgName + sliced;
            alert('new - ' + newstr);
            image.src = newstr;
            //alert('change image to ' + image.src);
            //alert('New : ' + newstr);
            // onload="changeImage('/Content/Images/ShowsEvents/Shows_Cereals.png')">
        }

    </script>
   
