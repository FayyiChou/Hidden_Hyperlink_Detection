<script type="text/javascript">
	
	$(function(){
		
		$(".is_UpUl li").hover(function(){
			$(this).find("em").show(200);
		}, function(){
			$(this).find("em").hide(200);
		});
		
		
		$("#open_QQService").click(function(){
			$(this).find("iframe").contents().find("#launchBtn").click();
		});
	});
	
</script>



<script type="text/javascript">
$(function(){
	
	//定位当前菜单
	var l_href = window.location.href;
	var l_arr = l_href.split("/");
	var wlh_href = l_arr[l_arr.length-2];
	
	//特殊情况定位
	if(wlh_href == "towcode") wlh_href = "supply";
	var nav_flag = 0;
	$(".headNav li a").each(function(){
		if($(this).attr("nav") == wlh_href){
			$(this).closest(".headNav").find(".active").removeClass("active");
			$(this).addClass("active");
			nav_flag++;
		}
	});
	if(nav_flag == 0 && l_href.indexOf("index/help") > 0){
		$(".headNav").find(".active").removeClass("active");
		$(".headNav li a[nav='help']").addClass("active");
	}
	
	
	$.post("http://www.itmys.com:80/index/getUserOrderCount.json",function(res){
		$('#header_user_order_count_i').html(res.userOrderCount);
		$('.is_UpUl .cartNum ').html(res.userOrderCount); 
	});
	
	$.post("http://www.itmys.com:80/index/getUserInfo.json",function(res){
		if(null==res.userName)
		{
			$('#userName_desc').html("");
		}else
		{
			$('#userName').html(res.userName);
		}
	});

	$.post("http://www.itmys.com:80/index/getMessage.json",function(res){
		$('#header_user_message_count').html(res.count);
	});
	
	$(".head_search_btn").click(function(){
		var headSearchText = $(".head_search_text").val();
		var url = encodeURI("http://www.itmys.com:80/supply/headSearchSupplyList?headSearchText="+headSearchText);
		window.location.href = url;
	});
	
	$(".head_search_text").keydown(function(e){
		if(e.keyCode == 13){
			var headSearchText = $(".head_search_text").val();
			var url = encodeURI("http://www.itmys.com:80/supply/headSearchSupplyList?headSearchText="+headSearchText);
			window.location.href = url;
		}
	});
	
	//图标闪烁
	setInterval(function(){
		var jump_a = $(".top_left").find("a");
		
		if(jump_a.attr("jumpFlag") == "1"){
			jump_a.css("background-position", "1px 1px");
			jump_a.removeAttr("jumpFlag");
		}else{
			jump_a.css("background-position", "1px 999px");
			jump_a.attr("jumpFlag", "1");
		}
		
	}, 500);
	
});

function SetHome(obj,url){
  try{
    obj.style.behavior='url(#default#homepage)';
    obj.setHomePage(url);
  }catch(e){
    if(window.netscape){
     try{
       netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
     }catch(e){
       newAlert('3',"抱歉，此操作被浏览器拒绝！\n\n请在浏览器地址栏输入“about:config”并回车然后将[signed.applets.codebase_principal_support]设置为'true'");
     }
    }else{
    	newAlert('3',"抱歉，您所使用的浏览器无法完成此操作。\n\n您需要手动将【"+url+"】设置为首页。");
    }
 }
}
  
function AddFavorite(title, url) {
 try {
   window.external.addFavorite(url, title);
 }catch (e) {
   try {
    window.sidebar.addPanel(title, url, "");
  }catch (e) {
	  newAlert('3',"抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请进入网站后使用Ctrl+D进行添加");
   }
 }
}

//打开聊天窗口
function openIm() {
	imWin = window.open('about:blank','im');
	var url = "http://www.itmys.com:80/im/index.itm";
	var formHtml = '<form id="submitForm" action="'+url+'" method="post">';
	formHtml += '</form>';
    imWin.document.write(formHtml);
    imWin.document.getElementById('submitForm').submit();
}
</script>



	
