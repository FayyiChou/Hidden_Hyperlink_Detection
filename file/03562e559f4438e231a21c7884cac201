	<script type="text/javascript">
		function purchaseQuote(purchaseId,isActivate){
			if(userId==null){
				newAlert(3,'您尚未登录，请登录后进行报价',function(){
					window.location.href = 'http://www.itmys.com/login.jsp';
				});
			}else{
				if(isActivate != '1'){
					window.location.href = 'http://www.itmys.com/member/dataVerification?type=2';
					return;
				}
				var price = $('[purchaseDetailId="'+purchaseId+'"]').eq(0).val();
				var amount = $('[purchaseDetailId="'+purchaseId+'"]').eq(1).val();
				if(price<=0){
					newAlert(2,'价格不能小于等于0');
				}else if(amount<=0){
					newAlert(2,'可供应数量不能小于等于0');
				}else if(price !="" && amount != ""){
					$.post('http://www.itmys.com/purchase/addQuote.json',{purdetailId:purchaseId,price:price,amount:amount},function(res){
						if(res.messageType=='success'){
							newAlert(1,res.message,function(){
								 $('[purchaseDetailIdBtn="'+purchaseId+'"]').remove();		
								 $('[purchasedetailId="'+purchaseId+'"]').eq(0).parent().parent().append('<span class="j_p_btn"><input type="button" class="okBj" value="已报价" /></span>');
							});
						}else{
							newAlert(2,res.message);
						}
					});
				}else{
					newAlert(2,'请将信息录入完整');
				}
			}
		}
		$(function(){
			//banner切换
			$('.flexslider').flexslider({
				directionNav: true,
				pauseOnAction: false,
				pauseOnHover: true,
				slideshowSpeed:3000 
			});	
			
			
			//采购数据切换
			var scroll_flag = true;
			$(".scroll_dynamic .j_tab_td").live({
				mouseover: function(){
					scroll_flag = false;
				},
				mouseout: function(){
					scroll_flag = true;
				}
			});
			
			var scroll_page1, scroll_page2;
			$(".scroll_dynamic").each(function(i, e){
				var scroll_page = $(this).find(".j_tab_body").length;
				if(i==0) scroll_page1 = scroll_page;
				if(i==1) scroll_page2 = scroll_page;
				
				var scroll_index = $(this).closest(".index_dynamic").find(".dynamic_num ul");
				for(var i=0; i<scroll_page; i++){
					scroll_index.append("<li></li>");
				}
				scroll_index.find("li").css("width", Math.round(930/scroll_page));
				scroll_index.find("li:first").addClass("active");
			});
			
			//采购单滚动
			var scroll_num1 = 0;
			if(scroll_page1 > 1){
				setInterval(function(){
					if(scroll_flag){
						var first_dynamic1 = $(".scroll_dynamic").eq(0);
						scroll_num1++;
						
						if(scroll_num1 == scroll_page1) scroll_num1 = 0;
						
						var s_indexObj = first_dynamic1.closest(".index_dynamic").find(".dynamic_num");
						s_indexObj.find("li.active").removeClass("active");
						s_indexObj.find("li").eq(scroll_num1).addClass("active");
						
						first_dynamic1.animate({"margin-left":"-930px"}, 1000, function(){
							first_dynamic1.append(first_dynamic1.find(".j_tab_body").eq(0));
							first_dynamic1.css("margin-left", "0");
						});
					}
					
				}, 15000);
			}
			
			//询价单滚动
			var scroll_num2 = 0;
			if(scroll_page2 > 1){
				setInterval(function(){
					if(scroll_flag){
						var first_dynamic2 = $(".scroll_dynamic").eq(1);
						scroll_num2++;
						
						if(scroll_num2 == scroll_page2) scroll_num2 = 0;
						
						var s_indexObj = first_dynamic2.closest(".index_dynamic").find(".dynamic_num");
						s_indexObj.find("li.active").removeClass("active");
						s_indexObj.find("li").eq(scroll_num2).addClass("active");
						
						first_dynamic2.animate({"margin-left":"-930px"}, 1000, function(){
							first_dynamic2.append(first_dynamic2.find(".j_tab_body").eq(0));
							first_dynamic2.css("margin-left", "0");
						});
					}
					
				}, 15000);
			}
			
			$(".dynamic_num li").live("click", function(){
				var li_index = $(".index_dynamic").index($(this).closest(".index_dynamic"));
				var first_dynamic = $(".scroll_dynamic").eq(li_index);
				var _thisIndex = $(this).index();
				
				var scroll_num, scroll_page;
				if(li_index == 0) {
					scroll_num = scroll_num1;
					scroll_num1 = _thisIndex;
					scroll_page = scroll_page1;
				}
				if(li_index == 1) {
					scroll_num = scroll_num2;
					scroll_num2 = _thisIndex;
					scroll_page = scroll_page2;
				}
				
				var animateNum;
				if(_thisIndex == scroll_num) return false;
				if(_thisIndex > scroll_num) animateNum = _thisIndex - scroll_num;
				if(_thisIndex < scroll_num) animateNum = scroll_page + _thisIndex - scroll_num;
				
				scroll_flag = false;
				
				var s_indexObj = first_dynamic.closest(".index_dynamic").find(".dynamic_num");
				s_indexObj.find("li.active").removeClass("active");
				s_indexObj.find("li").eq(_thisIndex).addClass("active");
				
				first_dynamic.animate({"margin-left":"-930"*animateNum + "px"}, 1000, function(){
					for(var i=0; i<animateNum; i++){
						first_dynamic.append(first_dynamic.find(".j_tab_body").eq(0));
						first_dynamic.css("margin-left", "0");
					}
					
					scroll_flag = true;
				});
			});
			
			//新闻
			$(".index_news li").live("mouseover", function(){
				$(this).siblings(".active").removeClass("active");
				$(this).addClass("active");
			});
			
			//常见问题
			$(".index_questions li").bind("mouseover", function(){
				$(this).siblings(".active").removeClass("active");
				$(this).addClass("active");
			});
			
			//企业滚动
			var eb_left = -245;
			var eb_flag = true;
			setInterval(function(){
				if(eb_flag){
					$(".entering_brand").animate({"margin-left":eb_left+"px"}, 500, function(){
						var eb_obj = $(".entering_brand .brandLogo:first");
						$(".entering_brand").append(eb_obj);
						$(".entering_brand").css("margin-left", "0");
					});
				}
				
			}, 2000);
			
			//播放视频
			$("#playVideo").live("click", function(){
				$("body").append('<div style="position: fixed; top:0; left: 0; z-index: 99999; width: 100%; height: 100%; background: url(http://www.itmys.com/images/shadow_bg4.png) repeat;"><a href="javascript:;" id="closeVideo" style="display: block; font-size: 48px; color: #fff; background: #000; position: absolute; left: 50%; margin-left: 335px; width: 50px; height: 50px; line-height: 45px; text-align: center; top: 50%; margin-top: -225px;">×</a><iframe style="border: 5px solid #000; width: 660px; height: 447px; position: absolute; top: 50%; margin-top: -225px; left: 50%; margin-left: -335px;" src="http://v.qq.com/iframe/player.html?vid=g0188493fic&tiny=0&auto=1" allowfullscreen=""></iframe></div>');
			});
			$("#closeVideo").live("click", function(){
				$(this).closest("div").remove();
			});
			
			
			//头部最新入驻滚动
			var ns_top = -104;
			var ns_flag = true;
			setInterval(function(){
				if(eb_flag){
					$(".newSettled_list").animate({"margin-top":ns_top+"px"}, 300, function(){
						var eb_obj = $(".newSettled_list > div:first");
						$(".newSettled_list").append(eb_obj);
						$(".newSettled_list").css("margin-top", "0");
					});
				}
				
			}, 2000);
			
			
			$(".i_supply_menu i").live("click", function(){
				$(this).siblings(".active").removeClass("active");
				$(this).addClass("active");
				
				var ism_temp = $(this).closest(".i_l_supply").find(".hot_recommend");
				ism_temp.hide();
				ism_temp.eq($(this).index()).show();
			});
			
			$(".entering_brand").bind({
				mouseover: function(){
					eb_flag = false;
				},
				mouseout: function(){
					eb_flag = true;
				}
			});
			
			//新闻资讯栏加载
			var param = {pageNo:1,rows:7,orderBy:'modifyDate',sort:'desc'};
			param.noModule='5';
			param.approved=3;
			$.post('http://www.itmys.com/news/findNewsList.json',param,function(res){
				if(res.total!=0){
					$("#newsList").setTemplateElement("newsTemp");
					$("#newsList").setParam('newsModuleMap', res.newsModuleMap);
					$("#newsList").processTemplate(res.newsList);
				}else{
					$('#newsList').html('');
				}
			});
			
			$.post('http://www.itmys.com/index/indexMainPlants.itm',function(res){
				$('.seedling_supply_left').html(res);
				addImpressionIndex();
				if(userId!=null){
					sendLog(null,userId);
				}else {
					sendLog(null,'unknown');
				}
				
			});
			getMaterialPurchaseDetail();
			quoteCountMap();
			getQuoteUserUnderThreeNumMapIndexMain();
			
			
			//新手引导
			if(document.cookie.indexOf("indexGuide=") == "-1"){
				$(".index_Guide").height($(document).height());
				$(".index_Guide, .ig_step1").show();
				$(".ig_step1Btn1").bind("click", function(){
					$(".ig_step1").hide();
					$(".ig_step2").show();
				});
				$(".ig_step2Btn1").bind("click", function(){
					$(".ig_step2").hide();
					$(".ig_step3").show();
				});
				$(".ig_step3Btn1").bind("click", function(){
					$(".index_Guide, .ig_step3").hide();
				});
				
				var date = new Date();
			    var ms = 24*3600*1000*365;
			    date.setTime(date.getTime() + ms);
				document.cookie="indexGuide=true;expires=" + date.toGMTString(); 
			}
			
		});
		
		//判断新闻是否显示new图标
		function isNewsNew(createDate) {
			var date = '2017-02-08'
			if(createDate.indexOf(" ")>0) {
				var curDate = createDate.substring(0,createDate.indexOf(" "));
				if(curDate>date) {
					return true;
				}else {
					return false;
				}
			}else {
				return false;
			}
		}
		
		//发送曝光日志信息
		function addImpressionIndex() {
			$('div.log_hid').each(function() {
				if(($(this).attr('type'))=='purchase') {
					addImpression('id='+$(this).attr('id'),$(this).attr('index'),$(this).attr('list'));
				}else {
					addImpression('barcode2D='+$(this).attr('id'),$(this).attr('index'),$(this).attr('list'));
				}
				
			});
		}
	</script>
<script>

(function(){
    var bp = document.createElement('script');
    var curProtocol = window.location.protocol.split(':')[0];
    if (curProtocol === 'https') {
        bp.src = 'https://zz.bdstatic.com/linksubmit/push.js';        
    }
    else {
        bp.src = 'http://push.zhanzhang.baidu.com/push.js';
    }
    var s = document.getElementsByTagName("script")[0];
    s.parentNode.insertBefore(bp, s);
})();
</script>
