// JavaScript Document	
function changeSize(n){			
	txt=document.getElementById("xw_box").getElementsByTagName("span");
	txt=document.getElementById("xw_box").getElementsByTagName("p");
	txt2=document.getElementById("xw_box").getElementsByTagName("div");
	txt3=document.getElementById("xw_box").getElementsByTagName("table");
	for(i=0;i<txt.length;i++){
		txt[i].style.fontSize=n;
	}
	for(i=0;i<txt2.length;i++){
		txt2[i].style.fontSize=n;
	}
	for(i=0;i<txt3.length;i++){
		txt3[i].style.fontSize=n;
	}
	
}

m=0;
