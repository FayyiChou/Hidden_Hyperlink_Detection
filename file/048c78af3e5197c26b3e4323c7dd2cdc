	//检查cookie
	if($.cookie("skinID")){
		$("#skin").attr("href","theme2014/css/s" + $.cookie("skinID") + ".css");	
	}
</script>
<script type=text/javascript>
$(function(){
	//点击
	$(".skin-list ul > li").each(function(){
		$(this).click(function(){
			var id = $(this).attr("id");
			var m = 1;
			if(id !== "def"){
				m = parseInt(id.slice(-1))+1;
			}
			var href = "theme2014/css/s" + m + ".css";
			$("#skin").attr("href",href);
			//设置cookie
			$.cookie("skinID",m , { path: '/', expires: 1000 });
		});								
	});		   
});
</script> 
