<script>
$(function(){
	$('.hot-search-arrow').click(function(t){
		if($('.toplist').is(':hidden')){
			$(this).addClass('hot-arrow-up');
			$('.toplist').show();
		}
		else{
			$(this).removeClass('hot-arrow-up');
			$('.toplist').hide();
		}
		t.stopImmediatePropagation();
	})
	$('.toplist').click(function(t){
		t.stopImmediatePropagation();
	})
})
function topList(){
	var popLeft=$('.ipt_serach').offset().left;
	var popTop=$('.ipt_serach').offset().top;
	$('.toplist').css({
		'left':popLeft+'px',
		'top':popTop+'px'
	})
}
$(function(){
	topList();
})
$(window).resize(function(){
	topList();
})
$(document).click(function(){
	$('.toplist').hide();
	$(".hot-search-arrow").removeClass('hot-arrow-up');

})
</script>



