.hot-arrow-up i{ border-width:0 5px 5px; border-color: transparent transparent #fb6f6f; border-style: dashed dashed solid; }
.toplist{ width:475px;position:absolute;z-index:9999; background:#fff;border:2px solid #3e86dc;border-top:0; margin-top:33px; display:none;}
.toplist .list-inner{padding:15px;border-top:1px solid #eee;}
