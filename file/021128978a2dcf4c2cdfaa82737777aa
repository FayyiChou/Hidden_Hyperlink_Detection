  function Marquee3(){
  if(bb2.offsetWidth-bb.scrollLeft<=0)
  bb.scrollLeft-=bb1.offsetWidth
  else{
  bb.scrollLeft++
  }
  }
  var MyMar3=setInterval(Marquee3,speed3)
  bb.onmouseover=function() {clearInterval(MyMar3)}
