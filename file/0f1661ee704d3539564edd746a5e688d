var glide = (function(picFather,picTag,oClass,speed) {
	function $(id,tagname){var ag = arguments.length;return ag==1?document.getElementById(id):typeof id||tagname=="string"?document.getElementById(id).getElementsByTagName(tagname):null;};
	var pic = $(picFather,picTag);
	var record,rnd;
	//var ni = $("no");
	var add = function() {
		rnd = Math.floor(Math.random()*pic.length);
		while(record == rnd) { rnd = Math.floor(Math.random()*pic.length);if(pic.length ==1 ) {break;} }
		for(var i=0;i<pic.length;i++) { pic[i].className = "" };
		pic[rnd].className = oClass;
		record = rnd;
		//ni.innerHTML += record;
	}
	setInterval(add,speed*1000);
})("picChange",'img','on',10);
