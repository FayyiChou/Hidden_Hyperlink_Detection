    <script type="text/javascript">
            var pages = 1 + 1,
                pagesize = 20,
                count = 52939,
                FamilyType = 0,
                FamilyCount = 0,
                FamilyHouse = 1,
                SearchKey = '',
                SearchType = 0;
                WaittingType = 0;

        $(function () {
            $("#sort-menu").hover(function () {
                $(this).children("div").show();
            }, function () {
                $(this).children("div").hide();
            });
            $("#searchkey").val(SearchKey);
            $("famliya").removeClass();
            $("#FamilyCount" + FamilyCount).addClass("cur");
            $("#waitType" + WaittingType).addClass("cur");
            $("#FamilyType" + FamilyType).addClass("cur");
            $("#FamilyHouse" + FamilyHouse).addClass("cur");
            if (FamilyHouse == 0){
                $("#FamilyHouse1").addClass("cur");
            }

            init();
            get_project();

        });

        function Search() {
            SearchKey = $("#searchkey").val();
            SearchType = $("#SelectSpan").attr("TypeId");
            if (SearchKey != "") {
                SearchKey = "?search=" + encodeURIComponent(SearchKey);
            }
            location.href = "/ajpm/1-" + SearchType + "-0-0-0-0/" + SearchKey;
        }

        function init() {
            if (count > 0) {
                if (SearchKey != "") {
                    SearchKey = "?search=" + encodeURIComponent(SearchKey);
                }
                $("#Pagination").pagination(count,
                       { callback: function (page) { },
                           current_page: --pages,
                           first_text: "首页",
                           prev_text: "上一页",
                           next_text: "下一页",
                           link_to: "/ajpm/__id__-" + SearchType + "-" + FamilyCount + "-" + FamilyType + "-" + FamilyHouse + "-" + WaittingType + "/" + SearchKey,
                           last_text: "尾页",
                           num_edge_entries: 2,
                           num_display_entries: 5,
                           items_per_page: pagesize
                       });
            }
        }

        function SelectType(Name) {
            $(".sele-menu-sup li").show();
            $("#SelectSpan").html($(Name).html()).attr("TypeId", $(Name).attr("TypeId"));
            $("#searchkey").attr("placeholder", $(Name).attr("data"));
            $(Name).hide();
        }

        function get_project() {
            $.ajax({
                url: '/Project/open_project',
                success: function (data) {
                    var result = JsonEval(data);
                    for (var i = 0; i < result.length; i++) {
                        var obj = result[i];
                        var str = "";
                        if (obj.source == 1)
                            str = "安居房";
                        if (obj.source == 2)
                            str = "公租房";
                        $("#top_project").append("<a href='http://bol.szhome.com/house/" + obj.id + ".htm' target='_blank' title='" + obj.XMMC + "-" + str + "'>" + obj.XMMC + "</a>");
                    }
                }
            });
        }

        function JsonEval(jsonObj) {
            var str = "";
            try {
                str = eval(jsonObj);
            } catch (err) {
                str = eval("(" + jsonObj + ")");
            }
            return str;
        }
