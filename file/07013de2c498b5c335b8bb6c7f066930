	function goThere() {
    var list = document.forms['departments'].products
        if(list.selectedIndex != 0) {

  document.location.href = list.options[list.selectedIndex].value
	    }
}

/*----- tabs-home -----*/
jQuery(document).ready(function() {
    jQuery('.tabs-home .tab-home-links a').on('click', function(e)  {
		var currentAttrValue = jQuery(this).attr('href');
		// Show/Hide Tabs-home
		jQuery('.tabs-home ' + currentAttrValue).siblings().slideUp(400);
		jQuery('.tabs-home ' + currentAttrValue).delay(400).slideDown(400);
		// Change/remove current tab-home to active-home
		jQuery(this).parent('li').addClass('active-home').siblings().removeClass('active-home');
		e.preventDefault();
    });
});


