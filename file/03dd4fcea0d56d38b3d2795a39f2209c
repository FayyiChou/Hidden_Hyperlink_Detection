<script language='JavaScript' type='text/javascript'>
    function DisplayMenu(index) {
        if (index == 0) {
            tb_fx.style.display = 'block';
        }
        else if (index == 1) {
            tb_more.style.display = 'block';
        }
        else if (index == 3) {
            tb_more.style.display = 'none';
        }
    }
</script>

