<script language="JavaScript" type="text/javascript"> 
var search_type=1;
function findp()
{
    var price=$("#select_price").val();var grade=$("#select_grade").val();var m=""; if(price=="0"&&grade=="0") {alert("请先选择搜索条件！");return;}  
    if(price!="0"){SetCookie("search_p",$("#select_price").find("option:selected").text()); } 
    else {SetCookie("search_p","");price=",";}
    if(grade!="0"){SetCookie("search_g",$("#select_grade").find("option:selected").text());}else SetCookie("search_g","");
    this.location.href="/car/search.aspx?ps="+encodeURIComponent(price)+"&grade="+grade;
}
function findi(){var keys=EnCode(Trim($("#textkeys").val()));SetCookie("search_p",keys);SetCookie("search_g","");if(keys==""||keys=="输入品牌或车系"||keys=="输入关键字") {alert("请输入车型名称或关键词！");return;}if(search_type==1) this.location.href="/car/search.aspx?m="+encodeURIComponent(keys);else 

this.location.href="/news/search.aspx?t=1&m="+encodeURIComponent(keys);}
function search_car() {search_type=1;document.getElementById("textkeys").value="输入品牌或车系";}
function search_news(){search_type=0;document.getElementById("textkeys").value="输入关键字";}
</script></td>
