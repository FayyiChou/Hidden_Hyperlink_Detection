<script>

//焦点图片//
var f1,f1Items;
var f2,f2Items; 
$(document).ready(function(){
    f1Items = {iL:$("#iL1 li"),bL:$("#bL1 a"),fn:"f1"}; 
    f2Items = {iL:$("#iL2 li"),bL:$("#bL2 a"),fn:"f2"};
    f1Items.bL.each(function(i){$(this).bind("click",function(){SF(f1Items,i);});});
    f2Items.bL.each(function(i){$(this).bind("click",function(){SF(f2Items,i);});});
    RF(f1Items);
    RF(f2Items);
});


//焦点开始方法
function RF(setting,index){
    var i = 0;                                                      //下标默认为0
    var fn1 = eval(setting.fn);                                     //获取定时器变量
    if(index!=undefined||index!=null)                               //下标为空默认为0
        i = index;                                                  //赋值下标
    var len = setting.iL.size();                                    //图片区集合
    setting.iL.eq(i).show().siblings().hide();                      //当前图片显示  
    setting.bL.eq(i).addClass("ch").siblings().removeClass("ch");   //当前按钮改变样式
    i=++i%len;                                                      //设置下标+1
    fn1 = setTimeout(function(){RF(setting,i);},3000);              //定时器递归
    eval(setting.fn+"="+fn1);                                       //定时器赋值变量
}
//设置焦点方法
function SF(setting,index){
    var f = eval(setting.fn);                                          //获取定时器变量
    if(f!=undefined&&f!=null)                                          //判断是否为空
        clearTimeout(f);                                               //清楚变量
    setting.iL.eq(index).show().siblings().hide();                     //当前图片显示
    setting.bL.eq(index).addClass("ch").siblings().removeClass("ch");  //当前按钮改变样式
    RF(setting,index);                                                 //开始计时
}
function setTab(name,cursel,n){
for(i=1;i<=n;i++){
var menu=document.getElementById(name+i);
var con=document.getElementById("con_"+name+"_"+i);
menu.className=i==cursel?"hover":"";
con.style.display=i==cursel?"block":"none";
}
}
function update(){callservice("common/Html_index.asmx/html_index","","");}
</script>
