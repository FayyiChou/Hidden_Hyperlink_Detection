  <script>
function tab_bonus(money){
	var tmp_money = money;
	var arrayObj = {100:'写的不错意思下，100点券请笑纳',500:'早餐加个蛋，写死大反派',1000:'既见一千，云胡不喜――摘自湿经',5000:'5000点券拿手中，作者你还不加更？',10000:'100块都给了你，作者好坏好坏哒',100000:'忍把千金酬一笑，毕竟相思不似点券好'};
	var _comment = arrayObj[money];
	 
	$(".def_flag").removeClass("active");
	$(".flag_"+money).addClass("active");
	var num = $('._num').attr("value");
	if(num>0){
		money = money * num;
	}
	$(".price").html(money);
	$("#comment").attr("value",_comment);
	$("#send_price").attr("value",tmp_money);
	$("#box").attr("value",money);
}

function sum_num(num){
	//value.replace(/[^\d]/g,'')
	//var preg = "/^[0-9]*$/";

	var preg = new RegExp("^[0-9]*$");
	var def_val = $("#send_price").attr("value");
	if(!preg.test(num)){
		alert('只能输入数字');
		$('._num').attr("value","1");
		$(".price").html(def_val);
		return;
	}
	if(num<1){
		alert('不能小于1哦');
		$('._num').attr("value","1");
		$(".price").html(def_val);
		return;
	}
	var price = $("#box").attr("value");
	var sum = num * price;
	$(".price").html(sum);
	if(num==''){
		$(".price").html($("#box").attr("value"));
	}
}
function tool_menu_check(show_elem, hide_elem){
	$(show_elem).show();	
	$(hide_elem).hide();	
}

function favorite_ajax(articleid){
	var url = '/book-favorite_ajax?articleid='+articleid;
	$.get( url, function(data){
		if(data == '1'){
			$(".read_tishi_nav p").html("恭喜加入书架成功！");
			$(".fav_book p").html("已加入");
			tool_menu_check('.read_tishi,.mask', '.chapters_list,.read_set');
		}else if(data == '3'){
			$(".read_tishi_nav p").html("已经加入过书架");
			tool_menu_check('.read_tishi,.mask', '.chapters_list,.read_set');
		}else if(data == '6'){
			$(".read_tishi_nav p").html("登录之后才可以加入书架哦");
			tool_menu_check('.read_tishi,.mask', '.chapters_list,.read_set');
		}else{
			$(".read_tishi_nav p").html("加入书架失败");
			tool_menu_check('.read_tishi,.mask', '.chapters_list,.read_set');
		}
	});
}

function send_bonus(articleid){
     var bonus = $("#send_price").attr("value");
	 var num = $("._num").attr("value");
     var comment = $("#comment").attr("value");
	 if( confirm('您确定要打赏作者吗？') ){
		$.ajax({
		   type: "POST",
		   url: "/book-send_bonus_submit",
		   data: "articleid="+articleid+"&bonus="+bonus+"&comment="+comment+"&num="+num,
		   success: function(msg){
			 alert( msg );
		   }
		});
	 }
	return;
}
$(window).scroll(function () {
		//$(window).scrollTop()这个方法是当前滚动条滚动的距离
		//$(window).height()获取当前窗体的高度
		//$(document).height()获取当前文档的高度
		if (($(window).scrollTop()) >= 100) {
			$(".left_list").removeClass("left_float_nav_top_45");
			$(".left_list").addClass("left_float_nav_top_0");
		}else{
			$(".left_list").removeClass("left_float_nav_top_0");
			$(".left_list").addClass("left_float_nav_top_45");
		}
		
});


var Reader = {
	contentId : '.zhangjie p',
	contentText : '#setup_font_size',
	//字体大小相关参数
	sizePId : '#setup_size_max',
	sizeMId : '#setup_size_minus',
	sizeMax : 32,
	sizeMin : 14,
	sizeNum : 2,

	//字体大小设置
	sizeP : function(){
		var size = parseInt( $(Reader.contentId).css("font-size") );
		size = size + Reader.sizeNum;
		Reader.sizeSet(size);
	},
	sizeM : function(){
		var size = parseInt( $(Reader.contentId).css("font-size") );
		size = size - Reader.sizeNum;
		Reader.sizeSet(size);
	},
	sizeSet : function(size){
		if( size < Reader.sizeMin ){
			return ;
		}
		if( size > Reader.sizeMax ){
			return ;
		}
		$(Reader.contentId).css("font-size", size + "px");
		$(Reader.contentText).html(size);
		Reader.set('size', size);
	},
	
	
	//其他
	get : function(key){
		return getCookie(key);
	},
	set : function(key, val){
		setCookie(key, val, 30);
	},
	
	init : function(){
		//判断文本字体大小设置 cookie是否保存 如果存在恢复设置
		Reader.sizeSet(Reader.get('size'));
		$(Reader.sizePId).click(Reader.sizeP);
		$(Reader.sizeMId).click(Reader.sizeM);
	}
}
$(function(){
$(".left_list").addClass("left_float_nav_top_45");
Reader.init();
})

</script> 
