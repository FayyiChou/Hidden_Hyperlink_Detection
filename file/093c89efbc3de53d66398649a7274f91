				notes.removeChild(more_notes_link);
				notes.removeChild(loading);
				notes.innerHTML = notes.innerHTML + notes_html;
			}
		}
		req.open('GET', 'http://nana021.lofter.com/morenotes?postid='+postId+'&offset='+offset,true);
		req.send();
	}
