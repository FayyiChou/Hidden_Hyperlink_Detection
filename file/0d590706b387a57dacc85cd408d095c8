<script>	
	if(isWeiXin()){			
		$("#down_win,#down_and,#down_ios").attr("onclick","openwx()");	
	}else{
		var _href = $("#down_win,#down_and,#down_ios").attr("data");
		$("#down_win,#down_and,#down_ios").attr("href",_href);
	}	
	function openwx(){			
			$(".wxTip").fadeIn("slow").show(2000);  
		}
		function weChat(){			
			$(".wxTip").fadeOut("slow").hide(2000); 
		}	
			
		function popChat()
		{
			$(".wxTip").fadeOut("slow").hide(2000);
		}			
		
		function isWeiXin(){ 
			var ua = window.navigator.userAgent.toLowerCase(); 
			if(ua.match(/MicroMessenger/i) == 'micromessenger'){ 
				return true; 
			}else{ 
				return false; 
			} 
		} 
		$("#down_and,#down_ios").click(function(e){			
			if(isWeiXin()){
				e.preventDefault();
				$(".wxTip").fadeIn(200).show(2000);
				$(".wxTip").one("click",function()
				{
					$(".wxTip").fadeOut(200).hide(2000);
				})
				return false;
			}
		});
		
		
</script>

