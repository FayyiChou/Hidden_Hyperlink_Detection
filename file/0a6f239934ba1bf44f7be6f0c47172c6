jQuery(".sytpxw").slide({ titCell:".num li", mainCell:".pic",effect:"fold", autoPlay:true,trigger:"click",
	//下面startFun代码用于控制文字上下切换
	startFun:function(i){
		jQuery(".sytpxw .txt li").eq(i).animate({"bottom":0}).siblings().animate({"bottom":-36});
		}
});

jQuery(".nltpxw").slide({ titCell:".num li", mainCell:".pic",effect:"fold", autoPlay:true,trigger:"click",
	//下面startFun代码用于控制文字上下切换
	startFun:function(i){
		jQuery(".nltpxw .txt li").eq(i).animate({"bottom":0}).siblings().animate({"bottom":-36});
		}
});

</script>

</html>
