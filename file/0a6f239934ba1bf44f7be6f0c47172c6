function searchDoc3(){
	var vs = document.all("searchString2").value;
	if(vs==null || vs=="undefined" || vs==""){ alert("请先输入查询条件"); return; }
	document.all("searchString2").value = vs;
	document.getElementById("pagenum").value = '2';
	window.open('/lxzbzx/include/ssresult.shtml?pagenum=2&wzSiteId=1&searchString='+vs,'_blank');	
}
function searchDoc2(){
	var vs = document.getElementById("searchString2").value;
	var type = document.getElementById("typeName2").value;
	if(vs==null || vs=="undefined" || vs==""){ alert("请先输入查询条件"); return; }
	document.getElementById("searchString2").value = vs;
	document.getElementById("pagenum").value = '2';
	if(type==null || type=="undefined" || type==""){ 
		window.open('/lxzbzx/include/ssresult.shtml?pagenum=2&wzSiteId=1&searchString='+vs,'_blank');	
	}else{	
		window.open('/lxzbzx/include/gjSsresult.shtml?pagenum=2&wzSiteId=1&'+type+'='+vs,'_blank');
	}	
} 
</script>

