            <script>
function searchDoc1(){
	var vs = document.all("searchString").value;
	if(vs==null || vs=="undefined" || vs==""){ alert("请先输入查询条件"); return; }
	document.all("searchString").value = vs;
	document.getElementById("pagenum").value ='';
	window.open('/lxzbzx/include/ssresult.shtml?wzSiteId=1&searchString='+vs,'_blank');	
}
function searchDoc(){
	var vs = document.getElementById("searchString").value;
	var type = document.getElementById("typeName").value;
	if(vs==null || vs=="undefined" || vs==""){ alert("请先输入查询条件"); return; }
	document.getElementById("searchString").value = vs;
	if(document.getElementById("pagenum").value !=null || document.getElementById("pagenum").value !="undefined" || document.getElementById("pagenum").value !=""){ 
		document.getElementById("pagenum").value ='';
	}
	if(type==null || type=="undefined" || type==""){ 
		window.open('/lxzbzx/include/ssresult.shtml?wzSiteId=1&searchString='+vs,'_blank');	
	}else{	
		window.open('/lxzbzx/include/gjSsresult.shtml?wzSiteId=1&'+type+'='+vs,'_blank');
	}
} 
</script>
