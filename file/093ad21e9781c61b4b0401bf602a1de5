  <script type="text/javascript">
sText = new Array("请输入您要查找的商家名称...");

function changeInputStyle(o) {
     if ( $(o).val()=="请输入您要查找的商家名称..."   ) {
         $(o).val("");	 
     }
  } 
function checkInputStyle(o) {
    if (!$(o).val()) {
         $(o).val(sText);
    }
}
function checkKey()
{
 	if($("#keyword").val()=="请输入您要查找的商家名称...")
	{
 		$("#keyword").focus();		
		return false;
	}
	return true;

}
</script> 
