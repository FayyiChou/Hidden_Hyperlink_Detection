  <script language="javascript">
/*$(".hysm_teb").load("netshop_product.asp?shopid=172701",function(){
																				  
	$(this).removeClass("hysm-load")
})*/
$(".hysm_teb").each(function(i){
 	$(".hysm_teb").eq(i).load("/store/netshop_product.asp?hyj="+$(".hysm_teb").eq(i).attr("powerid")+"&shopid="+$(".hysm_teb").eq(i).attr("shopid"),function(){
		$(".hysm_teb").eq(i).removeClass("hysm-load")
	})
})
$(".classbox a").click(function(){
	$(this).toggleClass("cls")
	$(".list-box .IsT").toggle()
})
$(".blueidea li").live("mouseenter",function(){
	$(this).find("span").stop().animate({opacity:"0.3",top:"90"},300)
})
$(".blueidea li").live("mouseleave",function(){
	$(this).find("span").stop().animate({opacity:"0.9",top:"0"},300)
})
$(".proNbox_r").live("click",function(){
	var blueid=$(this).siblings(".hysm_piclist").find(".blueidea")
	var puleft=parseInt(blueid.css("left"))
	var imgNm=blueid.children().length;
	var npl=imgNm*146+puleft-876
	if(imgNm*146+puleft>1752){
		blueid.stop().animate({left:puleft-876},800)
	}else if(imgNm*146+puleft>876){
		blueid.stop().animate({left:puleft-npl},800)
	}
})
$(".proNbox").live("click",function(){
	var blueid=$(this).siblings(".hysm_piclist").find(".blueidea")
	var puleft=parseInt(blueid.css("left"))
	if(puleft>-876){
		blueid.stop().animate({left:"0"},800)
	}else if(puleft<-876){
		blueid.stop().animate({left:puleft+876},800)
	}
})
</script> 
