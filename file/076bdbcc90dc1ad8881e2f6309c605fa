	<script type="text/javascript">
	  	// 路径配置
	    require.config({
	        paths: {
	            echarts: '/js/source'
	        }
	    });
    	// 循环产品
    	var funddate = "";
    	var funddwjz = "";
    	var fundljjz = "";
    	var newst = "";
    	var reg = new RegExp(",","g");
    	
		$(".fundchart").each(function() {
			var fundDivId = $(this).attr("id");
	        // 使用
	        require(
	            [
	                'echarts',
	                'echarts/chart/line'
	            ],
	            function (ec) {
	            	if (fundDivId != "000681" && fundDivId != "000682" && fundDivId != "000683") {
		            	$.post("/cgi/fundproduct/FundProduct?function=CharpFund&fundid=" + fundDivId + "&startdate=&enddate=", function(data) {
		            		var arr = data.split('|');
		            		
		            		funddate = arr[1];
		            		var array_fund1 = funddate.split(',');
		            		
		            		funddwjz = arr[2];
		            		var array_fund2 = funddwjz.split(',');
		            		
		            		fundljjz = arr[3];
		            		var array_fund3 = fundljjz.split(',');
		            	
			                // 基于准备好的dom，初始化echarts图表
			                var myChart = ec.init(document.getElementById(fundDivId)); 
			                
			                option = {
			                	    tooltip : {
			                	        trigger: 'axis'
			                	    },
			                	    legend: {
			                	        data:['单位净值','累计净值']
			                	    },
			                	    dataZoom : {
			            		        show : true,
			            		        realtime: true,
			            		        start : 0,
			            		        end : 100
			            		    },
			                	    xAxis : [
			                	        {
			                	            type : 'category',
			                	            boundaryGap : true,
			                	            data : array_fund1
			                	        }
			                	    ],
			                	    yAxis : [
			                	        {
								            type : 'value'
								        }
			                	    ],
			                	    series : [
			                	        {
			                	            name:'单位净值',
			                	            type:'line',
			                	            subtextStyle:{
			                	            	color:'#D2213E'
			                	            },
			                	            itemStyle:{
			                	            	normal:{
			                	            		lineStyle:{
			                	            			color:'#D2213E'
			                	            		}
			                	            	}
			                	            },
			                	            data:array_fund2
			                	        },
			                	        {
			                	            name:'累计净值',
			                	            type:'line',
			                	            subtextStyle:{
			                	            	color:'#032274'
			                	            },
			                	            itemStyle:{
			                	            	normal:{
			                	            		lineStyle:{
			                	            			color:'#032274'
			                	            		}
			                	            	}
			                	            },
			                	            data:array_fund3
			                	        }
			                	    ]
			                	};
			                // 为echarts对象加载数据 
			                myChart.setOption(option); 
		            	});
	            } else {
	            	$.post("/fundproduct/doFundNetvaluePage_HBX.jsp?funid=" + fundDivId + "&startdate=&enddate=", function(data) {
	            		/* console.warn( data ); */
	            		var arr = data.split('|');
	            		
	            		funddate = arr[1];
	            		var array_fund1 = funddate.split(',');
	            		
	            		funddwjz = arr[2];
	            		var array_fund2 = funddwjz.split(',');
	            		
	            		fundljjz = arr[3];
	            		var array_fund3 = fundljjz.split(',');
		                // 基于准备好的dom，初始化echarts图表
		                var myChart = ec.init(document.getElementById(fundDivId)); 
		                
		                option = {
		                	    tooltip : {
		                	        trigger: 'axis'
		                	    },
		                	    legend: {
		                	        data:['万分单位收益率','七日年化收益率']
		                	    },
		                	    dataZoom : {
		            		        show : true,
		            		        realtime: true,
		            		        start : 0,
		            		        end : 100
		            		    },
		                	    xAxis : [
		                	        {
		                	            type : 'category',
		                	            boundaryGap : true,
		                	            data : array_fund1
		                	        }
		                	    ],
		                	    yAxis : [
		                	        {
							            type : 'value'
							        }
		                	    ],
		                	    series : [
		                	        {
		                	            name:'万分单位收益率',
		                	            type:'line',
