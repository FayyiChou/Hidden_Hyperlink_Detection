                    }else if(browser.versions.ios){
                        location.href="https://itunes.apple.com/cn/app/id957667081?mt=8";
                    }
                    else{
                        location.href="http://a.app.qq.com/o/simple.jsp?pkgname=com.autohome.autoclub";
                    }
                },500);
                return false;
            })

            $("#close-app").click(function(){
                $(".postDiv").attr("style","margin-bottom:0px;")
                $(".open-app-foot").hide();
            })
        });

        //设置文章内容字体大小
        //16,20,24
        //1正常，2 稍大，3 超大
        function setFont(textSize) {
            switch (textSize) {
                case 1:
                    $(".userCont").css("font-size", 12);
                    setChildFont($(".userCont"), 12);
                    break;
                case 2:
                    $(".userCont").css("font-size", 16);
                    setChildFont($(".userCont"), 16);
                    break;
                case 3:
                    $(".userCont").css("font-size", 20);
                    setChildFont($(".userCont"), 20);
                    break;
                default:
                    break;
            }
        }

        //设置所有孩子节点字体
        function setChildFont(c, fz){
            c.each(function(){
                $(this).children().each(function(){
                    $(this).css("font-size", fz);
                    setChildFont($(this), fz);
                });
            });
        }

        function setColor(){
            $(".userCont span").css("color", "#000");
        }

        //定位当前楼层
        function getFloor() {
            //1=ios, 2=android
            return getFloor2(2);
            //return floor;
        }

        function getFloor2(pm){

            var pageindex = 1;

            var topY = $(document).scrollTop();
            //console.log(topY);
            var floor = 0;
            $(".mark-flow").each(function () {
                //$(this).css("offsetTop");
                floor++;
                if ($(this).offset().top > topY) {
                    return false;
                }
                //console.log($(this).offset().top);
            });

            if (floor == 0) {
                floor = $(".mark-flow").length;  //计算楼数，需要页数
            }

            var fs = (pageindex - 1) * 20 + floor;
            if(pm == 2){
                appgo("floor", fs);
            }
            else{
                return fs;
            }
            //console.log("floor:" + fs);
        }

        window.pvimg = new Image();
        window.pvimg.src = "http://app.club.autohome.com.cn/web/appcount/countforday?event_key=pv-share.html";
    </script>


