    function txfont(sdf)
{
  if (sdf.value.replace(/(^\s*)|(\s*$)/g, "")=="")
  {
     sdf.value="请输入您的运单号，最多30个， 多个单号请换行分隔。输入的数据不能包含空格或其它非数字和字母的字符";
  }
}
function cleanFont(sdf)
{
  if (sdf.value.replace(/(^\s*)|(\s*$)/g, "")=="请输入您的运单号，最多30个， 多个单号请换行分隔。输入的数据不能包含空格或其它非数字和字母的字符")
    {
       sdf.value="";
    }
}

function search()
            {
                var dh = document.all.item("txt_Number").value;
                var pwdReg = /[a-zA-Z0-9]/;
                if(dh.replace(/(^\s*)|(\s*$)/g, "")=="请输入您的运单号，最多30个， 多个单号请换行分隔。输入的数据不能包含空格或其它非数字和字母的字符")
                {
                alert('请输入运单号！');
                return;
                }
                if(!pwdReg.test(dh))
                {
                alert('输入的数据不能包含空格或其它非数字和字母的字符！');
                return;
                }
               
                var danhao = new Array(30);
                danhao[0]=dh.split("\r\n");
                window.open("ExpressInfo.aspx?dh=" + danhao);
            }
    </script>

