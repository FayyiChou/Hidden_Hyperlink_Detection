<script type="text/javascript"> 
var t; 
var showNot=false;
t=setTimeout(function(){egsolUI.showOverlay({target: $("#mwsPromoPP_overlay"),disBodyClick:true,showAtOnce:true}); showNot=true; dcsMultiTrack('DCS.dcsuri', '/RFQ.htm', 'WT.ad','Supp Overlay', 'WT.dl','99','WT.source','', 'WT.test','','WT.cg_n','','WT.cg_s','','WT.tab','');},1000*300); 
window.document.onkeydown=function(){
	if(!showNot){
		clearTimeout(t) ;
		t=setTimeout(function(){egsolUI.showOverlay({target: $("#mwsPromoPP_overlay"),disBodyClick:true,showAtOnce:true}); showNot=true; dcsMultiTrack('DCS.dcsuri', '/RFQ.htm', 'WT.ad','Supp Overlay', 'WT.dl','99','WT.source','', 'WT.test','','WT.cg_n','','WT.cg_s','','WT.tab','');},1000*300) ;
	}
}
window.document.onmousedown = function(){
	if(!showNot){
		clearTimeout(t) ;
		t=setTimeout(function(){egsolUI.showOverlay({target: $("#mwsPromoPP_overlay"),disBodyClick:true,showAtOnce:true}); showNot=true; dcsMultiTrack('DCS.dcsuri', '/RFQ.htm', 'WT.ad','Supp Overlay', 'WT.dl','99','WT.source','', 'WT.test','','WT.cg_n','','WT.cg_s','','WT.tab','');},1000*300) ;
	}
}
</script>

 


