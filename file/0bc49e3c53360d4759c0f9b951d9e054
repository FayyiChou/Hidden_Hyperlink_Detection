<script type="text/javascript">
   $(function () {
       $('#all').click(function () {
           $('#month').removeClass('king_title');
           $('#day').removeClass('king_title');
           $('#all').addClass('king_title');
           $('#all_list').css('display', 'block');
           $('#day_list').css('display', 'none');
           $('#month_list').css('display', 'none');

       });
       $('#day').click(function () {
           $('#month').removeClass('king_title');
           $('#all').removeClass('king_title');
           $('#day').addClass('king_title');
           $('#all_list').css('display', 'none');
           $('#month_list').css('display', 'none');
           $('#day_list').css('display', 'block');
       });

       $('#month').click(function () {
           $('#day').removeClass('king_title');
           $('#all').removeClass('king_title');
           $('#month').addClass('king_title');
           $('#all_list').css('display', 'none');
           $('#day_list').css('display', 'none');
           $('#month_list').css('display', 'block');
       });
   });
</script>

