<script type="text/javascript">
$(document).ready(function(){
    scrolltotop.offset(100,120);
    scrolltotop.init();
    $('.list .vote').click(function(){
    		var button = $(this);
		var prj_name = button.siblings('dt').find('a').text(),
			msg = '您将要投票的软件名是：\n\n' + prj_name + '\n\n最多只能投5票，投票后不可以取消！确定投票？';
		if(!confirm(msg)) return;
		login_and_do(function(){
        	var opt_id = button.attr('data-option');
        	var prj_id = button.attr('data-project');
        	button.attr('disabled','disabled');
			jQuery.ajax({
        		type	: 'POST',
        		cache	: false,
        		url		: '/action/pool/vote_zh_soft',
        		data	: {option:opt_id,project:prj_id},
				dataType: 'json',
        		success	: function(json){
    				button.removeAttr('disabled');
            		if(json.msg) {
            			alert(json.msg);
            		}
            		else if(json.vote>0) {
            			button.prev('.num').find('strong').text(json.vote);
						button.text('已投').addClass('voted');
						$('#vote_tip em.my_vote').text(json.my_vote);
						$('#vote_tip em.can_vote').text(json.can_vote);
            		}
    			},
        		error	: function(html){
					button.removeAttr('disabled');
        			alert("提交数据失败，代码:" +html.status+ "，请稍候再试");
        		}
        	});
		});
    	});
});
</script>
