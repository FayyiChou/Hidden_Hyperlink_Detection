<!--弹出层链接位置-->
<style type="text/css">
#v_urllink {text-align:center;}
#v_urllink a {color:#000000; text-decoration:none;}
#v_urllink a:hover {text-decoration:underline;}
#block {color:#999; text-decoration:none;}
</style>

<script>
//提示窗口
function extendurl(urlobj){
var turl = urlobj;
var linktitle = turl.substr(0,30)+'....';
var ask = '<font color=red>该链接非本站链接，并不保证完全安全，是否继续访问？</font><br />';
var select = '<br /><p id="v_urllink"><a href="'+ turl +'" target="_blank" onclick="hideMenu(\'fwin_dialog\', \'dialog\')"><font color=green>继续访问</font></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:;" onclick="hideMenu(\'fwin_dialog\', \'dialog\')">取消访问</a></p>';
var msg = ask + select;
showDialog(msg,'info',linktitle);
}
//根据类名得到层
function getcls(cls) {
var s = document.getElementsByTagName('div')
var arr = []
for(var i=0;i<s.length;i++) {
if(s[i].className == cls)
arr.push(s[i])
}
return arr;
}
//根据层与TAG名，得到具体元素
var linkobj = '';
function setObjTag(obj,tag) {
for(var i=0;i<obj.length;i++) {
linkobj = obj[i].getElementsByTagName(tag);
var j = 0;
while(j < linkobj.length){
linkcheck(linkobj[j]);
j++;
}
}
}
//为不符合链接规则的元素增加提示窗口
function linkcheck(linkobj) {
var udomain = linkobj.href.split("/");
udomain = udomain[2].split(".");
var j =2;
var urldomain = udomain[1];
while(j < udomain.length) {
urldomain += '.'+udomain[j];
j++;
}
var linklist = 'baidu.com|google.com'.split("|");
if(!in_array(urldomain,linklist) && urldomain != 'com' && '0'!=1) {
linkobj.href = "javascript:extendurl('"+linkobj.href+"')";
linkobj.target = "_self";	
} else if (!in_array(urldomain,linklist) && urldomain != 'com' && '0'==1) {
linkobj.id = 'block';
linkobj.removeAttribute('href');
linkobj.innerHTML = '***此链接已经被加农炮击杀***';
}
}
//取得classname为t_msgfontfix 的层
var linkdiv = getcls('t_msgfontfix');
//检测该层中的超级链接
setObjTag(linkdiv,'a');


</script>

