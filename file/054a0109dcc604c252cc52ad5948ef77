  <script>
	function logout(){
		swal({
			title: "注销提示",
			text: '确定要退出帐号吗?',
			type: "warning",
			showCancelButton: true,
			confirmButtonColor: "#DD6B55",
			confirmButtonText: "确定退出",
			cancelButtonText: "先等一下",
			closeOnConfirm: false,
			closeOnCancel: false
		}, function(isConfirm){
			if (isConfirm) {
				location.href = "/Public/loginout.html";
			} else {
//				location.href = "/Member/index.html";
				location.reload();
			}
		});
	}
	function toupiao(id){
		$.post("/Index/toupiao.html",{id:id},function(v){
			if(v.status==3){
				swal({
					title: "小提示",
					text: v.msg+ '!点击确定进入拍卖场。',
					type: "warning",
					showCancelButton: false,
					confirmButtonColor: "#DD6B55",
					confirmButtonText: "确定",
					closeOnConfirm: false,
					closeOnCancel: false
				}, function(isConfirm){
					location.href= v.url;
				});

			}else if(v.status==1){
				swal('','投票成功','success');
			}else{
				swal('', v.msg,'warning');
			}
		},'json');
	}
	function chujia(payid,gid,pay){
		var payval = $("#"+payid).val();
		var pay = pay>0 ? pay : payval;
		if(!(pay>0)){
			swal('','请输入正确的金额','error');
			return false;
		}
		$.post("/Index/chujia.html",{pay:pay,gid:gid},function(v){
			if(v.status){
				swal('','出价成功','success');
			}else{
				swal('', v.info,'warning');
				if(v.info=='请登录后出价'){
					setTimtout(function(){
						location.href = v.url;
					},2000);
				}
			}
		},'json');
	}
	function getbaojia_list(){
		if(getbaojia){
			getbaojia();
		}
	}
	$.post("/Lunxun/koupiao.html");
</script> 
