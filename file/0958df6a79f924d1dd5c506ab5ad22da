							{@/if}
							{@/if}
							{@if user.userId&amp;&amp;it.userId!=user.userId&amp;&amp;uid==user.userId}
							&lt;li&gt;&lt;a href="javascript:;" title="隐藏这条微博" action-type="exclude" action-data="{{it.id}}"&gt;隐藏&lt;/a&gt;&lt;/li&gt;
							{@/if}
							{@if user.userId&amp;&amp;it.userId!=user.userId&amp;&amp;uid==user.userId}
							&lt;li&gt;
								&lt;a href="javascript:;" title="不再显示来自{{it.userName}}的消息" action-type="exclude" action-data="{{it.id}},{{it.userId}}"&gt;屏蔽悦友&lt;/a&gt;
							&lt;/li&gt;
							{@/if}
							{@if user.userId&amp;&amp;it.userId==user.userId}
							&lt;li&gt;&lt;a action-type="remove" href="javascript:;" action-data="{{it.id}}"&gt;删除&lt;/a&gt;&lt;/li&gt;
