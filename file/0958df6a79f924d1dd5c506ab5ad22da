<script>
	function statis (value) {
		if(value){
			var _Time = new Date().getTime();
			var _img = new Image();
			var _url = window.location.host;
			var _srcGif = 'http://stats.yinyuetai.com/stats.gif';
			var _src = _srcGif + '?s=' + value + '&t=' + _Time + '&from=' + _url;
			_img.src = _src;
			document.body.appendChild(_img);
			setTimeout(function(){
				document.body.removeChild(_img);
			}, 1000)
		}
	}
</script>
