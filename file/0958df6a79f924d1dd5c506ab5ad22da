						&lt;em class="c_9 reply"&gt;(&lt;span name="parentTotalComments"&gt;{{it.parentPost.commentNums}}&lt;/span&gt;)&lt;/em&gt;&lt;/a&gt;
				&lt;/div&gt;
				{@/if}
			&lt;/div&gt;
		&lt;/div&gt;
		{@/if}
		&lt;div class="feed_dinfo_oper clearfix"&gt;
			&lt;div class="fl"&gt;
				&lt;span class="c_9"&gt;
					&lt;a class="c_996" href="/{{it.userId}}/feed/detail?id={{it.id}}"&gt;{{it.frienddate}}&lt;/a&gt;
					来自
					&lt;a href="#" class="c_996"&gt;{{it.source}}&lt;/a&gt;
				&lt;/span&gt;
			&lt;/div&gt;
			&lt;div class="fr"&gt;
				&lt;a action-type="forward" href="javascript:void(0)" class="c_6 mr_l25 reply" action-data="{{it.id}},{{it.userId}},{{it.parentId}},{{it.parentUserId}},{{it.userName}},{{it.parentPost&amp;&amp;it.parentPost.userName}},{{it.oriMessage|commaEncode}}"&gt;转发
					&lt;em class="c_9 reply"&gt;(&lt;span name="totalCopys"&gt;{{it.copyNums}}&lt;/span&gt;)&lt;/em&gt;&lt;/a&gt;
				&lt;em class="arrow" style="display: none;"&gt;&lt;/em&gt;
				&lt;a action-type="comment" href="javascript:void(0)" class="c_6 mr_l25 reply" action-data="{{it.id}},{{it.userId}},{{it.parentId}},{{it.parentUserId}}"&gt;评论
					&lt;em class="c_9 reply"&gt;(&lt;span name="totalComments"&gt;{{it.commentNums}}&lt;/span&gt;)&lt;/em&gt;&lt;/a&gt;

				&lt;div class="com_more"&gt;

					&lt;a href="javascript:;" class="ico_com_more c_6 reply"&gt;更多&lt;/a&gt;

					&lt;div class="p_com_name"&gt;
						&lt;ul&gt;
							{@if preview &amp;&amp; uid==user.userId}
							{@if !it.top}
							&lt;li&gt;&lt;a href="javascript:;" title="置顶" action-type="set_top" action-data="{{it.id}}"&gt;置顶&lt;/a&gt;&lt;/li&gt;
							{@else}
