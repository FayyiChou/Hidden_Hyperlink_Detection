<script type="text/javascript">
	window.onload = function() {
		var urls = window.TRACK_SHOW;
		if (!urls || !urls.push) {
			urls = [];
		}
		urls.push("/timeline/get-time-line");
		for (var i = 0, len = urls.length; i < len; i++) {
			var url = urls[i];
			if (url.indexOf("?") < 0) {
				url += "?" + (new Date()).getTime();
			} else {
				url += "&" + (new Date()).getTime();
			}
			var img = new Image();
			img.src = url;
			img.width = 1;
			img.height = 1;
			document.body.appendChild(img);
		}
	};
</script>



