							{@if user.userId&amp;&amp;it.userId!=user.userId}
							&lt;li&gt;&lt;a href="javascript:;" title="举报" action-type="report" action-data="{{it.id}},{{it.userId}}"&gt;举报&lt;/a&gt;&lt;/li&gt;
							{@/if}
