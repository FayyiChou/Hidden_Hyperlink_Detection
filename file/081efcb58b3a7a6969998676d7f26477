<script type="text/javascript">
	$(document).ready(function(){
	    $.post('data1.html?act=new_search',function(a){
			var banner1 = "";
			var drink1 = "";
			var food1 = "";
			var goods1 = "";
			var coffee1 = "";
			var picList1 = "";
			var p_jesadd4 = "";
	
			$.each(a.banner_search,function(){
				p_jesadd4 += '<li><a href='+this.url+' style="display:block; height:460px; background: url('+this.pic_path+') no-repeat center top; "></a></li>';
			});
			$('ul.p_jesadd1').append(p_jesadd4);
			jQuery(".slideBox").slide({ mainCell:".bd ul",effect:"leftLoop",vis:1,scroll:1,autoPlay:true});
	
			$.each(a.top_search,function(){
				picList1 +='<li><a href="shop.html?shop_no='+this.shop_no+'"><img src=/images/shop/'+this.shop_pic_path+'><p><b><label>'+this.shop_name+'</label><i></i></b></p></a></li>';
			})
			
			$('ul.picList').append(picList1);
			jQuery(".leftLoop").slide({ mainCell:".bd ul",effect:"leftLoop",vis:3,scroll:3,autoPlay:true});	
			
			drink1 = '<a class="add pabs" href="drink.html?SeriesNo=1" style="display:none;"></a><strong>DRINK</strong><span>饮品</span><a href="drink.html?SeriesNo=1"><img style="width: 200px;height: 200px;" src=/images/'+a.drink_search.pic_path+'></a><p style="display:none;">'+a.drink_search.goods_name+'</p>';
			$('div.drink').append( $(drink1) );
			
			food1 +='<a class="add pabs" href="drink.html?SeriesNo=2" style="display:none;"></a><strong>FOOD</strong><span>食品</span><a href="drink.html?SeriesNo=2"><img style="width: 200px;height: 200px;" src=/images/'+a.food_search.pic_path+'></a><p style="display:none;">'+a.food_search.goods_name+'</p>';
			$('div.food').append(food1);
			
			goods1 +='<a class="add pabs" href="drink.html?SeriesNo=3" style="display:none;"></a><strong>PRODUCTS</strong><span>商品</span><a href="drink.html?SeriesNo=3"><img style="width: 200px;height: 200px;" src=/images/'+a.goods_search.pic_path+'></a><p style="display:none;">'+a.goods_search.goods_name+'</p>';
			$('div.goods').append(goods1);
		
			var goods_name = a.coffee_search.goods_name;
			goods_name = goods_name.replace('?', '™');
			coffee1 +='<a class="add pabs" href="drink.html?SeriesNo=4" style="display:none;"></a><strong>COFFEE BEANS</strong><span>咖啡豆</span><a href="drink.html?SeriesNo=4"><img style="width: 200px;height: 200px;" src=/images/'+a.coffee_search.pic_path+'></a><p style="display:none;">'+goods_name+'</p>';
			$('div.coffee').append(coffee1);
	    },'json');
	})
