								var pics = '';
								var links = '';
								var texts = '';
							
								function ati(url, img, title)
								{
									if(pics != '')
									{
										pics = "|" + pics;
										links = "|" + links;
										texts = "|" + texts;
									}
							
									pics = escape(img) + pics;
									links = escape(url) + links;
									texts = title + texts;
								}
