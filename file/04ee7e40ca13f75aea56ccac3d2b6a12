    $('.footer-plates').hide();
    $('.footer-select').click(function(){
        $('.footer-plates').hide();
        $('.'+$(this).attr('plates')+'-plates').show();
    });
    $('.footer-infos').mouseleave(function(){
        setTimeout(function(){$('.footer-plates').hide();},500);
    });
})(jQuery)

//统一登录退出功能添加history
;(function($) {
    var base64CurrentUrl = "aHR0cDovL3N5LmZhbmcuYW5qdWtlLmNvbS9sb3VwYW4veGlhbmdjZS0yNDMxNzIveWJqLmh0bWw/ZnJvbQ==";
   $(document).ready(function() {
        var href;
        if(base64CurrentUrl) {
            $("body").on("click.logout", "a", function(e) {
                var elm = $(this);
                if(elm.html() == "退出" && (href = elm.attr("href")) ) {
                    e.preventDefault();
                    window.location.href = href + (href.indexOf("?") >= 0 ? "&" : "?") + "history=" + base64CurrentUrl;
                }
            });
        }
    }); 
})(jQuery);
