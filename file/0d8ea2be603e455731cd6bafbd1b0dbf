  <script>
	$(document).ready(function(){
	var userLoginId = getUserLoginId();
	//获取当前index，改变图标显示
	var current_index = $('#current-ft-bt-index').val();
	if(current_index){
		var i = parseInt(current_index);
		if(!isNaN(i)){
			var li = $('.footer-nav li');
			var current_li = li[i-1];
			if(current_li){
				var current_a = $(current_li).find('a');
				if(current_a && current_a.addClass){
					current_a.addClass('current');
				}
			}
			//点击底部切换图片，改变文字样式
			$('.footer-nav li:eq(0)').bind('touchstart',function(){
				$("#footer_home").removeClass('home1').addClass('home2');
				$(this).find('a').addClass('current');
			});
			$('.footer-nav li:eq(1)').bind('touchstart',function(){
				$("#footer_shop").removeClass('shop1').addClass('shop2');
				$(this).find('a').addClass('current');
			});
			$('.footer-nav li:eq(2)').bind('touchstart',function(){
				$("#footer_center").removeClass('center1').addClass('center2');
				$(this).find('a').addClass('current');
			});
			$('.footer-nav li:eq(3)').bind('touchstart',function(){
				$("#footer_eye").removeClass('eye1').addClass('eye2');
				$(this).find('a').addClass('current');
			});
			//除了当前页对应的图片，松开点击取消样式
			if(i == 1){
				$("#footer_home").removeClass('home1').addClass('home2');
				shoppingReset();
				userReset();
				eyeReset();
			}else if(i == 2){
				$("#footer_shop").removeClass('shop1').addClass('shop2');
				homeReset();
				userReset();
				eyeReset();
			}else if(i == 3){
				$("#footer_center").removeClass('center1').addClass('center2');
				homeReset();
				shoppingReset();
				eyeReset();
			}else if(i == 4){
				$("#footer_eye").removeClass('eye1').addClass('eye2');
				homeReset();
				shoppingReset();
				userReset();
			}
		}
	}
	//生成一个时间戳
	var timestamp = new Date().getTime();
	//console.log(timestamp);
	//把a标签的链接最后加上时间戳
	$("ul.footer-nav li a").each(function(){
		var url = $(this).attr("href");
		url+="?_t="+timestamp;
		$(this).attr("href",url);
	});
		srvMap.add("cart-count", "", "front/sh/shopCart!queryCountByCustId?custId=8888&uid=sc002");
        srvMap.add("user-count", "http://10085.cn:20500/wemall-html/mallC/assets/data/order/all.json","front/sh/order!querySubOrderCountForWeChat?uid=o004&odrStsCd=1042");
		if(userLoginId){
		Util.ajax.postJson(srvMap.get("cart-count"), "", function(data, flag){
			if(data && data.bean && data.bean.totalMcds){
				var count = data.bean.totalMcds;
				if(count>0){
					$('#J_cart_count').html(count).show();
				}
			}
		});
		Util.ajax.postJson(srvMap.get("user-count"), "", function(data, flag){

			if(data && data.bean && data.bean.total){
				var count = data.bean.total;
				if(count>0){
					$('#J_user_count').show();
						var li = $('.footer-nav li');
			            var current_li = li[3];

						if(current_li){
							var current_a = $(current_li).find('a');
							if(current_a && current_a.addClass){
								$('#J_user_count').addClass('current');
							}
						}
				}
			}
		});
		}

    });
	function homeReset(){
		$('.footer-nav li:eq(0)').bind('touchend',function(){
			$("#footer_home").removeClass('home2').addClass('home1');
			$(this).find('a').removeClass('current');
		});
	}
    function shoppingReset(){
		$('.footer-nav li:eq(1)').bind('touchend',function(){
			$("#footer_shop").removeClass('shop2').addClass('shop1');
			$(this).find('a').removeClass('current');
		});
    }
	function eyeReset(){
		$('.footer-nav li:eq(3)').bind('touchend',function(){
			$("#footer_eye").removeClass('eye2').addClass('eye1');
			$(this).find('a').removeClass('current');
		});
	}
	function userReset(){
		$('.footer-nav li:eq(2)').bind('touchend',function(){
			$("#footer_center").removeClass('center2').addClass('center1');
			$(this).find('a').removeClass('current');
		});
	}
  </script>
  <!-- 底部导航 end -->
