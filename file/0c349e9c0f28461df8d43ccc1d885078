		function cc (corrCon,corrNm,thiss){ 
			  $(thiss).live('click',function() {
                    var userName = $("#hiddens").val();
                    if(userName==""||userName==null){
                    	layer.confirm("对不起，您还未登录!", {
                    		btn : ['登录', '取消'], // 可以无限个按钮
                    		shift:false,
                    		yes : function(index, layero){	
                    			window.location.href = "/login"
                    		},cancel : function(index, layero){
                    			layer.close(index);
                    		}
                    	});
                    }else{
                    	$("#corrNm").text(corrNm) ;
                    	$("#corrCon").text(corrCon);
                    	var urleorr = $("#err").html();
	                    layer.open({
	                        type: 1,
	                        title: '参数纠错',
	                        shadeClose: false,
	                        // 点击遮罩关闭层
	                        btn: ["确定", "取消"],
	                        area: ['515px', '270px'],
	                        shift:false,
	                        content: urleorr,
	                        success: function(layero, index){
	                        	var divs=layero.find(".divimg");
	                        	$(divs).click(function(){
	                        		var imgSrc = layero.find("#imgObj");
	                        		var urlsrc = imgSrc.attr("src");
	                        		imgSrc.attr("src", chgUrl(urlsrc)); 
	                        	});
	                        	function chgUrl(urlsrc){
	                        		var timestamp = (new Date()).valueOf();
                        			urlurl = urlsrc.substring(0, 17);
	                        		if ((urlsrc.indexOf("?") >= 0)) {
	                        			urlurl = urlsrc + "&t=" + timestamp;
	                        		} else {
	                        			urlurl = urlsrc + "?t=" + timestamp;
	                        		} 
	                        		return urlurl;
	                        	}
	                        	$(divs).click();
	                        },
	                        yes: function(index,layero){
	                        	var corrConS = layero.find("#corrCon").val();
	                        	var corrNmS = layero.find("#corrNm").text();
	                        	var corrWsiteCon = layero.find("#corrWsiteCon").val();
	                        	if (corrConS == "" || corrConS == null) {
	                           	 layer.alert("纠错内容不能为空!",{
	                           		shift:false,
	                           		icon: 2
	                           	 });
	                               return false;
	                           }
	                        	var veryCode = layero.find("#veryCode").val();
	                        	if (veryCode == null || veryCode == "") {
	                                layer.alert("请输入验证码!",{
	                                	shift:false,
	                                	icon: 7
	                                });
	                                return false;
	                            }else{
	                            	$.ajax({ 
	                            		type: "post",
	                            	    url: "/ResultServlet",
	                            	    data: "veryCode=" + veryCode,
	                            	    dataType: "json",
	                            	    success: function(d) {
	                            	    	 if (d.success == true) { 
	                            	    		 $.ajax({
	                            	    			url: '/saveEorr',
	                             	                data: {
	                             	                    corrCon: corrConS,
	                             	                    corrNm: corrNmS,
	                             	                    corrWsiteCon: corrWsiteCon,
	                              	                    corrNo: 'CIM0003770552',
	                              	                    regerNm: ''
	                             	                },
	                             	                type: "post",
	                             	                dataType: "json", 
	                             	                success: function(d) {
	                             	                	if (d.success == true) { 
	                             	                			layer.closeAll();
	                             	                			layer.alert("感谢您的纠错，后台会进行审核，审核通过后会奖励您5个科创币！",{icon: 6, title:'提示'},function(index){
	                             	                				layer.close(index);
	                             	                			}) 
    	                    								}else{
    	                    									layer.alert("网络异常,请稍等~~!",{
    	                 		                                	shift:false,icon: 5
    	                 		                                });
    	                 		                                return false;
    	                    								}
	                             	                	} 
	                            	    		 });
	                            	    	 }else{
	                            	    		 layer.alert("验证码不正确!",{
	     		                                	shift:false,
	     		                                	icon: 2
	     		                                });
	     		                                return false; 
	                            	    	 }
	                            	    }
	                            	});
	                            }
	                        }, 
	                        cancel: function(index){ 
	                            parent.layer.close(index); //执行关闭
	                        }
	                    });
                    }  
                });
		}  
		$(".sidenav ul li").click(function() { 
		    var index = $(this).index();
			$(this).children("a").css("color:","#288D41"); 
			var hei = $("#menu" + (index + 1)).offset().top;
				$("html,body").stop().animate({
					scrollTop :(hei-5) 
				}, 500);  
		}); 
		$("#produceimg").click(function() {   
			  layer.open({
	                type: 1, 
	                title: false,
	                shadeClose: false, //点击遮罩关闭层 
	                area: ['520px', '520px'],
	                content: '<img style="margin-left:10px;margin-top:10px;" src="/fciImg?seqId=3770552" />' 
	             }) 
		}); 
		bro = $.browser;
		if (bro.msie && bro.version <= 9.0) {
	        $("#moni").hide();
	        $("#cnmrgif").hide();
	        $("#hnmrgif").hide();
	        $("#msgmoni").show(); 
	    } else {
	    	var zhSnm='苯乙酸异丁酯';
	    	 $("#moni").show();
	    	 $("#msgmoni").hide();
	    	 var resultStr=zhSnm.replace(/[\r\n]/g,"");//去掉回车换行  
	    	 $.ajax({
				url : "/fciNmr",
				data : {
					zhSnm : resultStr,
					casNo : '102-13-6',
					seqId : '3770552',
				},
				type : "post",
				dataType : "json",
				success : function(d) {
					if(d.jcamp1 == ''){
						$("#jcamp1").html("氢谱不能进行计算模拟");
					}else{
						$("#hnmrgif").hide();
						$("#nrmscript").html("<div style=\"border:1px solid;text-align: center;padding:10px;padding-top:10px\">" +
								"<script type=\"text/javascript\">" + 
								"var jcamp = " + d.jcamp1 + ";" +
								"var observerNMR = new ChemDoodle.PerspectiveCanvas('HNMR', 650, 360);" +
								  "observerNMR.specs.plots_showYAxis = false;" +
								  "observerNMR.specs.plots_showIntegration = true;" +
								  "observerNMR.specs.plots_flipXAxis = true;" +
								  "var spectrumJcampFile = jcamp;" +
								  "var spectrum = ChemDoodle.readJCAMP(spectrumJcampFile);" +
								  "observerNMR.loadSpectrum(spectrum);" +
							"<\/script></div>").hide();
					}
					if(d.jcamp == ''){
						$("#jcamp").html("炭谱不能进行计算模拟");
					}else{
						$("#cnmrgif").hide();
						$("#nrmscript1").html("<div style=\"border:1px solid;text-align: center;padding:10px;padding-top:10px\">" +
								"<script type=\"text/javascript\">" + 
								"var jcamp = " + d.jcamp + ";" +
								"var observerNMR = new ChemDoodle.PerspectiveCanvas('CNMR',650, 360);" +
								  "observerNMR.specs.plots_showYAxis = false;" +
								  "observerNMR.specs.plots_showIntegration = true;" +
								  "observerNMR.specs.plots_flipXAxis = true;" +
								  "var spectrumJcampFile = jcamp;" +
								  "var spectrum = ChemDoodle.readJCAMP(spectrumJcampFile);" +
								  "observerNMR.loadSpectrum(spectrum);" +
							"<\/script></div>").hide();
					}
				}
			});
	    } 
	});
</script> 
  <script type="text/javascript">
	$(window).scroll(function() {
		var sTop = $(window).scrollTop(); 
		var top=$(".detailright").offset().top;
		if(sTop<=(top-10)){
			$(".detailleft").css("position","absolute").css("top",top); 
		}else{
			$(".detailleft").css("position","fixed").css("top","40px");  
		}
		var numl = $('#menu1').offset().top;
		var num2 = $('#menu2').offset().top;
		var num3 = $('#menu3').offset().top;
		var num4 = $('#menu4').offset().top;
		var num5 = $('#menu5').offset().top;
		var num6 = $('#menu6').offset().top;
// 		var num7 = $('#menu7').offset().top;
// 		var num8 = $('#menu8').offset().top;
		//var num9 = $('#menu9').offset().top;
		
		if (sTop >= numl) {
			$(".sidenav ul li:eq(0) a").css("color","#288D41");
			$(".sidenav ul li:eq(0)").siblings().each(function(index, element) {
					$(this).children('a').css("color","#444");
			});
		}
		if (sTop >= num2- 50 ) {
			$(".sidenav ul li:eq(1) a").css("color","#288D41");
			$(".sidenav ul li:eq(1)").siblings().each(function(index, element) {
				$(this).children('a').css("color","#444");
			});
		}
		if (sTop >= num3 - 50) {
			$(".sidenav ul li:eq(2) a").css("color","#288D41");
			$(".sidenav ul li:eq(2)").siblings().each(function(index, element) {
				$(this).children('a').css("color","#444");
			});
		}
		if (sTop >= num4- 50) {
			$(".sidenav ul li:eq(3) a").css("color","#288D41");
			$(".sidenav ul li:eq(3)").siblings().each(function(index, element) {
				$(this).children('a').css("color","#444");
			});
		}
		if (sTop >= num5- 50) {
			$(".sidenav ul li:eq(4) a").css("color","#288D41");
			$(".sidenav ul li:eq(4)").siblings().each(function(index, element) {
				$(this).children('a').css("color","#444");
			});
		}if (sTop >= num6- 50) {
			$(".sidenav ul li:eq(5) a").css("color","#288D41");
			$(".sidenav ul li:eq(5)").siblings().each(function(index, element) {
				$(this).children('a').css("color","#444");
			});
		}	
	});	
// 		if (sTop >= num5- 50) {
// 			$(".sidenav ul li:eq(4) a").css("color","#288D41");
// 			$(".sidenav ul li:eq(4)").siblings().each(function(index, element) {
