  <script type="text/javascript">
$(function() {
    bro = $.browser;
    $("#search-content").keyup(function() {
        var reg = new RegExp("^[1-9]+[0-9]+[-]{1}"); //构造一个含有目标参数的正则表达式对象
        var r = $(this).val().match(reg); //匹配目标参数 
        var zhSnm;
        if ($(".select").attr("data-id")==1)  
       	 if (r != null) {
            $.ajax({
                url: '/findCas',
                data: {
                    "casNo": $(this).val()
                },
                dataType: "json",
                type: "GET",
                traditional: true,
                success: function(json) {
                    if (json.length > 0) {
                        $("#auto_content").html("");
                        $("#auto_content").show();
                        $("#auto_content").css("z-index", "999");
                        for (var i = 0; i < json.length; i++) {
                            str = json[i].zhSnm;
                            if (str != "") {
                                zhSnm = str.substring(0, 30);
                            }
                            $("#auto_content").append("<li data='" + json[i].casNo + "'><span>"+json[i].casNo+"("+zhSnm+")"+"</span></li>")
                        }
                    } else {
                        $("#auto_content").html("");
                        $("#auto_content").hide();
                        $("#auto_content").css("z-index", "");
                    }
                }
            });
        } else {
            $("#auto_content").html("");
            $("#auto_content").hide();
        } 
    });
    $("#auto_content li").live('click',function(index) {
        $("#search-content").val($(this).attr("data"));
        var cur =$(".select").attr("data-id");
        $("#auto_content").hide();
        var content = $("#search-content").val(); 
        //window.location.href='/fhead?content=' +content+ '&cur=' + encodeURI(cur); 
        window.location.href='/fhead/'+content+'_'+cur+'.html'; 
    })  
}) 
</script>
