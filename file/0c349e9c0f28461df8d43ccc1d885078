                 $(this).children("em.edit-none").show();
            },
            function() {
                 $(this).children("em.edit-none").hide();
            }); 
		}); 
	    $.ajax({
	        url: "/fciMstInfo",
	        data: {
	            ciMstNo: 'CIM0003770552'
	        },
	        type: "post",
	        dataType: "json",
	        success: function(d) {
//				$("#menu1").html(d.menu1);
	            $("#menu2").html(d.menu2);
	            $("#menu3").html(d.menu3);
	            $("#menu4").html(d.menu4);
	            $("em.edit-none").hide();
	            $(".eorrDiv").each(function() {
	                var $dsd = $(this).parent();
	                $dsd.each(function() {
	                    $(this).hover(function() {
	                        $(this).children().children("em.edit-none").show();
	                    },
	                    function() {
	                        $(this).children().children("em.edit-none").hide();
	                    });
	                });
	            });
	            $("em.edit-none").each(function() {
	                $(this).hover(function() {
	                    $(this).removeClass("edit-none").addClass("edit-block");
	                },
	                function() {
	                    $(this).removeClass("edit-block").addClass("edit-none");;
	                }); 
	                var corrCon = $(this).prev().text();
	                var corrNm = $(this).parent().parent().prev().children().text(); 
	                var corrNmspan = $(this).prev().prev("span").text();  
	                if(corrCon==null||corrCon=="" ){
	    				  corrCon = $(this).parent().children("p").eq(1).text();//内容
	    			}
	    			if(corrNm==null||corrNm==""){
	    				corrNm=$(this).parent().children("p").eq(0).text();//要修改的列名
	    			}
	    			if(corrNmspan!=""){
	    				corrCon=corrNmspan;
	    			}
	                var thiss=$(this);
	       			cc(corrCon,corrNm,thiss)
	            }); 
	        } 
	    });  
