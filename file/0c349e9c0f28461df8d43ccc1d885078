  <script>
 $(function(){
	 $(".nsdivhide").each(function(){ 
		var divheight=$(this).height(); 
		var divtext=$(this).text().length; //获得字符串长度  
		if(divheight>70|| divtext>140){
			$(this).siblings(".chaoheight").show(); 	
		}
})
$(".chaoheight").each(function(){
	$(this).toggle(function(){ 
		$(this).siblings(".nsdivhide").show();
		$(this).siblings("label").hide(); 
	},function(){
		$(this).siblings(".nsdivhide").hide();
		$(this).siblings("label").show(); 
		});
	})  
})
</script> 
