		function getUrl(name){
	    	var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)', 'i');
		    var r = window.location.search.substr(1).match(reg);
		    if (r != null) {
		        return unescape(r[2]);
		    }
		    return null;
	    }
	    var source = getUrl('isJump');
	    var redi = location.host == 'm.58supin.com';
		if(isiOS && redi && !source){
			var param = location.search;
			location.replace('//www.jump.supin.58.com/mlist/redirect'+param);
		}
	</script>
