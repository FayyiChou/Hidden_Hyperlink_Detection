.select{
	color:#fff;
	background-image:url(images/index_27.gif)
}
#qiehuanneirong_2{
	display:none;
}
	</style>
	<script>
	function qiehuan(x,y){
$("#qiehuan_"+x).addClass("select");
$("#qiehuan_"+y).removeClass("select");
$("#qiehuanneirong_"+x).show();
$("#qiehuanneirong_"+y).hide();

if(x==1){
$("#dangfengzhengfeng_href").attr("href","erji.asp?bm=dangfengzf&lmm=党风政风");
}
if(x==2){
$("#dangfengzhengfeng_href").attr("href","erji.asp?bm=jilvshenc&lmm=纪律审查");
}
if(x==3){
$("#zhengcefg_href").attr("href","erji.asp?bm=paizhuck&lmm=派驻窗口");
}
if(x==4){
$("#zhengcefg_href").attr("href","erji.asp?bm=zhengcefg&lmm=政策法规");
}
if(x==5){
$("#lianzhengwh_href").attr("href","erji.asp?bm=lianzhengwh&lmm=廉政文化");
}
if(x==6){
$("#lianzhengwh_href").attr("href","erji.asp?bm=duiwujs&lmm=队伍建设");
}
if(x==7){
$("#tashanzs_href").attr("href","erji.asp?bm=tashanzs&lmm=他山之石");
}
if(x==8){
$("#tashanzs_href").attr("href","erji.asp?bm=qingfenglt&lmm=清风论坛");
}


}
	
	
	</script>
	
