<script  type="text/javascript">
var jj=1;
function gettu(stats){
if(stats=='next' && jj<4){
if(document.getElementById('items_'+jj).style.display=='block'){
document.getElementById('items_'+jj).style.display='none';
document.getElementById('items_'+(jj+1)).style.display='block';
document.getElementById('num').innerHTML=(jj+1);
jj++;
}
}else if(stats=='next' && jj>=4){
if(document.getElementById('items_4').style.display=='block'){
document.getElementById('items_4').style.display='none';
document.getElementById('items_1').style.display='block';
document.getElementById('num').innerHTML=1;
jj=1;
}
}else if(stats=='pre' && jj>1){
if(document.getElementById('items_'+jj).style.display=='block'){
document.getElementById('items_'+jj).style.display='none';
document.getElementById('items_'+(jj-1)).style.display='block';
document.getElementById('num').innerHTML=(jj-1);
jj--;
}
}else if(stats=='pre' && jj<=1){
if(document.getElementById('items_1').style.display=='block'){
document.getElementById('items_1').style.display='none';
document.getElementById('items_4').style.display='block';
document.getElementById('num').innerHTML=4;
jj=4;
}
}
}
function addfavorite(){
if (document.all){
window.external.addFavorite('http://www.shanxii.com/','陕西新媒体圈');
}else if (window.sidebar){
window.sidebar.addPanel('陕西新媒体圈', 'http://www.shanxii.com', "陕西新媒体圈");
}
}
function SetHome(obj){
try{
obj.style.behavior='url(#default#homepage)';
obj.setHomePage('http://www.shanxii.com/');
}catch(e){
if(window.netscape){
try{
netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
}catch(e){
alert("抱歉，此操作被浏览器拒绝！nn请在浏览器地址栏输入“about:config”并回车然后将[signed.applets.codebase_principal_support]设置为'true'");
};
}else{
alert("抱歉，您所使用的浏览器无法完成此操作。nn您需要手动将'http://www.shanxii.com/'设置为首页。");
};
};
}

