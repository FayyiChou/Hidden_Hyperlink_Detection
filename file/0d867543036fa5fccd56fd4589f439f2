<script type="text/javascript">
	function replaceTel() {
		var url = window.location.href;
		var begin = url.indexOf('?') + 1;
		var end   = url.indexOf('=');
		var proto = url.substring(begin,end);

		var flag = false;

		if(proto == 'bdclkid' || proto == 'gclid'){
			flag = true;
		}

		if (flag) {
			document.getElementById("head_tel010").style.display="none";
			document.getElementById("head_tel400").style.display="block";

			document.getElementById("foot_tel010").style.display="none";
			document.getElementById("foot_tel400").style.display="block";
		}
	}
</script>
