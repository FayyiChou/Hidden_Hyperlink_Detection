  <script>

        $(function() {
            saveRecommend();
        });

        function saveRecommend() {
            $('.goRecommend').click(function() {
                var data = $('#form_recommend').serialize() + "&business=" + $('.recommend-selected').find('p').html();
                var form = $('#form_recommend');
                var flag = form.validationEngine('validate', { scroll: false });
                if (flag) {
                    $.ajax({
                        type: "post",
                        data: data,
                        url: '/Common/SaveRecomender',
                        dataType: "json",
                        success: function(d) {
                            $('.going').hide();
                            $('.goRecommend').show();
                            if (d.r == -1) {
                                myAlert('<p>该推荐信息已提交过，您可进入“<a href="/member/recommends">会员中心-我的推荐</a>”查看处理状态</p>', '', true);
                            } else {
                                     Pop_Open('550', '250', '/Home/RecommendValid?rid=' + d.r);
                            }

                            
                        },
                        beforeSend: function() {
                            $('.going').show();
                            $('.goRecommend').hide();
                        },
                        error: function() {
                            $('.going').hide();
                            $('.goRecommend').show();
                        }
                    });

                }

            });

        }

        $(function () {
            $('.js-tab-title-indexness').hover(function () {
                $(this).addClass('sp');
                $(this).siblings().removeClass('sp');
                var index = $(this).index();
                var obj = $('.service-Box')[index];
                $(obj).show();
                $(obj).siblings('.service-Box').hide();
                
            })
        })



        //$(document).ready(function () {
        //    $(".slideInner").slide({
        //        slideContainer: $('.slideInner a'),
        //        effect: 'easeOutCirc',
        //        autoRunTime: 5000,
        //        slideSpeed: 1000,
        //        nav: true,
        //        autoRun: true,
        //        prevBtn: $('a.prev'),
        //        nextBtn: $('a.next')
        //    });
        //});

        //互联网+企业服务 图片轮播
        var index = 0;
        var $pictureItem = $('.pictureItem');
        var $picture = $('.picture');
        var $pictureslideDot = $('.pictureslideDot span');
        $pictureItem.eq(0).css({ 'opacity': 1, 'z-index': 2 }).siblings('.pictureItem').css({ 'opacity': 0, 'z-index': 1 });

        function pictureslide() {
            if (index == $pictureItem.length - 1) {
                index = 0;
            } else {
                index++;
            }
            $pictureItem.eq(index).animate({ "opacity": "1" }).css('z-index', 2).siblings('.pictureItem').animate({ "opacity": "0" }).css('z-index', 1);
            $pictureslideDot.eq(index).addClass('pictureslideSel').siblings().removeClass('pictureslideSel');
        }

        $('.pictureleftArrow').on('click', function () {
            if (index == 0) {
                index = $pictureItem.length - 1;
            } else {
                index -= 1;
            }
            $pictureItem.eq(index).animate({ "opacity": "1" }).css('z-index', 2).siblings('.pictureItem').animate({ "opacity": "0" }).css('z-index', 1);
            $pictureslideDot.eq(index).addClass('pictureslideSel').siblings().removeClass('pictureslideSel');
        })
        $('.picturerightArrow').on('click', function () {
            if (index == $pictureItem.length - 1) {
                index = 0;
            } else {
                index += 1;
            }
            $pictureItem.eq(index).animate({ "opacity": "1" }).css('z-index', 2).siblings('.pictureItem').animate({ "opacity": "0" }).css('z-index', 1);
            $pictureslideDot.eq(index).addClass('pictureslideSel').siblings().removeClass('pictureslideSel');
        })
        var picturesetSlide = setInterval(pictureslide, 4000);
        $picture.hover(function() {
            clearInterval(picturesetSlide);
        }, function() {
            picturesetSlide = setInterval(pictureslide, 4000);
        })
        $pictureslideDot.click(function () {
            index = $(this).index();
            if (!$(this).hasClass('pictureslideSel')) {
                $(this).addClass('pictureslideSel').siblings().removeClass('pictureslideSel');
                $pictureItem.eq(index).animate({ "opacity": "1" }).css('z-index', 2).siblings('.pictureItem').animate({ "opacity": "0" }).css('z-index', 1);
            }
        })

        //视频播放
        $('.video_start').on('click', function () {
            $('.video_window').show();
        })
        $('.cloce_video').on('click', function(){
            $('.video_window').hide();
        })
    </script> 
