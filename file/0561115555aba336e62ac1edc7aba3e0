        function cloceBtBanner() {
            $('#btbanner').hide();
            $('#qqDialog').css('bottom', '0');
            $('.video_start').css('bottom', '70px');
        }


    function formatMoney(number, places, symbol, thousand, decimal) {
        number = number || 0;
        places = !isNaN(places = Math.abs(places)) ? places : 2;
        symbol = symbol !== undefined ? symbol : "$";
        thousand = thousand || ",";
        decimal = decimal || ".";
        var negative = number < 0 ? "-" : "",
            i = parseInt(number = Math.abs(+number || 0).toFixed(places), 10) + "",
            j = (j = i.length) > 3 ? j % 3 : 0;
        return symbol + negative + (j ? i.substr(0, j) + thousand : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + thousand) + (places ? decimal + Math.abs(number - i).toFixed(places).slice(2) : "");
    }
    $(function () {

       

        var url = "/Member/GetNoReadMsgAndOrders";
        var Name = '4bfa1131b87a4c539d994923f7c12930';
        if (Name == "" || Name.length<=0)
        {
            var _html = '<div class="msgList">请登录，查看最新订单信息！</div>';
            var htmllink = '<div class=""><a href="/member/order/" class="btnCheckMore">查看全部</a></div>';
            $("._order").append(_html + htmllink);
            _html = '<div class="msgList">请登录，查看最新消息提醒</div>';
            htmllink = '<div class=""><a href="/member/message/" class="btnCheckMore">查看全部</a></div>';
            $("._msg").append(_html + htmllink);
            $("._msgicon").hide();
            $("._ordicon").hide();
            return false;
        }

        $.ajax({
            type: "POST",
            url: url,
            cache: false,
            data: {},
            dataType: "json",
            beforeSend: function () {

            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                //  alert('error' + XMLHttpRequest.status);

            },
            success: function (json) {

                //{ OrderInfo=orderList,MsgInfo=msgList};
                if (json.OrderInfo.length > 0) {
                    var html = "";
                    $("._ordicon").addClass("icon_v_h icon_tips02").text(json.OrderInfo.length).show();
                    $.each(json.OrderInfo, function (index, data) {
                        var refUrl = "";
                         if (data.ProductId == 1028) {
                             if (data.Status == 2) {
                                 refUrl = "/regcompany/steptwo/" + data.OrderId;
                             }
                             if (data.Status == 3) {
                                 refUrl = "/regcompany/moreservice/" + data.OrderId;
                             }
                             if (data.Status == 4) {
                                 refUrl = "/pay/confirm/" + data.OrderId;
                             }
                             if (data.Status == 5) {
                                 refUrl = "/regcompany/stepFour/" + data.OrderId;
                             }
                             if (data.Status > 5) {
                                 refUrl = "/member/orderDetail/" + data.OrderId;
                             }
                         }
                        if (data.ProductId == 1038) {
                            if (data.Status == 4) {
                                refUrl = "/pay/confirm/" + data.OrderId;
                            }
                            if (data.Status >= 5) {
                                refUrl = "/member/orderDetail/" + data.OrderId;

                            }
                        }
                        if (data.Status < 5 && data.PayWay == 'payoffline') {
                            refUrl = "/Pay/HalfFinish/" + data.OrderId;

                        }

                        var cnname = data.FullName;
                        var oid = data.OrderID;
                        html += ' <div class="msgList"><div class="clearfix"> <h3 class="left">' + data.ProductName + '</h3><span class="right price">' + formatMoney(data.OrderPrice, 0, "RMB ") + '</span></div>' +
                            '<div class="clearfix"><a href="' + refUrl + '" class="left msgDetail w255">' + cnname + '</a><span class="right statu">' + getStatusName(data.Status) + '</span></div> </div>';

                    });

                    html += '<div class=""><a href="/member/orders/" class="btnCheckMore">查看全部</a></div>';
                    $("._order").append(html);
                } else {
                    var html = '<div class="msgList colorBlue">暂时没有最新订单</div>';
                    html += '<div class=""><a href="/member/orders/" class="btnCheckMore">查看全部</a></div>';
                    $("._order").append(html);


                }
                if (json.MsgInfo.length > 0) {

                    var mc = json.MsgInfo.length;
                    if (mc > 0)
                    {
                        $("._msgicon").addClass("icon_v_h icon_tips").text(mc).show();
                    }
                    var html = "";

                    $.each(json.MsgInfo, function (index, data) {
                        if (data.isRead == "未读")
                        {
                            var MsgType = data.MsgType == null || data.MsgType == "" ? "订单消息提示" : data.MsgType;
                            var content = data.MsgContent;
                            if (data.MsgContent.length > 30)
                            {
                                content = data.MsgContent.substring(0, 30);
                            }
                            html += ' <div class="msgList"> <h3>' + MsgType + '</h3> <a href="javascript:Pop_Open(520, 430,' + '\'' + '/member/messageDetail/' + data.MsgID + '\')" class="msgDetail">' + content + '</a> </div>';
                        }
                    });
                    html += ' <div class=""><a href="/member/messages/" class="btnCheckMore">查看全部</a></div>';
                    $("._msg").append(html);
                } else {
                    var html = '<div class="msgList colorBlue">暂时没有未读消息</div>';
                    html += ' <div class=""><a href="/member/messages/" class="btnCheckMore">查看全部</a></div>';
                    $("._msg").append(html);
                }
            }
        });


    });

        $(function() {
            initRightHelp();
            $(window).scroll(function () {
                var li4 = $('#gotop').siblings('.li4');
                var bottomLine = li4.find('.help-right-bottomline');
                if ($(window).scrollTop() > 100) {
                    $("#gotop").fadeIn();
                    if (bottomLine.length == 0) {
                        li4.append('<div class="help-right-bottomline pa "></div>');
                    }
                    
                } else {
                    $("#gotop").hide();
                    bottomLine.remove();
                }
            });
            $("#gotop").click(function () {
                $('html,body').animate({ 'scrollTop': 0 }, 500);
            });

            $('#onlineService').click(function() {
               // DayQQshow();
                //mvclick();
            });

            $('.help-right-jumplink').click(function() {
