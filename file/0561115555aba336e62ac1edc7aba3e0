            });

            getQuantity();

        });



        


        function initRightHelp() {
            $('.help-right-box').find('ul').find('li').each(function() {
                $(this).hover( function() {
                    var imgBox = $(this).find('.help-right-img');
                    var dataId = $(this).attr('data-id');
                    var bottomLine = $(this).find('.help-right-bottomline');
                    var helpDesc = $(this).find('.help-right-desc');
                    var noHoverClassName = 'help-right-img' + dataId;
                    var overClassName = noHoverClassName + '-over';
                    imgBox.removeClass(noHoverClassName).removeClass('help-right-bw').addClass(overClassName).addClass('help-right-bb');
                    bottomLine.addClass('help-dn');
                    //helpDesc.fadeIn();
                    helpDesc.removeClass('help-dn');
                },
                function () {
                    var imgBox = $(this).find('.help-right-img');
                    var dataId = $(this).attr('data-id');
                    var bottomLine = $(this).find('.help-right-bottomline');
                    var helpDesc = $(this).find('.help-right-desc');
                    var noHoverClassName = 'help-right-img' + dataId;
                    var overClassName = noHoverClassName + '-over';
                    imgBox.removeClass(overClassName).removeClass('help-right-bb').addClass(noHoverClassName).addClass('help-right-bw')
                    bottomLine.removeClass('help-dn');
                    //helpDesc.hide();;
                    helpDesc.removeClass('help-dn').addClass('help-dn');
                    }
                );
            });
        }

        
    


