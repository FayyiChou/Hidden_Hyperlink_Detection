    function mvclick() {
        var d = new Date();
        var id = d.getYear().toString() + (d.getMonth() + 1).toString() + d.getDate().toString() + d.getHours().toString() + d.getMinutes().toString() + d.getSeconds().toString();
        id += "." + Math.random();
        _mvq.push(['custom', 'jzqu1', /*咨询ID*/ id, '']);
        _mvq.push(['$logConversion']);
    }
