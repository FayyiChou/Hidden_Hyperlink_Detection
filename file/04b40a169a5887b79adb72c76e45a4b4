sel = document.selection.createRange();
}
pos = getCaret();
}

selection = sel ? (wysiwyg ? sel.text : sel.text) : mw_highlighter_getSel();//note 修改sel.HtmlText修改为Text，编辑器中的可视化用插件实现比较困难
if(menu) {
