<script type="text/javascript">
    $(document)
        .ready(new function() {
            //获取弹屏开启条件
            GetSceenPopupCondition();
            //关闭弹框
            $('.boxFrom .boxFromMain img')
                .click(function() {
                    $('.boxFrom').hide();
                });
            //纠错页面
            $('.boxFrom .boxFromMain .li01')
                .click(function() {
                    //去掉弹窗入口
                    $('.boxFrom').hide();
                    var url = "/jingqu/Product/ProductErrorCorrection?comefr=ScreenPopup&s=" + Math.random();
                    window.location = url;
                });
            //稍后提醒
            $('.boxFrom .boxFromMain .li02')
                .click(function() {
                    //debugger;
                    var time = getCookie("timesp");
                    if (time == null || time == "" || time == 0) {
                        time = 30;
                    }
                    setCookie("timesp", time);
                    $('.boxFrom').hide();
                    timesScreenPopup();

                });

        });

    function setCookie(name, value) {
        //var Days = 30;
        //var exp = new Date();
        //exp.setTime(exp.getTime() + 365*24*60*60*1000);
        document.cookie = name + "=" + escape(value);
        // + ";expires=" + exp.toGMTString();
    }

    function getCookie(name) {
        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
        if (arr = document.cookie.match(reg)) return unescape(arr[2]);
        else {
            return null;
        }
    }

    function delCookie(name) {
        var exp = new Date();
        exp.setTime(exp.getTime() - 1);
        var cval = getCookie(name);
        if (cval != null)
            document.cookie = name + "=" + cval + ";expires=" + exp.toGMTString();
    }

    //获取弹窗
    function GetSceenPopupCondition() {
        var mark = "";
        if (mark != null && mark == "isY") {
            var time = getCookie("timesp");
            if (time > 0) {
                //删除Cookie中的值 
                delCookie("timesp");
            }
            return;
        }
        $.ajax({
            type: "POST",
            url: "/jingqu/Product/GetSceenPopup?&s=" + Math.random(),
            success: function(data) {
                //debugger 
                if (data != null && data != "" && data == "1") {
                    var time = getCookie("timesp");
                    if (time == null || time == "" || time == 0) {
                        $('.boxFrom').show();
                    }
                } else {
                    $('.boxFrom').hide();
                }
            }
        });
    }

    //弹窗计时器
    function timesScreenPopup() {
        var time = getCookie("timesp");
        if (time == null) {
            return;
        }
        time--;
        setCookie("timesp", time);
        //递归
        var func = setTimeout("timesScreenPopup()", 60000);
        if (time <= 0) {
            //debugger;
            $('.boxFrom').show();
            //删除Cookie中的值 
            delCookie("timesp");
            //删除递归调用函数  
            clearTimeout(func);
        }
    }
</script>
<script src="/jingqu/Scripts/Ebooking/displayReplace.js"></script>


<script type="text/javascript">
    //Enter登陆
    document.onkeydown = function (event) {
        var e = event || window.event || arguments.callee.caller.arguments[0];
        if (e && e.keyCode == 13) {
            LoginOn();
        }
    };
    function codeclick() {
        var url = "../AjaxComm/CodeDemo.ashx?Date=" + new Date();
        $("#imgcode").attr("src", url);
        $("#uCheckCode").val("");
        $("#tishi").removeClass().addClass("tishi");
    }
    //登录
    function LoginOn() {
        var name = $.trim($("#uName").val());
        var pwd = $.trim($("#uPwd").val());
        var vCode = $.trim($("#uCheckCode").val());

        if (name == "") {
            $("#uName").focus();
            return;
        }

        if (pwd == "") {
            $("#uPwd").focus();
            return;
        }

        if (vCode == "") {
            $("#uCheckCode").focus();
            return;
        }

        $(".loginBtn").attr("disabled", "disabled");
        $(".loginBtn").css("background-color", "#BCC2C5");
        $(".loginBtn").css("cursor", "default");
        $(".loginBtn").val("登录中...");
        $.post("/jingqu/Account/LoginOn",
            { loginName: name, password: pwd, verifyCode: vCode },
            function(data) {
                if (data.IsSuccess) {
                    $(".errormsg").hide();
                    if (data.RedirectUrl == undefined || data.RedirectUrl == "") {
                        if (data.IsRedirectEbookingPage === true) {
                            location.href = "/jingqu/OrderManage/ProcessedOrder";
                        } else if (data.Platform == '1320202') {
                            location.href = "/jingqu/MerchantInfo/ContractProgress?isFirstLoad=1";
                        } else {
                            location.href = "/jingqu/Account/Default";
                        }
                    } else {
                        location.href = "/jingqu" + data.RedirectUrl;
                    }
                }
                else {
                    $(".loginBtn").val("登录");
                    $(".errormsg p").text(data.ErrorMessage).parent().show();
                    $(".loginBtn").removeAttr("disabled");
                    $(".loginBtn").css("background-color", "#a1d7f7");
                    $(".loginBtn").css("cursor", "pointer");
                    codeclick();
                    return;
                }
            });
    }
    

    var hoverAtFn = {
        init: function (tab, claname) {
            var timer = null;
            tab.hover(function () {
                clearTimeout(timer);
                tab.addClass(claname);
            }, function () {
                timer = setTimeout(function () {
                    tab.removeClass(claname);
                }, 200)
            })
        }
    };

    //hoverAtFn.init($(".forgetPwd"), "show")
    hoverAtFn.init($(".oldUser"), "show")

    var height = $(".both").height();
    $(".coorperImg").toggle(function () {
        $(".contentBox").css("display", "block");
        $(".coorperImg i").addClass("up");
        $(window).scrollTop(height);
    }, function () {
        $(".contentBox").css("display", "none");
        $(".coorperImg i").removeClass("up");
        $(window).scrollTop(0);
    });

    $(window).resize(function () {
        var w_adapte = -($(".top .pic").width()) / 2;
        $(".top .pic").css("margin-left", w_adapte);
    })
    var w_adapte = -($(".top .pic").width()) / 2;
    $(".top .pic").css("margin-left", w_adapte);

    if ($(window).width() < 1400) {
        $(".login_right").css({ "right": "18%", "top": "20%" });
        $(".coorperImg").css({ "margin": "10px auto 30px auto" });
    } else {
        $(".login_right").css({ "right": "23%", "top": "26%" });
        $(".coorperImg").css({ "margin": "30px auto 50px auto" });
    }
</script></body></html>