<script>
$(function(){
	$('.apps-preg .section1 .icon-appdown').css({ y: '50px', opacity: 0 }).delay(100).transition({ y: 0, opacity: 1 });
    $('#fullpage').fullpage({
	    css3: true,
	    navigation: true,
	    loopBottom: true,
	    navigationPosition: 'right',
	    anchors: ['page1', 'page2', 'page3', 'page4', 'page5','page6'],
	    menu: '#menu',
	    onLeave: function (index, nextIndex, direction){
	    	if (index == 1) {
	            $('.apps-preg .section1 .icon-appdown').transition({ y: '-30px', opacity: 0 });
	        }
	        if (index == 2) {
	            $('.apps-preg .section2 .icon-title').transition({ x: '50px', opacity: 0 });
	            $('.apps-preg .section2 .icon-pic').transition({ x: '-50px', opacity: 0 });
	        }
			if (index == 3) {
	            $('.apps-preg .section6 .icon-title').transition({ x: '50px', opacity: 0 });
	            $('.apps-preg .section6 .icon-pic').transition({ x: '-50px', opacity: 0 });
	        }
	        if (index == 4) {
	            $('.apps-preg .section3 .icon-title').transition({ x: '50px', opacity: 0 });
	            $('.apps-preg .section3 .icon-pic').transition({ y: '-50px', opacity: 0 });
	        }
	        if (index == 5) {
	            $('.apps-preg .section4 .icon-title').transition({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section4 .icon-pic').transition({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section4 .icon-pic2').transition({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section4 .icon-hand').transition({ y: '50px', opacity: 0 });
	        }
	        if (index == 6) {
	            $('.apps-preg .section5 .icon-title').transition({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section5 .icon-pic2').transition({ y: '50px', opacity: 0 });
	            $('.apps-preg .section5 .icon-picbg').transition({ y: '-50px', opacity: 0 },1000);
	        }

	        if (nextIndex == 1) {
	            $('.apps-preg .section1 .icon-appdown').css({ y: '-30px', opacity: 0 });
	        }
	        if (nextIndex == 2) {
	            $('.apps-preg .section2 .icon-title').css({ x: '50px', opacity: 0 });
	            $('.apps-preg .section2 .icon-pic').css({ x: '-50px', opacity: 0 });
	        }
			if (nextIndex == 3) {
	            $('.apps-preg .section6 .icon-title').css({ x: '50px', opacity: 0 });
	            $('.apps-preg .section6 .icon-pic').css({ x: '-50px', opacity: 0 });
	        }
	        if (nextIndex == 4) {
	            $('.apps-preg .section3 .icon-title').css({ x: '50px', opacity: 0 });
	            $('.apps-preg .section3 .icon-pic').css({ y: '-50px', opacity: 0 });
	        }
	        if (nextIndex == 5) {
	            $('.apps-preg .section4 .icon-title').css({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section4 .icon-pic').css({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section4 .icon-pic2').css({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section4 .icon-hand').css({ y: '50px', opacity: 0 });
	        }
	        if (nextIndex == 6) {
	            $('.apps-preg .section5 .icon-title').css({ y: '-50px', opacity: 0 });
	            $('.apps-preg .section5 .icon-pic2').css({ y: '50px', opacity: 0 });
	            $('.apps-preg .section5 .icon-picbg').css({ y: '-50px', opacity: 0 },1000);
	        }
	    },
	    afterLoad: function (anchorLink, index){
	        if (index == 1) {
	            $('.apps-preg .section1 .icon-appdown').transition({ y: 0, opacity: 1 });
	        }
	        if (index == 2) {
	            $('.apps-preg .section2 .icon-title').transition({ x: 0, opacity: 1 });
	            $('.apps-preg .section2 .icon-pic').delay(300).transition({ x: 0, opacity: 1 });
	        }
			if (index == 3) {
	            $('.apps-preg .section6 .icon-title').transition({ x: 0, opacity: 1 });
	            $('.apps-preg .section6 .icon-pic').delay(300).transition({ x: 0, opacity: 1 });
	        }
	        if (index == 4) {
	            $('.apps-preg .section3 .icon-title').transition({ x: 0, opacity: 1 });
	            $('.apps-preg .section3 .icon-pic2').delay(300).transition({ x: 0, opacity: 1 });
	            $('.apps-preg .section3 .icon-pic').delay(800).transition({ y: 0, opacity: 1 });
	        }
	        if (index == 5) {
	            $('.apps-preg .section4 .icon-title').transition({ y: 0, opacity: 1 });
	            $('.apps-preg .section4 .icon-pic').delay(300).transition({ y: 0, opacity: 1 });
	            $('.apps-preg .section4 .icon-pic2').delay(500).transition({ y: 0, opacity: 1 });
	            $('.apps-preg .section4 .icon-hand').delay(1000).transition({ y: 0, opacity: 1 },1000);
	        }
	        if (index == 6) {
	            $('.apps-preg .section5 .icon-title').transition({ y: 0, opacity: 1 });
	            $('.apps-preg .section5 .icon-pic2').delay(300).transition({ y: 0, opacity: 1 });
	            $('.apps-preg .section5 .icon-picbg').delay(800).transition({ y: 0, opacity: 1 });
	        }
	    }
    });
})
</script>

