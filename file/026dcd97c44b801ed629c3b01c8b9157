<script>
    //点赞
    function vote(dom , typeId , workId , keyId) {
        var username = "";
        if( !isCorrectVal(username)){
            $.simpleAlert({
                content:"请您先登录！",
                ok:function(){
                    login();
                }
            });
            return false;
        }else {
            $.ajax({
                type: "get",
                url:"/wulin2/sixdoor2/vote",
                data:{typeId : typeId , workId : workId , keyId : keyId },
                dataType:"json",
                success:function(data){
                    if(data.success){
                        $(dom).text(data.status);
                        $.simpleAlert(data.message);
                    }else{
                        $.simpleAlert(data.message);
                    }
                }
            });
        }
    }
</script>

<!--初始化登录js -->
<script type="text/javascript">

    $(function(){
        loginandlogout();
    })

    function loginandlogout() {
        var username = "";
        if( !isCorrectVal(username)){
            $(".login_btn").show();
            $(".logout_btn").hide();
            $("#login_btn").click(function () {
                login();
            })
        }else{
            $("#username").html(username);
            $(".logout_btn").show();
            $(".login_btn").hide();
            $("#logout_btn").click(function () {
                logout();
            })
        }
    }
    function login(){
        wanmei.passport.islogin({session:'USER',_false:function(){
            wanmei.passport.login({session:'USER',hdid:'wulin2_sixdoor2_status'});
        }});
    }

    function logout(){
        wanmei.passport.logout({session:'USER'});
    }

    //判断一个字符串为空
    function isCorrectVal(value) {
        if( value == null || value == undefined || value == ""){
            return false;
        }
        return true;
    }
</script>



