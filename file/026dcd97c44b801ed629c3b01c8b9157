});
</script>   
<script>
$(function(){
    $('.btn_pop1').click(function(){
        $.simpleDialog({
           skin:false,
           content: '#pop1',
           id : 'pop1'
        });     
    });
    $('.close_pop1').click(function(){
        $.simpleDialog.close('pop1');
    });
 var _event={};
  _event.stop_bubble = function(e) {
    e = e || window.event;
    if (e.stopPropagation) {
      e.stopPropagation();
    } else {
      e.cancelBubble = true;
    }
  };
  $('#setwork').on('click',function(e){
    _event.stop_bubble(e);
    $.simpleDialog({
      skin:false,
      content: '#pop2',
      id : 'pop2'
    });
      $(document).on('click',function(){
        $.simpleDialog.close('pop2');
      });  
  });
})
  
</script>



