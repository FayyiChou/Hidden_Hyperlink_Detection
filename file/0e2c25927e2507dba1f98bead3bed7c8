                             src="/v2/Content/Images/defaultSquareImage.gif"
                             data-temp-src="{{::item.HotSaleRanking_PicUrl}}"
                             alt="{{::item.HotSaleRanking_SalePageTitle}}"/>
                        <span class="ranking-num" data-ng-bind="::$index+1"></span>

                        <div class="show-saved"
                             data-ng-init="item.IsFav = (HotSaleRankCtrl.TraceSalePageIdsList.indexOf(item.HotSaleRanking_SalePageId)>=0)"
                             data-ng-show="item.IsFav"></div>
                        <div class="buttons-layer">
                            <div class="buttons-layer-block"
                                 data-ng-class="{'hide': HotSaleRankCtrl.IsTouchDevice}">
                                <div class="save layer-btn"
                                     data-ns-ga-event-track
                                     data-track-category="銷售排行"
                                     data-track-action="click"
                                     data-track-label="收藏商品"
                                     data-ng-click="HotSaleRankCtrl.ToggleIsFav($event,item);"
                                     data-ng-class="{'disappearing-love':item.disappearingLove}">
                                    <i class="fa"
                                       data-ng-class="{'fa-heart-o':!item.IsFav,'fa-heart':item.IsFav}"></i>
                                </div>
                                <div class="addcart layer-btn popup-btn"
                                     data-ns-ga-event-track
                                     data-track-category="銷售排行"
