            <li class="cabinet-li"
                data-ng-repeat="item in ::HotSaleRankCtrl.HotSaleRankingList">
                <ns-watch-scroll-visiable
                        data-ng-if="$index == 0 || $index % 8 == 0 ||($index % 8 != 0 && $index==HotSaleRankCtrl.HotSaleRankingList.length)"
                        on-visiable="HotSaleRankCtrl.TrackHotSaleRankingProductImpression($index,8,'銷售排行')">
                </ns-watch-scroll-visiable>
                <a data-ng-href="/SalePage/Index/{{::item.HotSaleRanking_SalePageId}}">
                    <div class="cabinet-top"
                         data-ns-ga-event-track
