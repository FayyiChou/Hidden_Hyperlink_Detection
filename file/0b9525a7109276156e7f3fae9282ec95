        function ShowMessage(message) {
            alert(decodeURIComponent(message));
        }
        function Search() {
            if ($("#txtSearch").val() != "")
                window.location.href = "/Display/search.aspx?value=" + escape($("#txtSearch").val());
        }

        $(document).ready(function () {
            $('#txtSearch').keydown(function (e) {
                if (e.keyCode == 13) {
                    Search();
                }
            });
        });
    </script>
