    <script type="text/javascript">
        var dirx = 1;var diry = 1;var xpos = 0;var ypos = 0;

        function aa() {
            var obj = $("#div_showdyImg");
            obj.css("top", "0px");
            obj.css("left", "0px");
            window.setInterval("move()", 100);
        }

        function move() {
            var obj = $("#div_showdyImg");
            xpos += 2 * dirx;
            ypos += 2 * diry;
            obj.css("top", ypos+"px");
            obj.css("left", xpos + "px");
            if (xpos <= 0 || xpos + obj[0].offsetWidth >= document.body.clientWidth)
                dirx = -dirx;
            if (ypos <= 0 || ypos + obj[0].offsetHeight >= 800)
                diry = -diry;
        }

        $(document).ready(function () {
            aa();
        });

        stepcarousel.setup({
            galleryid: 'mygallery', //id of carousel DIV
            beltclass: 'belt', //class of inner "belt" DIV containing all the panel DIVs
            panelclass: 'panel', //class of panel DIVs each holding content
            panelbehavior: { speed: 300, wraparound: false, persist: true },
            autostep: { enable: true, moveby: 3, pause: 5000 },
            defaultbuttons: { enable: true, moveby: 4, leftnav: ['/Scripts/jqueryPPt/images/butt-left.gif', 0, 50], rightnav: ['/Scripts/jqueryPPt/images/butt-right.gif', -11, 50] },
            statusvars: ['statusA', 'statusB', 'statusC'], //register 3 variables that contain current panel (start), current panel (last), and total panels
            contenttype: ['inline'], //content setting ['inline'] or ['external', 'path_to_external_file']
            oninit: function () {
                isloaded = true
                document.getElementById('displaycssbelt').style.visibility = "visible";
                //document.getElementById('stocklevels').style.visibility = "visible";
            }
        });


    /*中部鼠标切换*/
    function scrollDoor() {
    }
    scrollDoor.prototype = {
        sd: function (menus, divs, openClass, closeClass) {
            var _this = this;
            if (menus.length != divs.length) {
                alert("菜单层数量和内容层数量不一样!");
                return false;
            }
            for (var i = 0; i < menus.length; i++) {
                _this.$(menus[i]).value = i;
                _this.$(menus[i]).onmouseover = function () {
                    for (var j = 0; j < menus.length; j++) {
                        _this.$(menus[j]).className = closeClass;
                        _this.$(divs[j]).style.display = "none";
                    }
                    _this.$(menus[this.value]).className = openClass;
                    _this.$(divs[this.value]).style.display = "block";
                }
            }
        },
        $: function (oid) {
            if (typeof (oid) == "string")
                return document.getElementById(oid);
            return oid;
        }
    }
    window.onload = function () {
        var SDmodel = new scrollDoor();
        SDmodel.sd(["m11", "m12", "m13", "m14", "m15"], ["c11", "c12", "c13", "c14", "c15"], "i_m_lm02", "i_m_lm01");
        SDmodel.sd(["m21", "m22", "m23", "m24", "m25","m26"], ["c21", "c22", "c23", "c24", "c25","c26"], "i_m_lm02", "i_m_lm01");
        SDmodel.sd(["m31", "m32", "m33", "m34", "m35", "m36"], ["c31", "c32", "c33", "c34", "c35", "c36"], "i_m_lm02", "i_m_lm01");
        SDmodel.sd(["m41", "m42", "m43", "m44", "m45"], ["c41", "c42", "c43", "c44", "c45"], "i_m_lm02", "i_m_lm01");
    }
    function MM_jumpMenu(targ, selObj, restore) { //v3.0
        var url = selObj.options[selObj.selectedIndex].value;
        if (url != "0") {
            window.open(url, "", "", "");
            //window.parent.location.href = url;
            //eval(targ + ".location='" + url + "'");
            if (restore) selObj.selectedIndex = 0;
        }
    }
    
    $(document).ready(function () {
        setInterval("autoChange('ulShow2','imgShowBig2')", 5000);
        //autoChange();
    });
    
    function limouvseOver(obj) {

        $(obj).parent().children().each(function () {
            $(this).removeAttr("class");
            $(this).attr("class", "normal");
        });

        $(obj).attr("class", "Controlcurrent");
    }

    function lionclick(obj, showimgid,id) {
        var url = $($(obj).children()[0]).attr("src");
        setTransition(showimgid);
        $("#" + showimgid).attr("src", url);
        $("#" + showimgid).attr("title", $(obj).attr("shwoTitile"));
        playTransition(showimgid);

        //var id = $(obj).attr("opportid");
        $("#ashowbingimg2").attr("href", "/Display/zx/show.aspx?id=" + id);
        $("#showimg2Title").text($(obj).attr("shwoTitile"));
    }

    var currentIndex = 0;    
    function autoChange(ulId, bigImgId) {
        //var currobj = $($($("#" + ulId).children()[currentIndex]).children()[0]);
        var currobj = $($("#" + ulId + " li")[currentIndex]);
        var url = currobj.attr("src");
        var id = currobj.attr("opportid");
        var name = currobj.attr("shwoTitile");

        $("#ashowbingimg2").attr("href", "/Display/zx/show.aspx?id=" + id);

        $("#" + ulId).children().each(function () {
            $(this).removeAttr("class");
            $(this).attr("class", "normal");
        });
        $($("#" + ulId).children()[currentIndex]).attr("class", "Controlcurrent");

        setTransition(bigImgId);
        $("#" + bigImgId).attr("src", url);
        $("#" + bigImgId).attr("title", name);
        playTransition(bigImgId);

        currentIndex++;
        if (currentIndex == 4) {
            currentIndex = 0;
            ChangeImg();//当前4个切换完了 换一组显示
        }
    }

    var groupId = 2;//从2开始
    function ChangeImg() {
        var cuay = new Array();
        var begin = 0;
        
        var allList = $("#div_all_img img");
        if (groupId == 1) begin = 0;
        else if (groupId==2) begin = 4;
        else if (groupId == 3) begin = 8;
        if (begin < 0) begin = 0;
        if (begin > allList.length) begin = allList.length - 4;
        var i = 0;
        allList.each(function () {
            if (i >= begin) {
                var obj = {};
                obj.Id = $(this).attr("opportid");
                obj.Title = $(this).attr("shwoTitile");
                obj.Src = $(this).attr("src");
                cuay.push(obj);
            }
            i++;
        });
        groupId++;
        if (groupId == 4) { groupId = 1; }
        var j=0;
        $("#ulShow2 img").each(function () {
            if (cuay.length > j) {
                $(this).attr("src", cuay[j].Src);
                $(this).attr("opportid", cuay[j].Id);
                $(this).attr("shwoTitile", cuay[j].Title);
            }
            j++;
        });
        
    }

    function setTransition(showimgid) {
        if (document.all) {
            document.getElementById(showimgid).style.filter = "revealTrans(duration=3,transition=" + Math.floor(Math.random() * 23) + ")";
            document.getElementById(showimgid).filters.revealTrans.apply();
        }
    }

    function playTransition(showimgid) {
        if (document.all)
            document.getElementById(showimgid).filters.revealTrans.play()
    }
    
    </script>

