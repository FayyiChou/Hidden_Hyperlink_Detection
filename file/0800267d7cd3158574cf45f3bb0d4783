<script type="text/javascript" src="../js/sn_lazyload.js"></script>
<script type="text/javascript">
	<!-- 获取cookie -->
	function d(b) {
		var a;
		return (a = document.cookie.match(RegExp("(^| )" + b + "=([^;]*)(;|$)"))) ? decodeURIComponent(a[2].replace(/\+/g, "%20")) : null;
	};
	$(function(){
		var sop_user_biz = d('sop_user_biz');
		if(sop_user_biz == 'P'){
			$("[name='P_show']").show();
		 	$("[name='P_hidden']").hide();
		}else if(sop_user_biz == 'C'){
		 	$("[name='C_show']").show();
		 	$("[name='C_hidden']").hide();
		}

	});
</script>



