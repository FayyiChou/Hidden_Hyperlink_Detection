    $(document).ready(function() {


        $('.1231btn-primary').on('click', function() {
            data = $(this).data('link') + "?lang=ara";
            $('.modal-body a').attr('target', "_blank");

            //facebook
            $('.modal-body .facebook').attr('href', "https://www.facebook.com/sharer.php?u=" + data);
            //twitter
            $('.modal-body .twitter').attr('href', "https://twitter.com/home?status=" + data);
            //google+
            $('.modal-body .google').attr('href', "https://plus.google.com/share?url=" + data);
            //linkedIn
            $('.modal-body .linkedin').attr('href', "https://www.linkedin.com/shareArticle?mini=true&url=&title=&summary=&source=" + data);
            //Reddit
            $('.modal-body .reddit').attr('href', "http://reddit.com/submit?url=" + data);
            //digg
            $('.modal-body .digg').attr('href', "http://digg.com/submit?url=" + data);
            //stumbleupon
            $('.modal-body .stumbleupon').attr('href', "http://www.stumbleupon.com/submit?url=" + data);
            //whatsapp
            data = $(this).data('link');
            $('.modal-body .whatsapp').attr('href', "whatsapp://send?text=" + data);
            //instagram
            //$('.modal-body .instagram').attr('href', "https://api.instagram.com/v1/media/" + data);



        });

    });
</script>
 
