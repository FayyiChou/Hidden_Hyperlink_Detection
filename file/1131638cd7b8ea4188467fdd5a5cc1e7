<script type="text/javascript" src="/js/jqueryscript.min.js"></script>
<script language="javascript">


        function fetch_select(strat = "no")
        {
            $(".Timings").html("");
            $('#loadingmessage').show();
            if (strat == "kw") {
                country_id = "kw";
            } else {
                var country_id = $(".countries_ul").val();
            }
            $.ajax({
                type: "GET",
                url: "/countries/get_data/1/" + country_id,
                data: "",
                success: function(data) {
                    $(".Timings").html(data);
                    $('#loadingmessage').hide();
                }
            });
        }

        $(document).ready(function() {
            fetch_select("kw");
        });
    </script>
 
