    seajs.use(["jquery"], function ($) {
        if ($('#dealerPriceBox').attr('data-val') != '' && $('#dealerPriceBox').attr('data-val') != undefined) {
            $.ajax({
                type: "GET",
                url: "/Evaluation/GetDealerPriceStr?serialId=" + $('#dealerPriceBox').attr('data-val') + "&radom=" + Math.random() + "&yearId=" + $('#dealerPriceBox').attr('data-val2'),
                data: "",
                success: function (msg) {
                    if (msg != "-1") {
                        $('#dealerPriceBox').find('span').html(msg + '万');
                    } else {
                        $('#dealerPriceBox').find('span').html('暂无');
                    }
                }
            });
        }
        $('.manual-ul li').click(function () {
            $('.manual-ul li', $(this).parents('.tab-content-item')).removeClass('current');
            $(this).addClass('current');
            $(this).parents('.tab-content-item').find('.manual-cont,.auto-cont').hide();
            //if ($(this).val() == '0') {
            //    //$(this).parents('.tab-content-item').find('.manual-cont').show();
            //    //$("[group=manual]").show();
            //}
            //else {
            //    $(this).parents('.tab-content-item').find('.auto-cont').show();
            //        $("[group=auto]").show();
            //    }
            $(this).parents('.tab-content-item').find('[group={0}]'.replace('{0}',$(this).attr('group'))).show();
        })

        $(function () {
            $.ajax({
                type: "get",
                url: "http://ms.i.autohome.com.cn/bluev/GetBlueVUsersBySeriesId", // 这个就是不同于当前域的一个URL地址，这里单纯演示，所以同域
                dataType: "jsonp",
                jsonpCallback: "koubeiseriescallback",  // 指定回调函数，这里名字可以为其他任意你喜欢的，比如callback，不过必须与下一行的GET参数一致
                data: "seriesid=3347&callback=koubeiseriescallback&checkreguserfollower=0&seriesSwitch=1",
                success: function (json) {
                    if (json.ReturnCode == 0 && json.Result != "") {
                        var blueVicon = "<a href='http://i.autohome.com.cn/" + json.Result[0].UserID + "#pvareaid=" + json.Result[0].SeriesPvId + "' target=\"_blank\" class=\"saf-automakers\"><i class=\"icon16 icon16-v\"></i><span>咨询厂商</span></a>";
                        $(".subnav-title-name").append(blueVicon)

                    }
                }
            });
        });
        $(function () {
            $.ajax({
                type:"get",
                url:"/Controls/HaveSeriesVideo",
                async:true,
                data:"seriesId="+3347,
                success:function(data)
                {
