<script>
    seajs.use(["jquery"], function ($) {
        $.ajax({
            type: "get",
            url: "/Controls/GetHelperLog",
            data: "idListStr=" + $('#koubeiIdList').val() + "&mandm=" + Math.random(),
            success: function (msg) {
                msg = msg + ',';
                $('.js-help').each(function () {
                    if (msg.indexOf($(this).attr('data-val')) >= 0) {
                        $(this).html('感谢您对口碑的支持！快点击“评论”跟车主互动聊车吧~！');
                    }
                });
            }
        });
        var initCommentCount = function ($) {
            var url = "http://reply.autohome.com.cn/api/getData_ReplyCounts.ashx?appid=5&datetype=jsonp&objids=" + $('#koubeiIdList').val() + "&callback=?";
            $.getJSON(url, function (json) {
                if (typeof (json) != "undefined" && typeof (json) != "undefined") {
                    updateCommentCount(json, $);
                }
            });
        }
        var updateCommentCount = function (json, $) {
            $(json.commentlist).each(function (i, n) {
                $('#Comment_' + n.objid).html(n.replycount);
            });
        }
        initCommentCount($);
    });
</script>
<script>
    seajs.use(["formmodle", "commentfunmodle", "commentlistmodle"], function ($, commentfunmodle, commentlistmodle) {
        $(".js-showmessage").each(function (j, n) {
            $(n).click(
                function () {
                    var s = $(this).attr('data-val');
                    $(".js-textcont" + s).removeAttr("class");
                    $(".js-addhidemessage" + s).addClass("fn-hide");

                    //异步加载feeling
                    var myUrl = "/FrontAPI/GetFeelingByEvalId?evalId=" + s;
                    $.ajax({
                        type: "get",
                        url: myUrl,
                        data: "",
                        success: function (msg) {
                            if (msg) {
                                $("#divfeeling_" + s).html(msg);
                            }
                        }
                    });
                    $(this).hide();

                    //try {
                    //    _hmt.push(['_trackEvent', '口碑内容', '点击', '展开全部口碑']);
                    //}
                    //catch (ex) { console.log(ex); }
                }
            )
        });
    });
