        //    })
        //    .on('mouseout', function () {
        //        clock = setTimeout(function () { $('#DivIdHrml .wrapper').fadeOut(); }, 5000);
        //    });
        //}

        $().ready(function () {
            importantinit();
            commentfunmodle.Imagescorll();
            $('.js-medalico').hover(
  function () {
      $('.js-medalname', $($(this).attr('data-target'))).html($(this).attr('data-medalname'));
      $('.js-medalbigimage', $($(this).attr('data-target'))).attr('src', $(this).attr('data-bigimage'));
  },
  function () {
      $('js-medalinformationBox').hide();
