    <script>
        seajs.use(["jquery"], function ($) {
            if (document, document.getElementById('returnTop')) {
                (function (d, target) {
                    var isIE6 = !!(window.attachEvent && !window.opera && /msie 6/i.test(navigator.userAgent)),
				 tStyle = target.style,
				 dd = d.documentElement,
				 db = d.body,
				 top = null,
				 ceil = Math.ceil;
                    // tStyle.display = 'none';
                    window.onscroll = sColl;
                    window.onresize = function () {
                        setTimeout(function () {
                            sColl();
                        }, 200);
                    };
                    function sColl() {
                        top = dd.scrollTop || db.scrollTop;
                        top > 100 ? $(target).find('.go-top').fadeIn("slow") : $(target).find('.go-top').fadeOut("slow");
                        if (isIE6) {
                            //tStyle.display = 'none';
                            tStyle.position = 'absolute';
                            tStyle.top = (dd.clientHeight / 5 * 4) + dd.scrollTop + 'px';
                            //tStyle.display = 'block';
                            if (tStyle.top + document.documentElement.scrollTop > document.documentElement.clientHeight) {
                                $(target).find('.go-top').hide();
                            }
                        }
                    }
                    function rTop() {
                        var top = dd.scrollTop || db.scrollTop,
					step = ceil(top / 5);
                        window.onscroll = null;
                        $(target).find('.go-top').fadeOut("slow");
                        if (top > -step) {
                            top -= step;
                            dd.scrollTop ? dd.scrollTop = top : db.scrollTop = top;
                            setTimeout(rTop, 10);
                        } else {
                            window.onscroll = sColl;
                        }
                    }
                    target.onclick = rTop;
                })(document, document.getElementById('returnTop'));
            }
        });
    </script>
    

    <script>
        document.body.oncopy = function () {
            copyToClipboard();
            event.returnValue = false;
        }
        function copyToClipboard() {
            if (window.clipboardData) {
                window.clipboardData.setData("Text", "");
            }
            else if (window.netscape) {
            }
            return false;
        }
    </script>

