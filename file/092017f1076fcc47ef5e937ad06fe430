    <script type="text/javascript">
        function getCookie(name) {
            var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
            if (arr = document.cookie.match(reg))
                return unescape(arr[2]);
            else
                return 0;
        }
        seajs.use(["jquery", "button", "textarea", "wordCount", "pop", "tab", "gotop", "support", "overlay", "tip", "layer", "share"], function ($) {

            $('.icon-kb').hover(function () {
                var $this = $(this);
                if (!this.bReady) {
                    $.get('/frontapi/GetUserLV?userId=' + $this.data('target').split('-')[1], function (data) {
                        var json = data.result[0];
                        var oDiv = $('<div class="poplv" id="' + $this.data('target') + '"></div>')
                        var html = '<i class="icon-jt-01"></i><i class="icon-jt-02"></i>'
                        + '<ul class="rank-ul blue">'
                        + '<li class="font-yh font-12-b"><span class="fn-right">LV ' + json.nextgrade + '</span>'
                        + '<span class="fn-left">LV ' + json.grade + '</span> </li>'
                        + '<li class="strip">'
                        + '<p class="strip-cont" style="width:' + json.integral / json.nextgradeintegral * 100 + '%">'
                        + '</p>'
                        + '</li>'
                        + '<li><span>' + json.grade + '级</span> <span>当前积分：' + json.integral + '</span>'
                        + '</li>'
                        + '</ul>';
                        oDiv.html(html).appendTo($this.parent());
                    });
                    this.bReady = true;
                } else {
                    $this.next().show();
                }
            }, function () {
                $(this).next().hide();
            });

            //提交图片loding
            $('textareabox').delegate('.js-submitepinlunbtn', 'click', function () {
                var btn = $(this);
                if (!btn.hasClass('btn-disabled')) {
                    btn.button('loading');
                    setTimeout(function () {
                        btn.button('reset')
                    }, 3000);
                }
            });
            $('.icon-gao').on('mouseenter', function () {
                $(".js-gao").text($(this).attr('data-val'));
            })
            $('.icon-di').on('mouseenter', function () {
                $(".js-di").text($(this).attr('data-val'));
            });
            $("#share-list").share();

            //锚点定位
            $(document).ready(function () {
                $.each($('.page').find('a'), function (index) {
                    var url = $(this).attr('href');
                    if (url.indexOf("#dataList") <= 0) {
                        url += "#dataList";
                        $(this).attr('href', url);
                    }
                })
            })

            //好人卡浮层
            $(function () {
                document.domain = "autohome.com.cn";
                var src = ['common', 'bronze', 'argentum', 'golden', 'diamond'];
                var style = $('<style>.task-tips .fn-left{float:left;}.task-tips .fn-right{float:right;}.task-tips{position:absolute;top:25px;left:-25px;width:170px;padding:2px;z-index:100;font-size:12px;font-weight:normal;background:url(http://x.autoimg.cn/as/images/shadow_bg.png?201306241635) repeat}.task-tips .task-tips-content{border:1px solid #ccd3e4;background-color:#fff;font-size:12px;line-height:18px;padding:8px 10px;position:relative;z-index:1;color:#666;text-align:left}.task-tips .task-tips-arrow{display:inline-block;overflow:hidden;height:11px;width:15px;position:absolute;left:30px;top:-8px;z-index:2;background:url(http://x.autoimg.cn/club/v1Content/images/Detail/layer_arrow24.png) no-repeat 0 -26px}.task-level-curr{font-family:simsun}.task-level-curr span{font-weight:bold;color:#f60}.task-level-info{margin-top:4px;font-family:simsun}.task-level-to{height:16px;margin-top:6px;zoom:1}.task-progress-box{width:100%;height:12px;position:relative;overflow:hidden;margin-top:2px}.task-progress{height:10px;line-height:10px;border:1px solid #ddd;background:#f7f7f7}.task-progress-when{position:absolute;left:0;top:-1px;height:12px;width:30%;background:#5eb95e}</style>');
                style.appendTo($('head'));
                $('[data-toggle="help"]').hover(function () {
                    var currentUser = getCookie('autouserid');
                    var $this = $(this);
                    var url = 'http://k.autohome.com.cn/evaluation/GetHelpAllStatus?userId=' + $this.data('target');
                    if (!this.bReady) {
                        $.get(url, function (data) {
                            var result = eval('(' + data + ')');
                            var json = result.result[0];
                            var level = json.LevelId - 1;
                            if (level < 0)
                                level = 0;
                            var update = "";
                            if (currentUser == json.UserId)
                                update = '<p class="task-level-info">' + json.Tips + '</p>';
                            var oDiv = $('<div class="task-tips">'
                            + '<div class="task-tips-content">'
                            + '<div class="task-level-curr">当前帮助值：<span>' + json.HelpScore + '</span></div>'
                            + '<div class="task-progress-box"><div class="task-progress"><div class="task-progress-when" style="width:' + json.UpdateProgress + '%"></div></div></div>'
                            + '<div class="task-level-to">'
                            + '<img class="fn-right" src="http://x.autoimg.cn/Space/images/' + src[json.NextLevelId - 1] + '.png" width="26" height="16" />'
                            + '<img class="fn-left" src="http://x.autoimg.cn/Space/images/' + src[level] + '.png" width="26" height="16" />'
                            + '</div>'
                            + update
                            + '</div>'
                            + '<span class="task-tips-arrow"></span>'
                            + '</div>');
                            $this.attr('style', 'position:relative');
                            oDiv.appendTo($this);
                        });
                        this.bReady = true;
                    } else {
                        $this.children('.task-tips').show();
                    }
                }, function () {
                    $(this).children('.task-tips').hide();
                })
            });
        });
        seajs.config({
            version: "account"
        });
    </script>

