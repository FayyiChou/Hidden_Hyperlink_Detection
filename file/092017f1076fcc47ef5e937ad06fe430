    var callBackFunAccountPopLogin;
    seajs.use(["jquery", 'dialog', 'popUpNickName', 'popLogin', 'popmobilebind', 'md5', "layer", "pop", "tip", "textarea", "select", "selectpop"],
        function ($, dialog, popUpNickName, popLogin, popmobilebind) {
            var initCurrentUser = function () {
                _currentuser = null;
                $.ajax({
                    type: "post",
                    url: "/Controls/GetCurrentUserId",
                    data: "",
                    async: false,
                    success: function (returndata) {
                        _currentuser = returndata;
                    },
                    error: function () {
                    }
                });
            }
            callBackFunAccountPopLogin = function () {
                initCurrentUser();
                autoHeaderObj.init();
                // AutohomeUserInfo.init();
                if (_currentuser == null) {
                    popLogin.init();
                }
                else {
                    if (_currentuser.State == '1') {
                        popmobilebind.init();
                    } else if (_currentuser.State == '2') {
                        popUpNickName.init();
                    }
                    else {
                        $('.js-replynoverifybox').hide();
                        $('.js-textareabox').show();
                    }
                }
            };
            $(document).ready(function () {
                closeAccountPop = reload;
                callBackFunAccountPopUpNick = callBackFunAccountBind = bindCallback;
                $('.js-openpinlunbox').live('click', function () {
                    verify();
                });
                $('.js-opencomplateaccountbox').live('click', function () {
                    verify();
                });
               
            });
            function reload() {
                dialog.hide();
            };
            function verify() {
                initCurrentUser();
                if (_currentuser == null) {
                    popLogin.init();
                }
                else {
                    if (_currentuser.State == '1') {
                        popmobilebind.init();
                    } else if (_currentuser.State == '2') {
                        popUpNickName.init();
                    }
                }
            };
            function bindCallback() {
                $('.js-replynoverifybox').hide();
                $('.js-textareabox').show();
                initCurrentUser();
            }
        });
</script>
