    var _currentuser;
    seajs.use(["jquery", "commentfunmodle", 'popLogin'], function ($, commentfunmodle, popLogin) {
        $('.js-addEvaluation').click(function () {
            var evalueObject = $.parseJSON($(this).attr('data-val'));
            commentfunmodle.judgePublish({ title: '口碑', editeText: '修改口碑', addText: '发表新口碑' }, evalueObject.objectId, evalueObject.objectType, this);
            if (commentfunmodle.judgeReturnValue) {
                return true;
            }
            else {
                return false;
            }
        })
        $('.jhkou,.tjkou,.mjkou').mouseenter(function () {
            $(this).parent('.conlzo').find('.confcwc').show();
        }).mouseleave(function () {
            $(this).parent('.conlzo').find('.confcwc').hide();
        });
        $('.js-addusernumber').click(function () {
            var evalueObject = $.parseJSON($(this).attr('data-val'));
            commentfunmodle.addUserNumber(evalueObject.objectId, evalueObject.objectType, this);
            try {
                var timestamp = (new Date()).valueOf();
                _trackEvent.push({ 'eid': '1|30|368|0|200022|300014', 'val': 1, 'a1': '0', 'a2': "'" + evalueObject.objectId + "'", 'a3': "'" + timestamp + "'" });
            }
            catch (ex) {
                alert(ex);
            }
        })
        $('.js-openpinlunbox').live('click', function () {
            var evalueObject = $.parseJSON($(this).attr('data-val'));
            commentfunmodle.openPinLunBox(evalueObject.objectId, evalueObject.objectType, evalueObject.modelId, evalueObject.targetId, this, evalueObject.islist, evalueObject.louceng);
        })
        $(".js-submitepinlunbtn").live("click", function () {
