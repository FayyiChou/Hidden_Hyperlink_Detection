        <script>
            seajs.config({ version: "1430790721710" });
            seajs.use(["jquery"], function ($) {
                function getCookie(name, defaultValue) {
                    var value, cookie = document.cookie, start = cookie.indexOf(name + '='), end;
                    if (start !== -1) {
                        start += name.length + 1;
                        end = cookie.indexOf(';', start);
                        if (end === -1) {
                            end = cookie.length;
                        }
                        value = decodeURIComponent(cookie.substring(start, end) || '');
                    }
                    return value || defaultValue || '';
                }
                function setCookie(name, value, expires, domain) {
                    var sCookie = name + '=' + encodeURIComponent(value);
                    if (expires) {
                        var exp = new Date(new Date().getTime() + expires);
                        sCookie += '; expires=' + exp.toGMTString();
                    }
                    if (domain) {
                        sCookie += '; domain=.' + domain;
                    }
                    sCookie += '; path=/';
                    document.cookie = sCookie;
                }
                var v = getCookie("guidance");
                if (!v && "True".toLowerCase() === "true") {
                    $('#mask').show();
                    $('.lead-step').show();
                    $('html,body').animate({ 'scrollTop': ($('.step01').offset().top - 100) + 'px' }, 200);
                    $('.next-step').click(function () {
                        $('.step01').fadeOut(300).siblings('.step02').removeClass('fn-hide');
                        $('html,body').animate({ 'scrollTop': ($('.step02').offset().top - 100) + 'px' }, 200);
                    });
                    $('.close,.go').click(function () {
                        $('#leadStyle').remove();
                        $('.lead-step,.lead-mask').remove();
                        setCookie("guidance", true, 50 * 365 * 24 * 60 * 60 * 1000, "k.autohome.com.cn");
                    })
                }
            })
        </script>
