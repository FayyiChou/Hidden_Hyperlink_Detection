        var gloablePailiang = '';
        var gloableMoreRowShow = false;
        $('.js-searchspec').click(function(){

            searchSpec($(this).attr('data-val'), this);
            try {
                var s=$(this).attr('data-val');
                var timestamp=(new Date).valueOf();
                if(s===null|s=="")
                {
                    _trackEvent.push({'eid':'1|30|366|0|200023|300015','val':1,'a1':'3347','a2':'0','a3': "'" +timestamp+"'"});
                }
                else
                {
                    _trackEvent.push({'eid':'1|30|366|0|200023|300015','val':1,'a1':'3347','a2': "'" +s+"'" ,'a3':"'" +timestamp+"'"});
                }
            }
            catch (ex) { }
        });

        var searchSpec = function (pailiang, object) {
            gloablePailiang = pailiang;
            initVisible();
            $('.olitabbtn a').removeClass('selected');
            $(object).addClass('selected');
        }

        var changeMoreStatus = function (object) {
            gloableMoreRowShow = !gloableMoreRowShow;
            initVisible();
            if ($(object).hasClass('btn-down')) {
                $(object).removeClass('btn-down');
                $(object).addClass('btn-top');
                $(object).html('折叠更多车型<i class="icon10 icon10-top"></i>');
            }
            else {
                $(object).removeClass('btn-top');
                $(object).addClass('btn-down');
                $(object).html('展开更多车型<i class="icon10 icon10-down1"></i>');
            }
        }
        $('#showmorespecBtn').click(function(){
            changeMoreStatus(this);
            try
            {
                var timestamp=(new Date).valueOf();
                _trackEvent.push({'eid':'1|30|366|0|200024|300016','val':1,'a1':'3347','a2':timestamp});
            }
            catch(ex){}
        });

        function getAllSpecHtml(seriesId,specState,yearIdStr)
        {
            var myUrl="/FrontAPI/GetSpecListBySeriesId?seriesId="+seriesId+"&specState="+specState+"&yearIdStr="+yearIdStr;

            $.ajax({
                type: "GET",
                url: myUrl ,
                data: "",
                success: function (specList)
                {
                    var specAllHtml="";
                    specAllHtml+="<li>";
                    specAllHtml+="<div class=\"emiss-title c999\">车型</div>";
                    specAllHtml+="<div class=\"emiss-price c999\">厂商指导价</div>";
                    specAllHtml+="<div class=\"emiss-fen c999\">用户评分</div>";
                    specAllHtml+="<div class=\"emiss-ren c999\">参与人数</div>";
                    specAllHtml+="<div class=\"emiss-btn c999\">&nbsp;</div>";
                    specAllHtml+="</li>";
                    for(var i=0;i<specList.length;i++)
                    {
                        var spec=specList[i];

                        specAllHtml+="<li class=\"specrow\" data-value=\""+spec.CapacityArrange+"\">";
                        specAllHtml+="<div class=\"emiss-title\">";
                        specAllHtml+="<a href=\"/spec/"+spec.SpecId+"/\" target=\"_blank\">"+ spec.SpecName +"</a>";
                        specAllHtml+="</div>";

                        specAllHtml+="<div class=\"emiss-price\">";
                        if(spec.MinPrice>0)
                        {
                            specAllHtml+="<a href=\"http://www.autohome.com.cn/spec/"+spec.SpecId
                                +"/price.html\" class=\"font-14 red font-arial mr-5\" target=\"_blank\">"+spec.MinPrice +"</a>万";
                        }
                        else
                        {
                            specAllHtml+="--";
                        }

                        specAllHtml+="</div>";

                        specAllHtml+="<div class=\"emiss-fen\">";
                        if(spec.Average>0)
                        {
                            specAllHtml+="<a href=\"/spec/"+spec.SpecId+
                                "/\" target=\"_blank\" class=\"font-18 red font-arial\"><span class=\"score score-kb\"><b style=\"width:"
                                + (spec.Average / 5) * 100+"%;\"></b></span>"+spec.Average+"</a>";
                        }
                        else
                        {
                            specAllHtml+="--";
                        }
                        specAllHtml+="</div>";

                        specAllHtml+="<div class=\"emiss-ren\">";
                        specAllHtml+="<a href=\"/spec/"+spec.SpecId+"/\" target=\"_blank\" class=\"font-14 red font-arial mr-5\">"
                            +spec.EvaluationCount+"</a>人";
                        specAllHtml+="</div>";

                        specAllHtml+="<div class=\"emiss-btn\">";

                        specAllHtml+="<a href=\"/form/carinput/add/"+spec.SpecId+"\" addurl=\"/form/carinput/add/"+spec.SpecId
                            +"\" target=\"_blank\"";
                        specAllHtml+=" class=\"btn btn-small btn-orange js-addEvaluation\" editeurl=\"/form/carinput/edit/\" data-val=\"{\"objectId\":\""
                            + spec.SpecId+"\",\"objectType\":\"1\"}\">";
                        specAllHtml+="发表口碑</a>";

                        specAllHtml+="</div>";
                    };
                    $("#specListUL").html(specAllHtml);


                    if(gloablePailiang!="")
                    {
                        pailiangShow();
                    }
                    else
                    {
                        if($('li', $('#specListUL')).length>8){$('.year-btn-cont').show();}else{$('.year-btn-cont').hide();}
                    }
                }
            });
        }

        var initVisible = function () {
            var SpecReturnListCount=8;
            var specListTableCount=5;
            if (SpecReturnListCount > 8) {
                specListTableCount = 5;
            }else{ specListTableCount = 8;}

            if (gloablePailiang == '')
            {
                if (gloableMoreRowShow) {
                    //异步获取所有车型信息，并重新渲染页面
                    if($('li', $('#specListUL')).length!= SpecReturnListCount+1)
                    {
                        getAllSpecHtml(3347,1,0 );
                    }
                    else
                    {
                        $('li.specrow', $('#specListUL')).removeClass('morerow');
                    }
                }
                else {
                    $('li.specrow', $('#specListUL')).addClass('morerow');
                    $('li.specrow', $('#specListUL')).slice(0, specListTableCount).removeClass('morerow');
                }
                if($('li', $('#specListUL')).length>5){$('.year-btn-cont').show();}else{$('.year-btn-cont').hide();}
            }
            else
            {
                    //异步获取所有车型信息，并重新渲染页面
                    if($('li', $('#specListUL')).length!= SpecReturnListCount+1)
                    {
                        getAllSpecHtml(3347,1,0 );
                    }
                    else
                    {
                        pailiangShow();
                    }
            }
        }

        function pailiangShow()
        {
            $('li.specrow', $('#specListUL')).addClass('morerow');
            var speccount=$('li[data-value="' + (gloablePailiang) + '"]', $('#specListUL')).length;
            if(speccount>8)
            {
                specListTableCount=5;
            }
            else
            {
                specListTableCount=speccount;
            }
            if (!gloableMoreRowShow) {
                $('li[data-value="'+(gloablePailiang)+'"]', $('#specListUL')).slice(0, specListTableCount).removeClass('morerow');
            }
            else {
                $('li[data-value="' + (gloablePailiang) + '"]', $('#specListUL')).removeClass('morerow');
            }
            if($('li[data-value="' + (gloablePailiang) + '"]', $('#specListUL')).length>8){$('.year-btn-cont').show();}else{$('.year-btn-cont').hide();}
        }
    });
</script>
