<script>
    var usedProviceArr=[[340000,'anhui','安徽'],[820000,'aomen','澳门'],[110000,'beijing','北京'],[500000,'chongqing','重庆'],[350000,'fujian','福建'],[620000,'gansu','甘肃'],[440000,'guangdong','广东'],[450000,'guangxi','广西'],[520000,'guizhou','贵州'],[460000,'hainan','海南'],[130000,'hebei','河北'],[230000,'heilongjiang','黑龙江'],[410000,'henan','河南'],[420000,'hubei','湖北'],[430000,'hunan','湖南'],[320000,'jiangsu','江苏'],[360000,'jiangxi','江西'],[220000,'jilin','吉林'],[210000,'liaoning','辽宁'],[150000,'namenggu','内蒙古'],[640000,'ningxia','宁夏'],[630000,'qinghai','青海'],[610000,'shan_xi','陕西'],[370000,'shandong','山东'],[310000,'shanghai','上海'],[140000,'shanxi','山西'],[510000,'sichuan','四川'],[710000,'taiwan','台湾'],[120000,'tianjin','天津'],[810000,'xianggang','香港'],[650000,'xinjiang','新疆'],[540000,'xizang','西藏'],[530000,'yunnan','云南'],[330000,'zhejiang','浙江']];
    var usedProviceId=0;
    usedProviceId=parseInt(usedProviceId/10000)*10000;
    var usedpinyin;
    if(usedProviceId==0){
        usedpinyin ="china";
    }
    else{
        for (var i = 0; i < usedProviceArr.length; i++) {
            if(usedProviceArr[i][0]==usedProviceId) {
                usedpinyin=usedProviceArr[i][1];}
        }
