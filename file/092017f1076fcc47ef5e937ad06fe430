        var returntipcollection = function (title) {
            return '<div class="layer layer-collect"><div class="layer-content"><div class="fn-clear"><span class="fn-left"><i class="icon33 icon33-ok"></i>' + title + '！</span><p><a href="http://i.autohome.com.cn/favor?tabid=11" target="_blank">查看我的收藏&gt;&gt;</a></p></div></div></div>';
        }
        var reloadstatus = function () {
            if (_currentuser != null) {
                $('.js-collecteBtn').each(function (i, n) {
                    var koubeiId = $(n).attr('data-val');
                    $.ajax({
                        url: 'http://ms.i.autohome.com.cn/ajax/collection/exist_by_accountId?' + "accountId=" + _currentuser.MemberId + "&objId=" + koubeiId,
                        dataType: "jsonp",
                        jsonp: "callback",
                        success: function (data) {
                            if (data) {
                                $('.js-collectionstart', $(n)).removeClass('icon16-star1').addClass('icon16-star2');
                                //$(n).html($(n).html().replace('收藏该口碑', '取消收藏'));
                            } else {
                                $('.js-collectionstart', $(n)).removeClass('icon16-star2').addClass('icon16-star1');
                                // $(n).html($(n).html().replace('取消收藏', '收藏该口碑'));
                            }
                        }
                    });
                });
            }
        }
        var hideCollectTip = function () {
            $('.js-collectionTipBox').hide();
        }
        $('.js-collecteBtn').click(function () {
            if (_currentuser == null) {
                window.location = 'http://account.autohome.com.cn/?backurl=' + window.location; return;
            }
            var objectId = $(this).attr('data-val');
            var currentBtn = this;
            var haveNoCollection = $('.js-collectionstart', $(currentBtn)).hasClass('icon16-star1');
            if (haveNoCollection) {
                $.ajax({
                    type: "post",
                    url: "/Controls/CollectionEvaluation",
                    data: "id=" + objectId,
                    success: function (returndata) {
                        if (returndata.returncode == 0) {
                            $('.js-collectionTipBox').html(returntipcollection('收藏成功'));
                            $('.js-collectionTipBox', $(currentBtn).parents('.js-collectionbox')).css('opacity', '1').show().animate({ opacity: '0.7' }, 2500, hideCollectTip);
                            //                            $('.js-collectionstart', $(currentBtn)).removeClass('icon16-star1').addClass('icon16-star2');
                            reloadstatus();
                        } else {
                            reloadstatus(); // alert(returndata.message);
                        }
                    },
                    error: function () {
                        alert('收藏失败！');
                    }
                });
            } else {
                $.ajax({
                    type: "post",
                    url: "/Controls/DeleteCollection",
                    data: "id=" + objectId,
                    success: function (returndata) {
                        if (returndata.returncode == 0) {
                            $('.js-collectionTipBox').html(returntipcollection('取消收藏'))
                            $('.js-collectionTipBox', $(currentBtn).parents('.js-collectionbox')).css('opacity', '1').show().animate({ opacity: '0.7' }, 2500, hideCollectTip);
                            //                            $('.js-collectionstart', $(currentBtn)).removeClass('icon16-star2').addClass('icon16-star1');
                            reloadstatus();
                        } else {
                            reloadstatus(); //alert(returndata.message);
                        }
                    },
                    error: function () {
                        alert('删除收藏失败！');
                    }
                });
            }
        });

        var importantinit = function () {
            $.ajax({
                type: "post",
                url: "/Controls/GetCurrentUserId",
                data: "",
                success: function (returndata) {
                    _currentuser = returndata;
                    reloadstatus();
                    //init_jili(_currentuser ? _currentuser.Id : 0);
                },
                error: function () {
                    //init_jili(0);
                }
            });
        }

        //// 激励体系
        //function init_jili(userid) {
