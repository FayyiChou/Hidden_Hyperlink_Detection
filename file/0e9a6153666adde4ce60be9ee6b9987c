	function getCode(){
		var sUser = fTrim(document.getElementById("userName").value);
		if(sUser==null||sUser==""){
			alert("输入用户名！");
			return;
		}
		var sPass = fTrim(document.getElementById("password").value);
		if(sPass==null||sPass==""){
			alert("输入密码！");
			return;
		}
        sPass = rsaEncrypt(sPass);
		var codeUrl = "https://m.scmcc.com.cn/tailor/http://eip.scmcc.com.cn/WebSecurityCodeService/SecurityCodeService.ashx";
		var thisData = "method=getCode&UserLoginName="+sUser+"&UserPassword="+sPass+"&sBrowser=brow";
		var oXMLHttpRequest =new XMLHttpRequest(); 
		oXMLHttpRequest.onreadystatechange = bulidMsgList;  			
		oXMLHttpRequest.open("post",codeUrl,true); 		oXMLHttpRequest.setRequestHeader("Content-Type","application/x-www-form-urlencoded");
		oXMLHttpRequest.send(thisData);
		function bulidMsgList(){			
			if(oXMLHttpRequest.readyState == 4){	
				var res = oXMLHttpRequest.responseText;
				if(res){
					var ss  =res.split("|");
					if(ss.length==2){
						var a1 = ss[1];
                        if(a1.indexOf("<")>-1){
                            a1 = a1.substring(0,a1.indexOf("<"));
                        }
						alert(a1);
					}
				}
			}
		}
	}
