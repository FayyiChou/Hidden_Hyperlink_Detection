    function fSetRemember() {
		var isRemember = getCookie("chkRemember") || "";
		var ochkRemember= document.getElementById("chkRemember"); 
		if (isRemember=="1") {
	 		ochkRemember.checked=true;
		} else {
			ochkRemember.checked=false;
		}
