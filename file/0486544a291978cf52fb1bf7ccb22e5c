    <script type="text/javascript">
    

    var sso_url_glo =  "https://sso.kongzhong.com";
    KZLoginHandler.loginServer = sso_url_glo;
    var require_vcode_glo = 0;
    var reLoginCnt_glo = 5;

    function f_readLoginUser(){
       var userAccount = $.trim($.KzCookie.g('UDT-KZG'));
       if(userAccount != null && userAccount != ""){
    		//处理登录成功之后该显示那些信息。******
    		loginsuccess();
       }else{
    	    //处理没有登录，该显示那些信息。******
    		alert("登录失败！请重新登录！");
    		window.location.href = window.location.href;
    		cancelloading();
       }
    }

    $(function(){
    		$('#chg_vcode_btn').click(function(){
    			$(this).attr("src", sso_url_glo+"/createVCode?w=80&h=30&"+Math.random());
    		});
    		
    		$('#login_btn').click(function(){
    			var vcode = "";
    			
    			if(!check_input()){
    				return ;
    			}
    			if(require_vcode_glo == 1 ){
    				vcode = $('#vcode').val();
    			}
    			
    			KZLoginHandler.login(encodeURIComponent($.trim($('#username').val()).toLocaleLowerCase()), encodeURIComponent($.trim($('#password2').val())), false, vcode, function(data){
    				if(data.logged == false){
    					if(data.errors != null 
    						&& data.errors != "" 
    						&& data.errors[0] != null){
    						if(data.errors[0].code == "INVALID_TICKET2" 
    							|| data.errors[0].code == "INVALID_TICKET"){
    							if(reLoginCnt_glo > 0){
    								reLoginCnt_glo--;
    								return ;
    							}
    						}
    					}
    					
    					if(data["requirevcode"]){
    						require_vcode_glo=1;//下次登录需验证码
    						$('#chg_vcode_btn').click();
    						$('#vcode_area').show();
    					}else{
    						$('#chg_vcode_btn').attr("src", "");
    						$('#vcode_area').hide();
    					}
    					var errs = decodeURIComponent(data.errors&&data.errors[0].etext)||"登录失败";
    					alert(errs);
    					cancelloading();
    				}else{
    					//登录成功
    					f_readLoginUser();
    				}
    			});
    		});
    		
    		check();
    		
    	});

    function check(){
    	//判断是否已经登录
    	KZLoginHandler.check(function(data){
    		if(data.logged){
    			f_readLoginUser();
    		}else{
    			//需要验证码
    			if(data["requirevcode"]){
    				require_vcode_glo = 1;
    				$('#chg_vcode_btn').click();
    				$('#vcode_area').show();
    			}
    		}
    	});	
    }

    	
    function check_input(){
    	addloading();
    	var user = document.getElementById("username").value;
    	var pwd = document.getElementById("password2").value;
    	var vcodeobj = document.getElementById('vcode');
    	if(user==""){
    		$("#usernameerror").html(errorhtml("请输入账号名"));
    		$("#usernameerror").css("display","block");
    		cancelloading();
    		return false;
    	}
    	
    	if(pwd==""){
    		$("#password2error").html(errorhtml("请输入密码"));
    		$("#password2error").css("display","block");
    		cancelloading();
    		return false;
    	}
    	
    	if(require_vcode_glo == 1 && vcodeobj.value==""){
    		$("#vcodeerror").html(errorhtml("请输入验证码"));
    		$("#vcodeerror").css("display","block");
    		cancelloading();
    		return false;
    	}
    	
    	return true;
    	//checkVcode(vcodeobj);
    }
    	
    function quitmall(){
    	window.location.href = sso_url_glo+"/logout?service="+window.location.href;//http://gw2.kongzhong.com/ztm/handsel/index.html
    }
    
    function logout(){
    	var logoutUrl = sso_url_glo+"/logout";
    	if($("#logout").length>0){
    		check();
    		$("#logout").attr("src",logoutUrl);
    	}
    }
    
    function changeImg(){
    	$('#chg_vcode_btn').click();
    }
    
    
    function loginsuccess(){
		$.ajax({
			url : "/login/checklogin",
			async : false,
			dataType : "json",
			cache : false,
			success : function(data) {
				if(data.indexOf("||")>-1){
					var info = data.split("||");
					showmalluserinfo(info[1],info[2],info[3],info[4],info[5],info[6],info[7]);
				}
				if("NoLogin"==data){
					logout();
				}
			}
			
		});
    }
	</script>
