function loadHotBrandList(lang, bid, product){
    var hotBrands = "";
    var bid = bid;
    var lang ="chinese";
    var product =product;
        var url = "/product/load_hot_brands?bid=" + bid + "&product=" + product + "&lang=" + lang;
    $.getJSON(url,function(data){
        $.each(data, function(i,item){
            var brand_code = "";
            if ("" == item.cname || lang != "chinese") {
                brand_code = item.ename;
            } else {
                brand_code = item.cname
            }
            hotBrands += "<span><a href=\"javascript:void(0)\" onclick=\"closeWin('brand_text_win');openWin('model_text_win');loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '" + item.id + "', '')\">" +  brand_code  + "</a></span>";
        });
        $('#hotBrands').html(hotBrands + '</ul>');
    })
}

var brand_first_list = [];
var brand_data = {};
var selectBrand = function(first, lang, bid, product) {
    loadHotBrandList(lang,bid,product);
    if (first === '') {
        brand_first_list = [];
        brand_data = {};
    }
    if (brand_first_list.length == 0) {
        var url = "/product/choose_brands?bid=" + bid + "&product=" + product + "&lang=" + lang;
        if (first !== '') {
            url = "/product/choose_brands?bid=" + bid + "&product=" + product + "&lang=" + lang + "&type=" + first;
        }
        $.ajaxSettings.async = false;
        $.getJSON(url, function(data) {
            $.each(data, function(key, value) {
                brand_first_list.push(key);
            });
            brand_data = data;
        });
    }
    var first_letter_str = "";
    if (lang == 'arabic') {
        first_letter_str = "<p>快速定位</p><a href=\"javascript:void(0)\" onclick=\"selectBrand('all', '"+ lang +"', '"+ bid +"', '"+ product +"')\" style='color: #F60;'>الكل</a>";
    } else {
        first_letter_str = "快速定位<a href=\"javascript:void(0)\" onclick=\"selectBrand('all', '"+ lang +"', '"+ bid +"', '"+ product +"')\" style='color: #F60;'>ALL</a>";
    }
    if (first !== '' && first != 'all') {
        if (lang == 'arabic') {
            first_letter_str = "<p>快速定位</p><a href=\"javascript:void(0)\" onclick=\"selectBrand('all', '"+ lang +"', '"+ bid +"', '"+ product +"')\">الكل</a>";
        } else {
            first_letter_str = "快速定位<a href=\"javascript:void(0)\" onclick=\"selectBrand('all', '"+ lang +"', '"+ bid +"', '"+ product +"')\">ALL</a>";
        }
    }
    var charlistStr = "";
    if (first !== '' && first != 'all') {
        for (var j = 0; j < brand_first_list.length; j++) {
            var first_letter = brand_first_list[j];
            if (first_letter == first)
                first_letter_str += "<a href=\"javascript:void(0)\" onclick=\"selectBrand('" +first_letter+ "', '"+ lang +"', '"+ bid +"', '"+ product +"')\" style='color:#F60;'>" + first_letter + "</a>";
            else
                first_letter_str += "<a href=\"javascript:void(0)\" onclick=\"selectBrand('" +first_letter+ "', '"+ lang +"', '"+ bid +"', '"+ product +"')\">" + first_letter + "</a>";
        }
        var brandList = brand_data[first];
        if('undefined' != typeof(brandList)) {
            charlistStr += "<dl><dt><a name='" + first + "'></a>" + first + "</dt><dd>";
            var brand_code = "";
            for (var i = 0; i < brandList.length; i++) {
                if ("chinese" == lang) {
                    brand_code = brandList[i].cname;
                } else {
                    brand_code =  brandList[i].ename;
                }
                charlistStr += "<a href='javascript:void(0)' onclick=\"closeWin('brand_text_win');openWin('model_text_win');loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '" + brandList[i].id + "', '')\">" + brand_code+ "</a>";
            }
            charlistStr += "</dd></dl>";
        }
    } else {
        for (var j = 0; j < brand_first_list.length; j++) {
            var first_letter = brand_first_list[j];
            first_letter_str += "<a href=\"javascript:void(0)\" onclick=\"selectBrand('" +first_letter+ "', '"+ lang +"', '"+ bid +"', '"+ product +"')\">" + first_letter + "</a>";
            var brandList = brand_data[first_letter];
            if('undefined' != typeof(brandList)) {
                charlistStr += "<dl><dt><a name='" + first_letter + "'></a>" + first_letter + "</dt><dd>";
                var brand_code = "";
                for (var i = 0; i < brandList.length; i++) {
                    if ("chinese" == lang) {
                        brand_code = brandList[i].cname;
                    } else {
                        brand_code = brandList[i].ename;
                    }
                    charlistStr += "<a href='javascript:void(0)' onclick=\"closeWin('brand_text_win');openWin('model_text_win');loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '" + brandList[i].id + "', '')\">" + brand_code+ "</a>";
                }
                charlistStr += "</dd></dl>";
            }
        }
    }
    $('#brand_rapid_pos').html(first_letter_str);
    $('#brand_text_list').html(charlistStr + '<div class="space_15"></div>')
}

var openDownload = function(lang, bid, product, mid, type){
    window.location.href="/product/product_down_" + product + ".shtml?lang=" + lang + "&bid=" + bid + "&mid=" + mid + "&by=" + type;;
};

/**
 * 加载机型列表
 * @param {} brandId
 * @param {} first
 */
var model_data = {};
var model_first_list = [];
function loadModelList(lang, bid, product, brandId, first) {
     $('#choose_img_btn').attr('brand', brandId);
    if (first === '') {
        model_first_list = [];
        model_data = {};
    }
    if (model_first_list.length == 0) {
        if (typeof(brandId) == 'undefined') {
            brandId = $('#choose_img_btn').attr('brand');
        }
        var url = "/product/load_models?brandId=" + brandId + "&product=" + product + "&bid=" + bid + "&lang=" + lang;
        if (first !== '') {
            url = "/product/load_models?brandId=" + brandId + "&product=" + product + "&bid=" + bid + "&lang=" + lang + "&type=" + first;
        }
        $.ajaxSettings.async = false;
        $.getJSON(url, function(data) {
            $.each(data, function(key, value){
                model_first_list.push(key);
            });
            model_data = data;
        });
    }
    var first_letter_str = "";
    if (lang == 'arabic') {
        first_letter_str = "<p>快速定位:</p><a href=\"javascript:void(0)\" onclick=\"loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '" + brandId + "', 'all')\" style='color:#F60'>الكل</a>";
    } else {
        first_letter_str = "快速定位:<a href=\"javascript:void(0)\" onclick=\"loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '" + brandId + "', 'all')\" style='color:#F60'>ALL</a>";
    }
    if (first !== '' && first != 'all') {
        if (lang == 'arabic') {
            first_letter_str = "<p>快速定位:</p><a href=\"javascript:void(0)\" onclick=\"loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '" + brandId + "', 'all')\">الكل</a>";
        } else {
            first_letter_str = "快速定位:<a href=\"javascript:void(0)\" onclick=\"loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '" + brandId + "', 'all')\">ALL</a>";
        }
    }
    var charlistStr = "";
    if (first !== '' && first != 'all') {
        for (var j = 0; j < model_first_list.length; j++) {
            var first_letter = model_first_list[j];
            if (first_letter == first)
                first_letter_str += "<a href=\"javascript:void(0)\" onclick=\"loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '"+ brandId+"', '" +first_letter+ "')\" style='color:#F60;'>" + first_letter + "</a>";
            else
                first_letter_str += "<a href=\"javascript:void(0)\" onclick=\"loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '"+ brandId +"', '" +first_letter+ "')\">" + first_letter + "</a>";
        }
        var modelList = model_data[first];
        if('undefined' != typeof(modelList)) {
            charlistStr += "<dl><dt><a name='" + first + "'></a>" + first + "</dt><dd>";
            for (var i = 0; i < modelList.length; i++) {
                charlistStr += "<span><a href=\"javascript:void(0)\" onclick=\"openWin('model_win');openDownload('"+ lang +"', '"+ bid +"', '"+ product +"', '"+ modelList[i].id +"', 'slb');return false;\">"+ modelList[i].keywords +"</a></span>";
            }
            charlistStr += "</dd></dl>";
        }
    } else {
         for (var j = 0; j < model_first_list.length; j++) {
             var first_letter = model_first_list[j];
             first_letter_str += "<a href=\"javascript:void(0)\" onclick=\"loadModelList('"+ lang +"', '"+ bid +"', '"+ product +"', '"+ brandId +"', '"+ first_letter+ "')\">"+ first_letter +"</a>";
             var modelList = model_data[first_letter];
             if('undefined' != typeof(modelList)) {
                 charlistStr += "<dl><dt><a name='" + first_letter + "'></a>" + first_letter + "</dt><dd>";
                 for (var i = 0; i < modelList.length; i++) {
                     charlistStr += "<span><a href=\"javascript:void(0)\" onclick=\"openWin('model_win');openDownload('"+ lang +"', '"+ bid +"', '"+ product +"', '"+ modelList[i].id +"', 'slb');return false;\">"+ modelList[i].keywords +"</a></span>";
                 }
                 charlistStr += "</dd></dl>";
             }
         }
    }
    $('#model_rapid_pos').html(first_letter_str);
    $('#model_text_list').html(charlistStr + '<div class="space_15"></div>');
}

/**
 * 加载品牌Logo
 */
function loadBrandLogo(lang, bid, product) {
    var content = '<ul>';
    var brand_data_all = [];
    var url = "/product/choose_brands?bid=" + bid + "&product=" + product + "&lang=" + lang + "&by=sli";
    $.getJSON(url, function(data) {
        $.each(data, function(key, value) {
            $.each(value, function(i, b) {
                brand_data_all.push(b);
            });
        });
    });
    var brand_data_order = brand_data_all.sort(
        function(a, b)
        {
            if(a.displayOrder < b.displayOrder) return -1;
            if(a.displayOrder > b.displayOrder) return 1;
            return 0;
        }
    );
    $.each(brand_data_all, function(i, value) {
        var brand_name = ("chinese" == lang ? value.cname : value.ename);
        if (value.logo == '1') {
            content += '<li><a onclick="closeWin(\'brand_logo_win\');openWin(\'model_pic_win\');loadModelPic(\'' + lang + '\', \'' + bid + '\', \'' + product + '\', \'' + value.id+ '\')"><img src="http://image.uc.cn/s/uae/g/0d/brand_logo/' + value.id + '" /><p>' + brand_name + '</p></a></li>';
        } else {
            content += '<li><a onclick="closeWin(\'brand_logo_win\');openWin(\'model_pic_win\');loadModelPic(\'' + lang + '\', \'' + bid + '\', \'' + product + '\', \'' + value.id+ '\')"><img src="/public/pdia_res/other/no_brand.gif" /><p>' + brand_name + '</p></a></li>';
        }
    });
    content += '</ul>';
    $('#brand_logo_list').html(content);
}

function loadModelPic(lang, bid, product, brandId) {
    if (brandId === '') {
        brandId = $('#choose_img_btn').attr('brand');
    }
    var url = "/product/load_models?bid=" + bid + "&brandId=" + brandId + "&product=" + product + "&lang=" + lang + "&by=sli";
    $.getJSON(url, function(data) {
        var content = "<ul>";
        $.each(data, function(key, value) {
            for(var i = 0; i < value.length; i++) {
                if (value[i].logo == '1') {
                    content += "<li><a href=\"javascript:void(0)\" onclick=\"openDownload('"+ lang +"', '"+ bid +"', '"+ product +"', '" + value[i].id + "', 'sli');return false;\"><img src=\"http://image.uc.cn/s/uae/g/0d/model_logo/" + value[i].id + "\" /><p>" + value[i].keywords + "</p></a></li>";
                } else {
                    content += "<li><a href=\"javascript:void(0)\" onclick=\"openDownload('"+ lang +"', '"+ bid +"', '"+ product +"', '" + value[i].id + "', 'sli');return false;\"><img src=\"/public/pdia_res/other/no_model.gif\" /><p>" + value[i].keywords + "</p></a></li>";
                }
            }
        });
        $('#model_pic_list').html(content + '</ul>');
    });
}

/**
 * 加载某平台
 * @param {} platform
 */
function loadPlatform(platform, type){
    var content = "";
    if (platform == 'wpad') {
        content = '<div class="radius_title"><h3><img id="platform_win_title_img" src="/public/pdia_res/other/wp.jpg">请选择适合您手机的UC版本</h3></div>';
    } else {
         content = '<div class="radius_title"><h3><img id="platform_win_title_img" src="/public/pdia_res/other/' + platform + '.jpg">请选择适合您手机的UC版本</h3></div>';
    }
    var  url = '/product/resource_pack?product=ucbrowser&bid=999&lang=chinese&platform=' + platform +'&type=' + type+ '&_t=' + (new Date()).getTime();
    $.get(url, function(data){
        $('#pfid_list').html(content + data);
        if(data.indexOf("platform_win_div") > 0) {
            openWin('platform_win');
        }
    });
}

function modelSearch() {
    var keyword = $('#search_input').attr('value');
    if(keyword == '输入手机型号,如N97')
        return;
    var product ="ucbrowser";
    var lang = "chinese";
    var bid = "999";
    var url = "/product/search_models?bid=" + bid + "&keyword=" + keyword + "&product=" + product + "&lang=" + lang;
    $.getJSON(url, function(data) {
        var listContent = '<div class="search_list">';
        $.each(data, function(i,item){
            var list_models =item.list;
            listContent +="<p>"+ item.brandName+"</p>";
            for(var j = 0; j < list_models.length; j++) {
                listContent += '<a href="/product/product_down_' + product + '.shtml?bid=' + bid + '&mid=' + list_models[j].id + '&lang='+ lang +'">' + list_models[j].keywords + '</a>';
            }
        });
        if(data.length>0){
            $('#hotModels').html(listContent + '</div>');
        } else {
            $('#hotModels').html('<div class="down_hot_list">未找到您要查找的机型</div>');
        }
    });
}

var search_keywords_before;

function initSearchForm(){
    $("#search_input").focus(function(){
    search_keywords_before = this.value;
    this.value='';
    $(this).css('color', "#000");
});

    $("#search_input").blur(function(){
    if(this.value.length == 0){
        this.value=search_keywords_before;
        $(this).css('color',"");
        loadHotModelList();
    }
});
}

function getScript(url, op){
    var doc = window.document,
        s = doc.createElement('script'),
        h = doc.getElementsByTagName('head')[0],
        done = false;
    s.type = 'text/javascript';
    if (!op)
        op = {};
    else if (typeof op == 'function')
        op = { callback: op };
    if (op.charset)
        s.charset = op.charset;
    s.src = url;
    s.onload = s.onreadystatechange = function () {
        if (!done && (!this.readyState || this.readyState == 'loaded' || this.readyState == 'complete')) {
            done = true;
            s.onload = s.onreadystatechange = null;
            h.removeChild(s);
            if (op.callback)
                op.callback();
        }
    };
    h.appendChild(s);
}

function start(lang,bid,product){
    initSmsForm();
    initSearchForm();
};

var bid, params = {}, paramArr = window.location.search.split('?');
if(typeof(paramArr[1]) != 'undefined') paramArr = paramArr[1].split('&');
for(var i = 0; i < paramArr.length; ++i){
    var pArr = paramArr[i].split('=');
    params[pArr[0]] = encodeURIComponent(pArr[1]);
}
bid = parseInt(params.bid) || '999';
   start('chinese','999','ucbrowser');
</script>


