}

function getVendorCnNameByVid(vid){
    for(var i in arr){
        if(arr[i][0].toLowerCase() == vid.toLowerCase()) return arr[i][1];
    }
    return null;
}

function loadHotModels(lang, bid, product) {
    var hotModels = "";
    hotModels += '<ul class="down_hot_list clearfix">';
    var url = "/product/load_hot_models?bid=" + bid + "&product=" + product + "&lang=" + lang;
    $.getJSON(url, function(data) {
        $.each(data, function(i, item) {
            hotModels += '<li><a href="/product/product_down_' + product + '.shtml?lang=' + lang + '&bid=' + bid + '&mid=' + item.id + '&by=slh">' + item.keywords + '</a></li>';
        });
        $('#hotModels').html(hotModels + '</ul>');
    });
}

loadHotModels('chinese', '999', 'ucbrowser');

 /**
  * 加载热门品牌
