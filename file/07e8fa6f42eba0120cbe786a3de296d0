<div class="clear"></div><script type="text/javascript">
        var currentIndex = 0
        function addLoadEvent(func) {
            var oldonload = window.onload;
            if (typeof window.onload != 'function') {
                window.onload = func;
            } else {
                window.onload = function() {
                    oldonload();
                    func();
                }
            }
        }


        function menuhover() {
            var menu = document.getElementById("menubox");
            var links = menu.getElementsByTagName("li");
            for (c = 0; c < links.length; c++) {
                links[c].onmouseover = function() {
                    for (m = 0; m < links.length; m++) {
                        links[m].className = '';
                    }
                    this.className = 'Classon';
                }
             }
        }
function displaySubMenu(id){

if(document.getElementById("menu_list_"+397) != null){
document.getElementById("menu_list_"+397).style.display="none";
}

if(document.getElementById("menu_list_"+1397) != null){
document.getElementById("menu_list_"+1397).style.display="none";
}

if(document.getElementById("menu_list_"+1400) != null){
document.getElementById("menu_list_"+1400).style.display="none";
}

if(document.getElementById("menu_list_"+1425) != null){
document.getElementById("menu_list_"+1425).style.display="none";
}

if(document.getElementById("menu_list_"+1403) != null){
document.getElementById("menu_list_"+1403).style.display="none";
}

if(document.getElementById("menu_list_"+1398) != null){
document.getElementById("menu_list_"+1398).style.display="none";
}

if(document.getElementById("menu_list_"+1419) != null){
document.getElementById("menu_list_"+1419).style.display="none";
}

if(document.getElementById("menu_list_"+1405) != null){
document.getElementById("menu_list_"+1405).style.display="none";
}

if(document.getElementById("menu_list_"+1404) != null){
document.getElementById("menu_list_"+1404).style.display="none";
}

if(document.getElementById("menu_list_"+id) != null){
document.getElementById("menu_list_"+id).style.display="block";
}
}
displaySubMenu(0);
addLoadEvent(menuhover);
    </script></div>
