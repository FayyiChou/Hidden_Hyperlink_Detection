  function OnSearchCheckAndSubmit(){
      var keyword = document.getElementById("keyword").value;
      if (keyword == '' || keyword == null) {
          alert("请填写您想搜索的关键词");
          return;
      }
      else {
      
          window.location = "/search.aspx?searchtype=0&Keyword=" + escape(keyword);
      }
  }
  </script>
