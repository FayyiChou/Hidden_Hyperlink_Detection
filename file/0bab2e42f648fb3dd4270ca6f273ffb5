		<script>

						function close () {
							$.arcticmodal('close');
						}
						$('a#comp').click(function() {

							var name = $(this).attr('data-name');
							var element_id = $(this).attr('data-id');
							var list = $(this).attr('data-list');
							var c = $('<div class="b-modal" />');
							c.html('Товар '+name+' добавлен в <a href="/compare.php">таблицу сравнения</a>');

							$.arcticmodal({
								content: c
							});

							setTimeout(close, 2500);

							$.get(
							  document.location.href,
							  {
								action: "ADD_TO_COMPARE_LIST",
								id: element_id
							  },
							  onAjaxSuccess
							);

							function onAjaxSuccess(data){
							  // Здесь мы получаем данные, отправленные сервером и выводим их на экран.
							}

							$.cookie('compare_section', list);

							if($.cookie('com') < 1) {
								$.cookie('com', 0);
							}

							var com = parseInt($.cookie('com'))+1;

							$.cookie('com', com);

							alert($.cookie('com'));

							//$('a#compare').attr('href', list+'compare.php');
							if(com > 1) {
								$('a#compare').css('display', 'block');
							}
							return false;
						});
					</script>




