<script>
$(function(){

	var $compare_failure = $("#compare_failure").appendTo("body");
		//如果存在cookie中，从cookie中取出信息并显示
		if ($.cookie('compare_wh')) {
			var products = $.cookie('compare_wh').split('&&');
			for ( i = 0; i < products.length; i++) {
				var $product = $("#product_contrast").show();
				var pro_json = jQuery.parseJSON(products[i].toString());
				var $cl = $product.find(".tpvslist").eq(0).clone().show().attr('id', 'tp_' + pro_json.id).attr("pro_id", pro_json.id);
				$cl.find('.typeimg img').attr({
					src : pro_json.img_src
				});
				$cl.find('.typeinfos .product_name').html(pro_json.product_name);
				//$cl.find('.typeinfos .product_exp').html('最低认购：');
				$cl.find('.typeinfos .sytxt').html('返佣：外汇'+pro_json.wh+'，黄金'+pro_json.hg+'<br/>　　　白银'+pro_json.by+'，原油'+pro_json.yy);
				$product.find("#list_cnt").append($cl);
				$("#" + pro_json.id).addClass("checked");
			}

		}

		$(".md_checkbox").click(function() {
			var root = $(this);
			root.hasClass("checked") ? root.removeClass("checked") : root.addClass("checked");	//模拟checkbox
			var id = root.attr("id");
			var str = '{ "img_src": "' + root.attr('img_src')  +
			            '",  "product_name": "' + root.attr('product_name')+
			            '",  "wh": "' + root.attr('wh')+
			            '",  "hg": "' + root.attr('hg')+
			            '",  "by": "' + root.attr('by')+
			            '",  "yy": "' + root.attr('yy')+
			            '", "id": "' + id +'"}';

			//选中产品响应事件
			if (root.hasClass("checked")) {

				var $product = $("#product_contrast").show();
				if ($product.find("#list_cnt .tpvslist").length > 3) {

				$compare_failure.find("span").text('').text('最多只能对比四个产品')
				var $jfz_dg = new $.JFZ_Dialog({
	   		 	 		source: {
						"inline": $compare_failure.show()
						},
						overlay_close : false,
						buttons : false,
						type    : false,
						width   : 390


				 });
				 $('.p-btnsure').click(function(){
						$jfz_dg.close();
					})

					root.removeClass("checked");
				} else {

					//选中产品加入到cookie
					if ($.cookie('compare_wh')) {
						var old = $.cookie('compare_wh');
						var new_str = old + "&&" + str;
						$.cookie('compare_wh', new_str, {
							path : '/'
						});
					} else {
						$.cookie('compare_wh', str, {
							path : '/'
						});
					}

					if (!$.cookie('wh_product_' + id)) {
						//$.cookie('product_'+id, '{ "name": "cxh", "sex": "man" }');
						$.cookie('wh_product_' + id, '{ "img_src": "' + root.attr('img_src') +
						                                '",  "product_name": "' + root.attr('product_name') +
						                                '",  "wh": "' + root.attr('wh')+
											            '",  "hg": "' + root.attr('hg')+
											            '",  "by": "' + root.attr('by')+
											            '",  "yy": "' + root.attr('yy')+
						                                '", "id": "' + id + '"}', {
							path : '/'
						});

					}
					var pro_json = jQuery.parseJSON($.cookie('wh_product_' + id));
					var $cl = $product.find(".tpvslist").eq(0).clone().show().attr('id', 'tp_' + pro_json.id).attr("pro_id", pro_json.id);
					$cl.find('.typeimg img').attr({
						src : pro_json.img_src
					});
					$cl.find('.typeinfos .product_name').html(pro_json.product_name);
					//$cl.find('.typeinfos .product_exp').html('最低认购：');
					$cl.find('.typeinfos .sytxt').html('返佣：外汇'+pro_json.wh+'，黄金'+pro_json.hg+'<br/>　　　白银'+pro_json.by+'，原油'+pro_json.yy);
					$product.find("#list_cnt").append($cl);
				}

			} else {
				$("#tp_" + id).remove();
				$.cookie("wh_product_" + id, null);

				var p = $.cookie('compare_wh').split('&&')
				for ( i = 0; i < p.length; i++) {
					if (p[i] == str) {
						p.splice(i, 1);
						$.cookie('compare_wh', p.join('&&'), {
							path : '/'
						})
						break;
					}
				}

				if ($("#product_contrast").find("#list_cnt .tpvslist").length == 0) {
					$("#product_contrast").hide();
				}
			}

		});

		//关闭对比框
		$(".p-xtclose").click(function() {
			var $product = $("#product_contrast").hide();
			$product.find("#list_cnt .tpvslist").remove();
			//清除所有产品cookie
			$.cookie('compare_wh', null, {
				path : '/'
			});

			$(".checked").removeClass("checked");
		});

		//删除单个对比产品
		$("#list_cnt").delegate(".remove_image", "click", function() {
			$(this).parents('.tpvslist').remove();
			if ($("#product_contrast").find("#list_cnt .tpvslist").length == 0) {
				$("#product_contrast").hide();
			}

			var id = $(this).parents('.tpvslist').attr('pro_id');
			$("#" + id).removeClass("checked");
			var str = $.cookie('wh_product_' + id)
			$.cookie('wh_product_' + id, null, { path : '/' });
			var p = $.cookie('compare_wh').split('&&')
			for ( i = 0; i < p.length; i++) {
				if (p[i] == str) {
					p.splice(i, 1);
					$.cookie('compare_wh', p.join('&&'), { path : '/' })
					break;
				}
			}
		});

		//对比链接
		$(".linkvs").click(function() {
			var $pro_length = $("#product_contrast").find("#list_cnt .tpvslist").length
			if ($pro_length < 2) {

					$compare_failure.find("span").text('').text('至少有两个产品进行对比')
				var $jfz_dg = new $.JFZ_Dialog({
	   		 	 		source: {
						"inline": $compare_failure.show()
						},
						overlay_close : false,
						buttons : false,
						type    : false,
						width   : 390


				 });
				 $('.p-btnsure').click(function(){
						$jfz_dg.close();
					})
				return false;
			}


			var check_array = $("#list_cnt .tpvslist")
			// var check_array = $("input[name='contrast[]']:checked");
			var arr = [];
			var str = '';
			$.each(check_array, function(i, n) {
				arr[i] = $(n).attr('pro_id');
			});
			str = arr.join("_");
			$(this).attr('href', '/waihui/compare/index/ids/' + str)
		});

		$(".ping").hover(function(){
			$(this).children().show();
		}, function(){
			$(this).children().hide();
		})




    //悬停效果
    $(".pptxtmod").hover(function(){
        $(this).addClass('ppbg-hover');
    }, function(){
        $(this).removeClass('ppbg-hover');
    });

		init();
		function init(){
			ajaxsort();
		}
		//ajax排序
		function ajaxsort(){
			$(".fy_pagination li a").on('click', function(event){
				event.preventDefault();
				$.get($(this).attr("href"), function(data, status){
					$(".fy_main").html(data);
				});
				return false;
			});

			$(".wh_sort").on('click', function(){
				$.get("/waihui/fanyong/listAjax.html"+"?sort="+$(this).attr("data-sort"), function(data, status){
					$(".fy_main").html(data);
				});
				return false;
			});
		}
	});

    $('.footer, .kh_control').click(function(){
        var $yuyue_win = $("#Jrsv"),
            title = '立即开户';
        $jfz_dg = new $.JFZ_Dialog2({
            source : {
                "inline" : $yuyue_win.show()
            },
            overlay_close : false,
            buttons : false,
            width : 450,
            title : title,
            subtitle : ''
        });

        $('.dialog_close').click(function(){
            $jfz_dg.close();
            $("#Jrsv").hide().appendTo("body");
        })
    });

    $('#Jrsv').rsvform({name:'rsv_name1', phone:'rsv_phone1', ps:'rsv_ps'},{type:1, biz_type:6},{hintType : 2,
        submitSucceedFn:function(){
            $jfz_dg.close();
            $("#Jrsv").hide().appendTo("body");
            return true;
        }
    });

</script>

