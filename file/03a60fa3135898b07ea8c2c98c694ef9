<script type="text/javascript">
//获取字符串长度
function len(s) { 
var l = 0; 
var a = s.split(""); 
for (var i=0;i<a.length;i++) { 
if (a[i].charCodeAt(0)<299) { 
l++; 
} else { 
l+=2; 
} 
} 
return l; 
}
//替换临时网址URL  
$(function(){

	
	$("a").click(function(){
		//alert();
		var t_url	=	$(this).attr("href");
		//alert(t_url);
		q_url = t_url.substr(0,30);
		if(q_url=="http://qbfygov.hk2.chuanhai.cc"){
			h_url	=	t_url.substr(30);
			t_url	=	"http://www.qbfy.gov.cn/"+h_url;
			
		}
		
		zaijie	=	t_url.substr(0,23);
		zaijie2	=	t_url.substr(0,5);
		
		pan360	=	t_url.substr(0,17); 
		
		if(pan360!="http://yunpan.cn/"){
			
			if(zaijie!="http://www.qbfy.gov.cn/" && zaijie2!="index" ){
				h_url	=	t_url.substr(22);
				t_url	=	"http://www.qbfy.gov.cn/"+h_url;
				
			}
		}
		//alert(t_url);
		window.location.href=t_url;
		return false;
	});	
});
</script>
