<script type="text/javascript">
	var fyz_click = 0;
	var fyz_timer = null;
	$('#fyz_focus>ul.f426x2401>li').eq(0).css('display','block');
	$('a#fyz_next').click(function(){
		fyz_click ++;
		if(fyz_click ==5){fyz_click = 0;};
		if(fyz_click <=4){
			$('#fyz_focus>ul.f426x2401>li').eq(fyz_click).stop(true,true).fadeIn().siblings().stop(true,true).fadeOut();
		}else{
			fyz_click = -1;
			$('#fyz_focus>ul.f426x2401>li').eq(fyz_click).stop(true,true).fadeIn().siblings().stop(true,true).fadeOut();
		};
	});
	$('a#fyz_prev').click(function(){
		fyz_click --;
		console.log(fyz_click);
		if(fyz_click == -1){fyz_click = 4;};
		if(fyz_click >=0){
			$('#fyz_focus>ul.f426x2401>li').eq(fyz_click).stop(true,true).fadeIn().siblings().stop(true,true).fadeOut();
		}else{
			fyz_click = 4;
			$('#fyz_focus>ul.f426x2401>li').eq(fyz_click).stop(true,true).fadeIn().siblings().stop(true,true).fadeOut();
		};
	});
	clearInterval(fyz_timer);
	function autoPlay(){
		fyz_timer = setInterval(function(){
			fyz_click ++;
			if(fyz_click ==5){fyz_click = 0;};
			if(fyz_click <=4){
				$('#fyz_focus>ul.f426x2401>li').eq(fyz_click).stop(true,true).fadeIn().siblings().stop(true,true).fadeOut();
			}else{
				fyz_click = -1;
				$('#fyz_focus>ul.f426x2401>li').eq(fyz_click).stop(true,true).fadeIn().siblings().stop(true,true).fadeOut();
			};
		},2500);
	};
	autoPlay();
	$('div#fyz_focus').hover(function(){
		clearInterval(fyz_timer);
	},function(){
		clearInterval(fyz_timer);
		autoPlay();
	});
</script>





