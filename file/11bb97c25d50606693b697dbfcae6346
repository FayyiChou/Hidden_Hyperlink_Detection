<script>
var big_pic_auto_change_timer=null;
$(document).ready(function(){
	//图片自动切换
	var winWidth=$(window).width()/2;
	if(winWidth>=480){
		$("#bigpic").css("left",480-winWidth);
		$("#bigpic").css("width",winWidth*2);
		widthValue=winWidth*2;
		$("#leftbt").css("left",winWidth-530);
		$("#rightbt").css("right",winWidth-530);
		$("#bigpic").find("p").find("img").css("marginLeft",winWidth-1000);
		$("#bigpic").find("p").eq(5).css({"width":winWidth*2,"left":0-winWidth*2,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(4).css({"width":winWidth*2,"left":0-winWidth*2,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(3).css({"width":winWidth*2,"left":0-winWidth*2,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(2).css({"width":winWidth*2,"left":0-winWidth*2,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(1).css({"width":winWidth*2,"left":0-winWidth*2,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(0).css({"width":winWidth*2,"left":0,zIndex:10});
	}else{
		widthValue=960;
		$("#bigpic").find("p").css({"width":960});
		$("#bigpic").find("p").find("img").css({"marginLeft":-520});
		$("#bigpic").find("p").eq(5).css({"width":960,"left":-960,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(4).css({"width":960,"left":-960,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(3).css({"width":960,"left":-960,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(2).css({"width":960,"left":-960,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(1).css({"width":960,"left":-960,zIndex:5,display:"none"});
		$("#bigpic").find("p").eq(0).css({"width":960,"left":0,zIndex:10});
		
	}
	for(var i=0;i<4;i++){
	    $("#bigpic").find("p").find("img").eq(i).click(function(){
			window.open($(this).attr("extralink"), "_blank"); 
		});
	}
/*	$("#bigpic").find("p").find("img").click(function(){
		window.open($(this).attr("extralink"), "_blank"); 
	});*/
	$("#bigpic").css("display","");
	$("#leftbt").mouseenter(function(){
		$("#leftarrow").css("display","block");
	});
	$("#leftbt").mouseleave(function(){
		$("#leftarrow").css("display","none");
	});
	$("#rightbt").mouseenter(function(){
		$("#rightarrow").css("display","block");
	});
	$("#rightbt").mouseleave(function(){
		$("#rightarrow").css("display","none");
	});
	
	//动态加载新闻资讯
	load_news_info();
	
	picautochange();
	//定时滚动论坛信息
	setInterval("scrollup();",8000);
	//定时滚动菜单栏下的大图
	//big_pic_auto_change_timer=setInterval("prepic();",10000);
	
	//初始化共同页面元素
	window.head_init(1);
	window.foot_init(1);
	window.float_layer_init(1);
});
//鼠标移动到微信logo上，显示二维码
function show_2d(){
	$("#wechat").css("display","block");
}

function hide_2d(){
	$("#wechat").css("display","none");
}
function load_news_info(){
	load_news_ajax("01");//
	load_news_ajax("02");//
	load_news_ajax("03");//
}
function load_news_ajax(type){
	$.ajax({
   		type:"get",
   		data:{type:type},
		dataType:"json",
   		url:"ajax_req_data.action",
   		error:function(dataAll){
   			if(dataAll.readyState==4&&dataAll.status==200){
	   			switch(type){
	   				case "01":
		   				load_news_success_type1(dataAll);
		   				break;
	   				case "02":
		   				load_news_success_type2(dataAll);
		   				break;
	   				case "03":
		   				load_news_success_type3(dataAll);
		   				break;
	   			}
   			}else{
	   			//alert('查询购物车失败');
   			}
   		},
   		success:function(jsondata){
   			switch(type){
   				case "01":
	   				load_news_success_type1(jsondata);
	   				break;
   				case "02":
	   				load_news_success_type2(jsondata);
	   				break;
   				case "03":
	   				load_news_success_type3(jsondata);
	   				break;
   			}
   		}
   	});
}
function load_news_success_type1(jsondata){
	if(jsondata.status!=0){
		return;
	}
	var data = jsondata.data;
	var html="";
	for(var i=0;i<data.length;i++){
		html+='<li class="current">';
		html+='<a title="'+data[i].title+'" href="'+data[i].link+' target="_blank" width="576" height="268">';
		html+='<img src="images/cmsupload/'+data[i].imgsrc+'" alt="'+data[i].title+'" style="width:578px;height:268px;"/>';		
		html+="</a></li>";
		var title = data[i].title;
		if(title.length>15){
			title = title.substring(0,15);
		}
	}
	
	$("#box_num2").find("li").each(function(i){
		$(this).html(data[i].title);
	});
	$("#box_list").html(html);
}
function load_news_success_type2(jsondata){
	if(jsondata.status!=0){
		return;
	}
	
	var data = jsondata.data;
	var html="";
	var default_img = data[0].imgsrc;
	var point_index = default_img.lastIndexOf(".");
	var file_type = default_img.substring(point_index);
	if(default_img=="null"||!(file_type==".jpg"||file_type==".png"||file_type==".gif")){
		default_img = "38.jpg";
	}
	var first_img_src="images/cmsupload/"+default_img;
	for(var i=0;i<data.length;i++){
		var title = data[i].title;
		title = title.length>20?title.substring(0,20)+'...':title;
		html+='<li><a title="'+data[i].title+'" class="newslink" href="'+data[i].link+'" target=_blank>'+title+'</a></li>';
	}
	$("#news_center_list").html(html);
	$("#news_center_img").attr("src",first_img_src);
}
function load_news_success_type3(jsondata){
	if(jsondata.status!=0){
		return;
	}
	
	var data = jsondata.data;
	var html="";
	for(var i=0;i<data.length;i++){
		var title = data[i].title;
		title = title.length>22?title.substring(0,22)+'...':title;
		html+='<li><a title="'+data[i].title+'" href="'+data[i].link+'" target="_blank" class="newslink"><img src="images/shouhubao/littleloge_02.png" style="margin-right:6px;"/>';
		html+=title+'</a></li>';
	}
	$("#lttitle").html(html);
	$("#lttitle2").html(html);
	
}
/*****论坛滚动开始****/
var scrollflag=true;
var scrollindex=0;
var scrollobj=0;
function scrollup(){
	if(scrollflag!=true){
		return;
	}
	scrollflag=false;
	if(scrollobj==0){
		//lttitle在当前显示
		var top=parseInt($("#lttitle").css("top"));
		if(top>0){
			var top2=parseInt($("#lttitle2").css("top"));
			$("#lttitle2").animate({"top":(top2-100)+"px"},1000);
			$("#lttitle").animate({"top":(top-100)+"px"},1000);
		}else if(top>=-25&&top<=0){
			$("#lttitle").animate({"top":(top-100)+"px"},1000);
		}else if(top<-25&&top>=-125){
			$("#lttitle2").css("top",(275+top)+"px");
			$("#lttitle").animate({"top":(top-100)+"px"},1000);
			$("#lttitle2").animate({"top":(275+top-100)+"px"},1000);
		}else{
			scrollobj=1;
			var top2 = parseInt($("#lttitle2").css("top"));
			$("#lttitle").animate({"top":(top-100)+"px"},1000);
			$("#lttitle2").animate({"top":(top2-100)+"px"},1000);
		}
	}else{
		//lttitle2在当前显示
		var top=parseInt($("#lttitle2").css("top"));
		if(top>0){
			var top2=parseInt($("#lttitle").css("top"));
			$("#lttitle").animate({"top":(top2-100)+"px"},1000);
			$("#lttitle2").animate({"top":(top-100)+"px"},1000);
		}else if(top>=-25&&top<=0){
			$("#lttitle2").animate({"top":(top-100)+"px"},1000);
		}else if(top<-25&&top>=-125){
			$("#lttitle").css("top",(275+top)+"px");
			$("#lttitle2").animate({"top":(top-100)+"px"},1000);
			$("#lttitle").animate({"top":(275+top-100)+"px"},1000);
		}else{
			scrollobj=0;
			var top2 = parseInt($("#lttitle").css("top"));
			$("#lttitle2").animate({"top":(top-100)+"px"},1000);
			$("#lttitle").animate({"top":(top2-100)+"px"},1000);
		}
	}
	scrollafter();
}
function scrolldown(){
	if(scrollflag!=true){
		return;
	}
	scrollflag=false;
	if(scrollobj==0){
		//lttitle在当前显示
		var top=parseInt($("#lttitle").css("top"));
		if(top<-175){
			var top2 = parseInt($("#lttitle2").css("top"));
			$("#lttitle").animate({"top":(top+100)+"px"},1000);
			$("#lttitle2").animate({"top":(top2+100)+"px"},1000);
		}else if(top<=-100&&top>=-175){
			$("#lttitle").animate({"top":(top+100)+"px"},1000);
		}else if(top>-100&&top<=50){
			$("#lttitle2").css("top",(top-275)+"px");
			$("#lttitle").animate({"top":(top+100)+"px"},1000);
			$("#lttitle2").animate({"top":(-175+top)+"px"},1000);
		}else{
			scrollobj=1;
			var top2 = parseInt($("#lttitle2").css("top"));
			$("#lttitle").animate({"top":(top+100)+"px"},1000);
			$("#lttitle2").animate({"top":(top2+100)+"px"},1000);
		}
	}else{
		//lttitle在当前显示
		var top=parseInt($("#lttitle2").css("top"));
		if(top<-175){
			var top2 = parseInt($("#lttitle").css("top"));
			$("#lttitle2").animate({"top":(top+100)+"px"},1000);
			$("#lttitle").animate({"top":(top2+100)+"px"},1000);
		}else if(top<=-100){
			$("#lttitle2").animate({"top":(top+100)+"px"},1000);
		}else if(top>-100&&top<=50){
			$("#lttitle").css("top",(top-275)+"px");
			$("#lttitle2").animate({"top":(top+100)+"px"},1000);
			$("#lttitle").animate({"top":(-175+top)+"px"},1000);
		}else{
			scrollobj=0;
			var top2 = parseInt($("#lttitle").css("top"));
			$("#lttitle2").animate({"top":(top+100)+"px"},1000);
			$("#lttitle").animate({"top":(top2+100)+"px"},1000);
		}
	}
	scrollafter();
}
function scrollafter(){
	var p = setInterval(function(){
		scrollflag=true;
		clearInterval(p);
	},1100);
}
/*****论坛滚动结束****/
/*****图片自动变换开始****/
function picautochange(){
		var oBox = document.getElementById("box");
	var imgUl = document.getElementById("box_list");
	var numUl = document.getElementById("box_num");
	var aImg = imgUl.getElementsByTagName("li");
	var aNum = numUl.getElementsByTagName("li");
	var numUl2 = document.getElementById("box_num2");
	var aNum2 = numUl2.getElementsByTagName("li");
	var timer = play = null;
	var i = index = 0;	
	//切换按钮
//	alert("aNum:"+aNum.length);
	for (i = 0; i < aNum.length; i++)
	{
		aNum2[i].index = i;
		aNum2[i].onmouseover = function ()
		{
		//	alert(this.index);
			show(this.index);
		}
	}
	
	//鼠标划过关闭定时器
	oBox.onmouseover = function ()
	{
		clearInterval(play)	;
	};
	
	//鼠标离开启动自动播放
	oBox.onmouseout = function ()
	{
		autoPlay();
	};	
	
	//自动播放函数
	function autoPlay ()
	{
		play = setInterval(function () {
			index++;
			index >= aImg.length && (index = 0);
			show(index);
			
		},6000);	
	}
	autoPlay();//应用
	//图片切换, 淡入淡出效果
	function show (a)
	{
		//指定第几个层变色
	
		//alert(a);
		index = a;
		var alpha = 0;
		for (i = 0; i < aNum.length; i++){
			aNum[i].className = "";
		}
		aNum[index].className = "current";
		clearInterval(timer);			
		
		for (i = 0; i < aImg.length; i++)
		{
			aImg[i].style.opacity = 0;
			aImg[i].style.filter = "alpha(opacity=0)";
			aImg[i].className="hide";
		}
		
		aImg[index].className="current";
		timer = setInterval(function () {
			alpha += 2;
			alpha > 100 && (alpha =100);
			aImg[index].style.opacity = alpha / 100;
			aImg[index].style.filter = "alpha(opacity = " + alpha + ")";
			alpha == 100 && clearInterval(timer)
		},50);
		
		/* aImg[index].style.opacity = 1;
		aImg[index].style.filter = "alpha(opacity = " + 100 + ")"; */
		//alert(aImg[index].getElementsByTagName("a")[0].href);
	}
}
/*****图片自动变换结束****/
</script>
<script>

function overvideodiv(t){
	$(t).css("background","#7AAA15");
}
function outvideodiv(t){
	var id = $(t).attr("id");
	if(id.indexOf(videobgnumber)==-1){
		$(t).css("background","");
	}
}
var videobgnumber=0;
function showvideo(number){
	$("#movie_player"+number).css("display","block");
	$("#shiping"+number).css("background","#7AAA15");
	videobgnumber=number;
	for(var n=0;n<3;n++){
		if(n!=number){
			$("#movie_player"+n).css("display","none");
			$("#shiping"+n).css("background","");
		}
	}
}
/************菜单下大图片左右切换开始*****************/
var widthValue=960;
var isplay = true;
var bigpicindex=0;
var all=8;
var changeTime=500;
var activeNo=0;
function prepic(){
	if(isplay==false){
		return;
	}
	isplay=false;
	var nextIndex=(bigpicindex+1)%8;
	$("#a"+nextIndex).css("left",widthValue+"px");
	$("#a"+bigpicindex).css({"zIndex":5});
	$("#a"+bigpicindex).animate({"left":"-"+widthValue+"px"},changeTime,function(){
		$(this).css("display","none");
	});
	activeNo=nextIndex;
	$("#a"+nextIndex).css({"zIndex":10,display:""});
	$("#a"+nextIndex).animate({"left":"0"},changeTime,function(){
		$(".dot").find("a").removeClass("active");
		
		$(".dot").eq(activeNo).find("a").addClass("active");
	});
	bigpicindex=nextIndex;
	after();
}
function afterpic(){
	if(isplay==false){
		return;
	}
	isplay=false;
	var preIndex=0;
	if(bigpicindex-1<0){
		preIndex=8-1;
	}else{
		preIndex=bigpicindex-1;
	}
	nextIndex=preIndex;
	$("#a"+preIndex).css("left","-"+widthValue+"px");
	$("#a"+bigpicindex).css("zIndex",5);
	$("#a"+bigpicindex).animate({"left":widthValue+"px"},changeTime,function(){
		$(this).css("display","none");
	});
	activeNo=nextIndex;
	$("#a"+preIndex).css({"zIndex":10,"display":""});
	$("#a"+preIndex).animate({"left":"0"},changeTime,function(){
		$(".dot").find("a").removeClass("active");
		
		$(".dot").eq(activeNo).find("a").addClass("active");
	});
	bigpicindex=preIndex;
	after();
}
function goactive(num){
	if(isplay==false||num==activeNo){
		return;
	}
	isplay=false;
	var nextIndex=num;
	$("#a"+nextIndex).css("left",widthValue+"px");
	$("#a"+bigpicindex).css({"zIndex":8});
	$("#a"+bigpicindex).animate({"left":"-"+widthValue+"px"},changeTime,function(){
		$(this).css("display","none");
	});
	activeNo=nextIndex;
	$("#a"+nextIndex).css({"zIndex":10,display:""});
	$("#a"+nextIndex).animate({"left":"0"},changeTime,function(){
		$(".dot").find("a").removeClass("active");
		$(".dot").eq(activeNo).find("a").addClass("active");
	});
	bigpicindex=nextIndex;
	after();
	return false;
}
function after(){
	var t = setInterval(
		function(){
			isplay=true;
			clearInterval(t);
			clearInterval(big_pic_auto_change_timer);
			big_pic_auto_change_timer=setInterval("prepic();",10000);
		},changeTime+100);
}
function auto_change(){
	var nextIndex=(bigpicindex+1)%all;
	if(nextIndex==1){
		clearInterval(big_pic_auto_change_timer);
		setTimeout(function(){
			big_pic_auto_change_timer=setInterval("prepic();",5000);
		},5000);
	}
}
/************菜单下大图片左右切换结束*****************/
</script>

</HEAD>
