      function openCorrectionWin(){
        var referStr=document.location; // 取得当前页面的URL 
        var title=document.title;
        window.open("http://member.dongao.com/usercenter/editFeedback.html?feedbackType=5&title="+encodeURI(title) + "&r="+encodeURI(referStr), "favOpenWin", 
                    "height=600,width=800,status=yes,toolbar=no,menubar=no,location=no");
      }
      function openRecommandWin(){
        var title=document.title;
        var referStr=document.location; // 取得当前页面的URL
        window.open("http://member.dongao.com/usercenter/editFeedback.html?feedbackType=6&title="+encodeURI(title)+"&r="+encodeURI(referStr), "favOpenWin", 
                    "height=600,width=800,status=yes,toolbar=no,menubar=no,location=no");
      }
      
      function collectArticle(){
        var referStr=document.location; // 取得当前页面的URL
        var atitle=document.title;
        window.open("http://member.dongao.com/usercenter/favorite.html?url="+referStr+"&res=page&subject="+encodeURI(atitle)+"&favoriteType=4","favOpenWin", 
                    "height=600,width=800,status=yes,toolbar=no,menubar=no,location=no");
      }
    </script>
    <script>
      var status0='';
      var curfontsize=14;
      function fontZoomA(){
        if(curfontsize>14){
          document.getElementById('fontzoom').style.fontSize=(--curfontsize)+'px';
        }
      }
      function fontZoomB(){
        if(curfontsize<64){
          document.getElementById('fontzoom').style.fontSize=(++curfontsize)+'px';
        }
      }
