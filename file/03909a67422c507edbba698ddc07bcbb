<script type="text/javascript">
  //顶部导航
  $('.shortcut_right li').hover(function(){
    $(this).addClass('active').siblings('li').removeClass('active');
  },function(){
    $(this).removeClass('active');
  });
  //详情页导航
  $('.nav_left').hover(function(){
    var _this=$(this).find('.dt_bj')
    _this.addClass('active');
    _this.next('dl').show();
  },function(){
    var _this=$(this).find('.dt_bj');
    _this.removeClass('active');
    _this.next('dl').hide();
  });
</script>
<script type="text/javascript">
  $(function(){
	 /*登录部分*/  
	var url="http://passport.dongao.com/api/showUserStatus?time="+Math.random();
	$.getScript(url,function(){
	    if(typeof(_user_id)=='undefined' || _user_id == '' ){

		} else {
		    $('#noLogBar').css("display","none");
		    $('#logedBar').css("display","inline");
		    $('#loginName').html(_user_name);
		    $('#cartNum').html(_cart_num);
		    $('#shopping-amount').html(_cart_num);
		    $('#msgNum').html(_msg_num);
  		}
  	});

  	var currentUrl = "http://my.dongao.com/";
 	$('#loginId').attr("href","http://passport.dongao.com/login?forward="+currentUrl);
	$('#registId').attr("href","http://passport.dongao.com/register?forward="+currentUrl);    
  })
//在线客服
function op(){
    window.open('http://member.dongao.com/webchat/login.htm','88','top=0,left=200,width=850,height=500,scrollbars=yes,resizable=no,status=yes,z-look=yes,alwaysRaised=yes,location=no,depended=no,center:yes');
  }
  $(".kf_close").click(function(){
    $(".fu_kf").hide();
  });
</script>
