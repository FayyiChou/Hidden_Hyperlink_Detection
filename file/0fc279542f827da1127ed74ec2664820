<script>
$(document).ready(function(e) {
	
	//导航
	/*$('.navlist li').each(function(){
		var liwidth = $(this).eq(0).width();
		$(this).find(".nav-sub li").css({width:liwidth+'px'});
	});*/
    $(".navlist li").hover(function(){
		if(!$(this).find(".nav-sub").is(":animated") && $(this).find(".nav-sub li").length>0){
			$(this).find(".nav-sub").slideDown(200);
		}else{
			$(this).find(".nav-sub").stop(false,true);	
		}
		$(".navlist li").eq(0).removeClass("hover");
		$(this).addClass("hover");
		
	},function(){
		var _this = $(this);
		$(this).find(".nav-sub").slideUp(200,function(){
			_this.removeClass("hover");
		});	
		if(_this.find(".nav-sub li").length<1){
			_this.removeClass("hover");
		}
	});
	
	$(".navlist").mouseleave(function(){
		$(".navlist li").eq(0).addClass("hover");
	});
	
	//海报
	var slide_items = $(".slide-item li").length;
	var slide_tmp_count = 0;
	var slide_append_html = '';
	while(slide_items > slide_tmp_count){
		slide_tmp_count++;	
		slide_append_html += '<li></li>';
	}
	if(slide_append_html!=''){
		slide_append_html = '<div class="hd"><ul>'+slide_append_html+'</ul></div>';
		$('.slide-hd').append(slide_append_html);
	}
	jQuery(".slide-hd").slide({mainCell:".bd ul",autoPlay:true,delayTime:1000,interTime:5000});
	
	
	var logotips = null;

	$('.tdcode').hover(function(){

			logotips = layer.tips('<img src="/renestyle/style/images/b2dcode.png" width="280" height="280" >', this, {

			style: ['background-color:#FFF; color:#000;padding:10px;', '#FFF'],

			guide: 2

		});

	},function(){layer.close(logotips)});
	
	$('.f-sev li').hover(function(){
		$(this).removeClass('hover').addClass('hover');
	},function(){
		$(this).removeClass('hover');
	});
	
});
</script>
