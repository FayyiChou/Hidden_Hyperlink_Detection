    function mobilelogin(){
        var mobile = jQuery("#pass-login .mobile-number").val();
        if(!mobile.match(/1[3-8]+\d{9}/)) {
            jQuery(".wrong-mobile").show();
            jQuery(".wrong-dtm").hide();
            jQuery(".mobile-number").parent().addClass("ipt-error");
            jQuery(".mobile-number").prev().addClass("icon-phone-error");
            return;
        }
        var code = jQuery("#pass-login .dtm-number").val();



        if(code==""){
            jQuery(".wrong-mobile").hide();
            jQuery(".wrong-dtm").text("请输入短信验证码").show();
            jQuery(".dtm-number").parent().addClass("ipt-error");
            jQuery(".dtm-number").prev().addClass("icon-open-error");
            return ;
        }else{
            //check 手机短信验证是否正确
            var data ="mobile="+mobile+"&code="+code;
            var login_api = "http://api.redocn.com/Login/mobile";

            jQuery.ajax({
                type:'post',
                url : login_api,
                data:data,
                dataType : 'jsonp',
                jsonp:"callback",
                jsonpCallback:"success_jsonpCallback",
                success  : function(res) {
                    if(res.status== '1'){
                        //短信验证码通过
                        jQuery(".wrong-mobile").hide();
                        jQuery(".wrong-dtm").hide();
                        jQuery(".dtm-number").parent().removeClass("ipt-error");
                        jQuery(".dtm-number").prev().removeClass("icon-open-error");
                        //刷新父窗口
                        addAllHistory();
                        window.location.href="http://upload3.redocn.com/sucai/index";
                    }
                    else if(res.status=='3'){
                        jQuery(".wrong-mobile").hide();
                        jQuery(".wrong-dtm").text("请输入正确的短信验证码").show();
                        jQuery(".dtm-number").parent().addClass("ipt-error");
                        jQuery(".dtm-number").prev().addClass("icon-open-error");
                    }else{
                        alert('登录失败');
                    }
                },
                error : function() {
                    alert('fail');
                }
            });
        }
    }


    function userlogin(){
        var username = jQuery("#member-login .user-name").val();
        var password = jQuery("#member-login .pwd-number").val();
        if(username == "" || password ==""){
            if(username == ""){
                jQuery("#member-login .wrong-username").show();
                jQuery("#member-login .wrong-pwd").hide();
                jQuery(".user-name").parent().addClass("ipt-error");
                jQuery(".user-name").prev().addClass("icon-user-error");
                return false;
            }
            else{
                jQuery("#member-login .wrong-username").hide();
                jQuery("#member-login .wrong-pwd").show();
                jQuery(".pwd-number").parent().addClass("ipt-error");
                jQuery(".pwd-number").prev().addClass("icon-clock-error");
                return false;
            }
        }else{
            var verifycode = $("#member-login [name=verifycode]").val();
            if(verifycode==""){
                $("#member-login [name=verifycode]").parent().addClass("ipt-error");
                jQuery(".wrong-username").text("请输入验证码").show()
                jQuery(".wrong-pwd").hide();
                return false;
            }


//            var data ="username="+username+"&password="+password+"&verifycode="+verifycode;
            var data = {username:username,password:password,verifycode:verifycode};
            jQuery.post('/oauth/postlogin',data,function(res){
                if(res.status=="faild"){
                    if(res.code==2){
                        jQuery(".wrong-username").text("请输入正确用户名和密码").show()
                        jQuery(".wrong-pwd").hide();

                    }
                    if(res.code==3){
                        jQuery(".wrong-username").text("该账号禁止登陆有问题请联系管理员").show()
                        jQuery(".wrong-pwd").hide();
                    }
                    if(res.code==4){
                        jQuery(".wrong-username").text("请先激活您的账号").show()
                        jQuery(".wrong-pwd").hide();
                    }
                    if(res.code==5){
                        jQuery(".wrong-username").text("请输入正确的验证码").show()
                        jQuery(".wrong-pwd").hide();
                    }
                    if(res.code==6){
                        jQuery(".wrong-username").text("密码错误已达6次请在5小时以后再次登录红动").show()
                        jQuery(".wrong-pwd").hide();
                    }
                    if(res.code==11){
                        jQuery(".wrong-username").text("管理员请使用qq登录").show()
                        jQuery(".wrong-pwd").hide();
                    }
                }else{
                    addAllHistory();
                    window.location.href="http://upload3.redocn.com/sucai/index";
                }
            },'json');
        }
    }

    function addAllHistory(){
        $.ajax({
            url:"http://lajax.redocn.com/ajaxHistory/addallhistory",
            type:'get',
            dataType:'jsonp',
            jsonp:'jsoncallback',
            success:function(htmlhistory){

            }
        });
    }

