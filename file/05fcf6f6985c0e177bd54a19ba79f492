        function Focus(obj,newClass){
            jQuery(obj).focus(function(){
                jQuery(this).parent().addClass("ipt-focus");
                jQuery(this).prev().addClass(newClass);
            });
        }
        Focus(".mobile-number","icon-phone-focus");
        Focus(".dtm-number","icon-open-focus");
        Focus(".user-name","icon-user-focus");
        Focus(".pwd-number","icon-clock-focus");

        jQuery(".mobile-number").keyup(function(){
            var val = jQuery(this).val();
            if(val.length == 11){
                if(val.match(/1[3-8]+\d{9}/)){
                    jQuery(".wrong-mobile").hide();
                    jQuery(".wrong-dtm").hide();
                    jQuery(this).parent().removeClass("ipt-error");
                    jQuery(this).prev().removeClass("icon-phone-error");
                    jQuery(".get-dtm").addClass("get-dtm-hover");
                }
                else{
                    jQuery(".wrong-mobile").show();
                    jQuery(".wrong-dtm").hide();
                    jQuery(this).parent().addClass("ipt-error");
                    jQuery(this).prev().addClass("icon-phone-error");
                    jQuery(".get-dtm").removeClass("get-dtm-hover");
                }
            }
            else{
                if(val.length == 0){
                    jQuery(this).parent().removeClass("ipt-error");
                    jQuery(this).prev().removeClass("icon-phone-error");
                    jQuery(".wrong-mobile").hide();
                }
                if(val.length > 11){
                    jQuery(this).parent().addClass("ipt-error");
                    jQuery(this).prev().addClass("icon-phone-error");
                    jQuery(".wrong-mobile").show();
                }
                jQuery(".get-dtm").removeClass("get-dtm-hover");
            }
        });
        jQuery(".mobile-number").blur(function(){
            var val = jQuery(this).val();
            if(val.length != 11){
                if(val.length == 0){
                    jQuery(".wrong-mobile").hide();
                    jQuery(".wrong-dtm").hide();
                    jQuery(this).parent().removeClass("ipt-error").removeClass("ipt-focus");
                    jQuery(this).prev().removeClass("icon-phone-error").removeClass("icon-phone-focus");
