        jQuery(".get-dtm").click(function(){
            var val = jQuery.trim(jQuery(".mobile-number").val());
            if(!val.match(/1[3-8]+\d{9}/)){
                jQuery(".wrong-mobile").show();
                jQuery(".wrong-dtm").hide();
                jQuery(".mobile-number").parent().addClass("ipt-error");
                jQuery(".mobile-number").prev().addClass("icon-phone-error");
                return;
            }
            if(jQuery(".get-dtm").hasClass("count")) return;

            jQuery.post(vcodeurl,function(req){
                var vcode = req.code;
                if(val.match(/1[3-8]+\d{9}/)){
                    jQuery(this).removeClass("get-dtm-hover");
                    var total =	60;
                    var h = (total--) + '秒后重新发送';
                    jQuery('.get-dtm').html(h);
                    jQuery(".get-dtm").addClass('count');
                    t =	setInterval(function(){
                        var h = (total--) + '秒后重新发送';
                        jQuery('.get-dtm').html(h);
                        jQuery(".get-dtm").addClass('count');
                        if( total < 0 ){
                            var verifycode = jQuery(".mobile-number").val();
                            if (verifycode == ""){
                                //如果过了120s 用户还没有任何输入就换一个短信接口
                                msgurl = "http://user.redocn.com/ajax/sendPhonepmsold";
                            }
                            clearInterval(t);
                            jQuery(".get-dtm").removeClass('count');
                            jQuery('.get-dtm').html('获取手机动态码');
                        }
                    },1000);
                }
                //发送短信验证码
                var data = {mobile:val,scene:'userlogin',vcode:vcode};
                jQuery.post(msgurl,data,function(res){
                    res = jQuery.trim(res);
                    if(res=="ok"){

                    }else if(res=="maxpost"){
                        alert("免费获取手机验证码今日已经超过5次了，请明天再尝试，或者联系客服");
                    }else if(res=="verifyerror"){
                        clearInterval(t);
                        jQuery(".get-dtm").removeClass('count');
                        jQuery('.get-dtm').html('获取手机动态码');
                        alert("验证码输入错误");
                    }
                    else {
                        alert("获取手机验证码失败");
                    }
                });
            },'json');
        });

