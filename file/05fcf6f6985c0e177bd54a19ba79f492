function flash_code(o){
    var src = $(o).attr('data-src')+'&r='+Math.random();
    $(o).attr('src',src);
}

//兼容placeholder
function hideholder(obj){
	$(document).on("focus",obj,function(){
		$(this).parent().find(".show_holder").addClass("hideholder");
		$(this).parent().find(".clear-icon").show();
        $(this).parents(".dtm").find(".placeholder_tip").hide();
	});
	$(document).on("blur",obj,function(){
		var value = $(this).val();
		if(value == ''){
			$(this).parent().find(".show_holder").removeClass("hideholder");
            $(this).parents(".dtm").find(".placeholder_tip").show();
		}
		$(this).parent().find(".clear-icon").hide();	
	});
}
hideholder("#user-name");
hideholder("#pwd-number");
hideholder("#var-number");
hideholder("#mobile-number");
hideholder("#dtm-number");

