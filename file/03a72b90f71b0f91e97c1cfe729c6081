			$(this).addClass("nav-A-light");
			}else{
				$(this).text("夜间模式");
				$(this).removeClass("nav-A-light");
				$(this).addClass("nav-A-dark");
				}
		});
	
	});
*/


}else{
// JavaScript Document

//左侧专题推荐
jQuery("#slideBox").slide({
    mainCell: ".bd .c-12",
    titCell: ".hd ul",
    effect: "leftLoop",
    autoPlay: true,
    autoPage: true
});

//左侧推荐阅读
jQuery("#tabBox").slide({
    mainCell: ".bd ",
    titCell: ".hd li",
    effect: "fade"
});

//原广告显示数秒后隐藏，展示小图
$(document).ready(function() {
    $("#top-banner").slideDown("slow");
    $("#rightClose").click(function() {
        $("#top").hide();
    })
});
function displayimg() {
    $("#top-banner").slideUp(1000,
    function() {
        $("#top").slideDown(1000);
    })
}
setTimeout("displayimg()", 5000);

//
var divTop = $(".art-ad04").offset().top;
$(window).on('scroll',
function() {
    if ($(window).scrollTop() > divTop) {
        $('.art-ad04').addClass('movehead');
    } else {
        $('.art-ad04').removeClass('movehead');
    }

    if ($(window).scrollTop() > 250) {
        $('#go-top').css("display", "block");
    } else {
        $('#go-top').css("display", "none");
    }
});

$(function() {
	//字体缩放
    $(".font-ctrl").click(function() {
        var thisEle = $(".Con_FS *").css("font-size");
        var textFontSize = parseFloat(thisEle, 10);
        var unit = thisEle.slice( - 2); //获取单位 
        var cName = $(this).attr("id");
        if (cName == "font-big") {
            if (textFontSize <= 18) {
                textFontSize += 2;
            }
        } else if (cName == "font-small") {
            if (textFontSize >= 12) {
                textFontSize -= 2;
            }
        }
        $(".Con_FS *").css("font-size", textFontSize + unit);
    });

	//顶部广告cookie设置;
    if ($.cookie("test_cookie")) {
        $(".top").css("display", $.cookie("test_cookie"));
    }

    // 获取 cookie 
    $("#rightClose").click(function() {
        $(".top").css("display", "none");
        // 设置cookie，到期时间 
        $(function() {
            var date = new Date();
            date.setTime(date.getTime() + (1 * 24 * 60 * 60 * 1000));
            $.cookie("test_cookie", 'none', {
                path: '/',
                expires: date
            });
            return false;
        });
    });

	//右侧浮动控制
    $("#weixin").hover(function() {
        $(this).next().fadeIn("slow");
    },
    function() {
        $(this).next().fadeOut("slow");
    });
    $("#shouji").hover(function() {
        $(this).next().fadeIn("slow");
    },
    function() {
        $(this).next().fadeOut("slow");
    });
    $("#ios").hover(function() {
        $(this).next().fadeIn("slow");
    },
    function() {
        $(this).next().fadeOut("slow");
    });
    $("#android").hover(function() {
        $(this).next().fadeIn("slow");
    },
    function() {
        $(this).next().fadeOut("slow");
    });
    $("#dianhua").hover(function() {
        $(this).next().fadeIn("slow");
    },
    function() {
        $(this).next().fadeOut("slow");
    });
});

//返回顶部设置;
$("#go-top").click(function() {
    $('body,html').animate({
        scrollTop: 0
    },
    1000);
    return false;
});

