<script language="javascript">

 function start(){
  setTimeout('startFlickFairy(document.getElementById("fairy"));', 100);
  setTimeout('startFlick(document.getElementById("gift_img"));', 300);
  setTimeout("document.getElementById('fairy').style.visibility = 'visible'", 350);
 }

 // 0 - Ìåíþøêà
 // 1 - ÍÃ

 imagesLoaded = new Array(0,0);

 function onLoadImg(n) {

  imagesLoaded[n]++;

  if (imagesLoaded[0] == 4) {
   fade('menu_1', 'in', 1.2);
   fade('menu_2', 'in', 1.4);
   fade('menu_3', 'in', 1.6);
   fade('menu_4', 'in', 1.8);
   imagesLoaded[0]++;
  }

 }

 var elementToFlick;

 function startFlick(img) {
  img.onMaxTime = 5;      // ìàêñ. âðåìÿ íåìåðöàþùåé íàäïèñè, ñåê.
  img.flMaxTime = 1;      // ìàêñ. âðåìÿ ìåðöàíèÿ íàäïèñè, ñåê.
  img.flSwitchTime = 100; // ìàêñ. âðåìÿ ìåæäó ìåðöàíèÿìè, ìñåê.
  img.isOn = true;
  img.inFlick = false;
  elementToFlick = img;
  flickElement();
 }

 function startFlickFairy(img) {
  img.onMaxTime = 3;      // ìàêñ. âðåìÿ íåìåðöàþùåé íàäïèñè, ñåê.
  img.flMaxTime = 1;      // ìàêñ. âðåìÿ ìåðöàíèÿ íàäïèñè, ñåê.
  img.flSwitchTime = 50; // ìàêñ. âðåìÿ ìåæäó ìåðöàíèÿìè, ìñåê.
  img.isOn = true;
  img.inFlick = false;
  elementToFlick = img;
  flickElement();
 }

 function setWidth(){
  if(document.body.clientWidth < 1000){
  }else{
  }
 }

</script>

