    function collapse(objImg, objId) {
        var obj = document.getElementById(objId);
        if (objImg && obj) {
            if (objImg.src.toString().indexOf('icon4') > -1) {
                objImg.src = objImg.src.toString().replace('icon4', 'icon3');
                objImg.title = '显示';
                obj.style.display = 'none';
            }
            else if (objImg.src.toString().indexOf('icon3') > -1) {
                objImg.src = objImg.src.toString().replace('icon3', 'icon4');
                objImg.title = '隐藏';
                obj.style.display = '';
            }
        }
    }

