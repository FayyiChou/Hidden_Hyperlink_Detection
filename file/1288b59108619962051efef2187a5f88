<div id="baiduallis" style="width: 400px; height: 100px; "><iframe style="position: absolute; left: 0px; " width="400" height="100" frameborder="0" scrolling="no" marginwidth="0" marginheight="0" src="http://11.mydrivers.com/duba/baiduAfxMobile.html#c_id=u2925217"></iframe></div>
<script language="javascript">
function $allis(){
	var querystring = window.location.search.replace('?', '');
    var values = querystring.split('_');
	if(values.length>4){
		var element_alli=document.getElementById("baiduallis");
		if(element_alli){
			var ifrmae_alli = document.createElement('iframe');
			var left_top=(parseInt(values[3])-parseInt(values[1]))/2;
			element_alli.style.cssText="width:"+values[3]+"px; height:"+values[4]+"px;";
			ifrmae_alli.style.cssText="position:absolute; left:-"+left_top+"px;";
			ifrmae_alli.width=values[3];
			ifrmae_alli.height=values[4];
			ifrmae_alli.allowTransparency="true";
			ifrmae_alli.frameBorder="0";
			ifrmae_alli.scrolling="no";
			ifrmae_alli.marginWidth= "0";
			ifrmae_alli.marginHeight="0";
			ifrmae_alli.src="http://11.mydrivers.com/duba/baiduAfxMobile.html#c_id="+values[0];
			element_alli.appendChild(ifrmae_alli);
		}
	}
}
$allis();
</script>

