	<script type="text/javascript">
 	    $(document).ready(function() {
 	        function addMega() { $(this).addClass("hovering"); $($(this).find("h2 a")).addClass("hover"); } 
 	        function removeMega() { $(this).removeClass("hovering"); $($(this).find("h2 a")).removeClass("hover"); }
 	        var megaConfig = { interval: 100, sensitivity: 4, over: addMega, timeout: 100, out: removeMega };
 	        $("li.mega").hoverIntent(megaConfig)

 	        $('#featureTabs div').hover(
                function () {
                    if ($('#flashAnimationCloseButton').is(':visible')) return false;
                    $('#featureTabs div').removeClass('active');
                    $(this).addClass('active');
                    tabsId = $(this).attr('id').replace('featureTab','');
                    $('#featureWrapper a, #featureWrapper1').hide();
                    $('#featureWrapper' + tabsId).show();
                },
                function () {
                }
            );
 	        $('#playAnimationMap').click(function () {
 	            $('#featureWrapper1').hide();
 	            $('#contactWrapper').fadeOut();
 	            $('#flashAnimation,#flashAnimationCloseButton').show();
 	            return false;
 	        });
 	        $('#flashAnimationCloseButton').click(function () {
 	            $('#featureWrapper1').show();
 	            $('#contactWrapper').fadeIn();
 	            $('#flashAnimation,#flashAnimationCloseButton').hide();
 	        });

 	        $('.training .tabs li').hover(
                function () {
                    $('.training .tabs li').removeClass('active');
                    tabName = $(this).attr('class');
                    $(this).addClass('active');
                    $('.homepromo_list.courselist, .homepromo_list.friendlinklist').hide();
                    $('.' + tabName + 'list').show();
                },
                function () {
                }
                );
 	        $('.usual_services .tabs li').hover(
                function () {
                    $('.usual_services .tabs li').removeClass('active');
                    tabName = $(this).attr('class');
                    $(this).addClass('active');
                    $('.homepromo_list.service1list, .homepromo_list.service2list').hide();
                    $('.' + tabName + 'list').show();
                },
                function () {
                }
                );
 	    });
	</script>
    <script type="text/javascript">
        var analyticsEnabled = true;
        function animationTrack(sMarker) {
            if (sMarker == "finished") {
                // Hide animation
                hideAnimation();
                flashLoaded = false;
            }
        }
        function hideCTAbox() {
            
        }
        function showCTAbox() {
            
        }
        function hideAnimation() {
            $('#featureWrapper1').show();
            $('#contactWrapper').fadeIn();
            $('#flashAnimation,#flashAnimationCloseButton').hide();
            flashLoaded = false;
        }

    </script>
    <!-- script end -->
