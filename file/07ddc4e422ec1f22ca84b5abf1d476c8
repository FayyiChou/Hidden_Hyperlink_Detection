 	var iscloseApp=getCookie("iscloseApp");
 	if(iscloseApp == 1){
	    $(".download-app").hide();
 	}else{
	    $(".download-app").show();
 	}
 })

 	function getCookie(cname)
	{
		var name = cname + "=";
		var ca = document.cookie.split(';');

		for(var i=0; i < ca.length; i++){
		  var c = ca[i].trim();
		  if (c.indexOf(name)==0) return c.substring(name.length,c.length);
		}
		return "";
	}

	// 关闭下载App提示层
	$(".download-app .cancel-img").on('click',function(){
		document.cookie="iscloseApp=1";
	    $(".download-app").hide();
	})

});</script>
