		if (element.addEventListener) {
			element.addEventListener(eventType, functionName, false);
			return true;
		} else if (element.attachEvent) {
			var r = element.attachEvent('on' + eventType, functionName);
			return r;
		} else {
			element['on' + eventType] = functionName;
		}
	}

