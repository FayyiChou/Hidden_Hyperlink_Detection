<script language="javascript">
document.FORM.Submit.disabled = false;
var charset = 'gbk';
var stylepath = 'wind';
function checkpost(obj){
	var action =  "reply";
	if(strlen(obj.atc_content.value)<4){
		showDialog("error","多说两句，或许会有更多人关注你。",2);
		obj.atc_content.focus();
		return false;
	} else if(strlen(obj.atc_content.value)>150000){
		showDialog("error","你知道的太多了！",2);
		obj.atc_content.focus();
		return false;
	}
	
	//check表情
	if (isOnlyFace(obj.atc_content.value)) {
		showDialog("error","除了表情，再说两句吧，亲。。",2);
		obj.atc_content.focus();
		return false;
	}
	
	document.FORM.Submit.disabled = true;

	if (is_ie && newAtt.aid < 2) {
		document.FORM.encoding = "application/x-www-form-urlencoded";
	}	
	return true;
}

function isOnlyFace(str) {
	var str = str.replace(/\s*/g, '');
	var reg = /\[s:\d+\]/i;
	var arr = str.split(reg);
	var temp = '';
	for(var i = 0; i<=arr.length-1; i++) {
		 temp += arr[i];
	}
	if (1 > strlen(temp)) {
		return true;
	}
	return false;
}

function addsmile(NewCode,textarea_id){
	if(getObj(textarea_id).value=="回复时多说几句，会让您的发言得到更多关注"){
		getObj(textarea_id).value="";
	}
    getObj(textarea_id).value += ' [s:'+NewCode+'] ';
}
window.onbeforeunload = function() {
	saveData('msg', document.FORM.atc_content.value);
}
if (FetchCookie('ds') == 1) {
	try{dataStorage.load();}catch(e){}
}
</script>
