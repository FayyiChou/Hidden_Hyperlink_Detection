 <script type="text/javascript" language="javascript">
    var time = 500;
    var h = 0;
    function addCount()
    {
        if(time>0)
        {
            time--;
            h = h+5;
        }
        else
        {
            return;
        }
        if(h>500)  //高度
        {
            return;
        }
        document.getElementById("ads1").style.display = "";
        document.getElementById("ads1").style.height = h+"px";
        setTimeout("addCount()",30); 
    }
    
   function showAds()
    {

        addCount();
        setTimeout("noneAds()",10000); //停留时间自己适当调整
    }
    </script>
    
    <script type="text/javascript" language="javascript">
    var T = 500;
    var N = 500; //高度
    function noneAds()
    {
        if(T>0)
        {
            T--;
            N = N-5;
        }
        else
        {
            return;
        }
        if(N<0)
        {
            document.getElementById("ads1").style.display = "none";
            return;
        }
        
        document.getElementById("ads1").style.height = N+"px";
        setTimeout("noneAds()",30); 
    }
    </script>
