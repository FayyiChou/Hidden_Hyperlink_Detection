<script language="javascript">
function setTab(name,cursel,n){
	for(i=1;i<=n;i++){
		var menu=$('#'+name+i);
		var con=$("#con_"+name+"_"+i);
		menu[0].className=i==cursel?"hover":"unhover";
		con[0].style.display=i==cursel?"block":"none";
	}
}
</script>
