<script type="text/javascript">
    var jiayi9 = window.jiayi9 || {};
    jiayi9.www = jiayi9.www || {};
    /************************
    * jiayi 浮窗
    *************************/
    jiayi9.www.jiayiFloatad = {
        cfg: {
            floatad_winpop_id: "#floatad-winpop",
            jiayiPopUp_tab_li: "#jiayiPopUp_tab ul li",
            jiayiPopUp_content_li: "#jiayiPopUp_content ul li",
            datalazy_placeholder: "images/index/color3.gif",
            datalazy_attr: "original1",
            currentClass: "select",
            currentIndex: 0,
            isinited: false
        },
        tips_pop: function (str) {
            var self = jiayi9.www.jiayiFloatad;
            var MsgPop = $(self.cfg.floatad_winpop_id);
            var popH = parseInt(MsgPop.height());
            if (popH == 0 && str == "up") {
                MsgPop.show();
                self.cfg.show = setInterval(function () { self.changeH(str); }, 2);
            }
            else {
                self.cfg.hide = setInterval(function () { self.changeH(str); }, 2);
            }
        },
        changeH: function (str) {
            var self = jiayi9.www.jiayiFloatad;
            var MsgPop = $(self.cfg.floatad_winpop_id);
            var popH = parseInt(MsgPop.height());
            if (str == "up") {//向上
                if (popH < 210) {
                    MsgPop.height((popH + 3) + "px");
                }
                else {
                    clearInterval(self.cfg.show);
                }
            }
            else {//向下
                if (self.cfg.show) {
                    clearInterval(self.cfg.show);
                }
                if (popH>= 3) {
                    MsgPop.height((popH - 3) + "px");
                }
                else {
                    clearInterval(self.cfg.hide);
                    MsgPop.hide();
                }
            }
        },
        _showTab: function () {
            var self = jiayi9.www.jiayiFloatad;
            $(self.cfg.jiayiPopUp_tab_li).removeClass(self.cfg.currentClass);
            $(self.cfg.jiayiPopUp_tab_li).eq(self.cfg.currentIndex).addClass(self.cfg.currentClass);
            $(self.cfg.jiayiPopUp_content_li).hide();
            $(self.cfg.jiayiPopUp_content_li).eq(self.cfg.currentIndex).fadeIn();
            $("img[" + self.cfg.datalazy_attr + "]", $(self.cfg.jiayiPopUp_content_li).eq(self.cfg.currentIndex)).trigger("appear");
        },
        _autoScroll: function () {
            var self = jiayi9.www.jiayiFloatad;
            self._showTab();

            self.cfg.currentIndex++;
            self.cfg.currentIndex = self.cfg.currentIndex>= $(self.cfg.jiayiPopUp_content_li).length ? 0 : self.cfg.currentIndex;
            self.cfg.jiayiPopUpTimeoutID = setTimeout(self._autoScroll, 3000);
        },
        _stopScrolll: function () {
            clearTimeout(jiayi9.www.jiayiFloatad.cfg.jiayiPopUpTimeoutID);
        },
        init: function () {
            var self = jiayi9.www.jiayiFloatad;
            if (self.cfg.isinited == true) {
                return;
            }
            if ($(self.cfg.floatad_winpop_id).length> 0) {
                self.cfg.isinited = true;
                var tabid = 0;

                $("img[" + self.cfg.datalazy_attr + "]", self.cfg.floatad_winpop).lazyload({ placeholder: self.cfg.datalazy_placeholder, data_attribute: self.cfg.datalazy_attr, skip_invisible: true });
                for (var i = 0; i < $(self.cfg.jiayiPopUp_tab_li).length; i++) {
                    $(self.cfg.jiayiPopUp_tab_li).eq(i).attr("id", "p" + i);
                };
                $(self.cfg.jiayiPopUp_content_li).eq(0).show();
                //self._autoScroll();
                $(self.cfg.jiayiPopUp_tab_li).hover(function () {
                    //self._stopScrolll();

                    tabid = $(this).attr("id");
                    self.cfg.currentIndex = tabid.substring(tabid.length - 1);
                    self._showTab();

                }, function () {
                    tabid = $(this).attr("id");
                    self.cfg.currentIndex = tabid.substring(tabid.length - 1);
                    //self._autoScroll();
                });

                $("span.floatad-close", self.cfg.floatad_winpop_id).click(function () { self.tips_pop("down"); });

                $(self.cfg.floatad_winpop_id).height("0px");
                setTimeout(function () { self.tips_pop("up"); }, 800);
                setTimeout(function () { self.tips_pop("down"); }, 15000);

                if ($.browser.msie && $.browser.version == "6.0") {
                    $(window).bind("scroll resize", function () {
                        var target = $(self.cfg.floatad_winpop_id);
                        target.css("top", ($(window).height() - target.height() + $(window).scrollTop()));
                    });
                }
            }
        }
    };

</script>

