                    if (!cats[0]) return;

                    cats.each(function(cat, i) {
                      var popup = cat.getElement('.m-cat-popup');
                      if (popup) {
                        cat.addEvents({
                          'mouseenter': function() {
                            this.addClass('current');
                            var li = this.getSize();
                            popup.setStyles({
                              'display': 'block',
                              'margin': '0px 0 0 ' + (li.x - 8) + 'px'
                            });
                          },
                          'mouseleave': function() {
                            this.removeClass('current');
                            popup.setStyle('display', 'none');
                          }
                        });
                      }
                    });
                  });
          </script> 
       </div>
      </div> 
     </div> </li> 
   </ul> 
  </div> 
  <div class="float-box" id="login_box"> 
  </div> 
  <script type="text/javascript">
	jq("#Menu").click(function(){  
    	jq("ul li").each(function() {    
        	jq(this).removeClass();  
    	}); 
    	jq(this).addClass('cur'); 
  	});   
</script>
