  <script type="text/javascript">
			
   			//高亮的索引   
   			var highLightIndex = -1;
   			//超时的标识（对用户连续快速按下键盘的事件做延时处理，只对用户在500ms内最后一次请求，让其生效）   
   			var timeoutId;
   			jq(document).ready(function () {
	   		init();
	   		jq('#auto_txt').bind('keyup', processKeyup);
   			});
			
   			/**   
   				* 处理键盘按下后弹起的事件   
   				* @param event   
   			*/
   			function processKeyup(event) {
   				jq(this).css('color','#333');
	   			var myEvent = event || windows.event;
	   			var keyCode = myEvent.keyCode;
		   //输入是字母，或者退格键则需要重新请求   
		   if (keyCode == 38 || keyCode == 40) {
			   processKeyUpAndDown(keyCode);
			   //按下了回车键   
		   } else if (keyCode == 13) {
			   processEnter();
		   }else{
			   highLightIndex = -1;
			   clearTimeout(timeoutId);
			   timeoutId = setTimeout(processAjaxRequest, 100);
		   }
	   }
	
	   /***   
	   * 初始化弹出框列表的位置，大小   
	   */
       	   
	   function init() {
		   jq('#auto_div').hide();
		   var pos = jq('#auto_txt').position();
		   var topPosition = pos.top + jq('#auto_txt').height() + 7;
		   var leftPosition = pos.left;
		   jq('#auto_div').offset({ top: topPosition, left: leftPosition });
		   jq('#auto_div').width(jq('jqauto_txt').width());
		   //$('#auto_div').css('position','absolute');   
	   }
		
	   /**   
	   * 处理Ajax请求   
	   * @param data   
	   */
	   function processAjaxRequest() {
		   jq.ajax({
			   type: "post",       //http请求方法,默认:"post"   
			   url: "/index.php?m=product&a=ajax&tid=1",   //发送请求的地址   
			   data: "keyword=" + jq('#auto_txt').val(),
			   success: processAjaxResponse
		   });
	   }
	
	   /**   
	   * 处理Ajax回复   
	   * @param data Ajax请求得到的返回结果为dom文档对象   
	   */
	   function processAjaxResponse(data) {
		   jq('#auto_div').html('').show();
		   var words = data.split("###");
		   jQuery.each(words, function (i) {
			   var word_div = jq("<div class='result'></div>");
			   word_div.html(words[i]);
			   word_div.hover(fnOver, fnOut);
			   word_div.click(getAutoText);
			   jq('#auto_div').append(word_div);
		   });
	   }
	
	   /**   
	   * 处理鼠标滑过   
	   */
	   function fnOver() {
	  	   //alert($(this));   
		   changeToWhite();
		   jq(this).css('background-color', 'pink');
		   //alert($(this).index());   
		   highLightIndex = jq(this).index();
		   //下面一行注释掉了，百度搜索也没这功能，就是鼠标移动时，跟着改变搜索框的内容   
		   jq('#auto_txt').val(jq('#auto_div').children().eq(highLightIndex).html());   
	   }
	
	   /**   
	   * 处理鼠标移除   
	   */
	   function fnOut() {
		   jq(this).css('background-color', 'white');
	   }
	
	   /**   
	   * 得到自动填充文本   
	   */
	   function getAutoText() {
		   //有高亮显示的则选中当前选中当前高亮的文本   
		   if (highLightIndex != -1) {
			   jq('#auto_txt').val(jq(this).html());
			   jq('#auto_div').html('').hide();
		   }
	   }
	
	   /**   
	   * 处理按下Enter键   
	   * @param keyCode   
	   */
	   function processEnter() {
		   if (highLightIndex != -1) {
			   jq('auto_txt').val(jq('#auto_div').children().eq(highLightIndex).html());
			   jq('#auto_div').html('').hide();
		   }
	   }
	
	   /**   
	   * 处理按上下键的情况   
	   */
	   function processKeyUpAndDown(keyCode) {
		   var words = jq('#auto_div').children();
		   var num = words.length;
		   if (num <= 0) return;
		   changeToWhite();
		   highLightIndex = ((keyCode != 38 ? num + 1 : num - 1) + highLightIndex) % num;
		   words.eq(highLightIndex).css('background-color', 'pink');
		   jq('#auto_txt').val(words.eq(highLightIndex).html());
	   }
	
	   /**   
	   * 如果有高亮的则把高亮变白   
	   */
	   function changeToWhite() {
		   if (highLightIndex != -1) {
			   jq('#auto_div').children().eq(highLightIndex).css('background-color', 'white');
		   }
	   }   
	   
	</script> 
  <script type="text/javascript">
		/*侧边导航*/
		<!--
		var timeout         = 50;
		var closetimer    = 0;
		var ddmenuitem      = 0;
		
		function mopen(id)
		{ 
		  mcancelclosetime();
		
		  if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';
		
		  ddmenuitem = document.getElementById(id);
		  ddmenuitem.style.visibility = 'visible';
		
		}
		function mclose()
		{
		  if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';
		}
		
		function mclosetime()
		{
		  closetimer = window.setTimeout(mclose, timeout);
		}
		
		function mcancelclosetime()
		{
		  if(closetimer)
		  {
		    window.clearTimeout(closetimer);
		    closetimer = null;
		  }
		}
		
		document.onclick = mclose; 
</script> 
