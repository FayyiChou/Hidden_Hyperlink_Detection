           function nr(a){  
    for(var i=1;i<7;i++){   
      if(i==a){
        document.getElementById("li"+i).className="active"
        document.getElementById("nr"+i).style.display="block";  
      }else{
        document.getElementById("li"+i).className="lb2"
        document.getElementById("nr"+i).style.display="none";
      } 
    } 
  }

