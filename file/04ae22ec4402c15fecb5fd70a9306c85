<script type="text/javascript">
    function ShowInfos(id) {
        $("#DivInfos ul").hide();
        $("#ulTitles li").attr("class", "");
        $("#ulInfos" + id).show();
        $("#liTitle" + id).attr("class", "pro_xz");
    }

    $(window).load(function () {
        function LimitImgSize(objImg, width, height) {
            var oWidth = objImg.width();
            var oHeight = objImg.height();
            if (oWidth > width || oHeight > height) {
                oRate = parseInt(100 * oWidth / oHeight) / 100;
                nRate = parseInt(100 * width / height) / 100;
                if (oRate <= nRate) { if (oHeight > height) objImg.height(height) }
                else { if (oWidth > width) objImg.width(width) }
            };
            if (parseInt(objImg.height()) != 0) {
                var mgn = parseInt((height - objImg.height()) / 2);
                objImg.css("margin-top", mgn + "px");
            }           
        }

        $(".img145_112").each(function () {
            LimitImgSize($(this), 145, 112);
        });
        //eof load
    });

</script>

