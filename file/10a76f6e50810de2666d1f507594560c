	    <script type="text/javascript">
			jQuery(document).ready(function(){
	
				jQuery('.search_input').watermark('البحث ..', {useNative: false});
	
				var bg_color_focus = "#555";
				var bg_position_focus = "0px -24px";
	
				var bg_color_focusout = jQuery(".search").css("background");
				var bg_position_focusout = jQuery(".search_btn").css("background-position");
	
				jQuery('.search_input').focus(
					function(){
						jQuery(".search").stop().css("background",bg_color_focus);
						jQuery(".search_btn").stop().css("background-position",bg_position_focus);
					}
				);
				
				jQuery('.search_input').focusout(
					function(){
						jQuery(".search").stop().css("background",bg_color_focusout);
						jQuery(".search_btn").stop().css("background-position",bg_position_focusout);
					}
				);
			});
		</script>
	

						
