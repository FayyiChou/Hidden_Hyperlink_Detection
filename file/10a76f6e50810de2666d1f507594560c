	<script type="text/javascript">
		jQuery(document).ready(function() {
	        setInterval(function() {
			    if (jQuery('.urgent').length){
					var statue = 1;
			    }else{
					var statue = 0;
				}
				jQuery.ajax({
					data: {
						exist: statue,
						template_output: 'box/urgent_article'
					},
					type: "GET",
					success: function(data) {
						jQuery('.urgent_container').html(data);
					}
			   });
	        }, 5000);
		});
    </script>

