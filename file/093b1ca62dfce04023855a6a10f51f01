<script>
var settingArr = {"svip":170,"vip1":25.5,"vip2":127.5,"vip3":255,"vip4":510,"vip5":1020};
//确认关闭弹框
$('body').on('click','.buyNowPassWordBtn', function () {
	
	 var GetUserId = $(this).data('getuserid');
     var VipCode = $(this).data('vipcode');
     var PayType = $(this).data('paytype');
     $("#passwordshowGetUserId").val(GetUserId);
     $("#passwordshowVipCode").val(VipCode);
     $("#passwordshowPayType").val(PayType);
	$("#passwordshow").show();
    // location.reload();
}); 
$('.d-close').on('click', function () {
    $(this).parents('.pop').css('display', 'none');
});





$('.ok_goods_record_finance_password').on('click', function () {
    var goodsRecordFinancePassword = $('#goodsRecordFinancePassword').val();
    var searchdata = "&Id="+999;
    if(!goodsRecordFinancePassword) {
        alert('请输入金融密码！');return;
    }else {
        searchdata = searchdata + "&Password=" + goodsRecordFinancePassword;
    }
    $.ajax({
        url:'/User/checkFinancePassword',
        type:'post',
        data:searchdata,
        dataType:'json',
        success:function(res){
            if(res.CODE == 'ok'){
                $('#passwordshow').hide();
               
                buyNowBtnVip();
            }else {
                alert(res.MESSAGE);
            }
        }
    });
 
});
/** 用户点击立即充值按钮 **/
function buyNowBtnVip(){
	var GetUserId = $("#passwordshowGetUserId").val();
	var VipCode = $("#passwordshowVipCode").val();
	var PayType = $("#passwordshowPayType").val();
    var status = false;
    $('.error_tips').html('');

    $.ajax({
        url:'/Vip/vipBuyAjax/?GetUserId='+GetUserId+'&VipCode='+VipCode+'&PayType='+PayType,
        type:'get',
        dataType:'json',
        async:false,
        cache:false,
        success:function(res){
            if(res.CODE == 'ok')
            {
                if(PayType == 'coin_rmb')
                {
                    // 跳转到充值页面
                    $('#dataStr').val(res.DATA);
                    status = true;                        
                }
                else
                {
                    alert('恭喜您已经成功开通'+VipCode+'！请刷新页面查看~');
                    location.href = '/Vip/index';
                }

            }
            else
            {
                $('#buy_content .error_tips').html(res.MESSAGE);
                return;
            }
        }
    });

    if(status)
    {
        document.getElementById('open_link').submit();
    }
};

$(function(){
	var type = "";
	//console.log(type);
	if(type != '')
	{
		var select = ".v-con a[data-vipcode*='"+type+"']";
		$(select).click();
	}
});

</script>




