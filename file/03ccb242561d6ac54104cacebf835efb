    yr = d.getYear(); if (yr < 1000)
    yr += 1900; return yr;
    }
    function showDate() {
    timeString = new Date().toLocaleTimeString();
    var sValue = getFullYear(dNow) + "年" + (dNow.getMonth() + 1) + "月" + dNow.getDate() + "日&nbsp;&nbsp;" + "" + timeString +
