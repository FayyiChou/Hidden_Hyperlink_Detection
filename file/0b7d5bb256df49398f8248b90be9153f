<script>
   /**
    * Created by chengtingsen@zbj.com on 2017-03-22
    * 服务详情页，优酷视频图片https问题修复
   * */
   $(function () {
       /*
       * 获取 大图url
       * 发送请求
       * 替换 小图url
       * */
       var largeImgBox = $('.j-service-large-img');
       var smallImgBox = $('.j-service-small-img');
       var largeImgs = largeImgBox.find('[data-type=video]');
       var smallImgs = smallImgBox.find('.j-service-type-video');
       var reqUrl = [];
       var url;
       var obj = {};
       /*
       * 处理请求的参数
       * */

       if (!largeImgs.length) {
           console.log('没有视频链接，直接返回');
           return;
       }

       largeImgs.each(function (index) {
           url = $(this).attr('data-url');
           /*obj = {};
           obj[index] = url;*/
           reqUrl.push(url);
       });

       /*
       *测试数据
       * */
       /*reqUrl = [
               'http://player.youku.com/player.php/sid/XMjYyODQ3OTUzMg==/partnerid/8ff76e361f305b44/v.swf',
               'http://player.youku.com/player.php/sid/XMTMwNzc4MDExMg==/partnerid/8ff76e361f305b44/v.swf',
               'http://player.youku.com/player.php/sid/XMjYwMjExNTMzNg==/partnerid/8ff76e361f305b44/v.swf'
       ]*/

       $.ajax({
           url : '/service/GetVideoFirstImg',
           data: {
               urls: reqUrl
           },
           type :"POST",
           dataType:'json',
           success:function(res){
               console.log(res);
               var msg = res.msg;
               if (msg instanceof Array && msg.length) {
                   replaceImgUrl(msg);
               }
           },
           error: function (res) {
               console.log(res);
           }

       });

       function replaceImgUrl(data) {
           var i = 0;
           var length = data.length;
           var url;
           var $img;
           for (;i < length; i++) {
               url = data[i];
               if (url) {
                   $img = $('<img />').attr('src', url)
                   smallImgs.eq(i).prepend($img);
               }
           }
       }

   });
</script>

    
