        (function () {
            var $time = $('.J-time-remain');
            var remainTime = $time.attr('data-time-remain');
            var $hour = $time.find('.J-hour-remain');
            var $minute = $time.find('.J-minute-remain');
            var interval = setInterval(function () {
                if(remainTime <= 0) {
                    clearInterval(interval);
                    return;
                }
                remainTime --;
                $hour.html(parseInt(remainTime / 60 / 60));
                $minute.html(parseInt(remainTime / 60 % 60));
            }, 1000);
        })();

        




