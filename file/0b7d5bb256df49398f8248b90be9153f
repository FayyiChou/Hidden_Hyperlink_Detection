    <script>
    (function(){
        function gGetCookie(name){
            var value = document.cookie;
            var start = value.indexOf(" " + name + "=");
            if(start == -1){
                start = value.indexOf(name + "=");
                if(start > 0){
                    return null;
                }
            }
            if(start == -1){
                return null;
            }

            start = value.indexOf("=", start) + 1;
            var end = value.indexOf(";", start);
            if(end == -1){
                end = value.length;
            }
            return decodeURIComponent(value.substring(start, end));
        }
        window.ZBJInfo.localizedCityId = gGetCookie('local_city_id');
        window.ZBJInfo.localizedCityName = gGetCookie('local_city_name');
    })();

</script>

