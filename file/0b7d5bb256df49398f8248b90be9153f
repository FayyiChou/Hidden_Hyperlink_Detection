    <script>
    var imgList = [];
    var startTime = +new Date();

    function loadCb(){
        imgList.push({
            img: this,
            time: +new Date()
        });
    }
    function isLazy(item){
        var regLazy = /lazyload/;
        try{
            if(typeof item.className!='undefined' && regLazy.test(item.className)){
                return true
            }else{
                return false
            }
        }catch(e){
            return false
        }
    }
    void function(window, document) {
        var currentImgDoms = document.getElementsByTagName("img");

        var regLazy = /lazyload/;
        for ( var i = 0, item; item = currentImgDoms[i++]; ) {
            if ( item.complete && ! isLazy(item)) {
                loadCb.call( item );
            } else {
                item.onload = function(){
                    if(!isLazy(item)){
                        this.onload = null;
                        loadCb.call( this );
                    }
                }
            }
        }
        window.gMonitor.getFirstSImg = imgList;
        window.gMonitor.getFirstSTime = startTime;
    }(window, document);
