                    standardPName+=$(item).text() + " ";
                    standardPriceArr[index]=$(item).text();
                })
                $(".j-shop-cart").attr("data-standard-price-name",standardPName);
                $(".j-shop-cart").attr("data-standard-price-array",standardPriceArr);
