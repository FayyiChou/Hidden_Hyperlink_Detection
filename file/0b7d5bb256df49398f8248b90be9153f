        $(function(){
            var isLoading02comment = false;
            // 评价列表ajax请求
            var $commentWarp01 = $('#j-comment-list-1');
            var $commentWarp02 = $('#j-comment-list-2');
            var getUrl = '/service/evaluationlist-sid-' + sId + '-uid-' + uId + '-type-2';

            // 评价列表ajax请求
            $.get(getUrl, function(data){
                $commentWarp01.html(data);
            });

            //tab切换，请求评价数据
            $('#j-comment').click(function(){
                if(!isLoading02comment){
                    $.get(getUrl, function(data){
                        $commentWarp02.html(data);
                        isLoading02comment = true;
                    });
                }
            });

            //*/相似服务初始化请求地址
            var similarUrl = '/service/usersimilar-serviceid-'+ sId +'-limit-5.html';
            var similarPackageUrl = '/service/usersimilar-serviceid-'+ sId +'-limit-5-package-1.html';
            var similarWarp = $('#j-service-similar');
            var similarPackageWarp = $('#j-service-similar-package');
            var changeSimilarLink = $('#j-similar-change');
            var changeSimilarPackageLink = $('#j-similar-change-package');
            getSimilarData(similarUrl);
            getSimilarPackageData(similarPackageUrl);

            changeSimilarLink.click(function(){
                var loading = '<div class="service-loading"><div class="loading-txt">正在加载类似服务...</div></div>';
                var getUrl = $(this).attr('data-href');
                similarWarp.html(loading);
                getSimilarData(getUrl);
            });

            changeSimilarPackageLink.click(function(){
                var loading = '<div class="service-loading"><div class="loading-txt">正在加载类似服务...</div></div>';
                var getUrl = $(this).attr('data-href');
                similarPackageWarp.html(loading);
                getSimilarPackageData(getUrl);
            });

            function getSimilarData(url){
                $.get(url, function(json){
                    if(json.state == 1){
                        similarWarp.html(json.msg);
                        changeSimilarLink.attr('data-href', json.act_href);
                    }else{
                        similarWarp.closest('.other-service').remove();
                    }
                }, 'json');
            }

            function getSimilarPackageData(url){
                $.get(url, function(json){
                    if(json.state == 1){
                        similarPackageWarp.html(json.msg);
                        changeSimilarPackageLink.attr('data-href', json.act_href);
                    }else{
                        similarPackageWarp.closest('.other-service').remove();
                    }
                }, 'json');
            }

            // 店铺流量统计
            (new Image()).src = 'http://shop.zbj.com/main/UpShopView-uid-16725214';

