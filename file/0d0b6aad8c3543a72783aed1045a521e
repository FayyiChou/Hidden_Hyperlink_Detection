		$(".head_nav li").hover(
			function(){
					var e=$(this).attr("e");
					if(e){
						$(".ejcd").find(".ej_menu").hide();
						$(".ejcd").stop().animate({"top":"0px"},500);
						$(".ejcd").find(".ej_menu[e="+e+"]").show();
					}else{
						$(".ejcd").stop().animate({"top":"-480px"},500);
					}
			},
			function(){
				//$(".ejcd").stop(true,true).animate({"top":"-220px"},800);
			}
		);
		
		
		$(".ejcd .ej_menu").hover(
			function(){},
			function(){
				$(".ejcd").stop().animate({"top":"-480px"},500);
			}
		);	
		
		
	</script>
