	<script type="text/javascript">
	$("#gx_em").mouseover(function(){
		$("#gx_em_attr").show( 200);
	});
	$("#gx_em").mouseout(function(){
		$("#gx_em_attr").hide( 200);
	});
	$("#goTop").click(function(){  
		$('body,html').animate({scrollTop:0},500);  
		return false;  
		$(".side_panel").css('visibility','hidden')
	}); 
	window.onresize = window.onscroll = function() {
		var x=(document.body.scrollTop||document.documentElement.scrollTop);
		if(x<500){
			$(".side_panel").css('visibility','hidden')
		}else{
			$(".side_panel").css('visibility','visible')
		};     
		var scrollPos;var ie = /msie/i.test(navigator.userAgent),style;
		var o = document.getElementById('left_fu');
		var db = document.getElementById('db');
		var height_ = db.offsetTop-550;   
		if (typeof window.pageYOffset != 'undefined') {
			scrollPos = window.pageYOffset;
		} else if (typeof document.compatMode != 'undefined' && document.compatMode != 'BackCompat') {
			scrollPos = document.documentElement.scrollTop;
		} else if (typeof document.body != 'undefined') {
			scrollPos = document.body.scrollTop;
		}
		var _rig = document.getElementById('rig');
		if (scrollPos >= 2150  && scrollPos<height_) {
			if (!ie) {
				o.style.cssText += "position:fixed;right:0;left:" + getOffsetLeft(_rig) + "px;top:0px;";
			} else {
				o.style.cssText += "position:absolute;left:" + getOffsetLeft(_rig) + "px;top:" + (scrollPos) + "px;";
			}
		}else if (scrollPos >= height_) {
			o.style.cssText += "position:absolute;left:" + getOffsetLeft(_rig) + "px;top:" + height_+ "px;";
		} else {
			o.style.cssText = '';
		}
	}
	document.onkeydown = pageEvent;
	var pre = "/html/content_167987.html";
	function pageEvent(evt) {
		evt = evt || window.event;
		var key = evt.which || evt.keyCode;
		if (key == 37) {location = pre;}
		if (key == 39) {location = pre;}
	}
	function shownext() {
		var h = __('contentStyle').offsetHeight;
		var l = getLeft(__('contentStyle'));
		var hh = (h / 2 + 100) + 'px';
		__('lefts').style.left = (l + 1) + 'px';
		__('rights').style.left = (l + 650 - 84) + 'px';
		__('lefts').style.top = hh;
		__('rights').style.top = hh;
		__('lefts').style.display = 'block';
		__('rights').style.display = 'block';
	}
	
		function ImgLazyLoad(e) {
		function i() {
			$("#num").html($(window).height() + $(window).scrollTop()), t.Id.each(function() {
				s(this, t) || $(this).is(":visible") && $(this).trigger("appear");
			});
		}
		function s(e, t) {
			var n = $(window).height() + $(window).scrollTop();
			return n <= $(e).offset().top - t.threshold;
		}
		var t = {
			Id: $("img"),
			threshold: 100,
			effectspeed: 0,
			effect: "show"
		};
		$.extend(t, e);
		var n = t.Id,r = null;
		return $(window).bind("load reset", i), $(window).scroll(function() {
			r && clearTimeout(r), r = setTimeout(i ? i : "", 100);})
			, n.each(function(e) {
				var n = this;
				$(n).data("t", $(n).offset().top), s(n, t) ? n.loaded = !1 : $(n).is(":visible") && ($(n).attr("src", $(n).attr("data")), n.loaded = !0), $(n).one("appear", function() {
					this.loaded || $("<img />").bind("load", function() {
					$(n).hide().attr("src", $(n).attr("data"))[t.effect](t.effectspeed), n.loaded = !0;
					}).attr("src", $(n).attr("data"));
			});
		});
	}
	ImgLazyLoad({Id: $('#lazyload img'),effect: "fadeIn"});
	
	function setTab(name,cursel,n){
		for(i=1;i<=n;i++){
			var menu=document.getElementById(name+i);
			var con=document.getElementById("con_"+name+"_"+i);
			menu.className=i==cursel?"hover":"";
			con.style.display=i==cursel?"block":"none";
			
			var imga = con.getElementsByTagName('img');
			var imgb = imga.length;
			//alert(imgb);
			
			for(k=0;k<imgb;k++){
				
				var data = imga[k].getAttribute('data');
				imga[k].src = data;
				
			}
			
			
		}
		
	}
