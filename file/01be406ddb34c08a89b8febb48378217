  <script src="http://www.qxcu.com/js/jquery-1.10.2.min.js" type="text/javascript"></script> 
  <script type="text/javascript">
    $(document).ready(function() {
        var current = $("#masterid").val();
        var alist = new Array();
        if (current == "") {
            current = -1;
        }
        $("#nav>a").each(function(i, items) {
            alist[i] = $(items);
            $(alist[i]).click(function() {
                if (i != current) {
                    $(this).addClass("check");
                    $(alist[current]).removeClass("check");
                    current = i;
                }
            });
        });
        $("#nav>a").each(function(i, items) {
            alist[i] = $(items);
            if (i != current) {
                $(alist[i]).removeClass("check");
            }
        });
        $(alist[current]).addClass("check");
    });
</script> 
  <!--TAB--> 
  <script type="text/javascript">
    function setTab(m, n) {
        var tli = document.getElementById("menu" + m).getElementsByTagName("li");
        var mli = document.getElementById("main" + m).getElementsByTagName("ul");
        for (i = 0; i < tli.length; i++) {
            tli[i].className = i == n ? "hover" : "";
            mli[i].style.display = i == n ? "block" : "none";
        }
    }
</script> 
