</footer><script type="text/javascript">//关键词 URL
var rkey = '/zlist-15-0-0-0-0-0-0-0-0-0-key-230-0-0-0-0';document.getElementById("textkey").value = '侵略行为';

//修改关键词
function set_key(){
var key = document.getElementById("textkey").value;
//没变则不提交
if( key == '侵略行为'){
return ;
}

key = key==''?0:key;
url = rkey.replace('key',key);
location.href=url;
}

//如果是Firefox
if(document.addEventListener){
document.getElementById("textkey").addEventListener("keypress",fireFoxHandler, true);
}else{
document.getElementById("textkey").attachEvent("onkeypress",ieHandler);
}
//firefox
function fireFoxHandler(evt){
if(evt.keyCode==13){
   set_key();
}
}
//IE
function ieHandler(evt){
if(evt.keyCode==13){
set_key();
}
}
</script>

