  <script>
                        $(function(){ 
						      new menu({});
						      function menu(option){
							      var option = {
								      "menu" : "#menu",
									  "one" : "#menu .one",
									  "menu1" : "#menu .menu1",
									  "menu2" : "#menu .menu2" 
  								  }
						 
 								  
						 	 $(option.menu2).hover( 
								          function(){ 
										                    $("#menu .one .list2").hide();
														 	$(this).next(".list2").show(); 
															 
														   if($(this).parent().find(".list2 ul").length>0){ $(this).parent().parent().parent().css("width","400px"); }
														    $(this).parent().parent().parent().css("height",(parseInt($(this).next(".list2").css("height")) + parseInt($(this).parent().parent().css("height")))+"px");
  										  } 
 								 )
							      $(option.one).hover( 
										  function(){
														 	$(this).find(option.menu1).attr("class","menu1 menu1_hover");
													 		$(this).children(".list1").show(); 
														 	$(this).children(".menu1").css("border-bottom","solid 2px #e62128"); 
															$(this).find(".menu1 a").css("color","#e62128");
													 
															 
										  },
										  function(){
														 	 $(this).find(option.menu1).attr("class","menu1");
														 	 $(this).children(".list1").hide();  
														 	 $(this).children(".menu1").css("border-bottom","solid 2px #fff"); 
															 $(this).find(".menu1 a").css("color","#000");
														 
										  }
   								   )
								  
							  } 
 						 
						 })
</script> 
   
 



 
