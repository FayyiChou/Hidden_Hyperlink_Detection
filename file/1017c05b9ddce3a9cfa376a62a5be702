		<script type="text/javascript">
			$(function(){
				var num=$(".divgg ul li");
				//定义初始值
				var init=1;
				//定义一记时器
				var dt=null;
				//定义一个开关
				var ispause=false;
				autoplay();
				//切换
				function changegg(index){
				$(".divgg ul li:first").animate({"margin-top":-22},1400,function(){
					$(this).css("margin-top","0").appendTo(".divgg ul");
				});
				}
				//连续切换
				function play(){
					init++;
					if(init>num.length){
						init=1;
					}
					changegg(init);
				}
				//定时播放
				function autoplay(){
					dt=setInterval(function(){
						play();
					},3000);
				}
				//上切换
				$(".prevbtn").click(function(){
					init--;
					if(init<1){
						init=num.length;
					}
					changegg(init);
					pause();
				});
				//下切换
				$(".nextbtn").click(function(){
					init++;
					if(init>num.length){
						init=1;
					}
					changegg(init);
					pause();
				});
				//停止播放
				function pause(){
					clearInterval(dt);
				}
				$(".divgg").hover(function(){
					 pause();
				},function(){
					autoplay();
				});
			});
		</script>
		<script type="text/javascript">
        	jQuery("#slideBox5").slide({mainCell:".bd ul",autoPlay:true,effect:"leftLoop",interTime:5000,delayTime:1000,scroll:1,easing:"swing"});
        </script>
        
		
		
		        
		<script type="text/javascript">
			$(function(){
				var num=$(".divgg ul li");
				//定义初始值
				var init=1;
				//定义一记时器
				var dt=null;
				//定义一个开关
				var ispause=false;
				autoplay();
				//切换
				function changegg(index){
				$(".divgg ul li:first").animate({"margin-top":-22},1400,function(){
					$(this).css("margin-top","0").appendTo(".divgg ul");
				});
				}
				//连续切换
				function play(){
					init++;
					if(init>num.length){
						init=1;
					}
					changegg(init);
				}
				//定时播放
				function autoplay(){
					dt=setInterval(function(){
						play();
					},3000);
				}
				//上切换
				$(".prevbtn").click(function(){
					init--;
					if(init<1){
						init=num.length;
					}
					changegg(init);
					pause();
				});
				//下切换
				$(".nextbtn").click(function(){
					init++;
					if(init>num.length){
						init=1;
					}
					changegg(init);
					pause();
				});
				//停止播放
				function pause(){
					clearInterval(dt);
				}
				$(".divgg").hover(function(){
					 pause();
				},function(){
					autoplay();
				});
			});
		</script>

        
        

    
