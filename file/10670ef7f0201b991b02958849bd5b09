<script type="text/javascript">
function login_refreshVerifyCodeImg(){
	var e=document.getElementById("verifyCodeImg");
	var path="http://agent.koolearn.com";
	if(e){var src=path+"/jsp/getverifycode.jsp?a="+(new Date()).getTime();e.src=src;}
}
function checkForm(){
	if(document.agentlogin==null)
		return;
	if(document.agentlogin.userName.value==""){
		alert("请输入用户名");
		return false;
		}
	if(document.agentlogin.password.value==""){
		alert("请输入密码");
		return false;
		}
	if(document.agentlogin.verifyCode.value==""){
		alert("请输入验证码");
		return false;
		}
	
	return true;
}
</script>
