<script>
$(".snav").hide();
$(".nav li").hover(function(){
	$(this).children(".snav").fadeIn()
},function(){
	$(this).children(".snav").fadeOut()
})
$(".hed-b .search").click(function(){
	if($(this).hasClass("sec")){
		$(this).removeClass("sec").addClass("ca");
		$(".search-box").show();
	}else{
		var _this = $(this);
		_this.removeClass("ca").addClass("sec");
		$(".search-box").hide();

	}
})
</script>



