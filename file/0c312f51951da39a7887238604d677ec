							FromCat = 'counterpoint';
						} else if (SegTwo == 'hardware') {
							FromCat = 'hardware';
						} else if (SegTwo == 'training-and-support') {
							FromCat = 'training-and-support';
						} else if (SegTwo == 'our-partners') {
							FromCat = 'our-partners';
						}
					} else if (SegOne == 'can-we-help-you') {
						FromCat = 'can-we-help-you';
						if (SegTwo == 'your-company-size') {
							FromCat = 'your-company-size';
						}
					} else if (SegOne == 'industry-sectors') {
						FromCat = 'industry-sectors';
						if (SegTwo == 'marine-industry-software') {
							FromCat = 'marine-industry-software';
						} else if (SegTwo == 'caravan-industry') {
							FromCat = 'caravan-industry';
						} else if (SegTwo == 'outdoor-leisure') {
							FromCat = 'outdoor-leisure';
						} else if (SegTwo == 'diy-builders-merchants') {
							FromCat = 'diy-builders-merchants';
						} else if (SegTwo == 'charity-sector') {
							FromCat = 'charity-sector';
						}
					} else if (SegOne == 'contact') {
						FromCat = 'contact';
					} else {
						FromCat = '';
					}
				var objBtn = $(this),
					regPattern = /demo/i,
					strURL = regPattern.test(objBtn.text()) ? 'http://www.kudos-software.co.uk/request-a-demo/' + FromCat : 'http://www.kudos-software.co.uk/contact-form/' + FromCat;

				objBtn.removeAttr('title').fancybox({
					type:'iframe',
					href:strURL,
					padding:0,
					width:700,
					height:520
				});
			});
