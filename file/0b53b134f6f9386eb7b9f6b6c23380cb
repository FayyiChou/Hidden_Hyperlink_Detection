<script type="text/javascript">
$(function(){
	$(".day3 ul li").hover(function(){
		$(this).addClass("day-on").siblings().removeClass("day-on");
		var index= $(".day3 ul li").index(this);
		$(".weather").eq(index).show().siblings().hide();
	})
})
