	<script type="text/javascript">
	$(function () {
		var height = parseInt('');
		$('.left1').css('height', height + 3 + 'px');
		$('.right1').css('height', height + 3 + 'px');
		$('.left1 em').css('height', height + 3 + 'px');
		$('.right1 em').css('height', height + 3 + 'px');
		$('.left1 span img').css('top', height / 2 - 55 / 2 + 'px');
		$('.right1 span img').css('top', height / 2 - 55 / 2 + 'px');
	})
	function clickTo(key) {
		var id = $("#scoll img[rel='"+ key + "']").attr('name');
		window.location.href = "http://trip.elong.com/wuerhemoguicheng/tupian/" + id + ".html#pic";
	}
	function scollTo(key) {
		var px = parseInt(key) * 101;
		var width = $('#scoll').width();
		if (px > width - 450) {
			var leftwidth = (width - 808) < 0 ? '0px' : '-' + (width - 808) + 'px';
			$('#scoll').animate({
				left : leftwidth
			}, 10);
		} else if (px < 350) {
			$('#scoll').animate({
				left : '0px'
			}, 'fast');
		} else {
			var leftwidth = (px - 350) < 0 ? (px - 350) + 'px' : '-' + (px - 350) + 'px';
			$('#scoll').animate({
				left : leftwidth
			}, 'fast');
		}
		cursor = key;
	}
	scollTo(cursor);
	$('#scoll img').click(function () {
		var key = $(this).attr('rel');
		if (!checkCount(key)) {
			return false;
		}
		clickTo(key);
	});
	$('#rightScroll').click(function () {
		if (parseInt(cursor) + 8 > parseInt(photo_count)) {
			var key = parseInt(photo_count) - 1;
		} else {
			var key = parseInt(cursor) + 8;
		}
		if (parseInt(photo_count) < 9) {
			alert('当前只有一页');
			return false;
		}
		if (parseInt(cursor) > parseInt(photo_count) - 5) {
			alert('当前已经是最后一页，按确定浏览第一页');
			key = 0;
		}
		if (key == cursor) {
			return false;
		}
		if (!checkCount(key)) {
			return false;
		}
		scollTo(key);
	});
	$('.right1').click(function () {
		var key = parseInt($('#scoll img.on').attr('rel')) + 1;
		if (parseInt(cursor) == parseInt(photo_count) - 1) {
			alert('当前已经是最后一张，按确定浏览第一张');
			key = 0;
		}
		if (!checkCount(key)) {
			alert('当前已经是最后一张');
			return false;
		}
		clickTo(key);
	});
	$('#leftScroll').click(function () {
		if (parseInt(cursor) - 8 < 1) {
			var key = 0;
		} else {
			var key = parseInt(cursor) - 8;
		}
		if (parseInt(photo_count) < 9) {
			alert('当前只有一页');
			return false;
		}
		if (parseInt(cursor) < 4) {
			alert('当前已经是第一页，按确定浏览最后一页');
			key = parseInt(photo_count) - 1;
		}
		if (key == cursor) {
			return false;
		}
		if (!checkCount(key)) {
			return false;
		}
		scollTo(key);
	});
	$('.left1').click(function () {
		var key = parseInt($('#scoll img.on').attr('rel')) - 1;
		if (parseInt(cursor) == 0) {
			alert('当前已经是第一张，按确定浏览最后一张');
			key = parseInt(photo_count) - 1;
		}
		if (!checkCount(key)) {
			alert('当前已经是最后一张');
			return false;
		}
		clickTo(key);
	});
	function checkCount(key) {
		if (key < 0) {
			return false;
		}
		if (key > parseInt(photo_count) - 1) {
			return false;
		}
		return true;
	}
	function full(url) {
		var full = window.open(url,"IMGfullscreen","fullscreen=yes,status=no,location=no, toolbar=no,directories=no,menubar=no,resizable=yes,scrollbars=no");
		full.moveTo(0,0);
		full.resizeTo(screen.width,screen.height);
	}
	$(function () {
		$('.left1').mouseover(function () {
			$('.left1 img').css('display', 'block');
		})
		$('.left1').mouseout(function () {
			$('.left1 img').css('display', 'none');
		})
		$('.right1').mouseover(function () {
			$('.right1 img').css('display', 'block');
		})
		$('.right1').mouseout(function () {
			$('.right1 img').css('display', 'none');
		})
	})
	</script>
