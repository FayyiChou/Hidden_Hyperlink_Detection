<script>

	$(document).ready(function() {
		var timer;
		$("div.erweima").mouseleave(function() {
			showPPOut();


		});
		$("div.erweima").mouseenter(function() {
			clearPPTimer();
			$("#weixinBar").show();
			$("#ewm").attr('src', 'http://trip.elong.com/trip/default/static/images/erweima1.jpg');

		});
		$("div#weixinBar").mouseleave(function() {
			showPPOut();

		});
		$("div#weixinBar").mouseenter(function() {
			clearPPTimer();
			$("#ewm").attr('src', 'http://trip.elong.com/trip/default/static/images/erweima1.jpg');

		});

	});

	function clearPPTimer() {
		try {
			clearTimeout(timer);
		} catch (s) {
		}
	}
	function hideweixin() {
		$("#ewm").attr('src', 'http://trip.elong.com/trip/default/static/images/erweima.jpg');
		$("#weixinBar").hide()
	}
	function showPPOut() {
		timer = setTimeout("hideweixin()", 500);
	}

</script>
