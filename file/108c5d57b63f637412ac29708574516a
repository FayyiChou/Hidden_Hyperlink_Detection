<script type="text/javascript">
//  这个js 先放在这里
var sgid = "";
function link1() {
var href = window.location.href;
if (href.indexOf("\/1\/") > 0 || href.indexOf("\/2\/") > 0
|| href.indexOf("\/3\/") > 0 || href.indexOf("\/4\/") > 0 || href.indexOf("chezhan") > 0) {
window.location.href = 'http://price.pcauto.com.cn/';
return;
}
if (href.indexOf("http://price.pcauto.com.cn/price") < 0) {
if (href.indexOf("\/nb") > 0) {
var nbId = href.match(/nb\d+/);
if(nbId){
var _nbId = nbId.toString();
_nbId = _nbId.replace('nb','');
window.location.href = "http://price.pcauto.com.cn/price/nb"+_nbId+"/";
}else{
window.location.href = "http://price.pcauto.com.cn/price/";
}
} else if (href.indexOf("\/b") > 0) {
var bId = href.match(/b\d+/);
if(bId){
var _bId = bId.toString();
_bId = _bId.replace('b','');
window.location.href = "http://price.pcauto.com.cn/price/b"+_bId+"/";
}else{
window.location.href = "http://price.pcauto.com.cn/price/";
}
} else if (href.indexOf("\/sg") > 0) {
var sgId = href.match(/sg\d+/);
if(sgId){
var _sgId = sgId.toString();
_sgId = _sgId.replace('sg','');
window.location.href = "http://price.pcauto.com.cn/price/sg"+_sgId+"/";
}else{
window.location.href = "http://price.pcauto.com.cn/price/";
}
} else if (sgid != "") {
window.location.href = 'http://price.pcauto.com.cn/price/sg' + sgid + '/';
} else {
window.location.href = 'http://price.pcauto.com.cn/';
}
} else {
window.location.href = 'http://price.pcauto.com.cn/';
}
}
function link2(obj) {
var href = window.location.href;
var val = $(obj).html();
if ((href.indexOf("http://price.pcauto.com.cn/cars") < 0 || href.indexOf("http://price.pcauto.com.cn/tujie") < 0) && href.indexOf("http://price.pcauto.com.cn/chezhan") < 0) {
if (href.indexOf("\/nb") > 0) {
var nbId = href.match(/nb\d+/);
if(nbId){
var _nbId = nbId.toString();
_nbId = _nbId.replace('nb','');
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/nb"+_nbId+"/";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/nb"+_nbId+"/";
}
}else{
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/pic.html";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/";
}
}
} else if (href.indexOf("\/b") > 0) {
var bId = href.match(/b\d+/);
if(bId){
var _bId = bId.toString();
_bId = _bId.replace('b','');
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/b"+_bId+"/";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/b"+_bId+"/";
}
}else{
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/pic.html";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/";
}
}
} else if (href.indexOf("\/sg") > 0) {
var sgId = href.match(/sg\d+/);
if(sgId){
var _sgId = sgId.toString();
_sgId = _sgId.replace('sg','');
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/sg"+_sgId+"/";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/sg"+_sgId+"/";
}
}else{
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/pic.html";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/";
}
}
} else {
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/pic.html";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/";
}
}
} else {
if(val.indexOf("实拍图片") > -1){
window.location.href = "http://price.pcauto.com.cn/cars/pic.html";
}else if(val.indexOf("图解图片") > -1){
window.location.href = "http://price.pcauto.com.cn/tujie/";
}
}
}
function link3() {
var href = window.location.href;
if (href.indexOf("\/1\/") > 0 || href.indexOf("\/2\/") > 0
|| href.indexOf("\/3\/") > 0 || href.indexOf("\/4\/") > 0) {
window.location.href = 'http://price.pcauto.com.cn/video/';
return;
}
if (href.indexOf("http://price.pcauto.com.cn/video") < 0) {
if (href.indexOf("\/nb") > 0) {
var nbId = href.match(/nb\d+/);
if(nbId){
var _nbId = nbId.toString();
_nbId = _nbId.replace('nb','');
window.location.href = "http://price.pcauto.com.cn/video/nb"+_nbId+"/";
}else{
window.location.href = "http://price.pcauto.com.cn/video/";
}
} else if (href.indexOf("\/b") > 0) {
var bId = href.match(/b\d+/);
if(bId){
var _bId = bId.toString();
_bId = _bId.replace('b','');
window.location.href = "http://price.pcauto.com.cn/video/b"+_bId+"/";
}else{
window.location.href = "http://price.pcauto.com.cn/video/";
}
} else if (href.indexOf("\/sg") > 0) {
var sgId = href.match(/sg\d+/);
if(sgId){
var _sgId = sgId.toString();
_sgId = _sgId.replace('sg','');
window.location.href = "http://price.pcauto.com.cn/video/sg"+_sgId+"/";
}else{
window.location.href = "http://price.pcauto.com.cn/video/";
}
} else if (sgid != "") {
window.location.href = 'http://price.pcauto.com.cn/video/sg' + sgid + '/';
} else {
window.location.href = 'http://price.pcauto.com.cn/video/';
}
} else {
window.location.href = 'http://price.pcauto.com.cn/video/';
}
}
