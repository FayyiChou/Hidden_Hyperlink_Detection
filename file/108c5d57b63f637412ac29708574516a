});
/**
* 报价连接处理
*/
function newBrandLink(id,link){
var href = window.location.href;
if (href.indexOf("http://price.pcauto.com.cn/price") > -1) {
if(href.indexOf("http://price.pcauto.com.cn/price/nb") > -1 ||
href.indexOf("http://price.pcauto.com.cn/price/b")  > -1 ||
href.indexOf("http://price.pcauto.com.cn/price/sg") > -1 ){
window.location.href = link;
}else if(href.indexOf("http://price.pcauto.com.cn/price/q-") > -1){
var index, tmp;
if(href.indexOf("http://price.pcauto.com.cn/price/q-d") > -1){
index = href.indexOf("\/q-d");
tmp = href.substring(index + 4);
if(tmp.indexOf("-") > -1){
window.location.href = "http://price.pcauto.com.cn/price/q-d" + id + tmp.substring(tmp.indexOf("-"));
}else{
window.location.href = link;
}
}else{
index = href.indexOf("\/q-");
tmp = href.substring(index + 3);
window.location.href = "http://price.pcauto.com.cn/price/q-d" + id + "-" + tmp;
}
}else{
window.location.href = link;
}
}else{
window.location.href = link;
}
}
