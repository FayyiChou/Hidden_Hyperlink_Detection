<script src="http://js.3conline.com/min2/temp/v2/plugin-locate,plugin-locate_auto.js" charset="utf-8"></script>
<script type="text/javascript">
//去掉getScript方法自带的时间戳参数
$.ajaxSetup({
cache: true
});
//初始化
var _areaId;
LocateAuto.init({
device: 'pc',
cityCode: '',
callback:function(data){
_areaId = data.cityId;
serialLocalPrice(_areaId);//经销商报价
getFqProduct(_areaId); //分期
getSecondHandCarPrice(_areaId); //获取该地区车系最低二手车价格
}
});
$(".lieBiao").each(function(index, element) {
this.id="list"+index;
});
//展开与关闭符合条件的车型
$(".thlieBiao .btn").has("em").click(function(){
getLowestModelPrice(_areaId,$(this).attr("data-sgId"));//获取本地最低价
$(".lieBiao").removeClass("active");
$(".lieBiao").height(160);
$(this).parents(".lieBiao").addClass("active");
window.scrollTo(0,$(this).parents(".lieBiao").offset().top-90);
var a=parseInt($(this).parents(".lieBiao").find(".thTab_b").attr("rel"));
$(this).parents(".lieBiao").height(a)
toggleSale($(this).parents(".lieBiao").attr("id"));
});
$(".tblieBiao .fh").click(function(){
$(this).parents(".lieBiao").removeClass
("active");
$(this).parents(".lieBiao").height(160);
});
function getSecondHandCarPrice( rId ){
$("a[id^=second_hand_car_price_]").each(function(i){
var sgId = this.id.substr(22);
var sg = $(this);
$.ajax({//经销商报价下方显示
url: '/price/api/v1/cooperation/guazi/local_second_data/',
data: {
serialGroupId: sgId,
regionId: rId,
from: 1,
orderIndex : 2//价格升序
},
type: 'get',
dataType: 'json',
success: function (data2) {
var obj2 = data2.data[0];
sg.attr("href","http://price.pcauto.com.cn/sg" + sgId + "/second_hand_car.html");
sg.removeAttr("style");
if(obj2){
sg.html( "二手车" + obj2.price+ "万起>>");
}else{
sg.attr("href","http://price.pcauto.com.cn/dealer/secondcar/maiche.jsp");
sg.html( "卖车换新车>>");
}
}});
});
}
//在售车型与即将上市的切换
function toggleSale(d){
$("#"+d+" .contentdiv").hide();
$("#"+d+" .toc").removeClass("cur");
$("#"+d+" .contentdiv").eq(0).show();
$("#"+d+" .toc").eq(0).addClass("cur");
$("#"+d+" .toc").click(function(){
$("#"+d+" .contentdiv").hide();
$("#"+d+" .toc").removeClass("cur");
$(this).addClass("cur");
$("#"+d+" .contentdiv").eq($(this).index()).show();
});
}
var temp_sgid;
//获取本地最低价
function getLowestModelPrice(rId,sgId){
temp_sgid = sgId;
$.getScript('http://price.pcauto.com.cn/dealer/interface/price/getLowestModelPrice.jsp?sgId='+sgId+'&rId='+rId+'&callback=getPrice');
}
function getPrice(data){
var sgId = temp_sgid;
if(data != null && data.result){
var modelId,shtml,obj;
$("[id^=lowestPrice_"+sgId+"]").each(function(){
modelId = this.id.replace("lowestPrice_"+sgId+"_","");
if(undefined == data.price[modelId] || "" == data.price[modelId]){//没有最低报价
obj = $("#gfPrice_"+modelId);
var objValue = 0;
if(obj.html() != null){
objValue = obj.html().replace("万","");
}
if(objValue>0 && objValue!="0.00" && objValue.indexOf("暂无报价")==-1){//有官方价
shtml = '<a href="/m'+modelId+'/price.html" target="_blank" rel="nofollow">'+ '<em class="red">'+objValue+'万</em></a>'
+ '<a href="/dealer/commorders/order.jsp?ad=4285&t=0&mid='+modelId+'" target="_blank" rel="nofollow" class="icon_zs">询底价</a>';
}else{
shtml = '<em class="smoke">暂无报价</em>';
}
}else{
shtml = '<a href="/m'+modelId+'/price.html" target="_blank" rel="nofollow">'+ '<em class="red">'+data.price[modelId]+'万</em></a>'
+ '<a href="/dealer/commorders/order.jsp?ad=4285&t=0&mid='+modelId+'" target="_blank" rel="nofollow" class="icon_zs">询底价</a>';
}
$("#lowestPrice_"+sgId+"_"+modelId).html(shtml);
});
}
}
//车主点评
var commentRoot = "http://price.pcauto.com.cn/comment";
$('a[name=driver_score]').each(function(){
var sgid = this.id.substr(13);
$.getScript(commentRoot+"/interface/auto/serial/get_comment_avg_score_4serial_json.jsp?sgid="+sgid+"&callback=getDriverScore");
})
function getDriverScore(data){
if(data.count == 0){
$('#driver_score_'+data.sgid).html('<span>暂无评分</span>');
} else {
$('#driver_score_'+data.sgid).attr('href',commentRoot+"/sg"+data.sgid+"/");
$('#driver_score_'+data.sgid).html('<span class="point-star"><i style="width:90%"></i></span><span class="emphs">'+data.avgScore+'</span></em><span class="black">（<b>'+data.count+'</b>人点评）</span>');
}
}
//经销商报价
function serialLocalPrice(rid){
var DEALER4SROOT = "http://price.pcauto.com.cn";
var temp_sid = '';
$('dd[name="dealer_price"]').each(function(){
temp_sid = temp_sid + this.id.substr(13) + ',';
});
var sid = temp_sid.substr(0,temp_sid.length-1) ;
$.getScript(DEALER4SROOT+'/dealer/interface/price/get_serialgoup_local_price.jsp?sid='+sid+'&rid='+rid+'&callback=showSerialLocalPrice');
}
function showSerialLocalPrice(data){
$('dd[name="dealer_price"]').each(function(i){
if(data[i].lowestPrice == data[i].highestPrice){
if(data[i].lowestPrice > 0){
$('#dealer_price_'+this.id.substr(13)).html(data[i].lowestPrice+'万');
} else {
$('#dealer_price_'+this.id.substr(13)).html('暂无报价');
$('#dealer_price_'+this.id.substr(13)).attr('class','');
}
} else {
if(data[i].lowestPrice > 0){
$('#dealer_price_'+this.id.substr(13)).html(data[i].lowestPrice+'-'+data[i].highestPrice+'万');
} else {
$('#dealer_price_'+this.id.substr(13)).html(data[i].highestPrice+'万');
}
}
});
}
//分期购车
function getFqProduct(regionId) {
var JRROOT = "http://jr.pcauto.com.cn";
var temp_sgIds = '';
$('a[name="installment_car"]').each(function(){
temp_sgIds = temp_sgIds + this.id.substr(16)+',';
});
var sgIds = temp_sgIds.substr(0,temp_sgIds.length-1);
$.getScript(JRROOT+"/interface/outer/auto/getProductList.jsp?regionId="+regionId+"&sgIds="+sgIds+"&callback=setFqProducts");
}
function setFqProducts(data) {
$('a[name="installment_car"]').each(function(i){
var product = data[i].products;
var sgid = data[i].sgId;
if(product.length > 0) {
var pd = product[0];
if(pd.monthlyPay >= 6000) {
$('#installment_car_'+sgid).html('首付'+pd.downPayment+'万提车&gt;&gt;');
} else {
$('#installment_car_'+sgid).html('月供'+pd.monthlyPay+'元起&gt;&gt;');
}
}
})
}
$(".colorul li").hover(function(){
$(this).addClass("hover");
},function(){
$(this).removeClass("hover");
});
//更多颜色
$(".morecolor").hover(function(){
$(this).addClass("on");
},function(){
$(this).removeClass("on");
});
</script>
