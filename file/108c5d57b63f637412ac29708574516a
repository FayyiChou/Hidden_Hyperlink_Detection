.scrollTimer=c.setTimeout(function(){if(!p.winLoaded&&Math.abs(f.scrollTop-l)>q){p.analysisImg()}l=f.scrollTop;q=b.clientHeight;var u=Math.floor(l/100),t=Math.floor(q/100);k.loadImgs(u,u+t)},n)},cross:function(y){var k=arguments,t=k.length;if(t<=1){return}for(var u=1;u<t;u++){var x=k[u][0]>k[u][1]?-1:1;while(k[u][0]!=k[u][1]){var v=k[u][0],w=k[u][1];y(v);k[u][0]+=x}}}};window.setTimeout(function(){p.analysisImg();p.attach()},0);for(var j=1;j<=20;j+=j){c.setTimeout(function(){if(!p.winLoaded){p.analysisImg()}},j*500)}r.add(c,"load",function(){p.winLoaded=true;p.analysisImg(true);p.parseHideImgs()});r.add(f,"click",function(){});c.lazy=p})(window);
