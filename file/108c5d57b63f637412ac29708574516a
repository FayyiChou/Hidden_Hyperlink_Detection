<script>
function showPopupXdj(param) {
//统计点击量
new Image().src = "http://count.pcauto.com.cn/count.php?channel=432&screen="+screen.width+"*"+screen.height+"&refer="+encodeURIComponent(document.referrer)+"&anticache="+new Date().getTime()+"&url="+encodeURIComponent(location.href)+"&from=event";
var fmXdjElm = document.getElementById('JfmXdj');
if (!fmXdjElm) return;
var priceRoot = 'http://price.pcauto.com.cn';
fmXdjElm.src = !param ? 'about:blank' : priceRoot + '/dealer/commorders/popup/order.jsp?' + param + '&countIds='; // countIds 监控统计事件 id
fmXdjElm.parentNode.style.display = !param ? 'none' : 'block';
}
</script>
