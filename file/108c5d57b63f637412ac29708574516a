<script>
function bindMouseEnter(obj, fn) {
if (obj.attachEvent) {
obj.attachEvent('onmouseenter', fn);
} else {
obj.addEventListener('mouseover', function(e) {
var rt = e.relatedTarget;
if (rt !== obj && !isContain(obj, rt)) {
fn.call(obj, e);
}
}, false);
}
}
function bindMouseLeave(obj, fn) {
if (obj.attachEvent) {
obj.attachEvent('onmouseleave', fn);
} else {
obj.addEventListener('mouseout', function(e) {
var rt = e.relatedTarget;
if (rt !== obj && !isContain(obj, rt)) {
fn.call(obj, e);
}
}, false);
}
}
function isContain(a, b) {
try {
return a.contains ? a != b && a.contains(b) : !!(a.compareDocumentPosition(b) & 16);
}catch (e) {}
}
var JTOOL = document.getElementById('Jtool');
bindMouseEnter(document.getElementById('JtoolTit'), function(){
if (/\btool-hov\b/.test(JTOOL.className)) return;
JTOOL.className += ' tool-hov';
});
bindMouseLeave(JTOOL, function(){
JTOOL.className = JTOOL.className.replace(/\s*\btool-hov\b\s*/, '');
})
function showAllTool () {
var tool = document.getElementById('Jtool');
tool.className += ' tool-hov';
}
function hideAllTool () {
var tool = document.getElementById('Jtool');
tool.className = tool.className.replace(' tool-hov', '');
}
function toggleAllTool() {
if (/\btool-hov\b/.test(JTOOL.className)) {
hideAllTool();
} else {
showAllTool();
}
}
// 图片下拉菜单
(function(){
var target = document.getElementById('j-navDrap-pic');
if(!target) return;
var	targetClassName = target.className;
target.onmouseover = function(){
this.className += ' subNav-on';
}
target.onmouseout = function(){
this.className = targetClassName;
}
})();
</script>
</div>
