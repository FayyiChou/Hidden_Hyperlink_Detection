<input autocomplete="off" name="q" id="ksWord" class="inpTxt placehold" type="text" value="" data-placeholder="请输入关键字">
<a onclick="setFormAction()" class="inpBtn">搜 索</a>
</form>
<script>
// 模拟 placeholder
var Placeholder=function(){var f="placehold";function g(j,i,h){if(j.addEventListener){j.addEventListener(i,h,false)}else{if(j.attachEvent){j.attachEvent("on"+i,h)}}}function d(h,i){i=i||e(h)||"";c(h,i);a(h)}function b(h){h.holding=true;h.className+=" "+f;h.value=e(h)}function a(h){if(h.value==""){b(h)}g(h,"focus",function(){if(h.holding){var i=" "+h.className+" ";h.value="";h.className=i.replace(" "+f+" ","")}});g(h,"blur",function(){if(h.value!=""){h.holding=false}else{b(h)}})}function e(h){return h.getAttribute("data-placeholder")}function c(h,i){h.setAttribute("data-placeholder",i)}return{make:d}}();
</script>
<script>
function setFormAction(){
var vals=document.getElementById('ksWord').value.replace(/^\s+|\s+$/g,'');
var _ajaxsearch=document.getElementById('ajaxsearch');
var _li=_ajaxsearch.getElementsByTagName('li');
var flag=true;
if (!vals ||vals=='请输入关键字' ||!_ajaxsearch ||(_li&&_li[0].className.indexOf('no_search')>-1)) {
return false;
}else{
var form = document.getElementById("mainSNSSearchFromId");
var _href='';
for(var i=0;i<_li.length;i++){
if(_li[i].className=='ajaxsearch-item hover' || _li[i].className=='ajaxsearch-item ajaxsearch-item_price hover'){
_href=_li[i].getElementsByTagName('a')[0].getAttribute('href');
flag = false;
break;
}
}
if(flag){
_href = _ajaxsearch.getElementsByTagName('a')[0].href;
}
form.action = _href;
form.submit();
}
}
</script>
<script>Placeholder.make(document.getElementById('ksWord'));</script>
