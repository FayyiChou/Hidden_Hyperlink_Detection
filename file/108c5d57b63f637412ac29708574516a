function clearSelect(inx){var sel=$$('cpBrand_'+inx);sel.selectedIndex=0;sel=$$('cpFirm_'+inx);sel.selectedIndex=0;sel.disabled=true;sel=$$('cpCar_'+inx);sel.selectedIndex=0;sel.disabled=true}
function addBySelect(ele){var val=ele.value,inx=ele.id.substr(6);$.getJSON("/price/api/v1/model/compare_bar/info?mid="+val,function(returnData){if(returnData&&returnData.status=='ok'){if(compareBar.data.choosePro.length==0){$("#compareFirst").find("li.fSelect").hide()}if(parseInt(compareBar.data.numbox.text())>=3&&compareBar.data.choosePro.length>=3){compareBar.data.compareFaker.find("li.fSelect").hide()}clearSelect(inx);compareBar.addComparePro(returnData.url,returnData.modelName,returnData.photoPath,returnData.priceRange,10,returnData.id)}})}
document.getElementById('cpCar_0').onchange	= document.getElementById('cpCar_1').onchange = function() {
addBySelect(this);
};
//开始对比
$(".cpVsBtn").click(function(){if(compareBar.data.choosePro==''){alert('请选择车型');return false;}$(this).attr("href", '/choose.jsp?mid='+compareBar.data.choosePro);});
$('#JtoolBar').find('.tab i').removeClass('on');
}
function toggleCompareBar() {
var barClass = $.cookie("barClass"),
$barOut = $("#compareBarOut"),
$JtBar = $('#JtoolBar');
if (barClass == 'show') {
$.cookie("barClass", "hide", {
path: '/',
expires: 1
})
} else if (barClass == 'hide') {
$.cookie("barClass", "show", {
path: '/',
expires: 1
})
} else {
$.cookie("barClass", "hide", {
path: '/',
expires: 1
})
}
if ($barOut.hasClass("hide") || $JtBar.hasClass("hide")) {
$barOut.removeClass("hide");
$JtBar.removeClass("hide");
var pros = $JtBar.find('.pro'), tabis = $JtBar.find('.tab i');
pros.eq(0).removeClass("hide");
pros.eq(1).addClass('hide');
tabis.eq(0).addClass('on');
tabis.eq(1).removeClass("on");
} else {
$barOut.addClass("hide")
}
}
</script>
