if(navigator.appName!='Microsoft Internet Explorer') {
window.constructor.prototype.__defineGetter__("event", function(){
var o = arguments.callee.caller;
var e;
while(o != null){
e = o.arguments[0];
if(e && (e.constructor == Event || e.constructor == MouseEvent)) return e;
o = o.caller;
}
return null;
});
}
function selectIndex() {
var serialGroupTag = "#product_a_" + "";
var mamuTag = "#brand_" + "";
var brandTag = "#pictext_a_" + "0";
if ( serialGroupTag != "#product_a_" ) {
$(serialGroupTag).addClass("index");
} else if ( mamuTag != "#brand_" ) {
$(mamuTag).addClass("index");
} else if ( brandTag != "#pictext_a_" ) {
$($(brandTag).parent()).find("a").addClass("index");
}
}
//字母表锚点功能
function getOffsetTop1(ele) {
var t = 0;
while(ele.id != "leftTree"){
t += ele.offsetTop;
ele = ele.offsetParent;
}
return t;
}
function treeButon1(tid) {
var mum = document.getElementById(tid);
if ( !mum ) return;
var tB = getOffsetTop1(mum);
var treeInfo = document.getElementById("leftTree");
if ( treeInfo ) {
treeInfo.scrollTop = tB;
}
}
function needJS(src,callback,fn){if(arguments.length==1){var js=document.createElement('script');js.src=src;document.getElementsByTagName('head')[0].appendChild(js)}else{if(typeof callback!='function')return;if(arguments.length==3&&typeof fn!='undefined'&&fn!=null&&fn!='')return callback(false);var scripts=window.__needJS__||(window.__needJS__=[]),script=scripts[src]||(scripts[src]={loaded:false,callbacks:[]});if(script.loaded){return callback(false)}else script.callbacks.push(callback);if(script.callbacks.length==1){var js=document.createElement('script');js.src=src;js.onreadystatechange=js.onload=function(){if(!js.readyState||js.readyState=='loaded'||js.readyState=='complete'){script.loaded=true;for(var i=0;i<script.callbacks.length;i++)script.callbacks[i](true)}};document.getElementsByTagName('head')[0].appendChild(js)}}}
var _curId = "";
var _curTypeId = "cn";
function getBrandList() {
if ( window["brandList_"+_curId] ) {
var d = document.getElementById("J_loading");
document.getElementById("J_loading").parentNode.removeChild(d);
document.getElementById("subTable"+_curId).innerHTML=window["brandList_"+_curId];
switchBrand(_curId);
}
}
var historyTdObj;
var historySubTableObj;
var historyStId  = '' != '' ? '' : 1;//当前页选中项
var historyCriId = '';
function switchBrand(stId) {
var old_class = 'index';
var tdObj = document.getElementById("pictree_" + stId);
historyTdObj = document.getElementById("pictree_" + historyStId);
var subTableObj = document.getElementById("subTable" + stId);
historySubTableObj = document.getElementById("subTable" + historyStId);
if(stId == historyStId) {
tdObj.className = (tdObj.className == 'openChild') ? 'closeChild' : 'openChild';
subTableObj.className = (subTableObj.className == 'show brand') ? 'hid ' : 'show brand';
} else {
tdObj.className = 'openChild';
subTableObj.className = 'show brand';
if(historyTdObj!=null)
historyTdObj.className = 'closeChild';
if(historySubTableObj!=null)
historySubTableObj.className = 'hid';
treeButon1("pictree_".concat(stId));
historyStId = stId;
if(historyCriId != '') {
var historyCriObj = document.getElementById(historyCriId);
historyCriObj.className = old_class;
historyCriId = '';
}
}
selectIndex();
}
function switchSmallTypeFrame(stId) {
if(!window["brandList_"+stId]){
_curId=stId;
var d=document.getElementById("pictext_a_"+stId);
if(!!d){
d.innerHTML+=('<span id="J_loading">数据加载中...</span>');
<!-- TODO 切换左树js路径 -->
needJS("http://price.pcauto.com.cn/index/js/5_5/treedata-"+_curTypeId+"-"+stId+".js",getBrandList);
}
} else {
switchBrand(stId);
}
}
/* 左树当前页列表展开 */
switchSmallTypeFrame("0");
$("#leftTree").find("em").click(function(e) {
var id = $($(this).parent()).next().attr("id").substring(10);
switchSmallTypeFrame(id);
return false;
});
