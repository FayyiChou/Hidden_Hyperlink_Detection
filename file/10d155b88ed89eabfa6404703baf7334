<script type="text/javascript">
function changedb(){
	var selectdb = cabe_form.tipo_cab.value;
	$("#busco").unbind();
	if (selectdb==7)
		var dburlse="/includes/auto/autocomplete_i.php";
	else
		if (selectdb==6)
			var dburlse="/includes/auto/autocomplete_p.php";
		else
			if (selectdb==5)
				var dburlse="/includes/auto/autocomplete_s.php";
			else
				if (selectdb==3)
					var dburlse="/includes/auto/autocomplete_l.php";
				else
					if (selectdb==4)
						var dburlse="/includes/auto/autocomplete_j.php";
					else
					if (selectdb==2)
						var dburlse="/includes/auto/autocomplete_c.php";
					else
					if (selectdb==8)
						var dburlse="/includes/auto/autocomplete_a.php";
					else
						var dburlse="/includes/auto/autocomplete.php";
	$().ready(function() {
		$("#busco").autocomplete(dburlse, {
			width: 363,
			matchContains: true,
			//mustMatch: true,
			//minChars: 0,
			//multiple: true,
			//highlight: false,
			//multipleSeparator: ",",
			selectFirst: false
		});
	});
}
</script>
