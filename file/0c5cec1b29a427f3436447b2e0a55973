<script type="text/javascript">
	function SetCookie(name,value) {//两个参数，一个是cookie的名子，一个是值
		var Days = 30; //此 cookie 将被保存 30 天
		var exp  = new Date();    //new Date("December 31, 9998");
		exp.setTime(exp.getTime() + Days*24*60*60*1000);
		document.cookie = name + "="+ escape (value) + ";expires=" + exp.toGMTString();
	}
	function SetCookie2(name,value) {//两个参数，一个是cookie的名子，一个是值
		document.cookie = name + "="+ escape (value) ;
	}
	function getCookie(name) {//取cookies函数

		var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
		if(arr != null) return unescape(arr[2]); return null;

	}
	function delCookie(name) {//删除cookie
		var exp = new Date();
		exp.setTime(exp.getTime() - 1);
		var cval=getCookie(name);
		if(cval!=null) document.cookie= name + "="+cval+";expires="+exp.toGMTString();
	}
	$(function(){
		if(getCookie('xiaoqi')=="closeclick"){
			$(".yd").hide();
		}else{
			$(".yd").show();
		}
		if(getCookie('zkpx')=="closeclick"){
			$(".zkpx").hide();
		}else{
			$(".zkpx").show();
		}
		$('.gb').click(function(){
			$(".yd").hide();
			$(".newqp").hide();
			$(".zkpx").hide();
			SetCookie ("xiaoqi", "closeclick");
			SetCookie2 ("zkpx", "closeclick");
		})
	})
</script>
<script type="text/javascript">
$(document).ready(function() {
	/*列表两侧广告位*/
	var url='';
	var channel='12';
	var location=1;
	var flagIds="2,3,4";
	var gameId="G33";
	var gtid="100003";
	var groupId = "G33P081";
	var serverId = "";
	var serverName='';
	var groupName='经典七区';
	if(gameId=='G10'&&groupId!=null&&groupId!=""&&typeof(groupId)!='undefined'&&serverId!=null&&serverId!=""&&typeof(serverId)!='undefined'&&gtid!=null&&gtid!=""&&typeof(gtid)!='undefined'&&gtid=='100001'){
		$.ajax({
			type:'post',
			url:'/rechargecenter/transfer1771ServerInfo.action',
			data:{gameid:'G10',groupId:$("#groupid").val(),serverId:$("#serverid").val()},
			async:false,
			dataType:'json',
			success:function(data) {
				if (data.success == 'T') {
					var a=data.groupid1771;
					var b=data.serverid1771;
					$.ajax({
						type:'post',
						url:'/shouhuo/getChuhuoGoodsList.action',
						data:{gameid:'G10',groupid:a,serverid:b},
						dataType:'json',
						async:false,
						success:function(data) {
							if (data.success == 'T') {
								var dataList=eval("("+data.list+")");//转换为json对象
								var num_onestr=parseFloat(1/dataList[0].unitPrice).toFixed(2);
								$("#shouid").hide();
								$(".out-gmoey").show();
								$(".out-quserv").html(groupName+"/"+serverName);
								$(".out-price").html("1元="+num_onestr+"万金");
								$(".out-price-sm").html("("+dataList[0].unitPrice+"元/万金)");
							}
						}
					});
				}
			}
		});
	}

	if(((gameId=='G12'&&gtid=='100001') || (gameId=='G10'&&gtid=='100072') ||(gameId=='G479'&&(gtid=='100001'||gtid=='100045')))&&groupId!=null&&groupId!=""&&typeof(groupId)!='undefined'&&serverId!=null&&serverId!=""&&typeof(serverId)!='undefined'){
		$.ajax({
			type:'post',
			url:'/procurement/getShuoDetail.action',
			data:{gameid:gameId,groupid:groupId,serverid:serverId,gtid:gtid},
			dataType:'json',
			async:false,
			success:function(data) {
				if (data.success == 'ok') {
					var dataList=data.data;//转换为json对象
					var num_onestr=parseFloat(1/dataList.price).toFixed(2);
					$(".out-gmoey").show();
					$("#shouid").hide();

					var gameName = dataList.gameName;
					if(gameId == "G10"){
						gameName = "DNF";
					}
					$(".out-tit").html(gameName+dataList.goodTypeName+"出货");

					$(".out-quserv").html(groupName+"/"+serverName);
					$(".out-price").html(dataList.title);
					var price100 =Digit.floor(dataList.price/100,6);
					$(".out-price-sm").html("("+price100+"元/"+dataList.unit+")");
					$("#chuhuourl").attr("href","http://shou.7881.com/shipment/orderPlace.htm?gameId="+gameId+"&goodType="+gtid+"&groupId="+groupId+"&serverId="+serverId+"");
				    if(gameId=='G479'){
						$(".open-gmoey").css("background","url(http://pic.7881.com/7881-2016/images/chztbg.png) no-repeat");
					}
					if(gameId=='G12'){
						$(".open-gmoey").css("background","url(http://pic.7881.com/7881-2016/images/chjwbg.png) no-repeat");
					}

				}
			}
		});
	}

	$(".floadv-box").find("span").click(function(){
	       $(this).parent(".floadv-box").hide();
	});

});
function isPositiveNum(s){//是否为正整数
	var re = /^[0-9]*[1-9][0-9]*$/ ;
	return re.test(s)
}
</script>

<script type="text/javascript">
	$(document).ready(function(e){
		/*游戏广告位商*/
		var substr="";
		var data ='[]';
		if(data!=null&&data!=""){
			var dyList =eval('('+data+')');
			for(var i = 0; i<dyList.length;i++){
				var title="";
				if(dyList[i].title.length>38){
					title=dyList[i].title.substring(0,38)+"....";
				}else{
					title=dyList[i].title;
				}
				if(dyList[i].gtid == '100106'){
					substr += '<li>'+
							'<div><i>'+
							'<a href="/goods-'+dyList[i].goodsid+'.html" target="_blank" title="'+dyList[i].title+'">'+title+'</a></i></div><p><b>'+dyList[i].price+'</b>元';
					substr += '</p><a href="/goods-buying-'+dyList[i].gameid+'-100106-'+dyList[i].goodsid+'.html">立即购买</a>'+  '</li>';
				}else if(dyList[i].gtid == '100131'){
					substr += '<li>'+
							'<div><i>'+
							'<a href="/goods-'+dyList[i].goodsid+'.html" target="_blank" title="'+dyList[i].title+'">'+title+'</a></i></div><p><b>'+dyList[i].price+'</b>元';
					substr += '</p><a href="/goods-buying-'+dyList[i].gameid+'-100131-'+dyList[i].goodsid+'.html">立即购买</a>'+  '</li>';
				}else if(dyList[i].gtid == '100137'){
					substr += '<li>'+
							'<div><i>'+
							'<a href="/goods-'+dyList[i].goodsid+'.html" target="_blank" title="'+dyList[i].title+'">'+title+'</a></i></div><p><b>'+dyList[i].price+'</b>元';
					substr += '</p><a href="/goods-buying-'+dyList[i].gameid+'-100137.html">立即购买</a>'+  '</li>';
				}else{
					substr += '<li>'+
							'<div><i>'+ '<a href="/goods-'+dyList[i].goodsid+'.html" target="_blank" title="'+dyList[i].title+'">'+title+'</a></i></div><p><b>'+dyList[i].price+'</b>元';
					substr += '</p><a href="/goods-buying-'+dyList[i].goodsid+'.html">立即购买</a>'+ '</li>';

				}
			}
			var str='<div class="agent-pay"><div class="newqp"><a class="gb" href="javascript:void(0)" onclick="closediv()">关闭</a></div>'+'<ul>'+ substr+ '</ul>' + '</div>';
			if(substr!=""){
				$("#advert").html(str);
			}
		}
	});
	function closediv(){
		$(".newqp").hide();
	}
</script>

