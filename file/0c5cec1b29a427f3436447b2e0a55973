<script type="text/javascript">
	function locatePage(index){
		sorttypeseo = $('#sorttype').val();
		window.location.href='/buy-'+gameidseo+(gameidseo=='G21'&&camp!=''?('a'+camp):'')+'-'+carrieridseo+'-'+gtidseo+'-'+groupidseo+'-'+serveridseo+'-'+index+'-'+evsseo+'-'+tradetypeseo+'-'+($('#sorttypeValue').val()==''?'0':$('#sorttypeValue').val())+'-'+depositseo+'-'+priceSeo+'-0-0-0-0'
		+'-'+istradeplaceSeo+'-'+ismetastasisSeo+'.html'+addition_url;
	}
	
	//8: backspace  46: delete
	//48-57/96-105: 0-9	
	function restrict_digit_input(el) {
		el.bind('contextmenu', function(){
			return false;
		});
		// 屏蔽输入法
		el.css('ime-mode', 'disabled');
		
		el.bind('keydown', function(e) {
	        var key = window.event ? e.keyCode : e.which;
	        return (isSpecialKey(key)) || ((isNumber(key) && !e.shiftKey));
	    });
	}
	
	function gotoPage() {
		var no = document.getElementById('pageNumber').value;
		if (!no) {
			return false;
		}
		no = parseInt(no, 10);
		if (no == 0 || no > parseInt('10', 10)) {
			alert('页码无效');
			return false;
		}
		locatePage(no - 1);
	}
</script>
