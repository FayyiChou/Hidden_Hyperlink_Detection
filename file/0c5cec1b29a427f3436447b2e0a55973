<script type="text/javascript">
    $(function(){
        /*冒险岛2*/
        var t='G33';
        if(t=='G3415'){
            givetime();
        }
    });

    //倒计时start
    var nowtimes;
    function givetime(){
        nowtimes=new Date();//当前服务器时间
        window.setTimeout("DownCount()",1000)
    }
    function DownCount(){
        nowtimes=Number(nowtimes)+1000;
        var ulArray=$("ul.goods_info");
        $.each(ulArray,function(i,j){
            if($(this).attr("tradetime")=='00:01-24:00'){
                //start 倒计时
                var theDay=new Date($(this).attr("paytime")).getTime()+(3600000*parseInt($(this).attr("billstock")));
                if(theDay<=nowtimes){
                    document.getElementById("daojishi_"+$(this).attr("goodsid")).innerHTML = "卖家正在收回账号";
                }else{
                    timechange(theDay,$(this).attr("goodsid"));
                }
            }
        });
        window.setTimeout("DownCount()",1000)
    }
    function timechange(theDay,goodsid){
        var theDays=new Date(theDay);
        var seconds = (theDays - nowtimes)/1000;
        var minutes = Math.floor(seconds/60);
        var hours = Math.floor(minutes/60);
        var days = Math.floor(hours/24);
        var CDay= days;
        var CHour= hours % 24;
        var CMinute= minutes % 60;
        var CSecond= seconds % 60;
        var CHour=CHour+CDay*24;
        if(CMinute<10)
        {
            CMinute="0"+CMinute;
        }
        if(CHour<10)
        {
            CHour="0"+CHour;
        }
        if(CSecond<10)
        {
            CSecond="0"+CSecond;
        }
        if(!isNaN(CHour)){
            document.getElementById("daojishi_"+goodsid).innerHTML = CHour + "小时 " + CMinute + "分钟后可再次出租";
        }else{
            document.getElementById("daojishi_"+goodsid).innerHTML = "卖家正在收回账号";
        }
    }
    //倒计时 end
</script>

	
	
	
	
	

