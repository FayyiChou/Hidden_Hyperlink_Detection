<script>
    function calcTicketPrice(){
        var unitPrice = $("#ticketForm").find("input[name='rechargeTicketBean.unitPrice']").val();
        if(unitPrice == undefined || unitPrice == ""){
            unitPrice = 0;
        }

        unitPrice = parseFloat(unitPrice);
        var purchaseNumber = $("#ticketForm").find("input[name='rechargeTicketBean.purchaseNumber']").val();
        if(purchaseNumber == undefined || purchaseNumber == ""){
            purchaseNumber = 0;
        }
        purchaseNumber = parseFloat(purchaseNumber);

        $("#ticketForm").find(".dfk_total_num").html(Digit.floor(purchaseNumber*unitPrice/100,2));
    }

    function submitTicketForm(){
        var purchaseNumber = $("#ticketForm").find("input[name='rechargeTicketBean.purchaseNumber']").val();
        if(purchaseNumber == "" || purchaseNumber < 1000){
            alert("购买数量不能低于1000点券");
            $(this).val(1000);
            calcTicketPrice();
            return ;
        }
        $("#ticketForm").submit();
    }

    $(document).ready(function(){
        /*点卡豆腐块 start*/
        var btg = $('.dropdown-menu li a');
        btg.bind('click', function(ev) {
            var tgt = ev.target;
            var _t = $(this);
            _t.parent().parent().siblings('button.b-current').html(tgt.innerHTML);
            $('.show-price').html("￥"+_t.attr("title"));
            $("#dk_id").val(_t.attr("id"));
            $(".show-price").attr("title",_t.attr("id"));
            _t.parent().parent().hide();
        });
        $('button.dropdown-toggle').bind('click', function() {
            btg.parent().parent().toggle();
        });
        $('.game-logo').css({
            'margin-top': (98 - $('.game-logo img').height()) / 2
        });
        /*点卡豆腐块 end*/
        /**DNF点券start**/
        if($("#ticketForm").length > 0){
            var unitPrice = $("#ticketForm").find("input[name='rechargeTicketBean.unitPrice']").val();
            if(unitPrice == undefined || unitPrice == ""){
                $("#ticketForm").remove();
                return;
            }
            $("#ticketForm").find(".byAmount").html(Digit.floor(100/parseFloat(unitPrice),2));
            $("#ticketForm").find(".byPrice").html(parseFloat(unitPrice));

            calcTicketPrice();
            $("#ticketForm").find("input[name='rechargeTicketBean.purchaseNumber']").keyup(function(e){
                calcTicketPrice();
            });

            $("#ticketForm").find("input[name='rechargeTicketBean.purchaseNumber']").blur(this,function(e){
                var num = $(this).val();
                if(num == "" || num < 1000){
                    alert("购买数量不能低于1000点券");
                    $(this).val(1000);
                    calcTicketPrice();
                    return ;
                }
            });

        }
        /**DNF点券end**/



        restrict_digit_input($('#dnfInputCount'));
        restrict_digit_input($('#dnfInputCount_1'));
        restrict_digit_input($('#dnfInputCount_2'));

        /*豆腐块,按数量*/
        $(".dfk_label_num").click(function(){
            var index = $(this).data("index");
            var inputcount = "";
            $("#dfk_tr_"+index+" .dfk_input_amount").val(inputcount).keyup();

            $("#dfk_tr_"+index+" .dfk_price_unit").hide();
            $("#dfk_tr_"+index+" .dfk_num_unit").show();

            /*优化*/
            $("#dfk_tr_"+index+" .dfk_input_unit").text($("#dfk_tr_"+index+" .dfk_input_unit").data("unit"));

            $("#dfk_tr_"+index+" .dfk_total_unit").text("元");
            $("#userChoose_"+index).val("shuliang");

            $("#dfk_tr_"+index+" .dfk_input_amount").css("width",function(){
                var a = parseInt($(".inputOuter").css("width").replace("px",""));
                var b = parseInt($("#dfk_tr_"+index+" .dfk_input_unit").css("width").replace("px",""));
                var c = 21;
                return a-b-c;
            });

        });
        /*豆腐块,按金额*/
        $(".dfk_label_price").click(function(){
            var index = $(this).data("index");
            var inputcount = 10;

            $("#dfk_tr_"+index+" .dfk_num_unit").hide();
            $("#dfk_tr_"+index+" .dfk_price_unit").show();

            $("#dfk_tr_"+index+" .dfk_input_amount").val(inputcount).keyup();

            /*优化*/
            $("#dfk_tr_"+index+" .dfk_input_unit").text("元");

            $("#dfk_tr_"+index+" .dfk_total_unit").text($("#dfk_tr_"+index+" .dfk_total_unit").data("unit"));

            $("#userChoose_"+index).val("jine");

            $("#dfk_tr_"+index+" .dfk_input_amount").css("width",function(){
                var a = parseInt($(".inputOuter").css("width").replace("px",""));
                var b = parseInt($("#dfk_tr_"+index+" .dfk_input_unit").css("width").replace("px",""));
                var c = 21;
                return a-b-c;
            });

        });

        /*dnf广告点击事件，如果广告地址是submit则点击提交表单*/
        $(".dnfyxb").click(function(){
            var url = $(this).data("url");
            if(url == "submitFun"){
                $("#dfkGoodsForm_other").attr("action","/rechargecenter/doPurchaseFrom7881ForB.action");
                $("#dfkGoodsForm_other").submit();
            }else{
                window.location.href=url;
            }
        });

        /*修改豆腐块广告位和图标*/
        if(gameId == "G10"){
            if(gtId == "100001"){
                $(".dnfyxb").css("background","url(http://pic.7881.com/7881/market/images/dnfyxb.png) 0 0 no-repeat").data("url","/b2b/buy-G10.html");
            }else if(gtId == "100072"){
                $(".dnfyxb").css("background","url(http://pic.7881.com/7881/market/images/dnfsyp.jpg) 0 0 no-repeat");
                $(".dnfyxb").data("url","submitFun");
                $(".tofu-icon.tf-03").removeClass("tf-03").addClass("tf-10");
            }
        }

        /*1771dnf游戏币区服转换*/
        if(gameId == "G10" &&　gtId == "100001"){
            var groupid_1771 = $('#hlk_groupid_1771').val();
            var serverid_1771 = $('#hlk_serverid_1771').val();

            if($("#is1771_1").val() == "true"){
                $("#dfk_tr_1 [name='goods1771PurchaseBean.groupid']").val(groupid_1771);
                $("#dfk_tr_1 [name='goods1771PurchaseBean.serverid']").val(serverid_1771);
            }

            if($("#is1771_2").val() == "true"){
                $("#dfk_tr_2 [name='goods1771PurchaseBean.groupid']").val(groupid_1771);
                $("#dfk_tr_2 [name='goods1771PurchaseBean.serverid']").val(serverid_1771);
            }

            if($("#is1771_3").val() == "true"){
                $("#dfk_tr_3 [name='goods1771PurchaseBean.groupid']").val(groupid_1771);
                $("#dfk_tr_3 [name='goods1771PurchaseBean.serverid']").val(serverid_1771);
            }
        }


        /*待优化,初始化单价，为保证单价换算比率正确，用同一种方式取得单价*/
        var price_1 = $("#dfk_tr_1 [name='goods1771PurchaseBean.price_7881']").val();
        var price_2 = $("#dfk_tr_2 [name='goods1771PurchaseBean.price_7881']").val();
        var price_3 = $("#dfk_tr_3 [name='goods1771PurchaseBean.price_7881']").val();

        $("#dfk_tr_1 .dfk_num_unit .strong_amountprice").html(parseFloat(price_1).toFixed(4));
        $("#dfk_tr_1 .dfk_num_unit .strong_amountprice").data("value",price_1);
        $("#dfk_tr_1 .dfk_price_unit .strong_amountprice").html(parseFloat(1/price_1).toFixed(2));
        $("#dfk_tr_1 .dfk_price_unit .strong_amountprice").data("value",parseFloat(1/price_1).toFixed(2));

        $("#dfk_tr_2 .dfk_num_unit .strong_amountprice").html(parseFloat(price_2).toFixed(4));
        $("#dfk_tr_2 .dfk_num_unit .strong_amountprice").data("value",price_2);
        $("#dfk_tr_2 .dfk_price_unit .strong_amountprice").html(parseFloat(1/price_2).toFixed(2));
        $("#dfk_tr_2 .dfk_price_unit .strong_amountprice").data("value",parseFloat(1/price_2).toFixed(2));

        $("#dfk_tr_3 .dfk_num_unit .strong_amountprice").html(parseFloat(price_3).toFixed(4));
        $("#dfk_tr_3 .dfk_num_unit .strong_amountprice").data("value",price_3);
        $("#dfk_tr_3 .dfk_price_unit .strong_amountprice").html(parseFloat(1/price_3).toFixed(2));
        $("#dfk_tr_3 .dfk_price_unit .strong_amountprice").data("value",parseFloat(1/price_3).toFixed(2));

        /*DNF 游戏币默认按金额，其余类别默认按数量*/
        if(gameId == "G10" && gtId == "100001"){
            /*第一次点击事件，修改checked，第二次点击触发正确的价格计算*/
            $(".dfk_label_price").click();
            $(".dfk_label_price").click();
        }else{
            $(".dfk_label_num").click();
        }

        /*征途2s点券、征途2s金币、dnf深渊票 只有按数量*/
        if((gameId == "G479" && (gtId == "100045" || gtId == "100001"))
                || (gameId == "G10" && gtId == "100072")){
            $(".dfk_label_price").hide();
            $(".dfk_label_num").css({"margin-left":"32px","margin-right":"32px"});/*居中展示*/
        }

    });
    var gameId = "G33";
    var gtId = "100003";

    function changeBuyAmount(index){
        var dfk_choose =$("input:radio[name='dfk_label_choose_"+index+"']:checked").val();
        if(dfk_choose == "1"){
            changeBuyAmount_num(index);
        }else if(dfk_choose == "2"){
            changeBuyAmount_price(index);
        }
    }

    function changeBuyAmount_num(index){
        var inputAmount = $("#dfk_tr_"+index+" .dfk_input_amount").val();

        if(gameId == "G10" && gtId == "100001"){
            var price = $("#dfk_tr_" + index + " .dfk_price_unit .strong_amountprice").data("value");
            var amountPrice = parseFloat(1/price * inputAmount).toFixed(2);
            $("#dfk_tr_"+index+" .dfk_total_num").text(amountPrice);
        }else{
            var price = $("#dfk_tr_" + index + " .dfk_num_unit .strong_amountprice").data("value");
            var amountPrice = parseFloat(price * inputAmount).toFixed(2);
            $("#dfk_tr_"+index+" .dfk_total_num").text(amountPrice);
        }
    }

    function changeBuyAmount_price(index){
        var price = $("#dfk_tr_" + index + " .dfk_price_unit .strong_amountprice").data("value");
        var inputAmount = $("#dfk_tr_"+index+" .dfk_input_amount").val();
        var amountPrice = parseFloat(price * inputAmount).toFixed(2);
        //dnf游戏币商品展示整数
        if(gameId == "G10" && gtId == "100001"){
            $("#dfk_tr_"+index+" .dfk_total_num").text(parseInt(amountPrice));
        }else{
            $("#dfk_tr_"+index+" .dfk_total_num").text(amountPrice);
        }
    }

    /*豆腐块,点击购买*/
    function submitDouFuKuai(i){
        isOKFlag();
        if(userid==null || userid==""){
            dialog_show('#tp-login');
            return  false;
        }
        var currentUserId="";
        var goodsUserId = $("#dfk_userid_"+i).val();
        if(currentUserId == goodsUserId){
            alert("您不能购买自己发布的商品");
            return false;
        }

        var inputAmount = $("#dfk_tr_"+i+" .dfk_input_amount").val();
        if(inputAmount.trim() == '' || inputAmount == 0){
            alert('请输入金币数量或金额');
            return false;
        }
        $("#userInput_"+i).val(inputAmount);

        var choose = $("input:radio[name='dfk_label_choose_"+i+"']:checked").val();
        var price;
        if(choose == "1"){
            price = $("#dfk_tr_" + i + " .dfk_price_unit .strong_amountprice").data("value");
        }else{
            price = $("#dfk_tr_" + i + " .dfk_price_unit .strong_amountprice").data("value");
        }
        if(price == '' || price == 0){
            alert('单价不能为空');
            return false;
        }

        var stock = $("#stock_" + i).val();
        if(stock=='' || stock == 0){
            alert('该区服下没有可售商品');
            return false;
        }

        var amountPrice = 0;
        if(choose == "1"){
            amountPrice = 1/price * inputAmount;
        }else{
            amountPrice = price * inputAmount;
        }

        if(choose == "1"){
            $("#userChoose"+i).val("shuliang");
            if(parseInt(stock) < parseInt(inputAmount) ){
                alert('当前库存不足！');
                return false;
            }

            if(amountPrice < 10){
                alert('购买金额不得低于10元！');
                return false;
            }

            $("#amountprice_"+i).val(amountPrice);
            $("#amountSum_"+i).val(inputAmount);
            $("#amountnum_"+i).val(inputAmount);

        }else if(choose == "2"){
            $("#userChoose_"+i).val("jine");
            if(inputAmount < 10){
                alert('购买金额不得低于10元！');
                return false;
            }

            if(stock != '' && stock != 0 && stock <  parseInt(inputAmount*price)){
                alert('当前库存不足！');
                return false;
            }

            $("#amountprice_"+i).val(inputAmount);
            $("#amountSum_"+i).val(amountPrice);
            $("#amountnum_"+i).val(parseInt(amountPrice));//bean是lang类型，不能传小数
        }

        if(gameId == "G10" && gtId == "100001" && $("#is1771_"+i).val() == "true"){
            $('#dfkGoodsForm_'+i).attr('action', '/rechargecenter/doPurchaseFrom1771.action');
        }else{
            $('#dfkGoodsForm_'+i).attr('action', '/rechargecenter/doPurchaseFrom7881ForB.action');
        }
        $('#dfkGoodsForm_'+i).submit();
    }

    /*点卡豆腐块点击跳转*/
    function openLink(gameid){
        var dk_id = $(".show-price").attr("title");
        if(dk_id == "351951234" || dk_id == "351951235"){
            window.location='/rechargecenter/toRechargeDK.action?gameid='+gameid+"&region="+$("#groupid").val()+"&goodsType=100145&goodsid="+dk_id;
        }else{
            window.location='/rechargecenter/toRechargeDK.action?gameid='+gameid+"&region="+$("#groupid").val()+"&goodsType="+$("#gtid").val()+"&goodsid="+dk_id;
        }
    }

</script>

	
	
	
	

	
	


<script type="text/javascript">
    if(false){
        $(".antu-jg input").click(function(){
            if($(this).parent().find(".antuen-icon").hasClass("jtsxx")){
    			$(this).parent().find(".alltime").hide();
    			$(this).parent().find(".antuen-icon").removeClass("jtsxx");
    		}else{
    			$(".alltime").hide();
    			$(".antuen-icon").removeClass("jtsxx");
    			$(this).parent().find(".alltime").show();
    			$(this).parent().find(".antuen-icon").addClass("jtsxx");
    		}
        });
        $(".antu-jg .antuen-icon").click(function(){
            if($(this).hasClass("jtsxx")){
    			$(this).parent().find(".alltime").hide();
    			$(this).parent().find(".antuen-icon").removeClass("jtsxx");
    		}else{
    			$(".alltime").hide();
    			$(".antuen-icon").removeClass("jtsxx");
    			$(this).parent().find(".alltime").show();
    			$(this).parent().find(".antuen-icon").addClass("jtsxx");
    		}
        });
        $(document).bind("click",function(e){
            var target = $(e.target);
            $(".antu-jg").each(function(){
                var display = $(this).find(".alltime").css('display');
                if(display=="block"){
                    if(target.closest(".antu-jg").length == 0){
                        $(".alltime").hide();
                        $(this).find(".antuen-icon").removeClass("jtsxx");
                    }
                }

            })
        });

        $("#dnfjintuantop1 .alltime a").click(function(){
            if(!$(this).parent().hasClass("disan")){
                var _timetu = $(this).text();
                $(this).parents("#dnfjintuantop1 .antu-jg").find("input").val(_timetu);
                $(this).parents("#dnfjintuantop1").find(".sigpre span").html($(this).attr("price"));
                $(this).parents("#dnfjintuantop1").find(".kucun p").html($(this).attr("stock"));
                $(this).parents("#dnfjintuantop1").find(".list_buy_btn").attr("href","/goods-buying-"+$(this).attr("goodsid")+".html");
                $(this).parents("#dnfjintuantop1").find(".alltime").hide();
                $(this).parents("#dnfjintuantop1").find(".antuen-icon").removeClass("jtsxx");

            }
        });

        $("#dnfjintuantop2 .alltime a").click(function(){
            if(!$(this).parent().hasClass("disan")){
                var _timetu = $(this).text();
                $(this).parents("#dnfjintuantop2 .antu-jg").find("input").val(_timetu);
                $(this).parents("#dnfjintuantop2").find(".sigpre span").html($(this).attr("price"));
                $(this).parents("#dnfjintuantop2").find(".kucun p").html($(this).attr("stock"));
                $(this).parents("#dnfjintuantop2").find(".list_buy_btn").attr("href","/goods-buying-"+$(this).attr("goodsid")+".html");
                $(this).parents("#dnfjintuantop2").find(".alltime").hide();
                $(this).parents("#dnfjintuantop2").find(".antuen-icon").removeClass("jtsxx");

            }
        });

        $("#dnfjintuantop3 .alltime a").click(function(){
            if(!$(this).parent().hasClass("disan")){
                var _timetu = $(this).text();
                $(this).parents("#dnfjintuantop3 .antu-jg").find("input").val(_timetu);
                $(this).parents("#dnfjintuantop3").find(".sigpre span").html($(this).attr("price"));
                $(this).parents("#dnfjintuantop3").find(".kucun p").html($(this).attr("stock"));
                $(this).parents("#dnfjintuantop3").find(".list_buy_btn").attr("href","/goods-buying-"+$(this).attr("goodsid")+".html");
                $(this).parents("#dnfjintuantop3").find(".alltime").hide();
                $(this).parents("#dnfjintuantop3").find(".antuen-icon").removeClass("jtsxx");

            }
        });

        if('false' == 'true'){
            $("#dnfjintuantop1").find("input").val('');
            $("#dnfjintuantop1").find(".sigpre span").html('');
            $("#dnfjintuantop1").find(".kucun p").html('');
            $("#dnfjintuantop1").find(".list_buy_btn").attr("href","/goods-buying-.html");
        }
        if('false' == 'true'){
            $("#dnfjintuantop2").find("input").val('');
            $("#dnfjintuantop2").find(".sigpre span").html('');
            $("#dnfjintuantop2").find(".kucun p").html('');
            $("#dnfjintuantop2").find(".list_buy_btn").attr("href","/goods-buying-.html");
        }
        if('false' == 'true'){
            $("#dnfjintuantop3").find("input").val('');
            $("#dnfjintuantop3").find(".sigpre span").html('');
            $("#dnfjintuantop3").find(".kucun p").html('');
            $("#dnfjintuantop3").find(".list_buy_btn").attr("href","/goods-buying-.html");
        }
    }
</script>

	
	

