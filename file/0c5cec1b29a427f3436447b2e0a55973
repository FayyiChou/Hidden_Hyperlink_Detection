<script>
	/*左悬浮*/
	$(".colse-btn").click(function(){
		$(".open-gmoey").fadeOut("slow");
		$(".close-gmoey").fadeIn("slow");
	});
	$(".close-gmoey").click(function(){
		$(".open-gmoey").fadeIn("slow");
		$(".close-gmoey").fadeOut("slow");
	});

	/*主搜*/
    var c_gameid="G33";
    var c_gamename="蜀门";
    var c_gtid="100003";
    var c_groupid="G33P081";
    var c_groupName="经典七区";
    var c_serverid="";
    var c_serverName="游戏服务器";
    if(c_gameid!=""){
        $(".g_name").attr("gameid",c_gameid);
        $(".g_name").html(c_gamename);
    }
    if(c_gtid!=""){
        $(".g_type").attr("gtid",c_gtid);
        $(".g_type").html("账号");
    }

    if(c_groupid!="" && c_groupName!=""){
        $(".g_area").attr("groupid",c_groupid);
        $(".g_area").html(c_groupName);
    }
    if(c_groupName!=""){
        $(".g_area").html(c_groupName);
    }

    if(c_serverid!=""&&c_serverName!=""){
        $(".g_service").attr("serverid",c_serverid);
        $(".g_service").html(c_serverName);
    }

    if(c_serverName!=""){
        $(".g_service").html(c_serverName);
    }
	$(".placeholder").find("input:first").val("");
	if($(".placeholder").find("input").val() !=""){
		$(".placeholder").find("span").hide();
	};
</script>
