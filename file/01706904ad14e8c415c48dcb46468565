        'bdPic':"http://img.xiami.net/images/artistlogo/17/14181897004117_2.jpg"
    };
    var laiwang_config = {
        title:'分享音乐人 陈粒 ',
        location:'i.xiami.com/chenlee',
        pic:'http://img.xiami.net/images/artistlogo/17/14181897004117_2.jpg'
    };
    document.getElementById("bdshell_js").src = "http://bdimg.share.baidu.com/static/js/shell_v2.js?cdnversion=" + Math.ceil(new Date()/3600000);
    function shareWeibo(){
        var url = 'title='+encodeURIComponent("分享音乐人 陈粒 ")+'&url='+encodeURIComponent(location.href)+'&appkey='+bds_config.snsKey.tsina+'&ralateUid='+bds_config.wbUid+'&pic='+encodeURIComponent(bds_config.bdPic)+'&searchPic=false';
        window.open('http://service.weibo.com/share/share.php?'+url, 'newwindow', 'height=440,width=430, top='+(screen.height-440)/2+',left='+(screen.width-430)/2+', toolbar=no, menubar=no, scrollbars=no,resizable=yes,location=no, status=no');
        return false;
    }
    function shareLaiwang(){
        var url = 'content='+ encodeURIComponent(laiwang_config.title) + encodeURIComponent('http://'+laiwang_config.location)+'&title='+encodeURIComponent(laiwang_config.title)+'&url='+encodeURIComponent('http://'+laiwang_config.location)+'&image='+encodeURI(laiwang_config.pic);
        window.open('http://www.laiwang.com/stream/shareToLaiwang.htm?'+url+'&'+new Date().getTime(), 'newwindow', 'height=360,width=588, top='+(screen.height-360)/2+',left='+(screen.width-588)/2+', toolbar=no, menubar=no, scrollbars=no,resizable=yes,location=no, status=no');
        return false;
    }

</script>
