<script>
$(function(){
	$('.mod_share_list .toggleSync ').on('click', "dd.toggleSyncBindClick", function(event) {
		event.stopPropagation();
        if(typeof(hasBindWeibo)=='undefined' || !hasBindWeibo){
            return false;
        }
        if($(this).children().children()[0].checked) {
            $(this).children().addClass('active');
        }else {
            $(this).children().removeClass('active');
        }
	});
});
</script>
