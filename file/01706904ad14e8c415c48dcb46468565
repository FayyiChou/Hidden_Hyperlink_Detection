<script>
    $(document).ready(function(){
        $.getJSON('/count/getplaycount',{'id':artist_id,'type':'artist'},function(data){
            $('#play_count_num').html(data.plays);
        });

        $(".bulletin_content_hand").click(function(){
            if(this.id == "show_more"){
                this.id = "show_less";
                this.innerHTML = "收缩";
                $("#bulletin_content").removeClass("brief").addClass("detail");
            }else{
                this.id = "show_more";
                this.innerHTML = "详细";
                $("#bulletin_content").removeClass("detail").addClass("brief");
            }
        });
        function loadsimilarartist(){
            $.ajax({
                url:'/ajax/similar-artists?id='+artist_id+'&c=6',
                type: 'GET',
                dataType: 'json',
                timeout: 10000,
                error: function(xml){
                    $('#artist_recommend').hide();
                },
                success: function(artists){
                    var content = '';
                    if(artists&&artists.length>0){
                        for(var i = 0;i<artists.length;i++){
                            var artist = artists[i];
                            var artist_id=artist.artist_id;
                            var artist_name=artist.name;
                            var artist_logo = artist.logo;
                            var count_likes = artist.count_likes;
                            var musicianhtml = artist.ismusician? '<i class="G_ico12 icon_v12" title="音乐人" ></i>' : '';
                            content +='<li>\
								<div class="artist_like_item">\
									<a href="http://www.xiami.com/artist/'+artist_id+'"><img src="'+(artist_logo?artist_logo.replace("_1.","_3."):'http://img.xiami.net/res/glory/img/group_55.jpg')+'" height="55" width="55" alt="'+artist_name+'"/></a>\
									<p><a href="http://www.xiami.com/artist/'+artist_id+'" title="'+artist_name+'">'+artist_name+'</a>&nbsp;'+musicianhtml+'</p>\
									<p>'+ count_likes +'人喜欢</p>\
								</div>\
							</li>';
                        }
                        $(content).appendTo('#artist_like ul');
                        $('#artist_like').show();
                    }else{
                        $('#artist_like').hide();
                    }
                }
            });
        }
        loadsimilarartist();
    });
</script>


</div>
