<script>
 	//
    $(function(){
        $('.select-js').change(function(){
            var val=$(this).children('option:selected').html()
            $(this).prev().html(val);
            var val2=$(this).parent().next().children('select').children('option:selected').html()
            $(this).parent().next('.group').children('label').html(val2)
			 var val3=$(this).parent().next().next().children('select').children('option:selected').html()
            $(this).parent().next('.group').next('.group').children('label').html(val3)
        })
        $('.select-js').parent().css({
            position:'relative',
			display: 'inline-block',
			'vertical-align': 'middle',
        })
		
		$('#btn2').click(function(){
			var url1="/job/index.html";	
			var url2="/company/index.html";
			var url3="/news/search.html";
			if($('#slt').val()==1){
				$('#aaa').attr('action',url1)
			}else if($('#slt').val()==2){
				$('#aaa').attr('action',url2)
			}else if($('#slt').val()==3){
				$('#aaa').attr('action',url3)
			}
		})
    })
</script>