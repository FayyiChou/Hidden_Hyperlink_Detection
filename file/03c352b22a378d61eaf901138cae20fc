        $(function () {           
            /*鼠标经过时，换图片*/
            $(".denglu").mouseover(function () {
                /*利用图像地址属性换图*/
                $(".denglu").attr("src", "images/index/denglu_032.png")
            })
            /*鼠标离开的时候换回原有图片*/
            $(".denglu").mouseout(function () {
                $(".denglu").attr("src", "images/index/denglu_03.png")
            })

            $(".top_a").mouseover(function () {
                /*利用图像地址属性换图*/
                $(".top_a").attr("src", "images/index/anniu_07.png")
            })
            /*鼠标离开的时候换回原有图片*/
            $(".top_a").mouseout(function () {
                $(".top_a").attr("src", "images/index/anniu_06.png")

            })

            $(".zhuce").mouseover(function () {
                /*利用图像地址属性换图*/
                $(".zhuce").attr("src", "images/index/anniu_202.png")
            })
            /*鼠标离开的时候换回原有图片*/
            $(".zhuce").mouseout(function () {
                $(".zhuce").attr("src", "images/index/anniu_20.png")

            })
            $("#flashImg").mouseover(function () {
                $("#flashImg").attr("src", "images/index/shipin_03_03.png");
            })
            $("#flashImg").mouseout(function () {
                $("#flashImg").attr("src", "images/index/shipin_03.png");
            })
            $('.top_x').click(function () {
                $('html,body').animate({ scrollTop: 0 }, 'slow');
            });
            $(".android").mouseover(function () {
                $(".divAndroid").show();
                $(".android").mouseout(function () {
                    $(".divAndroid").hide();
                })
            })
            $(".iphone").mouseover(function () {
                $(".divIphone").show();
                $(".iphone").mouseout(function () {
                    $(".divIphone").hide();
                })
            })
            $(".pc").mouseover(function () {
                $(".divPC").show();
                $(".pc").mouseout(function () {
                    $(".divPC").hide();
                })
            })
            $("#kinMaxShow").kinMaxShow();
        })

    </script>



