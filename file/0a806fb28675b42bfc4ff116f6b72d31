    function getArray(s) {
        if (!s && typeof s !== 'Array') return [];
        var newArray=[];
        var temp = {};
        var item = {};
        for (var i = 0 ; (item= s[i]) != null; i++) {
            if (!temp[item]) {
                newArray.push(item);
                temp[item] = true;
            }
        }
        return newArray;
    }
    var userIdStr = getArray(userIds);
    $.getScript(BASECONFIG.UCROOT + "/intf/user_info.jsp?callback=displaySnsUserInfo&ids=" + userIdStr);
    $.getScript("//bbs.pcbaby.com.cn/util/user_score.jsp?callback=displayForumUserInfo&ids=" + userIdStr);
    var visitedfid = $.cookie("visitedfid");
    if (visitedfid == null || visitedfid == '') {
        visitedfid = '1917';
    } else {
        var fids = visitedfid.split('D');
        for (var i = 0; i < fids.length; i++) {
            if (fids[i] == '1917') {
                fids.splice(i, 1); break;
            }
        }
        fids.unshift('1917');
        if (fids.length > 10) fids.pop()
        visitedfid = fids.join("D");
    }
    $.cookie('visitedfid', null);
    $.cookie('visitedfid', visitedfid, { expires: 100, domain: 'pcbaby.com.cn'});
    /**
     *  数据格式：checkDaren({"result":[{"accountId":9121778,"bolStar":true,"bolFans":false},{"accountId":1,"bolStar":false,"bolFans":false}],"state":1})
     */
    function checkDaren(data) {
        if (data["state"] != 1) {
            return;
        }
        var result = data["result"];
        for (var i = 0; i < result.length; i++) {
            $(".darenEm_"+result[i].accountId).hide();
            if (result[i].bolStar) {
                $(".darenEm_"+result[i].accountId).show();
            } else {
                $(".darenSpan_"+result[i].accountId).hide();
                continue;
            }
            // 追随按钮
            if (result[i].bolFans) {
                $(".unfollow_"+result[i].accountId).show();
                $(".follow_"+result[i].accountId).hide();
            } else {
                $(".unfollow_"+result[i].accountId).hide();
                $(".follow_"+result[i].accountId).show();
            }
        }
    }
    var darenUrl = BASECONFIG.UCROOT + "/intf/star/account_star_fans.jsp?callback=checkDaren&accountId=30648424&accountId=10680233&accountId=44435348&accountId=44435348&accountId=44435348&accountId=44751454&accountId=44751454&accountId=44590666&accountId=13326216&accountId=30648424&";
    //var darenUrl = BASECONFIG.UCROOT + "/intf/star/account_star_fans.jsp?refresh=1&callback=checkDaren&accountId=16096119&accountId=15720484&";
    $.getScript(darenUrl);
     
    function checkFans(data) {
        if (data["state"] != 1) {
            return;
        }
        var result = data["result"];
        var $fansCount, $darenType;
        for(var i = 0; i<result.length; i++) {
            $fansCount = $(".fansCount_"+result[i].accountId);
            $darenType = $(".darenType_"+result[i].accountId);
            //达人粉丝数与达人类型
            $fansCount.html(result[i].fansCount);
            $fansCount.attr("href", result[i].fansUrl);
            $darenType.html(result[i].typeName);
            $darenType.attr("href", result[i].typeUrl);
        }
    }
    var fansUrl = BASECONFIG.UCROOT + "/intf/star/account_star.jsp?callback=checkFans&accountId=30648424&accountId=10680233&accountId=44435348&accountId=44435348&accountId=44435348&accountId=44751454&accountId=44751454&accountId=44590666&accountId=13326216&accountId=30648424&";
    $.getScript(fansUrl);
