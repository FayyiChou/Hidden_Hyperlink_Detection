    function showRequest(formData,jqForm,options) {
        $.blockUI({message:''});
    }
    function showResponse(responseText,statusText) {
        $.unblockUI();
        eval("var result=" + responseText);
        if (result.status==0) {
            window.location.href = "//bbs.pcbaby.com.cn/redirect.do?url=" + encodeURIComponent(window.location.href);
        }else{
            showMessage(result.desc);
        }
    }

