        <script type="text/javascript">
            /*搜索*/
            var s_input = $("#q");
            var form_div = $("#s-form");
            form_div.click(function(e) {                
                form_div.addClass('form-active');
                s_input[0].value = '';
                e.stopPropagation();
            })
            $('body').click(function(){
                form_div.removeClass('form-active')      
            });
            // 其他论坛按钮的悬停展开
            function otherForumshover(){
                var otherforumsTimeer;
                var obj = $('#JotherForums');
                obj.bind('mouseenter',function() {
                    clearTimeout(otherforumsTimeer);
                    obj.addClass('otherforums-show');
                });
                obj.bind('mouseleave',function() {
                    otherforumsTimeer = setTimeout(function(){
                        obj.removeClass('otherforums-show');
                    },200)
                });
            }
            otherForumshover();
        </script>

