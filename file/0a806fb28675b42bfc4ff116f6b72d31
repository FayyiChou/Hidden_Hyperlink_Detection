    var autoloadimg  = $.cookie('autoloadimg');
    var imgList = document.getElementsByName("LazyloadImg");
    if (autoloadimg) {
        for(var i=0, len = imgList.length; i<len; i++) {
            var img = imgList[i];
            var src2 = img.getAttribute("src2");
            if (src2) {
            	if(src2.indexOf("http:")==0){
    				src2 = src2.replace("http:","") ;
    			}
                img.src = src2;
            }
        }
    }else{
        lazyload = new Lazyload({ src2: "src2", ImgList: imgList, defaultimage: "//www1.pconline.com.cn/2011/images/0810/bbsLoading.jpg" });
        lazyload.loaded();
    }
    function btnSurePress(o,e) {
        o.value=o.value.replace(/[^\d]/g,'');
        var pNum = o.value;
        if (pNum == null || pNum == '' || pNum == undefined || pNum == 0) {
            pNum = 1;
        }
        pNum = pNum> 6 ? 6 : pNum;
        if ((window.event && e.keyCode == 13) || (e.which && e.which == 13)) {
            window.location =switchPage(pNum);
        }
    }
    function switchPage(pageNo) {
        str = 'tid=4695838';
        str = str.replace(/(^|&)pageNo=-?\d+/ig, '');
        str = (str != '' ? str + '&' : '') + 'pageNo=' + pageNo;
        return "//bbs.pcbaby.com.cn/topic.do?" + str;
    }

    function changeStatus(sendMsgStatus) {
        var topicId = 4695838;
        $.ajax({
            url: "//bbs.pcbaby.com.cn/action/topic/change_sendmsg.ajax",
            type: "POST",
            data: {sendMsg : sendMsgStatus,tid:topicId},
            success : function(msg) {
                var data = eval("(" + msg + ")");
                if (data.status == 0) {
                    if (sendMsgStatus == 'true') {
                        $("#cancelSendMsg").show();
                        $("#noticeSendMsg").hide();
                        $.cookie("sendMsgUncheceked",null,{path:'/'}); // 接收回复通知
                    } else {
                        $("#cancelSendMsg").hide();
                        $("#noticeSendMsg").show();
                        $.cookie("sendMsgUncheceked",true,{path:'/'}); // 取消回复通知
                    }
                }
            }
        });
    }

    // 操作 回复通知的状态
    (function () {
    //发帖是否给我消息，记录点击cookie
    if ($.cookie("sendMsgUncheceked")) { //  取消回复通知
        $("#cancelSendMsg").hide();
        $("#noticeSendMsg").show();
    } else {
         $("#cancelSendMsg").show(); 
         $("#noticeSendMsg").hide();
    }
    })();

