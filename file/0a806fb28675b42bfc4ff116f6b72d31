};
;function setCookie(name,value,days,domain,path){ document.cookie = name + "=" + encodeURIComponent(value) +("; expires=" + new Date(new Date().getTime() + ((!!days)?days*1:1)*24*60*60*1000).toGMTString()) + ((!!path) ? "; path=" + path : "/") + ((!!domain) ? "; domain=" + domain : "domain="+window.location.host); }
;function getCookie(name){ return decodeURIComponent(document.cookie.replace(new RegExp(".*(?:^|; )"+name+"=([^;]*).*|.*"),"$1"));}
})();
</script>
