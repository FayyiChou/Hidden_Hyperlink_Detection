        function initCaptchaById(id) {
            var imgv = $("#imgv" + id);
            var src = imgv.attr("src");
            if (src == null || src == "") {
                changeCaptchaById(id);
            }
        }
        function changeCaptchaById(id) {
            var imgId = "imgv" + id;
            var imgv = $("#" + imgId);
        if (imgv.css("display") == "none") {
                imgv.show();
            }
            document.getElementById(imgId).src = '//captcha.pcbaby.com.cn/captcha/v.jpg?' + new Date().getTime();
        }
        
