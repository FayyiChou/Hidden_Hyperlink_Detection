    function del_topic(tid) {
        showMsgObj.html(showConfirmMsg);
        tb_confirm('删除自己的帖子会扣分,确定要删除此帖吗？',function() {
            $.post("//bbs.pcbaby.com.cn/action/topic/delete.do",{tid:tid},function(data) {
                eval("var delResult=" + data);
                if (delResult.status == 0) {
                    showMsgObj.html(showSuccessMsg.replace('[:MSG]',"删除成功"));
                    tb_show_2('','#TB_inline?inlineId=showMsg&width=340',false);
                    tb_fadeOut(3000);
                    setTimeout("tb_redirect_forum()",2500);
                }else{
                    showMsgObj.html(showErrorMsg.replace('[:MSG]',delResult.desc));
                    tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);
                }
            });
        });
    }

    function del_post(tid, pid) {
        showMsgObj.html(showConfirmMsg);
        tb_confirm('删除自己的帖子会扣分,确定要删除此帖吗？',function() {
            $.post("//bbs.pcbaby.com.cn/action/post/delete.do",{
                tid:tid,
                pid:pid
            },function(data) {
                eval("var delResult=" + data);
                if (delResult.status == 0) {
                    showMsgObj.html(showSuccessMsg.replace('[:MSG]',"删除成功"));
                    tb_show_2('','#TB_inline?inlineId=showMsg&width=340',false);
                    tb_fadeOut(3000);
                    var add = "";
                    if (delResult.tid == 0) {
                        setTimeout("tb_redirect_forum()",2500);
                    }else{
                        //setTimeout("tb_redirect()",2500);
                        setTimeout("tb_redirect(window.location.href)",2500);
                    }
                }else{
                    showMsgObj.html(showErrorMsg.replace('[:MSG]',delResult.desc));
                    tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);
                }
            });
        });
    }
    //投票
    
    var lastSelectedPid = 0;
    

    var barTotalLen = 150;
    function displaySnsUserInfo(data) {
        var accounts = data.accounts;
        for(var n in accounts) {
            var obj = accounts[n];
            var userId = obj.userId;
            // 亲子网特有
            $(".age_" + userId).text(0);
            if (obj.myBabys > 0) {
                $(".age_" + userId).text(obj.babyAge);
                $(".kids_" + userId).text(obj.babyName1);
            } else {
                $(".kids_" + userId).text(obj.hasBabyDesc);
            }
            if (obj.status > 0) {
                $(".user_info_" + userId + "_from").text(obj.domicile);
                $(".user_sns_rank_" + userId).text(obj.rankName);
                $(".user_sns_score_" + userId).text(obj.experience + "/" + obj.nextExperience);
                var levelLen = obj.experience*barTotalLen/obj.nextExperience;
                $(".user_bar_sns_" + userId).width(levelLen);
                $(".user_info_baby1_" + userId).text(obj.babyName1);
                $(".user_info_baby2_" + userId).text(obj.babyName2);
                if (obj.babyCounts > 1) {
                    $(".user_info_baby2_tr_" + userId).show();
                }
                
                if (obj.isPregnant == 1 && obj.myBabys == -1) {
                    $(".hymoIocn_"+userId).html('<a href="#"><div class="hymoIocn" title="PCbaby官方认证" style="cursor:pointer;" onclick="window.open(\'//bbs.pcbaby.com.cn/topic-907011.html\');"></div></a>');
                    $(".score_"+userId).after("<br/>宝宝年龄：<em class=\"babyAge_30648424\">" + obj.hasBabyDesc + "</em>");
                }
            }
        }
    }

    function displayForumUserInfo(data) {
        for(var n in data) {
            var obj = data[n];
            var userId = obj.userId;
            $(".user_forum_rank_" + userId).text(obj.level + "级");
            $(".user_forum_score_" + userId).text(obj.contribution + "/" + obj.next);
            var levelLen = (obj.next === 0 || obj.next === '') ?
                0 : obj.contribution*barTotalLen/obj.next;
            $(".user_bar_forum_" + userId).width(levelLen);
        }
    }

    // 收缩评分框
    function switchOpera(obj,pId) {
    if ($(obj).hasClass("ico_sq")) {
        $(obj).removeClass("ico_sq").addClass("ico_zk");
        $("#"+pId).find(".tb").eq(0).addClass("hidden");
    }else{
        $(obj).removeClass("ico_zk").addClass("ico_sq");
        $("#"+pId).find(".tb").eq(0).removeClass("hidden");
    }
}

