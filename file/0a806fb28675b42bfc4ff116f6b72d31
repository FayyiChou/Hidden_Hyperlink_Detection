<script type="text/javascript">
    function recommend(fid) {
        $.post("//bbs.pcbaby.com.cn/plugin/recommend/userRecommend.do?tid=4695838&fid="+fid, null, function(data) {
            eval("var result = " + data);
            if (result.status == 0) {
                showMessage("推荐成功");
            } else {
                showMessage(result.desc);
            }
        });
    }   


    // 7.2新增-收藏
    function addFav(tid) {
        var userId = 1 * '0';
        if (!userId) {
            var returnURL = encodeURIComponent("//bbs.pcbaby.com.cn/topic-4695838.html");
            var tips = '您没有登录<div style="margin-top:10px;"><a href="' + BASECONFIG.UCROOT + '/login.jsp?return='+returnURL+'" style="color:#336797;" target="_blank">去登录/注册</a></div>';
            if (jQuery.browser.msie6) {
                tips = "您还没有登录,请先登录";
            }
            showMessage(tips);
        } else {
            var hasAdd = $("#addFav_" + tid).val();
            if (hasAdd == 0) {
                var jsonStr= eval("({withCookie:true,windowname:true,type:5,id:tid})");
                var collectURL = BASECONFIG.UCROOT + '/collection/intf/collection/collect.jsp'; //线上
                
                $.post(collectURL, jsonStr, function(data) {
                    eval("var result = " + data);
                    if (result.code == 1) {
                        $("#addFav_" + tid).val(1);
                    }
                    showMessage(result.msg);
                });
            } else {
                showMessage("重复收藏");
            }
        }
    }
    
 </script>
