<script>
Topic.toReplyForm2 = function (replyIdCls, topicId) {
	var replyId = parseInt(replyIdCls);
	var $forum = $("#form" + replyId);
	var isTopPost = replyId != replyIdCls;
	var $boxReply = $("#boxReply" + replyIdCls);
    if (!$forum[0]) { //如果还没添加，则插入form
        $.post("action/post/toReplyForm.ajax", {
            "pid": replyId,
            "tid": Topic.topicId
        }, function (data) {
            data = data.replace(/^\s*/, '');
            try {
                eval("var json=" + data);
                if (json) {
                    if (json.status == -1) {
                        var returnURL = encodeURIComponent(Topic.root + "/topic-" + Topic.topicId + ".html");
                        var rtnJson = (new Function("", "return " + json.desc))();
                        if (rtnJson.status == 7 || jQuery.browser.msie6) {
                            showMessage(rtnJson.desc);
                        } else {
                        	var  http_temp = "" ;
                            if(BASECONFIG.UCROOT.indexOf("//")==0){
                            	http_temp = "http:"+BASECONFIG.UCROOT ;                            	
                            }
                        	showMessage(rtnJson.desc + '<div style="margin-top:10px;"><a href='+http_temp+ '/login.jsp?return=' + returnURL + '" style="color:#336797;" target="_blank">去登录/注册</a></div>');
                        }
                        return;
                    }
                }
            } catch (e) {

            }

            $boxReply.append(data);
            if ($boxReply.is(":hidden")) {
                Topic.toggleReplyTable(replyIdCls, function() {
                	$("#msg" + replyId).focus();
                });
            } else {
                Topic.toggleReplyTable(replyIdCls);
            }
            //调整“箭头的位置”
            var left = $("#fastReply" + replyIdCls).offset().left;
            var leftWidth = document.body.clientWidth > 1000 ? ((document.body.clientWidth - 1000) / 2) : 0
            $("#ins" + replyId).css("left", left - leftWidth - 157);

        });
    } else {
    	if (!$boxReply.find('>#form' + replyId)[0]) { // 同个页面存在两个相同post时（置顶和正常），共用form
    		var $boxReply2 = $("#boxReply" + replyId + (isTopPost ? '' : '_top'));
    		if (!$boxReply2.is(':hidden')) {
    			$boxReply2.hide();
    		}
    		$forum.parent().children().appendTo($boxReply);
			Topic.toggleReplyTable(replyIdCls, function() {
				$("#msg" + replyId).focus();
			});
    	} else {
    		if ($boxReply.is(":hidden")) {
                Topic.toggleReplyTable(replyIdCls, function() {
                	$("#msg" + replyId).focus();
                });
            } else {
            	Topic.toggleReplyTable(replyIdCls);
            }
    	}
    }
}

</script>


