        location.href="//bbs.pcbaby.com.cn/post-" + pid + ".html";
    }
    //自动加载图片
    function clickAutoLoadImg(on) {
        if (on) {
            $.cookie('autoloadimg','1',{path:'/',expires:30});
            var selected = $('#chkAutoLoadImg').attr('checked');
            if (!selected) {
                $('#chkAutoLoadImg').attr('checked',true);
            }
        }else{
            $.cookie('autoloadimg','',{path:'/',expires:30});
        }
    }
    (function() {
        var autoloadimg = !!$.cookie('autoloadimg');
        $('#chkAutoLoadImg').attr('checked',autoloadimg);
        
        //重置引用部分大图的尺寸
        imgDementionLimit($('p.cont img,div.imgQuoteWrap img'));
    })();

    /*置顶回复*/
    function setPostTop(ele, tid, pid) {
        var newStatus = parseInt($(ele).attr('topstatus')) > 0 ? 0 : 1;
        var tipMsg = newStatus > 0 ? '确认置顶本回帖（最多置顶5个）？' : '确认取消置顶本回帖？';
        showMsgObj.html(showConfirmMsg);
        tb_confirm(tipMsg, function () {
            $.post('//bbs.pcbaby.com.cn/action/post/stick.do', {
                tid : tid,
                pid : pid,
                type : newStatus
               }, function(data) {
                eval("var result = " + data);
                if (result.status == 0) {
                    location.reload();
                } else {
                    showMessage(result.desc);
                }
            });
        });
    }
