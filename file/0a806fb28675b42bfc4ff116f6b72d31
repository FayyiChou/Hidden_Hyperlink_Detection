<script type="text/javascript">
	if(window.location.href.indexOf("snow=true")>0){
		SNOW.init() ;
	}
	

    var topicDefaultContent = $("#fast_post_title").val();
    var showMsgObj = $("#showMsg");
    var fastPostFlag = false;
    var sendingFlag = false;
    var advancePost = false;
    var postTextArea;
    function showMessage(message){
        showMsgObj.html(showErrorMsg.replace('[:MSG]',message));
        setTimeout(function(){tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);},50);
    }

    var editorid = 'f';
    var editorcurrentheight = 103;
    var editorminheight = 103;
    var editormaxheight = 103;
    var textobj = textobj || $$(editorid + '_textarea');
    ubb.editor.newEditor(1, ubb.bbcode.bbcode2html(textobj.value));
    ubb.editor.setCaretAtEnd();

    $("#fast_post_form").ajaxForm({beforeSubmit:showFastRequest,success:showFastResponse});
    var iframe = document.getElementById("f_iframe").contentWindow.document.body;

 
    $(document).ready(function() {
        var rapitSubmit = !!$.cookie('rapitSubmit');
        $('#rapitSubmit').attr('checked', rapitSubmit);
    });


    function setCheckboxCookie() {
        var value = "";
        var obj;
        obj = $("#rapitSubmit");
        if (obj.attr('checked')) {
            value = "true";
        }
        $.cookie("rapitSubmit", value, {path:'/',expires: 30});
    }

    function bindEvent() {
        sendingFlag = false;
        
            $("#sendBtn").attr("src", "//bbs.pcbaby.com.cn/images/kids/btn_rapitreply.png");
        
        $("#sendA").bind("click", fastpost);
        _attachEvent(iframe, "keydown", postWithEvent);
        if (document.getElementById("fast_post_title")) {
            $("#fast_post_title").bind("keypress",postWithEvent);
        }
        if (document.getElementById("fast_post_captcha")) {
            $("#fast_post_captcha").bind("keypress",postWithEvent);
        }
    }
    bindEvent ();
    function unbindEvent() {
        sendingFlag = true;
        if (!advancePost) {
            $("#sendBtn").attr("src", $("#sendingBtn").attr("src") === "" ? "//bbs.pcbaby.com.cn/images/kids/btn_sending.png" : $("#sendingBtn").attr("src"));
        }
        $("#sendA").unbind("click", fastpost);
        _detachEvent(iframe, "keydown", postWithEvent);
        if (document.getElementById("fast_post_title")) {
            $("#fast_post_title").unbind("keypress",postWithEvent);
        }
        if (document.getElementById("fast_post_captcha")) {
            $("#fast_post_title").unbind("keypress",postWithEvent);
        }
    }
    function postWithEvent(e) {
        if(e.ctrlKey && e.keyCode == 13 || e.keyCode == 10){
            _detachEvent(iframe, "keydown", postWithEvent);
            fastpost();
        }
     }
    function fastpost(){
        if (!sendingFlag) {
            postTextArea = iframe;
            unbindEvent();
            setValue ();
            $("#fast_post_form").submit();
        }
    }
    function setValue () {
        fastPostFlag = true;
        $("#message_hidden").val(ubb.editor.getEditorData());
        if (document.getElementById("sendMsg")) {
            $("#sendMsg_hidden").val(($("#sendMsg").attr("checked") === "checked" || $("#sendMsg").attr("checked")) ? "true" : "false");
        }
        if (document.getElementById("fast_post_title") && document.getElementById("fast_post_title").value != topicDefaultContent) {
            $("#title_hidden").val($("#fast_post_title").val());
        }
        if (document.getElementById("fast_post_captcha")) {
            $("#captcha").val($("#fast_post_captcha").val());
        }
    }
    function showFastRequest(formData,jqForm,options){
        if (document.getElementById("type_hidden")) {
            if($("#type_hidden").val() === ""){
                bindEvent();
                showMessage("请选择主题分类！");
                return false;
            }
        }
        
        var formValue =  $("#message_hidden").val().replace(/(^\s*)/g,'');
        var tip = $("#topicFlag").val() == "true" ? "发帖内容" : "回复内容";
        if(formValue === ""){
            bindEvent();
            showMessage(tip + "必须填写！");
            return false;
        } else {
            if(tip == '回复内容'){
                var quoteReg = /(\[quote\].*?\[\/quote\])+/i;
                if($.trim(formValue.replace(quoteReg, '')) == ''){
                    bindEvent();
                    showMessage("不允许全部为引用内容！");
                    return false;
                }
            }
            var minLen = $("#minContentLength").val();
            var maxLen = $("#maxContentLength").val();
            if (formValue.length < minLen) {
                bindEvent();
                showMessage(tip + "长度必须大于"+minLen);
                return false;
            }
            if (formValue.length > maxLen) {
                bindEvent();
                showMessage(tip + "长度必须小于"+maxLen);
                return false;
            }
        }
         
        var isUploading = $("#isUploading").val();
        if (isUploading == "1") {
            bindEvent();
            showMessage("图片正在上传，请稍后！");
            return false;
        }
         
        if (document.getElementById("fast_post_captcha")) {
            var value = $("#fast_post_captcha").val();
            if (value == "" || value == "请输入验证码") {
                bindEvent();
                showMessage("请输入验证码！");
                $("#fast_post_captcha").focus();
                return false;
            }
        }
    }
    
    function showFastResponse(responseText,statusText) {
        var fastResult = new Function("return ("+responseText+");")();
        if (fastResult.status == 0) {
            
                    var rapitSubmit = $("#rapitSubmit").attr('checked');
                    if (rapitSubmit) {
                        $.cookie("topic_action", "reply");
                        var redirectUrl = "//bbs.pcbaby.com.cn/topic-" + fastResult.tid + "--1.html#post_" + fastResult.pid;
                        redirectUrl = encodeURIComponent(window.location.protocol+redirectUrl);
                        window.location.href="//bbs.pcbaby.com.cn/redirect.do?url=" + redirectUrl;
                    } else {
                        $.cookie("topic_action", "reply");
                        var request = $.ajax({
                            url: "//bbs.pcbaby.com.cn/reply_floor.do",
                            type: "POST",
                            data: {tid:fastResult.tid,pid:fastResult.pid},
                            success : function(msg){
                                editdoc.body.innerHTML = "";
                                $("#post_list").append(msg);
                                $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
                                $body.animate({scrollTop: $("#post_"+fastResult.pid).offset().top}, 500);
                                bindEvent();
                                //调用sns接口(计算小家打工赚钱的发帖任务)
                                postSNSURL('0','4695838','起床晒太阳咯！')
                                //指定某个活动帖下奖杯雨
                                if((msg.indexOf("香港领先")!=-1||msg.indexOf("欧洲制造")!=-1||msg.indexOf("欧盟香港双重监管")!=-1) && fastResult.tid=='4368442'){
                                	SNOW.init() ;
                                }
                            }
                        });
                    }
                
            $('#img_hidden').val('');    // 清除图片
            $('#swfwrapubb>.close').click();
        } else {
            bindEvent();
            showMessage(fastResult.desc);
        }
        postTextArea = null;
    }
    

   
   function login(type) {
	   var returnURL = encodeURIComponent(location.href);
       if (type === 1) {
           location.href = "http://my.pcbaby.com.cn/passport/opens/authorize.jsp?type=qzone_baby&return=" + returnURL;
       } else if (type === 2) {
           location.href = "http://my.pcbaby.com.cn/passport/opens/authorize.jsp?type=sina_baby&return=" + returnURL;
       } else if (type === 3) {
    	   location.href ="http://my.pcbaby.com.cn/passport/opens/authorize.jsp?type=weixin_baby&return=" + returnURL;
       } else if (type === 4) {
           location.href ="http://my.pcbaby.com.cn/login.jsp?return=" + location.href;
       } else if (type === 5) {
           location.href ="http://my.pcbaby.com.cn/login.jsp?tab=1&return=" + location.href;
       } else{
    	   location.href ="http://my.pcbaby.com.cn/login.jsp?return=" + location.href;
       }
	}
   
    function toAdvancePost (id) {
        var postUrl = "";
        
            var postUrl = "//bbs.pcbaby.com.cn/reply.do?fid=1917&tid=4695838&action=reply&advance=true";
            
        var imgUbb = $('#img_hidden')[0] ? $('#img_hidden').val() : '';
        if($.trim(imgUbb) != ''){
            imgUbb = '[img]' + imgUbb + '[/img]';
        }
        $.cookie("adMessage", ubb.editor.getEditorData() + imgUbb, {path:'/',expires:1});
        if (document.getElementById("fast_post_title")) {
            if($("#fast_post_title").val() != topicDefaultContent){
                $.cookie("adTitle", $("#fast_post_title").val(), {path:'/',expires:1});
            } else {
                 // path与有值时的path必须保持一致，以免造成同时存在2个相同key的cookie
                $.cookie("adTitle", "", {path:'/',expires:1});
            }
        }
        location.href = postUrl;
    }

    function initCaptcha() {
        var imgv = $("#imgv");
        var src = imgv.attr("src");
        if (src == null || src == "") {
            changeCaptcha();
        }
    }
    function changeCaptcha() {
        var imgv = $("#imgv");
        if (imgv.css("display") == "none") {
            imgv.show();
        }
        document.getElementById("imgv").src = '//captcha.pcbaby.com.cn/captcha/v.jpg?' + new Date().getTime();
    }
    
    
      //快速回贴分类选择
    $(".fastpost_type").hover(function() {
        $(this).find("dt").addClass("hover");
        //$(this).find("dd").stop(true,true).fadeIn();
        $(this).find("dd").show();
    }, function() {
        $(this).find("dt").removeClass("hover");
        //$(this).find("dd").stop(true,true).hide();
        $(this).find("dd").hide();
    });
    $(".fastpost_type dd a").click(function() {
        $(this).addClass("on").siblings().removeClass("on");
        var tem_html = $(this).text();
        $(".fastpost_type dt a").text(tem_html);
        $(".fastpost_type dt").removeClass("hover");
        $(".fastpost_type dd").hide();
        $(this).blur();
        $("#type_hidden").val(tem_html);
        return false;
    });
    //更多版主下拉
    $(".forumuser_list").hover(function() {
        $(this).removeClass("forumuser_list").addClass("forumuser_list_h");
    }, function() {
        $(this).removeClass("forumuser_list_h").addClass("forumuser_list");
    });
</script>
<script type="text/javascript" src="//bbs.pcbaby.com.cn/js/upload/swfupload.js"></script>
<script type="text/javascript">
//快速发帖上传
var pageGlobal ={};
    var keepSrc = pageGlobal.uploadKeepSource?pageGlobal.uploadKeepSource:0;
    var readExif = pageGlobal.readExif?pageGlobal.readExif:0;
    var noWater = pageGlobal.noWater ? pageGlobal.noWater:0;
    var resizedImgCondition = "512KB";
    var command;
    var swfBtn = {
        id: 'spanButtonPlaceholder',
        width: 152,
        height: 34
    }
    var upload_url;

    if (keepSrc == 'true') {
        keepSrc = 'yes';
        resizedImgCondition = "5120KB";
    } else {
         keepSrc = 'no';
    }

    if (readExif == 'true') {
        readExif = 'yes';
    } else {
        readExif = 'no';
    }

    pageGlobal.genSwf = function(swf) {
        // pc.getCookie(pageGlobal.sessionname);
        // params[pageGlobal.sessionname] = 'B1C48BF1E2FDB06D6A85D5C7C4E9C8010033631E8E299B26'
        //params[pageGlobal.sessionname] = pc.getCookie(pageGlobal.sessionname)
        return new SWFUpload({
            // Backend Settings
            upload_url:"http://upc.pcbaby.com.cn/upload_quick.jsp?command=6004&command=6005&command=6006",
            post_params:{
                "application":"kidsbbs6",
                "keepSrc":keepSrc,
                "command":(noWater ? 5 : (0 || 4)),
                "common_session_id":$.cookie("common_session_id") ? $.cookie("common_session_id") : ""
            },
            // File Upload Settings
            file_size_limit : "5MB",    // 2MB
            file_types : "*.jpg;*.gif;*.jpeg;*.png;",
            file_types_description : "All Files",
            file_upload_limit : "0",
            use_query_string : true,
            // Event Handler Settings - these functions as defined in Handlers.js
            //  The handlers are not part of SWFUpload but are part of my website and control how
            //  my website reacts to the SWFUpload events.
            swfupload_preload_handler: preLoad,
            swfupload_load_failed_handler: loadFailed,

            file_queue_error_handler : fileQueueError,
            file_dialog_complete_handler : fileDialogComplete,

            upload_progress_handler : uploadProgress,
            upload_error_handler : uploadError,
            upload_success_handler : uploadSuccess,
            upload_complete_handler : uploadComplete,

            // Button Settings
            button_placeholder_id : swf['id'],
            button_width: swf['width'],
            button_height: swf['height'],
            button_window_mode:SWFUpload.WINDOW_MODE.TRANSPARENT,
            button_cursor: SWFUpload.CURSOR.HAND,

            // Flash Settings
            flash_url : "//bbs.pcbaby.com.cn/flash/swfupload.swf",
            flash9_url : "//bbs.pcbaby.com.cn/flash/swfupload_fp9.swf",
            custom_settings : {
                postId:swf['postId'],
                upload_target : swf['targetId'],
                resizedImgWidth : 1000,
                resizedImgHeight : 750,
                resizedImgQuality : 100,
                resizedImgCondition : resizedImgCondition
            },
            // Debug Settings
            debug: false
        })    
    }
    function uploadProgress(file, bytesLoaded) {  
        $("#isUploading").val("1");
        var placeholder = $('#swfwrap'+this.settings.custom_settings.upload_target); 
        try {
            if (placeholder[0]) {
                placeholder.show();  
                if (!($('img',placeholder).length > 0)) {
                    $('.swf-wrap',placeholder).html($('.swf-wrap',placeholder).html()+'<span class="loading"><img src="//www1.pconline.com.cn/2014/bbs/images/uploading.jpg" alt="" /><span>正在上传</span></span>');
                } else {
                    var contentImgHtml = '<span class="loading"><img src="//www1.pconline.com.cn/2014/bbs/images/uploading.jpg" alt="" /><span>正在上传</span></span>';
                    var contentImg = $(contentImgHtml)[0];
                    placeholder[0].replaceChild(contentImg,placeholder[0].lastChild);
                }       
            }
            document.getElementById("uploadednum").innerHTML = picnumindex;
            var percent = Math.ceil((bytesLoaded / file.size) * 100)<=100? Math.ceil((bytesLoaded / file.size) * 100):100;
            if (file.uploadtype === SWFUpload.UPLOAD_TYPE.RESIZED) {
                percent = 30;
            }
            var progress = new FileProgress(file,  this.customSettings.upload_target);
            progress.setProgress(percent);
        } catch (ex) {
            this.debug(ex);
        }
    }
    function uploadSuccess(file, serverData) {
        var placeholder = $('#swfwrap'+this.settings.custom_settings.upload_target); 
        $("#isUploading").val("0");
        try {       
            var data = eval('(' + serverData + ')');
            switch (data.retCode) {
                case 0:
                    //pick src_img
                    var src_img = data.files[data.files.length-1];
                    for(var i = 0; i < data.files.length; i++){
                        if (data.files[i].isorg == 1) {
                            src_img = data.files[i];
                        }
                    }

                    //pick img used by bbs
                    var need;
                    for(var i = 0; i < data.files.length; i++){
                        if(data.files[i].url.indexOf("1024x1024") != -1){
                            need = data.files[i];
                        }
                    }
                    if (!!need) {
                        data.files = [need];
                    } else {
                        if (data.files.length > 1) {
                            data.files.pop();//remove srcimg
                        }
                        while (data.files.length > 1) {//remove others
                            data.files.shift();
                        }
                    }
                    //go on                

                    if (placeholder.length > 0) {
                    	if(src_img.url.indexOf("http:")==0){   //改变成相对协议。2016-11-10
                    		src_img.url = src_img.url.replace("http:","") ; 
                    	}
                        var contentImgHtml = '<div><img src="'+src_img.url+'" width="120" height="90" alt=""><p>图片已上传至正文末尾</p></div>';
                        var contentImg = $(contentImgHtml)[0];
                        placeholder[0].replaceChild(contentImg,placeholder[0].lastChild);                 
                    }
                    $("#img_hidden").val(data.files[0].url);
                    break;
                case 5:
                    break;
                case 14:
                    alert('部分图片超过5M，请调整大小后再上传');                
                    break;
                case 6:
                    alert('GIF图片尺寸不能超过150*150');                
                    break;
                case 12:
                    alert('未登录或超时,请先登录');                
                    swfu.cancelQueue();
                    break;
                case 21:
                    alert('不支持此格式文件');                
                    break;
                default :
                    alert('系统出错<br />错误码：' + data.retCode);                
                    break;
            }
        } catch (ex) {
            this.debug(ex);
        }
    }

    function preLoad() {
        if (!this.support.loading) {
            alert("You need the Flash Player 9.028 or above to use SWFUpload.");
            return false;
        }
        return true;
    }
    function loadFailed() {
        alert("Something went wrong while loading SWFUpload. If this were a real application we'd clean up and then give you an alternative");
    }

    function fileQueueError(file, errorCode, message) {    
        try {
            var imageName = "error.gif";
            var errorName = "";
            if (errorCode === SWFUpload.errorCode_QUEUE_LIMIT_EXCEEDED) {
                errorName = "You have attempted to queue too many files.";
            }

            if (errorName !== "") {
                alert(errorName);
                return;
            }

            switch (errorCode) {
            case SWFUpload.QUEUE_ERROR.ZERO_BYTE_FILE:
                //imageName = "zerobyte.gif";
                break;
            case SWFUpload.QUEUE_ERROR.FILE_EXCEEDS_SIZE_LIMIT:
                //imageName = "toobig.gif";
                alert("部分图片超过5M，请调整大小后再上传");
                break;
            case SWFUpload.QUEUE_ERROR.ZERO_BYTE_FILE:
            case SWFUpload.QUEUE_ERROR.INVALID_FILETYPE:
                alert("不支持此格式文件");
                break;
            default:
                alert("系统错误，请联系管理员, 错误码:"+ errorCode + ";" +message);
                break;
            }

            //addImage("images/" + imageName);

        } catch (ex) {
            this.debug(ex);
        }

    }
    function fileDialogComplete(numFilesSelected, numFilesQueued, totalFilesQueued) {    
        // /** 插入占位符 */
        // SWFUploadUtils.insertPlaceholder("divFileProgressContainer",numFilesQueued);
        try {
            this.startResizedUpload(this.getFile(0).ID, this.customSettings.resizedImgWidth,
                this.customSettings.resizedImgHeight, SWFUpload.RESIZE_ENCODING.JPEG, this.customSettings.resizedImgQuality,
                false,this.customSettings.resizedImgCondition)
        } catch (ex) {
            this.debug(ex);
        }

    }
    function uploadError(file, errorCode, message) {    
        var imageName =  "error.gif";
        var progress;
        try {
            switch (errorCode) {
            case SWFUpload.UPLOAD_ERROR.FILE_CANCELLED:
                try {
                    progress = new FileProgress(file,  this.customSettings.upload_target);
                    progress.setCancelled();

                }
                catch (ex1) {
                    this.debug(ex1);
                }
                break;
            case SWFUpload.UPLOAD_ERROR.UPLOAD_STOPPED:
                try {
                    progress = new FileProgress(file,  this.customSettings.upload_target);
                    progress.setCancelled();
                }
                catch (ex2) {
                    this.debug(ex2);
                }
                break;
            case SWFUpload.UPLOAD_ERROR.UPLOAD_LIMIT_EXCEEDED:
                alert("部分图片超过5M，请调整大小后再上传");
                break;
            case SWFUpload.UPLOAD_ERROR.RESIZE:
                alert("可能是你的图片尺寸过大，请调整后再上传");
                break;
            default:
                alert("系统错误，请联系管理员, 错误码：" +  errorCode + ";" + message);
                break;
            }

            //addImage("images/" + imageName);

        } catch (ex3) {
            this.debug(ex3);
        }
    }
    function uploadComplete(file) {    
        try {
            /*  I want the next upload to continue automatically so I'll call startUpload here */
            if (this.getStats().files_queued > 0) {
               var progress = new FileProgress({id:"tmp_fpID"},  this.customSettings.upload_target);
               progress.setProgress(0);
               this.startResizedUpload(this.getFile(0).ID, this.customSettings.resizedImgWidth,
                    this.customSettings.resizedImgHeight, SWFUpload.RESIZE_ENCODING.JPEG, this.customSettings.resizedImgQuality,
                    false,this.customSettings.resizedImgCondition);

            } else {
                //duma.BeautifyScrollBar.init();
                document.getElementById("cancelUploadBtn").style.display = "none";
                document.getElementById("insertAllBtn").style.display = "inline-block";
                document.getElementById("uploading").className = "hidden";
                document.getElementById("uploaded").className = "";
                document.getElementById("uploadtotalnum").innerHTML = 0;
                updateImageNum();
                picnumindex = 1;

            }
        } catch (ex) {
            this.debug(ex);
        }
    }
    var replyimgbtn = $("#JubbUpload");
    if (replyimgbtn) {
        var swfsphtml = '<span id="swfreply" class="swf-sp"></span>';
        var swfsp = $(swfsphtml);                                        
        replyimgbtn.append(swfsp);
        replyimgbtn[0].insertBefore(pageGlobal.genSwf({
            id: 'swfreply',
            width: 18,
            height: 18,
            targetId:'ubb',
            postId:pageGlobal.pid
        }).movieElement, replyimgbtn[0].firstChild);                            
        var tempopupimghtml = '<div class="popupimgbox" style="display:none;"  id="swfwrapubb"><i class="before"></i><a href="javascript:void(0);" class="close"></a><div class="swf-wrap"></div></div>';
        var tempopupimg = $(tempopupimghtml);                                                
        replyimgbtn.append(tempopupimg);
        tempopupimg.find('.close').bind('click',function(e){
            $('#img_hidden').val('');
            tempopupimg.hide();
            e.stopPropagation();
        })                                        
    }
</script>
