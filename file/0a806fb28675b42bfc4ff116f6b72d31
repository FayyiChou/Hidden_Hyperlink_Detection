    //在线用户套橙色
    var DisplayOnline = (function () {
        var my = {};
        var unique = function(s) {
            if (!s && typeof s !== 'Array') return [];
            var newArray=[];
            var temp = {};
            var item = {};
            for (var i = 0 ; (item= s[i]) != null; i++) {
                if (!temp[item]) {
                    newArray.push(item);
                    temp[item] = true;
                }
            }
            return newArray;
        }
        my.displayOnline = function (ids) {
            for (var i = 0; i < ids.length; i ++) {
                //$('.user-' + ids[i]).addClass("uOnline");
            }
        }
        my.run = function () {
            var onlineIds = [];
            var re = /user-([\d]+).*/ig;
            $('.needonline').each(function() {
                onlineIds.push(this.className.replace(re,'$1'));
            });
            $.getScript("//bbs.pcbaby.com.cn/action/online_user_passport.do?cb=DisplayOnline.displayOnline&ids=" + unique(onlineIds).join(','));
        }
        return my ;
    }());
    DisplayOnline.run();
