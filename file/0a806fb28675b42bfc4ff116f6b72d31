<script type="text/javascript">
    $(function() {
        // 楼层复制start
        var clip = initClip();
        var baseUrl = BASECONFIG.ROOT + "/topic-" + BASECONFIG.TOPICID + "-" + BASECONFIG.PAGENO + ".html#post_";
        if (window.clipboardData) {
            $(".copyFloor").click(function() {
                var url = baseUrl + $(this).attr("pid");
                window.clipboardData.clearData();
                var flag = window.clipboardData.setData("Text",url);
                if (flag) {
                    showMsgObj.html(showSuccessMsg.replace('[:MSG]',"   帖子链接已复制"));
                    tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);
                }
                });
        } else {
            $(".copyFloor").mouseover(function() {
                var url = baseUrl + $(this).attr("pid");
                    clip.setText(url);
                    if (clip.div) {
                           clip.reposition(this);
                       } else {
                       clip.glue(this);
                    }
            });
            clip.addEventListener( "mouseOver", function(client) { 
                $(clip.div).attr("title","点击复制该贴链接");
                $(clip.div).css("border-width","1px").css("border-style","solid").css("border-color","black");
            });
            clip.addEventListener( "mouseOut", function(client) { 
                $(clip.div).css("border-width","0px");
            }); 
            clip.addEventListener('complete',function() {
                showMsgObj.html(showSuccessMsg.replace('[:MSG]',"   帖子链接已复制"));
                tb_show_2('','#TB_inline?inlineId=showMsg&width=310',false);
            });
        }
     // 楼层复制end
     
        //帖子浏览数     
         $.get("//bbs.pcbaby.com.cn/intf/topic/counter.ajax?tid=4695838&currentUrl=/topic.do?tid=4695838&currentReferer=&" + new Date().getTime(),function(result) {
               var rst = (new Function("", "return " + result))(); // str - json
               if (rst) {
                   $("#views").html(rst.views);
               }
           });
     
    });
    
    //楼主左侧用户信息滚动效果
    var isIE6= !!window.ActiveXObject&&!window.XMLHttpRequest;
    var floatUserProfile = function() {
        var D = $(".post_wrap_first");
        var post_top = $('.post_list_top'), post_offset_top = post_top.offset().top, post_top_height = post_top.outerHeight(),placeholder = jQuery('<div>');
        var post_info = $('.post_info').eq(0), post_info_top = post_info.offset().top, post_info_left = post_info.offset().left, post_info_pos_left=post_info.position().left, post_info_height=post_info.outerHeight(),info_holder = jQuery('<div>');

        placeholder.css('height',post_top_height);
        info_holder.css('height',post_info_height);
        var C = $(document),//document对象
        _ = D.find(".floor-userInfo").eq(0).css("z-index", 200),//左侧
        B = D.find(".post_right").eq(0),//右侧
        A = function() {
            $(window).bind("scroll resize",
            function() {
                var sctop = C.scrollTop();
                if(sctop>post_offset_top){
                    placeholder.show().insertBefore(post_top);
                    post_top.addClass('post_top_fixed');
                }else{
                    placeholder.hide();
                    post_top.removeClass('post_top_fixed');
                }
                if(sctop>parseInt(post_info_top - post_top_height)){
                    if(sctop - post_info_top + post_info_height < D.height() - post_info_height){
                        info_holder.show().insertBefore(post_info);
                        post_info.removeAttr('style').addClass('post_top_fixed');
                        post_info.css('top',post_top_height);
                    }else{
                        info_holder.show().insertBefore(post_info);
                        post_info.removeClass('post_top_fixed').css({
                            'position':'absolute',
                            'top':'auto',
                            'bottom':'0',
                            'z-index':'200',
                            'background-color':'#fff',
                            'left':post_info_pos_left
                        })
                    }
                    
                }else{
                    info_holder.hide();
                    post_info.removeAttr('style').removeClass('post_top_fixed');
                }
                var A = D.offset().left;//获取左侧距离文档最左边的距离
                var F = D.height() - 59,//一楼对象高度-59
                B = _.height(),//左侧对象高度
                D_top = D.offset().top,//距离顶部高度
                G = C.scrollTop(),//滚动条高度
                H = F - B;//一楼总体高度-左侧对象高度
                if (D.height() < screen.height) return;
                if (G > parseInt(D_top - post_top_height)) {//当滚动条高度大于对象距离顶部高度时
                    var E = G - D_top; //滚动高度-距离顶部高度
                    if (E + B > F) _.css("top", "auto").css("left", 0).css("bottom", "59px").css("position", "absolute");
                    else _.css("bottom", "").css("left", A).css("height",_.height()).css("top", post_top_height).css("position", "fixed").css("margin-left", "5px")
                } else _.css("top", 0).css("bottom", 0).css("left",0).css("position", "static").css("margin-left", "0px")
            })
        };
        if (!isIE6) {A();}
    };
