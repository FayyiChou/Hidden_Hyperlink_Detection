<script>
function chkdata(form) {
	with (form) {

		 if (password.value=='') {
		 	  alert('請輸入密碼');
		 	  password.focus();
		 	  return false;
		 }

	     if (ccompany.value=='') {
		 	  alert('請輸入公司名稱');
		 	  ccompany.focus();
		 	  return false;
		 }

	     if (taxid.value=='') {
		 	  alert('請輸入公司統編');
		 	  taxid.focus();
		 	  return false;
		 }

	     if (comp_tel.value=='') {
		 	  alert('請輸入公司電話');
		 	  comp_tel.focus();
		 	  return false;
		 }

	     if (comp_fax.value=='') {
		 	  alert('請輸入公司傳真');
		 	  comp_fax.focus();
		 	  return false;
		 }

	     if (comp_addr.value=='') {
		 	  alert('請輸入公司地址');
		 	  comp_addr.focus();
		 	  return false;
		 }


	     if (contact_c.value=='') {
		 	  alert('請輸入公司聯絡人');
		 	  contact_c.focus();
		 	  return false;
		 }
		 
		 if (!confirm('資料正確嗎?')) {
		 	  return false;
		 }
		 
	}
	
}
</script>
