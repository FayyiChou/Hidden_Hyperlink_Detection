function chkdata1(form) {
	with (form) {

		 if (comp.value=='') {
		 	  alert('請輸入單位名稱');
		 	  comp.focus();
		 	  return false;
		 }

	     if (office.value=='') {
		 	  alert('請輸入部門職稱名稱');
		 	  office.focus();
		 	  return false;
		 }

	     if (comtel.value=='') {
		 	  alert('請輸入單位電話');
		 	  comtel.focus();
		 	  return false;
		 }

	     if (comemail.value=='') {
		 	  alert('請輸入單位信箱');
		 	  comemail.focus();
		 	  return false;
		 }

	     if (comus.value=='') {
		 	  alert('請輸入單位_絡人');
		 	  comus.focus();
		 	  return false;
		 }

		 
		 if (!confirm('資料正確嗎?')) {
		 	  return false;
		 }
		 
	}
	
}
</script>
 
