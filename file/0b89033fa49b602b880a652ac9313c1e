<script  language="JavaScript">
    // JavaScript Document
    lastScrollY = 0;
    function heartBeat() {
        var diffY;
        if (document.documentElement && document.documentElement.scrollTop)
            diffY = document.documentElement.scrollTop;
        else if (document.body)
            diffY = document.body.scrollTop
        else
        {/*Netscape stuff*/ }
        //alert(diffY);
        percent = .1 * (diffY - lastScrollY);
        if (percent > 0) percent = Math.ceil(percent);
        else percent = Math.floor(percent);
        document.getElementById("bannerleft").style.top = parseInt(document.getElementById("bannerleft").style.top) + percent + "px";
        document.getElementById("bannerright").style.top = parseInt(document.getElementById("bannerleft").style.top) + percent + "px";
        lastScrollY = lastScrollY + percent;
        //alert(lastScrollY);
    }
    daoweb_left = "<div id=\"bannerleft\" style='left:8px;POSITION:absolute;TOP:200px; display:none'><a href='' target='_blank'><img border=0 src=Themes/www.yncpu.net/images/pxxzw.gif><br><a href=JavaScript:; onclick=\"bannerleft.style.visibility='hidden'\"><img border=0 src=Themes/www.yncpu.net/img/close.gif></a></div>"
    daoweb_right = "<div id=\"bannerright\" style='right:0px;POSITION:absolute;TOP:200px;'><a href='Web_Consultation_default.aspx' target='_blank'><img border=0 src=Themes/www.yncpu.net/img/pf_right.gif><br><a href=JavaScript:; onclick=\"bannerright.style.display = 'none'\"><img border=0 src=Themes/www.yncpu.net/img/close.gif></div>"
    document.write(daoweb_left);
    document.write(daoweb_right);
    window.setInterval("heartBeat()", 1);
</script>
