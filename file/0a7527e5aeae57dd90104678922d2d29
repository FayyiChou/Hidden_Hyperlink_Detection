    function iFrameHeight() {
        var ifm= document.getElementById("panter_iframe");
        var subWeb = document.frames?document.frames["panter_iframe"].document:ifm.contentDocument;
            if(ifm != null && subWeb != null) {
            ifm.height = subWeb.body.scrollHeight;
            }
    }
</script> 
