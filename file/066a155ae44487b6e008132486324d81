<script type="text/javascript">
$(function(){
	//search
	var skey = getCookie('top_s');
		if(skey){
		$("#t-sp").hide();
		} else {
			$("#t-sp").slideDown(800);
			}
		$("#t-sp .close").click(function(){
			setCookie('top_s','yes',1);
			$("#t-sp").hide();
	});	
});
</script></div>
