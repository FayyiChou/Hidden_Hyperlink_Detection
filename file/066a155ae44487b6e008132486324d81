var COOKIE_PRE = 'B72A_';var _CHARSET = 'utf-8';var LOGIN_SITE_URL = 'http://www.rhqrb.com/member';var MEMBER_SITE_URL = 'http://www.rhqrb.com/member';var SITEURL = 'http://www.rhqrb.com/shop';var SHOP_SITE_URL = 'http://www.rhqrb.com/shop';var RESOURCE_SITE_URL = 'http://www.rhqrb.com/data/resource';var RESOURCE_SITE_URL = 'http://www.rhqrb.com/data/resource';var SHOP_TEMPLATES_URL = 'http://www.rhqrb.com/shop/templates/default';
