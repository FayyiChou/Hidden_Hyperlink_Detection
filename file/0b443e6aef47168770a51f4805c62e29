<script type="text/javascript">
function sub_search()
{
	var wd =document.getElementById("keyword").value;
	var k = "";
	if(wd != "")
	{
		k +="?keyword="+encodeURIComponent(wd)+"&ie=utf-8";
	}
	window.location.href="http://so.xywy.com/club404.php"+k;
}
</script>

