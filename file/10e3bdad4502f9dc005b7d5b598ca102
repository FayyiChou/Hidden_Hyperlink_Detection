<script type="text/javascript">
jQuery(document).ready(function() {
var time = 0,
sliderTimer = null,
picNum = 0;
time = 1000;

slider = {
init: function(){
slider.slideBind();
jQuery('#ad li').hover(function(){
clearTimeout(sliderTimer);
//clearInterval(sliderTimer);
},function(){
clearTimeout(sliderTimer);
sliderTimer = setTimeout('slider.slideBind()', 5000);
//sliderTimer = setInterval('slider.slideBind()', 5000);
});
jQuery('#controler a').click(function(){
clearTimeout(sliderTimer);
//clearInterval(sliderTimer);
var idx = jQuery(this).index();
picNum = idx;
slider.slide(idx);
//picNum ++;
});
},
slide: function(num){
jQuery('#controler a').removeClass('active');
jQuery('#controler a').eq(num).addClass('active');
jQuery('#ad li').hide();
jQuery('#ad li').eq(num).fadeIn('1000');
picNum ++;
},
slideBind: function(){
if (picNum < 3){
//slider.slide(picNum);
//picNum ++;
}else{
picNum = 0;
//slider.slide(picNum);
//picNum ++;
}
clearTimeout(sliderTimer);
slider.slide(picNum);
sliderTimer = setTimeout('slider.slideBind()', 5000);
//sliderTimer = setInterval('slider.slideBind()', 5000);
}
};
slider.init();

function ChangeDownloadSubver3(id) {
var subver = 0;
var url = location.href;
var paraString1 = url.substring(url.indexOf("?") + 1, url.length);
var patt1 = new RegExp("^bd([0-9]+)$");
if(patt1.test(paraString1)) {
subver = Number(patt1.exec(paraString1)[1]);
} else if(paraString1 == "brand") {
subver = 33;
} else if(paraString1 == "notify") {
subver = 4;
} else {
var para = GetUrlParms();
if(para["subver"] != undefined) {
var result = Number(para["subver"]);
if(NaN == result) {
subver = 0;
} else {
subver = result;
}
}
}
if(subver > 1 && subver <= 99){
document.getElementById(id).href = "#";
}
}
ChangeDownloadSubver3("down1");
ChangeDownloadSubver3("down2");
ChangeDownloadSubver3("down3");
});
</script> 
