  <script type="text/javascript">
	$(function(){
		$(".a1").click(function(){
			var size = $(".zwnr").css("font-size");
			var fontSize = parseFloat(size,10);
			var unit = size.slice(-2);
			fontSize -= 2;
			$(".zwnr").css("font-size",fontSize + unit );
		});
		$(".a2").click(function(){
			var size = $(".zwnr").css("font-size");
			var fontSize = parseFloat(size,10);
			var unit = size.slice(-2);
			fontSize += 2;
			$(".zwnr").css("font-size",fontSize + unit );
		});
	})
