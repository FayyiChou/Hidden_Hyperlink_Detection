<script type="text/javascript">
    var uuid = '';
    var msgId = 0;
    var userId = '631989363371';
    var ref = "";
    var i = 0;
    $(function () {
        $.validator.setDefaults({
            submitHandler: function () {
                //                if ($("#tj").val() == "1") {
                //                    submitForm();
                //                } else {
                //                    Create();
                //                }
                Create(0);
            }
        });
        $('.cclose').on('click', function () {
            $('.codepop').hide();
            $("#tj").val("0");
            clearInterval(ref);
            i = 0;
            window.location.reload();
        });
        $.validator.addMethod("isQQ", function (value, element) {
            var qq = /[1-9][0-9]{4,14}/;
            return this.optional(element) || (qq.test(value));
        }, "格式有误，请重新输入");
        $.validator.addMethod("isTel", function (value, element) {
            var tel = /^1[3|4|5|7|8]\d{9}$/;
            return this.optional(element) || (tel.test(value));
        }, "格式有误，请重新输入");
        $("#form1").validate({
            rules: {
                username: { required: true },
                qq: { isQQ: true },
                tel: { required: true, isTel: true },
                code: { required: true },
                cont: { required: true }
            }, errorClass: "atip", errorElement: "p",
            errorPlacement: function (error, element) {
                error.appendTo(element.parent());
            },
            messages: {
                username: { required: "请输入您的姓名" },
                tel: { required: "请输入手机号码" },
                code: { required: "请输入您的验证码" },
                cont: { required: "请在此输入您咨询的问题" }
            }
        });
        $("#vcodeBtn").on("click", function () {
            var tel = /^1[3|4|5|7|8]\d{9}$/;
            var a = $(this);
            if (tel.test($("#tel").val())) {
                if (a.attr("load") === "false") {
                    a.attr("load", "true");
                    $.ajax({
                        url: "handler/SendSmsToUserHandler.ashx",
                        type: "post",
                        dataType: "json",
                        data: { tel: $("#tel").val() },
                        success: function (s) {
                            if (s.Flag === 1) {
                                a.removeClass("vcode").addClass("vcoded");
                                var num = 90;
                                var handId = setInterval(function () {
                                    a.text("已发送(" + num + ")");
                                    num--;
                                    if (num < 0) {
                                        clearInterval(handId);
                                        a.text("获取验证码").removeClass("vcoded").addClass("vcode").attr("load", "false");
                                    }
                                }, 1000);
                            } else {
                                a.attr("load", "false");
                            }
                            alert(s.Message);
                        },
                        error: function () {
                            a.attr("load", "false");
                            alert("发送失败，请重新发送！");
                        }
                    });
                }

            } else {
                alert("您输入的手机号码格式不正确！");
            }
        });
        $(".zanshare_0").on("click", function () {
            var a = $(this);
            var repliableid = a.attr("repliableid");
            var zancount = $("#zancount_" + repliableid);
            $.ajax({
                url: "handler/PostShareZanHandler.ashx",
                type: "post",
                dataType: "json",
                data: { repliableId: repliableid },
                success: function (s) {
                    if (s.Flag === 1) {
                        zancount.text(parseInt(zancount.text()) + 1);
                        a.addClass("active");
                    } else {
                        alert(s.Message);
                    }
                },
                error: function () {
                    alert("发送失败，请重新发送！");
                }
            });

        });
        $(".cl_WeChat").on("click", function () {
            var a = $(this);
            var repliableid = a.attr("repliableid");
            var sharenum = $("#sharenum_" + repliableid);
            $.ajax({
                url: "/handler/PostShareNumHandler.ashx",
                type: "post",
                dataType: "json",
                data: { repliableId: repliableid },
                success: function (s) {
                    if (s.Flag === 1) {
                        sharenum.text(parseInt(sharenum.text()) + 1);
                        a.addClass("active");
                    } else if (s.Flag === -1) {
                        //do nothing
                    } else {
                        alert(s.Message);
                    }
                },
                error: function () {
                    //alert("发送失败，请重新发送！");
                }
            });

        });
        $('.a-more').click(function () {
            var a = $(this);
            if ($.trim(a.text()) === "【展开全部】") {
                a.prev(".atxt").text(a.attr("cont"));
                a.parents('.askbottom dd,.askbottom').height('auto');
                a.text("【收起】");
            } else {
                a.prev(".atxt").text(a.attr("cont").substring(0, 80) + "...");
                a.parents('.askbottom dd,.askbottom').height('96');
                a.text("【展开全部】");
            }

        });

    });
    function submitForm() {
        $("#tj").val("1");
        $("#uuid").val(uuid);
        $("#form1").ajaxSubmit({
            url: "handler/PostReplyHandler.ashx",
            type: "post",
            dataType: "json",
            success: function (s) {
                if (s.Flag === 1) {
                    //$("#form1")[0].reset();
                    msgId = s.msgId;
                    $("#NoMa").html("恭喜提交成功");
                    $(".codepop").show();
                }
                else {
                    msgId = 0;
                    clearInterval(ref);
                    $(".codepop").hide();
                    alert(s.Message);
                }
            },
            error: function () {
                alert("留言失败，请重试");
            }
        });
    }

    function NoSaoma() {
        $(".codepop").hide();
        submitForm();
    }

    function startRequest() {
        i++;
        if (i < 12) {
            $.ajax({
                url: "/handler/wxCode.ashx",
                type: 'POST',
                data: { code: uuid, type: "checkcode", msgId: msgId, userId: userId },
                cache: false,
                success: function (result) {
                    if (result == 0) {
                        clearInterval(ref);
                        i = 0;
                        //NoSaoma();
                        window.location.reload();
                    }
                }
            });
        } else {
            //二维码超时了无法登录需要重新生成二维码
            //uuid = '';
            $("#erweima").html("<div> 二维码已过期，请重新点击此处<a href=\"Javascript:Create(" + uuid + ");\">刷新</a>，获取新的二维码！</div>");
            clearInterval(ref);
            i = 0;
        }
    }

    function Create(d) {
        $("#erweima").html("");
        $.post("/handler/wxCode.ashx", { type: "getuuid", ouid: d }, function (data) {
            uuid = data;
            if (uuid == "0") {
                return;
            }
            if (d == 0)
                submitForm();
            $.post("/handler/wxCode.ashx?uuId=" + uuid, { type: "ticketurl", uuid: uuid }, function (data) {
                $("#erweima").html("<img src='" + data + "'/>");
                //$(".codepop").show();
                ref = setInterval("startRequest()", 5000);
            });
        });
    }
</script>
</html>
