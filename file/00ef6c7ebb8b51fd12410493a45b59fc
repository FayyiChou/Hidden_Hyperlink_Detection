                          demo2.innerHTML = demo1.innerHTML
                          function Marquee1() {
                              if (demo2.offsetWidth - demo.scrollLeft <= 0)
                                  demo.scrollLeft -= demo1.offsetWidth
                              else {
                                  demo.scrollLeft++;
                              }
                          }
                          var MyMar1 = setInterval(Marquee1, speed)
                          demo.onmouseover = function () { clearInterval(MyMar1) }
