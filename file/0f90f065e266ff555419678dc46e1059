    <script type="text/javascript">

		var showWenJuan = "false";
		window.ssofrom = "cp_daohang";
var a = "";
function closeFlash() {
    if (document.getElementById("flashObj")) {
        if (document.getElementById("iconDiv")) {
            a = document.getElementById("iconDiv").innerHTML;
            document.getElementById("iconDiv").innerHTML = "<a href='#' onclick='openFlash()'><img src='//images.koolearn.com/www/eclass/course/gre.jpg' /></a>";
        }
    }
}
function openFlash() {
    if (a != "") {
        if (document.getElementById("iconDiv")) {
            document.getElementById("iconDiv").innerHTML = a;
        }
    }
}

//确定操作flash对象
function thisMovie(movieName) {
    if (navigator.appName.indexOf("Microsoft") != -1) {
        return window[movieName];
    }
    else {
        return document[movieName];
    }
}
var swfLoaded = false;
var htmlLoaded = false;
var isPause = true;

function playSwf() {
    //document.getElementById("flashtip").innerHTML += ",playSwf";
    var flashObj = thisMovie("flashObj");

    if(window.kPlay){
    	kPlay.play();
    	kPlay.jqObj.find(".jp-full-screen").click(function(){
    		$(this).closest(".playObject").addClass("playObject-full");
    		$(".path2,.videoCloseRt,#bdshare,.public_top").each(function(){
    			if($(this).is(":visible")){
    				$(this).addClass("data-full-hide").hide();
    			}
    		});
    	});
    	kPlay.jqObj.find(".jp-restore-screen").click(function(){
    		$(this).closest(".playObject").removeClass("playObject-full");
    		$(".data-full-hide").removeClass("data-full-hide").show();
    	});
    }

    if (swfLoaded && flashObj) {
        thisMovie("flashObj").playSwf();
    } else {
        isPause=false;//点早了
        return;
    }
}
function pauseSwf() {
    if (swfLoaded) {
        thisMovie("flashObj").pauseSwf();
    } else {
        alert("课件加载中，请稍等片刻...");
        return;
    }
}

function onSwfLoaded() {
    swfLoaded = true;
    //加载后先暂停，默认暂停
    if(isPause)
        pauseSwf();
}
function startPlay()
{
	playSwf();
}


function htmlVali(title){
	if(title.indexOf("Error")>=0){
		$("#iconDiv").prev(".play_btn").remove();
		$("#iconDiv").remove();
	}
}

var productId = 44042;
var versionId = 1;
$(function(){
	$("#colleSpan").bind("click",function(){
		if(_checkLogin() && _checkUserHaveCollect(productId, versionId)){
			popSty("over");
		} else if (_checkLogin()) {
			_shoucang(productId, versionId);
		} else {
			loginPopUp("cp_shoucang");
		}
	}),$('.tsfws a').hover(function(){
		$(this).append('<span class="tit_bg"><span class="title_ct tct1"></span> <a href="javascript:;" class="title_txt" target="_blank"></a></span>');
		var titleTxt = $(this).find('.txt_none').text();
		$(this).find('.title_txt').text(titleTxt);
	}, function(){
		$('.tit_bg',this).remove();
	});
	//var host = window.location.host;
	//document.domain=host.substring(host.indexOf('.')+1, host.length);

	$('.teachChoos:eq(0)').trigger('click');
	var chue = setTimeout(function(){$('.tipsy').remove()},5000);
	$('.cf2,.teach_nam').click(function(){
		clearTimeout(chue);
	});
	$("body").click(function(e){
		var target = $(e.target);
		if (target.closest(".tipsy").size() === 0) {
			clearTimeout(chue);
			$('.tipsy').remove();
		}
	})
});

window.onload = function(){
 	//$("#dialogBox_login").hide();
 	if (_checkLogin()) {
 		$("#unLoginStudentSound").hide();
 		$("#loginStudentSound").show();
 	} else {
 		$("#unLoginStudentSound").show();
 		$("#loginStudentSound").hide();
 	}

 	if (_checkUserHaveCollect(productId, versionId)) {
 		$("#collectImgSpan").removeClass("colle_over");
 	} else {
 		$("#collectImgSpan").addClass("colle_over");
 	}
	checkPreSqle(44042,1);
 };

/**由于做了页面缓存所以判断登录时用AJAX请求方法*/
function doFreeBuy(productId,versionId) {
	if (_checkLogin()) {
		lqcg(productId,versionId);
	} else {
		loginPopUp();
	}
}

function login_calback(code, msg) {
	if (0 == code) {
		$("#dialogBoxClose_login").click();
		if(login_calback.shoucang){
			_shoucang(productId, versionId);
		}else{
			location.reload(true);
		}
	}
}

function register_calback(code, msg) {
	if (0 == code) {
		$("#dialogBoxClose_login").click();
		if(login_calback.shoucang){
			_shoucang(productId, versionId);
		}else{
			location.reload(true);
		}
	}
}

function showShiTingLessons() {
	var $trialTab = $('.area3_lft .path2c:eq(2)');
	if($trialTab.hasClass('allTrial')){
		$trialTab.click();
	}else{
		$('.area3_lft .path2c:eq(1)').click();
	}

    var st = $("#stController");
    if(!st.is(":checked")){
    	st.attr("checked","checked").triggerHandler("click");
    }
}
document.domain='koolearn.com';

$(window).on('message', function(e){
    try{
        var res = JSON.parse(e.originalEvent.data);

        if(res.type === 'login.dialog.height'){
            $('#dialogBox_login').css('height', res.data.height+20);
        }
    }catch(e){
        console.log(e);
    }
});

function login_callback(code,msg){
	if(code==0){
		$("#dialogBoxClose_login").click();
			_shoucang(productId, versionId);
			window.parent.location.reload(true);
		}
}

function register_callback(code,msg){
	if(code==0){
		$("#dialogBoxClose_login").click();
		window.parent.location.reload(true);
	}
}
