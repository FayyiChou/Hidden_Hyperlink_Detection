			$('#search').click(function () { //搜索
		        if ($('#fromCity_3word').val() == '' || $('#fromCity').val() == '') {
		            alert("请选择出发城市");
		            return;
		        }
		        if ($('#toCity_3word').val() == '' || $('#toCity').val() == '') {
		            alert("请选择到达城市");
		            return;
		        }
		        if ($('#depDate').val() == '') {
		            alert("请选择出发日期");
		            return;
		        }
		        if ($('#type').val() == 2 && $('#returnDate').val() == '') {
		            alert("请选择返程日期");
		            return;
		        }
		        searchData();
		    });
		    $('#changeFlightSearchCity').click(function () {//国内机票出发城市和到达城市数据互换
		        var fromCity = $('#fromCity').val();
		        var toCity = $('#toCity').val();
		        $('#fromCity').val(toCity);
		        $('#toCity').val(fromCity);
		    });
		    function searchData(fromD, toD, tD) {
			    var parma, t = $("#type").val();
			    parma = 'fromCity=' + $('#fromCity_3word').val(); //encodeURIComponent('CAN'); ;
			    parma += '&toCity=' + $('#toCity_3word').val(); // encodeURIComponent('BJS');
			    parma += '&fromDate=' + $('#depDate').val();
			    parma += '&toDate=' + $('#returnDate').val();
			    parma += '&t=' + t;
			    parma += '&rand=' + Math.random();
			    location.href = $('#flighturl').val() + '?' + parma;
			}
			if (typeof (citys) != "undefined") {
		        $("#fromCity").suggest(citys, { hot_list: commoncitys, dataContainer: '#fromCity_3word', onSelect: function () { $("#toCity").click(); }, attachObject: '#suggest' });
		        $("#toCity").suggest(citys, { hot_list: commoncitys, dataContainer: '#toCity_3word', attachObject: "#suggest2" });
		    }
			//当滚动条的位置处于距顶部100像素以下时，跳转链接出现，否则消失 
	            $(window).scroll(function(){ 
	                var scrollTop = $(window).scrollTop();
	                if(scrollTop){
	                    $('.publicBox').each(function(i){
	                    	$(this).attr('t', i);
	                        if(scrollTop >= $(this).offset().top-68){
	                            var myT = $(this).attr('t');
	                            $('.leftNavBox li').removeClass('on');
	                            $('.leftNav li[v="'+myT+'"]').addClass('on');
	                        }
	                    });
	                    if(scrollTop > $('.choiceness').offset().top-100 && scrollTop < $('.travels').offset().top+100){
	                        $(".leftNav").fadeIn(100);
	                    }else{
	                        $(".leftNav").fadeOut(100); 
	                    }
	                }
	            });
	            $(".leftNav li").each(function(i){
	            	$(this).attr('v', i);
	            }).click(function() { 
	                var i = $(this).attr('v'); //取得按钮值
	                var t = $('.publicBox[t="'+ i +'"]'); //对应的id层
	                $(".leftNav li").removeClass('on');
                    $(this).addClass('on');
                    $("html,body").animate({scrollTop: t.offset().top-68}, 250); //一秒跳转到该ID的位置
	            });
	            /*切换菜单左侧自适应高度*/
				function searchHeight(){
					var searchHeight = $('.search_left').height();
					var num = $('.search_left').find('ul').children('li').length;
					var sum = searchHeight/num;
					$('.search_left').find('li').height(sum);
					$('.search_left').find('li').css({
						lineHeight: sum + "px",
					});
				}
