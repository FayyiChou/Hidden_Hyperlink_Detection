			        var val = $(this).attr('val');
			        $(this).parent().slideUp();
			        $(this).parent().parent().find('.input02').val(text);
			        $(this).parent().parent().find("input[type='hidden']").val(val);
			    });
			});
		    /*选择日期*/
			$(function() {
			    $( ".startDate" ).datepicker({//酒店日期选择
			    	numberOfMonths: 2,
			        onClose: function( selectedDate ) {
				        $( ".endDate" ).datepicker( "option", "minDate", selectedDate );
				    }
			    });
			    $( ".endDate" ).datepicker({
			    	numberOfMonths: 2,
			        onClose: function( selectedDate ) {
