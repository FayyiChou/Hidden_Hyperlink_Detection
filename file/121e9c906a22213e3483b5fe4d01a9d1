$(document).ready( function() {
		flowplayer("player", "imgLib/add_images/flowplayer-3.2.4.swf");
		function playeragain(){
			flowplayer("player", "imgLib/add_images/flowplayer-3.2.4.swf");
		}	
		$('#cancel').live("click", function(){
				$(".flash_content").hide();
				$("#player").css({display:"none"});
				$("#cancel").css({display:"none"});
				$("#screen").css({display:"none"});
				$(".flash_content").html("");
		});
		$('#show_player').bind("click", function(){		
				$(".flash_content").html("");
				$(".flash_content").html("<a  href='imgLib/add_images/ad_qiye.flv'  style='display:block;width:640px;height:360px;'  id='player'>"+ 
					"<img src='imgLib/add_images/ad_qiye_zh.jpg' width='640' height='360' border='0' />"+
				"</a>"+  
				"<div id = 'flash_cancel_content'>"+
					"<img id = 'cancel' src = 'cssLib/theme/default/images/cancel_flash.jpg' />"+
				"</div>");
				$(".flash_content").css({display:"block"});
				$("#cancel").css({display:"block"});
				$("#screen").css({display:"block"});
				$("#player").css({display:"block"});
				playeragain();
		});
		$('.ad:eq(0)').bind("click", function(){
				$(".flash_content").html("");
				$(".flash_content").html("<a  href='imgLib/add_images/ad_qiye.flv'  style='display:block;width:640px;height:360px;'  id='player'>"+ 
					"<img src='imgLib/add_images/ad_qiye_zh.jpg' width='640' height='360' border='0' />"+
				"</a>"+  
				"<div id = 'flash_cancel_content'>"+
					"<img id = 'cancel' src = 'cssLib/theme/default/images/cancel_flash.jpg' />"+
				"</div>");
				$(".flash_content").css({display:"block"});
				$("#cancel").css({display:"block"});
				$("#screen").css({display:"block"});
				$("#player").css({display:"block"});
				playeragain();
		});	
	
});
</script>
