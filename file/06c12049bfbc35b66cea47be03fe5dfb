<script type="text/javascript">
IsLogin();
$('#k-jumpToVideo').addClass("act");
$(window).scroll(function(){scrollShow();})
function scrollShow()
	{
		var t=$(window).scrollTop();
		var part1=$('#jumpToVideo').offset().top || document.getElementById('jumpToVideo').offsetTop;
		var part2=$('#jumpToJs').offset().top || document.getElementById('jumpToJs').offsetTop;
		var part3=$('#jumpToPlus').offset().top || document.getElementById('jumpToPlus').offsetTop;
		var part4=$('#jumpToTemplets').offset().top || document.getElementById('jumpToTemplets').offsetTop;
		var part5=$('#jumpToDesign').offset().top || document.getElementById('jumpToDesign').offsetTop;
		if((part1-t)<300 && (part1-t)>0){$('#k-jumpToVideo').addClass('act').siblings('li').removeClass('act');}
		if((part2-t)<300 && (part2-t)>0){$('#k-jumpToJs').addClass('act').siblings('li').removeClass('act');}
		if((part3-t)<300 && (part3-t)>0){$('#k-jumpToPlus').addClass('act').siblings('li').removeClass('act');}
		if((part4-t)<300 && (part4-t)>0){$('#k-jumpToTemplets').addClass('act').siblings('li').removeClass('act');}
		if((part5-t)<300 && (part5-t)>0){$('#k-jumpToDesign').addClass('act').siblings('li').removeClass('act');}
	}
</script>
