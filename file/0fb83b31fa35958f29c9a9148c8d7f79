<div id="countryModal" class="reveal-modal small copyPanel themeYellow" data-reveal>
    <h2>Are you visiting from: China?</h2>
    <div class="modalButtonPanel"  style="width:370px; margin:0px auto; max-width:100%;">
		
				<a id="CountryDetectionModal_rptMatchingAgencies_ctl00_hlkModalContinue" class="button expand countryDetectSelection" data-agency="chn" href="/chn">View China</a>
			
        <a href="#" class="button altButton expand electiveClose">Select another country</a>
    </div>
                
    <a class="close-reveal-modal"><span class="modalCloseIcon" title="Close"></span></a>
</div>


<script type="text/javascript">
			
	jQuery(document).ready(function () {

		$('.countryDetectSelection').on('click',function(e){
			e.preventDefault();
			var agency = $(this).attr('data-agency');
			var destination = $(this).attr('href');
			$.cookie("homeAgency", agency, { expires: 90, path: '/' });
			window.location.href = destination;
		});
				
		/*SPECIFIC TO THIS PAGE*/
		var showPopup = true;
		if(showPopup){
			$('#countryModal').foundation('reveal', 'open');
		}
		
		$('.electiveClose').on('click', function (e) {
			$('#countryModal').foundation('reveal', 'close');
		});
		
	});

</script>

