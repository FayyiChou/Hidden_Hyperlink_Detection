	<script type="text/javascript">
	     $(document).ready(function () { swapIMG(); });
	     
		function swapIMG() {
			var today=new Date();
			var s=today.getSeconds();
			var nvih = "";
			//
			if (s>50) { nvih='<img style="z-index:0" src="/SiteAssets/HomeImage1.jpg" alt="">' } 
			else if (s>40) { nvih='<img style="z-index:0" src="/SiteAssets/HomeImage2.jpg" alt="">' } 
			else if (s>30) { nvih='<img style="z-index:0" src="/SiteAssets/HomeImage3.jpg" alt="">' } 
			else if (s>20) { nvih='<img style="z-index:0" src="/SiteAssets/HomeImage4.jpg" alt="">' } 
			else if (s>10) { nvih='<img style="z-index:0" src="/SiteAssets/HomeImage5.jpg" alt="">' } 
			else { nvih='<img style="z-index:0" src="/SiteAssets/HomeImage6.jpg" alt="">' }
			if (nidviews.innerHTML != nvih) {
				nidviews.innerHTML = nvih;
			}
			t=setTimeout('swapIMG()',15000);
		}
	</script>
		
