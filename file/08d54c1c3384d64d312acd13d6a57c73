<script type="text/javascript">
	$('.report_tit span').click(function(){
		var _this = $(this).index();
		$('.report_tit span').removeClass('on');
		$(this).addClass('on');
		$('.report_main').hide();
		$('.report_main').eq(_this).show();
		
		var ease = $('.zr_loop').eq(_this);
		lory(ease,{
			infinite: 3,
			slidesToScroll: 3,
			slideSpeed: 300,
			ease: 'cubic-bezier(0.455, 0.03, 0.515, 0.955)',
			enableMouseEvents: true
		});
	});
</script></body></html>