<script type="text/javascript">
$(function(){
	var url = window.location.href;
	$("#nwyw").removeClass("on");
	$("#jdgz").removeClass("on");
	$("#yxcz").removeClass("on");
	$("#hydt").removeClass("on");
	$("#shzr").removeClass("on");
	$("#ydfw").removeClass("on");
	$("#qygg").removeClass("on");
	$("#gynw").removeClass("on");
	
	if(url.indexOf("nwyw")>0){
		$("#nwyw").addClass("on");
	}else if(url.indexOf("jdgz")>0){
		$("#jdgz").addClass("on");
	}else if(url.indexOf("yxcz")>0){
		$("#yxcz").addClass("on");
	}else if(url.indexOf("hydt")>0){
		$("#hydt").addClass("on");
	}else if(url.indexOf("shzr")>0){
		$("#shzr").addClass("on");
	}else if(url.indexOf("ydfw")>0){
		$("#ydfw").addClass("on");
	}else if(url.indexOf("qygg")>0){
		$("#qygg").addClass("on");
	}else if(url.indexOf("gynw")>0){
		$("#gynw").addClass("on");
	}
});	
</script>
