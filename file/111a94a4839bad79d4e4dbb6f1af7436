	if (window.external
			&& window.external.twGetRunPath
			&& window.external.twGetRunPath().toLowerCase().indexOf("360se") > -1) {
		alert('本站不支持360浏览器访问，请更换其他浏览器！');
		var re = window.open("", "_self");
	}
	function regHelp() {
		window.open('help/help.jsp');
	}
	function unRegHelp() {
		Ext.onReady(function() {
			Ext.MessageBox.alert('帮助', '未登记企业登录：还没有在工商机关登记的经营者登录');
		});
	}
	Browser = {
		IE : !!(window.attachEvent && !window.opera),
		IE_ : function() {
			var ie$ = "IE7以上";
			if (this.IE) {
				var browser = navigator.appName
				var b_version = navigator.appVersion
				var version = b_version.split(";");
				var trim_Version = version[1].replace(/[ ]/g, "");

				if (browser == "Microsoft Internet Explorer"
						&& trim_Version == "MSIE6.0") {
					ie$ = "IE6";
				}

			}
			return ie$;
		},
		Opera : !!window.opera,
		WebKit : navigator.userAgent.indexOf('AppleWebKit/') > -1,
		Gecko : navigator.userAgent.indexOf('Gecko') > -1
				&& navigator.userAgent.indexOf('KHTML') == -1,
		MobileSafari : !!navigator.userAgent.match(/Apple.*Mobile.*Safari/)
	}
	//alert(Browser.IE_());
	if (Browser.IE) {
		if ("IE6" == Browser.IE_()) {
			alert("请使用IE6以上的版本");
			var re = window.open("", "_self");
			re.close();
		}

	} else {
		alert("请使用IE浏览器！");
		var re = window.open("", "_self");
		re.close();
	}

		function unRegHelp(){
			Ext.onReady(function(){
			Ext.MessageBox.alert('帮助','未登记企业登录：还没有在工商机关登记的经营者登录');
		});}
		
