<script type="text/javascript">
    $(function () {

        $('#change_search_model').click(function () {
            if ($('.model_list').css('display') != 'block') {
                $('.model_list').css("top", ($('.model_project').offset().top + $('.model_project').height()+7) + "px")
                .css("left", ($('.model_project').offset().left-17) + "px").slideDown(500);
                try { clearTimeout(timer); } catch (e) { }
            } else {
                $('.model_list').mouseleave();
            };
        })

        $('.model_list').mouseleave(function () {
            timer = setTimeout(function () {
                $('.model_list').slideUp(100);
            }, 500);
        })

        $('.model_list a').click(function () {
            $('#change_search_model').text($(this).text())
            $('.model_list').slideUp(100);
            try { clearTimeout(timer); } catch (e) { }
        })

        $('.model_search_click').click(function () {
            var model = checkSearchModel();
            doModelSearch(model[0], model[1]);
        })

        $('.model_search_gjclick').click(function () {
            var model = checkSearchModel();
            location.href = '/search.aspx?Isgj=1&Type='+model[0];
        })

        var checkSearchModel = function () {
            var model = $('#change_search_model').text();
            switch (model) {
                case '政务公开':
                    return [1, 1];
                case '文章内容':
                    return [2, 1];
                case '办事服务':
                    return [3, 1];
            }
        }

        var doModelSearch = function (modelId, searchType) {
            var searchValue = $('.model_search').val();
            if (searchValue == null || searchValue == '') {
               location.href = '/search.aspx';
                return;
            }

            switch (modelId) {
                case 1:
                    location.href = '/search.aspx?Type=' + modelId + '&Keyword=' + (searchValue);
                    break;
                case 2:
                    location.href = '/search.aspx?Type=' + modelId + '&Keyword=' + (searchValue);
                    break;
                default:
                    location.href = '/search.aspx?Type=' + modelId + '&Keyword=' + (searchValue);
                    break;
            }
        }
    });
</script>
      
