<script type="text/javascript">
    function ShowCheck(index) {
        $(".block_2 .tit_tab a").each(function (i) {
            if (index == i) {
                $(this).addClass("cur");
                $("#shk" + i).show();
                $("#span_shk" + i).show();
            } else {
                $(this).removeClass("cur");
                $("#shk" + i).hide();
                $("#span_shk" + i).hide();
            }
        });
    }
</script>

