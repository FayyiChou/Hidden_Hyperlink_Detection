<script>

var jq = jQuery.noConflict();

jQuery(function(jq){

var classifyBtn = jq("#j_classify_btn"),

classifyUl = jq("#j_classify_ul"),

classifyLi = classifyUl.find("li"),

flag = false;



/* 隐藏显示搜索下拉框 */



var fnShow = function(event){

event.stopPropagation();

if(flag == false){

classifyUl.slideDown(120);	

flag = true;

}

else{

classifyUl.slideUp(120);	

flag = false;

}

}

classifyBtn.bind("click",fnShow);

classifyLi.each(function(index){

var j_this = jq(this);

j_this.mouseover(function(){

j_this.addClass("hover");

}).mouseout(function(){

j_this.removeClass("hover");

}).click(function(event){

event.stopPropagation();

classifyBtn.html(j_this.html());

classifyUl.slideUp(120);

flag = false;

});

});

jq(document).click(function(){

if(flag){

classifyUl.slideUp(120);

flag = false;

}

});



/* 隐藏显示回复框 */

var fnReply = function(){

jq(this).parent("p").siblings(".reply_m").slideToggle(100);

return false;

}

var replayBtn = jq(".reply_btn");

replayBtn.bind("click",fnReply);

});

</script>


