    $(document).ready(function () {
        //url获取参数
        var sid = GetQueryString("id");
       // getuserfind(sid, 1, 1);
     getuserfind(sid);
     getuserlist(sid, 1, 1);
        //隐藏最后两个
        $(".m-yd-2").hide();
        $(".m-yd-3").hide();
        //绑定
        var x = $(".m-nav ul li a");
        for (var i = 0; i < x.length; i++) {
            (function (num) {
                x[i].onclick = function () {
                    if (num == 0) {
                        editstyle(num);
                        if ($(".m-list3").is(":hidden")) {
                            $(".m-list3").show();
                        }
                        if ($(".m-yd-2").is(":visible")) {
                            $(".m-yd-2").hide();
                        }
                        if ($(".m-yd-3").is(":visible")) {
                            $(".m-yd-3").hide();
                        }
                    }
                    if (num == 1) {
                        editstyle(num);
                        if ($(".m-list3").is(":visible")) {
                            $(".m-list3").hide();
                        }
                        if ($(".m-yd-2").is(":hidden")) {
                            $(".m-yd-2").show();
                        }
                        if ($(".m-yd-3").is(":visible")) {
                            $(".m-yd-3").hide();
                        }
                    }
                    if (num == 2) {
                        editstyle(num);
                        if ($(".m-list3").is(":visible")) {
                            $(".m-list3").hide();
                        }
                        if ($(".m-yd-2").is(":visible")) {
                            $(".m-yd-2").hide();
                        }
                        if ($(".m-yd-3").is(":hidden")) {
                            $(".m-yd-3").show();
                        }
                    }
                }
            })(i);
        }
        $("#page_net").click(function () {
            var pageindex = $(".m-yd tbody tr").length;
            getuserlist(sid, pageindex == 0 ? 1 : pageindex + 1, 2);
        });
    });
function getuserfind(sid, type) {
        var reg = /^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$/;
        var reg1 = /^[0-9]*[1-9][0-9]*$/;
        if (!reg.test(sid)) {
            alert("请输入有效的参数");
            return false;
        }
        $.ajax({
            type: "GET",
             url: "http://jifen.91huayi.com/home/Getuserfind",
            data: { sid: sid, zip: Math.random() },
            cache: false,
            error: function () {
                //alert("程序出错,请联系管理员.");
                return false;
            },
            dataType: "jsonp",
            jsonp: 'callback',
            success: function (msg) {
                if (msg != null) {
                    if (msg.success == true) {
                        if (msg.isnull != true)
                        {
                            //头像
                            if (msg.img != null && msg.img != "") {
                                $(".g-sd1-1 img").attr("src", msg.img);
                            }
                            //加载用户信息
                            $(".g-sd1-2").html(msg.name != null && msg.name != "" ? msg.name : "");
                            $(".g-sd1-3 ul li:eq(0) span").html(msg.yt_no != null && msg.yt_no != "" ? msg.yt_no : "");
                            $(".g-sd1-3 ul li:eq(1) span").html(msg.dept_name != null && msg.dept_name != "" ? msg.dept_name : "");
                            $(".g-sd1-3 ul li:eq(2) span").html(msg.title_name != null && msg.title_name != "" ? msg.title_name : "");
                            $(".g-sd1-3 ul li:eq(3) span").html(msg.city_name != null && msg.city_name != "" ? msg.city_name : "" + "；" + msg.county_name != null && msg.county_name != "" ? msg.county_name : "");
                            $(".g-sd1-3 ul li:eq(4) span").html(msg.hospital_name != null && msg.hospital_name != "" ? msg.hospital_name : "");
                            //加载用户积分详情
                            $(".f-cb li:eq(0) div:eq(1) p").html(msg.zf + "个");
                            $(".f-cb li:eq(1) div:eq(1) p").html(msg.dj);
                            $(".f-cb li:eq(2) div:eq(1) p").html(msg.zc);
                            $(".f-cb li:eq(3) div:eq(1) p").html(msg.dl);
                        }
                        else
                        {
                            return false;
                        }
                    }
                    else {
                        alert(msg.message);
                        return false;
                    }
                }
            }
        });
    }
    function getuserlist(sid, startnumber, type) {
        var reg = /^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$/;
        var reg1 = /^[0-9]*[1-9][0-9]*$/;
        if (!reg.test(sid)) {
            alert("请输入有效的参数");
            return false;
        }
        if (!reg1.test(startnumber)) {
            alert("请输入有效的参数");
            return false;
        }
        $.ajax({
            type: "GET",
      url: "http://jifen.91huayi.com/home/Getuserlist",
 
            data: { sid: sid, startnumber: startnumber, zip: Math.random() },
            cache: false,
            error: function () {
                //alert("程序出错,请联系管理员.");
                return false;
            },
            dataType: "jsonp",
            jsonp: 'callback',
            beforeSend: function () {
                $("#page_net img").attr("src", "http://www.91huayi.com/huayi_yidou/images/page_loading.gif");
            },
            success: function (msg) {
                if (msg != null) {
                    if (msg.success == true) {
                        if (msg.isnull != true) {
                            $("#totalsa").val(msg.totalsa);
                            //加载积分历史列表
                            if (msg.list != null && msg.list != "") {
                                var str = "";
                                $.each(msg.list, function (i, item) {
                                    str += "<tr><td class='co-1'>" + formatdate(item.time_, "yyyy.MM.dd  hh.mm") + "  </td><td class='co-2'>" + item.rulename + "</td><td class='co-3'>" + item.integra + "</td></tr>";
                                });
                                $(".m-yd tbody").append(str);
                                //获取又有li总数和总数比较
                                if (msg.totalsa <= $(".m-yd tbody tr").length) {
                                    $("#page_net img").attr("src", "http://www.91huayi.com/huayi_yidou/images/page_end.jpg");
                                }
                                else {
                                    $("#page_net img").attr("src", "http://www.91huayi.com/huayi_yidou/images/page_nxt.jpg");
                                }
                            }
                            else {
                                $("#page_net img").attr("src", "http://www.91huayi.com/huayi_yidou/images/page_end.jpg");
                                if (type == 2) {
                                    return false;
                                }
                                else {
                                    $(".m-yd tbody").after("<div>暂无历史信息<div>");
                                }
                            }
                        }
                        else {
                            alert("暂未获取有效数据，请稍后再试");
                            $(".m-table div").remove();
                            $(".m-yd tbody").after("<div>暂无历史信息<div>");
                            $("#page_net img").attr("src", "http://www.91huayi.com/huayi_yidou/images/page_end.jpg");
                            return false;
                        }
                    }
                    else {
                        alert(msg.message);
                        return false;
                    }
                }
            }
        });
    }
    //处理json日期
    function formatdate(datestr, formatstr) {
        //g 全局 i不区分大小写
        var date = new Date();
        date.setTime(datestr.replace(/\/Date\((\d+)\)\//gi, '$1'));
        return date.Format(formatstr);
        //return date.Format("yyyy.MM.dd  hh.mm");
    }
    // 对Date的扩展，将 Date 转化为指定格式的String   
    // 月(M)、日(d)、小时(h)、分(m)、秒(s)、季度(q) 可以用 1-2 个占位符，   
    // 年(y)可以用 1-4 个占位符，毫秒(S)只能用 1 个占位符(是 1-3 位的数字)   
    // 例子：   
    // (new Date()).Format("yyyy-MM-dd hh:mm:ss.S") ==> 2006-07-02 08:09:04.423   
    // (new Date()).Format("yyyy-M-d h:m:s.S")      ==> 2006-7-2 8:9:4.18   
    Date.prototype.Format = function (fmt) { //author: meizz   
        var o = {
            "M+": this.getMonth() + 1,                 //月份   
            "d+": this.getDate(),                    //日   
            "h+": this.getHours(),                   //小时   
            "m+": this.getMinutes(),                 //分   
            "s+": this.getSeconds(),                 //秒   
            "q+": Math.floor((this.getMonth() + 3) / 3), //季度   
            "S": this.getMilliseconds()             //毫秒   
        };
        if (/(y+)/.test(fmt))
            fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
        for (var k in o)
            if (new RegExp("(" + k + ")").test(fmt))
                fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
        return fmt;
    }
    //utl获取参数
    function GetQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]); return null;
    }
    //替换样式
    function editstyle(x)
    {
        var li = $(".m-nav ul li");
        $.each(li, function (i) {
            if (i == x) {
                $(".m-nav ul li:eq(" + i + ")").attr("class", "first");
            }
            else {

                $(".m-nav ul li:eq(" + i + ")").removeClass("first");
            }
        });
    }
</script>
