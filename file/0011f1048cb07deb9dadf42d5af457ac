<script type="text/javascript">

$(function(){
	var tNo =  document.getElementById("code").value.substring(0,4);
	if (tNo==null||tNo==''){
		tNo = "ID00";
	}
	$("div#menuUl ul li").each(function(i,obj){
		 var liId=$(obj).attr("id");
		 if (liId == tNo){
			 $(obj).attr("class","bgImg");
		 } else{
			 $(obj).attr("class","bgImg1");
		 }
	});	
});
	
 function fnOver(obj){
	 var tNo =  document.getElementById("code").value.substring(0,4);
	 if (tNo==null||tNo==''){
	        tNo = "ID00";
	 }
	 var id1=$(obj).attr("id");
	 if(id1 != tNo){
		 $(obj).attr("class","bgImg");
	 }
}

 function fnOut(obj){
	var tNo =  document.getElementById("code").value.substring(0,4);
	if (tNo==null||tNo==''){
        tNo = "ID00";
    }
	var id1=$(obj).attr("id");
    if(id1 != tNo){
        $(obj).attr("class","bgImg1");
    }
}  
</script>
