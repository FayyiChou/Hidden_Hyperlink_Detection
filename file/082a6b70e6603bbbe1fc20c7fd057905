<script>
    function isTiming() {
        var value = document.getElementById('timing_action_save_do_top').value;
        $(":hidden[name='timing']").prop('value', '1');
        if(value == '预约置顶') {
            $(":submit[id='timing_action_save_do_top']").prop('value','取消预约');
            $(":hidden[name='cancel_timing']").prop('value','0');
        } else {
            $(":submit[id='timing_action_save_do_top']").prop('value','预约置顶');
            $(":hidden[name='cancel_timing']").prop('value','1');
        }
    }
    function normalDoTop() {
        $(":hidden[name='timing']").prop('value', '0');
    }
</script>
