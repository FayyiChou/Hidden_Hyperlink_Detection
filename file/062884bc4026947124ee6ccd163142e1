<script>
    var u = navigator.userAgent;
    var verios = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/);
    var ua = navigator.userAgent.toLowerCase();

    var sendLog = function(){

    };

    if(verios){
        if(ua.match(/MicroMessenger/i)!="micromessenger") {
            window.location.href="https://itunes.apple.com/us/app/id737310995";
        } 
        else{ 
            document.getElementById("weixintips").style.display = "none";
            document.getElementById("refresh-tip").style.display = "none";
            document.getElementById('iosspecial').style.display = 'block';
            document.getElementsByTagName('body')[0].style.background = '#ffd705';
        }
    }
    else{
        if(ua.match(/MicroMessenger/i)!="micromessenger") {
            document.getElementById("weixintips").style.display = "none";
            document.getElementById("refresh-tip").style.display = "block";
            document.getElementById('iosspecial').style.display = 'none';
            document.getElementsByTagName('body')[0].style.background = '#f8f8f8';
            
            var downloadUrl = "/download?channel=1038&&scheme=https";
            if(/baidubrowser/i.test(ua)){ //如果是百度浏览器
                var version = ua.match(/android\s([\d.]*)/);
                if(version && version[1].charAt(0) < 6){    //并且安卓版本小于6.0
                    downloadUrl = "/download?channel=1038&&scheme=http";
                }
            }

            $.ajax({
                url: 'https://s3.meituan.net/v1/mss_c4375b35f5cb4e678b5b55a48c40cf9d/waimai-mfe-bee/ok.apk',
                type: 'get',
                success: function(){
                  Analytics('event', {
                    nm : 'MGE',
                    val : {
                      act : 'download_apk',
                      cid : 'app_download',
                      lab : 'https',
                      val : '1'
                    }
                  });
                  location.href = downloadUrl;
                },
                error: function(){
                  Analytics('event', {
                    nm : 'MGE',
                    val : {
                      act : 'download_apk',
                      cid : 'app_download',
                      lab : 'https',
                      val : '0'
                    }
                  });
                  // location.href = '/download?channel=1038&&scheme=http';
                  location.href = downloadUrl;
                }
              });
        }
        else{
            document.getElementById("weixintips").style.display = "block";
            document.getElementById("refresh-tip").style.display = "none";
            document.getElementById('iosspecial').style.display = 'none';
            document.getElementsByTagName('body')[0].style.background = '#f8f8f8';
