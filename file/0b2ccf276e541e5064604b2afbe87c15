  <script>
$(document).ready(function(){
var slider = $('.bxslider').bxSlider({
	
	
autoHover: false, 
infiniteLoop: true,  	
	
  auto: true,
  mode: 'fade',
slideWidth: 2000,
  autoControls: true,
  controls: true,
  pager: false

});

	//auto
	$(".bx-wrapper a").click(function() {
	 var thumbIndex = $('.bx-wrapper a').index(this);
	 slider.goToSlide(thumbIndex);
	 slider.startAuto();
	 $('.bx-wrapper a').removeClass('active');
	 $(this).addClass('active');
	   return false;
  }); 



  });
    function iFrameHeight() {   
		var ifm= document.getElementById("wb_main");   
		var subWeb = document.frames ? document.frames["wb_main"].document : ifm.contentDocument;   
		if(ifm != null && subWeb != null) {
		   ifm.height = subWeb.body.scrollHeight;
		}   
} 
    function pic_iFrameHeight() {   
		var ifm= document.getElementById("wb_pic");   
		var subWeb = document.frames ? document.frames["wb_pic"].document : ifm.contentDocument;   
		if(ifm != null && subWeb != null) {
		   ifm.height = subWeb.body.scrollHeight;
		}   
}
$(function() {
	$(window).scroll(function() {
		
		if($(this).scrollTop() != 0) {
			$('#toTop').fadeIn();	
		} else {
			$('#toTop').fadeOut();
		}
	});
 
	$('#toTop').click(function() {
		$('body,html').animate({scrollTop:0},500);
	});	
});
  </script>
<style>
#toTop {position: fixed;bottom: 10px;right: 10px;cursor: pointer;display: none;z-index:99999;}
