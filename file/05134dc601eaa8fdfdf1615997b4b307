<script type="text/javascript">
  $(document).ready(function() {
     // Отправка AJAX запроса
    $('.form form').submit(function(e) {      // Обработка отправки данных формы
     e.preventDefault();
       e.stopImmediatePropagation();                  // Сброс стандартного обработчика формы
       formData = $(this).serialize() + "&web_form_submit=Отправить";   // Сохраняем массив введенных данных включая значение кнопки "Отправить", без этого компонент Битрикса не примет данные
       
       console.log("start post");

       $.ajax({
         url: $(this).attr('action') + '?AJAX_REQUEST=Y',
         type: 'POST',
         dataType: 'html',
         data: formData,
         success: function(response) {
          console.log("success");
          var message = $(response).find('.grey_form').html(); 
          if($(message).find(".errortext").length) {                        // Если в этих данных есть элементы для показа (ошибка или сообщение)
           var fancy_html = $(message).find(".errortext").html();
             //$.fancybox({content: fancy_html});
             //$(".form").append(fancy_html);
             $.fancybox.update();
           } else {
            $.fancybox({
              content: '<h2>Спасибо!</h2><p>Персональный менеджер свяжется<br>с вами в ближайшее время!</p>',
              padding   : [33,30,33,37],
              maxWidth  : 400,
              autoHeight  : true,
              fitToView : false,
              width   : '280px',
              height    : 'auto',
              autoSize  : false,
              closeClick  : false,
              openEffect  : 'none',
              closeEffect : 'none',
              afterLoad  : function(){
                setTimeout( function() {$.fancybox.close(); },10000);
              }
            });
             // Если данные формы приняты сбросим все поля
             $('.form input[type="text"]').val("");
             $('.form input[type="email"]').val("");
             $('.form textarea').val("");

           }
         }
       });

return false;
});
});

</script>

