        <script type="text/javascript">
$(document).ready(function() {
    var banner_docliner = getCookie("banner_docliner");
    if (banner_docliner == "false") {
        $(".cntnr-bnr").hide();
    } else {
        $(".cntnr-bnr").show();
    }
    $('.cntnr-bnr .close-button').click(function(){
        $('.cntnr-bnr').slideUp('slow');
        setCookie("banner_docliner","false");

    });
});
</script>

