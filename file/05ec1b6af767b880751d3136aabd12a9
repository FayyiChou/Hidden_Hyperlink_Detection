       <input type="button" name="btn" value=" Goto " onclick="changePage(document.getElementById('page').value)" class="input" /> 
       <script type="text/javascript">
var pageTotal = 836;
function changePage(pn)
{
	if (pn > pageTotal) {
		alert('超出最大页码范围');
		return;
	}
	var url = '/network';
	if (pn == 1){
		url += '_1.html';
	}else{
		url += '_' + pn + '.html';
	}
	location.href = url;
}
</script> 
