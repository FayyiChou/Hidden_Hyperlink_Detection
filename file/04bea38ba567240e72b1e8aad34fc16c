    <script type="text/javascript">
        //图片切换

        $(function () {
            showImg(0);
            var index = 1;
            var a = 0;
            var b = $('.banner ul li').size();
            var z = $('.slide-controls');
            for (var j = 0; j < b; j++) {
                z.append('<span>' + '</span>');
            }
            $(".slide-controls>span").eq(0).addClass("curr")
            //$(".banner ul li").hide();
            $('.slide-controls>span').hover(
                                    function () {
                                        if (MyTime) { clearInterval(MyTime); }

                                        showImg($('.slide-controls>span').index(this));
                                        index = $('.slide-controls>span').index(this) + 1;
                                        if (index == b) { index = 0; }
                                    },
                                    function () {
                                        MyTime = setInterval(function () {
                                            showImg(index);
                                            index++;
                                            if (index == b) { index = 0; }
                                        }, 5000);
                                    });
            var MyTime = setInterval(function () {
                if (b > 1) {
                    a = index;
                    showImg(index);
                    index++;
                    if (index == b) { index = 0; }
                }
            }, 5000);

        });
        function showImg(i) {
            $(".banner ul li").fadeOut(1000);
            $(".banner ul li").eq(i).fadeIn(1000);
            $(".slide-controls>span").eq(i).addClass("curr").siblings().removeClass("curr");
        }

    </script>

