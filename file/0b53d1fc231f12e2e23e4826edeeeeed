</div><div class="bab_opt_mod bab_opt_mod_22 customcode" data-floornum="22" data-template="customcode" data-moduleid="810835">
	<textarea class="template_code hide">		&lt;style&gt;
        .m-subnav {position:fixed;left:50%;bottom:0;-webkit-transform:translateX(-50%);transform:translateX(-50%);width:100%;max-width:600px;height:auto;z-index:1000;background-color:#fff;}
        .m_subnav_con {position:absolute;width:100%;left:0;top:0;height:100%;}
        .m-subnav a {float:left;width:33.33%;height:100%;text-indent:-999px;overflow:hidden;}
        .general_purpose.goto_top {bottom:55px!important;}
    &lt;/style&gt;
&lt;div class="m-subnav" id="mSubNav"&gt;
    &lt;div class="m_subnav_con" id="mSubnavCom" data-html="false"&gt;&lt;/div&gt;
    &lt;img id="mImgHei" width="100%" nolazy="" height="auto"&gt;
&lt;/div&gt;
&lt;script&gt;
    !function(){
        try{
            //配置链接地址
            var subnavLink = [
                        "//sale.jd.com/app/act/ZR7K0yeWmt2p.html",//链接1
                        "//pro.m.jd.com/mall/active/2uYvB35XrEhbc2ZeTYzY5wU5DzjS/index.html",//链接2
                        "//sale.jd.com/app/act/wgjl8zuDJv.html"//第N个链接
                    ],
            imgSrc = "https://m.360buyimg.com/babel/jfs/t4651/151/1206613123/21123/bf646c9a/58da231aNaa7d4d3b.jpg";//配置图片地址

            //创建构造函数
            function LinkChange(link,imgs){
                this.link = link || [];
                this.imgs = imgs || "11";
                this.mImgHei = document.getElementById("mImgHei");
                this.mSubnavCom = $("#mSubnavCom");

                if(this.mSubnavCom.attr("data-html") == "false"){
                    this.changeWQLink();
                    this.addDiv();
                }
            };
            LinkChange.prototype.addDiv = function(){
                var that = this;
                //创建撑高度的空div
                var emptyHtm = document.createElement("div");

                //判断图片是否加载完成，如加载完成，则取图片高度赋给底部div
                this.mImgHei.onload = function(){
                    if(1 == this.complete){
                        that.mImgHei.onload = null;
                        emptyHtm.style = "width:100%;height:"+$(that.mImgHei).height()+"px";
                        document.body.appendChild(emptyHtm);
                    }
                };
                that.mImgHei.src = this.imgs;
            };
            LinkChange.prototype.changeWQLink = function(){

                var mSubHref = window.location.href,
                        mSubHrefLower = mSubHref.toLowerCase(),
                        subnavCon="",
                        len = this.link.length,
                        arr1 = /\/wq\//,
                        arr2 = /\/dev\//,
                        arr3 = /\/mall\/|\/app\/|\/m\//,
                        arr4 = /\/pro.m.jd.com\//,
                        arr5 = /https:\/\/.*\.jd\.com|http:\/\/.*\.jd\.com|\/\/.*\.jd\.com/g;

                for(var i= 0;i&lt;len;i++){

                    //判断当前页面为微信手Q，切配置的链接不是微信手Q链接
                    if(mSubHrefLower.search(arr1) != -1 &amp;&amp; this.link[i].search(arr1) == -1){

                        //替换为微信手Q的链接
                        if(this.link[i].search(arr2) != -1){
                            this.link.splice(i,1,this.link[i].replace(arr2,"/wq/dev/"))
                        }else if(this.link[i].search(arr3) != -1){
                            this.link.splice(i,1,this.link[i].replace(arr3,"/wq/"))
                        }

                        //替换域名
                        if(this.link[i].search(arr4) != -1){
                            this.link.splice(i,1,this.link[i].replace(arr4,"/h5.m.jd.com/"))
                        }
                    }

                    //判断当前页面的链接是否在配置的链接里，如是，则赋空链接给导航
                    if(this.link[i].toString().toLowerCase().replace(arr5,"") == window.location.pathname.toLowerCase()){
                        this.link[i] = "javascript:;"
                    }

                    //创建底部链接
                    subnavCon += "&lt;a class=\"J_ping\" report-eventparam=\"00007588_"+i;
                    subnavCon += "\" report-eventlevel=\"3\" report-eventid=\"MShoppinSpree_BottomNavigation\" href=\""+this.link[i];
                    subnavCon += "\" target=\"_blank\" style=\"width:"+100/len;
                    subnavCon += "%\" report-pagename=\""+mSubHref+"\"&gt;会场&lt;\/a&gt;";
                }

                this.mSubnavCom.prepend(subnavCon);
                this.mSubnavCom.attr("data-html","true");

            };
            new LinkChange(subnavLink,imgSrc);
        }catch(e){}
    }();
&lt;/script&gt;	
	</textarea>
