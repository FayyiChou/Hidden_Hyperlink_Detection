<script type="text/javascript">
    $(function(){
	var activityBox=setTimeout(function(){
	   $('#activityBox').slideToggle(500);		
	   clearTimeout(activityBox);
	},1000);
	$(".start").click(function(){
       $(this).fadeOut("slow");
    });

	


	var lt, rt, pageNo = 0, interval = 5000;

       var aa = 1;

   $('#activityBtn').bind('mouseover', function () {
            $('#activityBox').slideDown(500);
        });
      $(document).ready(function () {
          $("#activityBox").hover(function () {
              aa++;
          },
             function () {
                 var ax = $("#activityBox");
                 if (aa % 2 == 1) {
                     $('#activityBox').slideDown(500);
                     aa++;
                 }
                 else {
                       $('#activityBtn').unbind();
                     aa++;
                     $('#activityBox').slideUp(500);
                     setInterval(function () {
                         $('#activityBtn').bind('mouseover', function () {
                             $('#activityBox').slideDown(500);
                         });
                     }, 1000);

                 }
             });
      });
	
	
	

	// 活动专区内容的分页显示
	function move(right, step){
		var atcs = $('#activities'), items = atcs.find('>div.item');
		if(right){
			pageNo = (pageNo + step) % 5;
			items.filter(':lt(' + (step * 3) + ')').animate({
				'margin-left': -300
			}, 500, function(){					
				atcs.find('>div.item:last').after($(this).detach().css('margin-left', 25));
			});
			
		}else{
			pageNo = (pageNo + 5 - step) % 5;
			items.filter(':gt(' + (items.length - step * 3 - 1) + ')').detach()
				.css('margin-left', -300).insertBefore(atcs.find('>div.item:first'))
				.animate({'margin-left': 25}, 500);
		}
		$('#pageBox a[data-pn=' + pageNo + ']').addClass('cur').siblings().removeClass('cur');		
	}
	
	$('#prevBtn').bind('click', function(){
		move(false, 1);
		return false;
	});
	
	$('#nextBtn').bind('click', function(){
		move(true, 1);
		return false;
	});
	
});
</script>


