<script type="text/template" id="provinceTemplate">
        <dl class="tt-list">
            { for(var province in cityJsonV2.ProvinceList){ }
            <dt><span>{=province}</span></dt>
                {for(var i=0;i<cityJsonV2.ProvinceList[province].length;i++){}
                <dd>
                    <a  href="#" {= cityJsonV2.ProvinceList[province][i].children ? '':'data-spell='+cityJsonV2.ProvinceList[province][i].engname}  {=cityJsonV2.ProvinceList[province][i].children ? 'data-action="city"':''}  data-id="{=cityJsonV2.ProvinceList[province][i].id}" class="{=cityJsonV2.ProvinceList[province][i].children ? '':'nbg'} {= cityJsonV2.ProvinceList[province][i].id.toString() == current_province.toString()  ? 'current':''}">
                        <p>{=cityJsonV2.ProvinceList[province][i].name}</p>
                    </a>
                </dd>
               {}}
            {}}
        </dl>
    </script>
    <!--省模板end-->
