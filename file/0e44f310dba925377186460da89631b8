                <li class="root"><a>{=provinceName}</a></li>
                {for(var i=0;i  <citys.length;i++){}
                    <li><a data-id="{=citys[i].id}" data-spell="{=citys[i].engname}" class="{= citys[i].id.toString() == current_city.toString()  ? 'current':''}">{=citys[i].name}</a></li>
                {}}
