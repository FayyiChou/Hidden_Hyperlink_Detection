		function lianjie(selObj) {
			window.open(selObj.options[selObj.selectedIndex].value);
		}
		$(function(){
			$("ul").find("li").find("a").each(function(){
				var t = $(this).attr("title");
				if(undefined!=t&&t.indexOf("台风")>-1){
					$(this).addClass("taifen_css");
				}
			});
		});
