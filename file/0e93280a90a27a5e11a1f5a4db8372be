		$("a.tabMore").click(function() {
			var $li = $(this).parent().find("li.on");
			location.href = encodeURI(encodeURI($li.attr("href")));
		});

		$("#more_quick_div").hover(function() {
			$("#quickMoreBar_div").show();
		}, function() {
			$("#quickMoreBar_div").hide();
		});
		
	});
