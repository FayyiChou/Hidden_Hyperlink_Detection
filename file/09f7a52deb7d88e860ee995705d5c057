        <script type='text/javascript'>

jQuery(function(){

  var nav=jQuery('.hotpro_header');
  var lis=jQuery('ul>li',nav);
  var cons=jQuery('.new_hotpro_con');
  lis.mouseover(function(){
    var se=jQuery(this);
    var num=se.index();
    cons.hide();
    cons.eq(num).show();
  });

})
/*-------------------
jQuery('.hotpro_header ul li').on('mouseover', function(){
	 var currentkey = 0;
    var lis = jQuery('.hotpro_header ul li');
    for(var i=0;i<lis.length;i++){
    	if(lis[i] == this){
    		currentkey = i;
    	}
    }
    var goodscontent = jQuery('.new_hotpro_con');
    for(var i=0;i<goodscontent.length;i++){
    	
    	if(i == currentkey){
    		goodscontent[i].show();
    	}else{
    		goodscontent[i].hide();
    	}
    }
});
