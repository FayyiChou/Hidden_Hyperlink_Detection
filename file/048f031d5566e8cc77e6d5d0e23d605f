<script>
	/* 改变区域设置商圈 */

function setArea1Return(request){
	var str=request;
	$("#AreaDiv1").html(str);
}
function changeArea1(){
		document.jdfm.Aid.value = 'all';
		var ps=document.jdfm.Tid.value;
		var url="AjaxAction/QueryAreaAction.jsp";
		$.post(url,{town_id:ps},setArea1Return);
	
}

function changeArea2(){
		document.cyfm.Aid.value = 'all';
		var ps=document.cyfm.Tid.value;
		var url="AjaxAction/QueryAreaAction.jsp";
		$.post(url,{town_id:ps},setArea2Return);	
}
function setArea2Return(request){
	var strr=request;
	$("#AreaDiv2").html(strr);
}

function getUrl(){
	var url = "";
	for(var i = 0; i < document.form1.project.length; i++){
		if(document.form1.project[i].checked == true){
			url = document.form1.project[i].value;
		}
	}
	if(url == ''){
		alert("请选择一项!");
			document.form1.project[0].focus();
	}else{
		window.open("/tourguide/guide.jsp?ProductNo="+url);
	}
}
</script>

