                <div id="shtr2fenye" align="right">
					<img id="zuob" src="images/zuob.gif" align="absmiddle" style="cursor:pointer" onclick="trunPreviousPage();" />
					<span style="font-size:14px">
						<span id="currentPage">1</span>/3
					</span>
					<img id="youbb" src="images/youb.gif" align="absmiddle" style="cursor:pointer" onclick="turnNextPage();" />
				</div>
<script type="text/javascript">
var maxSize = 9;
var maxPage = 3;
var remenlineNodes;
var crp = 1;
function trunPreviousPage() {
	var currentPage = document.getElementById("currentPage").innerHTML;
	turnPage(parseInt(currentPage) - 1);
}
function turnNextPage() {
	var currentPage = document.getElementById("currentPage").innerHTML;
	if(currentPage==maxPage){
		turnPage(1);
	}else{
		turnPage(parseInt(currentPage) + 1);
	}
}
function turnPage(index) {
	if (index < 1 || index > maxPage) {
		return;
	}
	for (var i = 0; i < maxSize; i++) {
		$(".remenline").eq(i).css("display", "none");
	}
	for (var i = (index - 1) * 3; i < index * 3; i++) {
		if (i >= 0 && i < maxSize) {
			$(".remenline").eq(i).css("display", "block");
		}
	}
	crp = index;
	document.getElementById("currentPage").innerHTML = index;
}
window.setInterval("turnPageAuto()", 5000);
function turnPageAuto() {
	turnPage(crp);
	if (crp == maxPage) {
		crp = 1;
	} else {
		crp++;
	}
}
</script>	
            </div>
        </div>
        

