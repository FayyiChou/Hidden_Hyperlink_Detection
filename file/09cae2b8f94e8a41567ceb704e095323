  	<script language="javascript">
  		function show_swapmenu(th)
  		{
  	  		var obj=$(th).find('a');
  	  		var offset=obj.offset();
  	  		var sm=$("#swapmenu");
  	  		sm.css('top',offset.top+obj.height());
  	  		sm.css('left',offset.left-( (sm.width()-obj.width())/2 ));
  			$('#swapmenu').show();
  	  	}
  	  	function hide_swapmenu()
  	  	{
  	  	  	$('#swapmenu').hide();
  	  	}
  	</script>
