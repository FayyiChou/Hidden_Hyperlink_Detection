<script language="javascript">

$(function(){
	$('#uloginfo').ajaxForm({
		success:       complete,  // post-submit callback
        dataType: 'json'
    });

	function complete(data){
		if(data.status==1)
		{	
			document.location.href=data.data+"edit";	
		}
		else
		{
			alert(data.info);
			$("#loginvc").val('');
		}
	}	
});



function verc(imgid)
{
	var src="/user/getlogincode.html";
	var ext=".html";
	src=src.substr(0,src.length-ext.length)+"/f/"+Math.random();
	$("#"+imgid).attr("src",src);
	$("#"+imgid).show();
	return false;
}

function fanlilogin()
{	
	$("#divsc").show();
}
