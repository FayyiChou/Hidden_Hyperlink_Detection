<script language="javascript">
var style_i=0;
var font_i=0;
var bg_i=0;
var mobg_i=0;
 //选择颜色
function ucolor(count){
   for(var i = 0;i<5;i++){
     if(i == count){
        var colorv = document.getElementById("color"+i).className;
        if(colorv.indexOf("2") != -1){
         colorv = colorv.substring(0,colorv.length-1);
        }
        document.getElementById("color"+i).className = colorv+"2";
        style_i=i;
     }else{
       var colorv = document.getElementById("color"+i).className;
       if(colorv.indexOf("2") != -1){
         colorv = colorv.substring(0,colorv.length-1);
       }
       document.getElementById("color"+i).className = colorv;
     }
   }
 }
 //选择图片
 function bcolor(count){
    for(var i=0;i<14;i++){
       if(i == count){
         document.getElementById("bg"+i).style.borderColor = "#f00";
         var bgc = document.getElementById("bg"+i).className;
         bg_i=i;
       }else{
         document.getElementById("bg"+i).style.borderColor = "";
       }
    }
 }


 //选择模块图片
 function mobcolor(count){
    for(var i=0;i<14;i++){
       if(i == count){
         document.getElementById("mobg"+i).style.borderColor = "#f00";
         var bgc = document.getElementById("mobg"+i).className;
         mobg_i=i;
       }else{
         document.getElementById("mobg"+i).style.borderColor = "";
       }
    }
 }
 
 //风格+背景 存放到cookie里面去
 function setStyleCookie(){
    var format = style_i+"|"+font_i+"|"+bg_i+"|"+mobg_i;
    format = base64encode(format);
    setcookie("FORMAT",format);  //cookie设置成功
    setStyle(style_i,font_i,bg_i,mobg_i);//接下来改变样式值
 }

 function initStyle(style,font,bg,mobg)
 {
	 ucolor(style);
	 bcolor(bg);
	 mobcolor(mobg);
	 setStyle(style_i,font_i,bg_i,mobg_i);
}
 
 
 //改变样式值
function setStyle(style_i,font_i,bg_i,mobg_i){
	
  if(bg_i==0){
  document.body.style.backgroundImage="";
  document.body.style.backgroundRepeat = "repeat-x";
  }else{
   document.body.style.backgroundImage = "url(/Public/home/images/bg/"+bg_i+".gif)"; // 修改背景图片
   document.body.style.backgroundRepeat = "repeat";
   } 
   closebut('models');
   document.getElementById("skin").href="/Public/home/css/face"+style_i+".css";

   var span_module=$("div.module");
   if(mobg_i==0)
   {
       span_module.css('background-image','');
       span_module.css('background-repeat','repeat-x');
   }
   else
   {
  	 span_module.css('background-image',"url(/Public/home/images/mobg/"+mobg_i+".gif)");
       span_module.css('background-repeat','repeat');
 	 }
}
 //恢复默认样式值
function resumeStyle(){
  font_i=0;
  ucolor(0);
	 bcolor(0);
	 mobcolor(0);
	setStyleCookie();
}
function closebut(ids){
	 document.getElementById(""+ids).style.display= "none";
}
function openmodel(ids){
 document.getElementById(""+ids).style.display= "";
}


function disHtml(name){
 window.location.href ="http://"+name+".geihui.net/user/register.html"
}

	
	//切换版本
	function updateStyle(){
		var view=0;
		if(view>=3)
			view=0;
		else
			view=view+1;
		window.location.href="/index"+view+".html";
	}

	
</script>
