<div class="search_link">
	<input id="search_link_key" onfocus="javascript:this.select();" type="text" style="width:120px;height:24px;line-height:24px;vertical-align:middle;" />
	<input type="button" id="search_link_do" value="搜收藏" style="vertical-align:middle;"/>
	<script language="javascript">
	$(function(){
		$("#search_link_key").bind('keypress',function(e){
			var event=$.event.fix(e);
			if(event.keyCode==13)
			{
				$("#search_link_do").click();
			}
		});
		
		
		$("#search_link_do").click(function(){
			var key=$("#search_link_key").val();
			
			if(key=="")
			{
				$("div.t_nr1 a").show();
				$("div.t_nr2 ul li a").show();
			}
			else
			{
				$("div.t_nr1 a").hide();
				$("div.t_nr2 ul li a").hide();
	
				var find=false;
				var nr1_list=$("div.t_nr1 a:contains('"+key+"'),div.t_nr1 a[pinyin*="+key+"]");
				if(nr1_list.length>0)
				{
					find=true;
					var first_link=nr1_list[0];
					var id=$(first_link).parents("div.module").attr("id").replace("line_","");
					$("#mclass_"+id).click();
					
					nr1_list.show();
					ScrollBody(first_link);
					
				}
				
				var nr2_list=$("div.t_nr2 a:contains('"+key+"'),div.t_nr2 a[pinyin*="+key+"]");
				if(nr2_list.length>0)
				{
					nr2_list.show();
					ScrollBody(nr2_list[0]);
				}
			}
				
		});


		
		
	});
	</script>
