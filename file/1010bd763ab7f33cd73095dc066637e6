  <script type="text/javascript">
function resetSize(size)
{
	var n = (10+size*2).toString();
	$('.t_'+n).addClass('t_now').siblings().removeClass('t_now');
	$('#article_content').find('span').css('font-size',n+'px');
}
</script> 
  <script type="text/javascript">
$(document).ready(function() {
	var speed = 500;
	$('#share_article').click(function(event){
		event.stopPropagation();
		var offset = $(event.target).offset();
		$('#Article-Tool').css({'left':offset.left + 'px','top':offset.top+$(event.target).height()+'px'}).show(speed);
	});
	$('#Article-Tool .close').click(function(event){$("#Article-Tool").hide(speed)});
});
</script> 
