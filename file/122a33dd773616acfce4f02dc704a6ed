<script type="text/javascript">
    $(function() {
        $(".placedate .iarr").click(function() {
            $(this).parents(".placedate dd").toggleClass("out").toggleClass("in");
        });
        // 浏览量
        $.ajax('http://s.trip.elong.com/article/view.html?articleid=1651420', {
            'type': 'get',
            'dataType': 'jsonp',
            'success': function(data) {
                if (typeof data != 'undefined') {
                    $('.clicknum').html(data.clicknum);
                    $('.commentnum').html(data.commentnum);
                    $(".zan-total-number").text("(" + data.favournum + ")");
                }
            }
        });
        if ($(document).height() <= $(window).height()) {
            more();
        }
        $(window).scroll(function() {
            more();
        });
        function more() {
            if ($(window).scrollTop() + $(window).height() < $('#footer').offset().top - $('#footer').height()) {
                return false;
            }
            if ($('.pictorial:hidden').size() > 0) {
                $('.pictorial:hidden').eq(0).show();
            } else {
                $('#loading').hide();
            }
        }
    });

</script>
<script type="text/javascript">
    var articleid = 1651420 ;
    $(function() {
        $(".fl-li1").click(function() {
            //var obj = this;
            var if_click = $(".fl-zan").attr("class");
            if (if_click == "fl-icon fl-zan") {

                if (user.uid < 0) {
                    $("body").append("<div class='login-shadow'></div>");
                    $("#popup").fadeIn();
                    window_width = $(window).width();
                    login_width = $("#popup").width();
                    login_left = (window_width - login_width) / 2
                    $("#popup").css({"position": "fixed", "top": 200, "left": login_left, "z-index": 9999});
                    return false;
                }
               
                $.ajax('http://s.trip.elong.com/article/favour.html?articleid=1651420' + '&uid=' + user.uid + "&SessionToken=" + user.SessionToken, {
                    'type': 'get',
                    'dataType': 'jsonp',
                    'success': function(data) {
                        if (data.success) {
                            if (typeof data.favournum != 'undefined') {
                                $(".fl-zan").children(".fl-add-num").fadeIn(10, function() {
                                    $(this).animate({top: "-15px"}, function() {
                                        $(this).fadeOut();
                                    });
                                });
                                $(".fl-zan").addClass("click-after");
                                $(".zan-total-number").text("(" + data.favournum + ")");

                            }
                        } else {
                            $(".fl-zan").children(".fl-after-zan").fadeIn(10, function() {
                                $(this).animate({top: "-15px"}, function() {
                                    $(this).fadeOut(function() {
                                        $(this).css("top", "0px");
                                    });
                                });
                            });

                        }
                    }
                });
            } else {
                $(".fl-zan").children(".fl-after-zan").fadeIn(10, function() {
                    $(this).animate({top: "-15px"}, function() {
                        $(this).fadeOut(function() {
                            $(this).css("top", "0px");
                        });
                    });
                });
            }

        });
        $(".fl-li3").click(function(){
		if($(".login-shadow").length == 0){
			$("body").append("<div class='login-shadow' style='z-index:6666;'></div>");
		}else{
			return;
		}
		$(".H5-share").animate({bottom:"61px"});
	});

	$("#share-close").click(function(){
		$(".login-shadow").remove();
		$(".H5-share").animate({bottom:"-103px"});
	});
    })

</script>


