<script src="/Public/Javascript/jquery.jcountdown1.3.min.js" type="text/javascript"></script>
<script type="text/javascript">
var scrolll,scrollTag,selectTag;
$(function(){
	$(".save_time").countdown({
		date: "March 9,2015",
		htmlTemplate: "还剩<span>%{d}</span>天<span>%{h}</span>小时<span>%{m}</span>分钟",
		onComplete: function( event ){
			$(this).html("活动已经结束，关注下一期！");},
		leadingZero: true,
		direction: "down"
	});
	var f = 0;
	var number = 0;
	$(".mar_r_2px").hover(
		function(){
			number = $(this).attr("title");
			$(".banner_pic a").hide();
			for(var i=1;i<6;i++){
				$("#contrl_"+i).find("div").eq(1).attr("class","banner_bg_bl");
			}
			$(this).find("div").eq(1).attr("class","banner_bg_gr");
			$("#slide_"+number).show();
		},
		function(){
			$(this).find("div").eq(1).attr("class","banner_bg_bl");
		}
	);
	function slide(){
		if(f!=0){
			$(".banner_pic a").hide();
		}
		$(".banner_pic a").eq(f).show();
		for(var i=1;i<6;i++){
			$("#contrl_"+i).find("div").eq(1).attr("class","banner_bg_bl");
		}
		$("#contrl_"+(f+1)).find("div").eq(1).attr("class","banner_bg_gr");
		f++;
		if(f>4){f=0;}
	}
	setInterval(slide,3000);

	//排行榜自动切换
	var obig=document.getElementById("big");
	selectTag = function(showContent,selfObj){
		var tag = document.getElementById("tags").getElementsByTagName("li");
		var taglength = tag.length;
		for(i=0; i<taglength; i++){
			tag[i].className = "";
		}
		selfObj.parentNode.className = "selectTag";
		for(i=0; j=document.getElementById("tagContent"+i); i++){
			j.style.display = "none";
		}
		document.getElementById(showContent).style.display = "block";
	}
	var x=0;
	scrollTag = function(){
		var tags=document.getElementById("tags").getElementsByTagName("a");
		if(x<2){
			x=x+1;
		}else{
			x=0;		
		}
		var tag = document.getElementById("tags").getElementsByTagName("li");
		var taglength = tag.length;
		for(var i=0; i<taglength; i++){
		  tag[i].className = "";
		}
		tags[x].parentNode.className = "selectTag";
		for(var i=0; j=document.getElementById("tagContent"+i);i++){
			j.style.display="none";
		}
		document.getElementById("tagContent"+x).style.display="block";
	}
	scrolll=setInterval(scrollTag,3000);
	$("#big").mouseover(function(){
		clearInterval(scrolll);
	}).mouseout(function(){
		scrolll=setInterval(scrollTag,3000);
	});
});
</script>
