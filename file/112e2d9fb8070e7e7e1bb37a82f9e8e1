function inputFocus(obj,side){
	obj.placeholder = "";
}

function inputBlur(obj,side){
	var placehold="";
	
	if(side=="username"){
		placehold=username_placeholder;
	}else{
		placehold=passwd_placeholder;
	}
		
	obj.placeholder = placehold;
}

function key_value(evt){
var key = window.event ? evt.keyCode : evt.which;	

	if(key == "13"){
		do_login();
	}
}

function close_flash(){
	document.all.show_flash.style.visibility='hidden';
}

function chk_acc(){
	var username=document.getElementById("username").value;
	var passwd=document.getElementById("passwd").value;
	
	top.nametop=username;
	
	var isChceked = document.getElementById("remember").checked;
	if(isChceked && username!=""){
		document.cookie = "username_cookie="+username+";";
	}

	document.getElementById("JE").value = navigator.javaEnabled();
	if(username==""){
		hr_info.innerHTML=top.account;
		document.all.username.focus();
		document.getElementById("err_info").style.display = "";
		return false;
	}
	if(passwd==""){
		hr_info.innerHTML=top.password;
		document.all.passwd.focus();
		document.getElementById("err_info").style.display = "";
		return false;
	}

	return true;
	//document.all.mac.value=top.aps.mac;
	//document.all.ver.value=top.aps.ver;
			
}
var login_version="SERVER";//SERVER or PHP
function do_login(){
	if(login_version=="PHP"){
		document.getElementById("err_info").style.display = "none";
		document.getElementById("LoginForm").action = "./login.php";
		document.getElementById("LoginForm").submit();
	}else{
		var username=document.getElementById("username").value;
		var passwd=document.getElementById("passwd").value;
		var langx=document.getElementById("langx").value;	
		var is_chk=chk_acc();
		if(is_chk){
			//var req;
			//req = new HttpRequest();
			//req.addEventListener("LoadComplete", show_msg);	
			//req.loadURL("/app/member/new_login.php","POST", "username="+username+"&passwd="+passwd+"&langx="+langx);	
			document.getElementById("LoginForm").submit();
		}
	}
}

