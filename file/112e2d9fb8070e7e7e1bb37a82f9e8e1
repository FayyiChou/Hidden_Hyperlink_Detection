function click_new(){
	var protocol = document.location.protocol;
	top.selLang="web_new";
	document.getElementById("lang_tis").style.display = "none";
	document.getElementById("lang_vi").style.display = "";
		if(document.getElementById("langx").value=="th-tis"){
		self.location.href=protocol+"//"+document.domain+"/app/member/translate.php?set=en-us&url=app/member/index.php";
	}
}

function click_old(){
	top.selLang="web_old";
	document.getElementById("lang_tis").style.display = "";
	document.getElementById("lang_vi").style.display = "none";
}

