<script>
  if(window.FS === undefined) { var FS = {}; }
  
  FS.TDW = {};
  
  FS.add_css = function (a){var b=document.createElement('style');b.type='text/css';if(b.styleSheet){b.styleSheet.cssText=a}else{b.appendChild(document.createTextNode(a))}document.getElementsByTagName('head')[0].appendChild(b)};
  
  FS.set_navigation_height = function() {
  
    var DESKTOP_BREAKPOINT = 1030,
        BANNER_HEIGHT      = 110,
        ROW_HEIGHT         = 22,
        PADDING_HEIGHT     = 40,
        LINKS_BAR_HEIGHT   = 50,
        BORDER_HEIGHT      = 1,
  
        rows_count    = 10,
        window_width  = window.outerWidth || document.documentElement.clientWidth,
        height;
  
    function calculate_height() {
      return BANNER_HEIGHT + PADDING_HEIGHT + LINKS_BAR_HEIGHT + BORDER_HEIGHT + (rows_count * ROW_HEIGHT);
    }
  
    if(window_width >= DESKTOP_BREAKPOINT) {
      FS.navigation_height_set = true;
      height = calculate_height();
      FS.add_css('.ie8 #js-navigation { height: ' + height + 'px; } @media only screen and (min-width: ' + DESKTOP_BREAKPOINT + 'px) { #js-navigation { height: ' + height + 'px; } }');
    }
  };
</script>
