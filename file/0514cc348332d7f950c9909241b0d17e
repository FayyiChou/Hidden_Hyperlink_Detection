<script type="text/javascript">

    $(".ul_top_menu li").mouseenter(function(){
        $(this).addClass('hover');
    }).mouseleave(function() {
        $(this).removeClass('hover');
    });

    function checkUser(obj){
        if(true){
            alert("你尚未登录，请先登录！");
            obj.target = "";
            obj.href = "/familyServices/index";
        }else{
            obj.target = "_blank";
        }
    }
    function AddFavorite(sURL, sTitle)
    {
        try
        {
            window.external.addFavorite(sURL, sTitle);
        }
        catch (e)
        {
            try
            {
                window.sidebar.addPanel(sTitle, sURL, "");
            }
            catch (e)
            {
                alert("加入收藏失败，请使用Ctrl+D进行添加");
            }
        }
    }

    //设为首页
    // 设置为主页
    function SetHome(obj,vrl){
//        try{
//            obj.style.behavior='url(#default#homepage)';obj.setHomePage(vrl);
//        }
//        catch(e){
            if(window.netscape) {
                try {
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                }
                catch (e) {
                    alert("此操作被浏览器拒绝！\n请在浏览器地址栏输入“about:config”并回车\n然后将 [signed.applets.codebase_principal_support]的值设置为'true',双击即可。");
                }
                var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
                prefs.setCharPref('browser.startup.homepage',vrl);
            }else{
                alert("您的浏览器不支持，请按照下面步骤操作：1.打开浏览器设置。2.点击设置网页。3.输入："+vrl+"点击确定。");
            }
//        }
    }
    // 加入收藏 兼容360和IE6
    function shoucang(sTitle,sURL)
    {
        try
        {
            window.external.addFavorite(sURL, sTitle);
        }
        catch (e)
        {
            try
            {
                window.sidebar.addPanel(sTitle, sURL, "");
            }
            catch (e)
            {
                alert("加入收藏失败，请使用Ctrl+D进行添加");
            }
        }
    }

    $( function getNoticeBoard(){
        var url = "/common/getVisitNums";
        $.ajax({
            type: "POST",
            url:url,
            async:false,
            success: function(data) {
                var html = '网站浏览数量：'+data;
                $("#pv").html(html);
            }
        });
    });
    $.cookie.raw = true;
    $(document).ready(function(){
        $(".btn_logout").click(function(){
            $.ajax({
                type: "POST",
                url: "/login/logout",
                async:false,
                success: function(res) {
                    if(res.code == 0){
                        $.removeCookie('#Cookie:YL:User',{domain:'.shdc.org.cn',path: '/' });
                        alert("注销成功!");
                        location.reload();
                    }
                }
            });
        });
    });


    function _payQueryUrl(obj){
        if(true){
            alert("你尚未登录，请先登录！");
            obj.target = "";
            obj.href = "/familyServices/index";
            return;
        }else{
            obj.target = "_blank";
            $.ajax({
                type: "POST",
                url:"/familyServices/getPayQueryUrl",
                data:{},
                async:false,
                dataType: 'json',
                success: function(res) {
                    if(res.code == 0){
                        window.open(res.data);
                    }
                }
            });
        }
        return false;
    }
</script>
