        <div class="home_dynamic">
            <div class="home_search">
                <input id="keyWords" type="text" class="public_input" placeholder="请输入关键词" value = "请输入关键词">
                <a class="btn_search" >搜索</a>
            </div>
            <script type="text/javascript">
                (function () {
                    $('#keyWords').bind("blur",function(){
                        if(this.value == ''){
                            this.value="请输入关键词";
                        }
                    });
                    $('#keyWords').bind("focus",function(){
                        if(this.value == '请输入关键词'){
                            this.value="";
                        }
                    });
                    $('.btn_search').bind("click",function(){
                        var keyWords = $("#keyWords").val();
                        if(keyWords=="请输入关键词"){
                            keyWords = "";
                        }
                        this.href = "/search/searchByWords?keyWords="+keyWords;
                    });
                })();
            </script>
