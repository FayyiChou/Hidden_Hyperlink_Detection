<script type="text/javascript">
    scrollPic();

    function checkUser(obj){
        if(true){
            alert("你尚未登录，请先登录！");
            obj.target = "";
            obj.href = "/familyServices/index";
        }else{
            obj.target = "_blank";
        }
    }

    (function () {
        subStr($(".aTitle15"),30);
        subStr($(".aTitle18"),36);
        subStr($(".aTitle14"),28);
    })();



    /*医院提示*/
    $(".list li").mouseenter(function(e) {
        $(this).addClass('hover');
        var lioffset  = $(this).offset();
        var tipHeight = $('.tipDiv', this).height();
        var litop   = lioffset.top + tipHeight + 34;
        var winheight = $(window).height() + $(window).scrollTop();
        var top;
        if(litop > winheight)
        {
            top = -( tipHeight + 34 ) + "px";
            $(this).find(".tipDiv").css("top",top);
        }
        else
        {
            $(this).find(".tipDiv").css("top","30px");
        }
    }).mouseleave(function(e) {
        $(this).removeClass('hover');
    });

  function _payQueryUrl(obj){
      if(true){
          alert("你尚未登录，请先登录！");
          obj.target = "";
          obj.href = "/familyServices/index";
          return true;
      }else{
          obj.target = "_blank";
          $.ajax({
              type: "POST",
              url:"/familyServices/getPayQueryUrl",
              data:{},
              async:false,
              dataType: 'json',
              success: function(res) {
                  if(res.code == 0){
                      window.open(res.data);
                  }
              }
          });
      }
      return false;
  }
    function closeAd(){
        $('.sk-ad').hide();
    }
</script>
</body>
