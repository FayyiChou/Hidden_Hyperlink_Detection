                      <!--[if IE]>
<style>#imgTitle_down {	margin-top:-13px;margin-left:2px;}</style>
<![endif]-->
                      <script language="JavaScript" type="text/javascript">
	var imgWidth=290;              //图片宽
	var imgHeight=190;             //图片高
	var textFromHeight=30;         //焦点字框高度 (单位为px)
	var textStyle="f12";           //焦点字class style (不是连接class)
	var textLinkStyle="p1"; //焦点字连接class style
	var buttonLineOn="#FF0000";           //button下划线on的颜色
	var buttonLineOff="#000000";          //button下划线off的颜色
	var TimeOut=5000;              //每张图切换时间 (单位毫秒);
	var imgUrl=new Array(); 
	var imgLink=new Array();
	var imgtext=new Array();
	var imgAlt=new Array();
	var adNum=-1;
	//焦点字框高度样式表 开始
	document.write('<div id="focuseFrom">');
	//焦点字框高度样式表 结束

	var oImgId = document.getElementById('swapimglist');
	var aImg = oImgId.getElementsByTagName("a");
	for(var i=0; i<aImg.length; i++){
	        imgUrl[i] = aImg[i].childNodes[0].src;
		imgtext[i]= aImg[i].nextSibling.nodeValue;
		imgLink[i]= aImg[i].href;
		imgAlt[i] = aImg[i].nextSibling.nodeValue;
	}

	function changeimg(n)
	{
		adNum=n;
		if(typeof theTimer != "undefined"){
			window.clearInterval(theTimer);
		}
		adNum=adNum-1;
		nextAd();
	}
	function goUrl(){
		if(typeof imgLink[adNum] != "undefined"){
			window.open(imgLink[adNum],'_blank');
		}
	}
	
	
	var count=0;
	for (i=0;i<imgUrl.length;i++) {
		if( (imgUrl[i]!="") && (imgLink[i]!="")&&(imgtext[i]!="")&&(imgAlt[i]!="") ) {
			count++;
		} else {
			break;


		}
	}
	function playTran(){
		if (document.all)
			imgInit.filters.revealTrans.play();		
	}
	var key=0;
	function nextAd(){
		if(adNum<count-1)adNum++ ;
		else adNum=0;
		
		if( key==0 ){
			key=1;
		} else if (document.all){
			imgInit.filters.revealTrans.Transition=6;
			imgInit.filters.revealTrans.apply();
					   playTran();
		}
		document.images.imgInit.src=imgUrl[adNum];
		document.images.imgInit.alt=imgAlt[adNum];
                document.getElementById("imgText").innerHTML = "<a href='javascript:goUrl()'>"+imgtext[adNum]+"</a>";	
		document.getElementById('link'+adNum).style.background=buttonLineOn;








		for (var i=0;i<count;i++)
		{
		   if (i!=adNum){document.getElementById('link'+i).style.background=buttonLineOff;}
		}	
		//focustext.innerHTML=imgtext[adNum];
		theTimer=setTimeout("nextAd()", TimeOut);
	}
	document.write('<a target=_self href="javascript:goUrl()"><img style="FILTER: revealTrans(duration=1,transition=5);" src="/web/waisb//web/xingzhan/images/blank.gif" width='+imgWidth+' height='+imgHeight+' border=0 vspace="0" name=imgInit class="imgClass"></a>');
	document.write('</div>');


	document.write(' <div id="imgTitle_down">');
	//数字按钮代码开始
	for(var i=0;i<imgUrl.length;i++){
		document.write('<a id="link'+i+'"  href="javascript:changeimg('+i+')" class="button" style="cursor:pointer" title="'+imgAlt[i]+'" onFocus="this.blur()">'+(i+1)+'</a>');
	}
	//数字按钮代码结束
	document.write('</div>');

	document.write('<div id="imgText" style="margin:5px;text-align:center;">');
	document.write('</div>');
	document.getElementById("imgText").innerHTML = "正在加载图片，请稍候......";	
	//alert();
                    </script>
                  </td>
                </tr>
