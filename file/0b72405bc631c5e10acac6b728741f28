function getIEVersion(){
                var ua = navigator.userAgent;
                var ver = 0;
                if(ua){
                       if(ua.match(/MSIE\s+([\d]+)\./i)){
                                ver = RegExp.$1;
                       }else if(ua.match(/Trident.*rv\s*:\s*([\d]+)\./i)){
                                ver = RegExp.$1;
                        }
                }
                return parseInt(ver);
        }
</script>
