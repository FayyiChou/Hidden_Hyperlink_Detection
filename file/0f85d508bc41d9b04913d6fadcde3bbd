<script>
$(function(){
var index=0,
 repeat;
  //小圆点切换
  $('.num li').mouseover(function(){
  index=$(".num li").index(this);
  imgshow(index);
  })
 //图片自动滚动
  $('.ad').hover(function(){	
clearInterval(repeat);						
  },function(){
repeat=setInterval(function(){
   imgshow(index);
   index++;
 if(index==3){
index=0;
 }
},2000)	
  }).trigger('mouseleave');
  
  function imgshow(index){
$('.pic li').eq(index).addClass('show').siblings().removeClass('show');
$('.num li').eq(index).addClass('on').siblings().removeClass('on');
  }
})

</script>

    
