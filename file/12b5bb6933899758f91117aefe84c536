    <script>
var dir33=1//每步移动像素，大＝快
var speed33=20//循环周期（毫秒）大＝慢
demo332.innerHTML=demo331.innerHTML
function Marquee33(){//正常移动
 //alert(demo332.offsetWidth+"\n"+demo33.scrollLeft)
 if (dir33>0  && (demo332.offsetWidth-demo33.scrollLeft)<=0) demo33.scrollLeft=0
 if (dir33<0 &&(demo33.scrollLeft<=0)) demo33.scrollLeft=demo332.offsetWidth
 demo33.scrollLeft+=dir33
 
 demo33.onmouseover=function() {clearInterval(MyMar33)}//暂停移动
 demo33.onmouseout=function() {MyMar33=setInterval(Marquee33,speed33)}//继续移动
}
function r_left1(){if (dir33=-1)dir33=1}//换向左移
function r_right1(){if (dir33=1)dir33=-1}//换向右移


var MyMar33=setInterval(Marquee33,speed33)

</script>
