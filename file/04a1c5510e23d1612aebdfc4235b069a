                    <script type="text/javascript">


var num1='3';
function ChangeChannels(module)
{
    var tabid="Infos_"+module;
      
    for(var s=1;s<=num1;s++)
    {
      var head="Channels"+s;
      var tab="Infos_"+s;


      if(s!=module)
      {
   
       document.getElementById(tab).style.display="none";
       document.getElementById(head).className="";

       }
      else
      {   

       document.getElementById(tab).style.display="block";
       document.getElementById(head).className="rmtj_home";
        }
        
    }

}


</script>
