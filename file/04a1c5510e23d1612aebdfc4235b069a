                <script type="text/javascript">


var num='6';
function ChangeChannel(module)
{
    var tabid="Info_"+module;
      
    for(var s=1;s<=num;s++)
    {
      var head="Channel"+s;
      var tab="Info_"+s;


      if(s!=module)
      {
   
       document.getElementById(tab).style.display="none";
       document.getElementById(head).className="";

       }
      else
      {   

       document.getElementById(tab).style.display="block";
       document.getElementById(head).className="zxnews_home";
        }
        
    }

}


</script>
