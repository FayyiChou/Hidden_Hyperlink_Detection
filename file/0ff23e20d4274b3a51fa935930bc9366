    <script>
(function(){
    //在搜索按钮的点击日志中增加搜索内容keyword字段
    try{
        var form=Q.$('form[data-suggest-elem=form]');
        var input = form.down('[data-suggest-elem=input]');
        var btn = form.down('[data-suggest-elem=btn]');
        input.on('keyup', function(e){
            btn.attr('data-pb', 'keyword=' + input.value());
        });
    }catch(e){}
})();
</script>
