<script type="text/javascript">
$(function(){
    // 用户登录
    var account = $('#account');
    var psw = $('input[name=psw]');
    var login_form = $('#login_form');
    var login_btn = $('#login_btn');
    var rememberme = $('#rememberme');
    //var errmsg = '';
    var _callback = '';
//    if(errmsg != '') {
//        UI.popTip(1, errmsg);
//    }

    login_btn.data('sclick', 0);
    var login_submit = function() {
        if(login_btn.data('sclick') == 1) {
            return false;
        }
        if(account.val() == '') {
            account.css({'border-color':'red'});
            return false;
        }

        if((psw.val() == '') || (6 > psw.val().length)) {
            $('#psw').css({'border-color':'#f00'});
            return false;
        }

        // 发送请求
        $.ajax({
            type:'post',
            url:'/wdreg/login',
            data:{
                'account':account.val(),
                'psw':psw.val(),
                'rememberme':rememberme.val()
            },
            dataType:'json',
            beforeSend:function(){
                login_btn.data('sclick', 1).val('提交中…');
            },
            success:function(data) {
                if(data.ret == 0) {
                    UI.popTip(0, '登录成功！');
                    if(_callback != '') {
                        window.location.href = _callback;
                    } else {
                        window.location.href = '/';
                    }
                    return false;
                } else if(data.ret == 302) {
                    UI.popTip(data.msg);
                    window.location.href = '/';
                    return false;
                } else {
                    UI.popTip(1, data.msg);
                    login_btn.data('sclick', 0).val('登录');
                }

            }
        });
    };
    $('#account,input[name="psw"]').on('keydown', function(e){
        if(e.which == 13) {
            login_submit();
        }
    });
    $(document).on('focus','#account,#psw',function(){
        $(this).css({'border-color':'#ff9c38','color':'#333333'});
    });
    $(document).on('blur','#account,#psw',function(){
        $(this).css({'border-color':'#dddddd','color':'#999999'});
    });
    login_btn.on('click', function(){
        login_submit();
    });
});
</script>



