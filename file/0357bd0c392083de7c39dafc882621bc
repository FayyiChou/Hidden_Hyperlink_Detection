<script type="text/javascript">
function dataFormat(timeStamp){
	if(timeStamp){
		var formDate=new Date(timeStamp);
		if(formDate){
			var month=formDate.getMonth()+1;
			var day=formDate.getDate();
			var year=formDate.getFullYear();
			var formatDate=year+"-"+month+"-"+day
			if(!formatDate){
				formatDate='';
			}
			return formatDate;
		}
	}
}

/*查询车类型*/

$(function(){
	 getHighGradeGoodsSource();
	 getHighGradeCarSource();
 })
 

function getHighGradeCarSource(){
	var data={};
	data.tlength=12;
	data.dataNum=2;
	data.siteId=$("#siteID").val();
	$.ajax({
		type: "GET",
		dataType: "json",
		data:data,
		url:"/logistics/carSourceList",
		success:function(data){	
			var list=null;
			if(data&&data.highGradeCarSource&&data.carSourceEntities.content){
				list=data.highGradeCarSource.content;
			}
			var resultHt2 = '';
			
			if(list&&list.length){
				for(var i=0;i<list.length;i++){	
					resultHt2+='<ul>';
					resultHt2+='<li><span>出发地：</span><em class="em1">'+list[i].fromAreaCodeName+'</em></li>';
					resultHt2+='<li><span>目的地：</span><em class="em2">'+list[i].toAreaCodeName+'</em></li>';
					resultHt2+='<li><span>车辆描述：</span><em>'+list[i].car.carType+'</em></li>';
					resultHt2+='<li><span>发布时间：</span><em>'+dataFormat(list[i].createDate)+'</em></li>';
					resultHt2+='</ul>';
				}
			}else{
				resultHt2+='<li><span><center>暂无记录</center></span></li>';
			}
			$("#goodsDiv").html(resultHt2);
		}
	});
}
 
function getHighGradeGoodsSource(){
	var data={};
	data.tlength=12;
	data.dataNum=2;
	data.siteId=$("#siteID").val();
	$.ajax({
		type: "GET",
		dataType: "json",
		data:data,
		url:"/logistics/getHighGradeGoodsSource",
		success:function(data){	
			var resutlHt='';
			var list=null;
			if(data&&data.code==200&&data.page){
				list=data.page.content;
			}			
			
			if(list&&list.length){	
				
				for(var i=0;i<list.length;i++){	
					resutlHt+='<ul>';
					resutlHt+='<li><span>出发地：</span><em class="em1">'+list[i].fromId.areaName+'</em></li>';
					resutlHt+='<li><span>目的地：</span><em class="em2">'+list[i].toId.areaName+'</em></li>';
					resutlHt+='<li><span>货物描述：</span><em>';
					if(list[i].title&&list[i].title.length>9){
						resutlHt+=list[i].title.substring(0,8);
					}else{
						resutlHt+=list[i].title;
					}
					resutlHt+='</em></li>';
					resutlHt+='<li><span>发布时间：</span><em>'+dataFormat(list[i].createDate)+'</em></li>';
					resutlHt+='</ul>';
					
					
					
				}
				
			}else{
				resutlHt+='<li><span><center>暂无记录</span></center></li>';
			}
			
			$("#carDiv").html(resutlHt);
		}
	});
}

</script>
