<script>
    $(function () { 
        var displayEx = function(){ 
            $("ul.ask-fcon-w960").each(function () { 
                if ($(this).height() > 72) { 
                    $(this).css({'height': '54px', "overflow": "hidden"}); 
                    $(this).next().show(); 
                } 
            }); 
            $("ul.ask-fname-w960").each(function () { 
                if ($(this).height() > 72) { 
                    $(this).css({'height': '54px', "overflow": "hidden"}); 
                    $(this).next().show(); 
                } 
            }); 
        }; 
        $(".impression-show-all").each(function() { 
            $(this).click(function () { 
                if ($(this).prev().height() > 72) { 
                    $(this).prev().css({'height': '54px', "overflow": "hidden"}); 
                    $(this).removeClass("impression-show-none"); 
                    $(this).text("展开"); 
                } 
                else 
                { 
                    $(this).prev().css({'height': 'auto'}); 
                    $(this).addClass("impression-show-none"); 
                    $(this).text("收起"); 
                } 
            });
        }); 
        var js_tab_li = $(".js-tab-list li"); 
        var js_tab_con = $(".js-tab-con"); 
        displayEx(); 
        js_tab_li.hover(function () { 
            var index = $(this).index(); 
            js_tab_li.eq(index).addClass("tab-cur").siblings().removeClass("tab-cur"); 
            js_tab_con.eq(index).addClass("tab-show").siblings().removeClass("tab-show"); 
            if(!$(this).data('hIdx')) { 
                displayEx(); 
            } 
            $(this).data('hIdx', 1); 
        }) 
    }) 
</script>
