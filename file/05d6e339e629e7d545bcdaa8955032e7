<script>
    /* 0129*/
    $(function(){
        var scroll_width = $(".zzx_yh_h1").width();
        var resize_top  = $(".zzx_yh_h1").offset().top;
        var scroll_left   = $(".zzx_yh_h1").offset().left;
        resizescroll();
        $(window).scroll(function(event){
            resizescroll();
        });
        function resizescroll(){
            if( $(document).scrollTop() > resize_top){
                $(".zzx_yh_h1").css({width:scroll_width,"position":"fixed","top":"0",/*"left":scroll_left,*/"z-index":12});
                $(".zx_pay_infos").css({width:scroll_width-42,"position":"fixed","top":"63px","left":scroll_left+1,"z-index":12})
                var $element = $(".js-retrack");
                if($element.attr('st') == 0){
                    $element.trigger('click');
                }
            }else {
                $(".zzx_yh_h1").css({"position":"static"})
                $(".zx_pay_infos").css({"position":"static"})
            }
        }
    })
    /* 0129*/
</script>
