<script>function ShowWindow(obj) {
var $o = $(obj);
var $popa = $(".popinfo_a");
var $href = obj.getAttribute('originalhref');
var $thunderhref = obj.getAttribute('thunderhref');
$(".pop").show("fast");
$.each($o.prop("attributes"), function() {
  if (/thunder/.test(this.name)) $popa.attr(this.name, this.value);
});
$popa.attr("href", $href).html($href);
$(".btn").attr("data-clipboard-text", $href);
return false;
}</script>
