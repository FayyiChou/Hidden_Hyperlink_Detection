<script type="text/javascript">
	function onloadPage()
	{
	  var obj = document.getElementById("User_Name");
	  var v = navyFun.getCookie_n("save_ylike_login_name");
	  if(obj.value =='')
	  {
		  if(v !=null && v != "" && v != " " && typeof(v) != 'undefined')
		  {
			 obj.value = v;
			
			 document.getElementById("User_Password").focus();
		  }else
		  {
			 //obj.focus();
			 obj.value="用户名/手机/邮箱";
		  }
	  }
	}
	function onKeydownByenter1(e)
	{
		if(e.keyCode==13)
		{
			if(document.getElementById('User_Name').value	==	'')
			{
				document.getElementById('User_Name').focus();
				return false;
			}else{
				document.getElementById('User_Password').focus();
			}
		}
	}
	function clear_text(obj)
	{
	   if(obj.value.match(/\//g))
	   {
		   obj.value="";
	   }
	}
	function check_text(obj)
	{
	   if(obj.value == "")
	   {
		   obj.value='用户名/手机/邮箱';
	   }
	}
	function check_text_check(obj)
	{
	   if(obj.value == "")
	   {
		   obj.value='验证码';
	   }
	}
	function clear_text_check(obj)
	{
	   if(obj.value == "验证码")
	   {
		   obj.value="";
	   }
	}
	function clear_label_focus()
	{
		var obj = $("#label_pw_hi");
	   if(obj.text() == "密码")
	   {
		   obj.attr({style:"display: none;"});
	   }
	}
	function check_label_onblur()
	{		
		var obj = $("#label_pw_hi");
	   if($("#User_Password").val() == "")
	   {
		   obj.attr({style:"display: block;"});
	   }
	}

	function checkLoginsave(){
		var obj = document.getElementById("User_Name");
		var isSavecheck = document.getElementById("save_ylike_login_name");
		if (obj.value != "")
		{
			if (isSavecheck.checked == true){
				navyFun.setCookie("save_ylike_login_name",obj.value);
			}else{
				navyFun.setCookie("save_ylike_login_name"," ");
			}
			return true ;
		}else{
			return false ;
		}
	}
</script>
