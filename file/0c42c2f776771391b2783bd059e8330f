    <script>
        $(function() {
            /*热门推荐hover*/
            $('.ds-list li').hover(function(){
                $(this).find('.ds-li-hover').stop(true,true).fadeIn();
            },function(){
                $(this).find('.ds-li-hover').stop(true,true).fadeOut();
            });
            
            /*懒加载*/
            var loadImg=function(){
                $('.main2-r .developers-container .ds-list li img').each(function(){//遍历所有图片
                    var _this=$(this),
                        top=_this.offset().top-$(window).scrollTop();//计算图片top - 滚动条top
                    if(top > $(window).height()){
                        return;
                    }else{
                        _this.attr('src',_this.attr('data-src'));
                    }
                });
            };
            loadImg();
            $(window).scroll(loadImg).resize(loadImg);
            
            var name = $('#detailsType').text();

            if(name == '') {
                $('#detailsType').attr('href', 'http://vr.99.com/observer.shtml');
                $('#detailsType').text('VR观察者');
            }else if(name == '最新资讯') {
                $('#detailsType').attr('href', 'http://vr.99.com/observer.shtml');
            }else if(name == '行业观察') {
                $('#detailsType').attr('href', 'http://vr.99.com/observer.shtml?1');
            }else if(name == '行业动态') {
                $('#detailsType').attr('href', 'http://vr.99.com/observer.shtml?2');
            }else if(name == '独家盘点') {
                $('#detailsType').attr('href', 'http://vr.99.com/exclusive.shtml');
            }else {
                $('#detailsType').attr('href', 'http://vr.99.com/observer.shtml');
            }
        })
    </script>

