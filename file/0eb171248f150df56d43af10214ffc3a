function showgkmu(sId){
	
	document.getElementById(sId).style.display = "block";
	
	

	
	document.getElementById(sId).style.color = "33FFFF";
	
}
function closegkmu(sId){
	
	document.getElementById(sId).style.display = "none";
	
	

	
