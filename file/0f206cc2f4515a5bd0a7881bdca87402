function get_pagination(mv_type, mv_id, sid, start) {
    var surl = "/?ct=ajax&ac=get_pagination&mv_type="+mv_type+"&mv_id="+mv_id+"&sid="+sid+"&start="+start;
    $.get(surl, function(data) {
        $('#all_pagination').html( data );
    }); 
};

</script>
