			$(document).ready(function($) {
				searchHeight();
				$(".slideBox").slide({titCell:".hd ul",mainCell:".bd ul",autoPlay:true,autoPage:true});//焦点图切换
				$(".search_box").slide({titCell:".search_left ul li",mainCell:".search_right",trigger:"click"});
