				$('.commentMod').slide({ mainCell:'.commentList', effect:'topLoop', vis:4, opp:true, autoPlay:true, delayTime: 800 });//点评
				$('#topNav').find('li').mouseover(function(event) {//顶部菜单
					$(this).addClass('open');
					$(this).children('div').show();
				}).mouseout(function(event) {
					$(this).removeClass('open');
					$(this).children('div').hide();
				});
				$('.ab_con .input02').click(function (event) { //搜索下拉
		            event.stopPropagation(); //取消事件冒泡
		            $(this).parent().find('dl').slideDown();
		            $(this).parent().siblings().find('dl').hide();
		        });
		        //点击空白处或者自身隐藏搜索下拉层
		        $(document).click(function (event) { $('.ab_con').find('div').find('dl').slideUp() });
		        $('.ab_con').each(function () {
		            $(this).find('dd:first').click();
		        });
		        $('.ab_con div dl dd').hover(
				    function () { $(this).addClass('on'); },
				    function () { $(this).removeClass('on'); }
			    ).click(function () {//下拉点击传参
			        var text = $(this).html();
			        var val = $(this).attr('val');
			        $(this).parent().slideUp();
			        $(this).parent().parent().find('.input02').val(text);
			        $(this).parent().parent().find("input[type='hidden']").val(val);
			    });
			});
		    /*选择日期*/
			$(function() {
			    $( ".startDate" ).datepicker({//酒店日期选择
			    	numberOfMonths: 2,
			        onClose: function( selectedDate ) {
				        $( ".endDate" ).datepicker( "option", "minDate", selectedDate );
				    }
			    });
			    $( ".endDate" ).datepicker({
			    	numberOfMonths: 2,
			        onClose: function( selectedDate ) {
				        $( ".startDate" ).datepicker( "option", "maxDate", selectedDate );
				    }
			    });
			    $("#depDate").datepicker({//机票日期选择
			    	numberOfMonths: 2,
			        onClose: function( selectedDate ) {
				        $( "#returnDate" ).datepicker( "option", "minDate", selectedDate );
				    }
			    });
			    $("#returnDate").datepicker({
			    	numberOfMonths: 2,
			        onClose: function( selectedDate ) {
				        $( "#depDate" ).datepicker( "option", "maxDate", selectedDate );
				    }
			    });
			    $("#cm1_1").datepicker();
			});
			/*机票脚本*/
			$('.airplane_rad li').find('label').click(function(event) {//机票单选按钮
				if ($(this).hasClass('return')) {
					$("#returnTimeDiv").show();
				}else{
					$("#returnTimeDiv").hide();
				};
				var val = $(this).parent().attr('val');
				$(this).find('s').addClass('rep_radiao_check');
				$(this).parent().siblings().find('s').removeClass('rep_radiao_check');
				$(this).parent().siblings("input[type='hidden']").val(val);
			});
			$('#search').click(function () { //搜索
		        if ($('#fromCity_3word').val() == '' || $('#fromCity').val() == '') {
		            alert("请选择出发城市");
		            return;
		        }
		        if ($('#toCity_3word').val() == '' || $('#toCity').val() == '') {
		            alert("请选择到达城市");
		            return;
		        }
		        if ($('#depDate').val() == '') {
		            alert("请选择出发日期");
		            return;
		        }
		        if ($('#type').val() == 2 && $('#returnDate').val() == '') {
		            alert("请选择返程日期");
		            return;
		        }
		        searchData();
		    });
		    $('#changeFlightSearchCity').click(function () {//国内机票出发城市和到达城市数据互换
		        var fromCity = $('#fromCity').val();
		        var toCity = $('#toCity').val();
		        $('#fromCity').val(toCity);
		        $('#toCity').val(fromCity);
		    });
		    function searchData(fromD, toD, tD) {
			    var parma, t = $("#type").val();
			    parma = 'fromCity=' + $('#fromCity_3word').val(); //encodeURIComponent('CAN'); ;
			    parma += '&toCity=' + $('#toCity_3word').val(); // encodeURIComponent('BJS');
			    parma += '&fromDate=' + $('#depDate').val();
			    parma += '&toDate=' + $('#returnDate').val();
			    parma += '&t=' + t;
			    parma += '&rand=' + Math.random();
			    location.href = $('#flighturl').val() + '?' + parma;
			}
			if (typeof (citys) != "undefined") {
		        $("#fromCity").suggest(citys, { hot_list: commoncitys, dataContainer: '#fromCity_3word', onSelect: function () { $("#toCity").click(); }, attachObject: '#suggest' });
		        $("#toCity").suggest(citys, { hot_list: commoncitys, dataContainer: '#toCity_3word', attachObject: "#suggest2" });
		    }
			//首页机票加载
			function flightData(obj, f, df, t, dt) {
			    if (!webconfig().flight) return;
			    var s=$('#flightpanel > li').index(obj.parent());
			    var toDate = new Date(Date.parse(webconfig().date.replace(/-/g, '/')) + 86400000);
			    toDate=toDate.getFullYear() + "-" + (toDate.getMonth() + 1) + "-" + toDate.getDate()
			    var parma;
			    parma = 'fromCity=' + obj.attr('v');
			    parma += '&fromDate=' + webconfig().date;
			    parma += '&toDate=' + toDate;
			    parma += '&rand=' + Math.random();
			    $.ajax({
			        type: "get",
			        url: webpath + "sys/ajax/flight/flight.ashx?act=bind",
			        data: parma,
			        dataType: "json",
			        success: function (data, textStatus) {
			            var ul = $('#flightpanel').parent().siblings('.bd').children('ul').eq(s);
			            ul.empty();
			            if (data.status == 1) {
			                $.each(data.flights, function (i, d) {
			                    var tmp = '';
								tmp+='	<li>';
								tmp+='		<a href="'+$('#flighturl').val()+'?fromCity='+d.deptcode+'"&toCity='+d.arrcode+'&fromDate='+d.depdate2+'" target="_blank">';
								tmp+='			<div class="s_name">'+ d.depairport +'→'+ d.arrairport +'</div>';
								tmp+='			<div class="s_info">';
								tmp+='				<span class="s_price">&yen;'+ d.ticketprice +'</span>';
								//tmp+='				<span class="s_off">3折</span>';
								tmp+='				<span class="s_day">'+ d.depdate2 +'</span>';
								tmp+='			</div>';
								tmp+='		</a>';
								tmp+='	</li>';
			                    ul.append(tmp);
			                });
			            } else {
			                ul.append("抱歉，没有搜索到相关的航班信息！");
			            }
			        }
			    });
			}
			$('#flightpanel > li > a').mousemove(function () { //Tab切换
		        if ($(this).attr('ajaxload') != undefined) return;
		        $(this).attr('ajaxload', 1);
		        flightData($(this));
			    }).first().mousemove();
			//当滚动条的位置处于距顶部100像素以下时，跳转链接出现，否则消失 
	            $(window).scroll(function(){ 
	                var scrollTop = $(window).scrollTop();
	                if(scrollTop){
	                    $('.publicBox').each(function(i){
	                    	$(this).attr('t', i);
	                        if(scrollTop >= $(this).offset().top-68){
	                            var myT = $(this).attr('t');
	                            $('.leftNavBox li').removeClass('on');
	                            $('.leftNav li[v="'+myT+'"]').addClass('on');
	                        }
	                    });
	                    if(scrollTop > $('.choiceness').offset().top-100 && scrollTop < $('.travels').offset().top+100){
	                        $(".leftNav").fadeIn(100);
	                    }else{
	                        $(".leftNav").fadeOut(100); 
	                    }
	                }
	            });
	            $(".leftNav li").each(function(i){
	            	$(this).attr('v', i);
	            }).click(function() { 
	                var i = $(this).attr('v'); //取得按钮值
	                var t = $('.publicBox[t="'+ i +'"]'); //对应的id层
	                $(".leftNav li").removeClass('on');
                    $(this).addClass('on');
                    $("html,body").animate({scrollTop: t.offset().top-68}, 250); //一秒跳转到该ID的位置
	            });
	            /*切换菜单左侧自适应高度*/
				function searchHeight(){
					var searchHeight = $('.search_left').height();
					var num = $('.search_left').find('ul').children('li').length;
					var sum = searchHeight/num;
					$('.search_left').find('li').height(sum);
					$('.search_left').find('li').css({
						lineHeight: sum + "px",
					});
				}
		</script>
	</div>
</body>
</html>