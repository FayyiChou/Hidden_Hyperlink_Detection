<script type="text/javascript">
// scrollTop
jq(window).scroll(function(){
var scrollValue = jq(window).scrollTop();
if(scrollValue > 200){
jq('#backtop').fadeIn(400);
}else{
jq('#backtop').fadeOut(400);
};
});
jq(function(){
// 返回顶部
jq('#backtop').click(function(){
jq('html').animate({scrollTop: 0},200);
jq('body').animate({scrollTop: 0},200);
});

// 鼠标滑过
jq('#backtop').hover(function(){
jq(this).css({background: '#ff6699', color: '#fff'});
},function(){
jq(this).css({background: 'none',color: '#ff6699'});
})

// 关闭手机访问
jq('.m_site i').click(function(){
jq(this).parent().remove();
})
})
</script>

