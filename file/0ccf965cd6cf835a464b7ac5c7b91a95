<script>
/*
id   主要滚动区域id 
speed 滚动速度，数值越小，滚动越快！
mw 滚动区域宽度
mh 滚动区域高度
网上的很多滚动代码很乱，所以自己整理了一下
*/
function w3cMarquee(id,speed,mw,mh){
var m=document.getElementById(id);
var m_body=document.getElementById(id+'_body');
var m_temp=document.getElementById(id+'_temp');
var speed=speed; //设置滚动的速度，数值越小，滚动越快！
with(m){
   style.width=mw+"px"; //滚动区域宽度
   style.height=mh+"px"; //滚动区域高度
   noWrap=false; //不换行
   onmouseover=stopm; //鼠标移动到上面，停止
   onmouseout=startm; //鼠标移出，开始
   scrollTop=0+"px"; 
   scrollLeft=0+"px";
}
while(m.offsetHeight-m_body.offsetHeight > 0){ 
   m_body.innerHTML+=m_body.innerHTML ;
} 
m_temp.innerHTML=m_body.innerHTML;
function up(){ 
  if(m_temp.offsetTop-m.scrollTop<=0){ 
    m.scrollTop-=m_body.offsetHeight;
  }else{ 
    m.scrollTop++;
  } 
} 
function stopm(){clearInterval(MyMar)} //停止函数
function startm(){MyMar=setInterval(up,speed)} //启动函数
var MyMar=setInterval(up,speed); 
}
w3cMarquee('m',80,370,115);

</script>

</td>
