<script>
    $(function(){
        //下拉源
        if($('.play-source').get(0)){
            $('.play-source').click(function(){
                $(this).toggleClass('up-triangle');
            });

            $('.play-source').click(function(event){
                $('.source').toggleClass('heightAuot');
                event.stopPropagation();
            });  
            $('.source dd').click(function(){
                $('.source dt').html($(this).html());
                var num = $(this).find('i').attr('rel');
                $.ajax({
                    url: '/index.php?ct=ajax&ac=sidm&mv_type=1&mv_id=47046&sid='+num+'&start=0',
                    type: 'GET',
                    success:function(data){
                        $('#play-list').html('');
                        $('#play-list').append(data);
                    }
                });
                
            });

            //切换集数
            $(document).on('click','.all-list dt a',function(){
                $(this).addClass('current').siblings('a').removeClass('current');
                var thisIndex = $(this).index();
                $(this).parents('.all-list').find('.pl-li').eq(thisIndex).show().siblings('.pl-li').hide();
            });
        }
    });
</script></body></html>