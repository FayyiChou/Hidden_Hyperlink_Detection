		<script type="text/javascript">
			$(function() {
				// 自动填写用户名
				if (getCookie("rememberEmail") != null) {
					$("#loginForm input[name='email']").val(getCookie("rememberEmail"));
					$("#loginForm input[name='password']").focus();
				} else {
					$("#loginForm input[name='email']").focus();
				}
				// 登录JS
				$("#loginForm").validate({
					rules: {
						email: {
							required: true,
							pattern: /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/
						},
						password: {
							required: true
						}
					},
					messages: {
						email: {
							email: "请输入正确的邮箱",
							required: "请填写邮箱",
							pattern: "请输入正确的邮箱地址",
						},
						password: {
							required: "请填写密码"
						}
					},
					submitHandler: function(form) {
						$.ajax({
							url: "/common/public_key/",
							type: "GET",
							dataType: "json",
							cache: false,
							beforeSend: function() {
								$(":submit").prop("disabled", true);
							},
							success: function(data) {
								var rsaKey = new RSAKey();
								rsaKey.setPublic(b64tohex(data.modulus), b64tohex(data.exponent));
								var enPassword = hex2b64(rsaKey.encrypt($("#loginForm input[name='password']").val()));
								$.ajax({
									url: "/login/submit/",
									type: "POST",
									data: {
										email: $("#loginForm input[name='email']").val(),
										enPassword: enPassword,
										service:1,
										remember: true
									},
									dataType: "json",
									cache: false,
									success: function(message) {
										addCookie("rememberEmail", $("#loginForm input[name='email']").val(), {expires: 365 * 24 * 60 * 60});
										if (message.type == "success") {
											if(message.content!=""){
												location.href = message.content;
											}else{
												location.href = "/member/myresume/";
											}
										} else {
											$(":submit").prop("disabled", false);
											alert(message.content);
										}
									}
								});
							}
						});
					}
				});
				
				// 注册JS
				$("#registerForm").validate({
					rules: {
						email: {
							required: true,
							pattern: /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/,
							remote: {
								url: "/register/check_email/",
								cache: false
							}
						},
						password: {
							required: true,
							pattern: /^[\@A-Za-z0-9\!\#\$\%\^\&\*\.\~]{6,20}$/
						},
						rePassword: {
							required: true,
							equalTo: "#registerForm input[name=password]"
						},
						treaty: {
							required: true
						}
					},
					messages: {
						email: {
							email: "请输入正确的邮箱",
							required: "注册邮箱必填",
							pattern: "请输入正确的邮箱地址",
							remote: "邮箱被禁用或已被注册"
						},
						password: {
							required: "登录密码必填",
							pattern: "请使用6~20个英文字母（区分大小写）、符号或数字"
						},
						rePassword: {
							required: "请重复输入密码",
							equalTo: "两次密码不一致"
						},
						treaty: {
							required: "请勾选用户协议"
						}
					},
					submitHandler: function(form) {
						$.ajax({
							url: "/common/public_key/",
							type: "GET",
							dataType: "json",
							cache: false,
							beforeSend: function() {
								$(":submit").prop("disabled", true);
							},
							success: function(data) {
								var rsaKey = new RSAKey();
								rsaKey.setPublic(b64tohex(data.modulus), b64tohex(data.exponent));
								var enPassword = hex2b64(rsaKey.encrypt($("#registerForm input[name='password']").val()));
								$.ajax({
									url: "/register/submit/",
									type: "POST",
									data: {
										
										
										email: $("#registerForm input[name='email']").val(),
										enPassword: enPassword
									},
									dataType: "json",
									cache: false,
									success: function(message) {
										if (message.type == "success") {
											$(":submit").prop("disabled", false);
											location.href="/";
										} else {
											alert(message.content);
											$(":submit").prop("disabled", false);
										}
									}
								});
							}
						});
					}
				});
			});
		</script>

	

