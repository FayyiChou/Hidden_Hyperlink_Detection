		function mr_echeck(str) {
        var at="@";
        var dot=".";
        var lat=str.indexOf(at);
        var lstr=str.length;
        var ldot=str.indexOf(dot);
        if (str.indexOf(at)==-1){
            return false;
        }
        if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
            return false;
        }
        if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
            return false;
        }
        if (str.indexOf(at,(lat+1))!=-1){
            return false;
        }
        if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
            return false;
        }
        if (str.indexOf(dot,(lat+2))==-1){
            return false;
        }
        if (str.indexOf(" ")!=-1){
            return false;
        }
        return true;
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for (var i=0;i<ca.length;i++){
        var c=ca[i];
        while(c.charAt(0)==' ') c=c.substring(1);
        if(c.indexOf(nameEQ)==0) return c.substring(nameEQ.length);
    }
    return null;
}

function checkForm722(myform){
    var fail=false;
    if (typeof _mri != 'undefined'){
        document.getElementById('contact_id722').value = readCookie('contact_id');
        document.getElementById('afft_722').value = readCookie('afft_');
        document.getElementById('ref_722').value = readCookie('ref_');
        document.getElementById('own_722').value = readCookie('own_');
        document.getElementById('sess_722').value = readCookie('sess_');
    }
    if (myform.firstname.value.trim().length == '0'){
        myform.firstname.style.border = '1px solid red';
        fail = true;
    }
    if (!mr_echeck(myform.email.value.trim())){
        myform.email.style.border = '1px solid red';
        fail=true;
    }
    if (fail){
        alert('Perfavore compila tutti i campi in rosso');
        return false;
    }
    return true;
}
