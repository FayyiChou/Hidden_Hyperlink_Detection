  <script>
    $(document).ready(function () {
        $(".rr-btn").click(function () {
            if ($(this).hasClass("rr-btn-on")) {
                $(this).removeClass("rr-btn-on");
                $(".rr-con .rr-cell:eq(1)").hide();
                $(".rr-con .rr-cell:eq(2)").hide();
            }
            else {
                $(this).addClass("rr-btn-on");
                $(".rr-con .rr-cell:eq(1)").show();
                $(".rr-con .rr-cell:eq(2)").show();
            }
        })
    });
	
</script> 
