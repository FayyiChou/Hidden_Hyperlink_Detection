  <script>
    jQuery(".bottomfd").slide({});

    $(document).ready(function () {
        $(".fdon").click(function () {
            $(".fdcont").slideToggle();
            $(".fdtab").addClass('hd');
            //$(".fdon").toggleClass('fdoff');
            $(".fdtabh").toggle('hd');
        });
        $(".conbtn0").mouseenter(function () {
            $(".fdcont").show();
            //$(".fdon").addClass('fdoff');
            $(".fdtabh").hide();
            $(".showbtn0").addClass('on');
            $(".showbtn0").siblings().removeClass('on');
            $(".ulbtn0").show();
            $(".ulbtn0").siblings().hide();
        });
        $(".conbtn1").mouseenter(function () {
            $(".fdcont").show();
            //$(".fdon").addClass('fdoff');
            $(".fdtabh").hide();
            $(".showbtn1").addClass('on');
            $(".showbtn1").siblings().removeClass('on');
            $(".ulbtn1").show();
            $(".ulbtn1").siblings().hide();
        });
        $(".conbtn2").mouseenter(function () {
            $(".fdcont").show();
            //$(".fdon").addClass('fdoff');
            $(".fdtabh").hide();
            $(".showbtn2").addClass('on');
            $(".showbtn2").siblings().removeClass('on');
            $(".ulbtn2").show();
            $(".ulbtn2").siblings().hide();
        });
        $(".conbtn3").mouseenter(function () {
            $(".fdcont").show();
            //$(".fdon").toggleClass('fdoff');
            $(".fdtabh").hide();
            $(".showbtn3").addClass('on');
            $(".showbtn3").siblings().removeClass('on');
            $(".ulbtn3").show();
            $(".ulbtn3").siblings().hide();
        });
        $(".bottomfd").mouseleave(function () {
            $(".fdcont").hide();
            $(".fdtabh").show();
            //$(".fdon").removeClass('fdoff');
        });
    });
</script> 
