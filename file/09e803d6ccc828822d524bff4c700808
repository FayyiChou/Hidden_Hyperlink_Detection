   <script>
        jQuery(".brand").slide({ titCell: ".tab li", mainCell: ".bd", trigger: "click" });
        jQuery(".zhuanti").slide({ titCell: ".tab li", mainCell: ".bd", trigger: "click" });
        jQuery(".video").slide({ titCell: ".tab li", mainCell: ".bd", trigger: "click" });
        jQuery(".newk").slide({ titCell: ".tab li", mainCell: ".bd", trigger: "click" });
        jQuery(".profour").slide({ mainCell: ".bd ul", effect: "fade", autoPlay: true });
        jQuery(".adright").slide({ titCell: ".hd ", mainCell: ".bd ul", effect: "leftLoop", autoPlay: true, autoPage: true });
        //jQuery(".prolist").slide({mainCell:".bd",effect:"leftLoop",autoPlay:true,delayTime:1000,interTime:5000});
        jQuery(".prolist .list li").each(function (i) { jQuery(".prolist .list li").slice(i * 10, i * 10 + 10).wrapAll("<ul></ul>"); });
        jQuery(".prolist").slide({ mainCell: ".list", autoPage: true, vis: 1, effect: "leftLoop", autoPlay: true, delayTime: 3000, interTime: 6000 });


        $(".search .tab li").bind('click', function (event) {
            var tht = $(this).text();
            $(this).siblings().removeClass("on");
            $(this).addClass("on");
            $(this).parents(".search").find("input[type='text']").attr("placeholder", "请输入" + tht + "关键字");
            $("#SearchType").val($(this).attr("data-src"));
            /* Act on the event */
        });
        $(".fasthtn .select").bind('click', function (event) {
            $(this).find(".list").show();
        });
        $(".fasthtn .select").bind('mouseleave', function (event) {
            $(this).find(".list").hide();
        });
        $(".fasthtn .provid .list li").live('click', function () {
            var text = $(this).text();
            var val = $(this).attr("data-val");
            var titw = $(this).parent().siblings(".onli").find(".titw");
            titw.text(text);
            titw.attr("data-val", val);
            $(this).parent().hide();
            $(".cityid .list").empty();
            loadCity(val, $(".cityid .list"));
            $(".fasthtn .cityid .titw").attr("data-val", "市").text("市");
        });
        $(".fasthtn .cityid .list li").live('click', function () {
            var text = $(this).text();
            var val = $(this).attr("data-val");
            var titw = $(this).parent().siblings(".onli").find(".titw");
            titw.text(text);
            titw.attr("data-val", val);
            $(this).parent().hide();
        });
        $(".fasthtn .keyone .list li").live('click', function () {
            //alert(1);
            var text = $(this).text();
            var val = $(this).attr("data-val");
            var titw = $(this).parent().siblings(".onli").find(".titw");
            titw.text(text);
            titw.attr("data-val", val);
            $(this).parent().hide();
            $(".keytwo .list").empty();
            loadKeyword(val, $(".keytwo .list"));
            $(".fasthtn .keytwo .titw").attr("data-val", "二级关键词").text("二级关键词");
            $(".keythree .list").empty();
            //loadKeyword(val, $(".keythree .list"));
            $(".fasthtn .keythree .titw").attr("data-val", "三级关键词").text("三级关键词");
        });
        $(".fasthtn .keytwo .list li").live('click', function () {
            var text = $(this).text();
            var val = $(this).attr("data-val");
            var titw = $(this).parent().siblings(".onli").find(".titw");
            titw.text(text);
            titw.attr("data-val", val);
            $(this).parent().hide();
            $(".keythree .list").empty();
            loadKeyword(val, $(".keythree .list"));
            $(".fasthtn .keythree .titw").attr("data-val", "三级关键词").text("三级关键词");
        });
        $(function () {
            $(".fasthtn .select .list").empty();
            loadCity(8, $(".provid .list"));
            loadKeyword(0, $(".keyone .list"));
        });
        function loadCity(id, obj) {
            $.post('/MillionKey/LoadCountry', { id: id }, function (data) {
                obj.empty();
                $(data).each(function (i) {
                    var option = $("<li>");
                    option.text(data[i].Name);
                    option.attr("data-val", data[i].Id);
                    option.appendTo(obj);
                });
            });
        }

        function loadKeyword(id, obj) {
            $.post('/MillionKey/LoadKeyWord', { id: id }, function (data) {
                obj.empty();
                $(data).each(function (i) {
                    var option = $("<li>");
                    option.text(data[i].Name);
                    option.attr("data-val", data[i].Id);
                    option.appendTo(obj);
                });
            });
        }
        //eof load
        //给等级提示
        var level = $(".name").find("span.dengji");

        var saylevel = "";
        level.each(function (index, val) {
            console.info(val);
            var levelname = $(val).attr("class");
            var levelname1 = levelname.split(" ");
            //console.info(val);
            switch (levelname1[2]) {
                case "newlevel_0":
                    saylevel = "免费会员";
                    break;
                case "newlevel_1":
                    saylevel = "免费会员";
                    break;
                case "newlevel_2":
                    saylevel = "银牌会员";
                    break;
                case "newlevel_3":
                    saylevel = "金牌会员";
                    break;
                case "newlevel_4":
                    saylevel = "铂金会员";
                    break;
                case "newlevel_5":
                    saylevel = "钻石会员";
                    break;
                case "newlevel_9":
                    saylevel = "10周年庆特别套餐合作供应商";
                    break;
                case "newlevel_8":
                    saylevel = "订单加油包会员";
                    break;
                case "newlevel_6":
                    saylevel = "托管会员";
                    break;
            }
            //                var tip = $("<div>", {
            //                    "class": "tips",
            //                    "html": saylevel
            //                });
            $(this).attr("title", saylevel);
