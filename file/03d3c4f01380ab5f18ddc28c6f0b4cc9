<script>
$(document).ready(function(){
$('.tabs li.with_icon a:not(:first)').addClass('inactive');
$('.pane.fluid').hide();
$('.pane.fluid:first').show();
$('.tabs li.with_icon a').click(function(){
  var t = $(this).attr('id');

  if($(this).hasClass('inactive')){ //this is the start of our condition 
    $('.tabs li.with_icon a').addClass('inactive');           
    $(this).removeClass('inactive');

    $('.pane.fluid').hide();
    $('#'+ t + 'C').fadeIn('slow');
 }
});
});
</script> 

