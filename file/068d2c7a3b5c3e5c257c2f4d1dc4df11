			$(function(){
				var ind = 0; 
				var nav= jQuery(".nav");
				var init = jQuery(".nav .m").eq(ind);
				var block = jQuery(".nav .block"); 
				block.css({"left":init.position().left}); 
				nav.hover(function(){
				},function(){ 
					block.hide();
					block.animate({"left":init.position().left},100);
				}); 
				jQuery(".nav").slide({ 
						type:"menu", //效果类型
						titCell:".m", // 鼠标触发对象
						targetCell:".sub", // 效果对象，必须被titCell包含
						effect:"slideDown",//下拉效果
						easing:"easeOutCubic",
						delayTime:100, // 效果时间
						triggerTime:0, //鼠标延迟触发时间
						defaultPlay:false, 
						returnDefault:true,
						defaultIndex:ind,
						startFun:function(i,c,s,tit){ 
							block.show();
							block.animate({"left":tit.eq(i).position().left},100);
						}
				});
			});
