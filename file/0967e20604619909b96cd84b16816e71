  <script type="text/javascript">
    //-------------------------------
    //导航
    function init_menu(){
        var $menu_default = $('.menu a.over');
        var menu_timer;
		var is_m_out = false;
        
        
        var mw = $('.menu').width();
        
        $('.menu a').each(function(index,element){
            $(this).attr('menu_index',index+1);
            
            var p = $(this).parent().position();
            var l = p.left;
            var w = $(this).parent().width();
            var ml = (1000-mw)+(l+(w/2));
            
            $(this).attr('l',ml);
        });
        $('.menu a').bind({
            'mouseover':function(){
                clearTimeout(menu_timer);
                var now_index = $(this).attr('menu_index');
                var last_index = $('.menu a.over').attr('menu_index');
                
                var now_mi = $(this).attr('mi');
                var last_mi = $('.menu a.over').attr('mi');
                //if(now_index!=last_index){
                    //关闭老的
                    $('.menu a.over').removeClass('over');
                    
                    //打开新的
                    $(this).addClass('over');
                    
                    var l = parseInt($(this).attr('l'));
					if(is_m_out){
						is_m_out = false;
					}
                //}
            },
            'mouseout':function(){
                menu_timer = setTimeout(menu_default,1000);
            }
        });
        
        function menu_default(){
            clearTimeout(menu_timer);
			is_m_out = true;
            if($menu_default.size()>0){
                $menu_default.trigger('mouseover');
            }else{
				is_m_out = false;
				$('.menu a.over').removeClass('over');
            }
        }
    }
    //导航
    
    
    $(function(){
        //导航
        init_menu();
    });
    </script> 
