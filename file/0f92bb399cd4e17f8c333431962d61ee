<script type="text/javascript">
    myComList();
    // 数据档案页面多个联动图表
    loadchart(2953);
    showArchivesChart(2953, 0, 0);

    // 图片查看
    $(".ftimgbox").imgshow();
    $(".da-xcsp .imgshowbox").imgshow();
    $(".da-xcsp .imgshowbox").each(function(index, el) {
        var t = $(this);
        var img = t.find('img');
        if(img.length>4){
            t.addClass('cur');
            var ul = t.find('ul');
            var x = 186;
            var s = img.length-4;
            var cs = 0;
            ul.width(img.length*x);
            t.find('.left').click(function(event) {
                if(cs<=0){cs = 0;return;}
                cs--;
                ul.animate({"margin-left":(-cs*x)}, 300);
                console.log(cs);
            });
            t.find('.right').click(function(event) {
                if(cs>=s){cs = s;return;}
                cs++;
                ul.animate({"margin-left":(-cs*x)}, 300);
                console.log(cs);
            });
        }
    });

    // 平台对比插件
    $(".common-header-da .right .on4 a:eq(1)").platDB(function(t) {
        t.html('<i class="da-icons da-ic-vs"></i>取消对比');
    },function(t) {
        t.html('<i class="da-icons da-ic-vs"></i>加入对比');
    });
    $(".gfzh-list-box .r .dbbtn").platDB(function(t) {
        t.text("取消");
    },function(t) {
        t.text("对比");
    });

    // 统计
    $.get("http://wwwservice.wdzj.com/api/statistic/platAccessStatistics",{"platId":$("#platId").val(),"channel":"1"},function(data){});
    $.get("http://www.wdzj.com/front_plat-update-views",{"platId":$("#platId").val()},function(data){});
</script>
<script>
$(function(){
    var r = $(".zzfwbox dd .dbjg"),
        r_h = r.height();
    if(r_h > 60){
        r.css({"height":"60px"});
        r.siblings(".r-all").show();
    }
    $(".r-all").click(function(){
        if($(this).hasClass("r-open")){
            r.animate({"height":"60px"},300);
            $(this).removeClass("r-open");
            $(this).html('展开全部<i class="daIcons"></i>');
        }else{
            r.animate({"height":r_h},300);
            $(this).addClass("r-open");
            $(this).html('收起<i class="daIcons"></i>');
        }
    })
});
</script>



