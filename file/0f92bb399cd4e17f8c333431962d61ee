<!-- 更新近30天动态分析数据 -->
<script>
    $(function() {
        var objs = $(".header-da-rb dd");
        var url = "http://wwwservice.wdzj.com/api/plat/platData30Days?platId="+$("#platId").val();

        objs.find('span').nextAll().remove();
        $.getJSON(url, function(data) {
            data = data.data;
            var platShujuMap = data.platShujuMap;

            // 平均预期收益率
            objs.eq(0).append("<em>" +(data.platOuterVo.platEarningsCode).toFixed(2) + "%</em>");
            // 预期投资期限
            objs.eq(1).append("<em>" +(data.platOuterVo.termWeight).toFixed(2) + "个月</em>");

            // 成交量（万元）
            if(platShujuMap["amount"] != null && platShujuMap["amount_percent"] != null){
                if(platShujuMap["amount"] != null){
                    objs.eq(2).append("<em>" +(platShujuMap["amount"]).toFixed(2)+ "</em>");
                }
                if(platShujuMap["amount_percent"] != null && platShujuMap["amount_percent"] > 0){
                    objs.eq(2).append('<em class="da-ic-up"><i class="da-icons"></i>' +(platShujuMap["amount_percent"]).toFixed(2)+ '%</em>');
                }else if(platShujuMap["amount_percent"] != null && platShujuMap["amount_percent"] < 0){
                    objs.eq(2).append('<em class="da-ic-down"><i class="da-icons"></i>' +Math.abs(platShujuMap["amount_percent"]).toFixed(2)+ '%</em>');
                }
            }else{
                objs.eq(2).append("<em>-</em>");
            }

            // 投资人数
            if( platShujuMap["bidder_num"]!= null && platShujuMap["bidder_num_percent"]!= null){
                if( platShujuMap["bidder_num"]!= null){
                    objs.eq(3).append('<em>'+platShujuMap["bidder_num"]+'</em>');
                }
                if( platShujuMap["bidder_num_percent"]!= null && platShujuMap["bidder_num_percent"] > 0){
                    objs.eq(3).append('<em class="da-ic-up"><i class="da-icons"></i>'+(platShujuMap["bidder_num_percent"]).toFixed(2)+'%</em>');
                }else if(platShujuMap["bidder_num_percent"]!= null && platShujuMap["bidder_num_percent"] < 0){
                    objs.eq(3).append('<em class="da-ic-down"><i class="da-icons"></i>'+Math.abs(platShujuMap["bidder_num_percent"]).toFixed(2)+'%</em>');
                }
            }else{
                objs.eq(3).append('<em>-</em>');
            }

            // 借款人数
            if( platShujuMap["borrower_num"]!= null && platShujuMap["borrower_num_percent"]!= null){
                if( platShujuMap["borrower_num"]!= null){
                    objs.eq(4).append('<em>'+platShujuMap["borrower_num"]+'</em>');
                }
                if( platShujuMap["borrower_num_percent"]!= null && platShujuMap["borrower_num_percent"] > 0){
                    objs.eq(4).append('<em class="da-ic-up"><i class="da-icons"></i>'+(platShujuMap["borrower_num_percent"]).toFixed(2)+'%</em>');
                }else if(platShujuMap["borrower_num_percent"]!= null && platShujuMap["borrower_num_percent"] < 0){
                    objs.eq(4).append('<em class="da-ic-down"><i class="da-icons"></i>'+Math.abs(platShujuMap["borrower_num_percent"]).toFixed(2)+'%</em>');
                }
            }else{
                objs.eq(4).append('<em>-</em>');
            }

            // 日资金净流入（万元）
            if( platShujuMap["net_inflow"]!= null && platShujuMap["net_inflow_percent"]!= null){
                if( platShujuMap["net_inflow"]!= null){
                    objs.eq(5).append('<em>'+(platShujuMap["net_inflow"]).toFixed(2)+'</em>');
                }
                if( platShujuMap["net_inflow_percent"]!= null && platShujuMap["net_inflow_percent"] > 0){
                    objs.eq(5).append('<em class="da-ic-up"><i class="da-icons"></i>'+(platShujuMap["net_inflow_percent"]).toFixed(2)+'%</em>');
                }else if(platShujuMap["net_inflow_percent"]!= null && platShujuMap["net_inflow_percent"] < 0){
                    objs.eq(5).append('<em class="da-ic-down"><i class="da-icons"></i>'+Math.abs(platShujuMap["net_inflow_percent"]).toFixed(2)+'%</em>');
                }
            }else{
                objs.eq(5).append('<em>-</em>');
            }

            // 日待还余额（万元）
            if( platShujuMap["money_stock"]!= null && platShujuMap["money_stock_percent"]!= null){
                if( platShujuMap["money_stock"]!= null){
                    objs.eq(6).append('<em>'+(platShujuMap["money_stock"]).toFixed(2)+'</em>');
                }
                if( platShujuMap["money_stock_percent"]!= null && platShujuMap["money_stock_percent"] > 0){
                    objs.eq(6).append('<em class="da-ic-up"><i class="da-icons"></i>'+(platShujuMap["money_stock_percent"]).toFixed(2)+'%</em>');
                }else if(platShujuMap["money_stock_percent"]!= null && platShujuMap["money_stock_percent"] < 0){
                    objs.eq(6).append('<em class="da-ic-down"><i class="da-icons"></i>'+Math.abs(platShujuMap["money_stock_percent"]).toFixed(2)+'%</em>');
                }
            }else{
                objs.eq(6).append('<em>-</em>');
            }

        });
    });
</script>
