    <script type="text/javascript" src="http://int.dpool.sina.com.cn/iplookup/iplookup.php?format=js"></script>
    <script type="text/javascript">
        var citysCache = [];
        if($.cookie('cityName') !='' &&　$.cookie('cityName')　!= null){
                $('#locate-cityName').text($.cookie('cityName'));
            $('#locate-city').text(remote_ip_info.city);
        }else{
            if(remote_ip_info.city == '') {
                $('#locate-cityName,#locate-city').text("合肥");
            }else{
                $('#locate-cityName,#locate-city').text(remote_ip_info.city);
            }

        }
        $.ajax({
            url:Global.webRoot+"/getCitys",
            type:"get",
            dataType:"json",
            success:function(data){
                var html = '';
                for(var o in data){
                    html += '<dl>';
                    html += '<dt class="highlight ng-binding">'+o+'</dt>';
                    var citys = data[o];
                    for(var i = 0,len=citys.length;i<len;i++){
                        var city = citys[i];
                        citysCache.push(city);
                        html += '<dd class="ng-scope"><a href="javascript:void(0);" data-code="'+city.code+'" class="ng-binding">'+city.name+'</a></dd>';
                    }
                    html += '</dl>';
                }
                $(html).appendTo($('#citys'));
            }
        });
        var _citySuggest = $('#city-suggest');
        $('#city-search').keyup(function(){
            var word = $.trim($(this).val()).toUpperCase();
            if(word==''){
                _citySuggest.empty();
                return;
            }
            var html = '';
            for(var i = 0,len = citysCache.length;i<len;i++){
                var cityName = citysCache[i].name;
                var cityNamePinyin = citysCache[i].namePinyin;
                var cityNamePinyinFirst = citysCache[i].namePinyinFirst;
                if(cityName.indexOf(word)!=-1||cityNamePinyin.indexOf(word)!=-1||cityNamePinyinFirst.indexOf(word)!=-1){
                    html += '<li><span class="pinyin">'+cityNamePinyin+'</span> <span>'+cityName+'</span></li>';
                }
            }
            _citySuggest.html(html);

        });
        /**
         * 搜索建议点击事件
         */
        _citySuggest.on('click','li',function(){
            var url = window.location.href;
            $.cookie('cityName', $(">span",this).eq(1).html());
            $('.city-wrap').hide();
            window.location.href = url;
        });
        /**
         * 自动定位城市点击事件
         */
        $('#locate-city').click(function(){
            var name = $(this).text();
            $.cookie('cityName', $(this).text());
            $(this).find('.locate-cityName').text(name);
            $('.city-wrap').hide();
            location.reload();
        });
        /**
         * 城市列表点击事件
         */
        $('#citys').on('click','dl dd a',function(){
            var url = window.location.href;
            $.cookie('cityName', $(this).text());
            $('.city-wrap').hide();
            window.location.href = url;
        });

    </script>
