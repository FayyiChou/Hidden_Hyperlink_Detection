		<script>   
			//点击显示地图弹层
			function forbidScrollFn(){
				var e = event;
				e.preventDefault();
				e.stopPropagation(); 
			} 
		
			$('#addressLink').on('click',function(){
				$('body').addClass('haslayer');
				$('#mapLayerBox').css('display','block');
				document.addEventListener("touchmove",forbidScrollFn,false);
			})
			
			$('#mapLayerBox .closed-btn').on('click',function(){ 
				$('body').removeClass('haslayer');
				$('#mapLayerBox').css('display','none'); 
				document.removeEventListener("touchmove",forbidScrollFn,false);
			}); 
			
			//点击报名
			
			;(function (){
				var carr = document.cookie.split("; ");
		       // var bbusername;  
		        //var bbusername = 'bradenhan'; //此处需要开发时候去掉，暂时 只是 起到模拟作用 ： hanjw20150917
		        
		        for(var i=0;i < carr.length;++i){   
		            var arr = carr[i].split('=');
		            if(arr[0] == 'bbusername')
		            {
		                bbusername = decodeURIComponent(arr[1]);
		                break;
		            }
		        }
		        
		        $('#registeLink').on('click',function(){
		        	if(bbusername){
			            	$.ajax({
			            		type:"POST",
			            		url:"/wapRegist/wapIndex", 
			            		dataType: 'json',
			            		data:{'status': 0, 'itemActiveId':4063},
		            			success : function(data){  
			            			if (data.exists==1){
			            				if(data.success == -5){
			            					alert('\u90ae\u7bb1\u672a\u6fc0\u6d3b\u7528\u6237\u4e0d\u5141\u8bb8\u62a5\u540d'); //邮箱未激活用户不允许报名
			            				}else if(data.success == -4){
			            					alert('\u8be5\u6d3b\u52a8\u5df2\u4e0d\u80fd\u518d\u62a5\u540d'); //该活动已不能再报名
			            				}else if(data.success == -3){
			            					alert('\u81ea\u5df1\u4e0d\u80fd\u62a5\u540d\u81ea\u5df1\u7684\u6d3b\u52a8'); //自己不能报名自己的活动
			            				}else if(data.success == -1){
			            					alert('\u9ed1\u540d\u5355\u7528\u6237\u4e0d\u80fd\u62a5\u540d'); //黑名单用户不能报名
			            				}else if(data.success == -2){
			            					alert('\u540d\u989d\u5df2\u6ee1'); //名额已满
			            				}else if(data.success == -6){
			            					alert('\u60a8\u7684\u7528\u6237\u4fe1\u606f\u4e0d\u5168\uff0c\u5e0c\u671b\u60a8\u80fd\u8865\u5168'); //用户信息不全，需要补全
			            					window.location.href="/regist-index-wap_4063_1.html"; 
			            				}else{ 
			            				    $('#registeLayerBox').css('display','block'); 
			            				}
			            			}else{
			            				window.location.href="/regist-index-wap_4063_1.html"; 
			            			}
		            			}
		            			,
		            			error: function(){ 
		            				alert('\u7f51\u7edc\u7e41\u5fd9\uff0c\u8bf7\u7a0d\u540e\u518d\u8bd5') //网络繁忙，请稍后再试
		            			}
			            	});
		        	}else{
		        		$(this).attr('href','http://m.fengniao.com/login.php?url='+window.location.href); //此处需要修改链接名称
		        	}
		        }); 
		        
		        $('#registeLayerBox').on('click','.confirm-button',function(){
		        	if(!$('#registeLayerBox').is(':hidden')){
        				window.location.href="/apply-wap-4063-1.html";
	        		}
		        }); 
			})()
			
