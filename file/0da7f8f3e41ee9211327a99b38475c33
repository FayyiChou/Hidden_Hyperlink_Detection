		<script type="text/javascript">
		function ok(obj){
			var obj = $(obj);
			var keyword = obj.children().eq(1).val();
			keyword = trim(keyword);
			if(keyword === ''){
				return false;
			}
			return true;
		}
		$(".cancel").click(function(){
		    $(".keyword").attr("value","");
		}); 
