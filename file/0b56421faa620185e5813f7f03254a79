    <script>
	$(function(){  
		var pages = 1;  
		var is = 4; //每版^?个图^?  
		//向后 按钮   
		$("a.nexts").click(function(){    //绑定click事件   
			 var contents = $("div#contents");   
			 var content_lists = $("div#content_lists");  
			 var v_widths = contents.width();  
			 var lens = contents.find("li").length;  
			 var page_counts = Math.ceil(lens / i) ;   
			 if( !content_lists.is(":animated") ){    //判断“内容展示区域”是否正在处于动^?  
				  if( pages == page_counts ){  //已经到最后一个版面了,如果再向后，必须跳转到第一个版面^?  
					content_lists.animate({ left : '0px'}, "slow"); //通过改变left值，跳转到第一个版^?  
					pages = 1;  
				  }else{  
					content_lists.animate({ left : '-='+v_widths }, "slow");  //通过改变left值，达到每次换一个版^?  
					pages++;  
				 }  
			 }  
	   });  
		//往^?按钮   
		$("a.prevs").click(function(){  
			 var contents = $("div#contents");   
			 var content_lists = $("div#content_lists");  
			 var v_widths = contents.width();  
			 var lens = contents.find("li").length;  
			 var page_counts = Math.ceil(lens / i);	  
			 if(!content_lists.is(":animated") ){    //判断“内容展示区域”是否正在处于动^?  
				 if(pages == 1 ){  //已经到第一个版面了,如果再向前，必须跳转到最后一个版面^?  
					content_lists.animate({ left : '-='+v_widths*(page_counts-1) }, "slow");  
					pages = page_counts;  
				}else{  
					content_lists.animate({ left : '+='+v_widths }, "slow");  
					pages--;  
				}  
			}  
		});  
	});  
	</script>
