<script type="text/javascript">
function closepop(){
	var MsgPop=document.getElementById("pop");
	var wds_bottom=document.getElementById("wds_bottom");
	MsgPop.style.display="none";//显示隐藏的窗口
	wds_bottom.style.display="block";//显示隐藏的窗口
}
function open_pop(){
	var MsgPop=document.getElementById("pop");
	var wds_bottom=document.getElementById("wds_bottom");
	MsgPop.style.display="block";//显示隐藏的窗口
	wds_bottom.style.display="none";//显示隐藏的窗口
}
</script>
