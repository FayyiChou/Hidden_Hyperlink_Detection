<script type="text/javascript">


function Selkeyword()
{


var _keyword=$("#keyword").val();
  $.post("/ajax.ashx?action=GetSelkeyword&t=" + Math.random(), {
        keyword: _keyword
    }, function(msg) {
       var sta = gav(msg, "state");
        var sMsg = gav(msg, "msg");
    
        for(var i=0;i<2;i++)
           {
            // $('#shkxs'+i).attr('display');
             
               if( document.getElementById("shkxs"+i).style.display=="block")
               {
           
                $("#shkxs"+i).html(sMsg);
               }
           }  

   
    });

}

    function ShowChecks(src) {
   
       $(src).parent().children().removeClass("cur");
        $(src).addClass("cur");
        var Index = $(src).attr("id");
        
         Index = Index.substring(5, Index.length);
        
         var ShowIndex = "shkxs" + Index
   for(var i=0;i<2;i++)
   {
        document.getElementById("shkxs"+i).style.display="none";
   }
    document.getElementById(ShowIndex).style.display="block";
    }
    var CheckIndex = "0";
    var SwichIDs = "#index_product";
    $(SwichIDs).find("#chkxs" + CheckIndex).addClass("cur");
   document.getElementById("shkxs0").style.display="block";
</script>


