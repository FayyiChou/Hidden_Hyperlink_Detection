newsSelectCurrentPosition();

function newsSelectCurrentPosition() {
    var lis = $(".gong ").find("dl");
 
    for (var i = 1; i <= lis.length; i++) {
      if(i%2==0){
            $(lis[i-1]).addClass("nones");  
        }
    }
}

</script>



