    $(document).ready(function () {
        var t;
        //二级菜单
        $(".menu ul li .erj").parent().hover(function () {
            if (t) {
                $(".menu ul li .erj").hide();
                clearTimeout(t);
                t = null;
            }
            $(this).find(".erj").show();
        }, function () {
            if (t) {
                clearTimeout(t);
                t = null;
            }
            var $this = $(this);
            t = setTimeout(function () {
                $($this).find(".erj").hide();
            }, 500);
        });
    });

