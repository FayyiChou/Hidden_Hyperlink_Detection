							<!-- 广告位：测试_新快报_头条_新闻页面_标题下方 -->
							<script type="text/javascript">left_1();</script><div id="_jweyo37o12oi529"></div><script charset="utf-8" src="http://pos.baidu.com/gcpm?di=3087830&amp;dri=0&amp;dis=4&amp;dai=3&amp;ps=223x0&amp;dcb=___adblockplus&amp;dtm=SSP_JSONP&amp;dvi=0.0&amp;dci=-1&amp;dpt=none&amp;tsr=0&amp;tpr=1491017605203&amp;ti=%E8%B0%A2%E9%9C%86%E9%94%8B%E8%B7%9F%E5%BC%A0%E6%9F%8F%E8%8A%9D%E4%B8%BA%E4%BB%80%E4%B9%88%E7%A6%BB%E5%A9%9A%EF%BC%9F%E5%8E%9F%E5%9B%A0%E5%8F%AA%E6%9C%89%E4%B8%80%E4%B8%AA%EF%BC%81_%E5%A8%B1%E4%B9%90%E9%A2%91%E9%81%93_%E5%B9%BF%E4%B8%9C%E5%A4%B4%E6%9D%A1&amp;ari=2&amp;dbv=2&amp;drs=1&amp;pcs=0x0&amp;pss=1002x223&amp;cfv=0&amp;cpl=0&amp;chi=1&amp;cce=true&amp;cec=UTF-8&amp;tlm=1484789961&amp;rw=320&amp;ltu=http%3A%2F%2Ftoutiao.xkb.com.cn%2Fa%2F170119171859339-2.html&amp;ecd=1&amp;uc=0x0&amp;pis=-1x-1&amp;ccd=0&amp;cja=false&amp;cmi=0&amp;col=en-US&amp;cdo=-1&amp;sr=0x0&amp;tcn=1491017605"></script><script type="text/javascript">
