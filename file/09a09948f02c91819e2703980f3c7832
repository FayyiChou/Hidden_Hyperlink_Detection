    function searchLabel(id) {
        var url ;
        if(id == -1) {
            url = "//m.cngold.org/kuaixun/";
        } else {
            if(currentPage == 1) {
                url = "//m.cngold.org/kuaixun/fl"+id+"/";
            } else {
                url = "//m.cngold.org/kuaixun/fl"+id+"/";
            }
        }
        location.href = url;
    }

