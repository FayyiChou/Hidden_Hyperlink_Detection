<script>

function showonline()
{
	if($("#onlineuser").css("display")=='none')
	{
		$("#onlineuser").show();
		online_user.focus();
	}
	else
	{
		$("#onlineuser").hide();
	}
}

function getByTopictype(id,mapnum)
{
	$(".mapnum_"+mapnum).attr("tpid",id);
	
	var topictype = "";
	var ordertype = 'desc';
	var orderby = "addtime";
	var topicstats = "";
	var topickeyword = "";
	var tpid = "";
	
	tpid = id+"|"+mapnum;
	
	var baseurl = $("#baseurl").val();
	
	orderf = "orderby-topictype,"+topictype+","+orderby+","+ordertype+","+topickeyword+","+tpid+","+topicstats;
	
	window.location = baseurl+orderf;
	
}

$(document).ready(function(){
	var pollopt_show = false;
	//var test=;
	//alert(test);
	$("#f_content").height("150px");
	if (pollopt_show)
	{
		
	}
	else
	{
		//alert(1);
		$(".pollopt").each(function(){
			$(this).hide();
		});
	}
	
	$("#AccMenuItem3").bind('mouseenter', function() 
	{
		$(this).attr("className","ccMenuItemTopOpen active_cat");
	})
	
	$("#AccMenuItem3").bind('mouseleave', function() 
	{
		$(this).attr("className","AccMenuItemTop");
		
		//this.className='AccMenuItemTop';
	})
	
	$("#word").keydown(function(e)
	{
		if(e.keyCode == 13 && !e.ctrlKey)
	 	{
	 		$("#search_word").click();
	 	}
			
    })
	
	$(".top-select").bind('mouseenter', function() 
	{
		$(".select-list").show();
	})
	
	$(".top-select").bind('mouseleave', function() 
	{
		$(".select-list").hide();
	})
	$(".top-select ul li").click(function()
	{
		var search_type = $(this).attr("searchtype");
		var typename = $(this).attr("typename");
		$(".input-select_hidden").val(search_type);		
		$(".input-select").val(typename);
		$(".select-list").hide();
	})	
	
		/*-------页面头部导航 start------*/
	$(".mainNav a").mouseover(function(){
		optdivname = "secondNav";//本机测试时需开启
	
		$(".secondNav").show();
		//修改选中与否状态
		$(".mainNav a").attr("class","");
		$("#"+this.id).attr("class","actived");
		
		var currentMenuNo = parseInt(this.id.substring(1));
		
		if(lastnavdiv != currentMenuNo)
		{
			$("#subNav"+lastnavdiv).hide();
			lastnavdiv = currentMenuNo;
		}
		else
		{
			//$("#subNav"+lastnavdiv).hide();
		}
	
	
		if(!$("#secondNav").find("#subNav"+currentMenuNo))
		{
			return ;
		}
	
		$(".subNav").each(function(){
			$(this).hide();
		});
		
		
		$("#subNav"+currentMenuNo).show();
		
		$("#subNav"+currentMenuNo).css("position","absolute");
	
	
		mainnavtop = $(this).offset().top;
		mainnavleft = $(this).offset().left;
		
		secNavleft = (mainnavleft-96-7+5+($(this).width()/2))+"px";
		
		if(currentMenuNo!=14)
		{
			$("#subNav"+currentMenuNo).css("left",secNavleft);
			//$("#subNav"+currentMenuNo).css("top",(mainnavtop+28)+"px");
			$("#subNav"+currentMenuNo).css("top","-2px");
			$("#subNav"+currentMenuNo).css("z-index","111");
		}
		else
		{
			subNav13w = 725-5;
			secNavleft = (mainnavleft-subNav13w+36-12+($(this).width()/2)-13)+"px";
			$("#subNav"+currentMenuNo).css("left",secNavleft);
			//$("#subNav"+currentMenuNo).css("top",(mainnavtop+28)+"px");
			$("#subNav"+currentMenuNo).css("top","-2px");
			$("#subNav"+currentMenuNo).css("z-index","111");
		}
		$("#subNav"+currentMenuNo).show();
		
		if ((navigator.userAgent.indexOf('MSIE') >= 0) && (navigator.userAgent.indexOf('Opera') < 0)){
			$(".secondNav").show();
		}else{
			if(currentMenuNo == 5 || currentMenuNo == 6 || currentMenuNo == 8 || currentMenuNo == 10 || currentMenuNo == 14){
				$(".secondNav").show();
			} else {
				$(".secondNav").hide();
			}
		}
		
		//optdivname = "secondNav";
		var hideintval = '';
		clearInterval(hideintval);
		hideintval = setInterval("hideadminopt()",1000);
		onoptlist = 1;	
	
	
	});


	$(".subNav").mouseout(function(){
		onoptlist = 0;
	})
	
	$(".subNav").mouseover(function(){
		onoptlist = 1;
	})
	
		/*-----搜索start-----*/
	$("#search_word").click(function(){
		var word = $("#word").val();
		var searchtype = $(".input-select_hidden").val();
		searchWord(word,searchtype);
	});
	
	$(".search_exists_word").click(function(){
		var word = $(this).attr("word");
		searchWord(word,2);
	});
	
	function searchWord(word,searchtype)
	{
		var url="http://bbs.chinaacc.com/searchchinaacc.php?keyword="+word+"&searchtype="+searchtype;
		window.location.href=url;
	//	window.open(url,"_blank");
	}
	/*-----搜索end-----*/	
	
	/*-----加V显示-----*/
	
	
	
	$(".applyv").bind('mouseenter', function(e) {
		tag = $(this).attr('tag');
		$("#v-"+tag).show();
		
	})

	$(".applyv").bind('mouseleave', function() 
	{
		tag = $(this).attr('tag');
		$("#v-"+tag).hide();
	})
	
});

</script>
