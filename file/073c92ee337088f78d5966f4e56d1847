		<script type="text/javascript">
			savedatat=1;
		    savedatac = 1;
		    autosave = 0;
			if(wysiwyg) {							
				newEditor(1, bbcode2html(textobj.value));
			} else {				
				newEditor(0, textobj.value);
			}	
			//var b = new Object();			
		   // b=$_(editorid + '_svd');
		   // alert(b.disabled);
		  //  b.disabled=true;
		    
			var ATTACHNUM = {'imageused':0,'imageunused':0,'attachused':0,'attachunused':0};
			function switchImagebutton(btn) 
			{		
				var btns = ['www', 'imgattachlist', 'albumlist'];
				btns.push('multi');
				switchButton(btn, btns);
				$_(editorid + '_image_menu').style.height = '';
			}				
			function switchAttachbutton(btn) {
				var btns = ['attachlist'];
