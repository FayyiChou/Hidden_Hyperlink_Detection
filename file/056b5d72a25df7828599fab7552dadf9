            <SCRIPT LANGUAGE="JavaScript">

function showblg(t){
	document.getElementById("showblogmu1").style.color='#707070';
	document.getElementById("showblogmu2").style.color='#707070';
	if(t==1){
		document.getElementById("blogshow1").style.display='';
		document.getElementById("blogshow2").style.display='none';
		document.getElementById("showblogmu1").style.color='#990000';	
	}else{
		document.getElementById("blogshow1").style.display='none';
		document.getElementById("blogshow2").style.display='';
		document.getElementById("showblogmu2").style.color='#990000';			
	}
}

