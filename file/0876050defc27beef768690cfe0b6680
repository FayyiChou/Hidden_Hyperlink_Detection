                            <script>
                               var pspeed = 50; //数字越大速度越慢
                                var ptab = document.getElementById("pemo");
                                var ptab1 = document.getElementById("pemo1");
                                var ptab2 = document.getElementById("pemo2");
                                ptab2.innerHTML = ptab1.innerHTML;
                                function pMarquee() {
                                    if (ptab2.offsetWidth - ptab.scrollLeft <= 0)
                                        ptab.scrollLeft -= ptab1.offsetWidth
                                    else {
                                        ptab.scrollLeft++;
                                    }
                                }
                                var pMyMar = setInterval(pMarquee, pspeed);
                                ptab.onmouseover = function () { clearInterval(pMyMar) };
                                ptab.onmouseout = function () { pMyMar = setInterval(pMarquee, pspeed) };
                            </script>
                	</div>
                </div>
                </div>
            </div>




