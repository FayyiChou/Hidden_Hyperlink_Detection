<script type="text/javascript">


      

      $(document).ready(function(){
           //假设是在主框架内，则需要刷新整个主框架,跳转到最外面的首页
		   var rightiframe =  $("#rightiframe",window.parent);
           if(rightiframe){
		      $("#loginIndex").click(function(event){
				   window.parent.location.href="http://www.xxtyd.fj.cn/";
			  });
		   }else{
		      $("#loginIndex").click(function(event){
				   window.location.href="http://www.xxtyd.fj.cn/";
			  });
		   }


      });

</script>

