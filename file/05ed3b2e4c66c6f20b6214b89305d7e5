<script type="text/javascript">
function  changeLink(){
	var  name =document.getElementById("name").value;
	var  password =document.getElementById("password").value;  
	var  jcaptcha =document.getElementById("jcaptcha").value;
	var  selec =document.getElementById("sele").value;
	
	if(name==null||name==""){
		alert("用户名不能为空，请确认！");
		return false;
}
	if(password==null||password==""){
		alert("密码不能为空，请确认！");
		return false;
}
	if(jcaptcha==null||jcaptcha==""){
		alert("验证码不能为空，请确认！");
		return false;
}
	if(selec==null||selec==""){
		alert("请您选择要登录的会员类型！");
		return false;
}
document.getElementById('form1').action=document.getElementById("sele").value;
document.getElementById('form1').submit();
}
 function fetchJcap(){
 	document.getElementById("captcha").src="/dlkf/jcaptcha?"+Math.random();
 }
</script>

