}

var oText_elcojkembghhbbobilfgolojkmdefdpo = $("idPicText_elcojkembghhbbobilfgolojkmdefdpo"), arrImg_elcojkembghhbbobilfgolojkmdefdpo = [];
var oNum_elcojkembghhbbobilfgolojkmdefdpo = $("idNum_elcojkembghhbbobilfgolojkmdefdpo"), arrNum_elcojkembghhbbobilfgolojkmdefdpo = [];

//设置图片列表
Each(rvt_elcojkembghhbbobilfgolojkmdefdpo.List, function(list, i){
    //图片式
    var img = document.createElement("img");
    img.src = list["img"]; img.alt = list["text"];
    arrImg_elcojkembghhbbobilfgolojkmdefdpo[i] = img;
    //按钮式
    var li = document.createElement("li");
    li.innerHTML = i + 1;
    li.title = list["text"];
    arrNum_elcojkembghhbbobilfgolojkmdefdpo[i] = li;
    oNum_elcojkembghhbbobilfgolojkmdefdpo.appendChild(li);
    //事件设置
    img.onmouseover = li.onmouseover = function(){ rvt_elcojkembghhbbobilfgolojkmdefdpo.Auto = false; rvt_elcojkembghhbbobilfgolojkmdefdpo.Index = i; rvt_elcojkembghhbbobilfgolojkmdefdpo.Start(); };
    img.onmouseout = li.onmouseout = function(){ rvt_elcojkembghhbbobilfgolojkmdefdpo.Auto = true; rvt_elcojkembghhbbobilfgolojkmdefdpo.Start(); };
});

//设置图片列表样式 文本显示区域
rvt_elcojkembghhbbobilfgolojkmdefdpo.onShow = function(){
    var i = this.Index, list = this.List[i];
    //按钮式
    Each(arrNum_elcojkembghhbbobilfgolojkmdefdpo, function(o){ o.className = ""; }); arrNum_elcojkembghhbbobilfgolojkmdefdpo[i].className = "on";
    //文本区域
    var clickable = "0";
    oText_elcojkembghhbbobilfgolojkmdefdpo.innerHTML = !!list.url ? ("<a" + (clickable=="1"?" href='" + list.url + "' target='_blank'":"") + ">" + list.text + "</a>") : list.text;
}

//文本显示区域
oText_elcojkembghhbbobilfgolojkmdefdpo.onmouseover = function(){ rvt_elcojkembghhbbobilfgolojkmdefdpo.Auto = false; rvt_elcojkembghhbbobilfgolojkmdefdpo.Stop(); };
oText_elcojkembghhbbobilfgolojkmdefdpo.onmouseout = function(){ rvt_elcojkembghhbbobilfgolojkmdefdpo.Auto = true; rvt_elcojkembghhbbobilfgolojkmdefdpo.Start(); };

//开始执行
setTimeout("rvt_elcojkembghhbbobilfgolojkmdefdpo.Start()", 0);
</script>

 
</div>          

