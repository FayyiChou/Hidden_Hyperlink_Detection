<script>
//焦点图js开始
(function($, $$) {
    var focus = K.focus = function(args) {
        return new _focus(args)
    }
   	
	_focus = function(args){
		var _this = this;
		this.dot = $$(args.dot);
		this.con = $(args.con);
		this.wrap = $(args.wrap);
		this.Time = args.Time;
		this.isAuto = args.isAuto;
		this.ul = $$(args.ul).item(0);
		this.slidT = null;
		var h = this.con.height();
		
		this.ul.node.style.height = 3*h + 'px';
		this.cur = 0;
		//alert(this.ul.width());
		this.dot.each(function(o, i){
			o.click(function(){
				_this.move(i);
			});	
		});
		this.wrap.bind('mouseover', function(){
			clearInterval(_this.slidT);
		});
		this.wrap.bind('mouseout', function(){
			_this.AutoPlay();
		});
		this.AutoPlay();	
	}
	
	_focus.prototype = {
		 move : function(n){
			var _this = this;
			this.cur = n; 
			var h = this.con.height(), l = this.ul.css('top');
			this.ul.go({
				top : - (n * h)	+ 'px'
			}, 'defaults', 'easeIn');
			this.clearHover(n);
		 },
		 clearHover : function(n){
			 var _this = this;
			for(var i = 0, len = _this.dot.length; i < len; i++){
				if(i == n){ _this.dot.item(n).addClass('hover'); }else{_this.dot.item(i).removeClass('hover')}	
			}	 
		 },
		 AutoPlay: function() {
			 var _this = this;
           if(this.isAuto){
			   this.slidT = setInterval(function(){
					_this.cur = _this.cur + 1 > 2 ? 0 : _this.cur + 1; 
					_this.move(_this.cur); 
				}, _this.Time);
			}
        }	
	}
	
})(K, KK);
//焦点图js结束

//焦点图js调用开始
args1 = {
	wrap : 'focus-wrap',
	con : 'focus-pic',
	ul : '#focus-pic ul',
	dot : '#f-dot span',
	Time : 3000,
	isAuto : true	
} 
K.focus(args1);
//焦点图js调用结束
</script>
