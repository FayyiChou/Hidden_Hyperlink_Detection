<script type="text/javascript" src="/assets/js/Koala.js"></script>
<script type="text/javascript" src="/assets/js/plusfx1.js"></script>
<script type="text/javascript">
//最新信息价滚动代码
$(function(){
	var ProListUl = $("#scrollobj ul");//滚动区域
	var len = $("#scrollobj ul li").length;
	var liHeight = $("#scrollobj ul li").height();

	var moveToTop;//滚动变量
	ProListUl.hover(function(){
		clearInterval(moveToTop);//鼠标进入指定的区域停止动画
	},function(){
		moveToTop = setInterval(function(){ //定义一个滚动时间间隔的方法
			ProListUl.stop().animate({marginTop:-liHeight},300,function(){  //定义每次滚动距离，时间
				ProListUl.find("li:first").appendTo(ProListUl).parent().css({marginTop:0}); //把css样式调回到原始的初始状态
			});
		},2400);//滚动的时间间隔
	}).trigger("mouseleave");//模拟鼠标离开时的时间，让此动画一开始就执行
	
	//新增会员多少条滚动代码
	var ProListUlNewadd = $("#scrollobj_newadd ul");
	var lenNewadd = $("#scrollobj_newadd ul li").length;
	var liHeightNewadd = $("#scrollobj_newadd ul li").height();

	var moveToTopNewadd;
	ProListUlNewadd.hover(function(){
		clearInterval(moveToTopNewadd);
	},function(){
		moveToTopNewadd = setInterval(function(){ 
			ProListUlNewadd.stop().animate({marginTop:-liHeightNewadd},1000,function(){  
				ProListUlNewadd.find("li:first").appendTo(ProListUlNewadd).parent().css({marginTop:0}); 
			});
		},6000);
	}).trigger("mouseleave");
	
	//呼助滚动代码
	var ProListUlHuzhu = $("#scrollobj_huzhu ul");//滚动区域
	var lenHuzhu = $("#scrollobj_huzhu ul li").length;
	var liHeightHuzhu = $("#scrollobj_huzhu ul li").height();

	var moveToTopHuzhu;//滚动变量
	ProListUlHuzhu.hover(function(){
		clearInterval(moveToTopHuzhu);//鼠标进入指定的区域停止动画
	},function(){
		moveToTopHuzhu = setInterval(function(){ //定义一个滚动时间间隔的方法
			ProListUlHuzhu.stop().animate({marginTop:-liHeightHuzhu},300,function(){  //定义每次滚动距离，时间
				ProListUlHuzhu.find("li:first").appendTo(ProListUlHuzhu).parent().css({marginTop:0}); //把css样式调回到原始的初始状态
			});
		},2400);//滚动的时间间隔
	}).trigger("mouseleave");//模拟鼠标离开时的时间，让此动画一开始就执行
	
	
});
</script>
