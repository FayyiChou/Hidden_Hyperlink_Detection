<script type="text/javascript">
	function setSpeClass(){
     $(".infor-wrap-child").each(function(i){
	     var abc=$(this).children(".tour-know");
		 abc.each(function(n){
			 var length=abc.length-1;
		     if(n==length){
			    $(this).addClass("spe");
			 }
		 })
	 })
  }
  function setActive(){
    
    var aList= $("#menu").find("a");
	var chnl="行李运输服务";
    aList.each(
              function(i)
   {
	     if($(this).text()==chnl)
         {
	         if(!$(this).hasClass("active"))
              {
		            $(this).addClass("active");
		           }
	        }
       else
         {
	          if($(this).hasClass("active"))
               {
		             $(this).removeClass("active");
		            }
	       }
	  }
        );
	var divList=$(".infor-wrap-child");
	divList.each(function(i){
	
	  if($(this).attr("id")==chnl){
	     $(this).show();
	  }else{
	     $(this).hide();
	  }
	});
  }
$(document).ready(function(){
 setActive();
 setSpeClass()
});
</script></head>




