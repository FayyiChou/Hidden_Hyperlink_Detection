            alert("请注意：此问卷红"+"包已被之前参与的用户领取完");
           else if(needHBAlert==-2)
               alert("请注意：抽中红"+"包后答卷还需要发布者审核，通过审核后才能发放红"+"包！");
           else if(needHBAlert==-3)
               alert("提示：只有通过微信作答才能抽取红"+"包");
           else
            alert("请注意：每个参与者填完问卷后有1/" + needHBAlert + "的概率抽中红"+"包");
        }
         var needAwardAlert=0;
        if(needAwardAlert){
            if(needAwardAlert<0)
                alert("请注意：此问卷奖品已被之前参与的用户领取完");
            else if(needAwardAlert==999)
                alert("请注意：此问卷平均每天低于1人抽中奖品");
            else if(needAwardAlert%1000==0)
                alert("请注意：此问卷平均每天会有" + (needAwardAlert/1000) + "人抽中奖品");
            else
                alert("请注意：每个参与者填完问卷后有1/" + needAwardAlert + "的概率抽中奖品");
        }
    </script>
   
    
      <script>
          var hasReferClient=1;
          function createItem(par,isSort) {
              var refered = $(par).attr("refered");
              if (!refered) return;
              var referedTopicArray = refered.split(',');
              for (var jjj = 0; jjj < referedTopicArray.length; jjj++) {//被引用的题目
                  var referTopic = referedTopicArray[jjj];
                  var q = questionsObject[referTopic];
                  if (!q) continue;
                  var displayContent = false; //是否显示内容
                  var divRef = document.getElementById("divRef" + referTopic);
                  if (!divRef) continue;
                  var startIndex = 0;
                  var type = q.attr("type");
                  var selEle = "input[type=checkbox]";
                  if (isSort) selEle = "input[type=hidden]";
                  var qtopic=q.attr("topic");
                  var groups=[];var hashGroups=new Object();var prevGroup=null;
                  var hasLabel=document.getElementById("lbl"+qtopic+"_1")?true:false;
                  switch (type) {
                      case "6":
                      case "9":
                      case "10":
                      case "12":
                          if(window.cepingCandidate)
                              continue;
                          if (type == "6")//标题项
                              startIndex = 1;
                        
                          $(selEle, $(par)).each(function () {
                              var cItem = this;
                              var isCheck = false;
                              if (isSort) isCheck = $(this).parents("li").attr("check")=="1";
                              else isCheck = cItem.checked;

                              var rvStr = "drv"+ qtopic+"_"+ cItem.value+"t";
                              var rvtitle=document.getElementById(rvStr);
                              if(rvtitle)
                                  rvtitle.style.display = isCheck ? "" : "none";
                              rvStr = "drv"+ qtopic+"_"+ cItem.value;
                              var rvcontent=document.getElementById(rvStr);
                              if(rvcontent){
                                  rvcontent.style.display = isCheck ? "" : "none";
                                  if (type == "12" || type == "9")
                                      initqSlider($(rvcontent));
                              }
                              if (isCheck) {
                                  displayContent = true; var rel = $(cItem).attr("rel");
                                  if (rel) {
                                      var itemText = document.getElementById(rel);
                                      if (!itemText.onblur) {
                                          itemText.onblur = function () {
                                              createItem(par);
                                          }
                                      }
                                      var val = itemText.value;
                                      var rvttt=rvtitle;
                                      if(!rvttt)rvttt=rvcontent;
                                      var th = $(".rowother", rvttt)[0];
                                      if (th) {
                                          if (val && val != defaultOtherText)
                                              th.innerHTML = "[<span style='color:red;'>" + val + "</span>]";
                                          else
                                              th.innerHTML = "";
                                      }
                                  }
                              }
                              if(hasLabel){
                                  var group=$((rvtitle||rvcontent)).attr("group");
                                  if(group){
                                      prevGroup=group;
                                      groups.push(group);
                                  } 
                                  if(prevGroup){
                                      if(isCheck && !hashGroups[prevGroup]){
                                          hashGroups[prevGroup]="1";
                                      }
                                  }
                              }
                          });
                          divRef.style.display = displayContent ? "none" : "";
                          q.displayContent = displayContent;
                          break;
                      case "3":
                      case "4":
                      case "11":
                          var selrc = ".ui-radio";
                          if (type == "4") selrc = ".ui-checkbox";
                          else if(type=="11") selrc=".ui-li-static";
                          var itemInputs = $(selrc, q);
                          var groups=[];var hashGroups=new Object();
                          var hasLabel=document.getElementById("lbl"+qtopic+"_1")?true:false;
                          $(selEle, $(par)).each(function () {
                              var cItem = this;
                              var cIndex = parseInt(cItem.value) - 1;
                              var isCheck = false;
                              if (isSort) isCheck = $(this).parents("li").attr("check") == "1";
                              else isCheck = cItem.checked;
                              itemInputs[cIndex].style.display = isCheck ? "" : "none"; //显示与隐藏li
                              if (isCheck) displayContent = true;
                              var rel = $(cItem).attr("rel");
                              if (rel) {
                                  var itemText = document.getElementById(rel);
                                  if (!itemText.cblur) {
                                      itemText.cblur = true;
                                      $(itemText).bind("blur", function () {
                                          createItem($(par));
                                      });
                                  }
                                  var mrel = $(itemInputs[cIndex]).find("input[type=text]")[0]; // document.getElementById();
                                  if (mrel)
                                      mrel.value = itemText.value;
                              }
                              if(hasLabel){
                                  var group=$(itemInputs[cIndex]).attr("group");
                                  if(group){
                                      prevGroup=group;
                                      groups.push(group);
                                  } 
                                  if(prevGroup){
                                      if(isCheck && !hashGroups[prevGroup]){
                                          hashGroups[prevGroup]="1";
                                      }
                                  }
                              }
                          });
                          divRef.style.display = displayContent ? "none" : ""; //显示与隐藏提示信息
                          q.displayContent = displayContent;
                          break;
                  }
                  if(hasLabel){
                      for(var xxx=0;xxx<groups.length;xxx++){
                          var group=groups[xxx];
                          var needDisplay=hashGroups[group];
                          var did="lbl"+referTopic+"_"+group;
                          var ele=document.getElementById(did);
                          if(ele)ele.style.display=needDisplay?"":"none";
                      }
                  }
              }
          }
      </script>
     
