    <script type="text/javascript">
        $(window).load(function () {
            $('#slider').nivoSlider({
                prevText: '上一张',
                nextText: '下一张'
            });

            $('#scrollArea').marquee('pointer').mouseover(function () {
                $(this).trigger('stop');
            }).mouseout(function () {
                $(this).trigger('start');
            }).mousemove(function (event) {
                if ($(this).data('drag') == true) {
                    this.scrollLeft = $(this).data('scrollX') + ($(this).data('x') - event.clientX);
                }
            }).mousedown(function (event) {
                $(this).data('drag', true).data('x', event.clientX).data('scrollX', this.scrollLeft);
            }).mouseup(function () {
                $(this).data('drag', false);
            });
        });

        function chgSaleInfo(v) {
            switch (v) {
                case "1":
                    document.getElementById("buylist").style.display = "none";
                    document.getElementById("saleslist").style.display = "";
                    document.getElementById("salestitle").style.color = "#1285a1";
                    document.getElementById("buytitle").style.color = "#999999";  
                    break;
                case "2":
                    document.getElementById("buylist").style.display = "";
                    document.getElementById("buytitle").style.color = "#1285a1";
                    document.getElementById("saleslist").style.display = "none";
                    document.getElementById("salestitle").style.color = "#999999";   
                   
                    break;
            }
        }
    </script>
