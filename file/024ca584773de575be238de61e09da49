<SCRIPT language="javascript" type="text/javascript">
    <!--//--><![CDATA[//><!--
    jQuery(".bannerSlide").slide({mainCell:".bd ul",autoPlay:true});
    jQuery(".pp0").slide({mainCell:".bd ul",autoPlay:true});
    //jQuery(".Zjtd").slide({mainCell:".Zjtd .details .detail",autoPlay:true,effect:"leftMarquee",vis:1,interTime:50,trigger:"click"});
    //-->
    function show(c,n){
        $("."+c+" .details").find('.detail').hide();
        $("."+c+" .details").find('.detail').eq(n-1).show();
        $("."+c).find('.topnav li').removeClass('on').eq(n-1).addClass('on');
    }
    function showal(c,n){
        $(c).find('.d_left').children().hide();
        $(c).find('.d_left').children().eq(n-1).show();
        $(c).find('.d_right li').removeClass('on').eq(n-1).addClass('on');
    }
</SCRIPT> 