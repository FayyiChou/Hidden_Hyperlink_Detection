				<script>
				$(function(){
					$(".search-listBox input[name='checkbox1']").change(function(){
						$(this).parent().siblings().find("input[name='checkbox1']").removeAttr('checked');
						if($(this).is(":checked")){
							$(".dataAction").addClass("on");
						}else{
							$(".dataAction").removeClass("on");
						}
					});
					$("#downBtn").click(function(){
						var _check = $(".search-listBox input[name='checkbox1']:checked");
						if(_check.length > 0){
							var _data = _check.val();
							XS.ajax('/file/' + _data + '.html', {} , 
								function(data){
									location.href = data.message;
								},
								function(data){
									XS.alert(data.message);
								}
							);
						}
					});
				});	
				</script>
