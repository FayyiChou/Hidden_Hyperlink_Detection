     <td id="butong_net_left2" valign="top"></td> 
     </tr> </table>
      </div>
     <script> 
     var speed=30
     butong_net_left2.innerHTML=butong_net_left1.innerHTML
     function Marquee3(){ 
     if(butong_net_left2.offsetWidth-butong_net_left.scrollLeft<=0) butong_net_left.scrollLeft-=butong_net_left1.offsetWidth 
     else{ butong_net_left.scrollLeft++ } }
      var MyMar3=setInterval(Marquee3,speed)
      butong_net_left.onmouseover=function() {
      clearInterval(MyMar3)}
      butong_net_left.onmouseout=function() 
      {MyMar3=setInterval(Marquee3,speed)} 
     </script> 
  </div>
</div>
</div>
