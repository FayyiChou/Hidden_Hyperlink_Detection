<!-- 漂浮广告代码
<div id="ad2" style="position:absolute">
<a href="dmnj.htm" target="_blank"><img src="dmnj.gif" width="180" height="120" border="0"></a></div>
  <script>
  var x = 50,y =100
  var xin = false, yin = false
  var step = 2 
  var delay = 40
  var obj=document.getElementById("ad2") 
  function floatAD2() {
     var L=T=20
     var R= document.body.clientWidth-obj.offsetWidth-500
     var B = document.body.clientHeight-obj.offsetHeight
     obj.style.left = x + document.body.scrollLeft
     obj.style.top = y + document.body.scrollTop
     x = x + step*(xin?1:-1)  
     if (x < L) { xin = true; x = L} 
     if (x > R){ xin = false; x = R} 
     y = y + step*(yin?1:-1) 
     if (y < T) { yin = true; y = T } 
     if (y > B) { yin = false; y = B } 
  }
  var itl= setInterval("floatAD2()", delay) 
obj.onmouseover=function(){clearInterval(itl)} 
obj.onmouseout=function(){itl=setInterval("floatAD2()", delay)}
  </script>

 漂浮广告代码结束 --> 



