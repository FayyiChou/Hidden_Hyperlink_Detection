    <script type="text/javascript">
        var leh1 = $(".cont_left").offset().top;
        var leh2 = leh1 + $(".cont_left").height();
        var leh3 = leh2 - $("#adfloat").height(); //左边底部有20px行高的空白
        var rih1 = $("#adfloat").offset().top;
        var rih2 = rih1 + $("#adfloat").height();
        $(document).ready(function() {
            setTimeout(function() {
                leh1 = $(".cont_left").offset().top;
                leh2 = leh1 + $(".cont_left").height();
                leh3 = leh2 - $("#adfloat").height(); //左边底部有20px行高的空白
                rih1 = $("#adfloat").offset().top;
                rih2 = rih1 + $("#adfloat").height();
            }, 1000);
            $(window).scroll(loadData);
        });
        function loadData() {
            var now = $(window).scrollTop();
            if (now >= rih1 && now <= leh3) {
                $("#adfloat").css("position", "fixed");
                $("#adfloat").css("top", "0px");
                $("#adfloat").css("bottom", "auto");
            }
            else if (now >= leh3) {
                $("#adfloat").css("position", "absolute");
                $("#adfloat").css("top", leh3 + "px");
                $("#adfloat").css("bottom", "auto");
            }
            else {
                $("#adfloat").css("position", "static");
                $("#adfloat").css("top", "0px");
                $("#adfloat").css("bottom", "auto");
            }
        }
    </script>
    <script>
        $(function() {
            $("#btn_reset_userd").click(function() {
                $("#ipt_qq").val('');
                $("#texta_descript").val('');
            });
            $("#btn_submit_user").click(function() {
                var qq = $.trim($("#ipt_qq").val());
                var descript = $.trim($("#texta_descript").val());

                var re = /^[1-9][0-9]{4,}$/;
                if (!re.test(qq)) {
                    alert("请输入正确的QQ号");
                    return false;
                }
                if (descript == '') {
                    alert('描述不能为空！');
                    return false;
                }
                $.ajax({
                    url: "ajax/index_user_demand16.aspx?qq=" + qq + "&descript=" + escape(descript),
                    dataType: "text",
                    success: function(data) {
                        if (data == "0") {
                            alert('亲,你的需求我们已经在处理,我们会尽快处理并和你取得联系！')
                            $("#ipt_qq").val('');
                            $("#texta_descript").val('');
                            return;
                        }
                        else {
                            alert('亲,你的输入有误，请重新核实在提交！')
                            return;
                        }
                    }
                });
            });
        });
    </script>

    <script type="text/javascript">
        var page1 = 2, page2 = 2, page3 = 2, page4 = 2;
        function ShowAllpl() {
            var id = $("#hiddataid").val();
            var url = "iframeAllEval.aspx?id=" + id;
            $.ajax({
                url: url,
                dataType: "text",
                success: function(data) {
                    if (data != "") {
                        var str = data.substring(0, data.indexOf("$#%#$"));
                        var htm = data.substring(data.indexOf("$#%#$") + 5);
                        var ids = str.split(',');
                        $("#pl1").html(ids[0]);
                        $("#pl2").html(ids[1]);
                        $("#pl3").html(ids[2]);
                        $("#pinglunDiv").append(htm);
                    }
                }
            });
        }
        function Showpl(obj, type) {
            $(obj).parent().parent().find("#loading").show();
            var id = $("#hiddataid").val();
            var url = "iframeEval.aspx?id=" + id + "&type=" + type + "&page=";
            if (type == 1) url += page1;
            if (type == 2) url += page2;
            if (type == 3) url += page3;
            if (type == 4) url += page4;
            $.ajax({
                url: url,
                dataType: "text",
                success: function(data) {
                    if (type == 1) page1++;
                    if (type == 2) page2++;
                    if (type == 3) page3++;
                    if (type == 4) page4++;
                    $(obj).parent().parent().find("#loading").hide();
                    if ($.trim(data) == "") {
                        if (obj) {
                            $(obj).html("没有更多了");
                            obj.onclick = null;
                        }

                    } else {
                        if (type == 1) $("#pinglunDiv1").append(data);
                        if (type == 2) $("#pinglunDiv2").append(data);
                        if (type == 3) $("#pinglunDiv3").append(data);
                        if (type == 4) $("#pinglunDiv4").append(data);
                    }
                }
            });
        }

