<script type="text/javascript">
    function query() {
        var obj = document.getElementById("textfield");
        if (obj.value == "请输入产品名称" || obj.value == "") {
            obj.focus();
            return;
        }
        location.href = '/products.aspx?category_id=0&key=' + obj.value;
    }
    
</script>


