        <script type="text/javascript">
           // window.onload = function()
           // {
           //    alert(1);
              

           // }

           $(function(){

             /*头部gif*/
              var oLogo = document.getElementById("logo-F1");
               var aImg = oLogo.getElementsByTagName("img")[0];
               var imgUrl = ["/dyweb/dythemes/diyou_trust/images-new/public/gpdm1.png", "/dyweb/dythemes/diyou_trust/images-new/public/gpdm2.png",
                             "/dyweb/dythemes/diyou_trust/images-new/public/gpdm3.png", "/dyweb/dythemes/diyou_trust/images-new/public/gpdm4.png",];
               var aLink = ["http://www.facaishu.com/xsbzt/index.html","http://www.facaishu.com/mtbd/a1714.html","http://www.facaishu.com/mtbd/a2456.html",
                            "http://www.facaishu.com/mtbd/a2511.html"];
               var timer = null;
               var num = 1;

             //封装定时器
             function tabImg()
             {
                  clearInterval(timer); //一开始清除定时器
                  timer = setInterval(function(){
                     aImg.src = imgUrl[num];
                     num++;
                     num%=imgUrl.length;

                },2000)
  
             };

             tabImg(); //调用


               //鼠标靠近，清除定时器
               oLogo.onmouseover = function()
               {
                 
                   clearInterval(timer);
               };

              //鼠标离开，开启定时器
                oLogo.onmouseout = function()
               {
                   tabImg();
               };

               oLogo.onclick = function()
               {
                  num == 0 ? oLogo.href = aLink[num+3] : oLogo.href = aLink[num-1];   
               };
               
               /*威信*/

              $(".header-L .wxh").hover(function(){
                     
                     $(this).find("img").attr("src","/dyweb/dythemes/diyou_trust/images-new/new-index/wx-tb-h.png");
                     $(".header-L img.wx-gzh").css("display","block");
                 
              },function(){
                    $(this).find("img").attr("src","/dyweb/dythemes/diyou_trust/images-new/new-index/wx-tb.png");
                    $(".header-L img.wx-gzh").css("display","none");
              })

              $(".header-L .wbh").hover(function(){
                     
                     $(this).find("img").attr("src","/dyweb/dythemes/diyou_trust/images-new/new-index/wb-tb-h.png");
                     $(".header-L img.gfwb-c").css("display","block");
                 
              },function(){
                    $(this).find("img").attr("src","/dyweb/dythemes/diyou_trust/images-new/new-index/wb-tb.png");
                    $(".header-L img.gfwb-c").css("display","none");
              })

              $(".header-R .sj-app").hover(function(){

                  $(this).css({"border-color":"#fff","color":"#fff","background-image":"url(/dyweb/dythemes/diyou_trust/images-new/new-index/sj-tb-h.png)"}); 

              },function(){

                 $(this).css({"border-color":"#8d96a0","color":"#96a0ab","background-image":"url(/dyweb/dythemes/diyou_trust/images-new/new-index/sj-tb.png)"}); 

              })
           })
        </script>
        

