        //鼠标悬浮二维码变大
        var oJdxc = document.getElementById("jdx-C");
        var aJdxcLi = oJdxc.getElementsByTagName("li");
  
        var aJdxcLi = $("#jdx-C").find("li"); 
        for(var i =0; i<aJdxcLi.length; i++)
        {
           //aJdxcLi[i].index = i;
           aJdxcLi[i].onmouseover = function()
           {
              $(this).find("img").css("width","150px");
              $(this).find("img").css("height","150px");
           }

           aJdxcLi[i].onmouseout = function()
           {
              $(this).find("img").css("width","94px");
              $(this).find("img").css("height","94px");
           }
