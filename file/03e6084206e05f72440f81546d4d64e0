/*平台动态、媒体报道、理财知识、政策法规切换*/
window.onload = function()
{
  var oPlmz = document.getElementById("plmz");
    var oPtdtc = document.getElementById("ptdtc");
  var oMtbdc = document.getElementById("mtbdc");
  var oLczsc = document.getElementById("lczsc");
  var oZcfgc = document.getElementById("zcfgc");
  var aplmzLi = oPlmz.getElementsByTagName("li");
  var oMoremsg = document.getElementById("moreMsg");
  var moreArr = ["http://www.facaishu.com/ptdt/index.html","http://www.facaishu.com/mtbd/index.html","http://www.facaishu.com/lczs/index.html","http://www.facaishu.com/zcfg/index.html"];

  for(var i=0; i<aplmzLi.length; i++)
  {
    aplmzLi[i].index = i;

    aplmzLi[i].onclick = function()
    {
      //alert(this.index);
       for(var i=0; i<aplmzLi.length; i++)
       {
          aplmzLi[i].className = "";
       }
       this.className = "active";

       oMoremsg.href = moreArr[this.index];

       if(this.index == 0)
       {
          //alert("进来了");
          oPtdtc.style.display = "block";
              oMtbdc.style.display = "none";
              oLczsc.style.display = "none";
              oZcfgc.style.display = "none";
       }
          
            if(this.index == 1)
       {
         
          oPtdtc.style.display = "none";
              oMtbdc.style.display = "block";
              oLczsc.style.display = "none";
              oZcfgc.style.display = "none";
       }

         if(this.index == 2)
       {
        
          oPtdtc.style.display = "none";
              oMtbdc.style.display = "none";
              oLczsc.style.display = "block";
              oZcfgc.style.display = "none";
       }
   
           if(this.index == 3)
       {
        
          oPtdtc.style.display = "none";
              oMtbdc.style.display = "none";
              oLczsc.style.display = "none";
              oZcfgc.style.display = "block";
       }



     
    }
  }
}
    </script>
     

