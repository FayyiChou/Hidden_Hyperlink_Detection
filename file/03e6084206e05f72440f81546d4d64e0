            })
        })


        //鼠标滚动出现导航栏
          $(function(){
            
              var navOffset=$(".fullfox").offset().top + 300;

                  $(window).scroll(function(){
                      var scrollPos=$(window).scrollTop();
                      if(scrollPos >=navOffset){
                          $(".fullfoxs").slideDown("slow");
                      }else{
                           $(".fullfoxs").slideUp("slow");
                      }
                  });

          })
        </script>
        

