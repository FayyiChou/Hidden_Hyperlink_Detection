        $(function(){

            var flag = 1;

            $(".bottomHref").find("a").each(function(index,element){


                if(index == 14)
                {
                   $(this).after("<br/>");
                }

                if(index > 14)
                {
                    $(this).hide();
                }



            })

            $(".zk-lj").click(function(){


                if(flag == 1)
                {
                     $(".bottomHref").find("a").each(function(index,element){

                       if(index > 14)
                        {
                            $(this).show();
                        }

                   })

                   $(".zk-lj span").html("");
                   $(".zk-lj img").attr("src","/dyweb/dythemes/diyou_trust/images-new/public/zk-up.png");

                    flag = 2;

                    return false;

                }

                if(flag == 2)
                {
                   $(".zk-lj span").html("");
                   $(".zk-lj img").attr("src","/dyweb/dythemes/diyou_trust/images-new/public/zk-down.png");

                     $(".bottomHref").find("a").each(function(index,element){

                       if(index > 14)
                        {
                            $(this).hide();
                        }

                   })

                   flag =1;
                   return false;
                }

            })


