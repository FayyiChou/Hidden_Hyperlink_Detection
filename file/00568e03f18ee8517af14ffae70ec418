    var getTimeText = function(second){
        if(typeof second == 'undefined' || !second){ return; }
        var day = parseInt(second / 24 / 3600);
        var hours = parseInt((second % (3600 * 24)) / 3600);
        var minutes = parseInt((second % 3600) / 60);
        var text = '获赠后';
        text += (day > 0) ? day+"天" : '';
        text += (hours > 0) ? hours+"小时" : '';
        text += (minutes > 0) ? minutes+"分" : '';
        text += '内有效';
        return text;
    };

    var getActivityInfo ={
        getActivityInfoFun :function(gid,zhouyi_ids){
            $.ajax({
                type: 'GET',
                url: '//shop.juanpi.com/deal/getActivitysInfo',
                data: {gid: gid,zhouyi_ids:zhouyi_ids},
                dataType: 'json',
                cache: true,
                success: function(res) {
                    if(res.code == 1000){
                        var obj = res.data;

                        //显示活动免邮
                        if(obj.actinfo && (obj.actinfo.abtype === 14)){
                            var item = $('.goods_show .by');
                            act_free_fare = 1;
                            item.html('活动免邮').removeClass('shipping hide').css("display","inline-block");
                            $('.deal-wrap .delivery-tip').remove();
                        }
                        //判断是否有限时购
                        if(obj.tbinfo){
                            var isover=$(".J-basic .limit-time").attr("data-over");
                            if(isover!="over") {
                                //取系统时间
                                $.ajax({
                                    type: "get",
                                    dataType: 'jsonp',
                                    jsonpCallback: 'callback_time1',
                                    url: '//upload.juancdn.com/time?ts=' + new Date(),
