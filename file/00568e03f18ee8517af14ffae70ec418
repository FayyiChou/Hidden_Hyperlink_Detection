                                        var servertime=time/1000;
                                        for(var i=0;i<obj.tbinfo.length;i++){
                                            if(servertime>=obj.tbinfo[i].stime && servertime<obj.tbinfo[i].etime ){
                                                getActivityInfo.showTBInfo(servertime,gid);
                                                break;
                                            }
                                        }

                                    }
                                });
                            }
                        }

                        //判断是否为展开状态
                        var expand=false;
                        var count=0;//计算有几条营销信息
                        if(obj.actinfo.m){
                            count=1;
                        }
                        if(obj.mzinfo.count>0){
                            count+=obj.mzinfo.count
                        }
                        if(count==0) return;//没有任何数据
                        if(count==1){
                            expand=true;
                        }


                        var _html=[];

                        if(expand){
                            _html.push('<dl class="activity-info expand clear">');
                            _html.push('    <dt>促销：</dt>');
                            _html.push('    <dd>');
                            _html.push('        <div class="activity-c fl">');
                        }else{
                            _html.push('<dl class="activity-info clear">');
                            _html.push('    <dt>促销：</dt>');
                            _html.push('    <dd>');
                            _html.push('        <div class="activity-c fl">');
                            //不展开状态需要显示收起文案
                            
                            for(var i=0;i<obj.mzinfo.data.length;i++){
                                _html.push('        <div class="show-item clear">');
                                _html.push('            <div class="brand-tag-red fl">满赠</div><div class="info fl">'+(obj.mzinfo.data[i][0].ms || obj.mzinfo.data[i][0].m) +'</div>');
                                _html.push('        </div>');
                            }

                           
                        }

                        if(obj.mzinfo.count>0){
                            _html.push('        <div class="mainzeng-c" >');
                            for(var key in obj.mzinfo.data){
                                for(var j=0;j<obj.mzinfo.data[key].length;j++){
                                    var curitem=obj.mzinfo.data[key][j];

                                    _html.push('            <div class="manzeng activity-item clear">');
                                    if(curitem.is_coudan==1){
                                        _html.push('                <div class="brand-tag-red fl">满赠</div><div class="info fl">'+(curitem.ms || curitem.m) +'<a class="detail" href="//shop.juanpi.com/factivity/'+curitem.abId+'" target="_blank">详情》</a></div>'); //(curitem.ms || curitem.m) 防止赠品为券时没有ms展开文案
                                    }else{
                                        _html.push('                <div class="brand-tag-red fl">满赠</div><div class="info fl">'+(curitem.ms || curitem.m) +'</div>'); //(curitem.ms || curitem.m) 防止赠品为券时没有ms展开文案
                                    }
                                        

                                    //赠品info有数据时才显示gift-box
                                    if(curitem.info.length>0){
                                        if(curitem.type==1){

                                            _html.push('                <div class="gift-box">');
                                            _html.push('                    <div class="head">赠品</div>');
                                            _html.push('                    <div class="gift-c">');
                                            for(var i=0;i<curitem.info.length;i++){
                                                _html.push('                    <div class="goods-item clear">');
                                                _html.push('                        <div class="item-l fl"><img src="'+curitem.info[i].pse_pic+'" width="35" height="35"/></div>');
                                                _html.push('                        <div class="item-r fl">');
                                                _html.push('                            <div class="item-info clear"><div class="name fl">'+curitem.info[i].ps_title+'</div><div class="num fr">X'+curitem.info[i].ps_num+'</div></div>');
                                                _html.push('                            <div class="price">价值'+curitem.info[i].pse_price+'元</div>');
                                                _html.push('                        </div>');
                                                _html.push('                    </div>');
                                            }
                                            _html.push('                    </div>');
                                            _html.push('                    <div class="arrow"></div>');
                                            _html.push('                 </div>');
                                        }
                                        //优惠券
                                        if(curitem.type==2){
                                            _html.push('                <div class="gift-box">');
                                            _html.push('                    <div class="head">赠优惠券</div>');
                                            _html.push('                    <div class="gift-c">');
                                            _html.push('                        <div class="coupon-item">');
                                            _html.push('                            <span class="price">￥'+curitem.info[0].aeMoney+'</span>满'+curitem.info[0].aeMinMoney+'元可用');
                                            if(curitem.info[0].abBrandCoupon == 2){
                                                _html.push('                            <div class="time">使用期限：'+getTimeText(curitem.info[0].abEndtimeExtend)+'</div>');
                                            }else{
                                                _html.push('                            <div class="time">使用期限：'+curitem.info[0].startTime+' 至 '+curitem.info[0].endTime+'</div>');
                                            }
                                            _html.push('                        </div>');
                                            _html.push('                    </div>');
                                            _html.push('                    <div class="arrow"></div>');
                                            _html.push('                </div>');
                                        }
                                        //券包
                                        if(curitem.type==3){
                                            _html.push('                <div class="gift-box">');
                                            _html.push('                    <div class="head">赠优惠券</div>');
                                            _html.push('                    <div class="gift-c">');
                                            for(var i=0;i<curitem.info.length;i++) {
                                                _html.push('                        <div class="coupon-item">');
                                                _html.push('                            <span class="price">￥' + curitem.info[i].aeMoney + '</span>满' + curitem.info[i].aeMinMoney + '元可用');
                                                if(curitem.info[i].abBrandCoupon == 2){
                                                    _html.push('                            <div class="time">使用期限：'+getTimeText(curitem.info[i].abEndtimeExtend)+'</div>');
                                                }else{
                                                    _html.push('                            <div class="time">使用期限：'+curitem.info[i].startTime+' 至 '+curitem.info[i].endTime+'</div>');
                                                }
                                                _html.push('                        </div>');
                                            }
                                            _html.push('                    </div>');
                                            _html.push('                    <div class="arrow"></div>');
                                            _html.push('                </div>');
                                        }
                                    }
                                    _html.push('            </div>');
                                }
                            }
                            _html.push('            </div>');
                        }

                        if(obj.actinfo.m){
                             var _details = "详情》";
                            if(state_class == "start") {
                                var _details = "";
                            }
                            _html.push('        <div class="manjian activity-item clear">');
                            switch (obj.actinfo.abtype){
                                case 4:
                                    _html.push('            <div class="brand-tag-red fl">满减</div><div class="info fl">'+obj.actinfo.m+'<a class="detail" href="//shop.juanpi.com/factivity/'+obj.actinfo.abId+'" target="_blank">'+_details+'</a></div>');
                                    break;
                                case 6:
                                	if(obj.actinfo.aeTypeInfo){
										_html.push('            <div class="brand-tag-red fl">'+obj.actinfo.aeTypeInfo+'</div><div class="info fl">'+obj.actinfo.m+'<a class="detail" href="//shop.juanpi.com/factivity/'+obj.actinfo.abId+'" target="_blank">'+_details+'</a></div>');
									}else{
										_html.push('            <div class="brand-tag-red fl">满件折</div><div class="info fl">'+obj.actinfo.m+'<a class="detail" href="//shop.juanpi.com/factivity/'+obj.actinfo.abId+'" target="_blank">'+_details+'</a></div>');
									}
                                    break;
                                case 7:
                                    _html.push('            <div class="brand-tag-red fl">满件减</div><div class="info fl">'+obj.actinfo.m+'<a class="detail" href="//shop.juanpi.com/factivity/'+obj.actinfo.abId+'" target="_blank">'+_details+'</a></div>');
                                    break;
								case 10:
                                    _html.push('            <div class="brand-tag-red fl">任选</div><div class="info fl">'+obj.actinfo.m+'<a class="detail" href="//shop.juanpi.com/factivity/'+obj.actinfo.abId+'" target="_blank">'+_details+'</a></div>');
                                    break;
                                case 11:
                                    _html.push('            <div class="brand-tag-red fl">满件免</div><div class="info fl">'+obj.actinfo.m+'<a class="detail" href="//shop.juanpi.com/factivity/'+obj.actinfo.abId+'" target="_blank">'+_details+'</a></div>');
                                    break;
                            }
                            _html.push('        </div>');
                        }
                        _html.push('        </div>');
                        //是否显示更多促销
                        if(!expand){
                            _html.push('        <div class="collapsed fr">');
                            _html.push('            更多促销<i></i>');
                            _html.push('        </div>');
                        }
                        _html.push('    </dd>');
                        _html.push('</dl>');

                        $(".J-basic .limit-time").after(_html.join(''));



                        //绑定 收缩 展开事件
                        $(".activity-info .collapsed").bind("click",function(){
                            if($(this).parents('.activity-info').hasClass("expand")){
                                $(this).html("更多促销<i></i>");
                                $(this).parents('.activity-info').removeClass("expand");
                            }else{
                                $(this).html("收起<i></i>");
                                $(this).parents('.activity-info').addClass("expand");
                            }
                        })

                        //绑定显示赠品详细事件
                        $(".activity-info .mainzeng-c .activity-item").hover(function(){
                            var pos=$(this).offset();
                            var giftboxj=$(this).find(".gift-box");
                            giftboxj.show();
                            var gifth=giftboxj.height();
                            giftboxj.css("top",-gifth/2+12);
                        },function(){
                            $(this).find(".gift-box").hide();
                        })
                    }
                }
            });
        },
        getBrandCouponInfoFun :function(shop_id,zhouyi_ids){
            var that = this;
            $.ajax({
                type: 'GET',
                url: '//shop.juanpi.com/deal/getBrandCouponInfo',
                data: {shop_id: shop_id,zhouyi_ids:zhouyi_ids},
                dataType: 'json',
                cache: true,
                success: function(res) {
                    var code = res.code;
                    if( code==1000) {
                        var data = res.data;
                        var Len = data.length;
                        var _html = '';
                        for(var i = 0;i < Len ; i++ ){
                            var status = data[i]['status'];
                            if(status == 1){ //立即领取
                                _html += '<em class="gc-smallblock" data-id="' + data[i]['coupon_id'] + '" data-min_money="'+data[i]['min_money']+'" data-type="'+data[i]['coupon_share']+'">';
                            }else if(status == 2){ //已领取
                                _html += '<em class="gc-smallblock disabled-coupon">';
                            }else if(status == 3){ //已抢光
                                _html += '<em class="gc-smallblock disabled-coupon">';
                            }else{ //状态不对,退出当前节点拼接
                                continue;
                            }
                            _html += data[i]['min_money']+'-'+data[i]['money'];
                            _html += '</em>';
                        }
                        if(_html){
                            _html = '<span>领券:</span>'+_html;
                            $('.get-coupons').html(_html).show();
                            that.addBrandCouponFunc();
                        }
                    }
                }
            });
        },
        addBrandCouponFunc:function(){
            $('.get-coupons .gc-smallblock').on('click',function(){
                if($(this).attr("data-type") == 1){
                    var content = '<div class="show_code"><img src="//jp.juancdn.com/juanpi/images/brand/app_code.jpg"/></div>';
                    var couponw = new XDLightBox({
                        title: "立即下载APP领取优惠券",
                        lightBoxId: "alert_coupon_app",
                        contentHtml: content,
                        scroll: false
                    });
                    couponw.init();
                    return false;
                }

                if(XDPROFILE.uid =='' || XDPROFILE.uid ==undefined){
                    return XD.user_handsome_login_init(),
                            XD.user_handsome_login(),
                            false;
                }
                var btn = $(this);
                if(btn.hasClass('disabled-coupon')){
                    return false;
                }
                if(btn.data('lock') == 1) {
                    return false;
                }
                var coupon_id =btn.attr('data-id');
                if(!coupon_id) return false;
                btn.data('lock', 1);
                var zhouyi_ids = getCookie('zhouyi_ids');
                zhouyi_ids = zhouyi_ids_filter(['c', 'b'],zhouyi_ids);
                zhouyi_ids = zhouyi_ids ? zhouyi_ids : 'C4';
                
                $.ajax({
                    type:'post',
                    url:'//shop.juanpi.com/deal/addBrandCoupon',
                    data:{coupon_id:coupon_id,zhouyi_ids:zhouyi_ids},
                    dataType:'json',
                    success:function(data){
                        var _html = '<div class="coupons-pop-opacity-wrap"></div>';
                        _html += '<div class="coupons-pop">';
                        _html += '<a href="javascript:void(0)" class="cp-close"></a>';
                        var code = data.code;
                        if(code==1000){ //领券成功
                            btn.addClass('disabled-coupon');
                            _html += '<div class="cp-success">';
                            _html += '<p class="cp-status"><span class="cpc-icon"></span>领取成功</p>';
                            _html += '<p class="cpc-amount">'+data.data+'元(满'+btn.attr('data-min_money')+'可用)</p>';
                            _html += '<a href="javascript:void(0)" class="cp-btn">知道了</a>';
                            _html += '</div>';
                        }else{ //领券失败
                            var _msg = '';
                            if(code == 50042){
                                _msg = '该券已领完';
                            }else{
                                _msg = '抢券小伙伴太多了，请稍后再试。';
                            }
                            _html += '<div class="cp-done">';
                            _html += '<p class="cp-status">'+_msg+'</p>';
                            _html += '<a href="javascript:void(0)" class="cp-btn">关闭</a>';
                            _html += '</div>';
                        }
                        _html += '</div>';
                        $('.coupons-coverbg').html(_html).show();
                        $('.coupons-coverbg').find('.cp-close,.cp-btn').on('click',function(){
                            $('.coupons-coverbg').hide();
                        });
                        btn.data('lock', 0);
                    },
                    error:function(e){
                        btn.data('lock', 0);
                    }
                });
            });
        },
        initFun :function(){
            var zhouyi_ids = XDTOOL.getCookie('zhouyi_ids');
            zhouyi_ids = zhouyi_ids_filter(['c', 'b'],zhouyi_ids);
            if(!zhouyi_ids){
                $.ajax({
                    url:'//www.juanpi.com'+'/usergroup/zhouyi',
                    type: 'GET',
                    dataType: 'jsonp',
                    success: function(data) {
                        if(data){
                            zhouyi_ids = zhouyi_ids_filter(['c', 'b'],data);
                        }
                        zhouyi_ids = zhouyi_ids ? zhouyi_ids : 'C4';
                        
                        getActivityInfo.getActivityInfoFun(gid,zhouyi_ids);
                        brandCouponFlag && getActivityInfo.getBrandCouponInfoFun(shop_id,zhouyi_ids);
                    }
                });
            }else{
                getActivityInfo.getActivityInfoFun(gid,zhouyi_ids);
                brandCouponFlag && getActivityInfo.getBrandCouponInfoFun(shop_id,zhouyi_ids);
            }

        },
        showTBInfo:function(servertime,gid){
            //取限时购信息
            $.ajax({
                type: "get",
                dataType: 'jsonp',
                jsonpCallback: 'callback_tb',
                data: {gid: gid},
                url: '//shop.juanpi.com/deal/getAsStock',
                success: function (obj) {
                    if(obj.data && (obj.data[0].asPrice || obj.data[0].asDiscount)){
                        var tbhtml="";
                        var money = '';
                        if(typeof(obj.data[0].asPrice) != 'undefined' && obj.data[0].asPrice){
                            money = obj.data[0].asPrice;
                        }

                        tbhtml+='<dd>';
                        tbhtml+='        <span class="limit-tag">'+obj.data[0].msg+'</span>';
                        tbhtml+='        <span class="limit-price"><em>&yen;</em>'+money+'</span>';
                        tbhtml+='           <span class="limit-clock" data-endtime="'+obj.data[0].etime+'"></span>';
                        if(obj.data[0].msg.indexOf("APP")!=-1){
                            tbhtml+='        <span class="limit-down fr">';
                            tbhtml+='           去手机购买<i class="phone"></i><i class="arrow"></i>';
                            tbhtml+='           <div class="dimension-code">';
                            tbhtml+='               <img src="//jp.juancdn.com/juanpi/images/global/jp-app-90-new.png"/>';
                            tbhtml+='           </div>';
                            tbhtml+='        </span>';
                        }
                        tbhtml+='</dd>';

                        $(".J-basic .limit-time").append(tbhtml);
                        if(typeof(obj.data[0].asDiscount) != 'undefined' && obj.data[0].asDiscount){
                            limitDiscount(obj.data[0].asDiscount/10);
                        }
                        //隐藏商品的倒计时
                        $(".djs").hide();
                        var countdown=function(){
                            var endtime=$(".J-basic .limit-time .limit-clock").attr("data-endtime");
                            timestamp = endtime - servertime;
                            //计算出相差天数
                            days = Math.floor(timestamp / (24 * 3600));
                            //计算出小时数
                            leave1 = timestamp % (24 * 3600);    //计算天数后剩余的毫秒数
                            hours = Math.floor(leave1 / (3600));
                            //计算相差分钟数
                            leave2 = leave1 % (3600);       //计算小时数后剩余的毫秒数
                            minutes = Math.floor(leave2 / (60));
                            //计算相差秒数
                            leave3 = leave2 % (60);      //计算分钟数后剩余的毫秒数
                            seconds = Math.round(leave3);
                            days = days < 0 ? 0 : days;
                            hours = hours < 0 ? 0 : hours;
                            minutes = minutes < 0 ? 0 : minutes;
                            seconds = seconds < 0 ? 0 : seconds;
                            if(days<=0&&hours<=0&&minutes<=0&&seconds<=0){
                                clearInterval(interval);
                                $(".limit-time .limit-clock").text("已结束");
                                return;
                            }
                            $(".J-basic .limit-time .limit-clock").text("还剩"+days+"天"+hours+"小时"+minutes+"分"+seconds+"秒");
                            servertime++;
                        }
                        countdown();
                        //倒计时
                        var interval=setInterval(function () {
                            countdown();
                        }, 1000);
                        $(".J-basic .limit-time").show();
                    }
                }
            });
        }
    }
    getActivityInfo.initFun();
    
    var gcat_gid = 5641688,
        gcat_id = 849,
        gcat_topid = 702;

        $.ajax({
            type: "get",
            dataType:'jsonp',
            jsonpCallback:'callback_time',
            url: '//upload.juancdn.com/time?ts='+new Date(),
            success: function (time) {
                var servertime = time / 1000;
                setInterval(function () {
                    timestamp = 1491008400 - servertime;
                    //计算出相差天数
                    days = Math.floor(timestamp / (24 * 3600));
                    //计算出小时数
                    leave1 = timestamp % (24 * 3600);    //计算天数后剩余的毫秒数
                    hours = Math.floor(leave1 / (3600));
                    //计算相差分钟数
                    leave2 = leave1 % (3600);       //计算小时数后剩余的毫秒数
                    minutes = Math.floor(leave2 / (60));
                    //计算相差秒数
                    leave3 = leave2 % (60);      //计算分钟数后剩余的毫秒数
                    seconds = Math.round(leave3);
                    days = days < 0 ? 0 : days;
                    hours = hours < 0 ? 0 : hours;
                    minutes = minutes < 0 ? 0 : minutes;
                    seconds = seconds < 0 ? 0 : seconds;
                    $("#d").html(days);
                    $("#h").html(hours);
                    $("#m").html(minutes);
                    $("#s").html(seconds);
                    servertime++;
            }, 1000);
        }});



    //控制滚动
    $(function() {
        $('.slide-box-tab li').bind('click', function(){
            var tagname = $(this).find('a').attr('_tagname');
            $('.info-title').each(function(){
                if( $(this).attr('_tagname') == tagname ){
                    var h =$(this).offset().top-120;
                    window.scrollTo(100,h);
                }
            });
        });
        $('.slide-box-main li').bind('click', function(){
            if ($(this).index()==1){
               var h =$('.buy-guidelines').offset().top-120; 
            };
            if($(this).index()==2){
               var h =$('.consumer-protection').offset().top-120;
            }
           if($(this).index()==0){
               return;
           }
           window.scrollTo(100,h);
        });
        //邮费说明气泡
        $(".fare-desc .del").on('click', function(event) {
            XDTOOL.setCookie('fare_inf'+gid,'1',{expires: null,path: "/"});
            $(this).parent().hide();
        });
    });
    //副图居中
    $('.deal-pic ul').css('width', $('.deal-pic ul li').length*65-5);
    function check_endtime(){
        var servertime = 1491061231;
        var end_time = 1491061231;
        if( servertime > end_time){
            var info = '<div class="con-go"><p class="txt">很抱歉，满减活动已结束！</p></div>';
            var box = new XDLightBox({
                title:'提示',
                lightBoxId:'alert_sign',
                contentHtml:info,
                scroll:false
            });
            box.init();
        }
    }

    $("#show-qcode").hover(function(){
        $("#new-qcode").css('display','block');
    },function(){
        $("#new-qcode").css('display','none');
    });
