        var asDiscount = 1;//限时折扣
        var skuObj = null;
        var default_color_select = null;
        var default_size_select = null;
        var act_free_fare = 0;//营销活动，免邮

        //用户分群过滤
        var zhouyi_ids_filter = function(filter, zhouyi_ids) {
            var result = '';
            if(!zhouyi_ids){
              return result;
            }
            if (filter instanceof Array && filter.length > 0) {
              var flen = filter.length;
              var pattern = '';
              for (var i = 0; i < flen; i++) {
                switch (filter[i]) {
                  case 'zhouyi':
                    pattern += '|_\\d+';
                    break;
                  default:
                    pattern += '|_' + filter[i] + '\\d+';
                    break;
                }
              }
              if (pattern) {
                zhouyi_ids = '_' + zhouyi_ids;
                pattern = pattern.substring(1);
                var re = new RegExp(pattern, 'gi');
                var arr = zhouyi_ids.match(re);
                if (arr) {
                  result = arr.join('');
                  result = result.substring(1);
                }
              }
            }
            return result;
        }

        //四舍五入取一位小数
        function round_num(number,fractionDigits){
            number = number.toFixed(2);  
            return Math.round(number*Math.pow(10,fractionDigits))/Math.pow(10,fractionDigits);   
        }

        //返回只有一位小数的数字
        var retainDecimal = function(price){
            if(parseInt(price) == price){
                return price;
            }
            var money = ( (price.toString().split(".")[1].length) > 1 ) ? (round_num(price,1)) : price;
            if(parseInt(money) == money){
                return parseInt(money);
            }
            return money;
        }

        //限时折扣-价格显示
        var limitDiscount = function(discount){
            if(skuObj){
                if(skuObj.info.class=="over"){
                    return false;
                }
                asDiscount = discount;//把折扣信息保存到全局
                if(skuObj.info.sku_color == false && skuObj.info.sku_size == false){
                    //只有一个sku
                    var money = retainDecimal(skuObj.info.si_cprice*asDiscount);
                    $(".limit-time .limit-price").html("<em>&yen;</em>"+money);
                }else{
                    if(default_color_select || default_size_select){
                        //触发默认sku点击
                        default_color_select ? default_color_select.trigger('click') : '';
                        default_size_select ? default_size_select.trigger('click') : '';
                    }else{
                        //没有默认sku点击，直接修改显示价格
                        var price_str = retainDecimal(skuObj.info.si_cprice*asDiscount);
                        $(".limit-time .limit-price").html("<em>&yen;</em>"+price_str);
                    }
                }
            }
        }

