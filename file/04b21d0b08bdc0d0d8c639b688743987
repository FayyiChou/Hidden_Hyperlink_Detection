        //y=loc.resources[0].point.coordinates[0];

        for (var i = 0; i < list.length; i++) {
          if (region == list[i]) {
            hasRegion=true;
          }
        }
      }
      if(!hasRegion) {region="北京";x=116.39722;y=39.90960;alert("无法定位您的位置，将使用默认地址。");}
