  var callback = function(ret){
    
    var hasRegion=false;
    if(ret.statusDescription.toUpperCase() !="OK" || ret.resourceSets.length < 1){

    }else{
      var loc=ret.resourceSets[0];
      var locdesc=loc.resources[0].address;
      var countrycode=locdesc.countryRegionIso2;
      var province=null;
      if(countrycode.toUpperCase() == "CN"){  //中国大陆
        province=provinces[locdesc.adminDistrict];
        if(!province) {province="北京";x=116.39722;y=39.90960;alert("无法定位您的位置，将使用默认地址。");}
        region=province;
        //x=loc.resources[0].point.coordinates[1];
        //y=loc.resources[0].point.coordinates[0];
        hasRegion=true;
      }else{  //其他
        region = countryCodes[countrycode.toUpperCase()];
