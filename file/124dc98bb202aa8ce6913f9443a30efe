<script>
function navSubmit(obj, defaultHref) {
    if (document.searchForm.query.value.length > 0) {
        var href = obj.href;
        var query = document.searchForm.query.value;
        href = href + encodeURIComponent(query);
        obj.href = href;
    }else{
    	obj.href = defaultHref;
    }
}

if("undefined" != typeof(head) && $("#"+head).length > 0)
	$("#"+head).addClass("cur");
</script>
