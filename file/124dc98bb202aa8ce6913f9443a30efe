<script type="text/javascript">
var albumTimer = null;
var albumCurrentIndex = 0;
function changeAlbumTag(_index){
	if(_index == albumCurrentIndex)
		return;
	
	$("#album_content_div ul:eq(" + _index + ")").show();
	$("#album_content_div ul:eq(" + albumCurrentIndex + ")").hide();
	
	albumCurrentIndex = _index;
	$("#album_tag_div span").removeClass("cur");
	$("#album_tag_div span:eq(" + albumCurrentIndex + ")").addClass("cur");
}
</script>
