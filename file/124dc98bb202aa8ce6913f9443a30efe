<script>
var dissCount = 4;
var myDissScroll;
var currentDissIndex = 0;
function scrollDissBox(_index, scrollDuration){
	var index = (_index + dissCount) % dissCount;

	if(index == currentDissIndex)
		return;
	
	$("#scrollDissBox div:eq(" + index + ")").fadeIn(scrollDuration);
	$("#scrollDissBox div:eq(" + currentDissIndex + ")").fadeOut(scrollDuration);
	
	currentDissIndex = index;
}
$(document).ready(function(){
	myDissScroll = setInterval(function(){ scrollDissBox(currentDissIndex + 1, 1500); }, 5000);
    $(".playlist_cover").hover(function(){
        clearInterval(myDissScroll);
        },function(){
            myDissScroll = setInterval(function(){ scrollDissBox(currentDissIndex + 1, 1500); }, 5000);
        }
	);
});
</script>
<!--zly--> 
    </div>
    


<script type="text/javascript">
var singerTimer = null;
var singerCurrentIndex = 0;
function changeSingerTag(_index){
	if(_index == singerCurrentIndex)
		return;
	
	$("#singer_content_div ul:eq(" + _index + ")").show();
	$("#singer_content_div ul:eq(" + singerCurrentIndex + ")").hide();
	
	singerCurrentIndex = _index;
	$("#singer_tag_div span").removeClass("cur");
	$("#singer_tag_div span:eq(" + singerCurrentIndex + ")").addClass("cur");
}
</script>
