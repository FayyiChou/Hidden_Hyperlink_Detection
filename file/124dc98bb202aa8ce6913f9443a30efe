<script type="text/javascript">
var dissTimer = null;
var dissCurrentIndex = 0;
function changeDissTag(_index){
	if(_index == dissCurrentIndex)
		return;
	
	$("#diss_content_div ul:eq(" + _index + ")").show();
	$("#diss_content_div ul:eq(" + dissCurrentIndex + ")").hide();
	
	dissCurrentIndex = _index;
	$("#diss_tag_div span").removeClass("cur");
	$("#diss_tag_div span:eq(" + dissCurrentIndex + ")").addClass("cur");
}
</script>
