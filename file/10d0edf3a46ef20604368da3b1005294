jQuery(".tit-r-tab a").hover(function(){
var index = jQuery(".tit-r-tab a").index(this);
jQuery(this).addClass("cut").siblings("a").removeClass("cut");
jQuery(".tit-r-tab .jt").eq(index).show().siblings(".jt").hide();
jQuery(".t-tab > div").eq(index).show().siblings("div").hide();
})
jQuery(".s-list a").click(function(){
var index = jQuery(".s-list a").index(this);
jQuery(this).parent().addClass("c").siblings().removeClass("c");
jQuery(this).siblings("span").show().parent().siblings("li").find("span").hide();
jQuery(".s-jt > i").eq(index).css("opacity","1").siblings().css("opacity","0");
jQuery(".s-list-con > div").eq(index).show().siblings().hide();
})
