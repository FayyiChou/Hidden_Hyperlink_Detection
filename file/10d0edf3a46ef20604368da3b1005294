function scr1(){
scrollPic_01 = new ScrollPic();
scrollPic_01.scrollContId   = "slider_list"; //图片容器ID
scrollPic_01.arrLeftId      = "left_but"; //左按钮ID
scrollPic_01.arrRightId     = "right_but"; //右按钮ID
scrollPic_01.frameWidth     = 730; //图片容器宽度
scrollPic_01.pageWidth      = 146; //每张图片宽度
scrollPic_01.speed          = 10; //移动速度(单位毫秒，越小越快)
scrollPic_01.space          = 10; //每次移动像素(单位px，越大越快)
scrollPic_01.autoPlay       = false; //自动播放
scrollPic_01.autoPlayTime   = 3; //自动播放间隔时间(秒)
scrollPic_01.initialize();  //初始化

}
