<script>
function switchLinksTo(divId) {
for (var i=1; i<=4; i++) {
var currentDivId = 'links-' + i;
var currentLinkId = 'links-a-' + i;
var currentDiv = document.getElementById(currentDivId);
var currentLink = document.getElementById(currentLinkId);
if (i==divId) {
currentDiv.style.display='block';
currentLink.setAttribute('class', 'current');
currentLink.setAttribute('className', 'current');
} else {
currentDiv.style.display='none';
currentLink.setAttribute('class', '');
currentLink.setAttribute('className', '');
}
}
}
</script>
