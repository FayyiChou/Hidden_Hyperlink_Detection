//弹出框开关
function openDiv() {
    jQuery("#xmdq").OpenDiv();
    var _IsInitXmdq = jQuery("#hidIsInitXmdq").val();
    if ("0" == _IsInitXmdq) {
        TopNav3.GetPlasticBigClass();
        TopNav3.GetXmdq_Hot();
        //ShowThreeMenu();
        jQuery("#hidIsInitXmdq").val("1");
    }
}

function closeDiv() {
    jQuery("#xmdq").CloseDiv();
}



