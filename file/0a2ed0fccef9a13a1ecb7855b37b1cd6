  <script>
    //滚动条滚动到指定的位置  全国整形医院排行模块固定悬浮 
    window.onscroll = function() {
        var iTop = document.body.scrollTop || document.documentElement.scrollTop;
        //document.title=iTop;
        if (iTop > 1100) {
            $("#suspension_R").addClass("topbg2");

        }
        else {
            $("#suspension_R").removeClass("topbg2");

        }

    }

 $(function() {
            /*导航资讯效果*/
            $(".nav_zx").hover(function() {
                $(".zx_nav").slideDown();
            }, function() {
                $(".zx_nav").css("display", "none");
                $(".zx_nav").hover(function() {
                    $(this).css("display", "block");
                }, function() {
                    $(".zx_nav").slideUp();
                });

            });
        })
</script> 
