<SCRIPT type=text/javascript>
var locked_scrollslide_2 = false;
function scrollslide_lock_2()
{
	locked_scrollslide_2 = true;
}
function scrollslide_unlock_2()
{
	locked_scrollslide_2 = false;
}
function scrollslide_2()
{
	if (!locked_scrollslide_2)
	{
		document.getElementById("slides_2").scrollTop +=2;
		if (document.getElementById("slides_2").scrollTop >= document.getElementById("slide0_2").offsetHeight) {
			document.getElementById("slides_2").scrollTop = 0;
		}
	}
}
document.getElementById("slide1_2").innerHTML = document.getElementById("slide0_2").innerHTML;
window.setInterval("scrollslide_2();", 100);
</SCRIPT>
		
		
		
		
		
		
