var speed=30
demoo2.innerHTML=demoo1.innerHTML
function Marquee2(){
if(demoo2.offsetWidth-demoo.scrollLeft<=0)
demoo.scrollLeft-=demoo1.offsetWidth
else{
demoo.scrollLeft++
}
}
var MyMar2=setInterval(Marquee2,speed)
demoo.onmouseover=function() {clearInterval(MyMar2)}
demoo.onmouseout=function() {MyMar2=setInterval(Marquee2,speed)}
