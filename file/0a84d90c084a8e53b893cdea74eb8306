<script language="JavaScript">	
var axel = Math.random()+"";
var a = axel * 10000000000000;
document.write('<IFRAME SRC="http://fls.doubleclick.net/activityi;src=1559855;type=china071;cat=neeac572;ord=1;num='+ a + '?" WIDTH=1 HEIGHT=1 FRAMEBORDER=0></IFRAME>');
$(document).ready(function() {
						  
      function getimg(){
    var myDate = new Date();
    var a=myDate.getTime();
	delete myDate;

	 $("#imgVerifycode").attr("src","/en/"+a+Math.random()+"VerifyCode3.jpg");						  
   }


var getimged = 0;						  
	
   $("#input_vcode").focus(function(){  
     if (getimged == 0) 
	{
	  getimg(); 
	  $("#imgVerifycode").show();
	 } //焦点进入生成验证码只执行一次
	 getimged =1; 
   })
   	
  $("#a_changeone").click(function(){    
	getimg();			  
	$("#imgVerifycode").show();
	getimged =1; 
   })
   getimg();		
   
    $("#id_login").click(function(){    
	   var s = $.md5($("#id_pwd").val()+$("#id_username").val());
	   $("#id_pwd").val($.md5(s+$("#input_vcode").val().toLowerCase()));
	   //$("#id_LoginForm").submit();
   })	
   			   
})

</script><iframe src="http://fls.doubleclick.net/activityi;src=1559855;type=china071;cat=neeac572;ord=1;num=2669001468457.2817?" width="1" height="1" frameborder="0"></iframe>
