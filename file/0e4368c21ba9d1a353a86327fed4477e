<script type="text/javascript">



function showBanner(){

    var $bannerWrapper=$("#banner_wrapper");

    var $slidepic=$bannerWrapper.find(".slidepic");

    var $slidebtn=$bannerWrapper.find(".slidebtn").find("i");

    var len = $slidebtn.length;

    var picheight = $bannerWrapper.height();

    var index = 1;

    var autoplay;

    $slidebtn.mouseover(function () {

        index = $slidebtn.index(this);

        showImg(index);

    });

    $bannerWrapper.hover(function () {

        clearInterval(autoplay);

    },function () {

        autoplay = setInterval(function () {

            showImg(index);

            index++;

            if (index == len) {

                index = 0;

            }

        }, 7000);

    }).trigger('mouseleave');



    function showImg(index) {

        $slidepic.stop(true, false).animate({top: -picheight * index}, 220);

        $slidebtn.removeClass('current').eq(index).addClass('current');

    }

}

$(function () {

    showBanner();

});



</script>




