		<script type="text/javascript">
		function checkForm(obj){
			var age=$(obj).find('input[name=age]');
			var phone=$(obj).find('input[name=phone]');
			var asktitle=$(obj).find('textarea[name=asktitle]');
			var r = /^[-+]?\d*$/;
			if(!$(obj).find("input:checked").length){
				alert('请选择性别。');
				return false;
			};
			if($.trim(age.val())==""){
				alert('请输入年龄。');
				age.focus();
				return false;				
		
			}else if($.trim(age.val())!="" && isNaN(age.val())){
				alert('请输入正确年龄。');
				age.focus();
				age.val('');
				return false;
			}else if(!r.test(age.val())){
				alert('请输入正确年龄。');
				age.focus();
				age.val('');
				return false;
			}else if(parseInt(age.val(),10)>100 || parseInt(age.val(),10)<=0 ){
				alert('请输入正确年龄。');
				age.focus();
				return false;
			};	
		
			if($.trim(phone.val())!="" && $.trim(phone.val())!="免费，不公开"){
				if(!/^(\+86|86|)1[358][0-9]{9}$/.test($.trim(phone.val()))){
					alert('输入正确的手机号！');
					phone.focus();
					phone.val('');
					return false;
				}
			};
		
			if($.trim(asktitle.val()).length<10){
				alert('最少输入10个汉字。');
				asktitle.focus();
				return false;
			};
		
			if($.trim(asktitle.val())=="提问字数大于10字以上，描述越清楚医生回答越详细哦" || lrtrim(asktitle.val())=="" ){
				alert('请输入问题描述。');
				asktitle.focus();
				asktitle.val('');
				return false;
			}
		
			if(/[^\u4E00-\u9FA5a-zA-Z0-9]{2}/.test($.trim(asktitle.val()))){
				alert('请输入问题描述。');
				asktitle.focus();
				asktitle.val('');
				return false;
			}
		}
		</script>

