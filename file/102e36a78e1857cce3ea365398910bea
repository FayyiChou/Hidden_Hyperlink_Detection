		$(function(){
			$('.hd_Seaput').smartShow({
				url: 'http://so.xywy.com/ajax/suggest.php',
				param: 'keyword',
				panel: '.smart_box',
				dataType: 'jsonp',
				success: function(i) {
					var h = '',
						f, g;
					if (i.result > 0 && i.data) {
						$('.smart_box').show();
						f = i.data.split(',');
						g = f.length;
						$.each(f, function(k, j) {
							h += '<li><a href="javascript:;" target="_blank">' + j + '</a></li>';
						});
						$('.smart_box').html(h);
					} else {
						$('.smart_box').hide();
						$('.smart_box').html('');
					}
				},
				operate: function() {
					
				}
			});
			$('.smart_box').on('click','a',function(){
