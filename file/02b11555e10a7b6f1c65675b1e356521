(function($, w){
	$.ajax({
		url : "plugin.php?id=uc_sign:page_view&ac=chk",
		type: "GET",
		dataType: "json",
		success : function(resp){
			if(resp.login === false){
				$("#sign_everyday").click(function(){
					w.location.href = 'member.php?mod=logging&action=login';
				});
			} else if(!resp.signed) {
				$("#sign_everyday").click(function(){
					showWindow('uc_sign_win', 'plugin.php?id=uc_sign:page_view&ac=to_sign', 'get', true);
				});
			} else {
				$("#sign_everyday_entrance").text("今日已签到");
				$("#sign_everyday").click(function(){		
					w.location.href = 'plugin.php?id=uc_sign:page_view';
				});
			}
		},
		error : function(e){
			console.log(e);
		}
	});
})(jQuery, window);
