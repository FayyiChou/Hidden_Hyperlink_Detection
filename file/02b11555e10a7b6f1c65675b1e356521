<div class="_bannerPar banner-par">
	<div class="_bannerSub banner-sub" style="display:block"><a href="http://rec.uc.cn/superstar/index?entry=bbs&amp;uc_param_str=dnfrpfbivesscpnieisids" target="_blank" title="杨颖"><img class="img-ban" src="data/attachment/portal/201509/17/092726wf96499x99xxeafa.jpg" alt="杨颖"></a><a href="http://rec.uc.cn/superstar/index?entry=bbs&amp;uc_param_str=dnfrpfbivesscpnieisids" target="_blank" title="汤唯"><img class="img-ban" src="http://bbs.uc.cn/data/attachment/forum/201509/16/190849wskyp58595zrxz58.png" alt="汤唯"></a></div>
	<div class="_bannerSub banner-sub"></div>
	<div class="_bannerSub banner-sub"></div>
</div><script>
(function($){
//bannerplay
$.fn.bannerPlay=function(){
	var tAnimate=1;
	$(this).each(function(index, element) {
    	$(this).delegate(".font","click",function(){
			$par=$(this).closest("._bannerPar");
			var $subV=$par.children("._bannerSub:visible");
			var indexV=$subV.index();
			var length=$par.children("._bannerSub").length;
			var indexN=getFlowIndex(false,indexV,length);
			if(tAnimate) play(false,indexN,$par);
		});
    	$(this).delegate(".next","click",function(){
			$par=$(this).closest("._bannerPar");
			var $subV=$par.children("._bannerSub:visible");
			var indexV=$subV.index();
			var length=$par.children("._bannerSub").length;
			var indexN=getFlowIndex(true,indexV,length);
			if(tAnimate) play(true,indexN,$par);
		});
	
	});
	//获取目标index
	var getFlowIndex=function(bo,index,length){
		if(bo) return index+1<length?index+1:0;
		else return index-1>=0?index-1:length-1;
	};
	//执行动画
	var play=function(bo,indexN,$par){
		tAnimate=0;
		var $subV=$par.children("._bannerSub:visible");
		var indexV=$subV.index();
		var width=$subV.width();
		var $boxV=$par.children("._bannerSub:eq("+indexV+")");
		var $boxN=$par.children("._bannerSub:eq("+indexN+")");
		if(bo){
			$boxN.css({
				"left":width+"px"
				,"display":"block"
			});
			$boxV.stop(false,true).animate({"left":"-="+width+"px"},500);
			$boxN.stop(false,true).animate({"left":"-="+width+"px"},500,function(){
				$boxV.hide();
				tAnimate=1;
			});
		}else{
			$boxN.css({
				"left":-width+"px"
				,"display":"block"
			});
			$boxV.stop(false,true).animate({"left":"+="+width+"px"},500);
			$boxN.stop(false,true).animate({"left":"+="+width+"px"},500,function(){
				$boxV.hide();
				tAnimate=1;
			});
		} 
	};
};
$("._bannerPar").bannerPlay();
})(jQuery);
</script>
