  <script>
function show_search(num)
{
	$$$("div_search_bg").style.display = "block";
	$$$("search_box").style.display = "block";
}
function hide_search()
{
	$$$("div_search_bg").style.display = "none";
	$$$("search_box").style.display = "none";
}

function showclass()
{
	$$$("div_BG").style.display = "block";
	$$$("class_box").style.display = "block";
}
function hideclass()
{
	$$$("div_BG").style.display = "none";
	$$$("class_box").style.display = "none";
}
function show_smallclass(id)
{
	if($$$("ol_sclass_"+id).style.display == "none")
	{
		$$$("ol_sclass_"+id).style.display = "";
	}
	else
	{
		$$$("ol_sclass_"+id).style.display = "none";
	}
}
</script> 
