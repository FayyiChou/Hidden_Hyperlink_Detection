  <script type="text/javascript">

function showcont()
{
	contbox = $$$("contbox");
	showbtn = $$$("showbtn");
	if(contbox.style.height!="100px")
	{
		contbox.style.height="100px";
		showbtn.innerHTML="展开";
		showbtn.className="";
	}
	else
	{
		contbox.style.height="100%";
		showbtn.innerHTML="收起";
		showbtn.className="span_up";
	}
}
</script>  
