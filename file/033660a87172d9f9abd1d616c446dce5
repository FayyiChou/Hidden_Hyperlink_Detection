    <script>
    (function(win) {
        var doc = win.document;
        var docEl = doc.documentElement;
        var tid;
        var isUC = /UCWEB|UCBrowser/i.test(navigator.userAgent);
        var devicePixelRatio = win.devicePixelRatio;
        var rem;
        if (devicePixelRatio >= 3) {
            dpr = 3;
        } else if (devicePixelRatio >= 2) {
            dpr = 2;
        } else {
            dpr = 1;
        }
        docEl.setAttribute('data-dpr', dpr);

        function refreshRem() {
            var bcRect = docEl.getBoundingClientRect(),
                width = Math.min(bcRect.width, 750);
                rem = width / 7.5;
            docEl.style.fontSize = rem + 'px';
            /*if (isUC) {
                //doc.body.style.fontSize = (w / 1e5 + 16) + 'px'; //修复uc的bug
            }*/
        }
        win.addEventListener('resize', function() {
            clearTimeout(tid);
            tid = setTimeout(refreshRem, 300);
        }, false);
        win.addEventListener('pageshow', function(e) {
            if (e.persisted) {
                clearTimeout(tid);
                tid = setTimeout(refreshRem, 300);
            }
        }, false);
        refreshRem();
        win.rem2px = function(d) {
            var val = parseFloat(d) * rem;
            if (typeof d === 'string' && d.match(/rem$/)) {
                val += 'px';
            }
            return val;
        }
    })(window);
    </script>
