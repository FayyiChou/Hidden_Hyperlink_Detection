                <script type="text/javascript">
 var pics="";
 var links="";
 var texts="";
var tbl=document.getElementById("picurl"); 
var tempTr,tempTd,tempTd1,tempTd2;
var count=0;
for(var i=0;i<getcount;i++)
{
tempTr=tbl.rows[i];
tempTd=tempTr.cells[0];
tempTd1=tempTr.cells[1];
tempTd2=tempTr.cells[2];

if(tempTd.innerHTML!="" && count<6)
{
count++;
 if(pics=="")
 {
  pics=tempTd.innerText;
  links=tempTd2.innerText;
  texts=tempTd1.innerText;
 }
  else
 { 
  pics+="|"+tempTd.innerText;
  links+="|"+tempTd2.innerText;
  texts+="|"+tempTd1.innerText;
  }
}
}
	 var focus_width=336
	 var focus_height=236
	 var text_height=18
	 var swf_height = focus_height+text_height
	 document.write('<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0" width="'+ focus_width +'" height="'+ swf_height +'">');
	 document.write('<param name="allowScriptAccess" value="sameDomain"><param name="movie" value="./images/focus.swf"><param name="quality" value="high"><param name="bgcolor" value="#F0F0F0">');
	 document.write('<param name="menu" value="false"><param name=wmode value="opaque">');
	 document.write('<param name="FlashVars" value="pics='+pics+'&links='+links+'&texts='+texts+'&borderwidth='+focus_width+'&borderheight='+focus_height+'&textheight='+text_height+'">');
	 document.write('</object>');							
                </script> 
                
