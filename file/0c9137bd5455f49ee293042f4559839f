    <script type="text/javascript">
        //切换新闻
        function ToggleNews() {
            $("#ulNews").show();
            $("#ulNotice").hide();
            $("#aNotice").removeClass("active");
            $("#aNews").addClass("active");
        }
        
        //切换公告
        function ToggleNotice() {
            $("#ulNotice").show();
            $("#ulNews").hide();
            $("#aNews").removeClass("active");
            $("#aNotice").addClass("active");
        }

        //切换广告图
        $(function() {
            //图片广告
            var nav = '';
            $('#banner').find('a').each(function(i){
                var item = $(this),
                    id = 'banner_item_' + i;

                item.attr('id', id);
                nav += '<li class="ui-switchable-trigger'
                    + (i === 0 ? ' ui-switchable-active' : '')
                    +'" data-rel="' + id + '"></li>';
            });


            nav = $('#banner-nav').html(nav).find('li');

            nav.powerSwitch({
                animation: "translate",
            	classAdd: 'ui-switchable-active',
            	container: $('#banner-control'),
            	classPrev: 'ui-switchable-upper',
            	classNext: 'ui-switchable-next'
            });
        });
    </script>
