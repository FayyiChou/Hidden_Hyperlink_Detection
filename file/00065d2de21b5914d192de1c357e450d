<script src="http://img.job5156.com/static/js/v1/third/plug/jBox/jquery.jBox.min.js?skin=v3"></script>
<script src="http://img.job5156.com/static/js/v3/common/base_common.min.js?v=1490688332375"></script>
<script src="http://img.job5156.com/static/js/v1/data/area.min.js"></script>
<script src="http://img.job5156.com/static/js/v1/data/jobFun.min.js"></script>
<script src="http://img.job5156.com/static/js/v1/data/otherData.min.js"></script>
<script src="http://img.job5156.com/static/js/v1/common/jqPlugs_common.min.js"></script>
<script src="http://img.job5156.com/static/js/v3/common/jquery.boxSelector.min.js"></script>
<script src="http://img.job5156.com/static/js/v3/header.min.js"></script>
<script src="http://img.job5156.com/static/js/v3/search/searcher.min.js"></script>
<script src="http://img.job5156.com/static/js/v3/search/searchPage.min.js?v=1482808572361"></script>

<script>
  
  var now=1491050332791;
  var keyword="客服";
  var isSysLogin=false;
  var isSys=null;
  var cate=null;
  var locationList=[14080000];
  var isSys=null;

  var searchPage = new module.searchPage();

  searchPage.initListPage();
  $(function(){
        common.fn.toolBar()
    })


  var $jobtip = $("#jobtip"),$tipBg = $("#tipBg"),posId="",comId="",comName="",perId=$("#topPerUserId").val();
  var setJobtucao=function(){
    var jobtucao=common.Cookie.get("jobtucao")||0;
      jobtucao=parseInt(jobtucao)+1;
      common.Cookie.set("jobtucao",jobtucao,{hours:24*30,path:"/"});
      $("#jobTipForm")[0].reset();
      $jobtip.find(".item").removeClass("selected");
      $jobtip.find(".textarea1").removeClass("show");
  }
  $("#tipBtn").click(function jobtucao(){
    var $this=$(this);
    common.fn.checkLogin("per",function(data){
        if(data.state) {
            var jobtucao=common.Cookie.get("jobtucao")||0;
            jobtucao=parseInt(jobtucao);
            if(jobtucao>1){
                return
            }
            var json=JSON.parse($this.val());
            posId=json.posId,
            comId=json.comId,
            comName=json.comName;
            $.ajax({
                url:"/apply/job/"+posId+"/jobtucao.json",
                success:function(data){
                    if(data.quedingTuCao){
                        $tipBg.addClass("show");
                        $jobtip.attr("class","jobtip show");
                    }
                }
            });
        }
    });


  });
  $jobtip.find(".item").on("click",function () {
      var $this = $(this);
      if($this.is(".selected")){
          $this.removeClass("selected");
          $this.find("input").removeAttr("checked");
      }else{
          $this.addClass("selected");
          $this.find("input").attr("checked",true);
      }
  });
  $jobtip.find(".more").on("click",function () {
      if($(this).is(".selected")){
          $jobtip.find(".textarea1").addClass("show");
      }else{
          $jobtip.find(".textarea1").removeClass("show");
      }
  });
  $jobtip.find(".close").on("click",function(){
      setJobtucao();
      logPerBehavWeb("jobucao:close","",{"posid":posId,"perId":perId});
      $jobtip.attr("class","jobtip tipHide");
      $tipBg.removeClass("show");
  });
  $jobtip.find(".btn").on("click", function () {
      var $this = $(this);
      $this.attr("disabled", true);
      var submt = function () {
          logPerBehavWeb("jobucao:submit","",{"posid":posId,"perId":perId});
          $.ajax({
              url: '/apply/job/save/41.json?posId=' + posId + '&&comId=' + comId + '&&comName=' + comName,
              type: 'POST',
              dataType: 'json',
              data: $("#jobTipForm").serialize(),
              success: function (data) {
                  if (data.errMsg) {
                      $.jBox.error(data.errMsg);
                  } else {
                      setJobtucao();
                      $jobtip.attr("class", "jobtip tipHide");
                      $tipBg.removeClass("show");
                      $.jBox.tip("保存成功!");
                      $this.removeAttr("disabled");
                  }
              }
          });
      }
      submt();
  })

</script>
