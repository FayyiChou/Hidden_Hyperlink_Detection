<script type="text/javascript">

  var p1 = new BMap.Point(104.301181,30.603368);
  var p2 = new BMap.Point(103.968,30.5844);
  var start_lng = (104.301181 + 103.968) / 2;
  var start_lat = (30.603368 + 30.5844) / 2;
  var base_point = new BMap.Point(start_lng, start_lat);
  function showMap(mapType) {
    var map = new BMap.Map("allmap");
    map.centerAndZoom(base_point, 11);
    if (mapType == 'bus') {
        var transit = new BMap.TransitRoute(map, {renderOptions:{map: map, autoViewport: true}});
        transit.search(p1, p2);
    } else if (mapType == 'car') {
        var driving = new BMap.DrivingRoute(map, {renderOptions:{map: map, autoViewport: true}});
        driving.search(p1, p2);
    } else {
        var walking = new BMap.WalkingRoute(map, {renderOptions:{map: map, autoViewport: true}});
        walking.search(p1, p2);
    }
  }
</script>

