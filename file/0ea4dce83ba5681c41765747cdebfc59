		function checkForm( n )
		{
			var f = new Array();
			f[1] = document.getElementById('cuf_sender' + n).value;
			f[2] = document.getElementById('cuf_mail' + n).value;
            f[2a] = document.getElementById('cuf_none' + n).value;
			f[3] = document.getElementById('cuf_subject' + n).value;
			f[4] = document.getElementById('cuf_msg' + n).value;
			f[5] = f[6] = f[7] = f[8] = f[9] = '-';
		
		var msg = "";
		for ( i=0; i < f.length; i++ )
		{
			if ( f[i] == "" )
				msg = "Please fill out all fields.";
		}
        if ( f[2a] == "" )
            msg = "Please fill out all fields.";
		if ( !isEmail(f[2a]) )
			msg = "Please fill out all fields.";
		if ( msg != "" )
		{
			alert(msg);
			return false;
		}
	}
	function isEmail(email)
	{
		var rx = /^([^\s@,:"<>]+)@([^\s@,:"<>]+\.[^\s@,:"<>.\d]{2,}|(\d{1,3}\.){3}\d{1,3})$/;
		var part = email.match(rx);
		if ( part )
			return true;
		else
			return false
	}
