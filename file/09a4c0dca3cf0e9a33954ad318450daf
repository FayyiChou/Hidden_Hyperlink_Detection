<script language="JavaScript">
var flag=false;
function DrawImage(ImgD){ 
var image=new Image();
image.src=ImgD.src;
if(image.width>0 && image.height>0){
flag=true;
if(image.width/image.height>= 210/165){
if(image.width>210){
ImgD.width=210;
ImgD.height=(image.height*210)/image.width;
}else{
ImgD.width=image.width;
ImgD.height=image.height;
}
ImgD.alt=image.width+"x"+image.height;
}
else{
if(image.height>165){
ImgD.height=165;
ImgD.width=(image.width*165)/image.height;
}else{
ImgD.width=image.width;
ImgD.height=image.height;
}
ImgD.alt=image.width+"x"+image.height;
}
}
}
</script>
