<script type="text/javascript">
  var t = document.getElementById("scroll");

  function sLeft(){
  t.scrollLeft += 3;
  if (t.scrollLeft >= 400)
  {
    t.scrollLeft = 0;
  }

  }
function xiangzuo(){xz = setInterval(sLeft,220)}
function xzTingzhi(){clearInterval(xz)}
  function sRight(){
  
  if (t.scrollLeft == 0)
  {
  t.scrollLeft = 400
  }

  t.scrollLeft += -3;
  }
function xiangyou(){xy = setInterval(sRight,220)}
function xyTingzhi(){clearInterval(xy)}
  </script>
