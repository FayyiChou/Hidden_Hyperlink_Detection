<script type="text/javascript">
//Tab切换
function showq(names,z,x){
  for(var e=1;e<=x;e++){
    var k = document.getElementById(names+e);
    var h = document.getElementById(names+"_"+e);
      k.className = e===z?"hover":"";
      h.className = e===z?"dis":"undis";
    }
  }
</script>             </div>
