<script>
var aDiv = document.getElementById("Jclassify");
var adl=document.getElementById("jsDl");
var ac=document.getElementById("jclass"); var om1=document.getElementById("m1");var om2=document.getElementById("m2");var om3=document.getElementById("m3");
(function (){
var oA = document.getElementById("Jbtn");
var add = adl.getElementsByTagName("dd");
var iHeight = add[0].offsetHeight * add.length;
var iLimitH = add[0].offsetHeight * 2;
var bShow = false;
if (add.length >2){
ac.style.height = iLimitH + "px";
oA.innerHTML = "展开";
oA.className="show";
}
else{
oA.innerHTML = "收起";
oA.className="hide";    
}
oA.onclick = function (){
bShow = !bShow;
doMove(ac, bShow ? iHeight : iLimitH);
oA.innerHTML = bShow ? "收起" : "展开";
oA.className = bShow ? "hide" : "show";
om1.className = bShow ? " mHide" : " ";
om2.className = bShow ? " mHide" : " ";
om3.className = bShow ? " mHide" : " ";
};
})()    
function doMove(obj, iTarget, callback){
clearInterval(obj.timer);
obj.timer = setInterval(function (){
var iSpeed = (iTarget - obj.offsetHeight) / 5;
iSpeed = iSpeed > 0 ? Math.ceil(iSpeed) : Math.floor(iSpeed);           
iTarget == obj.offsetHeight ? (clearInterval(obj.timer), callback && callback()) : obj.style.height = iSpeed + obj.offsetHeight + "px"
}, 30)      
}
</script>
