function   maximize_window(){ 
var   window_width=screen.availWidth 
var   window_height=screen.availHeight 
self.resizeTo(window_width,window_height) 
self.moveTo(0,0) 
} 

function checkinput(inti)
{
    //判断用户名、密码是否为空background:url('images/loginbg2012.jpg') no-repeat top center;
    if ((document.getElementById("txtNickname").value == "") || (document.getElementById("txtPassword").value == "")) {
        alert("请输入用户名和密码。");
        return false;
    }
    else
    {
        __doPostBack('btnLogin', '');
    }
}

function zhuce() {
    window.location.href = "Ind/Disclaimer.aspx" ;
}
