                        (function($){
                            if ( typeof window.pgvMain != 'function' ) {
                                window.pgvMain = function() {}
                            }
                            var username_obj = eval('(' + getCookie(cookiepre + 'loginmember') + ')');
                            var loginBox = $('#mzCust'),
                                notLogin = $('.not-login-block-sub'),
                                logDown = $('.log-dropdown'),
                                img = $('.log-img-box .user-avatar'),
                                imgbox = $('.log-img-box'),
                                loginArea = $('.loginArea'),
                                newsNum = $('#newsNum'),
                                userArror = $('.user-arrow'),
                                userArror2 = $('.arrow', imgbox)

                            newsNum.on('mouseenter', function(){
                                loginArea.removeClass('loginArea-hover');

                                if(typeof(username_obj.uid) == "undefined" || !username_obj.uid) {
                                    notLogin.hide();
                                } else {
                                    logDown.hide();
                                }
                            })

                            if(username_obj.uid) {
                                imgbox.css('position','relative');
                                imgSrc = username_obj.userhead.replace(/"\+\+.*/g,'');
                                img.attr('src',imgSrc);
                                userArror.hide();
                                userArror2.hide();
                            }else{
                                loginArea.css('position','relative');
                                newsNum.hide();
                            }
                            var _timer = null, _timer2 = null;
                            function clearTimer(){
                                if(_timer) clearTimeout(_timer);
                                if(_timer2) clearTimeout(_timer2);
                            }
                            imgbox.on('mouseenter', function(e){
                                clearTimer();
                                loginArea.addClass('loginArea-hover');
                                if(typeof(username_obj.uid) == "undefined" || !username_obj.uid) {
                                    notLogin.show();
                                } else {
                                    logDown.show();
                                }
                                hideMenu()
                                e.preventDefault();
                            }).on('mouseleave', function(){
                                _timer = setTimeout(function(){
                                    loginArea.removeClass('loginArea-hover');
                                    if(typeof(username_obj.uid) == "undefined" || !username_obj.uid) {
                                        notLogin.hide();
                                    } else {
                                        logDown.hide();
                                    }
                                }, 600);

                            });

                            function hideMenu() {
                                $('.meizu-header-sub').each(function(k,v){
                                    $(v).removeClass('meizu-header-sub-animation meizu-header-sub-show')
                                });
                            }

                            $('.not-login-block-sub,.log-dropdown').on('mouseenter', function(){
                                 clearTimer();
                            }).on('mouseleave', function(){
                                _timer2 = setTimeout(function(){
                                    loginArea.removeClass('loginArea-hover');
                                    if(typeof(username_obj.uid) == "undefined" || !username_obj.uid) {
                                        notLogin.hide();
                                    } else {
                                        logDown.hide();
                                    }
                                }, 600);
                            });

                            window.setTimeout(function(){
                              var num = $('#newsNum'),
                                  logDown = $('.log-dropdown'),
                                  _num = parseInt(num.text(), 10);
                              if(_num > 10) {
                                  logDown.css('right', '-44px');
                              } else if (_num > 99) {
                                  logDown.css('right', '-51px');
                              }
                          }, 800);
                        }(window.jQuery));
