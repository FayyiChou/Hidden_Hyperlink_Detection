<script type="text/javascript">
  function getCookie(objName){//获取指定名称的cookie的值
        var arrStr = document.cookie.split("; ");
        for(var i = 0;i < arrStr.length;i ++){
        var temp = arrStr[i].split("=");
        if(temp[0] == objName)
          return unescape(temp[1]);
        }
        return false;
}
var cookiepre = 'MZBBS_2132_';
var cookie_mobile = getCookie(cookiepre + 'mobile');


if(!navigator.userAgent.match(/pad|gt-p1000/i) && cookie_mobile !== 'no' && static_index && navigator.userAgent.match(/android|webos|ip(hone|od)|opera (mini|mobi|tablet)|iemobile|windows.+phone|mobile|fennec|kindle (Fire)|maemo|blackberry|playbook|bb10\; (touch|kbd)|Symbian(OS)|Ubuntu Touch/i)){
window.location.href='http://bbs.sysceo.com/index.php?mobile=yes';
}
    </script>
