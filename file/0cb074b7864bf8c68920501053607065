    <script type="text/javascript" language="javascript">

        function body_onload() {
            var mainCategoryId;
            var elementId = "hdnMainCategoryId";
            mainCategoryId = jQuery("#" + elementId)[0].value;

            if ((mainCategoryId == "1") || (mainCategoryId == "2") || (mainCategoryId == "3") || (mainCategoryId == "4")) {

                elementId = "mainTitleLi" + mainCategoryId;
                var selectedLiObj = jQuery("#" + elementId)[0];
                selectedLiObj.className = "current";

                elementId = "sideMainTitleLi" + mainCategoryId;
                selectedLiObj = jQuery("#" + elementId)[0];
                selectedLiObj.className = "current";
            }

            if ((mainCategoryId == "5") || (mainCategoryId == "7")) {
                elementId = "mainTitleLi" + mainCategoryId;
                var selectedLiObj = jQuery("#" + elementId)[0];
                selectedLiObj.className = "current";
            }

        }
    
    </script>


