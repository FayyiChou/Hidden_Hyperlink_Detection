    <script type="text/javascript">
        $(document).ready(function (e) {
            $(".tab li").click(function () {
                $(".tab li").eq($(this).index()).addClass("focus").siblings().removeClass('focus');
                $(".dinggou").hide().eq($(this).index()).show();
            });
        });

        function getParameter(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }

        function order(productCode, version) {
            if (document.getElementById("qr").checked != true) {
                layer.alert("请您仔细阅读并勾选同意签署协议！");
            }
            else {
                var arCo = getParameter("ArCo");
                if (arCo === null) {
                    window.open("/ajaxs/order.ashx?productCode=" + productCode + "&version=" + version);
                } else {
                    window.open("/ajaxs/order.ashx?productCode=" + productCode + "&version=" + version + "&arCo=" + arCo);
                }
            }
        }
    </script>


