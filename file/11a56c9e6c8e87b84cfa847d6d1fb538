              <span class="asspan ass-3 JassBtn"><span class="ass-text">物业很贴心</span></span>
              <span class="asspan ass-4 JassBtn"><span class="ass-text">干净</span></span>
              <span class="asspan ass-0 JassBtn"><span class="ass-text">小区比较安静</span></span>
              <span class="asspan ass-1 JassBtn"><span class="ass-text">绿化率高</span></span>
              <label><span class="ass-Input">
                  <input type="text" value="标签最多可输入8个字" maxlength="8" style="color: rgb(200, 200, 200); "></span><span class="ass-btn">+ 添加</span>
              </label>
          </div>
          <script type="text/javascript">
            ;(function(){
                var Url={
                    updata:'http://dict.58.com/xiaoqu/guomao/evaluate/addEvaluate'
                }
                var xiaoId=____json4fe.infoid;
                var defaultTpl='<span class="asspan ass-defult">小区还没有印象，赶快来评价一下吧！</span>';
                var nextTpl='<span class="asspan ass-next">...</span>';
                var ass={
                    text:'标签最多可输入8个字',
                    keyup:$.noop
                };
                var xqCookie=xq.cookie('xqyx',{domain:'58.com',expires:1});
                var arr=[];
                var hasArr=xqCookie.get('ARR'+xiaoId)||[];
                upDataDiv();
                bindEvent();
                function checkTimes(text){
                    var num=xqCookie.get('cs'+xiaoId)||0;
                    if(num>2){
                        xq.tip.call(this,{
                            text:'您已经添加3个印象了~明天再来吧~',
                            cls:'red_lt',
                            id:'tip_assInput',
                            x:0,
                            y:28
                        });
                        return false;
                    }
                    if(hasArr.indexOf(text)>-1){
                        xq.tip.call(this,{
                            text:'请不要重复评价',
                            cls:'red_lt',
                            id:'tip_assInput',
                            x:0,
                            y:28
                        });
                        return false;
                    }
                    return true;
                }
                function upDataArr(text,tag){
                    var hastext=false;
                    var n=Math.floor(Math.random()*6);//0-5;
                    var that=this;
                    if(!checkTimes.call(this,text))return;
                    
                    $.ajax({
                        url:Url.updata,
                        data:{
                            xiaoquId:xiaoId,
                            text:text
                        },
                        dataType:'jsonp',
                        success:function(data){
                        	if(!data)return;
                        	var msg=data.msg
                        	if(!data.tag){
                        		xq.tip.call(that,{
                                    text:msg,
                                    cls:'red_lt',
                                    id:'tip_assInput',
                                    x:0,
                                    y:28
                                });
                        		return;
                        	}
                        	var num=xqCookie.get('cs'+xiaoId)||0;
                        	xqCookie.set('cs'+xiaoId,num+1);
                        	$(arr).each(function(i,item){
                                if(item.text!=text)return;
                                hastext=true;
                                item.num=item.num||1;
                                item.num=!tag?item.num+1:itme.num-1;
                            });
                            !hastext&&arr.push({text:text,num:1,cls:'ass-'+n});
                            arr= $.grep(arr,function(item){
                                return item.num!==0;
                            });
                            upDataDiv();
                            hasArr.push(text);
                            xqCookie.set('ARR'+xiaoId,hasArr);
                            $(that).find('input').val('').focus();
                        }
                    })

                }
                function upDataDiv(){
                    if(arr.length==0){
                        $('.assessDiv').html(defaultTpl);
                        return
                    }
                    var tpl='';
                    $(arr).each(function(i,item){
                        tpl+='<span class="asspan '+item.cls+' JassBtn"><span class="ass-text">'+item.text+'</span>'+(item.num>1?'<span class="ass-num">（'+item.num+'）</span>':'')+'</span>'
                    })
                    tpl+=nextTpl;
                    $('.assessDiv').html(tpl);
                }
                function bindEvent(){
                    $('.ass-Input input').placeholder({text:ass.text});
                    $('.ass-btn').live('click',function(){
                        var text=$('.ass-Input input').val();
                        if(text==ass.text||text=='')return;
                        var isFail=(text.match(/[^\u2E80-\u9FFF]/g)&&(text.match(/[^\u2E80-\u9FFF]/g).length>4));
                        if(isFail){
                            var tip=xq.tip.call(this,{
                                text:'请填写中文印象词哟',
                                cls:'red_lt',
                                id:'tip_assInput',
                                x:-142,
                                y:28
                            });
                            return;
                        }
                        upDataArr.call($('.ass-Input')[0],text);

                    })
                    $('.JassBtn').live('click',function(){
                        var text= $(this).find('.ass-text').text();
                        upDataArr.call(this,text);
                    })
                }
                
            })();
          </script>

      </div>
  </div>
   
