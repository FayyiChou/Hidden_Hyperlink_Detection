<script type="text/javascript">
;(function(){
	var Url={
		guanzhu:'http://dict.58.com/xiaoqu/guomao/attention/ajaxAttention/'
	};
	
    function updataPic(opt){
        var opt=opt||{};
        var ulobj=$('#'+opt.id);
        var liobj=$('li',ulobj);
        var size=liobj.length;
        
        var step=liobj.width()+12;
        var n=3;
        var move;
        var min=0-step*(size-4);
        ulobj.css('width',step*size)
        if(size<=4){
        	$('.photolist .controlBtn').hide();
        }
        function initMove(){
            move=xq.moveMent('li',ulobj);
            move.setOptions({
                step:step*n,
                direc:['left','-='],
                min:min
            }).addControlBtn('.photolist .controlBtn');
        }

        initMove()
    }
    updataPic({id:'phcUl'});
   $('#phcUl li').live('click',function(){
    var bigSrc=$(this).find('img').attr('bigSrc')||$(this).find('img').attr('src');
    $('.cphoto img').attr('src',bigSrc);
    $(this).parent().find('li').removeClass('select');
    $(this).addClass('select');
   })
    //加载回到顶部
    $.scrollToTop();
    //加入对比
    xq.addDuibi();
    //nav标签切换
    ;(function(){
    var scrollObj={}
    $('.scrollNav li').each(function(){
        var id=$(this).attr('switchId');
        scrollObj[id]=xq.scroll({obj:$('#'+id),maxH:160});
    })
    $('.opNav li,.dpNav li').live('mouseenter',function(){
        var id=$(this).attr('switchId'),
            href=$(this).attr('switchHref'),
           cls=$(this).parent().attr('switchCls');
        $(this).parent().find('li').removeClass('select');
        $(this).addClass('select');
        cls&&$('.'+cls).removeClass('select');
        id&&$('#'+id).addClass('select');
        href&&$(this).parents('.opNav').find('.opNav_more').attr('href',href);
        scrollObj[id]&&scrollObj[id].updata();
    });
    })();

    function updataBottomPic(opt){
        var opt=opt||{},ulobj=$('#'+opt.id),liobj=$('li',ulobj),size=liobj.length,step=liobj.width()+30;
        var n= 4,move;
        var min=0-step*(size-5);
        ulobj.css('width',step*(size+1));
        if(size<=5){
        	$('.opMain .controlBtn').hide();
        }
        function initMove(){
            move=xq.moveMent('li',ulobj);
            move.setOptions({
                step:step*n,
                direc:['left','-='],
                min:min
            }).addControlBtn('.opMain .controlBtn');
        }
        initMove()
    }
    updataBottomPic({id:'opMainUl'});
    
    function updataBottomPic02(opt){
        var opt=opt||{},ulobj=$('#'+opt.id),liobj=$('li',ulobj),size=liobj.length,step=liobj.width()+10;
        var n= 4,move;
        var min=0-step*(size-4);
        ulobj.css('width',step*(size+1));
        if(size<=4){
        	$(opt.btn).hide();
        }
        function initMove(){
            move=xq.moveMent('li',ulobj);
            move.setOptions({
                step:step*n,
                direc:['left','-='],
                min:min
            }).addControlBtn(opt.btn);
        }
        initMove();
    }
	updataBottomPic02({id:'fangyuan0list',btn:".fangyuan0 .controlBtn"});
    updataBottomPic02({id:'fangyuan1list',btn:".fangyuan1 .controlBtn"});


    
    
    //加关注
     var gzIdCookie=xq.cookie('gzId');
    $('.guanzhuBtn ').live('click',function(){
        var id=____json4fe.infoid,that=this;
        var surl=xq.Qurl().set('gzId',id).getUrl(true);
        if(!xq.checkIfLogin()){
        	 gzIdCookie.set('id',id);
             xq.getLogin('http://'+location.host+location.pathname+surl);
            return;
          }
        if($(this).hasClass('hasguanzhu')){
        	 $.ajax({
                 url:Url.guanzhu,
                 data:{
                     xiaoquId:'975871',
                     status:'del'
                 },
                 dataType:'jsonp',
                 success:function(data){
                  $(that).removeClass('hasguanzhu');
                  
                 }
             })
        }else{
            $.ajax({
                url:Url.guanzhu,
                data:{
                    xiaoquId:975871,
                    status:'add'
                },
                dataType:'jsonp',
                success:function(data){
                    if(!data){
                    	xq.tip.call(that,{
                            cls:'red_lb',
                            id:'tip_hasguanzhu',
                            text:'您只能关注20个小区哟',
                            x:22,
                            y:-43
                        });
                        return;
                   }
                    $(that).addClass('hasguanzhu');
                    xq.tip.call(that,{
                        cls:'green_lb',
                        id:'tip_hasguanzhu',
                        text:'关注成功',
                        x:22,
                        y:-43
                    })
                }
            })
        }
    })
    var gzId=xq.Qurl().get('gzId');
    var cgzId=gzIdCookie.get('id');
    if(cgzId){gzIdCookie.del('id')}
    console.log(gzId,cgzId,____json4fe.infoid);
    if(gzId&&gzId==____json4fe.infoid&&gzId==cgzId){
    	!$('.guanzhuBtn').hasClass('hasguanzhu')&&$('.guanzhuBtn').click();
    }

    })();
