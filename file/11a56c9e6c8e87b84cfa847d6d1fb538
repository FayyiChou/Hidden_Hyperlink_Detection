		jsonp: 'callback',
		success: function(valdata) {
			try {
				//$("#xiaoquNav ul li").eq(3).after("<li><a target=\"_blank\"  onClick=\"clickLog('fc_sohu_xiaoqu1')\" href=\""+decodeURI(valdata.url)+"\"  rel=\"nofollow\" >新房</a></li>");
			} catch (e) {

			}
		}
		
	});	
	
});

$.extend({ isEmpty:function(v,allowBlank){ return v===null||v===undefined||(($.isArray(v)&&!v.length))||(!allowBlank?v==='':false); }, urlAppend:function(url,s){ if(!$.isEmpty(s)){ return url+(url.indexOf('?')===-1?'?':'&')+s; } return url; } });

function xinfang(){
	$.getJSON(
			"http://house.focus.cn/common/newhouse/api/loupandataforinfolist.api.php?city_id=7969&callback=?",
			function(data){
				if(!data || !data.items)
					return;
				var html = [];
				var key = '', value = '';
				$(data.items).each(function(i, item){
					for(var o in item){
						key = o;
						value = item[o];
					}	
					return false; 
				});
				var url=value.substring(0, value.lastIndexOf("focus.cn"));
				if(!$.isEmpty(url)){
					//url+="focus.cn/search/index.html";
					//$("#xiaoquNav ul li").eq(3).after("<li><a target=\"_blank\"  onClick=\"clickLog('fc_sohu_xiaoqu1')\" href=\""+url+"\"  rel=\"nofollow\">新房</a></li>");
				}				
			}
		);
}
</script>
