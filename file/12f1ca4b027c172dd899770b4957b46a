  <script type="text/javascript">
function showTime(yy,mm,dd,hh,mn){
var nowTime = new Date();
var pubTime = new Date();	
pubTime.setFullYear(yy);
pubTime.setMonth(mm-1);
pubTime.setDate(dd);
pubTime.setHours(hh);
pubTime.setMinutes(mn);	
var gap = nowTime.getTime() - pubTime.getTime();
function zero(i){
if(i < 10) return i = '0' + i;
else return i;
}	
if(gap < 1*60*60*1000) document.write(parseInt(gap/1000/60) + '分钟前');
if(gap >= 1*60*60*1000 && gap <  24*60*60*1000) document.write(parseInt(gap/1000/60/60) + '小时前');
if(gap >= 24*60*60*1000 && gap <  48*60*60*1000) document.write(parseInt(gap/1000/60/60/24) + '天前');
if(gap > 48*60*60*1000) document.write(yy + '-' + zero(mm) + '-' + zero(dd));	
}
</script>
