<script type="text/javascript">
    var ScrollingSpeed = 130;
    ImgDemo2.innerHTML = ImgDemo1.innerHTML;
    function ScrollMarquee() {
        if (125 - ImgDemo.scrollTop <= 0) {
            ImgDemo.scrollTop -= ImgDemo1.offsetHeight;
        }
        else {
            ImgDemo.scrollTop++;
        }
    }
    var ScrollingMar = setInterval(ScrollMarquee, ScrollingSpeed);
    ImgDemo.onmouseover = function () {
        clearInterval(ScrollingMar);
    } //鼠标移上时清除定时器达到滚动停止的目的
    ImgDemo.onmouseout = function () {
        ScrollingMar = setInterval(ScrollMarquee, ScrollingSpeed);
    } //鼠标移开时重设定时器 
</script>



           
