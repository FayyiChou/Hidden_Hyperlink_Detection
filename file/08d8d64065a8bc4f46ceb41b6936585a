    <script type="text/javascript">
        $(function () {            
            var where = IsDetailPage();
            if (where != "") {
                $("#kefu_yudi").show();
            }
            else {
                $("#kefu_yudi").hide();
            }

            //拉动滚动条显示top
            $(window).scroll(function () {
                if ($(document).scrollTop() >= 200) {
                    $(".kefu_top").show();
                } else {
                    $(".kefu_top").hide();
                }
            });
            //点击top返回顶部
            $(".kefu_top").click(function () {
                $("html,body").animate({ scrollTop: 0 }, 500)
            });
            //点击购物车返回预定位置
            $("#kefu_yudi").click(function () {
                //$("html,body").animate({ scrollTop: 200 }, 500)  //scrollTop值根据不同详细页预订按钮位置设置大小  签证：200，自由行：740，单项：690，周边门票：600
                var kkk = "asdf";
                var where = IsDetailPage();

                if (where != "") {
                    $('body,html').animate({
                        scrollTop: $(where).offset().top - 5
                    }, 400);
                }
            });
            //滑过服务热线显示电话号码
            $("#kefu_img02").mouseover(function () {
                $("#kefu_img02_tel").show();
                $("#kefu_img02_tel").stop().animate({ right: "72px" }, 200)
            });
            $("#kefu_img02").mouseout(function () {
                $("#kefu_img02_tel").stop().animate({ right: "90px" }, 200, function () { $("#kefu_img02_tel").hide() })

            });
            //滑过每个块改变底色
            $(".kefu_img1").mouseover(function () {
                $(this).addClass("kefu_img1_bg2");
            });
            $(".kefu_img1").mouseout(function () {
                $(this).removeClass("kefu_img1_bg2");
            });
        });
        //是否是产品详细页面
        function IsDetailPage() {
            var where = "";

            var href = window.location.href;
            if (href.indexOf('from') > -1) {
                return "";
            }
            //单项详细页
            if (href.indexOf("/dx/detail.aspx") > -1) {
                where = "#order_table";
            }

            //门票
            if (href.indexOf("/ticket/mpDetail.aspx") > -1) {
                where = ".mau.mp_yd.oh";
            }

            //签证
            if (href.indexOf("/visa/detail.aspx") > -1) {
                where = ".one.mau";
            }

            //2次确认自由行
            if (href.indexOf("/freedom/abroadDetail2") > -1 || href.indexOf("/freedom/abroadDetail1") > -1 || href.indexOf("/freedom/chinaDetail2") > -1 || href.indexOf("/freedom/chinaDetail1") > -1) {
                where = ".bor1.bc_white.mt20.w1000.oh";
            }

            return where;
        }
    </script>


