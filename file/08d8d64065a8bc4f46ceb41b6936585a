    <script type="text/javascript">
        $(document).ready(function () {
            var txt = $("#div_travels").html();
            if (txt == "" || txt == null || txt.length < 30) {
                //  $("#div_hottravel").fadeOut();
            }
        });

        //关闭底层方法
        function closebottom() {
            var width = $('#div_bottom_web').width();
            //关闭底层
            $('#div_bottom_web').animate({ left: "-=" + width + "px" }, 1000, 'swing', function () {
                //显示小底层
                var divwidth = $('#div_closeover').width();
                $('#div_closeover').animate({ right: "+=200px" }, 1000);

            });
        }

        //显示底层方法
        function showbottom() {
            //关闭小底层
            var divwidth = $('#div_closeover').width();
            $('#div_closeover').animate({ right: "-=200px" }, 500, "swing", function () {
                //显示底层
                var width = $('#div_bottom_web').width();
                $('#div_bottom_web').animate({ left: "+=" + width + "px" }, 800, "swing");
            });
        }







        //添加收藏夹
        function addFavorite() {
            var url = window.location;
            var title = document.title;
            var ua = navigator.userAgent.toLowerCase();
            if (ua.indexOf("360se") > -1) {
                alert("由于360浏览器功能限制，请按 Ctrl+D 手动收藏！");
            }
            else if (ua.indexOf("msie 8") > -1) {
                window.external.AddToFavoritesBar(url, title); //IE8
            }
            else if (document.all) {
                try {
                    window.external.addFavorite(url, title);
                } catch (e) {
                    alert('您的浏览器不支持,请按 Ctrl+D 手动收藏!');
                }
            }
            else if (window.sidebar) {
                window.sidebar.addPanel(title, url, "");
            }
            else {
                alert('您的浏览器不支持,请按 Ctrl+D 手动收藏!');
            }
        }


        $(function () {
            $('#div_closeover').animate({ right: "-=200px" }, 1);
        });






    </script>
    
    
    
    <script type="text/javascript">

        $(function () {
            getMonth();
            $(".lxtDXTitle").mouseover(function () {
                $(".lxtDXTitle").attr("class", "fl h40 ft12 w100 tc lxtDXTitle");
                $(this).attr("class", "fl h40 ft12 nlp kw w100 tc lxtDXTitle");

                var i = $(".lxtDXTitle").index(this);
                $(".lxtDXMore").hide();
                $(".lxtDXMore").eq(i).show();

                $(".lxtDXContent").hide();
                $(".lxtDXContent").eq(i).show();
            });
            $("#searchTitle div").click(function () {
                $("#searchTitle div").attr("class", "bc_ec bor2 c");
                $(this).attr("class", "bc_white bor4 c");

                var i = $("#searchTitle div").index(this);
                $("form").hide();
                $("form").eq(i).show();
            });
        });



        //月度推荐
        function getMonth() {
            var myDate = new Date();
            var i = myDate.getMonth();
            $("#month div").eq(i).attr("class", "vl1");
            getMonthList(i + 1);
        }


        function getMonthList(i) {
            $("#month div").attr("class", "vb1");
            $("#month div").eq(i - 1).attr("class", "vl1");
            $.ajax({
                url: "/guide/newguide.ashx",
                data: { Method: "getMonthList", Month: i },
                type: "POST",
                dataType: "json",
                async: true,
                error: function () { },
                success: function (json) {
                    var text = "";
                    if (json.List.length > 0) {
                        text += '<div id="content2" class="contentmoveon">';
                        text += ' <div class=" w1240 mau oh">';
                        $.each(json.List, function (i, item) {
                            //.item
                            //.item:hover

                            if (i == 0) {

                                text += '<div class="w490 fl pr mt40 item">';
                                text += '<a href="/guide/guidecity.aspx?cityid=' + item.ID + '" target="_blank">';
                                text += '<div class=" tc"><img height="320px" width="490px" src="' + item.path + '" onerror="this.src=\'/image/nopic.jpg\';"/></div>';
                                text += '<div class="pa ft20 w490  h40 tc bc_black fa60" style="left:0px; top:280px;"></div>';
                                text += '<div class="pa ft20 w490 tc color_white" style="left:0px; top:280px;">' + item.GuideName + '</div>';
                                text += '</a>';
                                text += '</div> ';


                            }
                            else {

                                //新的CSS是竖格分分配 每个竖格两个 

                                if (i % 2 != 0) {
                                    text += '<div class="w232 fl pr ml18 mt40">';
                                    text += ' <a href="/guide/guidecity.aspx?cityid=' + item.ID + '" target="_blank">';
                                    text += ' <div class="item">';
                                    text += '<div><img width="233px" height="155px" src="' + item.path + '" /></div>';
                                    text += '<div class="pa ft20 w232  h40 tc bc_black fa60" style="left:0px; top:115px;"></div>';
                                    text += '<div class="pa ft20 w232 tc color_white" style="left:0px; top:115px;">' + item.GuideName + '</div>';
                                    text += '</div>';
                                    text += '</a>';
                                }
                                else {


                                    text += '<a href="/guide/guidecity.aspx?cityid=' + item.ID + '" target="_blank">';
                                    text += ' <div class="item mt10">';
                                    text += ' <div><img  width="233px" height="155px" src="' + item.path + '"  /></div>';
                                    text += '<div class="pa ft20 w232  h40 tc bc_black fa60" style="left:0px; top:115px;"></div>';
                                    text += ' <div class="pa ft20 w232 tc color_white" style="left:0px; top:115px;">' + item.GuideName + '</div>';
                                    text += '</div>';
                                    text += '</a>';
                                    text += '</div>';
                                }

                            }

                        });
                        text += '</div></div>'; //最终加的div结束
                    }
                    else {
                        text = "";
                    }

                    $("#monthList").hide();
                    $("#monthList").html(text);
                    $("#monthList").fadeIn(1000);
                }
            });
        }
        //搜索
        function addListener1(element, e, fn) {
            if (element.addEventListener) {
                element.addEventListener(e, fn, false);
            } else {
                element.attachEvent("on" + e, fn);
            }
        }

        addListener1(document, "click", function (evt) {
            var evt = window.event ? window.event : evt,
            target = evt.srcElement || evt.target;
            if (isDefine(target.id)) {
                //境外自由行
                if (target.id.indexOf("cjzyxCFD") < 0) {
                    $("#cjzyxCFDDiv").hide();
                }
                else {
                    $("#cjzyxCFDDiv").show();
                }
                if (target.id.indexOf("cjzyxMDD") < 0) {
                    $("#cjzyxMDDDiv").hide();
                }
                else {
                    $("#cjzyxMDDDiv").show();
                }
                if (target.id.indexOf("cjzyxTS") < 0) {
                    $("#cjzyxTSDiv").hide();
                }
                else {
                    $("#cjzyxTSDiv").show();
                }
                if (target.id.indexOf("cjzyxJG") < 0) {
                    $("#cjzyxJGDiv").hide();
                }
                else {
                    $("#cjzyxJGDiv").show();
                }
                //境内自由行
                if (target.id.indexOf("gnzyxCFD") < 0) {
                    $("#gnzyxCFDDiv").hide();
                }
                else {
                    $("#gnzyxCFDDiv").show();
                }
                if (target.id.indexOf("gnzyxMDD") < 0) {
                    $("#gnzyxMDDDiv").hide();
                }
                else {
                    $("#gnzyxMDDDiv").show();
                }
                if (target.id.indexOf("gnzyxTS") < 0) {
                    $("#gnzyxTSDiv").hide();
                }
                else {
                    $("#gnzyxTSDiv").show();
                }
                if (target.id.indexOf("gnzyxJG") < 0) {
                    $("#gnzyxJGDiv").hide();
                }
                else {
                    $("#gnzyxJGDiv").show();
                }
                //签证
                if (target.id.indexOf("visaMDD") < 0) {
                    $("#visaMDDDiv").hide();
                }
                else {
                    $("#visaMDDDiv").show();
                }
                if (target.id.indexOf("visaType") < 0) {
                    $("#visaTypeDiv").hide();
                }
                else {
                    $("#visaTypeDiv").show();
                }
                //当地玩乐
                if (target.id.indexOf("dxType") < 0) {
                    $("#dxTypeDiv").hide();
                }
                else {
                    $("#dxTypeDiv").show();
                }
                if (target.id.indexOf("dxCity") < 0) {
                    $("#dxCityDiv").hide();
                }
                else {
                    $("#dxCityDiv").show();
                }

                if (target.id.indexOf("txt_mdd") < 0) {
                    $("#ZBdestination").hide();
                }
                else {
                    $("#ZBdestination").show();
                }


                if (target.id.indexOf("txt_type") < 0) {
                    $("#ZBtype").hide();
                }
                else {
                    $("#ZBtype").show();
                }

            }
            else {
                $("#cjzyxCFDDiv").hide();
                $("#cjzyxMDDDiv").hide();
                $("#cjzyxTSDiv").hide();
                $("#cjzyxJGDiv").hide();
                $("#gnzyxCFDDiv").hide();
                $("#gnzyxMDDDiv").hide();
                $("#gnzyxTSDiv").hide();
                $("#gnzyxJGDiv").hide();
                $("#visaMDDDiv").hide();
                $("#visaTypeDiv").hide();
                $("#dxTypeDiv").hide();
                $("#dxCityDiv").hide();
                $("#ZBdestination").hide();
                $("#ZBType").hide();
            }
        });
        function isDefine(value) {
            if (value == null || value == "" || value == "undefined" || value == undefined || value == "null" || value == "(null)" || value == 'NULL' || typeof (value) == 'undefined') {
                return false;
            }
            else {
                value = value + "";
                value = value.replace(/\s/g, "");
                if (value == "") {
                    return false;
                }
                return true;
            }
        }
        function cityChange(id, city) {
            $("#cjzyxMDDFarther div").attr("class", "fl w50 tc");
            $(id).parent().attr("class", "fl w50 tc kw nlp");
            $("#cjzyxMDDChild").html("");
            if (isDefine(city)) {
                $.ajax({
                    url: "/freedom/detail.ashx",
                    data: { Method: "getFreeCity", ID: city },
                    type: "POST",
                    dataType: "json",
                    async: true,
                    error: function () { },
                    success: function (json) {
                        var text = "";
                        if (json.List.length > 0) {
                            $.each(json.List, function (i, item) {
                                text += "<div class=\"fl w70 tc\"><a href=\"javascript:void(0);\" onclick=\"$('#cjzyxMDD').val('" + item.name + "');$('#cjzyxD').val('" + item.key + "');\">" + item.name + "</a></div>";
                            });
                        }
                        $("#cjzyxMDDChild").html(text);
                    }
                });
            }
            else {
                $("#cjzyxMDDChild").html("<div class=\"fl w70 tc\"><a href=\"javascript:void(0);\" onclick=\"$('#cjzyxMDD').val('全部');$('#cjzyxD').val('');\">全部</a></div>");
            }
        }
