     <!--搜索脚本-->
      <script type="text/javascript">
        function search() {
            var val = $.trim($("#input_search").val());
            var _id = /^([1-4])\d+$/;
            if (val.length == 0) {
                alert("请输入搜索目的地关键字");
            }
            else if (val.length > 30) {
                alert("搜索内容必须小于30个字");
            }
            else if (_id.test(val)) {
                var i = val.substr(0, 1);
                var url = "/error.aspx";
                switch (i) {
                    case "1":
                        url = "/freedom/detail.aspx?ID=" + val.substr(1); break;
                    case "2":
                        url = "/freedom/detail2.aspx?ID=" + val.substr(1); break;
                    case "3":
                        url = "/visa/detail.aspx?ID=" + val.substr(1); break;
                    case "4":
                        url = "/dx/detail.aspx?ID=" + val.substr(1); break;
                }
                location.href = url;
            }
            else {
                location.href = '/search.aspx?key=' + encodeURI(val);
            }
        }

        function entersearch() {
            var event = window.event || arguments.callee.caller.arguments[0];
            if (event.keyCode == 13)
            {
                search();
            }
        }



        
        function shuaxin() {
       
            var text = $("#input_search").val();
            var ss = $("#hidBehaviors");
            var hid = $("#hidBehaviors").val();


            if (text == hid || hid == "" || text == "请输入搜索目的地关键字" || text == null || text == "") {
                $("#xs").html("");
            } else {
                $("#hidBehaviors").val(text);

                var url = '/Ajax/Search.aspx?r=' + Math.random();
                $.ajax({
                    url: url,
                    cache: true,
                    type: 'GET',
                    dataType: 'html',
                    data: {
                        Gett: "XCPList",
                        d: $("#input_search").val()

                    },
                    error: function (xhr) {  },
                    success: function (response) {
                    
                   
                        $("#xs").html("");
                        //声明字符串
                        var text = "";
                        var name = $("#input_search").val();
                        if (response != "") {
                            var json = eval("(" + response + ")");

                            //清空div

                           

                            for (j = 0; j < json.length; j++) {
                                if (json[j].Type == "自由行") {
                                    text += '<a href="../search.aspx?type=2&key=' + name + '"><div class="w300 h30 ml oh"><div class="fl ml10 w90 oh nlp ft11">自由行</div><div class="fr w80 oh fb">约' + json[j].Num + '个结果</div></div></a>'
                                    for (z = 0; z < json[j].rows.length; z++) {
                                        text += '<div class="w300 h30 ml oh"><div class="fl ml20 w300 oh"><a href="/freedom/detail.aspx?ID=' + json[j].rows[z].ID + '">' + json[j].rows[z].Title + '</a></div></div>';
                                    }
                                }
                                if (json[j].Type == "单项") {
                                    text += '<a href="../search.aspx?type=3&key=' + name + '"><div class="w300 h30 ml oh"><div class="fl ml10 w90 oh nlp ft11">单项</div><div class="fr w80 oh fb">约' + json[j].Num + '个结果</div></div></a>'
                                    for (z = 0; z < json[j].rows.length; z++) {
                                        text += '<div class="w300 h30 ml oh"><div class="fl ml20 w300 oh"><a href="../dx/detail.aspx?ID=' + json[j].rows[z].ID + '">' + json[j].rows[z].Title + '</a></div></div>';
                                    }
                                }
                                if (json[j].Type == "签证") {
                                    text += '<a href="../search.aspx?type=1&key=' + name + '"><div class="w300 h30 ml oh"><div class="fl ml10 w90 oh nlp ft11">签证</div><div class="fr w80 oh fb">约' + json[j].Num + '个结果</div></div></a>'
                                    for (z = 0; z < json[j].rows.length; z++) {
                                        text += '<div class="w300 h30 ml oh"><div class="fl ml20 w300 oh"><a href="../visa/detail.aspx?ID=' + json[j].rows[z].ID + '">' + json[j].rows[z].Title + '</a></div></div>';
                                    }
                                }

                                if (json[j].Type == "周边游") {
                                    text += '<a href="../search.aspx?type=4&key=' + name + '"><div class="w300 h30 ml oh"><div class="fl ml10 w90 oh nlp ft11">周边游</div><div class="fr w80 oh fb">约' + json[j].Num + '个结果</div></div></a>'
                                    for (z = 0; z < json[j].rows.length; z++) {
                                        text += '<div class="w300 h30 ml oh"><div class="fl ml20 w300 oh"><a href="../ticket/mpDetail.aspx?ID=' + json[j].rows[z].ID + '">' + json[j].rows[z].Title + '</a></div></div>';
                                    }
                                }



                            }
                        }

                        $("#xs").html(text);
                        $("#xs").show();
                    }

                });

            }


        }
        


       
    </script> 




    <script type="text/jscript">
        $(function () {
            //境外
            $("#tjw,#tjw1").mouseover(function () {
                $("#tjw1").show();
                $("#tjw").addClass("t_color");
                $("#tjw").children("span").html("▲");
            });
            $("#tjw,#tjw1").mouseout(function () {
                $("#tjw1").hide();
                $("#tjw").removeClass("t_color");
                $("#tjw").children("span").html("▼");
            });
            //境内
            $("#tjn,#tjn1").mouseover(function () {
                $("#tjn1").show();
                $("#tjn").addClass("t_color");
                $("#tjn").children("span").html("▲");
            });
            $("#tjn,#tjn1").mouseout(function () {
                $("#tjn1").hide();
                $("#tjn").removeClass("t_color");
                $("#tjn").children("span").html("▼");
            });
            //甜程蜜旅
            $("#tml,#tml1").mouseover(function () {
                $("#tml1").show();
                $("#tml").addClass("t_colorml");
                $("#tml").children("span").html("▲");
            });
            $("#tml,#tml1").mouseout(function () {
                $("#tml1").hide();
                $("#tml").removeClass("t_colorml");
                $("#tml").children("span").html("▼");
            });
            //游记
            $("#tyj,#tyj1").mouseover(function () {
                $("#tyj1").show();
                $("#tyj").addClass("t_color");
                $("#tyj").children("span").html("▲");
            });
            $("#tyj,#tyj1").mouseout(function () {
                $("#tyj1").hide();
                $("#tyj").removeClass("t_color");
                $("#tyj").children("span").html("▼");
            });

            //全球目的地
            $(".world_text,.world_nav1,#world_navt,.world_ejz,.world_ejzk").mouseover(function () {
               // $(".world_nav1").stop().slideDown(200);
               // $("#world_navt").stop().slideDown(200);
            });
            $(".world_text,.world_nav1,#world_navt,.world_ejz,.world_ejzk").mouseout(function () {
               // $(".world_nav1").stop().slideUp(200);
              //  $("#world_navt").stop().slideUp(200);
            });


            //目的地列表
            $(".world_ej").mouseover(function () {
                $(this).find(".tlco>a").addClass("co_yel");
                $(this).find(".world_ejleft").stop().animate({ marginLeft: '1px' });
                $(".world_ejz").show();
                $(".world_ejzk>div").eq($(".world_ej").index(this)).show().siblings().hide();

            });
            $(".world_ej").mouseout(function () {
                $(this).find(".tlco>a").removeClass("co_yel");
                $(this).find(".world_ejleft").stop().animate({ marginLeft: '10px' });
                $(".world_ejz").hide();
                $(".world_ejzk>div").hide();
            });
            $(".world_ejzk>div").mouseover(function () {
                $(".world_ejleft").eq($(".world_ejzk>div").index(this)).find(".tlco>a").addClass("co_yel");
                $(this).show();
                $(".world_ejz").show();
                $(".world_ejleft").eq($(".world_ejzk>div").index(this)).stop().animate({ marginLeft: '1px' });
            });
            $(".world_ejzk>div").mouseout(function () {
                $(".world_ejleft").eq($(".world_ejzk>div").index(this)).find(".tlco>a").removeClass("co_yel");
                $(this).hide();
                $(".world_ejz").hide();
                $(".world_ejleft").eq($(".world_ejzk>div").index(this)).stop().animate({ marginLeft: '10px' });
            });

            $(".world_ej").eq(6).find(".bor1_b").hide();





        });
    </script>
    
    













