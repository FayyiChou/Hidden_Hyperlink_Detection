    $(document).scroll(function(){
        if( $(document).scrollTop() > 200){
            $("#js-go-top").css('display','block');
        }else{
            $("#js-go-top").css('display','none');
        }
    });


    $("#js-go-top").click(function(){
        $('html,body').animate({scrollTop: '0px'}, 800);
    });
