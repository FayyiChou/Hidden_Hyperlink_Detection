	<script>
			function pubu(){

				$(window).scroll(function(){
					if(getDataCheck() && dataOff){
						var box=$("#ne_news").find("ul");
						var newsid = $("#newsid").val();
						$(".loading").show();
						dataOff=false;
						$.ajax({
							url:'/ajax/car/get_news_list_ordid.php',
							data:'newsid='+newsid+'&type=xinwen&callback=json&num=10',
							success:function(data){
								var data = eval('('+data+')');
								var nids = new Array();
								if(data){
									for(var m in data){
										box.append("<li><a href='/news/"+data[m].id+".html'><img src='http://icon.cheshi.com/picloading.gif' data-original='"+data[m].title_pic1+"'><h4>"+data[m].title+"</h4><span class='author'>作者："+data[m].author+"</span><span class='time'>"+data[m].story_date+"</span></a></li>")
										nids[m] = data[m].id;
									}
									nids.sort();
									//console.log(nids[0]);
									$(".loading").hide();
									var n = data.length
									$("#newsid").attr("value",nids[0]);
									if(n >= 10){
										dataOff=true;
									}
									$(".ne_word img").lazyload({effect : "fadeIn"});
								}
								
							}

						});
					}
				})
			}

		pubu();

		//判断请求数据的开关
		var dataOff = true;
		function getDataCheck(){
			var pubu = $("#ne_news");
			var box = pubu.find("li");
			var obj = $(box[box.length-1])[0];
			var objheight  = (obj.offsetHeight);
			var lastboxHeight =  Math.floor( $(box[box.length-1]).offset().top+objheight ) ;
			var documentHeight = $(window).height();
			var scrollTop = $(document).scrollTop();
			return lastboxHeight<documentHeight+scrollTop?true:false;
			
		}
	</script>
	
    
