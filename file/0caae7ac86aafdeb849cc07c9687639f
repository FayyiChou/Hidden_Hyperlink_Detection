  <script language="javascript">
<!--Baidu Search-->
function setspanbg(E)
{
$("#searchfrm span").removeClass("homeSearchspanBG");
$("#searchfrm span").addClass("homeSearchspan");
$(E).addClass("homeSearchspanBG");
}
function gowhere0()
{
String.prototype.Trim = function(){return this.replace(/(^\s*)|(\s*$)/g, "");} 
String.prototype.LTrim = function(){return this.replace(/(^\s*)/g, "");} 
String.prototype.RTrim = function(){return this.replace(/(\s*$)/g, "");}

var keywordStr=document.searchfrm.keyword.value.Trim();
if(keywordStr==""){alert("请输入查询关键字");document.searchfrm.keyword.focus();return false};
if(keywordStr.length<2){alert("请输入2个以上查询关键字");document.searchfrm.keyword.focus();return false};
return true;
}
function gowhere1()
{
var url;
var keywordStr=document.searchfrm.keyword.value;
if(!gowhere0()){return false};
if (document.searchfrm.myselectvalue.value == "0")
url = "http://www.baidu.com/baidu?tn=baidu&word="+keywordStr;
if (document.searchfrm.myselectvalue.value == "4")
url = "http://news.baidu.com/ns?tn=news&cl=2&rn=20&word="+keywordStr;
if (document.searchfrm.myselectvalue.value == "5")
url = "http://image.baidu.com/i?lm=-1&cl=2&tn=baiduiamge&ct=201326592&word="+keywordStr;
if (document.searchfrm.myselectvalue.value == "2")
url = "http://video.baidu.com/v?ct=301989888&rn=20&word="+keywordStr;
window.open(url);
}
</script> 
