  <script type="text/javascript">
function b(){
if($(window).scrollTop() >= 100){
$('#goTop').fadeIn(400);
} else {
$('#goTop').fadeOut(200);
}
}
$(document).ready(function(e) {
$('#goTop').hide();
b();
$('#gotop').click(function(){
$(document).scrollTop(0);	
})	
$('.goTop').click(function(){
$('html,body').animate({scrollTop : '0px'}, 100);
});
});
$(window).scroll(function(e){
b();		
})
</script> 
