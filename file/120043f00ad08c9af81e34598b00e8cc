                  <script type="text/javascript">
            var activeM = 1;
            var activeMContent = 1;
            var M;
            for (M = 2; M <= 4; M++) {
                document.getElementById("Mcontent"+M).style.display = "none";
            }
            function showMContent(fg) {
                document.getElementById("Mcontent" + activeMContent).style.display = "none";
                document.getElementById("Mcontent" + fg).style.display = "";
                activeMContent = fg;
            }
            function showM(fg) {
                document.getElementById("M" + activeM).className = "MNormal";
                document.getElementById("M" + fg).className = "MActive";
                activeM = fg;
                showMContent(fg);
            }
            </script>
