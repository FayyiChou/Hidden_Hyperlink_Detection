            <script language="javascript">
function formcheck()
{
 
 if (document.shiting.member_name.value == "")
    {
    alert("请填姓名！");
    document.shiting.member_name.focus();
    return (false);
    }
 if (document.shiting.member_mobile.value == "")
    {
    alert("请填写手机号码！");
    document.shiting.member_mobile.focus();
    return (false);
    }	
if (document.shiting.member_mobile.value.length < 11)
  {
    alert("请输入正确的手机号码！");
	 document.shiting.member_mobile.focus();
    return false;
  }
 if (document.shiting.member_code.value == "")
    {
    alert("请填写试听课程！");
    document.shiting.member_code.focus();
    return (false);
    }	  
 if (document.shiting.member_page.value == "")
    {
    alert("请填写学习目的！");
    document.shiting.member_page.focus();
    return (false);
    }	
 return true;
}
</script>
