            var activeTContent = 1;
            var T;
            for (T = 2; T <= 4; T++) {
                document.getElementById("Tcontent"+T).style.display = "none";
            }
            function showTContent(mt) {
                document.getElementById("Tcontent" + activeTContent).style.display = "none";
                document.getElementById("Tcontent" + mt).style.display = "";
                activeTContent = mt;
            }
            function showT(mt) {
                document.getElementById("T" + activeT).className = "TNormal";
                document.getElementById("T" + mt).className = "TActive";
                activeT = mt;
                showTContent(mt);
            }
