            <script type="text/javascript">
window.onload=function()
{
var oNav=document.getElementById("nav");
var oDl=oNav.getElementsByTagName('dl');
var i=0;
var timer=null;
for(i=0;i<oDl.length;i++)
if(oDl[i].className=='one'){
oDl[i].onmouseover=function(){
var odd=this.getElementsByTagName('dd')[0];
var aa=this.getElementsByTagName('a')[0];
aa.className='hover';
odd.style.display='block';
}

oDl[i].onmouseout=function (){
var odd=this.getElementsByTagName('dd')[0];
var aa=this.getElementsByTagName('a')[0];
aa.className='';
odd.style.display='none';
}
}
}
</script>
            <script type="text/javascript">
            var activeG = 1;
            var activeGContent = 1;
            var G;
            for (G = 2; G <= 8; G++) {
                document.getElementById("Gcontent"+G).style.display = "none";
            }
            function showGContent(dh) {
                document.getElementById("Gcontent" + activeGContent).style.display = "none";
                document.getElementById("Gcontent" + dh).style.display = "";
                activeGContent1 = dh;
            }
            function showG(dh) {
                document.getElementById("G" + activeG).className = "GNormal";
                document.getElementById("G" + dh).className = "GActive";
                activeG = dh;
                showGContent(dh);
            }
            </script>
