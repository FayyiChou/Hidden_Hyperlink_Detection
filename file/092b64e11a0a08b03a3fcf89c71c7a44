  $('#remove-bulletin').on('click',function(){
    $.ajax({
      type: "POST",
      url: "/remove_bulletin",
      data: {'remove_bulletin':'no_invite_ad'},
      success: function(data) {
        $('#git-bulletin').hide();
      }
    });
  });
</script>
