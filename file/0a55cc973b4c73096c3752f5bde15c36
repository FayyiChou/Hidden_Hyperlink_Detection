    type2date = "",
    type3date = "",
    type4date = "";
var page = 1,
    type0page = 1,
    type1page = 1,
    type2page = 1,
    type3page = 1,
    type4page = 1;
var type = 1;
if (type) {
    type = "";
};

$(function (){
    update.init = {
        showType:0,
        rowNumber:7,
        outerWidth:132,
        outerHeight:207,
        tagStop:false,
        continueCurrent:false,
        listIsAnimated : true,
        oTag:$(".animLoad_box"),
        oParent:$("#update_content"),
        listClassName:"left_update",
        titleClassName:"update_tit",
        tagMarginTop:30,
        typeHtml0:"",
        typeHtml1:"",
        typeHtml2:"",
        typeHtml3:"",
        typeHtml4:""
    }
    var w = getTotalWidth();
    if (w < 1280) {
        update.init.rowNumber = 5;
        update.init.outerWidth = 148;
    }
    update.initStyle();
    update.initTagTop();
    $(".animLoad_box").quickFlip();
    var rightTop = $(".right_update").offset().top;
    var isLoad = false;
    var loadnum = 0;
    loadbook(function(){
        $(".categray_list").find('i').hide();
        if ($('.comicList li').length <= 14) {
            loadbook(function(){$(".categray_list").find('i').hide();});
        };
    });
    $(window).scroll(function (){
        var scrollTop = $(document).scrollTop();
        var docHeight = $(document).height();
        var screenHeight = $(window).height();
        if(!isLoad && update.init.listIsAnimated){
            if(scrollTop > docHeight - screenHeight - 10){
                loadbook(function(){$(".categray_list").find('i').hide();});
            }
        }
        if(scrollTop+47 >= rightTop){
            $(".right_update").addClass("fixed")
            if (w < 1280) {
                $('.right_update').css('margin-left','243px');
            };
        }else{
            if (w < 1280) {
                $('.right_update').css('margin-left','0px');
            };
            $(".right_update").removeClass("fixed")
        }
    })

    function loadbook(callback){
        $(".categray_list .current i").show();
        isLoad = true;
        //在此处加载数据
        switch(update.init.showType){
            case 0 :
                page = type0page;
                date = type0date;
            break;
            case 1 :
                page = type1page;
                date = type1date;
            break;
            case 2 :
                page = type2page;
                date = type2date;
            break;
            case 3 :
                page = type3page;
                date = type3date;
            break;
            case 4 :
                page = type4page;
                date = type4date;
            break;
        }
        $.ajax({
            url:"http://comic.u17.com/comic/ajax.php?mod=comic&act=get_choiceness_comic&type="+type+"&page="+page+"&date="+date+"&status="+ update.init.showType,
            type: "get",
            dataType: 'json',
            success: function(data){

                if (data.code>0) {
                    page = data.page;
                    date = data.date;
                    update.init.tagStop = true;
                    var tempHtml = "";
                    if(!data.status){
                        update.init.continueCurrent = false;
                        tempHtml += "<div class='left_update cf' show='0'>"
                        tempHtml += "<div class='update_tit'>"
                        tempHtml += "<em>"+data.date+"</em><span>更新</span>"
                        tempHtml += "</div>"
                        tempHtml += "<ul class='comicList cf'>"
                        tempHtml += data.data;
                        tempHtml += "</ul>"
                        tempHtml += "</div>"

                        update.init.continueCurrent = data.status;

                        $("#update_content").append($(tempHtml));
    
                    }else{
      
                        update.init.continueCurrent = true;
                        $(".left_update").last().children(".comicList").append($(data.data));
                    }


                    isLoad = false;

                    update.loadNext(callback);
                    switch(update.init.showType){
                        case 0 :
                            type0page = data.page;
                            type0date = data.date;
                        break;
                        case 1 :
                            type1page = data.page;
                            type1date = data.date;
                        break;
                        case 2 :
                            type2page = data.page;
                            type2date = data.date;
                        break;
                        case 3 :
                            type3page = data.page;
                            type3date = data.date;
                        break;
                        case 4 :
                            type4page = data.page;
                            type4date = data.date;
                        break;
                    }
                };
            }
        });
    }

    $(".categray_list a").each(function(index, element) {
        $(this).click(function (){

            if(update.init.listIsAnimated){
                var _this = $(this);
                _this.children("i").show();
                update.init["typeHtml"+update.init.showType] = $("#update_content").html();
                if(index == 4){
                    update.init.showType = 0;
                }else{
                    update.init.showType = index + 1;
                }
                $(this).addClass("current").siblings("a").removeClass("current")
                update.switchoverLi(function(){
                    if(update.init["typeHtml"+update.init.showType] == ""){
                        loadbook(function(){
                            $(".categray_list").find('i').hide();
                            if ($('.comicList li').length <= 14) {
                                loadbook(function(){$(".categray_list").find('i').hide();});
                            };
                        });
                    }
                });
            }
        })
    });
})

var update = {
    init:{
        showType:0,                    //显示的类型
        rowNumber:0,                   //每行的个数
        outerWidth:0,                  //单块的宽度
        outerHeight:0,                 //单块的高度
        tagStop:false,
        continueCurrent:false,          //是否继续在当前日期加载
        listIsAnimated : true,
        oTag:null,
        oParent:null,                 //加载卡片是否停止（数据是否加载完成）true代表加载完成，可以停止
        listClassName:"",              //按日期分块的外层元素的class
        titleClassName:"",             //按日期分块的时间标题元素的class
        tagMarginTop:60,
        typeHtml0:"",
        typeHtml1:"",
        typeHtml2:"",
        typeHtml3:"",
        typeHtml4:""
    },
    initStyle :　function (){
        $("."+this.init.listClassName+"[show=1]").each(function(index, element) {
            if(update.init.showType == 0 ){
                $(this).find("li").each(function(index, element) {
                    var pos = update.getLiPosition(index);
                    $(this).css({"left":pos.left,"top":pos.top});
                    $(this).show();
                });
                $(this).show();
            }else{
                $(this).find("[showType=" + update.init.showType + "]").each(function(index, element) {
                    var pos = update.getLiPosition(index);
                    $(this).css({"left":pos.left,"top":pos.top});
                    $(this).show();
                });
                $(this).show();
            }
        });
    },
    initTagTop : function (oTag){
        var row;
        row = Math.ceil($("." + update.init.listClassName).eq(0).find("[showType]").length/update.init.rowNumber);
        var titleHeight = $("."+update.init.titleClassName).outerHeight();
        update.init.oTag.css("top",row*update.init.outerHeight + titleHeight + update.init.tagMarginTop)
    },
    tagAnimeTimer:null,
    //单块计算坐标
    //需要计算位置的li对象和他的序号
    getLiPosition:function (index){
        var cTop = Math.floor(index/this.init.rowNumber)*this.init.outerHeight;
        var cLeft = (index%this.init.rowNumber)*this.init.outerWidth;
        return {left:cLeft,top:cTop}
    },
    getListPosition:function (oList){
        //需要获取top的list的top值等于之前所有list里title的个数*title高 + 这里所有显示元素的高*行数
        var pIndex = $(oList).prevAll("[show=1]").length;
        var titleTotalHeight = pIndex*$("."+update.init.titleClassName).outerHeight();
        var liTotalRow = 0;
        for(var i = 0 ; i < pIndex ; i++ ){
            var row = Math.ceil($("." + update.init.listClassName+"[show=1]").eq(i).find("[showType]").length/this.init.rowNumber);
            liTotalRow += row;
        }
        return liTotalRow*update.init.outerHeight + titleTotalHeight;
    },
    loadNext : function (callback){
        if(update.init.continueCurrent){
            update.init.listIsAnimated = false;
            var nowObj = $("."+update.init.listClassName).last();
            nowObj.show();
            this.tagAnimate(function (){
                var row = 0;
                row = Math.ceil(nowObj.find("li").size()/update.init.rowNumber);
                var liTop = row*update.init.outerHeight + update.init.tagMarginTop;
                nowObj.find("li[displayState=0]").css({"top":liTop,"left":"354px"});
                update.sortLi(nowObj,callback);
            });
        }else{
            update.init.listIsAnimated = false;
            var nowObj = $("."+update.init.listClassName+"[show=0]").last().attr("show",1);
            this.tagAnimate(function (){
                nowObj.css("top",update.getListPosition(nowObj));
                nowObj.show();
                var row = 0;
                row = Math.ceil(nowObj.find("li").size()/update.init.rowNumber);
                var liTop = row*update.init.outerHeight + update.init.tagMarginTop;
                nowObj.find("li[showType]").css({"top":liTop,"left":"354px"});
                update.sortLi(nowObj,callback);

            });
        }

    },
    sortLi : function (aLis,callback){
        var aLiIndex = 0;
        if(update.init.continueCurrent){
            aLiIndex  = aLis.find("li").length - aLis.find("[displayState='0']").length;
        }

        var aLisLength =aLis.find("li[showType]").length;
        aLis.timer = setInterval(function (){
            var pos = update.getLiPosition(aLiIndex);
            aLis.find("[showType]").eq(aLiIndex).show().animate({left:pos.left,top:pos.top},300,function(){
                //改变displayState的值
                $(this).attr("displayState","1");
            });

            if(aLiIndex >= aLisLength-1){
                clearInterval(aLis.timer);
                update.init.listIsAnimated = true;
                if(callback){
                    callback();
                }
            }
            aLiIndex++;
        },100)

    },
    //切换显示类型的时候排序
    switchoverLi : function (callback){
            var speed1 = 500;
            var speed2 = 500;
            if(callback){
                speed2 = 00;
            }
            update.init.listIsAnimated = false;
            var row;
            $(update.init.oParent).fadeOut(speed1,function(){
                $("#update_content").html(update.init["typeHtml"+update.showType])
                $("[showType]").fadeOut();
                switch(update.init.showType){
                    case 0:
                        $("#update_content").html(update.init.typeHtml0);
                    break;
                    case 1:
                        $("#update_content").html(update.init.typeHtml1);
                    break;
                    case 2:
                        $("#update_content").html(update.init.typeHtml2);
                    break;
                    case 3:
                        $("#update_content").html(update.init.typeHtml3);
                    break;
                    case 4:
                        $("#update_content").html(update.init.typeHtml4);
                    break;
                }



                var pIndex = $(update.init.oParent).children("."+update.init.listClassName + "[show=1]").length;
                var titleTotalHeight = pIndex*$("."+update.init.titleClassName).outerHeight();
                var liTotalRow = 0;

                for(var i = 0 ; i < pIndex ; i++ ){
                    var row = Math.ceil($("." + update.init.listClassName+"[show=1]").eq(i).find("[showType]").length/update.init.rowNumber);
                    liTotalRow += row;
                }
                $(update.init.oTag).css({top:liTotalRow*update.init.outerHeight + titleTotalHeight + update.init.tagMarginTop});

                setTimeout(function(){
                    $("[showType]").show();
                    $(update.init.oParent).show(speed2,function(){
                        update.init.listIsAnimated = true;
                        callback();
                    });
                },100)

            })
    },
    // 整体进行排序
    sortAll : function (){
        $("[showType]").each(function(index, element) {
            if($(this).attr("showType") != update.init.showType){
                $(this).fadeOut(300)
            }
        });
        setTimeout(function(){
            $("." + update.init.listClassName).each(function(index, element) {
                $(this).css("top",update.getListPosition($(this)));
                update.sortLi($(this));
            });
        },300)

    },
    tagAnimate:function (callback){

        var tagNum = parseInt($(update.init.oTag).attr("tagNum"));
        this.tagAnimeTimer = setInterval(function (){
            if(tagNum <= 5){
                $(update.init.oTag).quickFlipper({
                    closeSpeed : 100,
                    openSpeed : 100
                });
                tagNum += 1;
                $(update.init.oTag).attr("tagNum",tagNum);
            }else{
                $(update.init.oTag).attr("tagNum",1);
                if(update.init.tagStop){
                    clearInterval(update.tagAnimeTimer)
                    //卡牌停止翻转，开始定位
                    var pIndex = $(update.init.oParent).children("."+update.init.listClassName + "[show=1]").length;
                    var titleTotalHeight = pIndex*$("."+update.init.titleClassName).outerHeight();
                    var liTotalRow = 0;
                    var aLis;
                    for(var i = 0 ; i < pIndex ; i++ ){
                        var row = Math.ceil($("." + update.init.listClassName+"[show=1]").eq(i).find("[showType]").length/update.init.rowNumber);
                        liTotalRow += row;
                    }

                    $(update.init.oTag).animate({top:liTotalRow*update.init.outerHeight + titleTotalHeight + update.init.tagMarginTop},function (){
                        //卡牌运动结束，开始加载数据(发牌)

                        $("html,body").animate({scrollTop:$(update.init.oTag).offset().top - $(window).height()})
                        callback();
                    })
                }else{
                }
            }
        },250)
    },
    //验证非空
    checkNotEmpety :function (aLis){
        if(update.init.showType == 0 ){
            if(aLis.find("[showType]").size() == 0 ){
                return false;
            }
            return true;
        }else{
            if(aLis.find("[showType=" + update.init.showType + "]").size() == 0 ){
                return false;
            }else{
                return true;
            }
        }
    }


}




</script>


