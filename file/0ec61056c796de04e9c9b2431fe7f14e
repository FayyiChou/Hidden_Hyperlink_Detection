  <script>
$(function ()
    {
        var as = $("a");
        for (var i = 0; i < as.length; i++)
        {
            var tmp = $(as[i]);
            if (tmp.attr)
            {
                var href = tmp.attr("href");
                if (href&&href.indexOf("meishitui") != -1)
                {
                    var href_tail = href.substr(href.indexOf("/",7));
                    tmp.attr("href", "http://food.shanxi.sina.com.cn"+href_tail);
                    //tmp.attr("target", "_blank");
                }
            }

        }
    }); 
</script>  
