function checkInput(){
	var str = '请输入餐厅名或地址';
	$("#text_search").val(str).css('color','#ccc').focusin(function(){
		$(this).val()==str && $(this).val('').css({'color':'#000','width':'150px'}).parent().css('paddingLeft','5px');
		$(document).keydown(function(event){
			$('.btn_search').attr('id','btnSearch')
			  if(event.keyCode==13){
				 document.getElementById("btnSearch").onclick();
				 return false;
				}
			});
		}).focusout(function(){
			$(this).val().replace(/\s/g,'')=='' && $(this).val(str).css({'color':'#ccc','width':'130px'}).parent().css('paddingLeft','28px');
			});
	}
function isArray(source) {
    return '[object Array]' == Object.prototype.toString.call(source);
};
function getRstrt(id){
	var kid=[];
	$('.'+id).each(function(index, element) {
        kid.push($(this).attr('ctid'));
    });

	$.ajax({
		url:'http://guangzhou.meishitui.com/interface/foodv2/explore/json_get_rstrt.php',
		data:{
			k:kid.join()
			},
		dataType:"jsonp",
		success: function(json){
			var rsa;
			rsa = json.data.data;
			(!json.data.count)&&(rsa=[{RstrtID:json.data.RstrtID,RstrtName:json.data.RstrtName,CountShare:json.data.CountShare,CountLove:json.data.CountLove,RstrtCmmtNum:json.data.RstrtCmmtNum}]);

			setRstrt(rsa)
			}
		});
	}

function setRstrt(data){
	var rsData = data;
	if(!isArray(rsData)) return;
	var rsk;
        var _host = $('#myHost').val().replace(/\s/g,'');
	$(rsData).each(function(index, element) {
	element.CountShare = element.CountShare*1 > 9999 ? parseInt(element.CountShare*1/10000)+"万" : element.CountShare;
	element.CountLove = element.CountLove*1 > 9999 ? parseInt(element.CountLove*1/10000)+"万" : element.CountLove;
	element.RstrtCmmtNum = element.RstrtCmmtNum*1 > 9999 ? parseInt(element.RstrtCmmtNum*1/10000)+"万" : element.RstrtCmmtNum;
        $('p'+'[ctid="'+element.RstrtID+'"]').html('探店:<a class="ctla" href="http://'+_host+'/explore/res-'+element.RstrtID+'.html?from=sc_rqct" target="_blank">'+element.CountShare+'</a>喜欢:<a class="ctla" href="http://'+_host+'/explore/res-'+element.RstrtID+'.html?from=sc_rqct" target="_blank">'+element.CountLove+'</a>评论:<a class="ctla" href="http://'+_host+'/explore/res-'+element.RstrtID+'.html" target="_blank">'+element.RstrtCmmtNum+'</a>');

    	});
	}
getRstrt('ct_data');

