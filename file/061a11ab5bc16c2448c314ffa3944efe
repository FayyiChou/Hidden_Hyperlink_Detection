	<script src="js/modernizr.custom.js"></script>
	<script src="js/jquery-1.11.3.min.js"></script>
	<script src="js/lazyload.min.js"></script>
	<script src="js/vivus.min.js"></script>
	<script src="js/index.js"></script>
	<script>
		$(window).on('beforeunload', function(){
			$(window).scrollTop(0);
		});
		/*cache dom*/
		var nav = $('nav')
		var aboutHotchkiss = $('.about-hotchkiss-group');
		var options = $('.options');
		var optionsContainer = $('.options-container');
		var footer = $('.footer');
		$(nav).hide();
		//detect IE8 and below by checking if SVG works
		if(Modernizr.svg){
			$('.legacy-browser').hide();
			$(footer).hide();
			var hotchkissGroup = new Vivus('hotchkissGroup', {type: 'scenario-sync', duration: 3, start: 'autostart', dashGap: 20, forceRender: true},
				function (obj) {
					$('#hotchkissGroup').attr('class','finished');
					setTimeout(function(){
						aboutHotchkiss.addClass('slide-in');
						optionsSlideIn();
					},1000);
				});
			var optionsSlideIn = function() {
				setTimeout(function(){
					options.fadeIn();
					HOTCHKISSGROUP.equalHeights(jQuery);
					$(footer).show();
					setTimeout(function(){
						$(nav).fadeIn();
					},1000);
				},1000);
			}
		} else {
			/*change all svg images to png equivalents*/
			var imgs = document.getElementsByTagName('img');
			var svgVers = /.*\.svg$/;
			for (var i=0;i!=imgs.length;++i) {
				if(imgs[i].src.match(svgVers)) {
					imgs[i].src=imgs[i].src.slice(0,-3)+"png";
				}
			}
			aboutHotchkiss.addClass('slide-in');
			options.fadeIn();

			optionsContainer.addClass('slide-in');

			$('.hotchkiss-container').html('<img src="/images/logos/hotchkiss-group-logo.png" alt="Hotchkiss Group" width="100%">');
			setTimeout(function(){
				$(nav).fadeIn();
			},1000);
			/*add margin to footer so that we can still see it*/
			marginToAdd = $('.legacy-browser').outerHeight();
			$(footer).css('margin-bottom',marginToAdd);
		}
		$(document).ready(function(){
			/*$(".option img").lazyload({effect:"fadeIn"});*/
			$('.backtotop').on('click',function(e){
				e.preventDefault();
				$('html, body').animate({scrollTop:0},400);
			});
			$(nav).on('click',function(){
				$(this).toggleClass('out');
			});
		});
	</script>
