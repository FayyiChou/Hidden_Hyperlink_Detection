var mce_preload_checks2 = 0;
function mce_preload_check2(){
    if (mce_preload_checks2>40) return;
    mce_preload_checks2++;
    try {
        var jqueryLoaded=jQuery;
    } catch(err) {
        setTimeout('mce_preload_check2();', 250);
        return;
    }
    try {
        var validatorLoaded=jQuery("#fake-form").validate({});
    } catch(err) {
        setTimeout('mce_preload_check2();', 250);
        return;
    }
    mce_init_form2();
}
function mce_init_form2(){
    jQuery(document).ready( function($) {
      var options = { errorClass: 'mce_inline_error', errorElement: 'div', onkeyup: function(){}, onfocusout:function(){}, onblur:function(){}  };
      var mce_validator = $("#mc-embedded-subscribe-form2").validate(options);
      $("#mc-embedded-subscribe-form2").unbind('submit');//remove the validator so we can get into beforeSubmit on the ajaxform, which then calls the validator
      options = { url: 'http://puntadeleste.us2.list-manage.com/subscribe/post-json?u=4b6f583e1da2d13585a99f5f3&id=0911e44ddc&c=?', type: 'GET', dataType: 'json', contentType: "application/json; charset=utf-8",
                    beforeSubmit: function(){
                        $('#mce_tmp_error_msg2').remove();
                        $('.datefield','#mc_embed_signup2').each(
                            function(){
                                var txt = 'filled';
                                var fields = new Array();
                                var i = 0;
                                $(':text', this).each(
                                    function(){
                                        fields[i] = this;
                                        i++;
                                    });
                                $(':hidden', this).each(
                                    function(){
                                        var bday = false;
                                        if (fields.length == 2){
                                            bday = true;
                                            fields[2] = {'value':1970};//trick birthdays into having years
                                        }
                                    	if ( fields[0].value=='MM' && fields[1].value=='DD' && (fields[2].value=='YYYY' || (bday && fields[2].value==1970) ) ){
                                    		this.value = '';
									    } else if ( fields[0].value=='' && fields[1].value=='' && (fields[2].value=='' || (bday && fields[2].value==1970) ) ){
                                    		this.value = '';
									    } else {
	                                        this.value = fields[0].value+'/'+fields[1].value+'/'+fields[2].value;
	                                    }
                                    });
                            });
                        return mce_validator.form();
                    }, 
                    success: mce_success_cb2
                };
      $('#mc-embedded-subscribe-form2').ajaxForm(options);      
		$('#mc_embed_signup2').hide();
        cks = document.cookie.split( ';' );

		var show=true;
        for(i=0;i<cks.length;i++){
            parts = cks[i].split('=');
            if( parts[0].indexOf('MCEvilPopupClosed2')!= -1 ){
				show = false;
			}
        }
        if (show){
            $('#mc_embed_signup2 a.mc_embed_close2').show();
			setTimeout( function(){ $('#mc_embed_signup2').fadeIn();} , 30000);
	        $('#mc_embed_signup2 a.mc_embed_close2').click(function(){ mcEvilPopupClose2();});
        }

        $(document).keydown(function(e){
            if (e == null) { 
              keycode = event.keyCode;
            } else { 
              keycode = e.which;
            }
            if(keycode == 27){
                mcEvilPopupClose2();
            }
        });

        function mcEvilPopupClose2(){
            //alert("close");
			$('#mc_embed_signup2').hide();
            var now = new Date();
            var expires_date = new Date( now.getTime() + 31536000000 );
            document.cookie = 'MCEvilPopupClosed2=yes;expires=' + expires_date.toGMTString()+';path=/';
        }

    });
}
function mce_success_cb2(resp){
    $('#mce-success-response2').hide();
    $('#mce-error-response2').hide();
    if (resp.result=="success"){
        $('#mce-'+resp.result+'-response2').show();
        $('#mce-'+resp.result+'-response2').html(resp.msg);
        $('#mc-embedded-subscribe-form2').each(function(){
            this.reset();
    	});
    } else {
        var index = -1;
        var msg;
        try {
            var parts = resp.msg.split(' - ',2);
            if (parts[1]==undefined){
                msg = resp.msg;
            } else {
                i = parseInt(parts[0]);
                if (i.toString() == parts[0]){
                    index = parts[0];
                    msg = parts[1];
                } else {
                    index = -1;
                    msg = resp.msg;
                }
            }
        } catch(e){
            index = -1;
            msg = resp.msg;
        }
        try{
            if (index== -1){
                $('#mce-'+resp.result+'-response2').show();
                $('#mce-'+resp.result+'-response2').html(msg);            
            } else {
                err_id = 'mce_tmp_error_msg2';
                html = '<div id="'+err_id+'" style="'+err_style+'"> '+msg+'</div>';
                
                var input_id = '#mc_embed_signup2';
                var f = $(input_id);
                if (ftypes[index]=='address'){
                    input_id = '#mce-'+fnames[index]+'-addr1';
                    f = $(input_id).parent().parent().get(0);
                } else if (ftypes[index]=='date'){
                    input_id = '#mce-'+fnames[index]+'-month';
                    f = $(input_id).parent().parent().get(0);
                } else {
                    input_id = '#mce-'+fnames[index];
                    f = $().parent(input_id).get(0);
                }
                if (f){
                    $(f).append(html);
                    $(input_id).focus();
                } else {
                    $('#mce-'+resp.result+'-response2').show();
                    $('#mce-'+resp.result+'-response2').html(msg);
                }
            }
        } catch(e){
            $('#mce-'+resp.result+'-response2').show();
            $('#mce-'+resp.result+'-response2').html(msg);
        }
    }
}

</script>
<!--End mc_embed_signup2-->
<!-- FIN js sacado del view META -->

<script type="text/javascript">
function cargarEdificio(obj){
	var data = obj.value;
	data = data.split(",");
	if(data[0] == 0)
		return false;
	location.href = "http://www.puntadeleste.com/es/informacion/punta_del_este/Edificios/"+data[1]+"/"+data[0];
}
function cargarProyecto(obj){
	var data = obj.value;
	data = data.split(",");
	if(data[0] == 0)
		return false;
	location.href = "http://www.puntadeleste.com/es/informacion/punta_del_este/Edificios_en_Construccion/"+data[1]+"/"+data[0];
}
</script>

