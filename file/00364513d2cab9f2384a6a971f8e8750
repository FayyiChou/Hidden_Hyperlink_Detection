			$("#fechaDesdeH").datepicker({ showOn: "both", buttonImage: "/images/hotel-bot-crono.jpg", minDate: +0 });
			$("#fechaHastaH").datepicker({ showOn: "both", buttonImage: "/images/hotel-bot-crono.jpg", minDate: +0 });
		
