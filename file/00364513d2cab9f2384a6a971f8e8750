	<script type="text/javascript">
		$(document).ready(function(){
			$("img.pointer").click(function(e){
				e.preventDefault();
				var url = $(this).attr('value2');
				var target = $(this).attr('value3');
				var foto_id = $(this).attr('value4');
				
				$.post("http://www.puntadeleste.com/home/registrar_click",{home_cabezal_id:foto_id});
				
				if(target == '1')
					window.open(url);
				else
					location.href = url;
			});
			
			$(".buttonall").mouseover(function(){
				$(this).css('background-color','#2d6097');
			}).mouseout(function(){
				$(this).css('background-color','#5898df');
			});
			
						
			$("#btnBuscarHotel").mouseover(function(){
				$(this).css('background-color','#1a4879');
			}).mouseout(function(){
				$(this).css('background-color','#5898DF');
			});
			
			
			setInterval( "slideSwitch()", 6000 );
			
			var from_date = $('#fechaDesdeH').val();
			var to_date = $('#fechaHastaH').val();
			var lang = window.location.pathname;
			lang = lang.split('/');
			
			if(from_date != "" && to_date != ""){
				//cambiar formato fecha si el idioma es español
				if( lang[1] != "en" ){
					from_date = from_date.split("/");
					from_date = from_date[1]+"/"+from_date[0]+"/"+from_date[2];
					to_date = to_date.split("/");
					to_date = to_date[1]+"/"+to_date[0]+"/"+to_date[2];
				}
				document.getElementById('fechaDesdeH').value = from_date;
				document.getElementById('fechaHastaH').value = to_date;
				actualizarNochesBusc('');
			}
			
		});
		
		function mostrarHotel(obj){
			if(obj.value != "Seleccionar")
				window.location = "/es/hoteles/punta_del_este/"+obj.value;
		}
		
		function slideSwitch() {
			var $active = $('#slideshow .sl_img IMG.active');
			//var $active = $('#slideshow IMG.active');
			if ( $active.length == 0 ) $active = $('#slideshow .sl_img:last IMG');
			// use this to pull the images in the order they appear in the markup
			var $next =  !$active.parent().hasClass('last') ? $active.parent().next('div').find('img')
		//	var $next =  $active.next().length ? $active.next()
				: $('#slideshow .sl_img:first IMG');
			$next.parent().css('z-index','11');
			// uncomment the 3 lines below to pull the images in random order		
			// var $sibs  = $active.siblings();
			// var rndNum = Math.floor(Math.random() * $sibs.length );
			// var $next  = $( $sibs[ rndNum ] );
			$active.addClass('last-active');
			$next.css({opacity: 0.0})
				.addClass('active')
				.animate({opacity: 1.0}, 1000, function() {
					$active.removeClass('active last-active');
					$active.parent().css('z-index','10');
				});
		}
</script>


