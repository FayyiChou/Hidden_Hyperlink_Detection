		function trim(myString){
			return myString.replace(/^\s+/g,'').replace(/\s+$/g,'')
		}

		function validarForm(){
			var faltaConfirmar = "";
			
			var nombre = $('#nombre').val();
			if(nombre == "")
				faltaConfirmar = faltaConfirmar+"-Complete su nombre<br/>";
			var apellido = $('#apellido').val();
			if(apellido == "")
				faltaConfirmar = faltaConfirmar+"-Complete su apellido<br/>";
			var pais = $('#pais').val();
			if(pais == 0)
				faltaConfirmar = faltaConfirmar+"-Escoja su pais<br/>";
			var ciudad = $('#ciudad').val();
			if(ciudad == "")
				faltaConfirmar = faltaConfirmar+"-Complete su ciudad<br/>";
			var codigo_postal = $('#codigo_postal').val();
			/*if(codigo_postal == "")
				faltaConfirmar = faltaConfirmar+"-Complete su codigo postal<br/>";
				*/
			var telefono = $('#telefono').val();
			if(telefono == "")
				faltaConfirmar = faltaConfirmar+"-Complete su telefono<br/>";
			var email = $('#email').val();
			email = trim(email);
			
			if(email == "")
				faltaConfirmar = faltaConfirmar+"-Complete su email<br/>";
			var confirmar_email = $('#confirmar_email').val();
			confirmar_email = trim(confirmar_email);
			if(confirmar_email == "")
				faltaConfirmar = faltaConfirmar+"-Confirme su email<br/>";
			var tiposHab = $('#tiposHab').val();
			if(tiposHab == 0)
				faltaConfirmar = faltaConfirmar+"-Escoja su habitacion<br/>";
			
			if( (email != confirmar_email) && (email != "")  && (confirmar_email != "") )
				faltaConfirmar = faltaConfirmar + "-Los mails no coinciden<br/>";
			
			//para el form de las promociones
			//if(document.forms[0].fechaDesdePromo && document.forms[0].fechaHastaPromo){
			try{
				var fdesdep = $("#fechaDesdePromo").val();
				var fhastap = $("#fechaHastaPromo").val();
				
				if(fdesdep == "")
					faltaConfirmar = faltaConfirmar + "-Escoja fecha de ingreso<br/>";
				if(fhastap == "")
					faltaConfirmar = faltaConfirmar + "-Escoja fecha de egreso<br/>";
				
				var lang = window.location.pathname;
				lang = lang.split('/');

				var dias = 0;
				//cambiar formato fecha si el idioma es espa^ol o portugues
				if( lang[1] != "en" ){
					fdesdep = fdesdep.split("/");
					fdesdep = fdesdep[1]+"/"+fdesdep[0]+"/"+fdesdep[2];
					fhastap = fhastap.split("/");
					fhastap = fhastap[1]+"/"+fhastap[0]+"/"+fhastap[2];
					
					dias = calcularDias(fdesdep,fhastap);	

					fdesdep = fdesdep.split("/");
					fdesdep = fdesdep[0]+"/"+fdesdep[1]+"/"+fdesdep[2];
					fhastap = fhastap.split("/");
					fhastap = fhastap[0]+"/"+fhastap[1]+"/"+fhastap[2];					
				}
				else
					dias = calcularDias(fdesdep,fhastap);
					
				if( dias <= 0 ){
					faltaConfirmar = faltaConfirmar + "-La fecha de inicio no puede ser menor o igual a la fecha de fin<br/>";
				}
			}
			catch (err) {}
			
			//Validar edades de menores en promo
			try {
			  if (document.getElementById('cantidad_menores').selectedIndex > 0 && document.getElementById('rango_edad_menores').value == '') {
				faltaConfirmar = faltaConfirmar + "Por favor indique las edades de los menores","Atencion";
			  }
			  else{
				 if(document.getElementById('cantidad_menores').selectedIndex == 0){
					$("#rango_edad_menores").val('');
					document.formPromo1.rango_edad_menores.disabled = true;
				}
				else
					document.formPromo1.rango_edad_menores.disabled = false;
					
				$("#selMen_index").val(document.getElementById('cantidad_menores').selectedIndex);
			  }
			}
			catch (err) {}
					
			return faltaConfirmar;
		}
		
		function validar() {
			//Validar fechas completadas
			if (document.getElementById('fechaDesde').value == '') {
			$("#progreso").hide();
			  jAlert("-Escoja fecha de ingreso", "Atencion");
			  return false;
			}
			if (document.getElementById('fechaHasta').value == '') {
			$("#progreso").hide();
			  jAlert("-Escoja fecha de egreso","Atencion");
			  return false;
			}
			
			try {
				var habSel = document.getElementById('tiposHab');
				if (habSel.value == 0) {
					if(habSel.length == 1){
						$("#progreso").hide();
						jAlert("No hay tarifa disponible para alguna de las fechas seleccionadas","Atencion");
					}
					else{
						$("#progreso").hide();
						jAlert("-Escoja su habitacion","Atencion");
					}
					return false;
				}
			}
			catch (err) {}
			
			//Validar edades de menores
			try {
			  if (document.getElementById('selectMenores').selectedIndex > 0 && document.getElementById('rangoMenores').value == '') {
			  $("#progreso").hide();
				jAlert("Por favor indique las edades de los menores","Atencion");
				return false;
			  }
			  else{
				 if(document.getElementById('selectMenores').selectedIndex == 0){
					$("#rangoMenores").val('');
					document.formReserva.rangoMenores.disabled = true;
				}
				else
					document.formReserva.rangoMenores.disabled = false;
					
				$("#selMen_index").val(document.getElementById('selectMenores').selectedIndex);
			  }
			}
			catch (err) {}
			
			return true;
		}

		
		function actualizarNoches(obj){
			var id_Room = document.getElementById('tiposHab').value;
			var from_date = document.getElementById('fechaDesde').value;
			var to_date = document.getElementById('fechaHasta').value;
			
			if(to_date == '')
			{
				document.getElementById('fechaHasta').value = from_date;
				to_date = from_date;
			}
			if(from_date == '')
			{
				document.getElementById('fechaDesde').value = to_date;
				from_date = to_date;
			}	
			
			var lang = window.location.pathname;
			lang = lang.split('/');

			var dias = 0;
				//cambiar formato fecha si el idioma es espa^ol
				if( lang[1] != "en" ){
					from_date = from_date.split("/");
					from_date = from_date[1]+"/"+from_date[0]+"/"+from_date[2];
					to_date = to_date.split("/");
					to_date = to_date[1]+"/"+to_date[0]+"/"+to_date[2];
					
					dias = calcularDias(from_date, to_date);
					
					from_date = from_date.split("/");
					from_date = from_date[0]+"/"+from_date[1]+"/"+from_date[2];
					to_date = to_date.split("/");
					to_date = to_date[0]+"/"+to_date[1]+"/"+to_date[2];
				}
				else
					dias = calcularDias(from_date, to_date);		
					
			if(dias >= 0)
			{
				$("#cantNoches").text($("#cantNoches").attr('alt') + dias);
				document.getElementById('noches').value = dias;
			}
			else
				$("#cantNoches").text($("#cantNoches").attr('alt') + '0');	

			var seleccionar = "Seleccionar";
			// cargar en combo de hab solo las que tengan precios para esas fechas
			
			$.post('/es//habitacionesDisponibles', { lang: lang[1], nameHotel: '', fromDate: from_date, toDate: to_date},function(data){	
				
				if(data != " "){
					var datos = $.parseJSON(data);
					
					$('#tiposHab').find('option').remove();
					$('#tiposHab').append('<option value="0">'+seleccionar+'</option>');
					
					for(var i = 0; i < datos.length ; i++){
						if(datos[i].room_id == id_Room) {
							$('#tiposHab').append('<option value="'+datos[i].room_id+'" selected="selected">'+datos[i].name+'</option>').val(id_Room);
						}
						else
							$('#tiposHab').append('<option value="'+datos[i].room_id+'">'+datos[i].name+'</option>');
					}
				}
				else {
					$('#tiposHab').find('option').remove();
					$('#tiposHab').append('<option value="0">'+seleccionar+'</option>').val(0);
					jAlert("No hay tarifa disponible para alguna de las fechas seleccionadas","Atencion");
				}
				return false;
			});
			
		}
		
		function actualizarNochesBusc(obj){			
			var from_date = document.getElementById('fechaDesde').value;
			var to_date = document.getElementById('fechaHasta').value;
			
			if(to_date == '')
			{
				document.getElementById('fechaHasta').value = from_date;
				to_date = from_date;
			}
			if(from_date == '')
			{
				document.getElementById('fechaDesde').value = to_date;
				from_date = to_date;
			}	
			
			var lang = window.location.pathname;
			lang = lang.split('/');
			var dias_d = 0;
			
				if( lang[1] != "en" ){
					var from_d = from_date;
					from_d = from_d.split("/");
					from_d = from_d[1]+"/"+from_d[0]+"/"+from_d[2];
					var to_d = to_date;
					to_d = to_d.split("/");
					to_d = to_d[1]+"/"+to_d[0]+"/"+to_d[2];
										
					dias_d = calcularDias(from_d, to_d);
					
					from_d = from_d.split("/");
					from_d = from_d[0]+"/"+from_d[1]+"/"+from_d[2];
					to_d = to_d.split("/");
					to_d = to_d[0]+"/"+to_d[1]+"/"+to_d[2];
				}
				else{
					dias_d = calcularDias(from_date, to_date);
				}
				
			if(dias_d <= 0)
			{
				document.getElementById('fechaHasta').value = from_date;
				to_date = from_date;
				$("#cantNoches").text($("#cantNoches").attr('alt') + '0');
			}
			else{
				$("#cantNoches").text($("#cantNoches").attr('alt') + dias);
				document.getElementById('noches').value = dias;
			}
			/*

			var dias = 0;
				//cambiar formato fecha si el idioma es espa^ol
				if( lang[1] != "en" ){
					var fr_d = from_date;
					fr_d = fr_d.split("/");
					fr_d = fr_d[1]+"/"+fr_d[0]+"/"+fr_d[2];
					var tod_d = to_date;
					tod_d = tod_d.split("/");
					tod_d = tod_d[1]+"/"+tod_d[0]+"/"+tod_d[2];
										
					dias = calcularDias(fr_d, tod_d);
					
					fr_d = fr_d.split("/");
					fr_d = fr_d[0]+"/"+fr_d[1]+"/"+fr_d[2];
					tod_d = tod_d.split("/");
					tod_d = tod_d[0]+"/"+tod_d[1]+"/"+tod_d[2];
				}
				else
					dias = calcularDias(fr_d, tod_d);		
			*/
