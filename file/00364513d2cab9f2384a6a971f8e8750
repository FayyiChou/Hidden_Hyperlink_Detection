  <script type="text/javascript">
$(document).ready(function(){
	$("#btnSubmit").click(function(){
		var email = $("#email").val();
		var confirmar_email = $("#confirmar_email").val();
		var nombre = $("#nombre").val();

		if(email != confirmar_email){
			jAlert("Por favor verifique su email.",'Atencion');
			return false;
		}
		else{
			$.post("/es/home/suscribirse",{nombre: nombre, email: email, confirmar_email: confirmar_email},
				function(data){
					jAlert(data,'Atencion');
				});
		}
		return false;
	});
});
</script>
