			$("#btnBuscarHotel2").click(function(e){
				//e.preventDefault();
				$("#progreso").show();
				var from_date = $("#fechaDesdeH").val();
				var to_date = $("#fechaHastaH").val();
				
				if ( from_date == '') {
					jAlert("-Escoja fecha de ingreso", "Atencion");
					$("#progreso").hide();
					return false;
				}
				if ( to_date == '') {
					jAlert("-Escoja fecha de egreso", "Atencion");
					$("#progreso").hide();
					return false;
				}
				
				var current_date = hoy();

				var lang = window.location.pathname;
				lang = lang.split('/');

				var dias = 0;
				var from_date2 = from_date;
				var to_date2 = to_date;
				//cambiar formato fecha si el idioma es espa^ol
				if( lang[1] != "en" ){
					from_date2 = from_date2.split("/");
					from_date2 = from_date2[1]+"/"+from_date2[0]+"/"+from_date2[2];
					to_date2 = to_date2.split("/");
					to_date2 = to_date2[1]+"/"+to_date2[0]+"/"+to_date2[2];
					
					dias = calcularDias(current_date,from_date2);						
				}
				else
					dias = calcularDias(current_date,from_date2);
			
			
				if( dias < 2){
					jAlert("La fecha de entrada debe ser 2 dias posterior al dia de la fecha", 'Atencion');
					$("#progreso").hide();
					return false;
				}
				
				var dif_dias = 0;
				var from_date3 = from_date;
				var to_date3 = to_date;
				if( lang[1] != "en" ){
					from_date3 = from_date3.split("/");
					from_date3 = from_date3[1]+"/"+from_date3[0]+"/"+from_date3[2];
					to_date3 = to_date3.split("/");
					to_date3 = to_date3[1]+"/"+to_date3[0]+"/"+to_date3[2];
					
					dif_dias = calcularDias(from_date3,to_date3);						
				}
				else
					dif_dias = calcularDias(from_date3,to_date3);
				
				if(dif_dias <= 0){
					jAlert("La fecha de egreso no puede ser menor o igual a la de ingreso.", 'Atencion');
					$("#progreso").hide();
					return false;
				}
				else{
					$('#fechaDesdeH').val(from_date2);
					$('#fechaHastaH').val(to_date2);
					
					document.getElementById('formBuscadorHotel').submit();
				}
			});	
			
