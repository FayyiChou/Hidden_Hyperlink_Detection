			var fdesdev = $("#fdesde").val();
			var fhastav = $("#fhasta").val();
			
			if(fdesdev && fhastav){
				fdesdev = fdesdev.split("-");
				fdesdev = fdesdev[1]+"/"+fdesdev[2]+"/"+fdesdev[0];
				fhastav = fhastav.split("-");
				fhastav = fhastav[1]+"/"+fhastav[2]+"/"+fhastav[0];
				
				var current_date = hoy();
				
				var lang = window.location.pathname;
				lang = lang.split('/');

				var dias = 0;
				var fin = 0;
				//cambiar formato fecha si el idioma es espa^ol
				dias = calcularDias(current_date,fdesdev);
				if(dias < 0)
					dias = 0;

				
				fin = calcularDias(current_date,fhastav);
				if(fin < 0)
					fin = 0;
				
				$("#fechaDesdePromo").datepicker({ minDate: dias, maxDate: fin });
				$("#fechaHastaPromo").datepicker({ minDate: dias, maxDate: fin });
			}
			
			$(".textField").click(function(){
				//e.preventDefault();
				$('#room_id').val(this.value);
				$('#season_id').val(this.id);
			}); 
			
			$("#quedarRegistrado").click(function(){
				//e.preventDefault();
				if( document.getElementById('quedarRegistrado').value == "No")
					document.getElementById('quedarRegistrado').value = "Si";
				else if( document.getElementById('quedarRegistrado').value == "Si")
					document.getElementById('quedarRegistrado').value = "No";
			}); 
			
			$("#btnSiguientePromo1").click(function(e){
				//e.preventDefault();
				$("#progreso").show();
				var errores;
				errores = validarForm();
				if( errores != "" ){
					$("#progreso").hide();
					jAlert(errores, "Atencion");
					return false;
				}
				else{
					var titular_comentario = $('#titular_comentario').val();
				
					if( titular_comentario.indexOf("@") != -1){
						$("#progreso").hide();
						jAlert("Por favor no ingrese dirección de correo electrónico o datos de contacto. Gracias.","Atencion");
						return false;
					}
				
					//chequeo dispo de fechas
					var fdesdep = $("#fechaDesdePromo").val();
					var fhastap = $("#fechaHastaPromo").val();
					
					var lang = window.location.pathname;
					lang = lang.split('/');

					var dias = 0;
					//cambiar formato fecha si el idioma es espa^ol o portugues
					if( lang[1] != "en" ){
						fdesdep = fdesdep.split("/");
						fdesdep = fdesdep[1]+"/"+fdesdep[0]+"/"+fdesdep[2];
						fhastap = fhastap.split("/");
						fhastap = fhastap[1]+"/"+fhastap[0]+"/"+fhastap[2];
						
						dias = calcularDias(fdesdep,fhastap);	

						fdesdep = fdesdep.split("/");
						fdesdep = fdesdep[0]+"/"+fdesdep[1]+"/"+fdesdep[2];
						fhastap = fhastap.split("/");
						fhastap = fhastap[0]+"/"+fhastap[1]+"/"+fhastap[2];					
					}
					else{
						dias = calcularDias(fdesdep,fhastap);
					}
					
					var tiposHab = $('#tiposHab').val();
					
					var faltaConfirmar = "";
					
					if(dias > 0){
						$('#tablaDisp').html('');
						$('#tablaDisp').hide();
						document.getElementById('formPromo1').submit();					
						/* estas validaciones ya no se hacen. mail fernando el dia 11/06/12 */
						/*$.post('/es//minimoNoches', { dias: dias, nameHotel: '', fromDate: fdesdep, toDate: fhastap},function(data){
							if(data != "" && data != undefined) {
								faltaConfirmar = faltaConfirmar + data + "<br/>";
								$("#progreso").hide();
								jAlert(faltaConfirmar,"Atencion");
								return false;
							}
							else{
								$.post('/es//nochesDisponibles', { nameHotel: '', idRoom: tiposHab, fromDate: fdesdep, toDate: fhastap},function(data){	
									if(dias > parseInt(data)){
										faltaConfirmar = faltaConfirmar + "No hay tarifa disponible para alguna de las fechas seleccionadas<br/>";
										$("#progreso").hide();
										jAlert(faltaConfirmar,"Atencion");
										return false;
									}
									else{
										$.post('/es//disponibilidad', { nameHotel: '', idRoom: tiposHab, fromDate: fdesdep, toDate: fhastap},function(data){
											if(data != undefined && data != ''){
												$('#tablaDisp').html(data);
												$('#tablaDisp').show();
												faltaConfirmar = faltaConfirmar + "No hay disponibilidad para algunas fechas.";
												$("#progreso").hide();
												jAlert(faltaConfirmar,"Atencion");
												return false;
											}
											else{
												$('#tablaDisp').html('');
												$('#tablaDisp').hide();
												document.getElementById('formPromo1').submit();
											}
										});
									}
								});
							}
						});	
						*/
					}					
				}
				return false;
			});
			
			$("#btnSiguientePaso1").click(function(e){
				//e.preventDefault();
				$("#progreso").show();
				if (!validar()) {
					$("#progreso").hide();
					return false;
				}
				
				var req_adicional = $('#req_adicional').val();
				
				if( req_adicional.indexOf("@") != -1){
					$("#progreso").hide();
					jAlert("Por favor no ingrese dirección de correo electrónico o datos de contacto. Gracias.","Atencion");
					return false;
				}
				
				var cant_noches = $('#noches').val();
				var id_Room = $('#tiposHab').val();
				var from_date = $('#fechaDesde').val();
				var to_date = $('#fechaHasta').val();
				
				var lang = window.location.pathname;
				lang = lang.split('/');
				
				var dias = 0;
				//cambiar formato fecha si el idioma es espa^ol
				if( lang[1] != "en" ){
					from_date = from_date.split("/");
					from_date = from_date[1]+"/"+from_date[0]+"/"+from_date[2];
					to_date = to_date.split("/");
					to_date = to_date[1]+"/"+to_date[0]+"/"+to_date[2];
					
					dias = calcularDias(from_date, to_date);
					
					from_date = from_date.split("/");
					from_date = from_date[0]+"/"+from_date[1]+"/"+from_date[2];
					to_date = to_date.split("/");
					to_date = to_date[0]+"/"+to_date[1]+"/"+to_date[2];
				}
				else
					dias = calcularDias(from_date, to_date);		
				
				var errorFechas = "";
				var errorMinimo = "";
				var errorNoches = "";
				var errorDisponibilidad = "";
				var lang = window.location.pathname;
				lang = lang.split('/');
					
				if( cant_noches == 0 ){
					$("#progreso").hide();
					errorFechas = "La fecha de egreso no puede ser igual o inferior a la fecha de ingreso."+'\r\n';
					jAlert(errorFechas,"Atencion");
					return false;
				}
									
				var minimoOk = false;
				$.post('/es//minimoNoches', { dias: dias, nameHotel: '', fromDate: from_date, toDate: to_date},function(data){
						
						if(data != "" && data != undefined) {
							//$("#progreso").hide();
							errorMinimo = data+"<br/>";
							minimoOk = false;
						}
						else{
							 minimoOk = true;
						}
						
						var nochesOk = false;
						$.post('/es//nochesDisponibles', { nameHotel: '', idRoom: id_Room, fromDate: from_date, toDate: to_date},function(data){	
							if(cant_noches > parseInt(data))
							{
								//$("#progreso").hide();
								errorNoches = "No hay tarifa disponible para alguna de las fechas seleccionadas<br/>";
								nochesOk = false;
							}
							else {
								 nochesOk = true;
							}
							
							var dispoOk = false;
							$.post('/es//disponibilidad', { nameHotel: '', idRoom: id_Room, fromDate: from_date, toDate: to_date},function(data){
								//$("#progreso").hide();
								if(data != undefined && data != '')
								{
									$('#tablaDisp').html(data);
									errorDisponibilidad = "No hay disponibilidad para algunas fechas.";
									dispoOk = false;
								} else {
									$('#tablaDisp').html('');
									dispoOk = true;
								}
								
								if(minimoOk == true && nochesOk == true && dispoOk==true ){
									//$("#progreso").hide();
									
									document.getElementById('fechaDesde').value = from_date;
									document.getElementById('fechaHasta').value = to_date;
									document.getElementById('formReserva').submit();
								}
								else{
									$("#progreso").hide();
									
									jAlert(errorFechas+errorMinimo+errorNoches+errorDisponibilidad,"Atencion");
								}
							});
						});
				});
			});
			
