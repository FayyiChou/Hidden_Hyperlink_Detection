				document.getElementById('noches').value = dias;
			}
			else
				$("#cantNoches").text($("#cantNoches").attr('alt') + '0');		

			*/
		}
		
		function hoy(){
			var fechaActual = new Date();
			dia = fechaActual.getDate();
			mes = fechaActual.getMonth() +1;
			
			//si es IE
			/*if(navigator.appName.indexOf("Explorer") != -1)
				anno = fechaActual.getYear();
			else
				anno = fechaActual.getYear()+1900;
			*/
			
			//alert(navigator.appName);
			//alert(navigator.appCodeName);

			if( (navigator.appName=="Microsoft Internet Explorer") )
				//si es el ie 9 en adelante
				if( navigator.appVersion.substring(0,4) >= 5.0 )
					anno = fechaActual.getYear()+1900;
				else
					anno = fechaActual.getYear();
			else //otro
				anno = fechaActual.getYear()+1900;
			
			
			//anno = fechaActual.getYear()+1900;
			if (dia <10) dia = "0" + dia;
			if (mes <10) mes = "0" + mes;  
			fechaHoy = mes + "/" + dia + "/" + anno;

			return fechaHoy;
		}

		function calcularDias(from_date, to_date) {
			var desde = from_date.split('/');
			var fdesde = new Date(desde[2], desde[0]-1, desde[1]);
			var hasta = to_date.split('/');
			var fhasta = new Date(hasta[2], hasta[0]-1, hasta[1]);
			var diferencia = fhasta.getTime() - fdesde.getTime();
			return dias = Math.floor(diferencia / (1000 * 60 * 60 * 24));
		}
		
		function mostrarDatos(obj){
			document.getElementById('idRoomActual').value = document.getElementById('tiposHab').value;
			
			$.post('/es//getRoom', { nameHotel: '', idRoom: obj.value},function(data){
				if(data){
					var valores = data.split(',');
					$("#capEstandar").text($("#capEstandar").attr('alt') + valores[0]);
					document.getElementById('capEstandar').value = valores[0];
					if(!valores[1])
						valores[1]='';
					$("#camaExtra").text($("#camaExtra").attr('alt') + valores[1]);
					document.getElementById('camaExtra').value = valores[1];
					var desde = 1;
					var capAdic = document.getElementById('camaExtra').value;
					var cant = parseInt(valores[0]) + parseInt(capAdic);
					
					//verifico que indice habia chequeado del combo de cantidad de habitaciones
					var cantHabit = document.getElementById('cantHabitSel').value;
					if(!cantHabit)
						cantHabit = 1;
					
					/*internamente actualiza menores*/
					actualizarMayores(cantHabit*cant);
					
					for(desde ; desde<=10 ; desde++)
						$('#cantHabit').append('<option value="'+desde+'">'+desde+'</option>').val(cantHabit);
					
					if( valores[1] == 0)
						$("#cantCamasAdicionales").text('No acepta');
					else
						$("#cantCamasAdicionales").text(0);
					document.getElementById('camasAdicionales').value = 0;
				}
			});
		}
		
		function actualizarMayores(cant){			
			$('#selectMayores').find('option').remove();
			desde = 1;
			//verifico que indice habia chequeado del combo de mayores
			var cantMay = document.getElementById('cantMayoresSel').value;
			if(!cantMay)
				cantMay = 2;
			for(desde ; desde<=cant ; desde++)
				$('#selectMayores').append('<option value="'+desde+'">'+desde+'</option>').val(cantMay);
			
			var obj = document.getElementById('selectMayores');
			actualizarMenores(obj);
		}
		
		function actualizarMenores(obj){
			$('#selectMenores').find('option').remove();
			
			document.getElementById('cantMayoresSel').value = obj.value;
			
			//verifico que indice habia chequeado del combo de menores
			var index = document.getElementById('selMen_index').value;			
			if(!index)
				index = 0;
			$('#selectMenores').find('option').remove();
			var elSel = document.getElementById('selectMayores');
			var cantMenores = elSel.length - obj.value;
			
			var i=0;
			if(cantMenores == 0){
				$('#selectMenores').append('<option value="0">0</option>').val(index);
				document.formReserva.rangoMenores.disabled = true;
			}
			else{
				for(i ; i<=cantMenores ; i++)
					$('#selectMenores').append('<option value="'+i+'">'+i+'</option>').val(index);
				document.formReserva.rangoMenores.disabled = false;
			}
			
			actualizarCamasAdicionales(obj);
		}
		
		function actualizarDatos(cant){
			document.getElementById('cantHabitSel').value = cant;
			capacidad = document.getElementById('capEstandar').value;
			var capAdic = document.getElementById('camaExtra').value;
			cant = parseInt(cant) * (parseInt(capacidad) + parseInt(capAdic) );
			actualizarMayores(cant);
		}
		
		function actualizarCamasAdicionales(obj){
			//Obtener info para recalcular
			var cantidadHabit = parseInt(document.getElementById('cantHabit').value);
			var capReal = parseInt(cantidadHabit) * parseInt(document.getElementById('capEstandar').value);
			var capAdic = parseInt(cantidadHabit) * parseInt(document.getElementById('camaExtra').value);
			var cantMay = parseInt(document.getElementById('selectMayores').selectedIndex+1);
			var men = parseInt(document.getElementById('selectMenores').selectedIndex);
		
			//si la cantidad de menores seleccionada es 0 --> limpio el input de rango de edades
			if(men == 0){
				$("#rangoMenores").val('');
				document.formReserva.rangoMenores.disabled = true;
			}
			else{
				document.formReserva.rangoMenores.disabled = false;
			}
			
			//Calcular camas adicionales necesarias
			var camasAdic = 0;
			if (cantMay <= capReal) {
			  if (capReal - (cantMay + men) >= 0)
				camasAdic = 0;
			  else
				camasAdic = (cantMay + men) - capReal;
			}
			else
			  camasAdic = cantMay + men - capReal;
			  
			if($("#cantCamasAdicionales").text() != "No acepta")
				$("#cantCamasAdicionales").text(camasAdic);
			
			document.getElementById('camasAdicionales').value = camasAdic;
		}
		
		function agregar(){
			var bookmarkUrl=location.href;
			var bookmarkTitle=document.title;
			
			/*if ((navigator.appName=="Microsoft Internet Explorer") && (parseInt(navigator.appVersion)>=4)) {
				window.external.AddFavorite(url,titulo);
			}
			else {
				if(navigator.appName == "Netscape"){
					alert(titulo+url);
					window.sidebar.addPanel(titulo,url,'');
				}
			}*/
			if (window.sidebar) { // For Mozilla Firefox Bookmark

                window.sidebar.addPanel(bookmarkTitle, bookmarkUrl,"");
			} else if( window.external || document.all) { // For IE Favorite
					if(navigator.appName == "Netscape"){ // For Chrome
						alert("Presione CTRL+D para agregar a favoritos");
					}
					else
						window.external.AddFavorite( bookmarkUrl, bookmarkTitle);
			} else if(window.opera) { // For Opera Browsers
					var elem = document.createElement('a');
					elem.attr("href",bookmarkUrl);
					elem.attr("title",bookmarkTitle);
					elem.attr("rel","sidebar");
			} else { // for other browsers which does not support
			alert(navigator.appName);
					 alert("Su navegador no soporta esta accion de agregar en favoritos");
					 return false;
			}

		}

		
		function mostrarHotel(obj){
			if(obj.value != "Seleccionar")
				window.location = "/es/hoteles/punta_del_este/"+obj.value;
		}
		
		
		//no se para que la uso
		function actualizarNochesBusqueda(obj){
			var from_date = document.getElementById('fechaDesdeBusqueda').value;
			var to_date = document.getElementById('fechaHastaBusqueda').value;
			
			if(to_date == '')
			{
				document.getElementById('fechaHastaBusqueda').value = from_date;
				to_date = from_date;
			}
			if(from_date == '')
			{
				document.getElementById('fechaDesdeBusqueda').value = to_date;
				from_date = to_date;
			}	
			if(from_date > to_date)
			{
				document.getElementById('fechaHastaBusqueda').value = from_date;
				to_date = from_date;
			}
			
			var dias = calcularDias(from_date, to_date);			
			if(dias >= 0)
			{
				$("#cantNochesBusqueda").text(dias);
				$("#nochesBusqueda").val(dias);
			}
			else
				$("#cantNochesBusqueda").text('0');			
		}
		
		function cambiarIdioma(){
			var url = $("#selectIdioma").val();
			var link = url;
			lang = url.split('/');
			
			if( lang[lang.length-1] == "sinTraduccion"){
				$('#flag'+lang[3]).trigger('click');
			}
			else if( lang[lang.length-1] == "noDispo"){
				$('#flag'+lang[1]).trigger('click');
			}
			else if( lang[lang.length-1] == "noDispoHotel"){
				$('#flag'+lang[1]).trigger('click');
			}
			else
				location.href = link;
		}
		
	</script>
	
