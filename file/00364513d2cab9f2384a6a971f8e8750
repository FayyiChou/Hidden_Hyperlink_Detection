<script type="text/javascript">
var title = document.getElementsByTagName("title")[0].text;
title = title.replace(":","%3A");
title = title.replace("/","%2F");
title = title.replace(",","%2C");
title = title.replace("(","%28");
title = title.replace(")","%29");
var a = document.getElementById('tweetLink');
if(a){
	a.href = "http://twitter.com/?status="+title+"%20http%3A%2F%2Fwww.puntadeleste.com%2Fes";
	document.formSub.action = "http://www.puntadeleste.com/es/recomendar/site/"+title;
}
</script>

<script type="text/javascript">
$("#sendLink").click(function(e){
	e.preventDefault();
	var title = document.getElementsByTagName("title")[0].text;
	var recommend_url = "http://www.puntadeleste.com/es";
	$("#url").val(recommend_url);
	$("#title").val(title);
	
	$("#formSub").submit();
	return true;
});

$(document).ready(function(){	
	$(".votar").click(function(e){
		e.preventDefault();
		var contentName = "";
		var url = "";
		var type = this.rel;
		var typeContent = "";
		var uri4 = "";
		var item = "";
		var uri2 = "";
		var uri1 = "es";
		//id de noticia o evento
		var uri6 = "";
		var lang_content = "informacion";
		var lang_hotel = "hoteles";
		var lang_events = "espectaculos-eventos";
		var lang_notices = "noticias";
		var lang_edificios = "Edificios";
		var lang_proyectos = "Edificios_en_Construccion";
		var lang = "es";
		
		if(uri2 == lang_content && (uri4 != lang_events && uri4 != lang_notices && uri4 != lang_edificios && uri4 != lang_proyectos)){
			//contenido
			if(item == "")
				contentName = uri4;
			else
				contentName = item;
			url = "http://www.puntadeleste.com/"+lang+"/"+lang_content+"/votar";
			typeContent = "contenido";
		}
		else if(uri2 == lang_content && uri4 == lang_events){
			//evento
			if(item == "")
				contentName = uri4;
			else
				contentName = item;
			url = "http://www.puntadeleste.com/"+lang+"/"+lang_content+"/votar";
			typeContent = "evento";
		}
		else if(uri2 == lang_content && uri4 == lang_notices){
			//noticia
			if(item == "")
				contentName = uri4;
			else
				contentName = item;
			url = "http://www.puntadeleste.com/"+lang+"/"+lang_content+"/votar";
			typeContent = "noticia";
		}
		else if(uri2 == lang_content && uri4 == lang_edificios){
			//edificio
			if(item == "")
				contentName = uri4;
			else
				contentName = item;
			url = "http://www.puntadeleste.com/"+lang+"/"+lang_content+"/votar";
			typeContent = "edificio";
		}
		else if(uri2 == lang_content && uri4 == lang_proyectos){
			//proyecto
			if(item == "")
				contentName = uri4;
			else
				contentName = item;
			url = "http://www.puntadeleste.com/"+lang+"/"+lang_content+"/votar";
			typeContent = "proyecto";
		}
		else if(uri2 == lang_hotel){
			//hotel
			contentName = uri4;
			url = "http://www.puntadeleste.com/"+lang+"/"+lang_hotel+"/votar";
			typeContent = "hotel";
		}		
		else if(uri2 == "" && (uri1 == lang || uri1 == "")){
			//estoy en la home
			contentName = "home";
			url = "http://www.puntadeleste.com/"+lang+"/staticpages/votar";
			typeContent = "home";
		}
		
		$.post(url,{contentName: contentName, type:type, typeContent:typeContent, id:uri6},function(data){
			data = data.split(",");
			if(data[0] == -1){
				var msg = "Usted ya ha votado.";
				alert(msg);
			}
			else{
				$("#votosPos").text(data[0]);
				$("#votosNeg").text(data[1]);
			}
			return false;
		});
	});
	
	setInterval( "slideSwitch()", 6000 );
	console.log('aca');
}); 

function slideSwitch() {
	
	var $active = $('#slideshow .sl_img IMG.active');
	//var $active = $('#slideshow IMG.active');
	if ( $active.length == 0 ) $active = $('#slideshow .sl_img:last IMG');
	// use this to pull the images in the order they appear in the markup
	var $next =  !$active.parent().hasClass('last') ? $active.parent().next('div').find('img')
//	var $next =  $active.next().length ? $active.next()
		: $('#slideshow .sl_img:first IMG');
	$next.parent().css('z-index','11');
	// uncomment the 3 lines below to pull the images in random order		
	// var $sibs  = $active.siblings();
	// var rndNum = Math.floor(Math.random() * $sibs.length );
	// var $next  = $( $sibs[ rndNum ] );
	$active.addClass('last-active');
	$next.css({opacity: 0.0})
		.addClass('active')
		.animate({opacity: 1.0}, 1000, function() {
			$active.removeClass('active last-active');
			$active.parent().css('z-index','10');
		});
}
</script>
