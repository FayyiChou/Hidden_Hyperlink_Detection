<script type="text/javascript">

$(document).ready(function(){	
    var cityName =$("#cityName").val();
    var cityId =$("#cityId").val();
    if(cityId!=null && cityName!=null&&cityId!=''&&cityName!=''){
    	$(".head_city_ycc_cont span").removeClass("active");
    	$("#"+cityId).addClass("active");
    	$(".head_city").html(cityName);

    }

});

//搜索框js
$(".head_search input").click(function(){
	$(".head_search_icon").show();
	$('.head_city').hide();
	$(".head_search span").show();

});

$(".head_search span").click(function(){
	$(".head_search_icon").hide();
	$('.head_city').show();
	$(".head_search span").hide();
});

$(document).click(function(event){
	  if(!$(event.target).parents('.head_search').length && !$(event.target).parents('.head_search input').length){
		  $(".head_search_icon").hide();
		  $('.head_city').show();
		  $(".head_search span").hide();
	  };
});

$(".head_search span").click(function(){
	$(".head_search input").val("");
});

//搜索框跳转
function searchSlide() {
    var keyword = $('.search_input').val();
	location.href="/mobileService/search.htm?keyword=" + encodeURI(encodeURI(keyword));
};

//地市选择js
$(".head_city_ycc_cont span").click(function(){
	$(".head_city_ycc_cont span").removeClass("active");

	$(this).addClass("active");
	setTimeout(function(){
		hideds('coverds','head_city_ycc')
	},50);
	var munuType = 	$("#munuType").val();
 	if(!munuType){
//		$("#cityName").val($(this).find("i").html());
    var  city =$(this).attr('id');
		$("#cityId").val($(this).attr('id'));
		$("#hidden_form").submit()
	}else{
     var cityName =$(this).find("i").html();
	$(".head_city").html(cityName);
//	 $.ajax({
//  type:"POST",
//  url:"/mobileService/putCookiesByPar.htm",
//  data:{
// 	 cityId:$(this).attr('id')
//  	},
//  datatype: "json",
//  success:function(data){
//      }
//	 });
         $.cookie("wapCookiesCityId", $(this).attr('id'), {path: "/"});
         $.cookie("wapCookiesCityName", cityName, {path: "/"});
	}

});

//地市弹出js
function showds(coverds,id){
	var Sys = {};
	var ua = navigator.userAgent.toLowerCase();
	var s;
	(s = ua.match(/msie ([\d.]+)/)) ? Sys.ie = s[1] :
	(s = ua.match(/chrome\/([\d.]+)/)) ? Sys.chrome = s[1] :
	(s = ua.match(/version\/([\d.]+).*safari/)) ? Sys.safari = s[1] : 0;

	var objCover=document.getElementById(coverds);
	var objId=document.getElementById(id);
	var scrollW=document.documentElement.scrollWidth;
	if(document.documentElement.clientHeight >= document.documentElement.scrollHeight){
		var scrollH=document.documentElement.clientHeight;
	}else{
		var scrollH=document.documentElement.scrollHeight;
	}
	if (Sys.safari || Sys.chrome) {
		var T=(document.documentElement.clientHeight-objId.clientHeight)/2+document.body.scrollTop;
	}else{
		var T=(document.documentElement.clientHeight-objId.clientHeight)/2+document.documentElement.scrollTop;
	}
	var L=(document.documentElement.clientWidth-objId.clientWidth)/2+document.documentElement.scrollLeft;

	objCover.style.width=6.4+"rem";
	objCover.style.height=scrollH+"px";
	objCover.style.visibility="visible";

	objId.style.top=0+"px";
	objId.style.left=0+"px";
	objId.style.visibility="visible";

	window.onresize=function (){
		var objCover=document.getElementById(coverds);
		var objId=document.getElementById(id);
		var scrollW=document.documentElement.scrollWidth;
		if(document.documentElement.clientHeight >= document.documentElement.scrollHeight){
			var scrollH=document.documentElement.clientHeight;
		}else{
			var scrollH=document.documentElement.scrollHeight;
		}
		if (Sys.safari || Sys.chrome) {
			var T=(document.documentElement.clientHeight-objId.clientHeight)/2+document.body.scrollTop;
		}else{
			var T=(document.documentElement.clientHeight-objId.clientHeight)/2+document.documentElement.scrollTop;
		}
		var L=(document.documentElement.clientWidth-objId.clientWidth)/2+document.documentElement.scrollLeft;


		objCover.style.width=6.4+"rem";
		objCover.style.height=scrollH+"px";

		objId.style.top=0+"px";
		objId.style.left=0+"px";
	}
}
function hideds(coverds,id){

	var Sys = {};
	var ua = navigator.userAgent.toLowerCase();
	var s;
	(s = ua.match(/msie ([\d.]+)/)) ? Sys.ie = s[1] :
	(s = ua.match(/version\/([\d.]+).*safari/)) ? Sys.safari = s[1] : 0;
	var objCover=document.getElementById(coverds);
	var objId=document.getElementById(id);
	objCover.style.visibility="hidden";
	objId.style.visibility="hidden";
}
</script>
    







 
    
    
    
    
    

