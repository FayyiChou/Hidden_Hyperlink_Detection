<script type="text/javascript">
    function footerCss() {
        var winH = $(window).height();
        var bodyH = $("body").height();

        if (winH > bodyH) {
            $(".help_footer").css({
                "position": "absolute",
                "bottom": "0"
            });
        }
        else {
            $(".help_footer").css({
                "position": "static"

            });
        }
    }
    function navmove() {

        $(window).scroll(function () {
            var infoTop = $('.searchhead').height();
            var scrolla = $(window).scrollTop();
            if (scrolla > infoTop) {
                $('.top_nav_move').show();
            } else {
                $('.top_nav_move').hide();
            }
        });
    }

    function closedowmad() {
        $('.closeiconup').bind('click', function () {
            $('#uploadcontainer').hide();
        })
    }

    window.onload = function () {
        footerCss();
        //changeli();
        //navmove();
        closedowmad();
        //$('.swiper-slide').children('img').error
        
        //$('.swiper-slide').children('img').attr("onerror","alert('dd')");
        
    };
    function stopBubble(e){
        var evt = e||window.event;
        evt.stopPropagation?evt.stopPropagation():(evt.cancelBubble=true);//阻止冒泡
        //evt.preventDefault();//阻止浏览器默认行为，这样链接就不会跳转
    }
    //背景变色
    var flag = 0;
    $('.newmore').children('li').each(function () {
//        var url = $(this).attr('name');
        var me = $(this);
        var timeoutstart;
        $(this).bind('mousedown touchstart', function (e) {
            timeoutstart = new Date();
            $(this).addClass('onchecklist');
            //event.preventDefault();
            stopBubble(e);

        }).bind('mouseup touchend', function (e) {
            var timeoutend = new Date();
            var timeloop = timeoutend.getTime() - timeoutstart.getTime();
            //console.log('time=='+timeloop);
            stopBubble(e);

            if (timeloop < 80) {
//                window.location.href = $(this).attr('name');
                $(this).removeClass('onchecklist');
                //$(this).removeClass('onpoint');
            }
            //alert('333=='+$(this).attr('class'));
            $(this).removeClass('onchecklist');
            flag = 1;
        }).bind('mouseover touchmove', function (e) {
            stopBubble(e);
            timeout = setTimeout(function () {
            }, 200);
            $(this).addClass('onchecklist');
            setTimeout(function () {
                if (flag == 0) {
                    //alert('333=='+me.attr('class'));
                    me.removeClass('onchecklist');
                }
            }, 150);

        })

    })
</script>
</body></html>