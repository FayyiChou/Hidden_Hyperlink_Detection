<script type="text/javascript">
	var resourceHost = "http://music.res.meizu.com";
	 if(navigator.userAgent.match(/mobile|phone/i)){//手机
		 setActiveCss("/resources/music/css/error_mobile.css");
	 	}else{
	 		setActiveCss("/resources/music/css/error.css");
		 }
	function setActiveCss(cssPath) {
	    var head = document.getElementsByTagName("head")[0];
	    var link = document.createElement("link");
	    link.rel = "stylesheet";
	    link.type = "text/css";
	    link.media = "screen";
	    link.href = resourceHost + cssPath;
	    head.appendChild(link);
	}
</script><link rel="stylesheet" type="text/css" media="screen" href="http://music.res.meizu.com/resources/music/css/error.css">
