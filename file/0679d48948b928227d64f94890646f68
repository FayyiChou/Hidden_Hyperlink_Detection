	$(".phone dd:first").addClass("block");
	$(".phone dt > span").each(function(index){
        $(this).click(function(){
            if (index==0) $('#phone_url').attr("href","/category/sai-0-0-0-0-0-0-1.html");
            else $('#phone_url').attr("href","/category/more.html");
                
            $(".phone dd").removeClass("block");
            $(".phone dt span").removeClass("crumb");
            $(".phone dd").eq(index).addClass("block");
            $(this).addClass("crumb");
        });
    });
    
    function newzx()
    {
         $(".new_zx1111").css("top",$(document).scrollTop()+$(window).height() * 0.50);
    }
