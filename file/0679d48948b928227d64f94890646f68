    <script type="text/javascript">
        $(document).ready(function() {
            var flag = 1;
            $('.index_shop_list').hover(function() {
                    flag = 0;
            }, function() {
                    flag =1;
            })
            var i = $('.index_shop_list li').length;
            $('.item2_r').bind("click", function() {
                    Fun2()
            });
            $('.item2_l').bind("click", function() {
                    Fun1();
            });
            function Fun0(i) {
                    $('.index_shop_list li').hide()
                    $('.index_shop_list li').eq(i).show();
                    $('.index_shop_list li').eq(i).next().show();
                    $('.index_shop_list li').eq(i).next().next().show();
                    $('.index_shop_list li').eq(i).next().next().next().show();
            }
            function Fun1() {
                    if (i <= 0) {
                            i = 40;
                            Fun0(i);
                    } else {
                            i = i - 3;
                            i--;
                            Fun0(i);
                    }
                    return (i);
            }
            function Fun2() {
                    if (i >= 36) {
                            i = 0;
                            Fun0(i);
                    } else {
                            i = i + 3;
                            i++;
                            Fun0(i);
                    }
                    return (i);
            }
            function timer() {
                    tt = setTimeout(timer, 2000);
                    if(flag == 1) Fun2();
            }
            timer();
                                
