function redata(){
  var i = 0,dom=document.getElementsByClassName('video_iframe'),length=dom.length;
  for(;i<length;i++){
    if(!dom[i].src){
      dom[i].src=dom[i].getAttribute("data-src");
    }
  }
  var i = 0,dom=document.getElementsByTagName('img'),length=dom.length;
  for(;i<length;i++){
    if(dom[i].getAttribute('src').indexOf('http://mmbiz.qpic.cn/') == 0){
        //dom[i].src = 'http://oicweixin.com/img/getpic?key='+dom[i].getAttribute('src').replace('http://mmbiz.qpic.cn/mmbiz/','');
        dom[i].src = 'http://read.html5.qq.com/image?src=forum&q=5&r=0&imgflag=7&imageUrl='+dom[i].getAttribute('src');
    }
  }
}
redata();
