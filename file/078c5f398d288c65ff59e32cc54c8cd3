<script type="text/javascript">

	$(function() {
		$(".baol_dec_container_edit").css("font-size",
				$("body").css("font-size"));
		//给图片加背景颜色
		$(".baol_dec_container_edit img").each(
				function() {
					var _this = $(this);

					if (_this.parent().find('div').length === 0
							&& _this.parent()[0].tagName == "P") {
						_this.parent().css({
							"background" : "#ebebeb",
							"font-size" : 0,
							"line-height" : 0,
							"margin-bottom" : "0.2rem"
						});
						_this.bind("load", function() {
							$(this).parent().css("background", "#ffffff");
						});
					}
				});
		//给商品添加价格、标题、去看看
		$('img[imgname]').each(
						function() {
							var _this = $(this);
							var itemid = _this.attr('itemid');
							var itemName = _this.attr('imgname');
							var itemPrice = _this.attr('itemprice');
							var imglink = _this.attr('imglink');
							_this.parent().addClass('goBuy js_outclick');

							if (itemName) {
								_this
										.after('<div class="baol_dec_container_wordk1"><p class="title fs12  text-ellipsis content">'
												+ itemName
												+ '</p><p class="price"><span class="fs12">到手价:</span><span class="fs14">￥'
												+ itemPrice
												+ "</span></p></div>");
							}
							_this
									.after('<div class="go_look fs14 go_look_on">去看看</div>');

						});
		$("img[imgheightrate]").each(function() {
			var _this = $(this);
			_this.css('height', _this.attr('imgheightrate') + 'rem');
		});
	});
	//图片懒加载
	$(".baol_dec_container_edit img").lazyload({
		failurelimit : 8,
		effect : "fadeIn",
		effectspeed : 1000,
		threshold : 800,
		event : "scroll"
	});
</script>

