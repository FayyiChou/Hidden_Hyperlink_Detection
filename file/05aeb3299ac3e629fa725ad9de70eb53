<script src="http://www.guoxinxinrong.com:80/webpage/guosen/js/swiper.min.js"></script>
<script type="text/javascript">
  var mySwiper = new Swiper('.swiper-container',{
    loop: true,
  autoplay: 3000,
  pagination : '.swiper-pagination',
  paginationClickable :true,
  }); 
</script>
<script>
    var lastPosition = 0;
    
    var cloud = document.getElementById("line");
    var ul = document.getElementById("navBar");
    var lis = ul.children;
   
    for (var i = 0; i < lis.length; i++) {
       
        lis[i].onmouseover = function () {
           
            //目标是 当前li的offsetLeft
            var target = this.offsetLeft;
            animate(cloud, target);
        }
        //鼠标离开li 还要 渐渐地 回去
        lis[i].onmouseout = function () {
            animate(cloud, lastPosition);
        }
        //点击当前li 记录位置
        lis[i].onclick = function () {
            lastPosition = this.offsetLeft;
        }
    }


    function animate(obj, target) {
        clearInterval(obj.timer);//防止重复调用
        obj.timer = setInterval(function () {
            //var target = 400;
            var leader = obj.offsetLeft;
            var step = (target - leader) / 5;
            step = step > 0 ? Math.ceil(step) : Math.floor(step);
            if (leader != target) {
                leader = leader + step;
                obj.style.left = leader + "px";
            } else {
                clearInterval(obj.timer);
            }
        }, 15);
    }
</script>
