$(document).ready(function (){
    
    var Comid = $("#hid_Comid").val();
    var Tradeid = $("#pub_Tradeid").val();
   
    //公司指数
    $.ajax({
        url: "../Template/Company_Num.aspx?Comid="+ Comid,
        type: "post",
        datatype: "text",
        success: function (returnValue) {
            if (returnValue != "" && returnValue != null) {
                $("#comNums").html(returnValue);
            }
        }
    })
    
});
