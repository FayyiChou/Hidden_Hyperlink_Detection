function PageReset(){
 location.href=window.location.href;
}
function HideOrShowMenu(){
 if(document.getElementById("menuDocument").style.display=="none"){
  document.getElementById("menuDocument").style.display="block";
 }else{
  document.getElementById("menuDocument").style.display="none";
 }
}

