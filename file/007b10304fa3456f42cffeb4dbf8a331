			alert(menuLiArray[i]);
			document.getElementById(menuLiArray[i]).className="";
			//document.getElementById("hengMenu_div").style.display='none';
		}
	}
	
	
	function onMouseOver_li(obj,menu_id){
		if(menu_id!=menu_str){
			obj.className="on";
			obj.getElementsByTagName("div")[0].style.display='block';
		}
	}
	
	function onMouseOut_li(obj,menu_id){
		if(menu_id!=menu_str){
			obj.className="";
			obj.getElementsByTagName("div")[0].style.display='none';
		}
	}


function menuShow(id,state){
 	if(state == 1){
	  	  $("#menu_nav").css("display", "none");
	  	  $("#moremenu").toggleClass("nav_more nav_more1");
 	}else{
 	  var position = $("#nav_more_lottery").position();
	  	  $("#menu_nav").css("display", "block");
	  	  $("#menu_nav").css("left", (position.left-48) + "px");
	      $("#menu_nav").css("top", (position.top+27) + "px");
	  	  $("#moremenu").toggleClass("nav_more1 nav_more");
 	}
} 

