    var ChangePageModel = (function () {
        function changePage() {
            var value = document.getElementById("SelectPageIndex").value;
            var urlStr = "http://baa.m.yiche.com/captiva/index-all-0-{0}-0.html";

            window.location.href = urlStr.replace("{0}", value);
        }
        return {
            ChangePageFun: changePage
        }
    })();

    document.getElementById("SelectPageIndex").onchange = ChangePageModel.ChangePageFun;
</script>

