        $(document).ready(function () {
            $('img[_src][lazyloaded!="true"]').trigger('lazyload', this);
        });

        //养护相关
        function YanghuFun(jsonData) {
            if (jsonData && jsonData.SerialID) {
                $('.yanghu-line').show();
                var yanghuA = $("#yanghuA");
                var moreUrl = jsonData.MoreMUrl;
                var gotopageUrl = "http://baa.bitauto.com/statistics/GoToUrl.ashx?t=" + encodeURIComponent(moreUrl) + "&a=r";
                yanghuA.attr('href', gotopageUrl);
                var name = jsonData.SerialName;
                if (jsonData.Prices.length > 0) {
                    var price = jsonData.Prices[0];
                    name = name + price.MaintainTypeName + price.Price + "元起";
                }
                yanghuA.html(name);
            }
        }
        
        document.write("<script src='http://i.yanghu.yiche.com/goods/maintainprice.ashx?cityID=" + bit_locationInfo.cityId + "&serialID=3357&callback=YanghuFun'><\/script>"); 
        

        var GlobalTopTopicTopCount =5
        //打开关闭公告;
        window.onload = HideGlobalTopic();
        function HideGlobalTopic() {
            var globalTopicCount = 0;
            document.getElementById('HideGlobalTopicBtn').style.display = "none";
            document.getElementById('ShowGlobalTopicBtn').style.display = "block";
            for (var i = 0; i < GlobalTopTopicTopCount; i++) {
                var id = "ctl00_main_globalTopTopicListRpt_ctl0" + i.toString() + "_topicListItem_imagesDiv";
                if (i > 1 && document.getElementById(id) !== null) {
                    var theElementNode = document.getElementById(id).parentNode.parentNode;
                    theElementNode.style.display = "none";
                    globalTopicCount++;
                }
            }
            //globalTopicCount沿用了index的逻辑，从0开始；此处逻辑表示置顶帖少于3条，隐藏按钮
            if (globalTopicCount <= 1) {
                document.getElementById('ShowGlobalTopicBtn').style.display = "none";
            }
        }
        function ShowGlobalTopic() {
            document.getElementById('HideGlobalTopicBtn').style.display = "block";
            document.getElementById('ShowGlobalTopicBtn').style.display = "none";
            for (var i = 0; i < GlobalTopTopicTopCount; i++) {
                var id = "ctl00_main_globalTopTopicListRpt_ctl0" + i.toString() + "_topicListItem_imagesDiv";
                if (i > 1 && document.getElementById(id) !== null) {
                    var theElementNode = document.getElementById(id).parentNode.parentNode;
                    theElementNode.style.display = "block";
                }
            }
        }

        var collectForum = function (obj) {
            dcsMultiTrack('DCS.dcsuri', '/car/onclick/mListCollect.onclick', 'WT.ti', 'm列表-收藏');
            $.get('ajax/CollectForums.ashx?seed=' + Math.random(), null, function (resp) {
                if (resp == 1) {
                    obj.html("已收藏");
                    obj.addClass('btn-grayborder-white');
                }
            });
        };
