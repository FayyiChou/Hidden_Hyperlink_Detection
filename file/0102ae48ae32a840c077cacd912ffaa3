<script type="text/javascript">

//限时折扣倒计时
function limitBuyEndtime(){
    $(".timeRemaining").each(function(){
        var lxfday=$(this).attr("lxfday");//用来判断是否显示天数的变量
        var endtime = new Date($(this).attr("endtime")).getTime();//取结束日期(毫秒值)
        var nowtime = new Date().getTime();//今天的日期(毫秒值)
        var youtime = endtime-nowtime;//还有多久(毫秒值)
        var seconds = youtime/1000;
        var minutes = Math.floor(seconds/60);
        var hours = Math.floor(minutes/60);
        var days = Math.floor(hours/24);
        var CDay= days ;
        var CHour= hours % 24;
        var CMinute= minutes % 60;
        var CSecond= Math.floor(seconds%60);//
        if(endtime<=nowtime){
            $(this).html("已过期")//如果结束日期小于当前日期就提示过期啦
        }else{
            if($(this).attr("lxfday")=="no"){
                var divCon = "<label>剩余</label>"
                             +"<label class=\"countdown cDarkRed\">"+ CHour +"</label>"
                             +"<label>小时</label>"
                             +"<label class=\"countdown cDarkRed\">"+ CMinute +"</label>"
                             +"<label>分</label>"
                             +"<label class=\"countdown cDarkRed\">"+ CSecond +"</label>"
                             +"<label>秒</label>";
                $(".timeRemaining").html(divCon);
            }else{
                var Hours = parseInt(days*24)+ parseInt(CHour);
                var divCon = "<label>剩余</label>"
                             +"<label class=\"countdown cDarkRed\">"+ Hours +"</label>"
                             +"<label>小时</label>"
                             +"<label class=\"countdown cDarkRed\">"+ CMinute +"</label>"
                             +"<label>分</label>"
                             +"<label class=\"countdown cDarkRed\">"+ CSecond +"</label>"
                             +"<label>秒</label>";
                $(".timeRemaining").html(divCon);
            }
            // 下一秒再次执行
            setTimeout("limitBuyEndtime()",1000);
        }
    });
}

function formatFloat(src, pos) {
    src = src + "";
    if(src.indexOf(".") > -1){
        src = src.substring(0, src.indexOf("."));
    }
    var f_x = parseFloat(src);
    if (isNaN(f_x)) {
        return false;
    }
    var f_x = Math.floor(parseFloat((src*Math.pow(10, pos)).toFixed(1)))/Math.pow(10, pos);
    
    var s_x = f_x.toString();
    var pos_decimal = s_x.indexOf('.');
    if (pos_decimal < 0) {
        pos_decimal = s_x.length;
        s_x += '.';
    }
    while (s_x.length <= pos_decimal + 2) {
        s_x += '0';
    }
    return s_x;
}

jQuery(function($) {
    var favorType = 0;
    var disccount = 8;
    var discountPrice = 0/1000;
    var numDepositPrice = 0/1000;
    var favorablePrice = 1200000/1000;
    var variablePrice = favorablePrice - numDepositPrice;
    
    var dicPrice = 0;
    if(favorType == "0"){ // 折扣
        dicPrice = variablePrice * (parseFloat(disccount/10)) + numDepositPrice;
        
    } else if(favorType == "1") { // 直降
        var diffPrice = variablePrice - discountPrice;
        if(diffPrice > 0) {
            dicPrice = diffPrice + numDepositPrice;
        } else {
            dicPrice = numDepositPrice;
        }
    }
    
    var actPrice = formatFloat(dicPrice,2);
    $("#favorablePrice").text(actPrice);
    limitBuyEndtime();
});
</script>
