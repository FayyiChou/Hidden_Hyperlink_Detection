    <script type="text/javascript">
        $(function(){
            for(var i=0; i<keyWords.length; i++){
                var keyword = keyWords[i];
                $("span#hotSearch a").eq(i).bind('click',{key: keyword},function(event){
                    if(/^http\:/.test(event.data.key.href)){
                        window.open(event.data.key.href)
                    }else{
                        toSearch(event.data.key.key);
                    }
                    return false;
                });
            }
            
            // 搜索提示
            
            $("#searchHeadBox").autocomplete(MallExtAppPath.search + "/suggest", {
                width: 367,
                max: 10,
                usehighlight: true,
                scroll: false,
                selectFirst:false,
                parse: function (data) {
                    var parsed = [];
                    var rows = data;
                    for (var i=0; i < rows.length; i++) {
                        var row = $.trim(rows[i]);
                        if (row) {
                            row = row.split("|");
                            parsed[parsed.length] = {
                                data: row,
                                value: row[0],
                                result:row[0]
                            };
                        }
                    }
                    return parsed;
                }
            });
        });
    </script>
