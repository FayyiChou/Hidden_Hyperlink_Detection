      <div class="timer">
        <ul class="timer-box">
          <li id="day">05</li>
          <li id="hours" style="*padding-left:10px;">15</li>
          <li id="mins" style="margin-right:30px;*padding-left:10px;">49</li>
          <li id="seconds" style="margin-right:0;*padding-left:10px;">08</li>
