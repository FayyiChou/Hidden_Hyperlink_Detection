<script language="JavaScript" type="text/javascript">
function LTrim(str){ 
        var i;
        for(i=0;i<str.length; i++) {
            if(str.charAt(i)!=" ") break;
        }
        str = str.substring(i,str.length);
        return str;
    }// 去掉字符串的尾空格（右空格）
    function RTrim(str){
        var i;
        for(i=str.length-1;i>=0;i--){
            if(str.charAt(i)!=" ") break;
        }
        str = str.substring(0,i+1);
        return str;
    }// 去掉字符串的头尾空格（左右空格）
    function Trim(str){
        return LTrim(RTrim(str));
    }
function set_SelectListBg(obj,SelectName,SelectBg,SelectList){
	var a_list = document.getElementsByName(SelectList);
	for(i=0; i<a_list.length; i++){
		if(a_list[i].getAttribute("checkTag") == "true"){
		    a_list[i].setAttribute("checkTag", "false");
		}
	}
	obj.setAttribute("checkTag", "true");
	if(SelectName=="useAreaNum"){
	var areaNum = 0;
	if(Trim(obj.innerHTML.substring(0,8))=="区域")
	areaNum = 1;
	document.getElementById("CardFrontSearch."+SelectName).value = areaNum;
	}else{
	var feeNum = 1;
	if(SelectName=="annualFee"){
	 if(Trim(obj.innerHTML.substring(0,8))=="150~300元")
	  feeNum = 2;
	  else if(Trim(obj.innerHTML.substring(0,8))=="300元以上")
	  feeNum = 3;
	  document.getElementById("CardFrontSearch."+SelectName).value = feeNum;
	}else{
	document.getElementById("CardFrontSearch."+SelectName).value = Trim(obj.innerHTML.substring(0,8));
	}
	}
	document.getElementById(SelectName).innerHTML = obj.innerHTML.substring(0,8);
	document.getElementById(SelectBg).style.display = 'none';
}
function get_SelectListBg(SelectName,SelectBg,SelectList){
	var a_list = document.getElementsByName(SelectList);
	for(i=0; i<a_list.length; i++){
		if(a_list[i].getAttribute("checkTag") == "true"){
			return a_list[i].getAttribute("dispCtgId");
		}
	}
	return "";
}
