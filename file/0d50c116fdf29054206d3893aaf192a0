<script>

var userInfo;
var time = new Date().getTime();

function callback(data){
	userInfo = data.data;
	if(userInfo){
		if(userInfo.isLogin == 1){
			var msgCount = userInfo.msgCount || 0;
			if(msgCount >99){
				$("#topbar_unread_msg").html("99+");
			}else{
				$("#topbar_unread_msg").html(userInfo.msgCount);
			}
			
			if(msgCount > 0){
				$("#user_msg").attr("href", "https://www.scrcu.com/ebccweb/baseInteractInfo.xhtml?isread=0");
			}
			var picUrl=userInfo.picUrl;
			var url="http://img.scrcu.com/fileServer/"
			if(picUrl==null || picUrl=="" || picUrl=="NULL" ){
			picUrl="/r/cms/www/public/upload/logo_def.png"
			}else{
			picUrl=url+picUrl;
			}
			$(".topbar_username").text(userInfo.userName);
			$("#active_level").html("<i>V</i>" + userInfo.activeLevel);
			$("#topbar_user_pic").prop("src",picUrl+"?w=67&h=67")
			$(".unlogin_element").hide();
			$('.login_element').show();
		}else{
			$(".unlogin_element").show();
			$('.login_element').hide();
		}
	}
}

$(function(){
	if(window.location.pathname != "/"){
		$("#back_to_index").show();
		$("#back_to_index_h").hide();	
	}else{
		$("#back_to_index").hide();
		$("#back_to_index_h").show();
	}
	

	$.ajax({
		url:"https://www.scrcu.com/private/ajax/topbarUserInfo.xhtml?t="+time,
		dataType:"jsonp",
		type:"get",
		async: false,
		complete:function(xhr, ts){
			if(xhr.status != 200){
				$.ajax({
					url:"http://www.scrcu.com/private/login/removecookie.xhtml",
					type:"get",
					dataType:"text",
					async: false,
					success: function(data){
						
					}
				});			
			}
		}
	});

	$('#userName').on('mouseenter',function(){
 		if($('.user_dropDown').is(":animated")) return false;
 		$('.user_dropDown').slideDown();
 	});

 	$('.user_dropDown').on('mouseleave',function(event) {
 		if($('.user_dropDown').is(":animated")) return false;
 		$(this).slideUp();
 	});

 	$('#total_nav_btn').on('click',function(){
		$('.total_nav').slideToggle();
	})
	$('html').on('click',function() {
		$('.total_nav').slideUp();
 	});
	$('#total_nav_btn').on('click',function(){
		return false;
	});
	
   if($('#tobar_pixie').val()=="1"){
	$(".online_pet").css("visibility","hidden");
	$(".online_pet").text("");
	$(".online_pet").attr("class","border_l");
	}
	
});
