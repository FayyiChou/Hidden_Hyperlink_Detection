<script>

	 $(function(){

			//重要公告弹窗
			var id = $("#ggId").val();
			var releaseDateStr = $("#ggreleaseDate").val();
			var endDateStr = $("#ggendDate").val();
			var expiresDate= new Date('2100/01/01 00:00:00');
			if(id!=undefined){
				if(($.cookie("gg"+id))!=1){
					if(releaseDateStr!=""&&releaseDateStr!=null){
						var nowDate = new Date();
						var sysTime = getGgSysTime();
						if(sysTime!=""){
							nowDate = new Date(sysTime.replace(/-/g,"/"));
						}
						var startDate = new Date(releaseDateStr.replace(/-/g,"/"));
						if(endDateStr!=""&&endDateStr!=null){
							var endDate = new Date(endDateStr.replace(/-/g,"/"));
							if(startDate<nowDate&&endDate>nowDate){
								popBox('.notice_popup');
								$.cookie("gg"+id,1,{path:'/',expires : expiresDate});
							}

						}else{
							if(startDate<nowDate){
								popBox('.notice_popup');
								$.cookie("gg"+id,1,{path:'/',expires : expiresDate});
							}
						}

					}



				}
			}



	 });



function getGgSysTime(){
	var times = "";
	$.ajax({
	 	   type: "POST",
	 	   url: "/public/branchesMap/getTime.xhtml",
	 	   async:false,
	 	   datatype : "json",
	 	   success: function(obj){
	 		    if(obj!=""){
		 	    	var data = eval('('+obj+')');
		 	    	times = data.time;
	 		    }else{
	 		    	times = "";
	 		    }
	 	   },
	 	   error:function(){
	 		  times =  "";
	 	  }

	 	});
    return times;
}


