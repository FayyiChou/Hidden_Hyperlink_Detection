<script>
/**删除cookie方法*/
function goto_other_o(){
		var hrefother=$('#hrefother').val();
		/**删除cookie*/
		$.cookie('_sub_site_list_cookie', '', { expires: -1 }); 
		$('#other_id_o').attr('href',hrefother);
		$('#other_id_o').click();
};


/*搜索记忆关键字和搜索类型*/

var cookies = document.cookie.split(";");
for(var i=0; i<cookies.length; i++) {
	
	if(cookies[i].indexOf("search_keyword")!=-1) {
                
		var keyvalues = cookies[i].split("=");
		var search_keyword = keyvalues[1];
		if(search_keyword != undefined && search_keyword != "" && search_keyword != null) {
                         
			$("#keywrd").val(decodeURIComponent(search_keyword));
		}
	}else if(cookies[i].indexOf("search_type")!=-1) {
                
		var keyvalues = cookies[i].split("=");
		var search_type = keyvalues[1];
		if(search_type != undefined && search_type != "" && search_type != null) {
                       
			$("#coustom_select").text(decodeURIComponent(search_type));
		}
	}
	
}




/*自定义下拉菜单*/
var channelId="";
$(".coustom_select").each(function(){
		var s=$(this);
		var z=parseInt(s.css("z-index"));
		var dt=$(this).children("dt");
		var dd=$(this).children("dd");
		var _show=function(){dd.slideDown(200);dt.addClass("cur");s.css("z-index",z+1);};   //展开效果
		var _hide=function(){dd.slideUp(200);dt.removeClass("cur");s.css("z-index",z);};    //关闭效果
		dt.click(function(){ dd.is(":hidden")?_show():_hide();});
		dd.find("a").click(function(){
			dt.html($(this).html());_hide();
			channelId = $(this).html();
			if(channelId=="理财超市"){
				channelId=1;
			}else{
				channelId=2;
			}
		});     //选择效果（如需要传值，可自定义参数，在此处返回对应的“value”值 ）
		$("body").click(function(i){ !$(i.target).parents(".coustom_select").first().is(s) ? _hide():"";});
});




