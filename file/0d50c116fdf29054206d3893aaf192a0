<script type="text/javascript">
 hasBaiduMap = false;
 $(function(){
    var expiresDate= new Date('2100/01/01 00:00:00');
    var cookies = document.cookie.split(";");
    var isflg=true;
	for(var i=0; i<cookies.length; i++) {
		if(cookies[i].indexOf("is_mobile")!=-1) {
		isflg=false;	
	   }
	 }   
	   
	   if(isflg){
	   var browser = { 
		        versions : function() { 
		            var u = navigator.userAgent, app = navigator.appVersion; 
		            return {                                                                                              
		            mobile : !!u.match(/AppleWebKit.*Mobile.*/) || !!u.match(/AppleWebKit/), /* 是否为移动终端 */                                  
		            ios : !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), /* ios终端  */                 
		            android : u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, /* android终端或者uc浏览器    */                               
		            iPhone : u.indexOf('iPhone') > -1,/*是否为iPhone或者QQHD浏览器 */                     
		            iPad: u.indexOf('iPad') > -1, /* 是否iPad */        
		            webApp : u.indexOf('Safari') == -1,//是否web应该程序，没有头部与底部 
		        }; 
		        }(), 
		    } 
		    var domain= $('#domain_down').attr("value");
	     if(browser.versions.ios || browser.versions.iPhone){
	      $.cookie("is_mobile","1",{path:'/',expires : '-1'});  
	      window.location.href="http://"+domain+"/r/cms/www/public/appdown/app_down_phone_1.html";
	     }
	     if(browser.versions.android){
	      $.cookie("is_mobile","1",{path:'/',expires : '-1'});  
	      window.location.href="http://"+domain+"/r/cms/www/public/appdown/app_down_phone_2.html";
	     }
	   
	   }
  
	 /*右侧快速导航*/
	$(window).scroll(function() {
		var e = $("#tbox"),
		t = $(document).scrollTop();
		var t = $(document).scrollTop();
		t > 1000 ?  e.fadeIn("slow") :e.fadeOut("slow")

		var scrollTop = $(window).scrollTop();
		$(".row").each(function() {
			var top = (Math.floor($(this).offset().top))-50;
			if(scrollTop < top + $(this).height()){
					$('.floor').removeClass('activeBg');
					var id = $(this).find('h2').attr('id');
					$('.floor[title=' + id.split('_')[1] + ']').addClass('activeBg');
					return false;
			}
        });


		//延迟1s加载百度地图
		var mapPos = $(window).scrollTop();
		if($("#index_3").length > 0){
			if(mapPos>=$("#index_3").offset().top-150&&mapPos<$("#index_3").offset().top+50) {
				if(!hasBaiduMap){
					initBaiduMap('成都市');
				}

			}
		}


	});


   $(".floor").click(function(event) {
      var index=this.title;
      var id='#'+'index_'+index;
     $("html,body").animate({scrollTop: $(id).offset().top-50}, 1000);
   });
   $(".goto_top").click(function(){
	   $("html,body").animate({scrollTop: 0},"slow");
	})


	$('.qrcode_login').click(function(){
	 $('.logining_staus').hide();
	 $('.qrcode_staus').show();
	})
	$('.pc_login').click(function(){
	 $('.logining_staus').show();
	 $('.qrcode_staus').hide();
	})
	$('.login_btn').click(function(){
	 $('.logining_staus').hide();
	 $('.logined_staus').show();
	})
	$('.logout_btn').click(function(){
	 $('.logining_staus').show();
	 $('.logined_staus').hide();
	})
	//判断验证码是否可见，按钮的边距宽度
    if($('.verify_row').is(":visible")) {
        $('.login_btn').addClass('login_btn_small');
     }
     else {
        $('.login_btn').removeClass('login_btn_small');
     }
	$('.use_help_btn').mouseenter(function(){
	 $('.big_eqcode_img').fadeOut();
	 $('.use_help_img').fadeIn();

	})
	$('.use_help_btn').mouseleave(function(){
	 $('.use_help_img').fadeOut();
	 $('.big_eqcode_img').fadeIn();
	})

	/////////////////////活动专题///////////////////////
	var sWidth = $("#activiy_focus").width(); //获取焦点图的宽度（显示面积）
	var len = $("#activiy_focus ul li").length; //获取焦点图个数
	var index = 0;
	var picTimer;

	//以下代码添加数字按钮
	var btn = "<div class='btn'>";
	for(var i=0; i < len; i++) {
		btn += "<span></span>";
	}
	btn += "</div>";
	$("#activiy_focus").append(btn);

	//为小按钮添加鼠标滑入事件，以显示相应的内容
	$("#activiy_focus .btn span").mouseenter(function() {
		index = $("#activiy_focus .btn span").index(this);
		showPics(index);
	}).eq(0).trigger("mouseenter");

	//计算出外围ul元素的宽度
	$("#activiy_focus ul").css("width",sWidth * (len));

	//鼠标滑上焦点图时停止自动播放，滑出时开始自动播放
	$("#activiy_focus").hover(function() {
		clearInterval(picTimer);
	},function() {
		picTimer = setInterval(function() {
			showPics(index);
			index++;
			if(index == len) {index = 0;}
		},3000);
	}).trigger("mouseleave");

	//显示图片函数，根据接收的index值显示相应的内容
	function showPics(index) {
		var nowLeft = -index*sWidth;
		$("#activiy_focus ul").stop(true,false).animate({"left":nowLeft},300);
		$("#activiy_focus .btn span").stop(true,false).removeClass("on").eq(index).stop(true,false).addClass("on");
	}
	/////////////////////活动专题///////////////////////


  })


	function initBaiduMap(city){
	    hasBaiduMap = true;
	    var baidumap = new BMap.Map("allmap"); // 创建Map实例
	    baidumap.enableScrollWheelZoom();
	    baidumap.addControl(new BMap.NavigationControl());//添加平移缩放控件
	    baidumap.addControl(new BMap.ScaleControl());//增加比列尺控件
	    baidumap.addControl(new BMap.OverviewMapControl());
	    baidumap.addControl(new BMap.MapTypeControl());
		/* map.centerAndZoom(city,12);
		map.addEventListener("click", function (e) {
			 window.location.href="/branchesMap/index.xhtml?fir=1";
		}); */
		function myFun(result){
			var cityName = result.name;
			//map.setCenter(cityName);
			baidumap.centerAndZoom(cityName,12);
			baidumap.addEventListener("click", function (e) {
				window.open("/public/branchesMap/index.xhtml?cityName="+encodeURIComponent(cityName));
			});

		}
		var myCity = new BMap.LocalCity();
		myCity.get(myFun);
 	}
 	function wjdc_lj(){
 		alert("目前暂无问卷调查,感谢关注!");
 	};
</script>

