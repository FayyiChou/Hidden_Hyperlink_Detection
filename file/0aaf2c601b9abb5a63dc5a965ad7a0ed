            var slhLawyer = $("#list_lawyer").on("mouseenter", ">", function() {
                $(this).addClass("recom-hover");
            }).on("mouseleave", ">", ".answer-list", function() {
                $(this).removeClass("recom-hover");
            }).setListHtml().on("setListItem", function($item, oM) {
                var $list = $item.children();
                $list.first().attr("href", oM.LawyerId > 0 ? "/lawyer/{0}/".format(oM.LawyerId) : oM.Url).children().attr("src", oM.HzImg);
                if (!$list.first().attr("href")) {
                    $list.first().attr("href", "javascript:void(0);").removeAttr("target");
                } else {
                    $list.first().attr("target", "_blank");
                }
                if (oM.LawyerId > 0) {
                    $list.first().addClass("recom-photo");
                    $list.eq(1).show();
                    $list.eq(2).show();
                    $list.eq(3).show();
                    $list.eq(4).show();
                    $list.last().show();
                    $list.eq(1).children("a").attr("href", "/lawyer/{0}/".format(oM.LawyerId)).children("i").toggle(oM.IsCooperation).end().contents().filter(function() {
                        return this.nodeType == 3;
                    }).get(0).nodeValue = oM.LawyerId > 0 ? "{0}律师".format(oM.LawyerName) : "";
                    $list.eq(2).html("{0} {1}".format(oM.ProvinceName, oM.CityName));
                    $list.eq(3).hide();
                    $list.eq(3).html("解答{0}个咨询".format(oM.HelpCount)).show();
                    $list.eq(4).children("i").removeClass().addClass(oM.CategoryIcon).addClass("ico-skill mr5").end().contents().filter(function() {
                        return this.nodeType == 3;
                    }).last().get(0).nodeValue = "擅长{0}".format(oM.CategoryName);
                    $list.last().children().first().attr("href", "/ask/ask.aspx?ly={0}".format(oM.LawyerId)).end().last().attr("href", "/zhenduan/xinxi_{0}.aspx".format(oM.LawyerId));
                } else {
                    $list.first().removeClass();
                    $list.eq(1).hide();
                    $list.eq(2).hide();
                    $list.eq(3).hide();
                    $list.eq(4).hide();
                    $list.last().hide();
                }
            }).on("getData", function() {
                var _ = this;
                var oM = oInit.get("oM");
                window.Ajax("/ajax/question.aspx", { Type: 6, pid: '330000', cid: '542500', tid: oM.TopSpecialtyId, ps: 3 }, function(oData) {
                    if (oData && oData.Result) {
                        _.setData(oData.Data, true);
                    }
                });
            });

            var slhSpecial = $("#list_special").setListHtml().on("setListItem", function($item, oM) {
                $item.prev().removeClass("borb0");
                $item.children().first().html(new window.DateTime(oM.AddTime).toString("yyyy-MM-dd")).end().last().attr({
                    href: "http://www.64365.com/special/{0}/".format(oM.EnName),
                    title: oM.Title,
                }).html(oM.Title);
            }).on("setListEnd", function(aData) {
                if (!aData || aData.length <= 0) {
                    return;
                }
                this.get("$").parent().slideDown();
            }).on("getData", function() {
            });

            var slhNewQuestion = $("#list_newQuestion").setListHtml({
                $list: function() {
                    return this.get("$").children(".h20");
                }
            }).on("setListItem", function($item, oM) {
                var $list = $item.hide().children();
                $list.first().children().first().html(oM.UserName).end().last().html(new window.DateTime(oM.AddTime).toString("yyyy-MM-dd :HH:mm:ss"));
                $list.last().attr("href", "/ask/{0}.aspx".format(oM.QuestionId)).html(oM.Title);
            }).on("setListEnd", function() {
                this.trigger("roll");
            }).on("roll", function() {
                var _ = this;
                var $list = _.get("$list", true);
                var $show = $list.filter(":visible:first").next();
                if ($show && $show.length > 0) {
                    $show.prev().slideUp();
                } else {
                    $show = $list.first();
                    $list.filter(":visible").slideUp();
                }
                $show.slideDown();
                window.setTimeout(function() {
                    _.trigger("roll");
                }, 3000);
            }).on("getData", function() {
                var _ = this;
                window.Ajax("/ajax/question.aspx", { Type: 7, tid: oInit.get("oM").TypeId, ps: 5 }, function(oData) {
                    if (oData && oData.Result) {
                        _.setData(oData.Data);
                    }
                });
            });

            var vContent = new window.ElementVerification({
                name: "Content",
                verification: function(v) {
                    var iMinLength = 10;
                    var iChineseMinLength = 10;
                    var bFlag = false;
                    if (!v.valueOf) {
                        window.showMsg("请描述您的问题");
                    } else if (!v.isLength(iMinLength)) {
                        window.showMsg("请填写{0}个字以上的问题描述".format(iMinLength));
                    } else if (!v.isCount(iChineseMinLength, null, 8)) {
                        window.showMsg("请填写{0}个汉字以上的问题描述".format(iChineseMinLength));
                    } else if (v.isMobilePhone(true)) {
                        window.showMsg("问题描述中含有疑似联系方式");
                    } else {
                        bFlag = true;
                    }
                    this.trigger("verificationEnd", bFlag);
                }
            });

            var oInit = $.extend({}, window.IDataClass, window.IEventClass).set("$", $("#list_reply").on("mouseenter", ".answer-list", function() {
            })).on("init", function() {
                var _ = this;
                _.set("oM", {"QuestionId":918681,"ProvinceId":370000,"CityId":372300,"TypeId":117,"Support":null,"Views":27,"TopSpecialtyId":"58","ArticleTypeId":"478"});
                var $list = _.get("$").children();
                $list.filter(".operate").children().first().each(function() {
                    $(this).on("click", function() {
                        _.trigger("addSupport", $(this));
                    });
                });
                this.trigger("addViews");
                slhLawyer.trigger("getData");
                slhSpecial.trigger("getData");
            }).on("addViews", function() {
                var oM = this.get("oM");
                window.Ajax("/ajax/question.aspx", { Type: 5, id: this.get("oM").QuestionId }, function(oData) {
                    if (oData && oData.Result) {
                        oM.Views += 1;
                    }
                }, {
                    type: "POST",
                });
            }).on("addSupport", function($this) {
                var _ = this;
                var oM = _.get("oM");
                var cName = "askSupport{0}".format(oM.QuestionId);
                if (cookie.get(cName) > 0) {
                    window.showMsg("您已赞过");
                } else {
                    if ($this.data("loading")) {
                        window.showMsg("正在处理……");
                        return;
                    }
                    window.Ajax("/ajax/question.aspx", { Type: 4, id: oM.QuestionId }, function(oData) {
                        if (oData && oData.Result) {
                            var dNow = new Date();
                            dNow.setFullYear(dNow.getFullYear() + 3);
                            cookie.set(cName, 1, dNow);
                            oM.Support += 1;
                        } else {
                            window.showMsg("网络繁忙");
                        }
                    }, {
                        type: "POST",
                        beforeSend: function() {
                            var iSupport = oM.Support;
                            $this.next().stop(true).css({ bottom: "30px", opacity: "1" }).show().animate({ bottom: "+=20px", opacity: "0" }, 1500, function() {
                                $this.children(":last").html("({0})".format(iSupport + 1));
                            });
                            $this.data("loading", true);
                        },
                        complete: function() {
                            $this.data("loading", false);
                        },
                        error: function() {
                            window.showMsg("点赞失败");
                            $this.next().html("({0})".format(oM.Support));
                        },
                        PostTimeout: 3000,
                    });
                }
            }).on("addQuestion", function() {
                var _ = this;
                vContent.check(function(oValue, bResult) {
                    if (bResult) {
                        if (+'0' > 0) {
                            if (vContent.get("loading")) {
                                window.showMsg("正在处理中……");
                                return;
                            }
                            window.Ajax("/ajax/question.aspx", { Type: 1, info: { Source: '11', Content: oValue } }, function(oData) {
                                if (oData && oData.Result) {
                                    var upOk = new window.UrlParam("/ask/askok.aspx");
                                    upOk.set("askid", oData.Data.QuestionId);
                                    upOk.set("typeid", oData.Data.TypeId);
                                    upOk.set("userid", oData.Data.UserId);
                                    upOk.set("asktitle", oData.Data.Title);
                                    upOk.set("isnewuser", oData.Data.IsNewUser);
                                    window.location.href = upOk.href;
                                } else {
                                    window.showMsg(oData && oData.Msg ? oData.Msg : "提交失败");
                                }
                            }, {
                                type: "POST",
                                beforeSend: function() {
                                    vContent.set("loading", true);
                                },
                                complete: function() {
                                    vContent.set("loading", false);
                                },
                                PostTimeout: 3000,
                            });
                        } else {
                            if (oValue.length > 200) {
                                alert("您输入的内容过长，请前往提问页详细描述");
                                window.open("http://www.64365.com/ask/ask.aspx");
                            } else {
                                var upAdd = new window.UrlParam("/ask/ask.aspx");
                                upAdd.set("c", oValue);
                                window.location.href = upAdd.href;
                            }
                        }
                    }
                });
            });

            $("#a_ask").on("click", function() {
                vContent.set("$", $("#form_content"));
                oInit.trigger("addQuestion");
            });
            
            $("#a_addAsk").on("click", function() {
                vContent.set("$", $("#form_text"));
                oInit.trigger("addQuestion");
            });
            
            oInit.trigger("init");
        });
    </script>   
