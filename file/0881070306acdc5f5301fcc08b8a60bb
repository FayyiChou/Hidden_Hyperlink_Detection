	<script>
        /*cookies操作方法*/
	 	function getCookie(name) {
			var cookie = null;
			var str = document.cookie.split(";");
			for (var i = 0; i < str.length; i++) {
				var str2 = str[i].split("=");
				if (str2[0].replace(/^ | $/g, '') == name) {
					cookie = unescape(str2[1]);
					break;
				}
			}
			return cookie;
		};
	   function setCookie(name, value, expires) {
			var cookieInfo = name + "=" + escape(value) + ";path=/";
			if (expires) {
				var exp = new Date();
				exp.setTime(exp.getTime() + expires * 24 * 60 * 60 * 1000);
				cookieInfo += ";expires=" + exp.toGMTString();
			}
			document.cookie = cookieInfo;
		};
        /*dom操作方法*/
	    function $$(id){
		  var elem =  document.getElementById (id);
		  elem.show = function(){
		     elem.style.display='block';
		  };
		  elem.hide = function(){
		     elem.style.display='none';
		  };
		  return document.getElementById(id);
		}
        /*定时器回调方法*/
	    function actvityClose() {
			if ($$("actvity-pic") != undefined) {
				$$("actvity-pic").hide();
			}
		}
        /*如果存在cookies中存在devunion_activity变量，说明离上次展示大图还没有过2小时，不再展示*/
		if(!getCookie("devunion_activity")){
            // 显示大图
			$$("actvity-pic").show();
            // 设置cookies失效时间为0.083天，即2小时
			setCookie("devunion_activity","no",0.083);
            // 6秒钟后隐藏大图
			setTimeout (actvityClose, 6000);
		}
		else{
            // 隐藏大图
			$$("actvity-pic").hide ();
		}	
	</script>
-->
