TJObject.et = "show";
TJObject.st = new Date().getTime();
if(TJObject.pt)
	tjMVT.setData(TJObject);

function tjClickEvent() {
	tjClick($(this).data("tj"));
}
function tjClick(p){
	TJObject.et = "click";
	if(typeof p == "string")
		p = JSON.parse(p);
	p = p || {};
	
	for(var i in p)
		TJObject[i] = p[i];
	TJObject.st = new Date().getTime();
	TJObject.idc = "hotel919";
	if(TJObject.pt)
		tjMVT.setData(TJObject);
}
$('.pages').off("click",".tjclick",tjClickEvent).on("click",".tjclick",tjClickEvent);
