<script>
	function addIframe(obj){
		var fcjyxtIfr = '<iframe frameborder="0" height="179" scrolling="no" src="http://210.75.213.165/aestate/inc/login3.asp?formStyle=1" width="260"></iframe>';
		if(obj.find("iframe").length != 1){
			obj.append($(fcjyxtIfr));	
		}
	}
	function tabSwitchFcjy(btn,box,btnclass){
	var btn = $(btn);
	var box = $(box);
	function mouseSwitch(i){
		btn.removeClass(btnclass).eq(i).addClass(btnclass);	
		box.hide().eq(i).show();
		if(i == 1){
			addIframe(box.eq(i));	
		}
	}
	mouseSwitch(0);
	btn.each(function(i){
		btn[i].onmouseover = function(){
			mouseSwitch(i);
		}
    });
}//选项卡
    tabSwitchFcjy("#bjjslogin h2 a", "#bjjslogin .login_box", "login_bg");
    showIpt($("#bjjslogin .ipt_txt"));
</script>
 
