$(function(){
         $("#xp").mouseover(function(){
 $(this).css('background','#e0e0e0');
 $("#zjc").hide();
 $("#cpc").hide();
 $("#xpc").show();                
        });
  $("#xp").mouseout(function(){
 $(this).css('background','#f6f6f6');           
        });
 $("#cp").mouseover(function(){
 $(this).css('background','#e0e0e0');
 $("#zjc").hide();
 $("#xpc").hide();   
 $("#cpc").show();    
 
        });
 $("#cp").mouseout(function(){
 $(this).css('background','#f6f6f6');           
        });
$("#zj").mouseover(function(){
 $(this).css('background','#e0e0e0');
  $("#xpc").hide();   
 $("#cpc").hide();
 $("#zjc").show();
                 
        });
  $("#zj").mouseout(function(){
 $(this).css('background','#f6f6f6');           
        });

});            
