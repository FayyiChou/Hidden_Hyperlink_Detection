    <script src="/js/v3/common.js"></script>
    <script>
        $(function () {

            //绑定历史记录
            xiaoxue.browsingHistory.bind();

            //列表搜索框
            $('#btnSearch').on('click', function () {
                var url = $('#hidURL').val();
                var kw = $('#tbxKeyWord').val();
                url += ("?filterkeyword=" + encodeURIComponent(kw));
                location.href = url;
            });

            //点击查看大图
            $(".softimg").each(function () {
                var fullsrc = $(this).attr("fullsrc");
                var imgs = fullsrc.split(",");
                var items = [];
                for (var i = 0; i < imgs.length; i++) {
                    items[items.length] = { src: imgs[i], type: "image" };
                }
                $(this).magnificPopup({
                    items: items,
                    gallery: {
                        enabled: true
                    },
                    type: 'image' // this is default type
                });
            });
           
            $(function () {

                //高亮显示关键字
                var keyword = $("#hidKeywords").val(); //获取要替换的关键字   
                if ($.trim(keyword).length == 0) {
                    return;
                }
                var reg;
                reg = new RegExp('(' + keyword + ')', 'ig');
                $(".softTitle").each(function () {
                    this.innerHTML = this.innerHTML.replace(reg, "<strong style='color:red'>$1</strong>");
                });
                $(".softIntro").each(function () {
                    $(this).html($(this).html().replace(reg, "<strong style='color:red'>$1</strong>"));
                });                
            });

            //设置导航选中的学科
            (function () {
                var subjectId = $("#currentSubject").val();
                if (subjectId && subjectId > 0) {
                    $("#Subject" + subjectId).addClass("blue");
                    if (subjectId > 3) {
                        $("#Subject_Other").css('color', '#26aef3');
                    }
                }
            })();            
        });

        xxxk.cmn.resNav();

        function goToPage(url) {
            var kw = $('#tbxKeyWord').val();
            location.href = url + (kw ? "&filterkeyword=" + encodeURIComponent(kw) : "");
        }

    </script>


</body></html>