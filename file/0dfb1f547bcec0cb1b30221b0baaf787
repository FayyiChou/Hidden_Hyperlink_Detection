<script>
function search_(id){
	keyword = document.getElementById(id).value;
	if(keyword=="请简述您要咨询的问题"||keyword==""){
		alert("搜索条件不能为空！");
	}else{
		 window.location.href="http://www.968966.net/guestbook/index.php?job=search&keyword="+keyword; 
	}
	
}
</script>        
