<script>
function searchpost(){
	var i = 0;
	wtbh = document.getElementById("wtbh").value;
	pwd = document.getElementById("pwd").value;
	if(wtbh==""||!wtbh){
		alert("受理编号不能为空！");
		i++;
	}else if(wtbh.substr(0,2)!="zx"&&wtbh.substr(0,2)!="zy"&&wtbh.substr(0,2)!="bs"&&wtbh.substr(0,2)!="ts"){
		alert("受理编号有误！");
		i++;
	}else if(wtbh.length!=8){
		alert("受理编号有误！");
		i++;
	}
	
	else if(pwd==""||!pwd){
		alert("查询密码不能为空！");
		i++;
	}else if(pwd.length!=6){
		alert("查询密码有误！");
		i++;
	}
	
	if(i){
		return false;
	}
}
</script>        	
