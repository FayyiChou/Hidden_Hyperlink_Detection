    <script>
        function InscriptionEmail() {
            var EmailAddr = $('#EmailAddr').val();
            var searchUrl = 'http://test/hotelSearch.aspx';

            if (EmailAddr && !checkInputEmail(EmailAddr)) {
                $('#EmailAddr').addClass(" Validform_error");
                // $('#EmailError').attr('style', 'display:block;');
                addErrorInfo($('#EmailError'), "")
                return;
            }
            else {
                $('#EmailAddr').removeClass(" Validform_error");
                $('#EmailError').attr('style', 'display:none;');
                $.ajax({
                    type: "post",
                    dataType: "json",
                    url: '../../Ajax/Customer/AjaxEmailInSubscription.aspx',
                    timeout: 30000,
                    data: { EmailAddr: $("#EmailAddr").val()
                    },
                    success: function (result) {
                        if (result.Data) {
                            //alert('订阅成功', '订阅成功', 'info');
                            showMsgWithMask('订阅成功。');

                        } else {
                            // alert('您已经订阅过了', '您已经订阅过了', 'info');
                            showMsgWithMask("您已经订阅过了！");
                        }
                        $("#EmailAddr").val('输入您完整的E-Mail地址');
                    },
                    error: function (XMLHttpRequest) {
                        //alert('订阅异常！', '订阅异常！', 'info')
                        showMsgWithMask("订阅异常！");
                    }
                });
            }
        }
        function showMsgWithMask(msg) {
            if ($("body").find("#blankShowmsg").length == 0) {
                $("body").append('<div class="blankShowmsg popWin" id="blankShowmsg"><div class="popWin_wrap"></div><a class="close" href="javascript:void(0)"></a></div>');
            }
            var pop_overlay = PopWin("#blankShowmsg");
            pop_overlay.fn.popIn();
            $.Showmsg("<p class='tc ie6png'>" + msg + "</p>");
            $("#Validform_msg").find(".Validform_close").bind("click", function () {
                pop_overlay.fn.popOut();
            });
        }


    </script>
</div>

