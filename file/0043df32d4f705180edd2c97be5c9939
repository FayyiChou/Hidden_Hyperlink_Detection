<script type="text/javascript">
function close_upapp_tip(){
	$("#bottomdl").hide();
	var expireDate = new Date();
	expireDate.setTime( expireDate.getTime() + ( 48 * 60 * 60 * 1000 ) );
	$.cookie('upapp_switch', '1', {expires:expireDate} );
}
$(function(){
	/* if ( $.cookie('upapp_switch') == '1' ) {
	$("#bottomdl").hide();
	}
	$(".shut").on("click",function(){
	close_upapp_tip();
	}); */
	$('.upapp').click(function(){
		close_upapp_tip();
		var guide = "";
		if (/ipad|iphone|mac/i.test(navigator.userAgent)){
			guide = 1;
		}else{
			guide = 2;
		}
		var ua = navigator.userAgent.toLowerCase();
		if(ua.match(/MicroMessenger/i)=="micromessenger") {
			$(this).attr('href','http://a.app.qq.com/o/simple.jsp?pkgname=com.jesson.meishi&g_f=991653');
		}else{
			if(guide==1){
				$(this).attr('href','http://i.meishi.cc/g/transit.php?ename=276&fc=wapbottomyin');
			}else{
				$(this).attr('href','http://i.meishi.cc/g/transit.php?ename=275&fc=wapbottomyin');
			}
		}
	});
	});
</script>
