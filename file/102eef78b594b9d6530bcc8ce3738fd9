        <script>
			$(function(){
				$('.index-news-right .content ul').width(642*$('.index-news-right .content li').length+'px');
				$(".index-news-right .tab a").mouseover(function(){
					$(this).addClass('on').siblings().removeClass('on');
					var index = $(this).index();
					number = index;
					var distance = -642*index;
					$('.index-news-right .content ul').stop().animate({
						left:distance
					});
				});
				
				var auto = 0;  //等于1则自动切换，其他任意数字则不自动切换
				if(auto ==1){
					var number = 0;
					var maxNumber = $('.index-news-right .tab a').length;
					function autotab(){
						number++;
						number == maxNumber? number = 0 : number;
						$('.index-news-right .tab a:eq('+number+')').addClass('on').siblings().removeClass('on');
						var distance = -642*number;
						$('.index-news-right .content ul').stop().animate({
							left:distance
						});
					}
					var tabChange = setInterval(autotab,5000);
					//鼠标悬停暂停切换
					$('.index-news-right').mouseover(function(){
						clearInterval(tabChange);
					});
					$('.index-news-right').mouseout(function(){
						tabChange = setInterval(autotab,5000);
					});
				  }  
				});
			</script>
    </div>
    
