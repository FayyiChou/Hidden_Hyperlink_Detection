    $(document).ready(function(){
        $(".cy_search_submit").removeAttr("id");
        $("#collectBtn").toggle(function(){
            $(".cy_topper .collect").css("display","block");
        },function(){
            $(".cy_topper .collect").css("display","none");
        })

    });
    $(function(){
        $(".cy_search_submit").click(function(){
            var key=$("#search_keywords").val();
            if(key==""){
                alert('搜索不能为空');
                return false;
            }
            var host="http://www.chuiyao.com/";
            var url=host+'search/'+key+'/';

            window.location.href=url;
            return true;
        })
        $(document).keydown(function(e){
            if(e.keyCode ==13){
                var key=$("#search_keywords").val();
                var host="http://www.chuiyao.com/";
                var url=host+'search/'+key+'/';
                if(key==""){
                    alert('搜索不能为空');
                    return false;
                }
                window.location.href=url;
                return true;
            }
        });
    })

