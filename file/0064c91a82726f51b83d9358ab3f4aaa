        function change_div(id) {
            if (id == "1") {                
                $('#new1').show();
                $('#new2').hide();
                $('#new3').hide();
                $("#item1").attr("class", "item01 default");
                $("#item2").attr("class", "item02");
                $("#item3").attr("class", "item03");
            }
             if (id == "2") {
                $('#new1').hide();
                $('#new2').show();
                $('#new3').hide();
                $("#item1").attr("class", "item01");
                $("#item2").attr("class", "item02 default");
                $("#item3").attr("class", "item03");
            }
            if (id == "3") {
                //alert("3");
                $('#new1').hide();
                $('#new2').hide();
                $('#new3').show();
                $("#item1").attr("class", "item01");
                $("#item2").attr("class", "item02");
                $("#item3").attr("class", "item03 default");
            }
        }
    </script>
