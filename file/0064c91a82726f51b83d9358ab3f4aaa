    <!-- 首頁大Banner效果設定 -->
    <script type="text/javascript">
        var Num = 1;
        $(function () {
            //首頁大banner      
            //alert($('#topbannerlist li.default').index());
            $('#topbannerlist li a').click(function () {
                $('#topbannerlist li a').removeClass();
                $('#topbannerlist li').removeClass();
                $(this).addClass('active');
                $(this).parent().addClass('default');
                $("#topbanner").css("background-image", "url(" + $(this).attr("url") + ")");
                $("#topbannerurl").attr("href", $(this).attr("adlink"));

                Num = parseInt($(this).attr("id").replace("a", ""), 10);
            });
            $("#topbanner").hover(function () {
                // 關閉開關及計時器
                //alert("close");
                clearTimeout(timer);
            }, function () {
                // 如果滑鼠移出 $block 時
                // 開啟開關及計時器        
                //alert("open1");
                timer = setTimeout(move, "6000");
            });
            //設定移動
            function move() {
                var _index = $('#topbannerlist li.default').index() + 1;
                var maxlength = $("#topbannerlist li").length;
                $("#topbannerlist li a").eq(_index % maxlength).click();
                timer = setTimeout(move, "6000");
            }
            timer = setTimeout(move, "6000");
        });
        
        function changebanner(btn) {
            if (btn == "p") {
                Num = Num - 1;
                if (Num < 1) {
                    Num = 4;
                }
                $("#topbanner").css("background-image", "url(" + $("#a" + Num).attr("url") + ")");
            }
            if (btn == "n") {
                Num = Num + 1;
                if (Num > 4) {
                    Num = 1;
                }
                $("#topbanner").css("background-image", "url(" + $("#a" + Num).attr("url") + ")");
            }
        }
    </script>
    <!-- 首頁大Banner效果設定 -->
