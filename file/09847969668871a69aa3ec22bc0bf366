<script type="text/javascript">
    //登录按钮点击事件
    function loginSubmit() {
        ErrorEvent("userName", "");
        ErrorEvent("userPwd", "");
        ErrorEvent("yzPwd", "");

        if ($('#userName').attr("ischange") != "true") {
            ErrorEvent("userName", "用户名不能为空");
            return;
        }
        if ($('#userName').val() == "") {
            ErrorEvent("userName", "用户名不能为空");
            return;
        }

        if ($('#userPwd').attr("ischange") != "true") {
            ErrorEvent("userPwd", "密码不能为空");
            return;
        }
        if ($('#userPwd').val() == "") {
            ErrorEvent("userPwd", "密码不能为空");
            return;
        }

        if ($('#yzPwd').attr("ischange") != "true") {
            ErrorEvent("yzPwd", "验证码不能为空");
            return;
        }
        if ($('#yzPwd').val() == "") {
            ErrorEvent("yzPwd", "验证码不能为空");
            return;
        }

        $('#span_loginError').html('');
        tmpReq = Math.random();
        var isSave = $('#in2week').attr('checked') == "checked"
        var req = 'Handler/Login/Login.ashx?name=' + $('#userName').val() + '&pwd=' + replaceSpecialWord($('#userPwd').val()) + '&validcode=' + escape($('#yzPwd').val()) + '&remember=' + isSave + '&req=' + tmpReq;
        JsLoad(req, doLoginResult);
    };
    /*
           URL特殊符号及对应的十六进制值编码：      
           
           */
    function replaceSpecialWord(str) {
        str = str.replace(/%/ig, "%25");
        str = str.replace(/@/ig, "%40");
        str = str.replace(/\*/ig, "%2A");
        str = str.replace(/_/ig, "%5F");
        str = str.replace(/\+/ig, "%2B");
        str = str.replace(/-/ig, "%2D");
        str = str.replace(/\./ig, "%2E");
        str = str.replace(/\//ig, "%2F");
        str = str.replace(/&/ig, "%26");
        str = str.replace(/#/ig, "%23");
        str = str.replace(/\^/ig, "%5E");
        str = str.replace(/\$/ig, "%24");

        return str;
    }
    //跨域请求数据
    function JsLoad(src, callBack) {
        var s = document.createElement("script");
        var b = false;
        s.setAttribute("type", "text/javascript");
        s.setAttribute("src", src);
        s.onload = s.onreadystatechange = function (a) {
            if (!b && (!this.readyState || this.readyState == 'loaded' || this.readyState == 'complete')) {
                b = true;
                callBack();
            }
        };
        document.getElementsByTagName("head")[0].appendChild(s);
    };
    //点击时重新生成验证码
    function ChangeImage() {
        $("#imgValidCode").attr("src", "/Handler/CreateImgCode.ashx?" + Math.random());
    };
    //处理返回的信息
    function doLoginResult() {
        var backUrl = '/index.html';
        if (typeof (code) != 'undefined' && code.toString() == "1") {
            location.href = '/index.html?backUrl=' + backUrl;
        }
        else if (typeof (code) != 'undefined' && code.toString().length > 0) {
            if (code.toString() == "0")
            { location.href = "/admin/password/"; }
            else
            {
                if (code.toString().indexOf("验证码") > 0) {
                    ErrorEvent("yzPwd", "验证码错误");
                }
                else {
                    ErrorEvent("userPwd", code.toString())
                }
                ChangeImage();
            }
        }
        else {
            ErrorEvent("userPwd", "系统繁忙，请稍后再试");
            ChangeImage();
        }
    };
    //报错提示
    function ErrorEvent(id, errorMsg) {
        if (errorMsg != "") {
            if (id == "userPwd") {
                id = "userName";
            }
            $("#" + id).addClass("error");
            $("#" + id).val(errorMsg);
            $("#" + id).attr("ischange", "false");
            $("#" + id).removeClass("input-text");
        } else {
            $("#" + id).removeClass("error");
        }
    }
    //绑定事件
    $(function () {
        //生成验证码
        ChangeImage();
        //刷新页面设置文本框内容不记忆
        $("input").each(function () {
            $(this).attr("ischange", "false");
            $(this).attr("style", "");
        });
        //回车事件
        $(document).keydown(function (event) { if (event.keyCode == 13) { loginSubmit() } });

        if ($('#userName').val() != "" && $('#userPwd').val() != "") {
            $('#in2week').attr('checked', true);
            $('#userName').attr("ischange", true);
            $('#userPwd').attr("ischange", true);
            $('#userName').addClass("input-text");
            $('#userPwd').addClass("input-text");
        };

        //商家注册鼠标滑过事件
        $("#registerChange").bind("mouseover", function () {
            $(this).find("h4").html("立即注册");
            $(this).addClass("current");
        }).bind("mouseout", function () {
            $(this).find("h4").html("商家注册");
            $(this).removeClass("current");
        });


        //input标签value提示绑定fouce事件
        $("input").bind("focus", function () {
            if ($(this).attr("ischange") != "true") {
                $(this).attr("ischange", "true");
                $(this).val("");
                $(this).addClass("input-text");
            }
        });
        $("input").bind("blur", function () {
            if ($(this).val() == "") {
                $(this).attr("ischange", "false");
                $(this).removeClass("input-text");
            }
            else {
                var id = $(this).attr("id");
                ErrorEvent(id, "");
            }
        });

        $("#downIos").bind("click", function () {
            
            try {
                PageStatistics.Send('c_pc_czy_login_download_AppStore', "");
            } catch (e) { }
            window.open("https://appdownload.autohome.com.cn/usedcar/czy/chezhiying.ipa", "_blank");
        });
        $("#downAndriod").bind("click", function () {
             
            try {
                PageStatistics.Send('c_pc_czy_login_download_Andriod', "");
            } catch (e) { }
            window.open("https://appdownload.autohome.com.cn/usedcar/czy/chezhiying.apk", "_blank");
        });

        /*QrLogin*/
        var clientid = '';
        var tflag;
        function QrLogin() {
            var status = AjaxOperator.AjaxData("/Handler/Login/codelogin.ashx?action=login&cid=" + clientid, false, "get");
            switch (status) {
                case "1":
                    $("#divQrLogin").show();
                    $("#divQrLogin").find(".black-ok").show();
                    $("#divQrLogin").find(".black-ok").text("扫描成功正在登录");
                    $("#divQrLogin").find(".black-error").hide();
                    break;
                case "2":
                    $("#divQrLogin").show();
                    location.href = "/index.html";
                    break;
                case "3":
                case "4":
                case "-1":
                    $("#divQrLogin").show();
                    $("#divQrLogin").find(".black-ok").hide();
                    $("#divQrLogin").find(".black-error").find("span").text("验证码已经失效");
                    $("#divQrLogin").find(".black-error").show();
                    clearInterval(tflag);
                    break;
            }
        }
        function InitCode() {
            var result = AjaxOperator.AjaxData("/Handler/Login/codelogin.ashx?action=init&cid=" + clientid, false, "get");
            var objResult = eval("("+result+")");
            clientid = objResult.cid;
            $("#imgQr").attr("src", "http://www.interface.che168.com/qrcode/qrcode.ashx?w=208&h=208&url=" + encodeURIComponent(objResult.url));
        }
        InitCode();
        $("#aQrReflush").click(function () {
            $("#divQrLogin").hide();
            InitCode();
            tflag = setInterval(QrLogin, 2000);
        });
        tflag = setInterval(QrLogin, 2000);
    });
</script>

<script>
    seajs.config({
        // charset: 'gb2312',
        version: '2016011610', // 版本号, 可选
        alias: {
            //自定义JS
            'index': './assets/scripts/app/index.js'
        },
        paths: {
            'as': 'http://s.autoimg.cn/as/as-3.0.0',//全站插件库
            '2sc': 'http://s.autoimg.cn/2sc_mass/js_pc/v1.0.2'//?二手车插件库
        }
    });
    seajs.use(["jquery", "as/tab", "2sc/tabFollowed", "as/pop", "2sc/public", "2sc/public", "2sc/search"],
     function () {
         //颜色选中框
         $(".j-t-color").on("click", "li", function () {
             $(this).toggleClass("click").siblings("li").removeClass("click");
         });
     });

</script>
