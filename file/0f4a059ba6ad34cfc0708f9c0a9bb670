        $().ready(function () {
            var tabs = $("#tabNav a").hover(function () { $(this).addClass("CurChoo"); }
            , function () {
                var labelIndex = $(this).prevAll().length;
                if (labelIndex != "0") { $(this).removeClass("CurChoo"); }
            });
