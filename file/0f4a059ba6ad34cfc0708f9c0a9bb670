       <script type="text/javascript">
    var menuflg = "0";
    function openSideMenu() {
	var mainDiv = document.getElementById("maindiv");
        if (menuflg == "0") {
            $('#menuside').addClass('translateX');
            $('#divbody').addClass('translateX');
            menuflg = "1";
 	    document.body.style.overflowX="hidden";
            //var h = window.document.documentElement.scrollHeight > window.document.documentElement.clientHeight ? window.document.documentElement.scrollHeight : window.document.documentElement.clientHeight;
            var h= window.document.documentElement.clientHeight;
            mainDiv.style.height = h + 'px';
            mainDiv.style.display = "block";
            document.getElementById("divbody").style.height = h + 'px';
        }
    }
    var userMenu = "0";
    function openUserMenu() {
        if("False" != "True")
        {
            window.location = "http://m.yizijia.cn/mdenglu";
            return;
        }
	var mainDiv = document.getElementById("maindiv");
        if (userMenu == "0") {
            $('#userside').addClass('translateX2');
            $('#divbody').addClass('translateX2');
            userMenu = "1";
 	    document.body.style.overflowX="hidden";
            //var h = window.document.documentElement.scrollHeight > window.document.documentElement.clientHeight ? window.document.documentElement.scrollHeight : window.document.documentElement.clientHeight;
            var h= window.document.documentElement.clientHeight;
            mainDiv.style.height = h + 'px';
            mainDiv.style.display = "block";
            document.getElementById("divbody").style.height = h + 'px';
        }
    }

    function hiddenMenu()
    {
	var mainDiv = document.getElementById("maindiv");
        if (menuflg == "1") {
	    $('#divbody').removeClass('translateX');
            $('#menuside').removeClass('translateX');
            menuflg = "0";
        }
        if (userMenu == "1") {
            $('#divbody').removeClass('translateX2');
            $('#userside').removeClass('translateX2');
            userMenu = "0";
        }
        document.getElementById("divbody").removeAttribute("style");
	    document.body.style.overflowX="auto";
        mainDiv.style.display = "none";
    }
    function setBodysize() {
        var realsize = $(window).width();
        var bodysize = realsize * 12 / 640 + "px";
        $("body").css("font-size", bodysize);
    }
    setBodysize();
    window.onresize = setBodysize;
    function StaticRefreshPage() {
        window.location.reload();
    }
</script> 
