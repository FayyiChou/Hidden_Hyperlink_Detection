       <script type="text/javascript">
    var pageNo = 1;
    function selectaegg(i) {
        for (var k = 1; k <= 2; k++) {
            var li = $("#Top" + k);
            if (k == i) {
                li.addClass("Newset");
                order = i;
                goPage(page);
            } else {
                li.removeClass("Newset");
            }
        }
    }
    var showTravel = false;
    $().ready(function () {
        showTravel = false;
        goPage(1);
    });




    var RootUrl = "http://www.yizijia.cn/";
    var order = 1;
    var areaid = "14000000029";
    var areaType = "1";
    var youji = "http://youji.yizijia.cn/";


    function Moredata() {
        pageNo += 1;
        goPage(pageNo);
    }


    function goPage(pageNo) {
        //        $("#MYYJList").css("display", "inline-block");
        var journeyStr = "";
        var Random = Math.random().toString();
        $("#nolist").hide();
        $.getJSON("../Service/travelListformobile.ashx", { "page": pageNo, "order": order, "areaid": areaid, "areaType": areaType, "Random": Random }, function (data) {

            if (data.journeyStr.length > 0) {
                $("#seemore").show();
                $("#MYYJList").append(data.journeyStr);

            } else {
                if (pageNo > 1) {
                    $("#seemore").hide();
                }
                else
                    $("#nolist").show();
            }
        });
    }
</script> 
