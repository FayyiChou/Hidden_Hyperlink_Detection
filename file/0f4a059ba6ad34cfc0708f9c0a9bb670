        function addcount(type) {
            if (IsLogined()) {
                var areaid = "14000000029";
                var userid = $.cookie("id");
                var Random = Math.random().toString();
                var areatype = "1";
                $.get("http://mudidi.yizijia.cn/Service/AddAreaCount.ashx", { "areaid": areaid, "userid": userid, "type": type, "areatype": areatype, "Random": Random }, function (data) {
                    if (data == "1") {
                        $("#ding0").html("+1");
                        showDiv('type' + type, 'ding0');
                        var xqcount = $("#type" + type).text();
                        if (type != "3") {
                            $("#type" + type).html((parseInt(xqcount) + 1) + "&nbsp;&nbsp;&nbsp;&nbsp;");
                        } else {
                            $("#type" + type).html((parseInt(xqcount) + 1) + "&nbsp;&nbsp;");
                        }
                        if (type == 1) {
                            $("#typefont").html((parseInt(xqcount) + 1) + "人去过");
                        }
                    } else if (data == "2") {
                        $("#ding0").html("已经点过了");
                        showDiv('type' + type, 'ding0');
                    } else if (data == "3") {

                    }

                });

            } else {
                ShowLoginBox(null)
            }
        }

        var lenght = 0;
        var FunMoveDiv;
        var showDivObjId;
        var moveHeight;
        function showDiv(obj, obj2) {
            clearInterval(FunMoveDiv);
            var xd = getElementPos(obj);
            var mydiv = document.getElementById(obj2); //获取div控件
            mydiv.style.top = xd.y + document.getElementById(obj).offsetHeight / 2 + "px"; //div y坐标
            mydiv.style.left = xd.x + document.getElementById(obj).offsetWidth / 2 - 10 + "px"; //div x坐标
            var mydivTop = xd.y + document.getElementById(obj).offsetHeight / 2;
            moveHeight = 20;
            mydiv.style.display = "block"; //显示div
            showDivObjId = obj2;
            FunMoveDiv = setInterval(moveDiv, 20);
        }
        function moveDiv() {
            var mydiv = document.getElementById(showDivObjId);
            if (lenght >= moveHeight) {
                lenght = 0;
                mydiv.style.display = "none"; //显示div
                clearInterval(FunMoveDiv);
            }
            else {
                mydiv.style.top = mydiv.style.top.substring(0, mydiv.style.top.toString().length - 2) - 1 + "px";
                lenght++;
            }
        }
        function closeMoreDiv() {
            $("#showDivdetial").hide();
        }
        function showMoreDiv() {
            $("#showDivdetial").show();
        }

