    <script type="text/javascript">
    var show1 = "0";
    var show2 = "0";
    var show3 = "0";
    var show4 = "0";
    function showSubMenu(id) {
        if (id == "1") {
            if (show1 == "0") {
                //alert(document.getElementById("mainmenu1").childNodes[0].innerHTML);
                document.getElementById("mainmenu1").innerHTML = "-";
                //alert(document.getElementById("mainmenu1").childNodes[0].innerHTML);
                document.getElementById("submenu1").style.display = "block";
                show1 = "1";
            }
            else {
                document.getElementById("mainmenu1").innerHTML = "+";
                document.getElementById("submenu1").style.display = "none";
                show1 = "0";
            }
        }
        else if (id == "2") {
            if (show2 == "0") {
                document.getElementById("mainmenu2").innerHTML = "-";
                document.getElementById("submenu2").style.display = "block";
                show2 = "1";
            }
            else {
                document.getElementById("mainmenu2").innerHTML = "+";
                document.getElementById("submenu2").style.display = "none";
                show2 = "0";
            }
        }
        else if (id == "3") {
            if (show3 == "0") {
                document.getElementById("mainmenu3").innerHTML = "-";
                document.getElementById("submenu3").style.display = "block";
                show3 = "1";
            }
            else {
                document.getElementById("mainmenu3").innerHTML = "+";
                document.getElementById("submenu3").style.display = "none";
                show3 = "0";
            }
        }
        else if (id == "4") {
            if (show4 == "0") {
                document.getElementById("mainmenu4").innerHTML = "-";
                document.getElementById("submenu4").style.display = "block";
                show4 = "1";
            }
            else {
                document.getElementById("mainmenu4").innerHTML = "+";
                document.getElementById("submenu4").style.display = "none";
                show4 = "0";
            }
        }
    }
    //注销登录
    function CancelLogin() {
        var stringCancelLoginUrl = "http://m.yizijia.cn/" + "Service/Login.ashx?Type=2&UserName=current&Random=" + Math.random();
        var result = "";
        $.ajax({
            url: stringCancelLoginUrl,
            type: "get",
            async: false,
            success: function (data) {
                result = data;
            }
        });
        if (result == "true") {
            //刷新当前页面
            window.location.href = "http://m.yizijia.cn/mdenglu";
        }
    }
</script> 
