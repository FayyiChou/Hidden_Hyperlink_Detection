(function(){
var Game = window.Game = {
	MAX_NORMAL_FLASH_WIDTH:680,
	MAX_WIDER_FLASH_WIDTH:940,
	MIN_NORMAL_FLASH_HEIGHT:520,
	MIN_SCORE_GAMEID:1000000,
	SPECIAL_GAME_LIST:
	[
	   27261
	],
	PRE_LOAD_GAME_LIST:[82658, 27261, 72884, 74150, 73275],
	isNewLoading:true,
	getQueryStr: function (name)
	{
		var reg = new RegExp("(^|\\?|&)"+ name +"=([^&]*)(\\s|&|$)", "i");
		if (reg.test(location.href)) return decodeURIComponent(RegExp.$2.replace(/\+/g, " ")); 
		return "";
	},
	escapeHTML: function(str)
	{
		return str.replace(/</g,"&lt;")
					.replace(/>/g,"&gt;")
					.replace(/"/g,"&quot;")
					.replace(/'/g,"&#39;");
	},
	getSwfObj: function(id, inframe)
	{
		try{
			var win;
			if(inframe)
			{
				win = document.getElementById('frameid').contentWindow;
			}
			else
			{
				win = window;
				
			}
			return win.document[id] || window[id];
		}catch(_){
			return null;
		}
	},
	initParm:function()
	{
		gGameInfoOptions["FlashId"] = "gameflash";
		gGameInfoOptions["OpenGameId"] = this.getQueryStr("opengameid");
		gGameInfoOptions["OpenUserFlag"] = this.getQueryStr("userflag");
		gGameInfoOptions["OpenWidth"] = this.getQueryStr("width");
		gGameInfoOptions["OpenHeight"] = this.getQueryStr("height");
		//开放平台预览页缩放，因此更新flashsize为url传入参数
		if (typeof (gGameInfoOptions["OpenWidth"]) != 'undefined' && typeof (gGameInfoOptions["OpenHeight"]) != 'undefined' && gGameInfoOptions["OpenWidth"] > 0 && gGameInfoOptions["OpenHeight"] > 0)
		{
			gGameInfoOptions["FlashHeight"] = gGameInfoOptions["OpenHeight"];
			gGameInfoOptions["FlashWidth"] = gGameInfoOptions["OpenWidth"]
		}
		gGameInfoOptions["Width"] = gGameInfoOptions["FlashWidth"];
		gGameInfoOptions["Height"] = gGameInfoOptions["FlashHeight"];
		
		if(gGameInfoOptions["ApiFlag"] & 0x20 || gGameInfoOptions["ApiFlag"] & 0x40 || gGameInfoOptions["ApiFlag"] & 0x80)
		{
			gGameInfoOptions["CanTakePhoto"] = 1;
		}
		this.initFlashSrc();
		this.initFlashSize();
		this.initFlashSecuryParm();
	},
	getFlashHtmlByFrame: function(options)
	{
		var vecStr = [];
		vecStr.push('<iframe src="');
		vecStr.push("http://img.3366.com/fileupload/flash/");
		vecStr.push(parseInt(gGameInfoOptions["GameId"])%100);
		//自定义目录结果为 gameid/自定义文件名
		if(typeof(gGameInfoOptions["sIFrameName"]) != "undefined"  && "" != gGameInfoOptions["sIFrameName"])
		{
		    vecStr.push("/"+gGameInfoOptions["GameId"] +"/"+gGameInfoOptions["sIFrameName"] + "?gid=");
		}
		else
		{
		    vecStr.push("/gameframe_new.html?gid=");
		}
		vecStr.push(gGameInfoOptions["GameId"]);
		vecStr.push("&v=1");
		if(options.Width == "100%")
		{
			vecStr.push("&full=1");
		}
		else
		{
			vecStr.push("&w=" + options.Width);
			vecStr.push("&h=" + options.Height);
		}
		vecStr.push("&cc=" + gGameInfoOptions["ReleaseTime"]);
		vecStr.push('" name="frameid" id="frameid" marginwidth="0" marginheight="0" scrolling="no" width="');
		vecStr.push(options.Width);
		vecStr.push('" height="');
		vecStr.push(options.Height);
		vecStr.push('" frameborder="0" allowtransparency="true"></iframe>');
		return vecStr.join("");
	},

	initFlashSrc:function()
	{
		if (parseInt(gGameInfoOptions["OpenGameId"]) > 0 && parseInt(gGameInfoOptions["OpenUserFlag"]) == 1)
		{
			gGameInfoOptions["BaseDir"] = 'http://view.3366.com/submit_game_flash/' + gGameInfoOptions["OpenGameId"] + "/";
			gGameInfoOptions["FlashSrc"] = 'http://view.3366.com/submit_game_flash/' + gGameInfoOptions["OpenGameId"] + '/' + gGameInfoOptions["OpenGameId"] + '.swf'; //审核端
		}
		else if (parseInt(gGameInfoOptions["OpenGameId"]) > 0 && parseInt(gGameInfoOptions["OpenUserFlag"]) == 2)
		{
			gGameInfoOptions["BaseDir"] = 'http://pubview.3366.com/dev_submit_check_pass_flash/' + gGameInfoOptions["OpenGameId"] + "/";
			gGameInfoOptions["FlashSrc"] = 'http://pubview.3366.com/dev_submit_check_pass_flash/' + gGameInfoOptions["OpenGameId"] + '/' + gGameInfoOptions["OpenGameId"] + '.swf'; 
		}
		else if (parseInt(gGameInfoOptions["OpenGameId"]) > 0 && parseInt(gGameInfoOptions["OpenUserFlag"]) == 3)
		{
			gGameInfoOptions["BaseDir"] = 'http://view.3366.com/temp_game_flash/';
			gGameInfoOptions["FlashSrc"] = 'http://view.3366.com/temp_game_flash/' + gGameInfoOptions["OpenGameId"] + '.swf'; 
		}
		else if(parseInt(gGameInfoOptions["OpenGameId"]) > 0)
		{
			gGameInfoOptions["BaseDir"] = 'http://flash.3366.com/resource/' + gGameInfoOptions["OpenGameId"] + "/";
			gGameInfoOptions["FlashSrc"] = 'http://flash.3366.com/opencgi-bin/open_down_gameflash?gameid=' + gGameInfoOptions["OpenGameId"]; //dev
		}
		else
		{
			var sFlashDomain =  'http://img.3366img.com';
			if (gGameInfoOptions["GameType"] == 2)
			{
				sFlashDomain = 'http://img.3366.com';
			}
			gGameInfoOptions["BaseDir"] = sFlashDomain + '/fileupload/flash/'+parseInt(gGameInfoOptions["GameId"])%100+'/'+gGameInfoOptions["GameId"] + "/";
			gGameInfoOptions["FlashSrc"] = sFlashDomain + '/fileupload/flash/'+parseInt(gGameInfoOptions["GameId"])%100+'/'+gGameInfoOptions["GameId"]+'.swf';
			if(typeof(gGameInfoOptions["OpenFlashBaseDirExp"]) != "undefined" && "" != gGameInfoOptions["OpenFlashBaseDirExp"])
			{
				gGameInfoOptions["BaseDir"] = sFlashDomain + '/fileupload/flash/'+parseInt(gGameInfoOptions["GameId"])%100+'/'+gGameInfoOptions["GameId"] + "/" + gGameInfoOptions["OpenFlashBaseDirExp"] + "/";
				gGameInfoOptions["FlashSrc"] = sFlashDomain + '/fileupload/flash/'+parseInt(gGameInfoOptions["GameId"])%100+'/'+gGameInfoOptions["GameId"] + "/" +gGameInfoOptions["OpenFlashBaseDirExp"] + "/"+gGameInfoOptions["GameId"]+'.swf';
			}
		}
		var sFlashUrlSuffix = "";
		if ( typeof(gGameInfoOptions["FlashUrlParaFlag"]) != "undefined" && typeof(gGameInfoOptions["FlashUrlPara"]) != "undefined")
		{
			if("1" == gGameInfoOptions["FlashUrlParaFlag"] && "" != gGameInfoOptions["FlashUrlParaFlag"])
			{
				sFlashUrlSuffix = "?" + gGameInfoOptions["FlashUrlPara"];
			}
		}
		gGameInfoOptions["FlashSrc"] += sFlashUrlSuffix;
		var sVertion = "";
		if ( typeof(gGameInfoOptions["ReleaseTime"]) != "undefined" && parseInt(gGameInfoOptions["ReleaseTime"]) > 1302105600)
		{
			if("" != sFlashUrlSuffix)
			{
				
				sVertion = "&cc=";
			} 
			else
			{
				sVertion = "?cc=";
			}
			sVertion +=  gGameInfoOptions["ReleaseTime"];
		}
		gGameInfoOptions["FlashSrc"] += sVertion;
		var newFlashSrc = this.escapeHTML(this.getQueryStr("swf"));
		if (newFlashSrc)
		{
			var baseId = parseInt(newFlashSrc)%100;
			newFlashSrc = 'http://img.3366.com/photo_check/check_game_flash/'+baseId+'/'+newFlashSrc+'.swf';
			gGameInfoOptions["FlashSrc"] = newFlashSrc;
			gGameInfoOptions["BaseDir"] = 'http://img.3366.com/photo_check/check_game_flash/'+baseId+'/';
		};
	},

	initFlashSize:function()
	{
		if (gGameInfoOptions["FlashWidth"] == "100%")
		{
			return ;
		}
		else if (gGameInfoOptions["FrameStatus"] == 1) //宽版页面的缩放规则。由页面上的gGameInfoOptions["FrameStatus"]指定
		{
			if (gGameInfoOptions["FlashWidth"] >  this.MAX_WIDER_FLASH_WIDTH)
			{
				gGameInfoOptions["FlashHeight"] =  gGameInfoOptions["FlashHeight"] * this.MAX_WIDER_FLASH_WIDTH/gGameInfoOptions["FlashWidth"];
				gGameInfoOptions["FlashWidth"] =  this.MAX_WIDER_FLASH_WIDTH;			
			}
		}
		else
		{ 
			if (parseInt(gGameInfoOptions["FlashWidth"]) > this.MAX_NORMAL_FLASH_WIDTH) //窄版页面缩放规则，同理
			{
				gGameInfoOptions["FlashHeight"] = gGameInfoOptions["FlashHeight"]* this.MAX_NORMAL_FLASH_WIDTH/gGameInfoOptions["FlashWidth"];
				gGameInfoOptions["FlashWidth"] =  this.MAX_NORMAL_FLASH_WIDTH;
			}
		}
		var width = gGameInfoOptions["FlashWidth"];
		var height = gGameInfoOptions["FlashHeight"];
		var FlashBoxId = document.getElementById('FlashBoxId');
				
		if((parseInt(height) + 20) < this.MIN_NORMAL_FLASH_HEIGHT)
		{
			height = this.MIN_NORMAL_FLASH_HEIGHT;
			FlashBoxId.style.margin = Math.floor((this.MIN_NORMAL_FLASH_HEIGHT - gGameInfoOptions["FlashHeight"])/2)+"px auto"
		}
		else
		{
			FlashBoxId.style.margin = "6px auto";
		
		}
		FlashBoxId.style.width = width + 'px';
		FlashBoxId.style.height = gGameInfoOptions["FlashHeight"] + 'px';
	},

	initFlashSecuryParm:function()
	{
		gGameInfoOptions["ScriptAccess"] = "sameDomain";
		gGameInfoOptions["AllowNetWorking"] = "internal";
		
		
		if (gGameInfoOptions["Open"] == 1) //开放平台游戏
		{
			 gGameInfoOptions["ScriptAccess"] = "sameDomain";
			 //gGameInfoOptions["ScriptAccess"] = "always";
			 gGameInfoOptions["AllowNetWorking"] = "all";
		}
		
		if(gGameInfoOptions["Open"] == 0 && (gGameInfoOptions["ApiFlag"] & 0x20 || gGameInfoOptions["ApiFlag"] & 0x40 || gGameInfoOptions["ApiFlag"] & 0x80))
		{
			gGameInfoOptions["AllowNetWorking"] = "all";
		}
		
		if (gGameInfoOptions["GameType"] == 2) //积分游戏
		{
			//gGameInfoOptions["ScriptAccess"] = "sameDomain";
			gGameInfoOptions["ScriptAccess"] = "always";
			gGameInfoOptions["AllowNetWorking"] = "all";		
		}
		
		if (gGameInfoOptions["NeedOutLink"] == 1) //开放外链
		{
			 gGameInfoOptions["AllowNetWorking"] = "all";		
		}
		if (gGameInfoOptions["GameId"] == 53185) //Special Game;
		{
			 gGameInfoOptions["ScriptAccess"] = "always";
			 gGameInfoOptions["AllowNetWorking"] = "all";
		}
	},
	_getLoadingAdLoc:function()
	{
		var strAdLoc = '';
		switch (parseInt(gGameInfoOptions["GameId"]))
		{			
			case 36699: strAdLoc = '3366_CAFC_loading'; break;
			case 14840: strAdLoc = '3366_CWLLK2_5_loading'; break;
			case 33980: strAdLoc = '3366_HDKDC_loading'; break;
			case 26330: strAdLoc = '3366_SLBHR_loading'; break;
			case 24518: strAdLoc = '3366_TTFP_loading'; break;
		}
		return strAdLoc;
	},
	getFlashContent: function(options)
	{
		var vecStr = [];
		vecStr.push('<object id="' + options.FlashId + '" width="' + options.Width + '" height="' + options.Height);
		vecStr.push('" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000"  codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,28,0">');	
		vecStr.push('<param name="movie" value="' + options.FlashSrc +   '" />');
		vecStr.push('<param name="allowScriptAccess" value="' + options.ScriptAccess +  '" />');
		vecStr.push('<param name="allowNetworking" value ="' + options.AllowNetWorking + '" />');
		vecStr.push('<param name="quality" value="high" />');
		vecStr.push('<param name="wmode" value="' + (options.WMode || 'opaque') + '" />');
		vecStr.push('<param name="base" value="' + options.BaseDir + '" />');
		vecStr.push('<embed  name="' + options.FlashId + '" ');
		vecStr.push('src="' + options.FlashSrc + '" ');
		vecStr.push('width="' + options.Width + '" ');
		vecStr.push('height="' + options.Height + '" ');
		vecStr.push('base="' + options.BaseDir + '"');
		vecStr.push('allowScriptAccess="' + (options.ScriptAccess || "sameDomain") + '" ');
		vecStr.push('allowNetworking="' + (options.AllowNetWorking || "internal") + '" ');
		vecStr.push('wmode="' + (options.WMode || 'opaque') + '" ');
		vecStr.push('quality="high" ');
		vecStr.push('pluginspage="http://www.adobe.com/shockwave/download/download.cgi?P1_Prod_Version=ShockwaveFlash" type="application/x-shockwave-flash" ');
		vecStr.push('/>');
		vecStr.push('</object>');
		//自定义参数，设置flash参数
		var flashVars = options.FlashVar;
		if(options.FlashParam)
		{
			var paramArr = options.FlashParam.split('&');
			for(var i=0; i<paramArr.length; i++)
			{
				var arr = paramArr[i].split('=');
				switch(arr[0])
				{
						//重新设置movie参数
					case 'movie':  				
						vecStr.splice(2, 1, '<param name="movie" value="' + arr[1] +   '" />');
						break;
						//重新设置allowScriptAccess
					case 'allowScriptAccess':   
						vecStr.splice(3, 1, '<param name="allowScriptAccess" value="' + arr[1] +  '" />');
						vecStr.splice(13, 1, 'allowScriptAccess="' + arr[1] + '" ');
						break;
						//重新设置allowNetworking
					case 'allowNetworking': 	
						vecStr.splice(4, 1, '<param name="allowNetworking" value ="' + arr[1] + '" />');
						vecStr.splice(14, 1, 'allowNetworking="' + arr[1] + '" ');
						break;
						//重新设置quality
					case 'quality': 			
						vecStr.splice(5, 1, '<param name="quality" value="'+arr[1]+'" />');
						vecStr.splice(16, 1, 'quality="'+arr[1]+'" ');
						break;
						//重新设置wmode
					case 'wmode': 				
						vecStr.splice(6, 1, '<param name="wmode" value="' + arr[1] + '" />');
						vecStr.splice(15, 1, 'wmode="' + arr[1] + '" ');
						break;
						//重新设置base
					case 'base': 				
						vecStr.splice(7, 1, '<param name="base" value="' + arr[1] + '" />');
						vecStr.splice(12, 1, 'base="' + arr[1] + '"');
						break;
						//默认设置flashVars参数
					default: flashVars += flashVars ? '&'+arr[0]+'='+arr[1] : arr[0]+'='+arr[1];break;
				}
			}
		}
		//插入flashvars到object
		vecStr.splice(5, 0, '<param name="flashvars" value="' +  flashVars + '" />');
		//插入flashvars到embed
		vecStr.splice(14, 0, 'flashvars="' + flashVars + '" ');
		return vecStr.join('');
	},
	initLoadingFlash:function()
	{
		strAdLoc = this._getLoadingAdLoc();
		/*var strLoadingName = "http://img.3366.com/fileupload/flash/0/newloading1213.swf?r=" + Math.random();//普通游戏加载进度条
		if(gGameInfoOptions["Open"] == 1 && gGameInfoOptions["AdShell"] == 2)//分离加壳的进度条
		{
			strLoadingName = "http://img.3366.com/fileupload/flash/adloading0328.swf?r=" + Math.random()
		}else
		{
			if(this.PRE_LOAD_GAME_LIST.toString().indexOf(gGameInfoOptions["GameId"])!= -1)
			{
				this.isNewLoading = true;
				strLoadingName = "http://www.3366.com/open3366/component/GameLoading.swf?r="+Math.random();//进度条卡住的解决方案，预加载的进度条
			}
		}*/
		var strLoadingName = "http://www.3366.com/open3366/component/GameLoading.swf?r="+Math.random();
		if(gGameInfoOptions["Open"] == 1 && gGameInfoOptions["AdShell"] == 2){
			this.isNewLoading = false;
			strLoadingName = "http://img.3366.com/fileupload/flash/adloading0328.swf?r=" + Math.random();
		}else if(gGameInfoOptions["Open"] == 1){
			this.isNewLoading = true;
			strLoadingName = "http://www.3366.com/open3366/component/GoogleAdLoading.swf?r=" + Math.random();
		}/*else{
			if(gGameInfoOptions["NeedFrame"] == 1){
				this.isNewLoading = false;
				strLoadingName = "http://www.3366.com/open3366/component/GameLoading.swf?r="+Math.random();
			}
		}*/
		var match = document.cookie.match(/uin=o0*(\d+);/),
			uin = 0;
		if(match && match.length && match[1]){
			uin = parseInt(match[1], 10);
		}
		this.uin = uin;
		var FlashParam = "";
		if ( typeof(gGameInfoOptions["iFlashParamFlag"]) != "undefined" && typeof(gGameInfoOptions["sFlashParam"]) != "undefined" && "1" == gGameInfoOptions["iFlashParamFlag"])
		{
			FlashParam = gGameInfoOptions["sFlashParam"];
		}
		var flashSrc = gGameInfoOptions["FlashSrc"] || '';
		if (gGameInfoOptions["NeedFrame"] == 1) 
		{
			flashSrc = flashSrc.replace('img.3366img.com', 'img.3366.com');
		};
		var options = 
		{
			"FlashId":"loadingflash",
			"FlashContainerId": "LoadingContainId",
			"FlashSrc":strLoadingName,
			"FlashVar":"adLoc=" + strAdLoc + "&uin=" + uin + "&gid=" +  gGameInfoOptions["GameId"] + "&adId=" + gGameInfoOptions["AdId"] + "&channelId=" + (gGameInfoOptions["ChannelId"]) + (gGameInfoOptions["ChannelId"] == ""? "200000" :",200000") + "&xpos=" + gGameInfoOptions["iPhotoPosX"] + "&ypos=" + gGameInfoOptions["iPhotoPosY"] + "&xspan=" + gGameInfoOptions["iPhotoSpanX"] + "&yspan=" + gGameInfoOptions["iPhotoSpanY"] + "&gameUrl="+flashSrc,
			"Width": gGameInfoOptions["FlashWidth"],
			"Height": gGameInfoOptions["FlashHeight"],
			"AllowNetWorking":"all",
			"ScriptAccess": "always",
			"FlashParam":FlashParam
		}
		var loadingContainer = document.getElementById('LoadingContainId');
		loadingContainer.style.position = 'absolute';
		loadingContainer.style.left = '6px';
		loadingContainer.style.top = '6px';
		loadingContainer.style.height = gGameInfoOptions["FlashHeight"] + 'px'
		document.getElementById('LoadingContainId').innerHTML = this.getFlashContent(options);
	},
	initGameFlash: function(){
		var match = document.cookie.match(/uin=o0*(\d+);/),
			uin = 0;
		if(match && match.length && match[1]){
			uin = parseInt(match[1], 10);
		}
		this.uin = uin;
		var FlashParam = "";
		if ( typeof(gGameInfoOptions["iFlashParamFlag"]) != "undefined" && typeof(gGameInfoOptions["sFlashParam"]) != "undefined" && "1" == gGameInfoOptions["iFlashParamFlag"])
		{
			FlashParam = gGameInfoOptions["sFlashParam"];
		}
		var options = 
		{
			"FlashId": "gameflash",
			"FlashSrc": gGameInfoOptions["FlashSrc"],
			"BaseDir": gGameInfoOptions["BaseDir"],
			"Width": parseInt(gGameInfoOptions["FlashWidth"]),
			"Height": parseInt(gGameInfoOptions["FlashHeight"]),
			"ScriptAccess": gGameInfoOptions["ScriptAccess"],
			"AllowNetWorking":gGameInfoOptions["AllowNetWorking"],
			"FlashVar":"pagedomain="+ window.location.host+ "&pid=" +  gGameInfoOptions["GameId"] + "&adId=" + gGameInfoOptions["AdId"] + "&uin=" + uin + "&platform=1" + "&cantakephoto=" + gGameInfoOptions["CanTakePhoto"] + "&apiflag=" + gGameInfoOptions["ApiFlag"] + "&channelId=" + gGameInfoOptions["ChannelId"]+",200000" + "&xpos=" + gGameInfoOptions["iPhotoPosX"] + "&ypos=" + gGameInfoOptions["iPhotoPosY"] + "&xspan=" + gGameInfoOptions["iPhotoSpanX"] + "&yspan=" + gGameInfoOptions["iPhotoSpanY"],
			"NeedFrame": gGameInfoOptions["NeedFrame"],
			"WMode": parseInt(gGameInfoOptions["ApiFlag"], 10) & 8 ? 'window' : 'opaque',
			"FlashParam": FlashParam
		};
		
		document.getElementById('FlashContainId').innerHTML = gGameInfoOptions['NeedFrame'] == 1 ? this.getFlashHtmlByFrame(options) : this.getFlashContent(options);
		document.getElementById('FlashContainId').defaultWidth = options.Width;
		document.getElementById('FlashContainId').defaultHeight = options.Height;
		if(document.getElementById('screencutdiv'))
		{
			setTimeout(function()   //避免同时加载两个flash，在某些版本的chrome下可能某一个会不加载
			{
				document.getElementById('screencutdiv').innerHTML = '<object style="position:absolute; left:0px; top:0px; z-index:50;" id="screencutflash" width="1" height="1" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,28,0" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" >\
								<param value="http://www.3366.com/open3366/component/ScreenCut.swf" name="movie">\
								<param value="all" name="allowNetworking">\
								<param value="always" name="allowScriptAccess">\
								<param value="high" name="quality"><param value="transparent" name="wmode">\
								<param name="flashvars" value="swfWidth='+options.Width+'&swfHeight='+options.Height+'">\
								<embed src="http://www.3366.com/open3366/component/ScreenCut.swf" flashvars="swfWidth='+options.Width+'&swfHeight='+options.Height+'" wmode="transparent" allowFullScreen="true" quality="high" width="1" height="1" align="middle" allowNetworking="all" allowScriptAccess="always" type="application/x-shockwave-flash" name="screencutflash"></embed>\
							</object>';
			},500);
			document.getElementById('screencutdiv').defaultWidth = options.Width;
			document.getElementById('screencutdiv').defaultHeight = options.Height;
		}
		if(typeof SR != 'undefined')
		{
			SR.set(6);
		}
		this.startLoadingTime = new Date().getTime();
		if(gGameInfoOptions['NeedFrame'] == 1){
			if(typeof window.Page == 'undefined'){
				window.Page = {};
			}
			if(typeof window.Page.Flash == 'undefined'){
				window.Page.Flash = {};
			}
			window.Page.Flash.onFlashFrameReady = function(){};
		}
	},
	init: function(){
		if(location.href.indexOf('preview_detail_wide') > -1){
			gGameInfoOptions["FrameStatus"] = 1;
		}
		var _this = this;
		this.needLoading = false;
		//debugger;
		this.needDelay = gGameInfoOptions.DelayLoadJsFlag == '1';
		
		this.initParm();
		if(gGameInfoOptions['NeedFrame'] == 1)//使用iframe加载的游戏父页面无需loading
		{
			this.initGameFlash();
		}
		else if(gGameInfoOptions["Open"] != 1 || (gGameInfoOptions["Open"] == 1 && gGameInfoOptions["AdShell"] == 2))
		{
			//开放平台无须loading
			this.needLoading = true;
			document.getElementById('FlashContainId').style.visibility = 'hidden';
			this.lastPercent = 0;
			this.initLoadingFlash();
			if (this.isNewLoading)
			{
				//新加壳采用壳里面自动获取进度，无须js通知
				setTimeout(function(){
					_this.loadingReady();
				}, 20000);
				// this.initGameFlash();
				return;
			};
			var timeout = setTimeout(function(){
				_this.percentLoaded(100);
				_this.loadingReady();
			}, 20000);
			if(this.needDelay){
				this.startDelayTime = new Date();
				this.totalDelay = 6000;
				var stime = 0;
				var timer = setInterval(function(){
					stime += 100;
					var percent = Math.ceil(100 * (stime / 6000));
					if(_this.percentLoaded(percent)){
						clearInterval(timer);
					}
				}, 100);
				setTimeout(function(){
					_this.initGameFlash();
				}, this.totalDelay);
			}else{
				this.initGameFlash();
				var timer = setInterval(function(){
					if(_this.percentLoaded()){
						clearInterval(timer);
					}
				}, 100);
			}
		}else if(gGameInfoOptions["Open"] == 1){
			this.needLoading = true;
			document.getElementById('FlashContainId').style.visibility = 'hidden';
			this.initLoadingFlash();
		}
		else
		{
			this.initGameFlash();
			document.getElementById('FlashContainId').style.visibility = '';
			var timer = setInterval(function(){
				if(_this.percentLoaded()){
					clearInterval(timer);
				}
			}, 100);
		}
	},
	percentLoaded: function(percent){
		var game = this.getSwfObj('gameflash', gGameInfoOptions["NeedFrame"] == 1);
		if(!game){
			return;
		}
		if(this.needLoading){
			try{
				var loading = this.getSwfObj('loadingflash');
				percent = percent || game.PercentLoaded();
				if(percent < 100 && percent <= this.lastPercent){
					return false;
				}
				this.lastPercent = percent;
				percent = Math.min(Math.max(0, parseInt(percent, 10)), 100);
				if(loading){
					loading.sendToActionScript(percent);
				}
				if(percent == 100){
					this.report();
					return true;
				}
				return false;
			}catch(_){}
		}else{
			try{
				if(game.PercentLoaded() == 100){
					this.report();
					return true;
				}
			}catch(_){}
		}
	},
	loadingReady: function(){
		try{
			document.getElementById('LoadingContainId').parentNode.removeChild(document.getElementById('LoadingContainId'));
			document.getElementById('FlashContainId').style.visibility = '';
			if (window.Game.isNewLoading) 
			{
				window.Game.initGameFlash();
				window.Game.report();
			};
		}catch(_){}
	},
	report: function(){
		if(this.reported){
			return;
		}
		this.reported = true;
		var totalTime = (new Date().getTime() - this.startLoadingTime) / 1000,
			totalSize = parseInt(gGameInfoOptions.FlashSize.replace(/\D/,''), 10),
			loadSpeed = Math.floor(totalSize / totalTime);
		if(typeof SR != 'undefined' && SR){
			SR.set(7, loadSpeed);
			if(SR.c == 7 && window.seajs){
				seajs.use('module/pagespeed_report', function(PR){
					PR.report(SR);
					RS = null;
				});
			}
		}
		if(window.navigator.userAgent.indexOf('AppleWebKit/535.1 (KHTML, like Gecko) Chrome/14.0.802.30 Safari/535.1 SE 2.X MetaSr 1.0') < 0)
		{
		var uin = this.uin;
		var img = new Image();
		if(Math.random() > 0.9) //抽样上报
		{
			img.onload = img.onerror = function(){img = null};
			img.src = 'http://openstat.3366.com/fcg-bin/mgp_stat?ossid=6001&subid=2&content=' + gGameInfoOptions.GameId + '|' + totalSize + '|' + totalTime * 1000 + '|' + (gGameInfoOptions.OpenGameId == 0 ? 0 : 1) + '|' + (gGameInfoOptions.NeedFrame == 1 ? 1 : 0) + '|0&charset=0&u=' + uin;
		}
		}
	},
	replay: function(){
		this.initGameFlash();
	}
};
window.loadingisready = function(){};
window.gameisready = Game.loadingReady;
Game.init();
})();
</script>	
				
