var title = htmlDecode("&nbsp;双十一结束了，你买的假货已经在路上&nbsp;");

if(headimg=="" || headimg.indexOf("add_box.jpg")!=-1 || headimg.indexOf(".gif")!=-1){
	imgUrl="http://oss.tieqq.com/resource/namecard/article/images/defaultPic.jpg";
}else if(summary==""){
	if(headimg=="" || headimg.indexOf("add_box.jpg")!=-1 || headimg.indexOf(".gif")!=-1){
		imgUrl ="http://oss.tieqq.com/resource/namecard/article/images/defaultPic.jpg";
	}else{
		imgUrl = "http://oss.tieqq.com/static2/user/746015/article/8a299a2950f5d0fe0150f60359c403b8/1447236426574.jpg@!square-96";
	}
	summary = title;
}else{
	imgUrl = "http://oss.tieqq.com/static2/user/746015/article/8a299a2950f5d0fe0150f60359c403b8/1447236426574.jpg@!square-96";
}

 
