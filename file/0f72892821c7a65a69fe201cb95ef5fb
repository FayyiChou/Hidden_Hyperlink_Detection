function htmlDecode(str){
     return str
           .replace(/&#39;/g, '\'')
           .replace(/<br\s*(\/)?\s*>/g, '\n')
           .replace(/&nbsp;/g, ' ')
           .replace(/&amp;nbsp;/g,' ')
           .replace(/&lt;/g, '<')
           .replace(/&gt;/g, '>')
           .replace(/&quot;/g, '"')
           .replace(/&amp;/g, '&');
 }

//服务号appId 分享图片 分享内容 分享标题
