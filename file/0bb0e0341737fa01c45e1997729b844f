<SCRIPT language=JavaScript>
  function check_data(){
    if(document.forminfo.T_Content.value==""){
	  alert("请填写订单的内容！")
	  forminfo.T_Content.focus();
	  return(false)
	}
    if(document.forminfo.T_Tel.value==""){
	  alert("请填写联系电话！")
	  forminfo.T_Tel.focus();	  
	  return(false)
	}
    if(document.forminfo.T_TrueName.value==""){
	  alert("请填写您的姓名，以便联系，谢谢！")
	 forminfo.T_TrueName.focus();	  
	  return(false)
	}		
  }
</SCRIPT>

