<link href="/cims/techcomp/security/css/login.css" rel="stylesheet" type="text/css">

<script type="text/javascript">
	dojo.addOnLoad(showLogin);
	var j_application = "";
	var webpath = "/cims";
	function showLogin(){
		var url = "/cims/techcomp/ria/commonProcessor!commonMethod.action";	
		if(url.indexOf('?')<0){
			url = url +"?method=post";
		}
		//alert(url);
		if(url.indexOf('indexforswitch.jsp')>=0){
			dojo.byId("hideform").action=webpath+"/mdpapp/main/menu/indexforswitch.jsp";
		}else{
			dojo.byId("hideform").action=webpath+"/mdpapp/main/menu/indexforswitch.jsp?path="+url;
		}
		dojo.require('unieap.dialog.DialogUtil');
		//alert(unieap.WEB_APP_NAME+unieap.relogin);
		DialogUtil.showDialog({
				url : unieap.WEB_APP_NAME+unieap.relogin,
				width: "563",
				height: "360",
				resizable:false,
				isExpand:false,
				isClose:false,
				title : '会话过期,请重新登录',
				onComplete:selectValueComplete
	 	 });
	}
	function selectValueComplete(value){
		if(value="success"){
			if(!rangeFramework()){
				dojo.byId("hideform").submit();
			}
		}
	}
	//此处和框架有耦合
	function rangeFramework(){
		var obj = window ,p;
		while((p = obj.parent) && (p != obj)){
			if(p.middern == obj){
				obj.refreshWorkArea && obj.refreshWorkArea();
				return true;
			}
			obj = p;
		}
		return false;
	}
</script>
