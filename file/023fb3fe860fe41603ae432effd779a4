	return;  
   }
    
  //登录后才可以收藏
  $(".btn_collect02").click(function(){
  var userid= getCookie('UserID');
  var aid ="190494" ;
  var posturl ="/video/20170329/190494.html";
  var title ="2017省考备考：左老师带你学资料分析中的增长量问题";
  if(userid==''||userid=='undefiend'){
	alert("该文章登录后才可以收藏");
	location.href="/reg/login.php";
