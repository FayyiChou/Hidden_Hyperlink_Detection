<script type="text/javascript">
var setInterval_int;
var totalDuration;
var position;
var ccplayer;

function on_cc_player_init( vid, objectID )
{
	ccplayer = getSWF( objectID );
}

function getSWF( swfID ) 
{
	if( navigator.appName.indexOf( "Microsoft" ) != -1 )
	{
		return window[ swfID ];
	}
	else if(navigator.userAgent.indexOf('iPad')!= -1)
	{
		return getVideo(movieName);
	} 
	else {
		return document[ swfID ];
	}
}

function on_spark_player_start() 
{
	setInterval_int = setInterval('getPlayPosition()',1000);
}

function on_spark_player_pause() 
{
	window.clearInterval(setInterval_int);
}

function on_spark_player_resume() {
	setInterval_int = setInterval('getPlayPosition()',1000);
}

function on_spark_player_stop() 
{
	window.clearInterval(setInterval_int);
}

function getPlayPosition() 
{ 
	
	//ccplayer.start();
	//console.info(ccplayer.getDuration()/60);
	totalDuration = ccplayer.getDuration();
	position = ccplayer.getPosition();
	document.getElementById('player_play_position_second').innerHTML = arrive_timer_format(position);
	if(position == totalDuration)
	{
		window.clearInterval(setInterval_int);
	}
	
	//console.info(Math.round(position/60*100)/100);
}

//时间秒数格式化
function arrive_timer_format(s) {
var t;
if(s > -1){
    hour = Math.floor(s/3600);
    min = Math.floor(s/60) % 60;
    sec = s % 60;
    day = parseInt(hour / 24);
    if (day > 0) {
        hour = hour - 24 * day;
        t = day + "day " + hour + ":";
        }
    else t = hour + ":";   
    if(min < 10){t += "0";}
        t += min + ":";
    if(sec < 10){t += "0";}
        t += Math.round(sec);
}
return t;
}

$(document).ready(function(){

   var userid= getCookie('UserID'); 
   var aid ="190494" ;
