<script type="text/javascript">

//换一换
$(".btn-change").click(function() {
	var param = {};
	param['special_id'] = $('#change').val();
	$.ajax({
		type: "post",
		url: "/zt/ChangeQuestion",
		cache: false,
		data: param,
		datatype: "json",
		success: function(data) {
			data = $.parseJSON(data);
			var str = '';
			for(var k in data) {
				var d = data[k];
				str += '<div><p class="q-con">\
                    <span>问：</span><a target="_blank" href="http://bbs.zhulong.com/'+d['code']+'_group_'+d['gid']+'/detail' + d.tid + '">' + d['title'] + '</a></p>\
                    <p class="q-info"><span>浏览：' + d.view_num + '</span><span>回复：' + d.reply_num + '</span><span><a target="_blank" href="http://bbs.zhulong.com/'+d['code']+'_group_'+d['gid']+'/detail' + d.tid + '">去回答</a></span></p>\
                    <div class="cut-line-2"></div></div>\
                  ';
			}
			$(".question-list").empty().append(str);
		},
		async: false
	});
});
//快速提问
$("#commit-asks").click(function() {
	if(!($(this).text()=="立即提问")){
		return false;
	}
	var param = {};
  	param['prof']=$("#prof").val();
  	param['special_id']=$("#change").val();
	param['title'] = $('#biaoti').val();
	param['content'] = $('#neirong').val();
	param['uid'] = "";
	if(param['title'] == '') {
		alert('提问标题不能为空');
		return false;
	}
	if(param['title'].length>20){
		alert('提问标题不能大于20个字');
		return false;
	}
	if(param['content']=='') {
		alert('提问内容不能为空');
		return false;
	}
	if(param['content'].length>50){
		alert('提问内容不能大于50个字');
		return false;
	}
	if(param['title'] ==  '给问题起个标题吧'){
		alert('提问标题不能为空');
		return false;
	}
	if(param['content'] == '填写您的问题'){
		alert('提问内容不能为空');
		return false;
	}
	$("#commit-asks").text("提交中...");
	$.ajax({
		type: "post",
		url: "/zt/New_topicdetail",
		cache: false,
		data: param,
		datatype: "json",
		success: function(data) {
			data = $.parseJSON(data);
      		var _str="";
			if(data['errNo'] != 0) {
				alert(data.msg);
			} else {
				alert('发布成功');
				$('#biaoti').val("给问题起个标题吧").css("color","#8b8a8a");
				$('#neirong').val("填写您的问题").css("color","#8b8a8a");
       			_str += '<div><p class="q-con">\
                    <span>问：</span><a target="_blank" href="' + data.url + '">' + data['title'] + '</a></p>\
                    <p class="q-info"><span>浏览：' + 0 + '</span><span>回复：' + 0 + '</span><span><a target="_blank" href="'+data.url + '">去回答</a></span></p>\
                    <div class="cut-line-2"></div><div>\
                  ';
        		$(".question-list").children("div:eq(0)").remove();
        		$(".question-list").prepend(_str);
			}
		},
		complete:function(){
			$("#commit-asks").text("立即提问");
		}
	});
});
//浏览量关闭
$("#msg-close").click(function(){
	$(".zt-count-area").fadeOut();
});
//收藏
function collectRequest(id){
	$.ajax({
		url:'/zt/favrate',
		data:{id:id},
		type:'post',
		dataType:'json',
		success:function(data, textStatus){
			if($('#favtn').html().indexOf('已收藏')>-1){
				$('#favtn').html('<img src="/img/zt_img/icons_coll.png"/>收藏');
			}else{
				$('#favtn').html('<img src="/img/zt_img/icons_coll.png"/>已收藏');
			}
		}
	});
}
//fix placeholder
$("#neirong").focus(function(){
	if($(this).val()=="填写您的问题"){
		$(this).val("").css("color","#000000");
	}
	$(this).css("color","#000000");
});
$("#neirong").blur(function(){
	if($(this).val().length<=0){
		$(this).val("填写您的问题").css("color","#8b8a8a");
	}
});
//获取浏览量
$(function(){
	var _text=random(7000, 12000);
	$("#pv-num").text(_text);
	$(".zt-count-area").slideDown();

	setInterval(function() {
		var _threshold = (random(100, 300));
		var _rdMethod = random(0, 2);
		var _nowNum = _text;
		if(_rdMethod == 0) {
			_nowNum = _text + _threshold;
		} else {
			_nowNum = _text - _threshold;
		}
		$("#pv-num").text(_nowNum);
	}, 30000);
});
//get a random num between min ~ max
function random(min,max){
    return Math.floor(min+Math.random()*(max-min));
}
