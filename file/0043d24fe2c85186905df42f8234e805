//正则匹配图片
function  getimgsrc(htmlstr){
    var reg=/<img.+?src=('|")?http:\/\/([^'"]+)('|")?(?:\s+|>)/gim;
    var arr = '';
    while(tem=reg.exec(htmlstr)){
        arr+=',http://'+tem[2];
    }
    return arr;
}
//获取微课，课程，精品资料，行业资讯，热点互动的图片,取关联的class。
var imgsweike=getimgsrc($(".main-wrap").html());
var imgs=getimgsrc($(".hd-list").html());
imgs=imgsweike+imgs;
imgs=imgs.substr(1);
