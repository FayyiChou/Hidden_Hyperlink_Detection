<!--
// Tab Menu Table Show & Hidden
function LeftMtable(no){
    var oDiv = null;
	for(var i=0; i<20; i++){
	   oDiv    = document.all['LFMtable'+i];

		if(oDiv == null){
		  return false;
		}

		if(no==i){
            oDiv.style.visibility = 'visible';oDiv.style.display = '';
		}else{
			oDiv.style.visibility = 'hidden';oDiv.style.display = 'none';
		}
    }
}

function LeftMtableOut(no){
	var oDiv    = document.all['LFMtable' + no];

	if (oDiv == null) {
	   return false;
	}

	oDiv.style.visibility = 'hidden';oDiv.style.display = 'none';
}
