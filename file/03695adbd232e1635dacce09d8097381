<script language="JavaScript" type="text/JavaScript">
 function cal(form){
 var cbm1
 if(form.volume.value!="")
 { cbm1=parseFloat(form.volume.value)*167
 form.kgs1.value=(cbm1).toString()
 }
}

function cal2(form){
 var cbm1
 if(form.volume3.value!="")
 { cbm1=parseFloat(form.volume3.value)*4.729
 form.kgs2.value=(cbm1).toString()
 }
}

function cal3(form){
 var cbm1
 if(form.textfield3.value!="")
 { cbm1=parseFloat(form.textfield3.value)*2.54
 form.textfield.value=(cbm1).toString()
 }
}

function cal4(form){
 var cbm1
 if(form.textfield6.value!="")
 { cbm1=parseFloat(form.textfield6.value)*0.4536
 form.textfield5.value=(cbm1).toString()
 }
}

function cal5(form){
 var cbm1
