<script language="javascript" type="text/javascript">
function tab_rank(tabnum)
{   	
	for(var i=0;i<4;i++)
	{
		document.getElementById("tab_content_"+i).style.display="none";
		document.getElementById("tab_menu_"+i).className="cB4B";			
	}
	document.getElementById("tab_content_"+tabnum).style.display="block";
	document.getElementById("tab_menu_"+tabnum).className="cWhite";
	if(tabnum==0){
		document.getElementById("conBg").style.background="url(images/bg9.gif) no-repeat";
		document.getElementById("conBg").style.height="93px";
	}
	if(tabnum==1){
		document.getElementById("conBg").style.background="url(images/bg10.gif) no-repeat";
		document.getElementById("conBg").style.height="110px";
	}
	if(tabnum==2){
		document.getElementById("conBg").style.background="url(images/bg11.gif) no-repeat";
		document.getElementById("conBg").style.height="93px";
	}
	if(tabnum==3){
		document.getElementById("conBg").style.background="url(images/bg12.gif) no-repeat";
		document.getElementById("conBg").style.height="93px";
	}    	
}
</script>
