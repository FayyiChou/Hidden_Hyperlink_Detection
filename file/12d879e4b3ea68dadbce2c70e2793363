    <script type="text/javascript">
        $(function () {
            // tab切换
            $(".main_tab span").click(function () {
                var ind = $(this).index();
                var itemType = parseInt($("#ItemType").val());
                var pageIndex = parseInt($("#PageIndex").val());
                if (ind == 0) {
                    $(".main_tab i").animate({ "left": '0' }, 200);
                    $(this).addClass("cur").siblings("span").removeClass("cur");
                    if (itemType != ind) {
                        $("#PageTotal").val($("#RingTotal").val());
                    }
                } else if (ind == 1) {
                    $(".main_tab i").animate({ "left": '33.33%' }, 200);
                    $(this).addClass("cur").siblings("span").removeClass("cur");
                    if (itemType != ind) {
                        $("#PageTotal").val($("#NecklaceTotal").val());
                    }
                }else if (ind == 2) {
                    $(".main_tab i").animate({ "left": '66.66%' }, 200);
                    $(this).addClass("cur").siblings("span").removeClass("cur");
                }
                $("#ItemType").val(ind);
                if (itemType != ind) {
                    $("#PageIndex").val("0");
                    PageChange(-1);
                }
            })
        });

        function PageChange(operation) {
            var pageIndex = parseInt($("#PageIndex").val());
            var pageTotal = parseInt($("#PageTotal").val());
            var ItemType = parseInt($("#ItemType").val());
            var CurrentAreaId = $("#CurrentAreaId").val();

            if (pageIndex == 0 && operation == 0) {
                alert("已经是第一页了");
                return;
            }
            if (pageIndex + 1 == pageTotal && operation == 1) {
                alert("已经是最后一页了");
                return;
            }
            if (operation == -1)
                pageIndex = 0;
            else if (operation == 1) {
                pageIndex = pageIndex + 1;
            }
            else
                pageIndex = pageIndex - 1;
            var ajaxContent = {
                type: "POST",
                url: "Default.aspx/GetPageData",
                data: "{PageIndex:" + pageIndex + ",ItemType:" + ItemType + ",CurrentAreaId:'" + CurrentAreaId + "'}",
                contentType: "application/json; charset:utf-8",
                dataType: "json",
                success: function (response) {
                    if (response.d != null) {
                        var data = eval("(" + response.d + ")");
                        if (data.IsSuccess) {
                            if (data.ReturnObject == "" || data.ReturnObject == null) {
                                $("#PageIndex").val(pageIndex - 1);
                                alert("已经是最后一页了");
                                //按钮置灰
                            }
                            else {
                                $("#RankList").html("");
                                $("#RankList").html(data.ReturnObject);
                                //填充页面列表
                                $("#PageIndex").val(pageIndex);
                            }
                        }
                    }
                }
            };
            $.ajax(ajaxContent);
        }

        function DoGetItem(itemType) {
            var ajaxContent = {
                type: "POST",
                url: "Default.aspx/DoGetItem",
                data: "{itemType:" + itemType + "}",
                contentType: "application/json; charset:utf-8",
                dataType: "json",
                success: function (response) {
                    if (response.d != null) {
                        alert(response.d);
                    }
                }
            };
            $.ajax(ajaxContent);
        }
    </script>


