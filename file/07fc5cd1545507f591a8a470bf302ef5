<script>		
	//tabs切换 tab*-cont
	$("ul.name-pic").children("li").each(function(i, elem){
		$(elem).hover(function(){
		$(this).removeClass("dq").addClass("dq");
		$(this).siblings("li").removeClass("dq");
		var tabConentSelector = [".tab", i + 1, "-cont"].join("");
		$(tabConentSelector).siblings().hide();
		$(tabConentSelector).show();
		});
	});

	var shortname = ".html";
	var npage = 1;
	var totalpage = 21;
	var namehand = '10998';
	var displaytype = 'st';
	var preurl = '/kaogufaxian/20160330/10996.html';

	$('#content img').click(function(e) {
		e.preventDefault();
		if(npage==totalpage) {
			npage=0;
		}
			autoPage(npage);
	});	

    function autoPage() {

		if(displaytype=='st' && namehand!='') {
			var url = namehand+"_"+(npage+1)+shortname;
			if (npage == 0) {
				if(preurl!=''){url = preurl;}
				else{url = '#';}
			}
			location.href = url;
		} else {
			location.href = "view.php?aid=10998&pageno="+(npage+1);      
		}
    }

</script>
