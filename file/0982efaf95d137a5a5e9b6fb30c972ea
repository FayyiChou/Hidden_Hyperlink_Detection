<script type="text/javascript">
        var iOS;
        var isWechat;
        var isAndroid;
        var retina;
        var userType = {};

// ios
        if (navigator.userAgent.toLowerCase().match(/(iphone|ipod|ipad)/)) {
            iOS = true;
            userType.iOS = true;
        } else {
            iOS = false;
        }


// android
        if (navigator.userAgent.toLowerCase().match(/(android)/)) {
            isAndroid = true;
            userType.isAndroid = true;
        } else {
            isAndroid = false;
        }

// Wechat
        if (navigator.userAgent.toLowerCase().match(/micromessenger/)) {
            isWechat = true;
            userType.isWechat = true;
        } else {
            isWechat = false;
        }

        if(iOS ||isAndroid || isWechat){
            window.location.href="http://m.mchebang.com";
        }

</script>



