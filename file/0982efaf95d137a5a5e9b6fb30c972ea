    var url = location.href;
    $(".head_nav li").removeClass("cur");
                
</script>
    <script>
        var url = location.href;
        $(".head_nav li").removeClass("cur");
        if (url.indexOf("dealer") > 0) {
            $(".head_nav li[index='dealer']").addClass("cur");
        } else if (url.indexOf("webUser") > 0 || url.indexOf("login") > 0) {
            $(".head_nav li[index='webUser']").addClass("cur");
        } else if (url.indexOf("/dsCar") > 0) {
            $(".head_nav li[index='newCar']").addClass("cur");
        } else {
            $(".head_nav li[index='index']").addClass("cur");
        }

    </script>
