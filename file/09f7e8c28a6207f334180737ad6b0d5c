	<script>
	function dosearch(f){
				var keys = f.keys.value;
				var sidVal = f.sidVal.value;
				if(keys==""){
						f.keys.focus();
						alert('请输入您要搜索的关键词');
						return false;	
				}
				var url="";
				if(sidVal==1){
					  url='/trade/list_keys-'+URLEncode(keys)+'.html';
				}else if(sidVal==2){
					  url='/trade/qz_keys-'+URLEncode(keys)+'.html';
				}else if(sidVal==3){
					  url='/trade/gy_keys-'+URLEncode(keys)+'.html';
				}else if(sidVal==4){
					  url='/news/list_keys-'+URLEncode(keys)+'.html';
				}else if(sidVal==5){
					  url='/company/list_keys-'+URLEncode(keys)+'.html';
					  
				}
				if(url!='')
					window.location=url;
				return false;
	}
	
	
	</script>
