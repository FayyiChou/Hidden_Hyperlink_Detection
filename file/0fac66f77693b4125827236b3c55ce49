				<a href="javascript:;" class="toright"></a>
				<div class="clearfix"></div>
			</div>
			<script type="text/javascript">


		var none_unit_width = 608; //每屏的偏移量
		var $parent = $(".tongbox");
		//设定默认根据pid打开的移动
		if( page !== 1 ){
			var leftdefault = 0 - (page - 1) * none_unit_width;
			$parent.css("left",leftdefault);  //通过改变left值，达到每次换一个版面
		}

		//常规

		if (totalpicnum - smallOffset > 8) {
			leftoffset = smallOffset + 8;
		} else {
			leftoffset = totalpicnum;
		}

		for (sid = smallOffset;sid < leftoffset ;sid++){
			//alert(sid);
			var this_pic = $(".tongbox img:eq("+sid+")");
			var this_src = $(".tongbox img:eq("+sid+")").attr("src");
			if(this_src == '/images/loading01.gif')
			{
				var oksrc = $(".tongbox img:eq("+sid+")").attr("oksrc");
				$(".tongbox img:eq("+sid+")").removeAttr("height");
				$(".tongbox img:eq("+sid+")").removeAttr("width");
				$(".tongbox img:eq("+sid+")").attr("src" , oksrc);
			}



			//$(".tongbox img:eq("+sid+")").removeAttr("oksrc");
		}

		//控制toleft toright效果
		$(".toleft").hover(
		function () {
			if ( page != 1 ) {
				//$(this).addClass("tolefthover");
			}
		},
		function (){
			if ($(this).hasClass("tolefthover")){
				$(this).removeClass("tolefthover");
			}
		}
		);
		$(".toright").hover(
		function () {
			if ( page != page_count ) {
				//$(this).addClass("torighthover");
			}
		},
		function (){
			if ($(this).hasClass("torighthover")){
				$(this).removeClass("torighthover");
			}
		}
		);
		$(".toleft").click(function(){
			if( !$parent.is(":animated") ){
				if( page !== 1 ){
					ga('pplay-prevbtn');
					page--;
					var left = 0 - none_unit_width*(page - 1)-2;
					$parent.animate({ left : left }, 800);  //通过改变left值，达到每次换一个版面

					var smallOffset = (page-1) * 8 ;
					for (si = smallOffset;si< (smallOffset + 8);si++){
						var oksrc = $(".tongbox img:eq("+si+")").attr("oksrc");

						var this_pic=$(".tongbox img:eq("+si+")");
						if(this_pic.attr('src') == '/images/loading01.gif')
						{
							this_pic.attr("src",this_pic.attr("oksrc"));

						}

					}
				}
			}
			return false;
		});
		$(".toright").click(function(){
			if( !$parent.is(":animated") ){
				if( page !== page_count ){
					//alert(page);
					//alert(page_count);
					ga('pplay-nextbtn');
					page++;
					var left = 0 - none_unit_width*(page - 1)-2;//偏移值2
					$parent.animate({ left : left}, 800);  //通过改变left值，达到每次换一个版面

					var smallOffset = (page-1) * 8 ;
					var totalpicnum = 9;
					var leftoffset;
					if (totalpicnum - smallOffset > 8) {
						leftoffset = smallOffset + 8;
					} else {
						leftoffset = totalpicnum;
					}
					for (si = smallOffset;si< leftoffset;si++){

						var this_pic=$(".tongbox img:eq("+si+")");
						if(this_pic.attr('src') == '/images/loading01.gif')
						{
							this_pic.attr("src",this_pic.attr("oksrc"));

						}

					}
				}
			};
			return false;
		});



			</script>
