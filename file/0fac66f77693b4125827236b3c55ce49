<script type="text/javascript">
	//<![CDATA[
	var shareTitle = '严重推荐#虎扑体育#强图《冉莹颖九连拍展示身材》';
	var shareUrl = 'http://photo.hupu.com/p13862-3.html';
	var shareSummary = '3月30日，邹市明娇妻冉莹颖在微博晒出一组自己的美图，细腰腰臀长腿一览无余，她还配上了浓浓的“鸡汤”：“一个完整真实的人啊，让盔甲和软肋共存，才能过得舒服。愿你的盔甲，都能保护软肋。”<br />';
	var titlepic = '//i1.hoopchina.com.cn/u/1703/31/480/3064480/547bd30fjpg.jpg';
	var vote = 1;
	var aid = "13862";
	var pid = "3";
	//图片缩略图的移动
	var page = 1; //当前页码
	//缩略图预先加载
	var smallOffset = (page-1) * 8 ;
	var totalpicnum = 9;
	var leftoffset;
	var i = 8; //每页图片多少张
	var len = 9; //总张数
	var page_count = Math.ceil(len / i) ;

	function big_img_onload(e)
	{
		//限制宽度
		var maxcontpicwidth = 664;
		if($(e).width() > maxcontpicwidth){
			$(e).width(maxcontpicwidth);
		}
		//预加载图片
		$("#yujiazai img").attr("src",$("#yujiazai img").attr("oksrc"));
	}

	function small_img_onload(f)
	{

		if($(f).attr('src') == '/images/loading01.gif')
		{
			return '';
		}
		$(f).removeAttr("height");
		$(f).removeAttr("width");
		var e = 60;
		var c = 60;
		if ($(f).width() < $(f).height()) {
			var d = $(f).height() / $(f).width() * c;
			$(f).css("top", (0 - Math.round((d - e) / 2)) + "px");
			$(f).attr("height", d);
			$(f).attr("width", e)
		} else {
			if ($(f).width() > $(f).height()) {
				var b = $(f).width() / $(f).height() * e;
				$(f).css("left", (0 - Math.round((b - c) / 2)) + "px");
				$(f).attr("height", c);
				$(f).attr("width", $(f).width() / $(f).height() * e)
			} else {
				$(f).attr("height", c);
				$(f).attr("width", e)
			}
		}
	}
	//键盘事件
	$(document).keyup(function(e)
	{

		//投票
		switch(e.which){
					//用键盘前后翻页
				//左键
			case 37:
				window.location.href="p13862-2.html";
				break;
			//右键
		case 39:
			window.location.href="p13862-4.html";
			break;
		}
	});
	$(document).ready(function() {
		//滚到位置
		//alert($(".tongbox img:eq(0)").attr("src"));
		$('#second_nav ul li').hoverIntent(
		function (){
			$(this).addClass('selected');
			if ($(this).attr('data'))
			{
				var menuName = '#m'+$(this).attr('data');
				$(menuName).show();
			}
		},
		function () {
			$(this).removeClass('selected');
			if ($(this).attr('data'))
			{
				var menuName = '#m'+$(this).attr('data');
				$(menuName).hide();
			}
		}
	)
		$("#nav_right li.j").hoverIntent(
		function(){
			$(this).addClass("bnav_over").find("ul").show()
		},function(){
			$(this).removeClass("bnav_over").find("ul").hide()
		}
	);

	});


	//]]>

</script>

