<script>
$(document).ready(function(){
	var timestamp = new Date().getTime();
	$.ajax({
                url: 'http://nlife.dchsoft.com/data2.asp?t='+timestamp,
                cache: true,
                async: false,
                dataType: "html", //If the response is json replace it with "json"
                /*beforeSend: function (XMLHttpRequest) {
                    ShowLoading();
                },
                complete: function (XMLHttpRequest, textStatus) {
                    HideLoading();
                },*/
                type: "GET",
                success: function (data, textStatus) {
				document.getElementById("myCarousel2").innerHTML=data;
//                    $(data).appendTo("#myCarousel2");

 $('.carousel').carousel({
  interval: false
 })
	$(function(){

        $('.carousel').click(function(e){
           $('#myCarousel, #myCarousel2').carousel( $(this).data() );
        });
	

    });//END document.ready
	
                    kk = 0;
                }
            });

});
</script>
