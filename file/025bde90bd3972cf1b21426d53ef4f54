       <script type="text/javascript">
var pageTotal = 808;
function changePage(pn)
{
	if (pn > pageTotal) {
		alert('超出最大页码范围');
		return;
	}
	var url = '/telecom';
	if (pn == 1){
		url += '_1.html';
	}else{
		url += '_' + pn + '.html';
	}
	location.href = url;
}
