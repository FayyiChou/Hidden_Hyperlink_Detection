<script>
$(document).ready(function(){
	
	$('.comiclist li p.text a').click(function(){
		var $thisp = $(this).parent();
		$thisp.css('display','none');
		var isIE6 		= $.browser.msie && ($.browser.version == "6.0") && !$.support.style;
		var $nextp_h = isIE6 ? $thisp.next('p').height() : $thisp.next('p').innerHeight();
		$thisp.next('p').css({display:'block',height:$thisp.css('height')}).animate({height:$nextp_h}, 300, 'swing');
	});
	$('.comiclist li p.textmore a').click(function(){
		var $thisp = $(this).parent();
		var $prevp_h = $thisp.prev('p').css('height');
		$thisp.animate({height:$prevp_h}, 300, 'swing', function(){																												 
			$thisp.css({display:'none',height:'auto'});											 
			$thisp.prev('p').css({display:'block'});
		});
	});
	
	$('#search_history').find('li').each(function(){
		var $this = $(this);
		$this.mouseover(function(){
			$this.children('p').addClass('del');
		}).mouseout(function(){
			$this.children('p').removeClass('del');
		}).find('em').click(function(){
			$this.remove();
			var del_value = $(this).siblings('a').html();
			var search_history = $.evalJSON($.cookie('search_history'));
			var new_search_history = {};
			for(k in search_history){
				if(search_history[k]!=del_value){
					new_search_history.push(search_history[k]);
				}
			}
			$.cookie('search_history', $.toJSON(new_search_history), { expires: 2592000, path: '/'});
		});
	});
	$('.del_all').click(function(){
		$('#search_history').find('li').remove();
		$.cookie('search_history', '', { expires: 2592000, path: '/'});							 
	});
});
function add_attention(fans_id){
	login_ajax(function(){
		var user=get_user();
		$.ajax( {
			type : "GET",
			url : "/i/ajax.php?mod=fans&act=add_attention&user_id="+user.id+"&fans_id="+fans_id,
			dataType : "json",
			cache:false,
			success : function(o) {
				if (o.code > 0) {
					popMsg(o.message);
				} else {
					showMsg(o.message);
				}
			}
		});
	});	
}


function favorite(comic_id){
	var user=get_user();
	$.ajax({
 		url: "/www/ajax.php?mod=comic&act=favorite",
 		data:  { user_id: user.id,comic_id: comic_id}, 
		type: "POST",
		dataType : 'json',
 		success: function(o){ 
 			showMsg( o.message); 
			if(o.code>0){
				$('#favnum_'+comic_id).html( parseInt($('#favnum_'+comic_id).html())+1 );
			}
 		} ,
 		error:function(){ showMsg('网络错误，请稍后重试');}
	});
}
</script>

</body></html>