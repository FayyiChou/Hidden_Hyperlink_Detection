<script language="javascript">
function menuControl(show)
{
window.event.cancelBubble=true;
var objID=event.srcElement.id;
var index=objID.indexOf("_");
var mainID=objID.substring(0,index);
var numID=objID.substring(index+1,objID.length);
if(mainID=="menubar")
{
if(show==1)
{
eval("showMenu("+"menu_"+numID+")");
}
else
{
eval("hideMenu("+"menu_"+numID+")");
}
}

}

var nbottom=0,speed=8;
function displayMenu(obj)
{
obj.style.clip="rect(0 100% "+nbottom+"% 0)";
nbottom+=speed;
if(nbottom<=100) 
{
timerID=setTimeout("displayMenu("+obj.id+"),70");
}
else clearTimeout(timerID);
}
function showMenu(obj)
{
obj.style.display="block";
obj.style.clip="rect(0 0 0 0)";
nbottom=5;
displayMenu(obj);
}
function hideMenu(obj)
{
nbottom=0;
obj.style.display="none";
}
function keepMenu(obj)
{
obj.style.display="block";
}
	</script>
<script language="JavaScript">
function ClipBoard() 
{
//document.getElementById("holdtext").innerText= copytext.innerHTML;
//Copied = document.getElementById("holdtext").createTextRange();
//Copied.execCommand("Copy");
mytext = copytext.innerHTML.replace(/&nbsp;/g,' ').replace(/<BR>/g,'\n');  
mytext = mytext.substring(mytext.indexOf('<FONT style="FONT-SIZE: 14px; LINE-HEIGHT: 26px">')+49,mytext.length-7);
document.getElementById("holdtext").innerText= mytext ;
Copied = document.getElementById("holdtext").createTextRange();
Copied.execCommand("Copy");
}
</script>




