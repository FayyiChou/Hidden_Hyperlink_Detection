<script language="javascript">
function window.onresize()
{
if (document.body.clientWidth < 1000 || document.body.clientHeight < 750)
    {
    document.body.scroll = "yes"
    this.width = 1024
    this.height = 768
    }
else
    {
    document.body.scroll = "no"
    this.width = "100%"
    this.height = "100%"
    }
}

window.onresize()


function sc()
{
document.getElementById("div1").scrollTop+=20
}
	</script>
