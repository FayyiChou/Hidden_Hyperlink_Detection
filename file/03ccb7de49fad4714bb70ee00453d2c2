	$('#tran_type').removeAttr('checked').next('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOff.png)');
	$("input[name='h']").attr('checked', 'true').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOn.png)');
	$('.btn_big').trigger("click");
});
