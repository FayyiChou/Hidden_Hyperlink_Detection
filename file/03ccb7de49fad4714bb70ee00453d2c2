	//重置
	function remake(){
		objFloatTip.find('.soTogether').attr('checked',false);
		objFloatTip.find('.allTogether').attr('checked',true);
		if(order_channel=='ganji'){
			objFloatTip.find(".checkbox").css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOff.png)');
			objFloatTip.find("[ke=checkAll]").css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOn.png)');
		}else{
			objFloatTip.find(".checkbox").css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOff.png)');
			objFloatTip.find("[ke=checkAll]").css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOn.png)');
		}

	}
	$('.btn_big_black').click(function(){
		remake();
	})
	$('.btn_big').click(function(){
		var isCheckAll = $('#tran_type').is(':checked');
		//alert(isCheckAll);return false;
		if(isCheckAll){
			var from_m = '黄山';
			var to_m = '北京';
			var date_m = '2017-04-03';
			location.href='http://m.tieyou.com/daigou?from='+from_m+'&to='+to_m+'&date='+date_m+'&utm_source=gaotie_h5';
			return false;
		}
		$('.sel_check_box').hide();
		var strTtanType = '',strFromTime='',strToTime='',fromTime='',toTime='',typ='';
		$('#check_txt1').find('.tran_type').each(function(){
			if($(this).is(':checked')){
				strTtanType += '#'+$(this).attr('name')+'#';
			}
		})

		$('#check_txt2').find('.fromTime').each(function(){
			if($(this).is(':checked')){
				strFromTime += '#'+$(this).attr('name')+'#';
			}
		})
		$('#check_txt3').find('.to_time').each(function(){
			if($(this).is(':checked')){
				strToTime += '#'+$(this).attr('name')+'#';
			}
		})
		//var altIndex = 1;
		$('.seo_sel').each(function(index){
			var fromTimeBool=0,toTimeBool=0,typBool=0;
			fromTime = '#'+$(this).attr('starttime')+'#';
			toTime = '#'+$(this).attr('totime')+'#';
			typ = '#'+$(this).attr('typ')+'#';
			if(strTtanType != ''){
				typBool = strTtanType.indexOf(typ);
			}
			if(strFromTime != ''){
				fromTimeBool = strFromTime.indexOf(fromTime);
			}
			if(strToTime != ''){
				toTimeBool = strToTime.indexOf(toTime);
			}
			if(typBool<0 || fromTimeBool <0 || toTimeBool<0 ){
				$(this).hide();
			}else{
				/* if(altIndex % 2 == 1){
					$(this).children('ul').addClass('alt');
				}else{
					$(this).children('ul').removeClass('alt');
				} */
				$(this).show();
				//altIndex++;
			}
		})
	})
})
//tab切换
function switchmodClickTag(){
	}
	switchmodClickTag.prototype = {
			st : function(menus,divs,openClass,closeClass){
				var _this = this;
				if(menus.length != divs.length)
				{
					alert("菜单层数量和内容层数量不一样!");
					return false;
				}
				for(var i = 0 ; i < menus.length ; i++)
				{
					_this.$(menus[i]).value = i;
					_this.$(menus[i]).onclick = function(){//如果想把效果变成点击切换，将此行onmouseover 改成onclick即可。

						for(var j = 0 ; j < menus.length ; j++)
						{
							_this.$(menus[j]).className = closeClass;
							_this.$(divs[j]).style.display = "none";
						}
						_this.$(menus[this.value]).className = openClass;
						_this.$(divs[this.value]).style.display = "block";
					}
				}
				},
			$ : function(oid){
				if(typeof(oid) == "string")
				return document.getElementById(oid);
				return oid;
			}
		}
	$(document).ready(function(){

		var STmodelClick = new switchmodClickTag();
		STmodelClick.st(["check_tab1","check_tab2","check_tab3"],["check_txt1","check_txt2","check_txt3"],"now","");//第一组动滑轮

	});
function showSX(){
	$(".sel_check_box").show();
