$(function(){
	var arrList = Array();
	function orderList(sortKey,order){
		return function(a,b){
			if(order == 1){
				return a[sortKey] - b[sortKey];
			}else{
				return b[sortKey] - a[sortKey];
			}
		}
	}
	function clearAddList(arrList){
		$('.main').html('');
		arrLength = arrList.length;
		//alert(arrList);return false;
		for(var i=0;i<arrLength;i++){

			$('.main').append(arrList[i][2]);
			//return false;
		}
	}
	function arrtohtml($arr,from,to,checi,date,channel_type){
		var buy='';
  		var channel = getCookie('order_channel');
  		var mobSysTypeDownAppUrl = $(".mobSysTypeDownAppUrl").val();
  		$arr.seat_name = $arr.seat_name.replace(/上|中|下/g,'');

  		if(strChannelConf.indexOf(channel)==-1){
			if($G("utm_source")!="minsheng" && getCookie("order_channel")!="minsheng"){
	  			if(channel_type != 'app' && channel != 'caibeiH5' && channel != 'etao' && $arr.seat_bookable!=1 && channel != 'huazhu' && channel != 'ganji'){
					buy+= '<li class="xq_btn">';
					buy+= '<a href="'+mobSysTypeDownAppUrl+'"  class="app_btn">免费预订</a></li>';
				}
			}
  		}
		var type = (channel_type=='app' || channel == 'caibeiH5') ? 'href' : 'url';
        if($arr.seat_bookable == 1){
        	buy+= '<li class="xq_btn">';
      	 	buy+= '<a app="'+mobSysTypeDownAppUrl+'" '+type+'="http://m.tieyou.com/buy/home.html?from='+from+'&to='+to+'&ymd='+date+'&checi='+checi+'&seat='+$arr.seat_name+'&channel_type='+channel_type+'" class="book_btn">预订</a></li>';
        }
		return buy;
	}

	function loadShow($this){
		//var load = '<ul><li class="loading"><img src="//img3.tieyou.com/images/loading4.gif" /></li></ul>'
		//$this.prepend(load);
		$this.children().hide();
		$this.children().eq(0).show();
		$this.show();
	}
	function messShow($this){
		$this.next().children().each(function(index){
			index > 0 ? $(this).show() : $(this).hide();
		})
		$this.next().show();
	}

	function zhankaiFun(jQueryObj){

		var channel = getCookie('order_channel');
			channel = channel ? channel : $G("utm_source");
		var buy = getCookie('buy');
			buy = buy ? buy : $G('buy');
		//alert(buy);return false;
		if(channel != 'baixing' && channel != 'minsheng' && channel != 'etao' && channel != 'jxtelehb' && buy != 'close' && channel != 'huazhu'){
			if(getCookie('_tieyou_app_dl_') != 'Y'){
				//location.href = 'http://www.tieyou.com/phone/ARK-ark_andriod_list_mfcgg.html';
				//auth:laoe
				if(app_sys_type == 'ios')
				{
					location.href = 'tieyouticket99://';
					appRDClick();
				}
				else if(app_sys_type == 'android')
				{
					location.href = 'http://m.tieyou.com/download/android.html';
					appRDClick();
				}
				else{
					location.href = 'http://www.tieyou.com/phone/ARK-ark_andriod_list_mfcgg.html';
				}
			}
			writeCookie('_tieyou_app_dl_', 'Y', 24*365);
		}

		var $from = $(jQueryObj).parent().parent().attr('from');

		var $to = $(jQueryObj).parent().parent().attr('to');
		var $checi = $(jQueryObj).parent().parent().attr('checi');
		var $date = $(jQueryObj).parent().parent().attr('date');
		var $channel_type = $(jQueryObj).parent().parent().attr('channel_type');
		var from = '&from='+$from;
		var to = '&to='+$to;
		var checi = '&checi='+$checi;
		var date = '&ticket_date='+$date;
		var html = '';
		var url = 'http://www.tieyou.com/mobile/checkTicketCanBuy.html?jsoncallback=?'+from+to+checi+date;


		var _this = $(jQueryObj).parent().parent();
		loadShow(_this.next());
		//alert('ggggggg');return false;
		var thisobj=$(jQueryObj);
		$.getJSON(url,function(date){
			$.each(date['return'],function(key,item){

				if(item.seat_id == 14){
					item.seat_id = 1;
				}
				if(item.seat_id == 13){
					item.seat_id = 5;
				}

				item.seat_bookableHtml = arrtohtml(item,$from,$to,$checi,$date,$channel_type);

				_this.next().children().each(function(index){
					var seat_id = $(this).attr('seat_id');

					if(seat_id == item.seat_id){
						if(item.seat_bookable==1 ){
							//硬座或二等座直接购
							if(seat_id == "1" || seat_id == "14")
							{
								location.href = "http://m.tieyou.com/buy/home.html?from=" + $from + "&to=" + $to + "&ymd=" + $date + "&checi=" + $checi + "&seat=" + item.seat_name + "&channel_type=" + $channel_type;
								//return;
							}

							yupiaoInfo="有票";
						}else{
							yupiaoInfo="无票";
						}
						$(this).find(".xq_yupiao").html(yupiaoInfo);
//						$(this).find('.yudingbtn').html(item.seat_bookableHtml);
					}
				})

				messShow(_this);

				var free=$G("free");
				if(free=="close" || getCookie("free")=="close"){
					$(".xq_app").hide();
				}
			})

			_this.parent().addClass("sel_list_show");
			_this.next().show();
		})
    }

	function shouqiFun(jQueryObj){
		var _this = $(jQueryObj).parent().parent();
		_this.parent().removeClass("sel_list_show");
		_this.next().hide();
	}

	$('.sel_list_tab').live('click', function(){

	    if( typeof($(this).attr('clickdata')) == 'undefined' ||  $(this).attr('clickdata') != 'shouqi_but')
		{
	    	//alert('fffee11');return false;
			$(this).attr('clickdata', 'shouqi_but');
			zhankaiFun($(this).find('.zhankai').eq(0));
		}
		else
		{

			$(this).removeAttr('clickdata');
			shouqiFun($(this).find('.shouqi').eq(0));
		}
	});

