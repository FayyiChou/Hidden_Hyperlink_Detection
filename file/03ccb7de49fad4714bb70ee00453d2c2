<script>
$(function(){
	var top=$G("top");
	var ad=$G("ad");
	var login=$G("login");
	var free=$G("free");
	var minshengus=$G("utm_source");

	if(top=="close" || getCookie("top")=="close"){
		$(".foot").hide();
		SetCookie("top","close");
		$(".appdownclose").hide();
	}
	if(ad=="close"){
		SetCookie("ad","close");
	}
	if(login=="close"){
		SetCookie("login","close");
	}
	if(free=="close"){
		SetCookie("free","close");
	}

	if(minshengus=="minsheng" || getCookie("order_channel")=="minsheng" ){
		$(".appdownclose").hide();
	}

})
</script>

