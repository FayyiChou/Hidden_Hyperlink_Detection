	function  getOrderList(){
		$('.seo_sel').each(function(index){
			var travel_time,from_time =  $(this).attr('from_time').replace(':','');
			//var travel_time = $(this).attr('travel_time').replace('天','0').replace('小时','.').replace('分','');

				if($(this).attr('travel_time').indexOf("小时") <0){
					travel_time = $(this).attr('travel_time').replace('天','0').replace('小时','.').replace('分','');
					travel_time = travel_time/100;
				}else{
					travel_time = $(this).attr('travel_time').replace('天','0').replace('小时','.').replace('分','');
				}
			var list_text =$(this).clone();
			//alert(list_text);return false;
			arrList[index] = Array(from_time,travel_time,list_text);
		})
		//alert(arrList);return false;
		return arrList;
	}
	$('#from_time').click(function(){
		listOrder($(this),0,'from_time');
	})
	$('#travel_time').click(function(){
		listOrder($(this),1,'travel_time');
	})


	//出发时间  旅行耗时排序     $this ID对象  orderNumber 排序名 既(时间,耗时) ID 是区分时间跟耗时排序方式    时间是先大后下  旅行是先小后大
	function listOrder($this,orderNumber,$id){
		var className = $this.attr('class'),toClass='',soClass='';
		toClass = className == 'time_up' ? 'time_down' :'time_up';
		if(className == 'time_up'){
			toClass = 	'time_down';
			soClass = 0;
		}else{
			toClass = 	'time_up';
			soClass = 1;
		}
		$this.attr('class',toClass);
		arrList = getOrderList();
		arrList.sort(orderList(orderNumber,soClass));
		clearAddList(arrList);
		//listEven();
	}
	function listEven(){
		var altIndex = 1;
		//隔行换色
		$('.sel_list_tab').each(function(index){
			if ($(this).parent().css('display') != 'none') {
				if (altIndex % 2 == 1) {
					$(this).addClass("alt");
				}else {
					$(this).removeClass("alt");
				}
				altIndex++ ;
			}
		})
	}
