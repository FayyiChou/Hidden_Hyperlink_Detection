$(function(){
	if(order_channel=='ganji'){
		$('.checkAll').css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOn.png)');
	}else{
		$('.checkAll').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOn.png)');
	}

	var from_time = $G('time');		//时间段筛选
	var tran_type = $G('type');		//列车类型筛选
	if(from_time != ''){
		if(order_channel=='ganji'){
			$('#fromTime').removeAttr('checked').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOff.png)');
			$('.fromTime[name='+from_time+']').attr('checked', 'true').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOn.png)');
		}else{
			$('#fromTime').removeAttr('checked').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOff.png)');
			$('.fromTime[name='+from_time+']').attr('checked', 'true').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOn.png)');
		}
	}

	if(tran_type == 'h' || tran_type == 'p'){
		if(order_channel=='ganji'){
			$('#tran_type').removeAttr('checked').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOff.png)');
			$('.tran_type[name='+tran_type+']').attr('checked', 'true').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOn.png)');
		}else{
			$('#tran_type').removeAttr('checked').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOff.png)');
			$('.tran_type[name='+tran_type+']').attr('checked', 'true').nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOn.png)');
		}
	}

	if(from_time != '' || tran_type != ''){
		$('.btn_big').trigger('click');
	}

	$('.float_check_list li').click(function(){
		var _this = $(this).find('input');
		var div =  $(this).find('div');
		var _class = _this.attr('class');
		var idtoclass = _this.attr('id');
		var checkAll = _class.indexOf('all');
		var strClass = _this.attr('class').split(' ');
		if(idtoclass){
			$('.'+idtoclass).attr('checked',false);
			if(order_channel=='ganji'){
				$('.'+idtoclass).nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOff.png)');
			}else{
				$('.'+idtoclass).nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOff.png)');
			}
		}else{
			$('#'+strClass[0]).attr('checked',false);
			if(order_channel=='ganji'){
				$('#'+strClass[0]).nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOff.png)');
			}else{
				$('#'+strClass[0]).nextAll('div').css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOff.png)');
			}
		}

		if(idtoclass && _this.is(':checked') == true){
			//不做操作
		}else{
			_this.attr('checked',!_this.is(':checked'));
		}
		if(_this.is(':checked')){
			if(order_channel=='ganji'){
				div.css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOn.png)');
			}else{
				div.css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOn.png)');
			}
		}else if(!idtoclass){
			if(order_channel=='ganji'){
				div.css('background-image','url(//img3.tieyou.com/images/mobile/gj/input_checkOff.png)');
			}else{
				div.css('background-image','url(//img3.tieyou.com/images/mobile/input_checkOff.png)');
			}
		}
	});

	//预订按钮绑定下载APP
	$('.xq_btn a').live('click', function(){
//		if(this.href == undefined || this.href == ''){
			//if(getCookie('_tieyou_app_dl_') != 'Y'){
			//	location.href = ($(this).attr('app') == '') ? $(this).attr('url') : $(this).attr('app');
			//	writeCookie('_tieyou_app_dl_', 'Y', 24*365);
			//}else{
				location.href = $(this).attr('url');
