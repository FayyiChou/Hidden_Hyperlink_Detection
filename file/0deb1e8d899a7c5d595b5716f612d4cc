  <script type="text/javascript">			
				K(function(){  				
					KK('#listZone div.INASEANtpList').each(function (obj, index) {
						 obj.hover(
							function(){ this.css("background:#f9fafd") },
							function(){ this.css("background:#fff")}
						)
					});
					if(K.B.ie6){
						KK('#upBtn').css("position:absolute;right:0px;"); 
					}
					KK('#upBtn').css("display:none;"); 
					window.onscroll = function(){
						if((document.documentElement.scrollTop || document.body.scrollTop) > 500){
							KK('#upBtn').css("display:block;"); 
						}else{
							KK('#upBtn').css("display:none;"); 	
						}
						if(K.B.ie6){
							document.getElementById('upBtn').style.top =  document.documentElement.scrollTop + document.documentElement.clientHeight - 58 + 'px';
						}
					}
				})  
</script> 
