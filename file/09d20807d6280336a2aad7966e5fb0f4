<script type="text/javascript">
	$(window).load(function() {
		$('#rotator ul li:not(:first)').css({opacity: 0});
		setInterval(function() {
			var cur_element = $('#rotator .show');
			var next_element = (cur_element.next('li').length) ? cur_element.next('li') : $('#rotator ul li:first');
			
			next_element.css({opacity: 0.0})
			.addClass('show')
			.animate({opacity: 1.0}, 1500);
 
			cur_element.animate({opacity: 0.0}, 1500)
			.removeClass('show');
		},
		5000);
		
	});
</script>	  
