	function fkLogTime(keyName){
		var This = (typeof Fai != "undefined" && Fai.top) || window;

		if(!This._fkTestMode){
			return;
		}
		var time = new Date().getTime();
		typeof console != "undefined" && console.log && console.log(keyName+" : "+((time - This._startTime)/1000)+"秒");
	}
	
