$(window).load(function(){
  var _top=$('#img_weixin').offset().top;
  var _top2=$('.love').offset().top;
  var _imgHeight=$("#bigimg").height();
  $(window).scroll(function(){
    if(_imgHeight>=1400){
      if($(this).scrollTop()>_top){
        $('#img_weixin').addClass('div2')
        if($(this).scrollTop()>_top2-580){
          $('#img_weixin').removeClass('div2')
          $('#img_weixin').addClass('div3')
        }
        else{
          $('#img_weixin').removeClass('div3')
          $('#img_weixin').addClass('div2')
        }
      }
      else{
        $('#img_weixin').removeClass('div2')
      }
    }
  })
})

function getDigg(aid)

{

	var taget_obj = document.getElementById('newdigg');

	myajax = new DedeAjax(taget_obj,false,false,'','','');

	myajax.SendGet2("/plus/digg_ajax.php?id="+aid);

	DedeXHTTP = null;

}

  var shortname = ".html";

  var npage = 4;

	var totalpage = 6;

	var namehand = '1359';

	var displaytype = 'st';

	var gtimer = null;

	//大图

	function dPlayBig()

	{

		var imgObj = document.getElementById("bigimg");

		window.open(imgObj.src);

	}

	//停止幻灯

	function dStopPlay()

	{

		if(gtimer) clearTimeout(gtimer);

		else dPlayNext();

	}

	//开始幻灯

	function dStartPlay()

	{

		if(npage!=totalpage) {

			gtimer = setTimeout("dPlayNext()",10000);

		}

	}



	//上一张

	function dPlayPre(){



	if(npage<2)

	{
		var purl = "/snmh/1357.html";
		if(purl=='javascript:;' || purl=='' ){
			location.href = $("#randurl").attr('href');
		}else{
			location.href = purl;					
		}
		//window.location.href='/snmh/1357.html';
	}

	else

	{

			if(npage==2) {

				if(namehand!='') location.href = namehand+shortname;

				else location.href = "view.php?aid=1359";

			} else if(displaytype=='st' && namehand!='') {

				location.href = namehand+"_"+(npage-1)+shortname;

			} else {

				location.href = "view.php?aid=1359&pageno="+(npage-1);

			}

		}

	}



	//下一张

	function dPlayNext()

	{

		if(npage==totalpage) {
			
			if($("#nexturl").attr('href')=='javascript:;' || $("#nexturl").attr('href')=='' ){
				location.href=$("#randurl").attr('href');
			}else{
				location.href=$("#nexturl").attr('href');					
			}
			//window.location.href='/snmh/1360.html'; 
		}

		else

		{

			if(displaytype=='st' && namehand!='') location.href = namehand+"_"+(npage+1)+shortname;

			else location.href = "view.php?aid=1359&pageno="+(npage+1);

		}

	}

$(function(){
	$("#nexturl").click(function(e){
		e.preventDefault();
		if(totalpage>1){
			if(displaytype=='st' && namehand!='') location.href = namehand+"_"+(npage+1)+shortname;
			else location.href = "view.php?aid=1359&pageno="+(npage+1);
		}else{
			if($(this).attr('href')=='javascript:;' || $(this).attr('href')=='' ){
				location.href=$("#randurl").attr('href');
			}else{
				location.href=$(this).attr('href');					
			}
		}
	});	
})
var img_w,img_h;
$(window).load(function(){

	img_w = $("#bigimg").width();	
	img_h = $("#bigimg").height();
	if(img_w > 560){
		img_h = Math.ceil( 560 * img_h / img_w );
		img_w = 560;
		$("#bigimg").css('width',img_w);
		$("#bigimg").css('height',img_h);	
	}
})


