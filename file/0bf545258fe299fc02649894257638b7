<script type="text/javascript">
window.onload=function(){
  new Marquee(
    "scroll", 
    0,  //向上滚动(0向上 1向下 2向左 3向右)<br />
    2,  //滚动的步长<br />
    761,  //容器可视宽度<br />
    84,  //容器可视高度<br />
    50,  //定时器 数值越小，滚动的速度越快(1000=1秒,建议不小于20)<br />
    3000,  //间歇停顿时间(0为不停顿,1000=1秒)<br />
    1000,  //开始时的等待时间(0为不等待,1000=1秒)<br />
   84//间歇滚动间距(可选)<br />
    );
  };

	function check_login(form){
		if(form.name.value == ""){
			form.name.focus();
			return false;
		}
		if(form.pass.value == ""){
			form.pass.focus();
			return false;
		}
		return true;
	}

	function check_search(form){
		if(form.kw.value == ""){
			form.kw.focus();
			return false;
		}
		return true;
	}
</script>


