<script type="text/javascript">
	var cur_base_id = "1001";
	if($G('nav_'+cur_base_id)){
		$G('nav_'+cur_base_id).className = "navbar_on";
		$G('class_'+cur_base_id).style.display = "block";
	}

	function mover(id){
		if( ! $G('nav_'+id))return;
		$G('nav_'+id).className = "navbar_on";
		$G('class_'+id).style.display = "block";
		if((id != cur_base_id)&&$G('nav_'+cur_base_id)){
			$G('nav_'+cur_base_id).className = "navbar_normal";
			$G('class_'+cur_base_id).style.display = "none";
		}
	}

	function mout(id){
		if( ! $G('nav_'+id))return;
		$G('nav_'+id).className = "navbar_normal";
		$G('class_'+id).style.display = "none";
		if($G('nav_'+cur_base_id)){
		$G('nav_'+cur_base_id).className = "navbar_on";
		$G('class_'+cur_base_id).style.display = "block";
		}
	}
</script>

