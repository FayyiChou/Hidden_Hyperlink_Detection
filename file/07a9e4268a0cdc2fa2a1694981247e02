                    if (maxWidth < currentWidth)
                        maxWidth = currentWidth;
                });
                $(this).children("ul").css({ "width": maxWidth + "px", "right": "-" + maxWidth + "px" });
            }
        });
        if (isDivider)
            jQuery("#nav_" + 'view_navigator_98_277124373').children("li:last").children("h3").children("a").css("border-bottom-width", "0px");//去掉最后一项的分隔线
        if ("False" == "False") {
            jQuery("#" + 'view_navigator_98_277124373').css("z-index", "999999");
            if (jQuery("#" + 'view_navigator_98_277124373').parent().attr("class") != "runTimeflowsmartView") {//导航可能放在容器控件中了，需要把容器控件的z-index的值设大
                jQuery("#" + 'view_navigator_98_277124373').parent(".w_container_content").parent().parent().css("z-index", "999998");
            }
        }
        SetNavSelectedStyle("nav_" + 'view_navigator_98_277124373', 'vnav');//选中当前导航
    })

</script>
