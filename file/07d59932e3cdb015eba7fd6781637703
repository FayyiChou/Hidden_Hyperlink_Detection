 <script>    
  $('#login_submit').click(function() {
  	if($('#login').val() && $('#passwd').val()){
      $('#frm_login').ajaxSubmit({success:showResponse}); 
	  
      return false;
    } 
  });

  function showResponse(responseText, statusText)  {

     if(responseText){
     $("#slogin").html(responseText);
	 }else{
	 	window.location.href="/member/?reurl=%2Fmember%2F&exp=incorrect&_d=member&_a=login";
	 }
  }

  if(/LOGIN_USER_ID/.test(document.cookie)){
     $.get("/api/index.php?f=get_login_status", function(result){
         $("#slogin").html(result);
     });
  }
