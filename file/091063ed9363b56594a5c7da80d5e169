	function search_loadGtid(gameid) {
		var game = {};
		$.each(allgames, function(i, item){
			if (item.gameid == gameid) {
				game = item;
				return false;
			}
		});
		$('#gtidSelect').empty();
		
		$('#gtidSelect').append('<dt><a href="/buy-'+gameid+'-type-0.html"><span>全部类型</span></a></dt>');
		$.each(game.splxids.split(','), function(i, item) {
			var lxid = item.split(':')[0];
			var lxname = item.split(':')[1];
			 if('100137' == lxid){
				$('#gtidSelect').append('<dd class="gc-select"><a href="/goods-buying-'+gameid+'-'+lxid+'.html"><span><font color="red">首充号代充</font><em>热</em></span></a></dd>');
			}else if('100136' == lxid){
				$('#gtidSelect').append('<dd class="gc-select"><a href="/buy-'+gameid+'-type-'+lxid+'.html"><span><font color="red">首充号</font><em>热</em></span></a></dd>');
			}else if('100131' == lxid){
				$('#gtidSelect').append('<dd class="gc-select"><a href="/goods-buying-'+gameid+'-'+lxid+'.html"><span><font color="red">安卓版代充</font><em>热</em></span></a></dd>');
			};
		});
		$.each(game.splxids.split(','), function(i, item) {
			var lxid = item.split(':')[0];
			var lxname = item.split(':')[1];
			 if('100137' != lxid&&'100136' != lxid&&'100131' != lxid){
				$('#gtidSelect').append('<dd><a href="/buy-'+gameid+'-type-'+lxid+'.html"><span>'+lxname+'<em>热</em></span></a></dd>');
			}
		});
		return false;
	}
  </script>
