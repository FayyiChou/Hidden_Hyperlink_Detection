      <!-- 咨询信息 --> 
      <script type="text/javascript">
            $(function(){ 
                window.onload=function(){ SearchQuestion();}

                $("#btnQuestion").click(function(){
                    /*-咨询问题层-*/
                    CreateBorder($("#questionLayer"), 0.5);
                });
     
                $(".lblCloseDeleteQuestion_t").click(function () {
                    //alert($(document).scrollTop());
                    HidePopup($("#questionLayer"));   //隐藏
                    HidePopup($("#questionLayer2"))
                });
                $(".btnReset").click(function(){
                    $("#Content").val("");
                    $("#Content2").val("");
                });

                //咨询关键字 - 搜索国家
                $("#keywordContent").focus(function(){ 
                    countryFocus = true;
                    var tThis = $(this);
                    if(tThis.val()=="请输入咨询关键字"){
                        tThis.val("");
                    }
                    var tTop = tThis.parent("span").offset().top+tThis.height()+1;
                    var tLeft = tThis.parent("span").offset().left;
                    $("#popupcountryadt").css({ "top": tTop, "left": tLeft }).show();
                }).blur(function(){
                    if ($(this).val()=="") {
                        $(this).val("请输入咨询关键字");
                    }
                });
             });

            /*-- AjaxAdvisorySearch.cshtml 赋值 */
            function ThisVal(t) {
              $('#keywordContent').val(t);
              $("#popupcountryadt").hide();
              SearchQuestion();
              $("#ajaxList").slideDown();
              $("#hideShow").val("收起内容");v=1;
            }

            function btnSearchQuestion()
            {
                SearchQuestion();
                $("#ajaxList").slideDown();
                $("#hideShow").val("收起内容");v=1;
            }

            function SearchQuestion() {
                var day = new Date()
                $.ajax({
                    url: "/Portal/Visa/GetQuestionList?a=" + day.getUTCMilliseconds(),
                    type: "get",
                    dataType: "html",
                    data:{"VisaId":$("#VisaId").val(),"CountryName":"","keywordContent":$("#keywordContent").val()},
                    success: function (data) {
                        $("#ajaxList").html(data);
                    }, error: function () {
                        //alert("error1");
                    }
                });
            };
            /*-ajax 分页-*/
            function ajaxPaging() {
                $.ajax({
                    url: encodeURI(arguments[0]),
                    type: "get",
                    dataType: "html",
                    success: function (data) {
                        $("#ajaxList").empty().html(data);
                    }, error: function () {
                        //alert("系统正在维护，请稍后再试.");
                    }
                });
            }
            /*-提交咨询问题-*/
            function ajaxAddQuestionSubmit()
            {
                if($("#Content").val()!=""){
                    $.ajax({
                        url: "/Portal/Visa/AddQuestionInfo",
                        type: "get",
                        dataType: "Json",
                        data:{"VisaId":$("#VisaId").val(),"NickName":$("#NickName").val(),"Content":$("#Content").val(),"CountryName":$("#Keyword").val()},
                        success: function (data) {
                            if (parseInt(data) > 0) {
                                alert("您咨询的问题已提交！");
                                $("#Content").val("");
                                HidePopup($("#questionLayer"));
                                SearchQuestion(); 
                            }else{
                                alert("您咨询的问题提交失败，请咨询客服！");
                            }
                        }, error: function () {
                            
                        }
                    });
                }else{
                    alert("请输入要咨询的内容");
                }
            }
            
            function AnswerShow(t){
                $("#QuestionId").val(t);
                /*-问题回复答案层-*/
                CreateBorder($("#questionLayer2"), 0.5);
            };

            /*-提交问题回复答案-*/
            function ajaxAddAnswerSubmit()
            {
                if($("#Content2").val()!=""){
                    $.ajax({
                        url: "/Portal/Visa/AddAnswerInfo",
                        type: "get",
                        dataType: "Json",
                        data:{"QuestionId":$("#QuestionId").val(),"NickName":$("#NickName2").val(),"Content":$("#Content2").val()},
                        success: function (data) {
                            if (parseInt(data) > 0) {
                                alert("您的回复已提交！");
                                $("#Content2").val("");
                                HidePopup($("#questionLayer2"));
                                SearchQuestion(); 
                            }else{
                                alert("您的回复提交失败！");
                            }
                        }, error: function () {
                            alert("error3");
                        }
                    });
                }else{
                    alert("请输入回复信息");
                }
            }
        </script> 
      <div class="visa_div lab" style="height: 100%; margin-top:10px;"> 
       <div class="divtitle"> 
        <strong>咨询信息</strong> 
       </div> 
       <div class="bg"> 
        <span> <input id="keywordContent" type="text" value="请输入咨询关键字" /></span> 
        <input class="smallbtn_1" type="button" value="查找问答" onclick="btnSearchQuestion()" /> 
        <input class="smallbtn_1" id="btnQuestion" type="button" value="我要咨询" /> 
        <input id="hideShow" type="button" value="查看内容" style=" float:right; border:1px dotted gray; margin:10px 15px 0 0;" /> 
       </div> 
       <div id="ajaxList"> 
       </div> 
