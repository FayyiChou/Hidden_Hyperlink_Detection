  <script type="text/javascript">
    var v =0;
    $(document).ready(function () {
      jQuery.jqtab = function (tabtit, tab_conbox, shijian) {
        $(tab_conbox).find("li").hide();
        $(tabtit).find("li:first").addClass("thistab").show();
        $(tab_conbox).find("li:first").show();

        $(tabtit).find("li").bind(shijian, function () {
          $(this).addClass("thistab").siblings("li").removeClass("thistab");
          var activeindex = $(tabtit).find("li").index(this);
          $(tab_conbox).children().eq(activeindex).show().siblings().hide();
          return false;
        });
      };
      /*调用方法如下：*/
      $.jqtab("#tabs", "#tab_conbox", "click"); //click or mouseenter

      //签证样品示例
        $(".btnSampleImg").colorbox({
            rel: 'btnSampleImg',
            maxWidth:750, //设置内容的最大宽度
            opacity:0.3, //透明度
        });

        $(".printVisaInfo").click(function(){
            var url = "/Portal/Visa/PrintVisaInfo?visaId=612&pvNum=";
            $("#tabs").find("li").each(function () {
                if($(this).attr("class")=="thistab"){
                    window.open(url+$(this).attr("data-val"),"_blank");
                }
            })
        }) 

        $("#btnvisat").click(function(){
            var url = "/Portal/Visa/RequiredInfo?visaId=612&pvNum=";
            $("#tabs").find("li").each(function () {
                if($(this).attr("class")=="thistab"){
                    window.open(url+$(this).attr("data-val"),"_blank");
                }
            })
        })

        //查看内容，收起内容
        $("#ajaxList").hide();

        $("#hideShow").click(function(){ 
            //var name = $("#hideShow").val();alert(name);
            if(v==0){
                $("#ajaxList").slideDown();
                $("#hideShow").val("收起内容");v=1;
                }else {
                $("#ajaxList").slideUp();
                $("#hideShow").val("查看内容");v=0;
            }
        })

        /*-- 显示特别提示 */
        $("#openread").toggle(
            function(){
                $("#readtable").slideDown();
                $("#openread").html('收起阅读')
            },
            function(){
                $("#readtable").slideUp();
                $("#openread").html('展开阅读')
            }
        );
    });
  </script> 
