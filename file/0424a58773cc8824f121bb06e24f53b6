   <script type="text/javascript">
            $(function () {
                seaCity();
            });

            var seaCity = function () {
                var sea_city = $(".sea_city .sea_city_in"), city_com = $(".sea_city .sea_city_com");
                var inc = "sea_city_inc";
                var sea_close = $(".sea_city_com a.btn_close")
                sea_city.click(function () {
                    var no = city_com.css("display");
                    if (no == "none") {
                        $(this).addClass(inc);
                        $(this).children("em").addClass("city_ico_x")
                        city_com.show();

                        city_com.children(".sea_city_com_d").children("a").click(function () {
                            sea_city.childrens("b").text($(this).text());
                            cityHide();
                        })
                    }
                    else {
                        cityHide();
                    }
                })
                sea_close.click(function () {
                    cityHide();
                })
                $("body").click(function (event) {
                    if (city_com.css('display') != 'none') {
                        var x = event.pageX - city_com.offset().left;
                        var y = event.pageY - city_com.offset().top;
                        var h = $('.sea_city').innerHeight();
                        var w = $('.sea_city').innerWidth();
                        h = parseInt(h); w = parseInt(w);
                        if (x < 0 || x > w || y < -30 || y > h) {
                            cityHide();
                        }
                    }
                })
                $("input.sea_text").focus(function () {
                    $(this).val("");
                })
                $("input.sea_text").blur(function () {
                    if ($(this).val() == "")
                        $(this).val("请输入目的地、主题或关键字");
                })

                var cityHide = function () {
                    city_com.hide();
                    sea_city.removeClass(inc);
                    sea_city.children("em").removeClass("city_ico_x")
                }

            }
 

        </script> 
