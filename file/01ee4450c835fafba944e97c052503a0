<script type="text/javascript">
function loginSubmit() {
	if (JSer("#userRegister").val() == '') {
		alertMessage("请先输入用户名后登录！","userRegister");
		return false;
	}
	if (JSer("#userPwd").val() == '') {
		alertMessage("请先输入密码后登录！","userPwd");
		return false;
	}
	JSer.url("/nhcms/user/do_login.jd").sel("#topLoginForm :input").ajax({
		method : 'post',
		type : 'json',
		success : function(d) {
			if (d.result == 'success') {
				JSer('#userInfo').html("<span>" + d.userName + "，欢迎您！</span>" + 
					"<a href='/nhcms/i/data.jd'>个人中心</a>" + 
					"<a href='javascript:void(0)' id='logout' onclick='$(\"#pop\").attr(\"class\",\"index displayyes\");$(\"#bg\").css(\"display\",\"block\");$(\"body\").css(\"overflow\",\"hidden\");'>退出</a>");

				if (window.location.href.indexOf("bureaux/parlor") > 0) {
					location.reload();
				}
			} else {
				alertMessage(d.result,"");
			}
		}
	});
}

function logout() {
	JSer.url("/nhcms/user/do_logout.jd").sel("#topLoginForm :input").ajax({
		method : 'post',
		type : 'json',
		success : function(d) {
			if (d.result == 'success') {
				
				JSer('#userInfo')
						.html(
								"<form action='/nhcms/user/do_login.jd' method='post' id='topLoginForm'>"
										+ "<span>账号：</span><input type='text' id='userRegister' name='userRegister' class='topUserInput'><span>密码：</span>"
										+ "<input type='password' id='userPwd' name='userPwd' class='topUserInput'>"
										+ "<input value='登录' type='button' onclick='loginSubmit()' class='topLogin'/>"
										+ "<a href='/nhcms/user/register.jd' target='_blank'>立即注册</a></form>");
				if (window.location.href
						.indexOf("bureaux/parlor") > 0) {
					location.reload();
				}
			} else {
				alertMessage(d.result,"");
			}
		}
	});
}
</script>
<div id="bg">
<div id="pop" class="displaynone">
