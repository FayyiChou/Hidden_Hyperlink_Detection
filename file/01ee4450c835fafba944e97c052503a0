			<div onclick="logout();$('#pop').attr('class','displaynone');$('#bg').css('display','none');$('body').css('overflow','auto');" class="btn_determine">确定</div>
			<div onclick="$('#pop').attr('class','displaynone');$('#bg').css('display','none');$('body').css('overflow','auto');" class="btn_cancel">取消</div>
