<script language="javascript">
	$(".float_gg_groups_table").mouseover(function(){
		$(this).addClass('slide-hover-on');
		$(".float_gg_groups_table A").each(function(){
			var o = $(this);
			o.mouseover(function(){
				o.addClass('item-active');
			});
			o.mouseout(function(){
				o.removeClass('item-active');
			});
			
		});
	});
	$(".float_gg_groups_table").mouseout(function(){
		$(this).removeClass('slide-hover-on');
	});

	
</script>
