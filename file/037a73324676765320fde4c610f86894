<script language="javascript">
	function show_t_forms_ele(n){
		if(n!=1)document.getElementById('t_s_form_tab_1').className='';
		if(n!=2)document.getElementById('t_s_form_tab_2').className='';
		if(n!=3)document.getElementById('t_s_form_tab_3').className='';
		if(n!=4)document.getElementById('t_s_form_tab_4').className='';
		document.getElementById('t_s_form_tab_'+n).className='active';

		if(n!=1)document.getElementById('t_s_form_1').style.display='none';
		if(n!=2)document.getElementById('t_s_form_2').style.display='none';
		if(n!=3)document.getElementById('t_s_form_3').style.display='none';
		if(n!=4)document.getElementById('t_s_form_4').style.display='none';

		document.getElementById('t_s_form_'+n).style.display='block';
	}
	function __nohandle(v1,v2){}
</script>
