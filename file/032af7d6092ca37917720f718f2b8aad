	function $(id){
		return document.getElementById(id);
	}
	function initDropDownColumn(){
		var showinfo=new Array("<td width='39' align='center' onmouseover=this.className='bg1'; onmouseout=this.className='';><a href='../../../' class='white'><strong>首页</strong></a></td>");
		var parent;
		var child;
		var handle="";
		for(var i=0;i<columnlist.length;i++){
			handle="";
			parent=columnlist[i][0];
			child=columnlist[i][1];
			/*
			<td width="39" align="center" class="bg1"><a href="#" class="white"><strong>首页</strong></a></td>
    <td width='6' align='center' class='t-white'>|</td>
			*/
			if(child!=null){
				showinfo.push("<td width='6' align='center' class='t-white'>|</td><td onmouseover=showChild(this);this.className='bg1'; onmouseout=hidChild(this);this.className=''; align='center' id='a"+i+"'>");
			}else{
				showinfo.push("<td width='6' align='center' class='t-white'>|</td><td onmouseover=this.className='bg1'; onmouseout=this.className=''; align='center' id='a"+i+"'>");
			}
			if(parent[1].indexOf("http://")>-1){
				showinfo.push("<a class='white' target='_blank' href='"+parent[1]+"'>"+parent[0]+"</a>");
			}else{
				showinfo.push("<a class='white' href='"+parent[1]+"'>"+parent[0]+"</a>");
			}
			
			if(child!=null){
				showinfo.push("<div id='showa"+i+"' class='child' style='display:none' onmouseover='showThis(this)' onmouseout='hidThis(this)'><ul>");
				for(var j=0;j<child.length;j++){
					showinfo.push("<li>");
					if(child[j][1].indexOf("http://")>-1){
						showinfo.push("<a target='_blank' href='"+child[j][1]+"'>"+child[j][0]+"</a>");
					}else{
						showinfo.push("<a href='"+child[j][1]+"'>"+child[j][0]+"</a>");
					}
					showinfo.push("</li>");
				}
				showinfo.push("</ul></div>");
			}
			showinfo.push("</td>");
		}	
		return showinfo.toString().replace(/,/gi,"");
	}
	
	function showChild(obj){
		var show=$("show"+obj.id);
		var tl=getAbsPoint(obj);
		var l=tl[0];
		var t=parseInt(tl[1])+parseInt(obj.offsetHeight);
		var w=null;
		var h=null;
		setAbsPoint(show,t,l,w,h);
		show.style.display="block";
	}
	function hidChild(obj){
		var show=$("show"+obj.id);
		show.style.display="none";
	}
	function showThis(obj){
		obj.style.display="block";
	}
	function hidThis(obj){
		obj.style.display="none";
	}
	function setAbsPoint(setobj,top,left,width,height){
		if(top!=null){
			setobj.style.top=top+"px";
		}
		if(left!=null){
			setobj.style.left=left+"px";
		}
		if(width!=null){
			setobj.style.width=width+"px";
		}
		if(height!=null){
			setobj.style.height=height+"px";
		}
	}
	function getAbsPoint(e){   
		var x = e.offsetLeft, y = e.offsetTop;   
		while(e=e.offsetParent){ 
			x += e.offsetLeft;   
			y += e.offsetTop;
		} 
		return (x+","+y).split(",");   
	}
