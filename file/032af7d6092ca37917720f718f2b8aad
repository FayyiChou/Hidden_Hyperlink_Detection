			document.write("<a href='"+path+"/"+root+"/"+filename+".htm' target='_blank' style='color:#0075EA;text-decoration: underline'>中标公示</a>");
		}else{
			//document.write("<a href=\"javascript:alert('中标公示尚未发布！')\">中标公示</a>");
		}
	}
	
