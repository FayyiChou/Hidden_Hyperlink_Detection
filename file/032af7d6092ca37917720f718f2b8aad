		var ajaxObj = null;
                var id="";
                function addInfo(obj,info){
                  return obj==""?info:obj+","+info;
                }
		function getList(){
		  if(window.XMLHttpRequest){
			  ajaxObj = new XMLHttpRequest();
		  }else if(window.ActiveXObject){
			  ajaxObj = new ActiveXObject("Microsoft.XMLHTTP");
		  }else{
			return;
		  }
		  
		  ajaxObj.onreadystatechange = callback;

		  ajaxObj.open ('GET', 'http://www.fjbid.gov.cn:8080/zbweb/user.do?method=getOtherPages&handle=getOtherPages&id='+id, true);
		  ajaxObj.send (null);
		 
		}

		function callback(){
			if(ajaxObj.readyState == 4){
				var result = ajaxObj.responseText;
				document.getElementById("list").innerHTML=result;
				fillDatas(document.getElementById("list").innerHTML);
				
				/*
				content = content.substring(content.indexOf("<DIV id=ScriptDiv></DIV>")+43);
				content = content.substring(0,content.indexOf("<br clear=all>"));

				var arr = content.split("<font size=\"3\">");
				for(var i=1;i<arr.length;i++){
					var linkcon = arr[i];
					linkcon = linkcon.substring(0,linkcon.indexOf("</font></a>"));
					//linkcon = linkcon.replace("<font color=#C60A00>","");
					//linkcon = linkcon.replace("</font>","");
					document.getElementById("list").innerHTML=document.getElementById("list").innerHTML+linkcon+"<br>";
				}
				*/
			}
		}
		
		function fillDatas(result){
			var list=result.split("***");
			var child;
			var path;
			for(var i=0;i<list.length;i++){
				if(list[i]!=undefined&&list[i]!=null&&list[i].replace(" ","").length>0){
					child=list[i].split("===");
					if(child[0]=='NOTICE'){
						path="/zbgg1/zbgs";
					}else{
						path="/zbgg1/zbjg";
					}
					document.getElementById((child[0]+child[1]).replace(" ","")).innerHTML="<a href='#' onmouseover=\"getUrl('"+path+"','"+child[2]+"',"+child[1]+",this)\" target='_blank'>点击查看</a>";
				}
			}
                }
  </script><script language="javascript">
	function initZbgs(path,id,date,title){
		if(date!='null'&&date!=''){
			var datearr=date.split(" ")[0].split("-");
			var root;
			var filename;
			root=datearr[0]+(datearr[1].length>1?datearr[1]:"0"+datearr[1]);
				
			filename="t"+root+""+(datearr[2].length>1?datearr[2]:"0"+datearr[2])+"_"+id;
			document.write("<a href='"+path+"/"+root+"/"+filename+".htm' target='_blank'>"+title+"</a>");
		}else{
			//document.write("<a href=\"javascript:alert('没有公示')\">中标公示</a>");
		}
	}
	
	function initZbjg(path,id,date,title){
		if(date!='null'&&date!=''){
			var datearr=date.split(" ")[0].split("-");
			var root;
			var filename;
			root=datearr[0]+(datearr[1].length>1?datearr[1]:"0"+datearr[1]);
				
			filename="t"+root+""+(datearr[2].length>1?datearr[2]:"0"+datearr[2])+"_"+id;
			document.write("<a href='"+path+"/"+root+"/"+filename+".htm' target='_blank'>"+title+"</a>");
		}else{
			//document.write("<a href=\"javascript:alert('没有结果')\">中标结果</a>");
		}
	}

