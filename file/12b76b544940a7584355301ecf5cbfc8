	function createCompareView(result){
		if('succ' !== result.status) return false;
		var viewUl = '<ul>';
		$.each(result.data, function(i, field){
			var fieldData = field.value.split("ζ");
			viewUl += '<li id="view_li_'+field.keyname+'" style="cursor:pointer;"><span><input class="lel" type="checkbox" name="toCompare" checked="checked" value="'+field.keyname+'"> <label class="lel">'+fieldData[0]+'</label></span><em onclick="del_li('+field.keyname+');"></em></li>';
		});
		viewUl  +="</ul>";
		$('#view_item').html(viewUl);
	}
	for(var k=0; k < essence_slideid.length; k++) {
