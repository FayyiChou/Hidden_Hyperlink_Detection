} else {var t =$.trim($("#indexsearchdestId").val());var a =$.trim($("#indexsearchdestArea").val());var m =$.trim($("#indexsearchMarketId").val());var type="t";if (a !="") {window.location.href ="http://weihai.cits.cn/dest/a" + a + ".html";} else if (t !="") {window.location.href ="http://weihai.cits.cn/dest/t" + t + ".html";} else if (m !="") {window.location.href ="http://weihai.cits.cn/dest/m" + m + ".html";} else if (t =="") {var visaflag=keyName.indexOf("签证");var channelflag="";if(visaflag>=0){channelflag="cqz-";keyName=keyName.replace("签证","");}
jQuery(cities).each(function() {if (this.b ==keyName) {t =this.a;if(this.d =="2"){type="m";}
return false;}
});if (t !="") {window.location.href ="http://weihai.cits.cn/dest/"+channelflag+ type + t + ".html";} else {window.location.href ="http://weihai.cits.cn/dest/k" + UrlEncode(keyName)
+ ".html";}
}
}
}
}
function UrlEncode(str) {var ret ="";var strSpecial ="!\"#$%&'()*+,/:;<=>?[]^`{|}~%";for (var i =0;i < str.length;i++) {var chr =str.charAt(i);if (chr ==" ")
ret +="";else if (strSpecial.indexOf(chr) !=-1)
ret +="";else
ret +=chr;}
return ret;}
function getQueryString(name) {var reg =new RegExp("(^|&)" + name + "=([^&]*)(&|$)","i");var r =window.location.search.substr(1).match(reg);if (r !=null) return unescape(r[2]);return null;} 
 function switchhost(host_name) {$.cookie('HOST_DOMAIN_PAGE',host_name,{expires:7,path:'/',domain:'.cits.cn',secure:false,raw:false});$.cookie('HOST_DOMAIN_USER',host_name,{expires :7,path :'/',domain :'.cits.cn',secure :false,raw :false
});var url ="http://" + host_name;if (getQueryString("fm") =="t") {url =url + "?fm=t";}
if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)
||/MSIE(\d+\.\d+);/.test(navigator.userAgent)) {var referLink =document.createElement('a');referLink.href =url;document.body.appendChild(referLink);referLink.click();} else {location.href =url;}
}
function showpic(thisObj) {var d =$(thisObj);var pos =d.offset();var t =pos.top + d.height() + 10 + "px";var l =pos.left + d.width() - 98 + "px";$("#picId").css('top',t);$("#picId").css('left',l);$("#picId").css('display','block');}
function hidepic() {$("#picId").css('display','none');}
function showpn(flag,tabname){if(tabname=='私人定制'){if(flag=="1"){$("#SR_submenu").css('display','block');}
if(flag=="0"){$("#SR_submenu").css('display','none');}
}
