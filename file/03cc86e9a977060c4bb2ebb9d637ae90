<script type="text/javascript">
$(document).ready(function(){   
	$.ajax({
		type: 'POST',
		url: '/MiniCart.asp?t=2017/2/23 下午 02:23:08',
		cache: false,
		success: function(result){
		    	$("#MiniCart").html(result);
		}
	});

	$("#CartNo").html("0");
	$("#CartNo_ScrollNav").html("0");
	$("#CartNoRight").html("0");
	$('#MiniCartNo2').html("0");

	$("#MiniCart").hide();
	$("#cart").mouseover(function(){$("#MiniCart").show();});
	$("#cart").mouseout(function(){$("#MiniCart").hide();});
	
	$("#MiniCart").hover(
		function(){$("#MiniCart").css("display","inline-block"); },
		function(){$("#MiniCart").css("display","none"); });

});  
</script>
