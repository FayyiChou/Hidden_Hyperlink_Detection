<!-- hide this script from non-javascript-enabled browsers
function setSJTarget(linkUrl, imgUrl){
	var str = "";
	if(linkUrl.length > 0 && imgUrl.length > 0){
		str = '<a href="' + linkUrl + '" onFocus="this.blur();"><img width=384 height=200 src="' + imgUrl + '" border="0"></a>';
		document.all.styleJin.innerHTML = str;
	}
}
// stop hiding -->
</script>
