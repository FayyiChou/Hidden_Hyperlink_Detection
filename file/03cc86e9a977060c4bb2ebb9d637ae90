<script type="text/javascript">
$(document).ready(function() {  
$(".fastform").submit(function(e)
{
	var FormChkYN=true;
	if (FormChkYN){
		$('.AjaxFormMsg').html("<img src=/gif/loading2.gif>").show();
		var postData = $(this).serializeArray(),
			formURL = $(this).attr("action"),
			id2 = $(this).attr("id2"),
			CartNo=0,
			myVar;
		$.ajax(
		{
			url : formURL,
			type: "POST",
			data : postData,
			success:function(data, textStatus, jqXHR) 
			{
				if(data.indexOf("【庫存不足】") > -1){
					$('#AjaxFormMsg'+id2).html("庫存不足!").show();
					myVar=setTimeout(function(){$('.AjaxFormMsg').fadeOut(100);},2000);
				} else {
					$('#AjaxFormMsg'+id2).html("加入成功!").show();
					$('#AjaxFormMsg'+id2).fadeOut(1500);
					$('#MiniCart').ajaxloader('load',{load:'min_car_MiniCart.asp?t='+Date(), fadespeed: 'fast'});
					$.get("min_car_CartNo.asp?t="+Date(), function(CartNo){
						$('#CartNo').html(CartNo);
						$('#CartNoRight').html(CartNo);
						$('#CartNo_ScrollNav').html(CartNo);
						$('#MiniCartNo2').html(CartNo);
					});
				}
			},
			error: function(jqXHR, textStatus, errorThrown) 
			{
			}
		});
	    e.preventDefault();	//STOP default action
	}
});
});
</script>

<script src="def/jquery-modal/jquery.modal.min.js" type="text/javascript" charset="utf-8"></script>
<link rel="stylesheet" href="def/jquery-modal/jquery.modal.css" type="text/css" media="screen" />

<script type="text/javascript" charset="utf-8">
$(document).ready(function(){
	$("div.lazy").lazyload({container: $(".mm")});


	$(".QuickBuyTypeN").click(function(e)
	{
		var product_id = $(this).attr("product_id"),
			TempQty = $("#Temp_Qty"+product_id).val(),
			myVar;
		$('#QuickBuyMsg'+product_id).html("<img src=/gif/loading2.gif>").show();
		$.ajax(
		{
			url : "shoppingbag.asp",
			type: "POST",
			data: {'product_id':product_id,'Temp_Qty':TempQty,'QuickBuyYN':'Y','NoJump':'Y'},
			success:function(data, textStatus, jqXHR) 
			{
				if(data.indexOf("【庫存不足】") > -1){
					$("#QuickBuyNotify").html("庫存不足!");
					$('#QuickBuyMsg'+product_id).hide();
					//$('#QuickBuyMsg'+product_id).html("庫存不足!").show();
					//myVar=setTimeout(function(){$('.AjaxFormMsg').fadeOut(100);},2000);
				} else {
					$("#QuickBuyNotify").html("加入購物車成功!");
					$('#QuickBuyMsg'+product_id).hide();
					//$('#QuickBuyMsg'+product_id).html("加入成功!").show();
					//$('#QuickBuyMsg'+product_id).fadeOut(1500);
					$.get("min_car_CartNo.asp?t="+Date(), function(CartNo){
						$('#CartNo').html(CartNo);
						$('#CartNoRight').html(CartNo);
						$('#CartNo_ScrollNav').html(CartNo);
						$('#MiniCartNo2').html(CartNo);
					});
					$.ajax({
						type: 'POST',
						url: '/MiniCart.asp?t='+Date(),
						cache: false,
						success: function(result){
					    	$("#MiniCart").html(result);
						}
					});
				}

				$("#QuickBuyNotify").modal({overlay: "#fff",opacity: 0.35});
				myVar=setTimeout(function(){$.modal.close();},800);

			},
			error: function(jqXHR, textStatus, errorThrown) {
			}
		});
	    e.preventDefault();	//STOP default action
	});

	$(".QuickBuyTypeC").click(function(e)
	{
		var product_id = $(this).attr("product_id");
		$.ajax({
			type: 'GET',
			url: 'ajax_product_TypeOn2C.asp',
			data: {'product_id':product_id},
			//dataType: 'json',
			cache: false,
			success: function(html2){
		    	$("#dialogModal_content").html(html2);
		    	$("#QuickBuyDialog").html(html2);
		    	$("#QuickBuyDialog").modal({overlay: "#fff",opacity: 0.35});
			}
		});
	});

	$(".QuickBuyTypeY").click(function(e)
	{
		var product_id = $(this).attr("product_id");
		$.ajax({
			type: 'GET',
			url: 'ajax_product_TypeOn2Y.asp',
			data: {'product_id':product_id},
			//dataType: 'json',
			cache: false,
			success: function(html2){
		    	$("#dialogModal_content").html(html2);
		    	$("#QuickBuyDialog").html(html2);
		    	$("#QuickBuyDialog").modal({overlay: "#fff",opacity: 0.35});
			}
		});
	});

	$(".QuickBuyTypeS").click(function(e)
	{
		var product_id = $(this).attr("product_id");
		var click1 = $(this).attr("click1");
		$.ajax({
			type: 'GET',
			url: 'ajax_product_TypeOn2S.asp',
			data: {'product_id':product_id,'click1':click1},
			//dataType: 'json',
			cache: false,
			success: function(html2){
		    	$("#dialogModal_content").html(html2);
		    	$("#QuickBuyDialog").html(html2);
		    	$("#QuickBuyDialog").modal({overlay: "#fff",opacity: 0.35});
			}
		});
	});

	$(".QuickBuyTypeU").click(function(e)
	{
		var product_id = $(this).attr("product_id");
		$.ajax({
			type: 'GET',
			url: 'ajax_product_TypeOn2U.asp',
			data: {'product_id':product_id},
			//dataType: 'json',
			cache: false,
			success: function(html2){
		    	$("#dialogModal_content").html(html2);
		    	$("#QuickBuyDialog").html(html2);
		    	$("#QuickBuyDialog").modal({overlay: "#fff",opacity: 0.35});
			}
		});
	});


	$(".QuickBuyTypeS_C").click(function(e){

		var product_id = $(this).attr("product_id"),
			TempQty = parseInt($("#Temp_Qty"+product_id).val()),
			PickSizeID = $(this).attr("PickSizeID"),
			PickColorID = $(this).attr("PickColorID"),
			ColorName = $("#pd_size"+product_id).attr("ColorName"),
			SizeName = $("#Size"+product_id+"_"+PickSizeID).text(),
			myVar,
			SendYN=true;
		var Product_select2=PickSizeID +"///"+ PickColorID +"///"+ TempQty +"///"+ SizeName +"///"+ "" +"///def_stock_"+String.fromCharCode(64+parseInt(PickSizeID))+PickColorID;
		if (PickSizeID==''){
			SendYN=false;
		    	$("#QuickBuyDialog").html("<div class='dialogModal_content2'><p>請先選擇尺寸！</p></div>");
		    	$("#QuickBuyDialog").modal({overlay: "#fff",opacity: 0.35});
				myVar=setTimeout(function(){$.modal.close();},800);
		}
		
		if (SendYN){
			$('#QuickBuyMsg'+product_id).html("<img src=/gif/loading2.gif>").show();
			$.ajax({
				url : "shoppingbag.asp",
				type: "POST",
				data: {'product_id':product_id,'Product_select1':PickSizeID,'Product_select2':Product_select2,'Temp_Qty':TempQty,'QuickBuyYN':'Y','NoJump':'Y'},
				success:function(data, textStatus, jqXHR) 
				{
					if(data.indexOf("【庫存不足】") > -1){
						$("#QuickBuyNotify").html("<font color=red>庫存不足!</font>");
						$('#QuickBuyMsg'+product_id).hide();
					} else {
						$("#QuickBuyNotify").html("加入購物車成功!");
						$('#QuickBuyMsg'+product_id).hide();
						$.get("min_car_CartNo.asp?t="+Date(), function(CartNo){
							$('#CartNo').html(CartNo);
							$('#CartNoRight').html(CartNo);
							$('#CartNo_ScrollNav').html(CartNo);
							$('#MiniCartNo2').html(CartNo);
						});
						$.ajax({
							type: 'POST',
							url: '/MiniCart.asp?t='+Date(),
							cache: false,
							success: function(result){
						    	$("#MiniCart").html(result);
							}
						});
					}
	
					$("#QuickBuyNotify").modal({overlay: "#fff",opacity: 0.35});
					myVar=setTimeout(function(){$.modal.close();},800);
	
					},
				error: function(jqXHR, textStatus, errorThrown) {
				}
			});
		    e.preventDefault();	//STOP default action
		}
	});


	$(".QuickBuyTypeS_D").click(function(e){

		var product_id = $(this).attr("product_id"),
			TempQty = parseInt($("#Temp_Qty"+product_id).val()),
			PickSizeID = $(this).attr("PickSizeID"),
			PickColorID = $(this).attr("PickColorID"),
			SizeName = $("#pd_color"+product_id).attr("SizeName"),
			ColorName = $("#Color"+product_id+"_"+PickColorID).text(),
			myVar,
			SendYN=true;
		var Product_select2=PickSizeID +"///"+ PickColorID +"///"+ TempQty +"///"+ "" +"///"+ ColorName +"///def_stock_"+String.fromCharCode(64+parseInt(PickSizeID))+PickColorID;
		if (PickColorID==''){
			SendYN=false;
		    	$("#QuickBuyDialog").html("<div class='dialogModal_content2'><p>請先選擇顏色！</p></div>");
		    	$("#QuickBuyDialog").modal({overlay: "#fff",opacity: 0.35});
				myVar=setTimeout(function(){$.modal.close();},800);
		}
		
		if (SendYN){
			$('#QuickBuyMsg'+product_id).html("<img src=/gif/loading2.gif>").show();
			$.ajax({
				url : "shoppingbag.asp",
				type: "POST",
				data: {'product_id':product_id,'Product_select1':PickColorID,'Product_select2':Product_select2,'Temp_Qty':TempQty,'QuickBuyYN':'Y','NoJump':'Y'},
				success:function(data, textStatus, jqXHR) 
				{
					if(data.indexOf("【庫存不足】") > -1){
						$("#QuickBuyNotify").html("<font color=red>庫存不足!</font>");
						$('#QuickBuyMsg'+product_id).hide();
					} else {
						$("#QuickBuyNotify").html("加入購物車成功!");
						$('#QuickBuyMsg'+product_id).hide();
						$.get("min_car_CartNo.asp?t="+Date(), function(CartNo){
							$('#CartNo').html(CartNo);
							$('#CartNoRight').html(CartNo);
							$('#CartNo_ScrollNav').html(CartNo);
							$('#MiniCartNo2').html(CartNo);
						});
						$.ajax({
							type: 'POST',
							url: '/MiniCart.asp?t='+Date(),
							cache: false,
							success: function(result){
						    	$("#MiniCart").html(result);
							}
						});
					}
	
					$("#QuickBuyNotify").modal({overlay: "#fff",opacity: 0.35});
					myVar=setTimeout(function(){$.modal.close();},800);
	
					},
				error: function(jqXHR, textStatus, errorThrown) {
				}
			});
		    e.preventDefault();	//STOP default action
		}
	});

	$(".PickSize").click(function(e){
		var SizeID = $(this).attr("SizeID");
		var Stock = $(this).attr("Stock");
		var product_id = $(this).parent().attr("product_id");
		$(this).parent().find(".PickUp").removeClass("PickUp");
		$(this).toggleClass("PickUp");
		if ($(this).hasClass("PickUp")){
			$("#PickClick"+product_id).attr("PickSizeID",SizeID);
				$("#Temp_Qty"+product_id).empty();
				//SelectNow.append("<option value='-' selected>請選擇數量</option>");
				for (i=1;i<=Math.min(Stock,10)&&i<=10;i++){
					$("#Temp_Qty"+product_id).append("<Option value="+ i +" >"+i+"</Option>");
				}
		} else {
			$(this).parent().attr("PickSizeID","");
		}
	});

	$(".PickColor").click(function(e){
		var ColorID = $(this).attr("ColorID");
		var Stock = $(this).attr("Stock");
		var product_id = $(this).parent().attr("product_id");
		$(this).parent().find(".PickUp").removeClass("PickUp");
		$(this).toggleClass("PickUp");
		if ($(this).hasClass("PickUp")){
			$("#PickClick"+product_id).attr("PickColorID",ColorID);
				$("#Temp_Qty"+product_id).empty();
				//SelectNow.append("<option value='-' selected>請選擇數量</option>");
				for (i=1;i<=Math.min(Stock,10)&&i<=10;i++){
					$("#Temp_Qty"+product_id).append("<Option value="+ i +" >"+i+"</Option>");
				}

		} else {
			$(this).parent().attr("PickColorID","");
		}
	});


    $(function($) {
		$("input.NumberSpinner").stepper();
    });


});
