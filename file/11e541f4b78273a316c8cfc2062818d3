<script type="text/javascript">

$(function(){

	$(".nav li").hover(function(){

		$(this).addClass("nav_hover").siblings().removeClass("nav_hover");

	})

})



//礼品名称搜索

$(function(){

	$("#search_name").focus(function(){

		var txt_value =$(this).val();

		if(txt_value=="输入礼品名称"){

			$(this).val("");

		}

	});

	$("#search_name").blur(function(){

		var txt_value =$(this).val();

		if(txt_value==""){

			$(this).val("输入礼品名称");

		}

	})

})

</script>

<script type="text/javascript">

//最新产品滑动

$(function(){

	var $div_li =$(".product_table_ul li");

	$div_li.hover(function(){

		$(this).addClass("li_select").siblings().removeClass("li_select");

		var index = $div_li.index(this);

		$("#product_table > ul").eq(index).show().siblings().hide();

	})

})

</script>

<script type="text/javascript">

function check_login(){

	if(document.form1.username.value==''){

		alert("请输入用户名!");

		document.form1.username.focus();

		return false;

	}

	if(document.form1.password.value==''){

		alert("请输入密码!");

		document.form1.password.focus();

		return false;

	}

}

function check_code(){

	if(document.form_code.f_search_code.value==""){

		alert("请输入产品编号！");

		document.form_code.f_search_code.focus();

		return false;

	}

}

</script>

