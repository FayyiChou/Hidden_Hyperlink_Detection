<script>
$('.view-more-snap').bind('click',function(){
  var snapshots = $('.thumb img');
  snapshots.show();
  $(this).hide();
  return false;
});
var detailPage = $('#page-detail');
$.each($('.thumb img'),function(index,item){
  $(item).attr('data-lsrc',$(item).attr('data-lsrc').replace('w400','w'+$(window).width()));
});
detailPage.off().on('touchend.thumbImg', '.thumb img', function () {
  $('#thumb-picwrap').show();
  $('#thumb-picwrap img').attr('src', $(this).attr('data-lsrc'));
});
//拖动的时候解除绑定事件
detailPage.on('touchmove', '.thumb img', function() {
  detailPage.off('touchend.thumbImg');
  detailPage.off('touchend.thumb');
  detailPage.on('touchend.thumb', '.thumb', function() {
    detailPage.on('touchend.thumbImg', '.thumb img', function () {
      $('#thumb-picwrap').show();
      $('#thumb-picwrap img').attr('src', $(this).attr('data-lsrc'));
    });
  });
});
detailPage.on('touchend', '#thumb-picwrap', function(e) {
  $('#thumb-picwrap').hide();
  $('#thumb-picwrap img').attr('src', '#');
  e.preventDefault(); // 阻止"默认行为"
});
</script></body></html>