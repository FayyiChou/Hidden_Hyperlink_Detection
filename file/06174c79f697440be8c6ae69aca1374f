if(classname == 'xunleid' || classname == 'qqdw'){
	Stat.gameyxdown(thunder_url);
}else if(classname == 'yxbao-gsd' || classname == 'kb-gsd' || classname == 'dx' || classname == 'wt' || classname == 'yd'){
    Stat.gameyxdown('http://down17.downyouxi.com/jingtiguanv142591zhongwenban.zip');
}else{
	var url=$(this).attr("href");
	Stat.gameyxdown(url);
}
});

function down(e,downurl){
	stopEvent(e);
	var url = '/?c=game&a=down&downurl='+downurl;
	window.open(url);
}

function stopEvent (e) {
	var e = e || window.event;
	if (e.preventDefault) {
		e.preventDefault();
	} else {
		e.returnValue = false;
	}
}
</script>                
