<script type="text/javascript" src="/Public/js/L/L.js"></script>

<script>
$(function(){
    $(window).on('scroll',function(){
        if($.cookie('phone')!=undefined) return;
        var st = $(document).scrollTop();
        if( st>500 ){
            $('#go-top').fadeIn(function(){
                $(this).removeClass('dn');
            });
        }else{
            $('#go-top').fadeOut(function(){
                $(this).addClass('dn');
            });
        }
    });
    //点击关闭
    $(".close-bottombar").click(function(){
        $(".bottom-alert").remove();
    })

});

</script>

