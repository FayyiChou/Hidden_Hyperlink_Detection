<!-- 回到顶部 -->
<script>
$(function(){
    $(window).on('scroll',function(){
        var st = $(document).scrollTop();
        if( st>500 ){
            $('#go-top2').fadeIn(function(){
                $(this).removeClass('dn2');
            });
        }else{
            $('#go-top2').fadeOut(function(){
                $(this).addClass('dn2');
            });
        }
    });
    showScroll();
    function showScroll(){
        $('#go-top2').click(function(){
            $("html,body").animate({scrollTop:0},200);
        });
    }
});
</script>
    <script>
        //数据初始化完毕回调
        function viewLoad(d){
            if(!!d.member && d.member.id>0){//已经登陆的情况
                //显示已经登陆的小窗口
                $('#login-window').hide();
                var str = template('logined-window-tpl',d)
                $('#logined-window').html(str);
                $('#logined-window').show();
            }
        }
    </script>

</body>
</html>