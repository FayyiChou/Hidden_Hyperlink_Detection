<script type=text/javascript>
	var t = n = 0;
	var count_142219562431047348 = jQuery(".newsPic_142219562431047348 a").size();
	jQuery(function(){
		var newsPic = ".newsPic_142219562431047348";
		var newsPicText = ".newsPic_142219562431047348 .text";
		var newsPicNum = ".newsPic_142219562431047348 .num span";
		var newsPicConcent = ".newsPic_142219562431047348 .content_142219562431047348 a";
		
		jQuery(newsPicConcent + ":not(:first)").hide();
		jQuery(newsPicText).html(jQuery(newsPicConcent + ":first").find("img").attr("alt"));
		jQuery(newsPicNum + ":first").addClass("on");
		jQuery(newsPicText).click(function(){window.open(jQuery(newsPicConcent + ":first").attr('href'), "_blank")});
		jQuery(newsPicNum).click(function() {
		   var i = jQuery(this).text() - 1;
		   n = i;
		   if (i >= count_142219562431047348)
		   	return;
		   jQuery(newsPicText).html(jQuery(newsPicConcent).eq(i).find("img").attr('alt'));
		   jQuery(newsPicText).unbind().click(function(){window.open(jQuery(newsPicConcent).eq(i).attr('href'), "_blank")})
		   jQuery(newsPicConcent).filter(":visible").hide().parent().children().eq(i).fadeIn(1200);
		   jQuery(this).removeClass("on").siblings().removeClass("on");
		   jQuery(this).removeClass("on2").siblings().removeClass("on2");
		   jQuery(this).addClass("on").siblings().addClass("on2");
		});
		t = setInterval("showNewsPic_142219562431047348()", 5000);
	});
	function showNewsPic_142219562431047348(){
		n = n >= (count_142219562431047348 - 1) ? 0 : ++n;
		jQuery(".newsPic_142219562431047348 .num span").eq(n).trigger('click');
	}
	function AutoResizeImage(maxWidth,maxHeight,objImg)
{
  var img = new Image();
  img.src = objImg.src;
  var hRatio;
  var wRatio;
  var Ratio = 1;
  var w = img.width;
  var h = img.height;
  wRatio = maxWidth / w;
  hRatio = maxHeight / h;
 if (maxWidth ==0 && maxHeight==0)
 {
   Ratio = 1;
 }else if (maxWidth==0)
 {
   if (hRatio<1) Ratio = hRatio;
 }else if (maxHeight==0){
    if (wRatio<1) Ratio = wRatio;
 }else if (wRatio<1 || hRatio<1){
   Ratio = (wRatio<=hRatio?wRatio:hRatio);
 }
 if (Ratio<1){
  w = w * Ratio;h = h * Ratio;
 }
 objImg.height = h;
 objImg.width = w;
}
