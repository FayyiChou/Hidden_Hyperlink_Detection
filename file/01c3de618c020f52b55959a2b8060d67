      });
      
      $(".hot-news1 .title1 h5").hover(function(){
        var id = $(this).attr("name");
        $(".hot-news1 .title1 h5 a").removeClass("current_tab");
        $(this).children("a").addClass("current_tab");
        $(".hot-news-c1").hide();
        $("#"+id).show();
        if(id == "tab_new_3"){
        	$("#emore").attr("href","/front/left_10015.jhtml");
        }
        else if(id ==  "tab_new_4"){
        	$("#emore").attr("href","/front/left_100015.jhtml");
        }
    })
</script>
