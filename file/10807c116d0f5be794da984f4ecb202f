<div id="dialog"></div>
<script src='../../js/cover.js'></script>
<script type="text/javascript">

    var xhttp;

    function addToCompare(item) {
        xhttp = null;
        if (window.XMLHttpRequest) {
            xhttp = new XMLHttpRequest();
        } else {
            // code for IE6, IE5
            xhttp = new ActiveXObject("Microsoft.XMLHTTP");
        }

        if (xhttp != null) {
            openDiv('newDiv');
            var url = '/cn/zh-cn/addtocompare/?PID=' + item.id + '&T=' + item.name;
            xhttp.onreadystatechange = state_Change1;
            xhttp.open("GET", url, true);
            xhttp.send(null);
        }
    }


    function state_Change1() {
        if (xhttp.readyState == 4) {
            if (xhttp.status == 200) {
                var result = JSON.parse(xhttp.responseText);
                $("#dialog").html(result.msg);
                if (result.status == -1) {
                    if (result.count > 1) {
                        $("#dialog").dialog(confirmDiag);
                    } else {
                        $("#dialog").dialog(alertDiag);
                    }
                } else {
                    if (result.count > 1) {
                        $("#dialog").dialog(confirmDiag);
                    } else {
                        $("#dialog").dialog(alertDiag);
                    }
                }
                closeDiv();
            }
        }
    }

    function clearCompare() {
        $.ajax({
            url: '/cn/zh-cn/compareclear/'
        }).done(function () {
            $("#dialog").dialog("close");
        });
    }



    var confirmDiag = { resizable: false, height: 300, width: 400, modal: true, buttons: { "现在比较": function () { $(this).dialog("close"); window.open('/cn/zh-cn/compare/'); }, "清空列表": function () { clearCompare(); }, "取消": function () { $(this).dialog("close"); } } };
    var alertDiag = { modal: true, resizable: false, height: 300, width: 400, buttons: { "OK": function () { $(this).dialog("close"); }, "清空列表": function () { clearCompare(); } } };

</script>

        
    </div>
    
