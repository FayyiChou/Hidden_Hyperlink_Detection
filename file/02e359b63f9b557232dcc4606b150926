var speed=30 
demz2.innerHTML=demz1.innerHTML 
demz.scrollTop=demz.scrollHeight 
function Marquee(){ 
if(demz1.offsetTop-demz.scrollTop>=0) 
demz.scrollTop+=demz2.offsetHeight 
else{ 
demz.scrollTop-- 
} 
} 
