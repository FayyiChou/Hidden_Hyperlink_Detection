                    $(function(){
                        // 切换应用标签
                        $('#appNavigation li').mouseover(function(){
                        	var liIndex = $(this).index();
                        	$(this).siblings().removeClass('cur');
                            $(this).addClass('cur');
                            $('.tabconbox2').hide();
                            $('.tabconbox2').eq(liIndex).show();
                        });
                        // 鼠标移动到应用导读图上时加上选中效果
                        $('.tabconbox2 li').mouseover(function(){
                            $(this).addClass('now');
                        });
                        // 鼠标离开应用导读图上时取消选中效果
                        $('.tabconbox2 li').mouseout(function(){
                            $(this).removeClass('now');
                        });
                    });
                </script>
