        <script>
            $('#sameSeriesProductsRank .fenlei-title li').mouseover(function(){
                $(this).siblings().removeClass('cur');
                $(this).addClass('cur');
                var liIndex = $(this).index();
                $('#sameSeriesProductsRank').children('ul').hide();
                $('#sameSeriesProductsRank').children('ul').eq(liIndex).show();
            });
            $('#sameSeriesProductsRank > ul').children('li').mouseover(function(){
                $(this).siblings().children('dl').hide();
                $(this).children('dl').show();
            });
        </script>
