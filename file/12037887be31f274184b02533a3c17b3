				window["_BFD"] = window["_BFD"] || {};
				_BFD.prodtRec = function(data,bid,req_id){
					
					var pstr = '';
					var ids = '';
					var datas = '';
					var hostUrl ="http://"+document.domain+"/picdetail/html/index.php?c=Detail_Index&m=AjaxGoodsInfo";
					$.each(data, function(i, item){
						id = item.iid>0 ? item.iid : false;
						if(id){
							ids +=id+",";
						}	  
				  	});
					$.ajax({
			            type: "GET",
			            url: hostUrl,
			            data: "proId="+ids,
			            dataType:'json',
			            success: function(goodsInfos){
			            	datas = goodsInfos;
			            	if(datas != -1){
				            	$.each(data, function(i, it){
							  		$.each(datas, function(i, item){
								  		if(it.iid == item.id){
								  			pstr +='<div class="tuijian-list">';
								  			pstr +='<div class="tuijian-img"><a href="'+item.indexUrl+'" target="_blank"><img src="'+item.headPic+'" height="120" onerror="javascript:this.src=\'http://icon.cnmo-img.com.cn/product/noimg160x120.jpg\';"></a></div>';
								  			pstr +='<div class="tuijian-des">';
								  			pstr +='<strong class="tuijian-title"><a href="'+item.indexUrl+'" target="_blank" title="'+it.name+'">'+it.name+'</a></strong>';
								  			pstr +='<span class="red">'+item.price+'</span>';
								  			pstr +='<ul class="tuijian-ul">';
								  			if(item.featureArticles){
							    				$.each(item.featureArticles, function(i, item2){		
							    					pstr +='<li><a href="'+item2.atl_url+'" target="_blank">'+item2.title+'</a></li>';
							    				});
								  			}else if(item.param){
								  				$.each(item.param, function(i, item2){		
							    					pstr +='<li><a href="'+item2.url+'" target="_blank">'+item2.param+'</a></li>';
							    				});
								  			}		
						    				pstr +='</ul></div></div>';
								  		}
							  		});
							  	});  	   
								$('#tuijian1-box').append(pstr);
			            	}else{
								$('#similarblock').remove();
				            }	
			            }
			        });
					_BFD.bind(data,"prodtRec",bid,req_id);//此处是推荐返回时百分点绑定事件的方法，请不要修改。
					return false;
				}	
