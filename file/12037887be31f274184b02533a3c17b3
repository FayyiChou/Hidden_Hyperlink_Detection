<div class="mt30 analogy" id="kanleyoukanlock">
	<strong class="analogy-pri">看过该产品的用户还看过</strong>
	<div class="look-list clearfix" id="kanleyoukan1_list">

			<script type="text/javascript">
				window["_BFD"] = window["_BFD"] || {};
				_BFD.prodtVAV = function(data,bid,req_id){
					
					var datavv = data;
					var str = '';
					var ids = '';
					var datas = '';
					var hostUrl ="http://"+document.domain+"/picdetail/html/index.php?c=Detail_Index&m=AjaxGoodsKanLeKanInfo";
					$.each(datavv, function(i, item){
						id = item.iid>0 ? item.iid : false;
						if(id){
							ids +=id+",";
						}	
				  	});
				  	
				  	$.ajax({
			            type: "GET",
			            url: hostUrl,
			            data: "proId="+ids,
			            dataType:'json',
			            success: function(goodsInfo){
			            	datas = goodsInfo;
			            	if(datas != -1){
				            	$.each(datavv, function(i, ite){
							  		$.each(datas, function(i, item){
								  		if(ite.iid == item.id){
									  		str +='<dl class="look clearfix"><dt>';
						    				str +='<a href="'+item.indexUrl+'" target="_blank"><img src="'+item.headPic+'" height="60" onerror="javascript:this.src=\'http://icon.cnmo-img.com.cn/product/noimg80x60.jpg\';"></a>';
						    				
						    				str +='</dt><dd><a href="'+item.indexUrl+'" target="_blank" title="'+ite.name+'">'+ite.name+'</a></dd>';
						    				str +='<dd class="red">'+item.price+'</dd></dl>';
								  		}
							  		});
							  	});
				            	$('#kanleyoukan1_list').append(str);
			            	}else{
			            		$('#kanleyoukanlock').remove();
			            	}     
       	
			            }
			        });
				  	_BFD.bind(data,"prodtVAV",bid,req_id);//此处是推荐返回时百分点绑定事件的方法，请不要修改。
				  	return false;
				}		
			</script>
		
	</div>
