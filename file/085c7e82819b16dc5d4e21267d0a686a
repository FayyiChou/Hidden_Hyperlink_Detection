<script type="text/javascript">
$(document).ready(function(){
	$("#top6").find("ul li:not('.top')").each(function(index){
		$(this).mouseover(function(){
			$("#top6").find("ul li:not('.top') a").removeClass("select");
			$(this).find("a").addClass("select");
			$("#top6").find("div").hide().each(function(index2){
				if(index===index2){
					$(this).show();
				}
			});
		});
	});
	
	$("#sales dt").find("a").each(function(index){
		$(this).mouseover(function(){
			$("#sales dd").find("ul").hide().each(function(index2){
				if(index===index2){
					$(this).show();
				}
			});
		});
	});
	
	$(".news_car dt").find("a").each(function(index){
		$(this).mouseover(function(){
			$(".news_car dt").find("a").removeClass("sel");
			$(this).addClass("sel");
			$(".news_car dd").hide();
		if(index===1){
			$(".news_car dd").eq("2").show().end().eq("3").show();
		}else{
			$(".news_car dd").eq("0").show().end().eq("1").show();
			}
		});
	});
	
	$("#carBJ").find("li").each(function(index){
		$(this).mouseover(function(){
			$("#carBJ").find("li a").removeClass("sel");
			$(this).find("a").addClass("sel");
			$("#content2 dd").find("div.bj").hide().each(function(index2){
				
				if(index===parseInt(index2/2)){
					if(index===0){
						$("#content2 dd").find("div.bj").eq("1").show();
						}
					$(this).show();
				}
			});
		});
	});
});
</script>
