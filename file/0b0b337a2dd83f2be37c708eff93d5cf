<tbody>
<tr>
    <td class="Caption_1_BG Caption_1_Title Green">信息员登录</td>
</tr>
<tr>
    <td height="5"></td>
</tr>
<tr>
    <td align="center" class="Table_Border_1"><table width="94%" border="0" align="center" cellpadding="0" cellspacing="0">
    <tbody>
    
    <form id="Form_UserLogin">
    <tr>
        <td height="35">用&nbsp;&nbsp;户：</td>
        <td align="left"><input type="text" maxlength="50" name="User_UserName" id="User_UserName" style="width:60px; height:18px; border:solid 1px #ccc;" /></td>
        <td>密&nbsp;&nbsp;码：</td>
        <td align="left"><input type="password" maxlength="50" name="User_UserPassword" id="User_UserPassword" style="width:60px; height:18px; border:solid 1px #ccc;" /></td>
    </tr>
    <tr>
        <td height="35">验证码：</td>
        <td align="left"><input type="text" maxlength="6" name="User_CheckCode" id="User_CheckCode" autocomplete="off" style="width:60px; height:18px; border:solid 1px #ccc;" /></td>
        <td align="left"><img id="imgVerify" src="ValidateCode.aspx?random=90997" alt="看不清？点击更换" onclick="this.src=this.src+'?random=' + Math.random()" style="width:50px; height:18px;" /></td>
        <td align="center"><img src="Images/login.gif" alt="" id="btnUser_Login" style="cursor:pointer;" /></td>
    </tr>
    </form>
    <script type="text/javascript">
        $(function () {
            $("#User_UserName").keydown(function () {
                EnterKeys("User_UserPassword");
            });

            $("#User_UserPassword").keydown(function () {
                EnterKeys("User_CheckCode");
            });

            $("#User_CheckCode").keydown(function () {
                SubmitKeyClick("btnUser_Login");
            });

            $("#btnUser_Login").click(function () {
                if (CheckLogin()) {
                    $.post("WebAction.ashx?Action=Login", $("#Form_UserLogin").serialize(), function (data) {
                        switch (data) {
                            case "001003":
                                alert("验证码错误！");
                                $("#User_CheckCode").focus();
                                $("#User_CheckCode").select();
                                break;
                            case "001000":
                                alert("登录成功！");
                                window.location.href = "User_ArticleManage.aspx";
                                break;
                            default:
                                alert("登录失败，用户名或密码错误！");
                                $("#User_UserName").focus();
                                $("#User_UserName").select();
                                break;
                        }
                    });
                }
                return false;
            });

            $("#btnUser_Reset").click(function () {
                $("#User_UserName").val("");
                $("#User_UserPassword").val("");
                $("#User_CheckCode").val("");

                $("#User_UserName").focus();
            });
        })

        function CheckLogin() {
            if ($("#User_UserName").val() == "") {
                alert("用户名不能为空！");
                $("#User_UserName").focus();
                return false;
            }

            if ($("#User_UserPassword").val() == "") {
                alert("密码不能为空！");
                $("#User_UserPassword").focus();
                return false;
            }

            if ($("#User_CheckCode").val() == "") {
                alert("验证码不能为空！");
                $("#User_CheckCode").focus();
                return false;
            }

            return true;
        }
    </script>
    
    </tbody>
    </table></td>
</tr>
