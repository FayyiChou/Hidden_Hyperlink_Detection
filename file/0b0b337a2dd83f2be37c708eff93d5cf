<script language="javascript" type="text/javascript">
function DoSearch()
{
    var SearchKey = $.trim($("#SearchKey").val());
    if (SearchKey == "")
    {
        alert("搜索内容不能为空!");
        $("#SearchKey").focus();
        return false;
    }
    switch (parseInt($("#SearchType").val()))
    {
        case 0:
            window.open("Info_Search.aspx?SearchKey=" + encodeURI(SearchKey) + "&ChannelID=" + $("#ChannelID").val());
            break;
        case 1:
            window.open("http://search.nbyz.gov.cn/search?query=" + encodeURI(SearchKey) + "&q=" + encodeURI(SearchKey) + "&radiobutton=2&database=60.190.2.14%12%13%14root%12%13%14nbgov%12%13%1420001%12%13%14nbgov%12%13%14proxy%12%13%14&all_file=on&filenamesearch=none&begin_file=*&end_file=*&sort=score&order=desc&output=Standard+Output&no_rec=15&match_type=MATCH_WORD&reltermoption=0&search_no=00000&owner=IDQUEST&sbf=0&area=1&pos=1&date=1&p=1&pg=10&Submit.x=23&Submit.y=10");
            break;
        case 2:
            window.open("http://60.190.2.12/jsearch/index.jsp?pagemode=result&appid=2&style=2&q=" + encodeURI(SearchKey));
            break;
        case 3:
            window.open("http://www.google.cn/search?hl=zh-CN&lr=lang_zh-CN&q=" + encodeURI(SearchKey));
            break;
    }
}

function IfShowSearchDetail()
{
    switch (parseInt($("#SearchType").val()))
    {
        case 0:
            $("#SearchDetail").show();
            break;
        default:
            $("#SearchDetail").hide();
            break;
    }
}

$(function () {
    $("#SearchKey").keydown(function () {
        EnterKeys("SearchType");
    });

    $("#SearchType").keydown(function () {
        SubmitKeyClick("btnSearch");
    });

    $("#SearchType").change(function () {
        //IfShowSearchDetail();
    });

    $("#btnSearch").click(function () {
        DoSearch();
    });

    $("#SearchDetail").hide();
})
</script>
    
