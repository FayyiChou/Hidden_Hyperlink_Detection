<script type="text/javascript" language="javascript">
    var Array_InfoMoreUrl_3 = new Array();
    Array_InfoMoreUrl_3[0] = "Info_More.aspx?ClassID=20301";
    Array_InfoMoreUrl_3[1] = "Info_More.aspx?ClassID=20303";
    Array_InfoMoreUrl_3[2] = "Info_More.aspx?ClassID=20304";
    var InfoMoreUrl_3 = Array_InfoMoreUrl_3[0];

    function ShowIndex_List_3(TypeIndex) {
        for (var intIndex = 1; intIndex <= 3; intIndex++) {
            if (intIndex == TypeIndex) {
                if (intIndex == 1) {
                    $("#Index_List_3_Navigate_" + intIndex).attr("class", "Caption_3_Title White");
                }
                else {
                    $("#Index_List_3_Navigate_" + intIndex).attr("class", "Caption_3_Title Green");
                }
                $("#Index_List_3_List_" + intIndex).show();

                InfoMoreUrl_3 = Array_InfoMoreUrl_3[intIndex - 1];
            }
            else {
                $("#Index_List_3_Navigate_" + intIndex).attr("class", "Caption_3_Title White");
                $("#Index_List_3_List_" + intIndex).hide();
            }
        }
    }

    $(function () {
        $("#Index_List_3_Navigate_1").mouseover(function () {
            ShowIndex_List_3(1);
        });

        $("#Index_List_3_Navigate_2").mouseover(function () {
            ShowIndex_List_3(2);
        });

        $("#Index_List_3_Navigate_3").mouseover(function () {
            ShowIndex_List_3(3);
        });

        $("#More_Index_List_3").click(function () {
            window.open(InfoMoreUrl_3);
        });

        $("#Index_List_3_List_2").hide();
        $("#Index_List_3_List_3").hide();
    })
</script>
    
