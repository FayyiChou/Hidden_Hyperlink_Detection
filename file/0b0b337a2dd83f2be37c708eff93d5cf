    var Array_InfoMoreUrl_2 = new Array();
    Array_InfoMoreUrl_2[0] = "Info_More.aspx?ClassID=20101";
    Array_InfoMoreUrl_2[1] = "Info_More.aspx?ClassID=20102";
    Array_InfoMoreUrl_2[2] = "Info_More.aspx?ClassID=20103";
    var InfoMoreUrl_2 = Array_InfoMoreUrl_2[0];

    function ShowIndex_List_2(TypeIndex) {
        for (var intIndex = 1; intIndex <= 3; intIndex++) {
            if (intIndex == TypeIndex) {
                $("#Index_List_2_Navigate_" + intIndex).attr("class", "Caption_7_Title Green");
                $("#Index_List_2_List_" + intIndex).show();

                InfoMoreUrl_2 = Array_InfoMoreUrl_2[intIndex - 1];
            }
            else {
                $("#Index_List_2_Navigate_" + intIndex).attr("class", "Caption_7_Title Green2");
                $("#Index_List_2_List_" + intIndex).hide();
            }
        }
    }

    $(function () {
        $("#Index_List_2_Navigate_1").mouseover(function () {
            ShowIndex_List_2(1);
        });

        $("#Index_List_2_Navigate_2").mouseover(function () {
            ShowIndex_List_2(2);
