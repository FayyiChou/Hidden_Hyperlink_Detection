<script type="text/javascript" language="javascript">
    var Array_InfoMoreUrl_7 = new Array();
    Array_InfoMoreUrl_7[0] = "Info_More.aspx?ClassID=20401";
    Array_InfoMoreUrl_7[1] = "Info_More.aspx?ClassID=20407";
    Array_InfoMoreUrl_7[2] = "Info_More.aspx?ClassID=20406";
    var InfoMoreUrl_7 = Array_InfoMoreUrl_3[0];

    function ShowIndex_List_7(TypeIndex) {
        for (var intIndex = 1; intIndex <= 3; intIndex++) {
            if (intIndex == TypeIndex) {
                $("#Index_List_7_Navigate_" + intIndex).attr("class", "Caption_6_Title Green");
                $("#Index_List_7_List_" + intIndex).show();

                InfoMoreUrl_7 = Array_InfoMoreUrl_7[intIndex - 1];
            }
            else {
                $("#Index_List_7_Navigate_" + intIndex).attr("class", "Caption_6_Title Green2");
                $("#Index_List_7_List_" + intIndex).hide();
            }
        }
    }

    $(function () {
        $("#Index_List_7_Navigate_1").mouseover(function () {
            ShowIndex_List_7(1);
        });

        $("#Index_List_7_Navigate_2").mouseover(function () {
            ShowIndex_List_7(2);
        });

        $("#Index_List_7_Navigate_3").mouseover(function () {
            ShowIndex_List_7(3);
        });

        $("#More_Index_List_7").click(function () {
            window.open(InfoMoreUrl_7);
        });

        $("#Index_List_7_List_2").hide();
        $("#Index_List_7_List_3").hide();
    })
</script>
    
