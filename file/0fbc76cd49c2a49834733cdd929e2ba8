<script type="text/javascript">
	window.onload = function() {
		var url = "http://i.yinyuetai.com/timeline/get-time-line";
		if (url.indexOf("?") < 0) {
			url += "?" + (new Date()).getTime();
		} else {
			url += "&" + (new Date()).getTime();
		}
		var img = new Image();
		img.src = url;
		img.width = 1;
		img.height = 1;
		document.body.appendChild(img);
	};
	/*百度合作*/
	function jia1() {
		sendToActionScript();
		return 12;
	}
	function sendToActionScript() {
		thisMovie("baidujia1").baiduadded();
	}

	function thisMovie(movieName) {
		if (navigator.appName.indexOf("Microsoft") != -1) {
			return window[movieName];
		} else {
			return document[movieName];
		}
	}
	/*百度合作end*/

	//播放器贴片广告
	function setMarkInfo(data) {
		if (data) {
			//下弹窗
			if (data.bottompop && data.bottompop.link) {
				$('<a href=' + data.bottompop.link + "&_t=" + new Date().getTime() + '"></a>').css('display',
						'none').appendTo(document.body);
			}

			//前贴
			if (data.preroll && data.preroll.link) {
				$('<a href=' + data.preroll.link + "&_t=" + new Date().getTime() + '"></a>').css('display', 'none').appendTo(document.body);
			}

			//暂停
			if (data.pauseroll && data.pauseroll.link) {
				$('<a href=' + data.pauseroll.link + "&_t=" + new Date().getTime() + '"></a>').css('display',
						'none').appendTo(document.body);
			}

			//后贴
			if (data.overlay && data.overlay.link) {
				$('<a href=' + data.overlay.link + "&_t=" + new Date().getTime() + '"></a>').css('display', 'none').appendTo(document.body);
			}
		}
	}
</script>
