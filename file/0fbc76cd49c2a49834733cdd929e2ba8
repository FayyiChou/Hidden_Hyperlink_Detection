	require(['juicer', 'uri'], function (juicer, Uri) {
		var uri = new Uri(location.href);

		juicer.register('tongji_link', function (link, index) {
			if (uri.getQueryParamValue('f')) {
				return link + '?f=' + uri.getQueryParamValue('f') + '-' + (parseInt(index) + 1);
			} else {
				return link;
			}
		});
	});
