<script type="text/javascript">
    //列表样式一
    $(function() {
        $("#view_list_5_277298043 .w-list-item").hover(function() {
            $(this).find(".w-list-text-wrap").stop().animate({ top: $(this).find(".w-list-title").height() + "px" }, 200);
            $(this).find(".w-list-text-wrap .w-list-text").stop().animate({ marginTop: ($(this).height() / 2 - $(this).find(".w-list-title").height())+"px" }, 200);
        }, function() {
            $(this).find(".w-list-text-wrap").stop().animate({ top: $(this).height() + "px" }, 200);
            $(this).find(".w-list-text-wrap .w-list-text").stop().animate({ marginTop: "0" }, 200);
        });
    });
