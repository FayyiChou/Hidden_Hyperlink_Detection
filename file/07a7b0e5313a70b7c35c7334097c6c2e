function bindCarTipEvt() {
window.favTipTimer = null;
var v_1 = document.getElementById("favTip1");
var v_2 = document.getElementById("favTip2");
var onInterest = document.getElementById("onInterest");
var interest = document.getElementById("interest");
var show = function() {
v_1.style.display = 'inline-block';
v_2.style.display = 'inline-block';
};
var hide = function() {
v_1.style.display = 'none';
v_2.style.display = 'none';
};
//添加鼠标移出和在div内移动事件
onInterest.addEventListener ? onInterest.addEventListener('mouseover', function(){
if(window.favTipTimer) {
clearTimeout(window.favTipTimer);
}
show();
}, false) : onInterest.attachEvent('on' + 'mouseover', function(){
if(window.favTipTimer) {
clearTimeout(window.favTipTimer);
}
show();
}); 
interest.addEventListener ? interest.addEventListener('mouseover', function(){
if(window.favTipTimer) {
clearTimeout(window.favTipTimer);
}
show();
}, false) : interest.attachEvent('on' + 'mouseover', function(){
if(window.favTipTimer) {
clearTimeout(window.favTipTimer);
}
show();
}); 
onInterest.addEventListener ? onInterest.addEventListener('mouseout', function(){
window.favTipTimer = setTimeout(function(){
hide();
}, 300);	
}, false) : onInterest.attachEvent('on' + 'mouseout', function(){
window.favTipTimer = setTimeout(function(){
hide();
}, 300);	
}); 
interest.addEventListener ? interest.addEventListener('mouseout', function(){
window.favTipTimer = setTimeout(function(){
hide();
}, 300);	
}, false) : interest.attachEvent('on' + 'mouseout', function(){
window.favTipTimer = setTimeout(function(){
hide();
}, 300);	
});
}
