function toggleshow(){
var obj2 = pc.getElems("#ulTopList li");
pc.each(obj2, function(e){
e.onmouseover=function(){
pc.each(obj2, function(i){
pc.removeClass(i, "topHover");
})
pc.addClass(e, 'topHover');
}
})
}
toggleshow();
// 返回顶部
function goTop(){
var h1 = document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop;
var h2 = document.documentElement.clientHeight ? document.documentElement.clientHeight : document.body.clientHeight;
var goTop = pagePub.$$('JgoTop');
var top = h1 + h2;
if(top > 2 * h2){
goTop.style.display = 'block';
}else{
goTop.style.display = 'none';
}
}
pagePub.on(window,'scroll',goTop);
pagePub.on(window,'resize',goTop);
function connect(){
var img = new Image;
img.src = "//acount.pcauto.com.cn/wzcount/artbrowse.php?id=CNT1358&response=1";
}
pagePub.on(pagePub.$$('JgoTop'),'click',connect);
//头部分享
function showshare() {
var box = pagePub.$$('JtopshareToole');
box.onclick = function() {
isclass = box.className == "topshare";
isclass ? box.className = "topshare topshare_active": box.className = "topshare";
}
box.onmouseout = function(e) {
e = e || event;
var relatedTarget = e.toElement || e.relatedTarget;
while (relatedTarget && relatedTarget != this) {
relatedTarget = relatedTarget.parentNode;
if (!relatedTarget) {
pagePub.removeClass(box, "topshare_active");
}
}
}
}
pagePub.on(window,'load',showshare);
//文章收藏
function collect() {
var pop = pc.getElem("#JcollectPop"),
addBtn = pc.getElem("#JaddFav"),
addFavIcon = pc.getElem("#JaddFavIcon"),
addBtnTxt = pc.getElem("#JaddBtnTxt"),
myCollect = pc.getElem("#JmyCollect"),
myLogUrl = pc.getElem("#JmyLogUrl"),
myLog = pc.getElem("#JmyLog"),
popTxtElem = pc.getElem("#Jtiptxt"),
tipIcon = pc.getElem("#JcollectTipIcon"),
collectManage = pc.getElem("#JcollectManage"),
closeBtn = pc.getElem("#JcollectClose"),
submitBtn = pc.getElem("#JsubmitBtn"),
popFlag = false;
myLogUrl.href = "//my.pcauto.com.cn/login.jsp?return="+escape(location.href);
function showTips(obj, popTxt) {
popTxtElem.innerHTML = popTxt;
tipIcon.className = "correct";
myCollect.style.display = "block";
obj.style.display = "block";
}
function hideTips() {
pop.style.display = "none";
}
function ifLog() {//是否登陆
if ( !! document.cookie.match(/(^|; )common_session_id=[^;]+/)) {
return true;
}
return false;
}
if (ifLog() == false) {
pagePub.on(addBtn, "click", function() {
showTips(pop, '请登陆后操作！');
tipIcon.className = "wrong";
myLog.style.display = "block";
myCollect.style.display = "none";
});
} else {
pc.need('ajax', function(){
pc.ajax({
type: "GET",
url: "//bip.pcauto.com.cn/intf/article.jsp?act=isCollect&siteId=2&articleId=9693595",
fresh: true,
dataType: 'json',
success: function(data1) {
var code1 = data1.code;
if (code1 == -1) { //1是已经收藏 2是未收藏 -1是未登录
addFavIcon.className = "icon";
addBtnTxt.innerHTML = "文章收藏";
} else if (code1 == 1) {
addBtnTxt.innerHTML = "已收藏";
addFavIcon.className = "icon done";
}
}
});
pagePub.on(addBtn, "click", function() {
pc.ajax({
type: "GET",
url: "//bip.pcauto.com.cn/intf/article.jsp?act=addArtCollect&siteId=2&articleId=9693595",
fresh: true,
dataType: 'json',
success: function(data2) {
var code2;
if (data2 != null) {
code2 = data2.code;
} else {
myCollect.style.display = "none";
tipIcon.className = "wrong";
showTips(pop, '亲，接口异常，请刷新页面！');
myCollect.style.display = "none";
return;
}
if (code2 == 0) { //-2未登录 -1异常 0收藏成功 1已收藏
collectManage.innerHTML = "查看";
showTips(pop, '收藏成功！');
addBtnTxt.innerHTML = "已收藏";
addFavIcon.className = "icon done";
} else if (code2 == 1) {
addBtnTxt.innerHTML = "已收藏";
addFavIcon.className = "icon done";
collectManage.innerHTML = "管理";
showTips(pop, '已收藏到个人中心！');
} else if (code2 == -2) {
addFavIcon.className = "icon";
addBtnTxt.innerHTML = "文章收藏";
showTips(pop, '请登陆后操作！');
tipIcon.className = "wrong";
myLog.style.display = "block";
myCollect.style.display = "none";
}
}
});
});
});
}
pagePub.on(closeBtn, "click", hideTips);
pagePub.on(submitBtn, "click", hideTips);
pagePub.on(myCollect, "click", hideTips);
}
collect();
