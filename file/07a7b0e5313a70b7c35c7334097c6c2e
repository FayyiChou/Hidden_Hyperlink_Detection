/*降价信息E*/
/*二手车 S*/
function getEscList(d) {
var html = '';
var priceArray = [];
var data = d.data;
if (data == undefined || data.length == 0) {
document.getElementById("j-esc-mod").style.display = "none";
return false;
}
for (var i = 0, len = 4; i < len; i++) {
if(data[i]){
html += '<li>'+
'<a title="' + data[i].modelName + '"  href="//price.pcauto.com.cn/price/api/v1/cooperation/second_hand_car/load/?from=1&source=5&url='+ data[i].link +'" target="_blank">'+
'<img width="120" height="90" alt="" src="' + data[i].picUrl + '">'+
'<span class="esc-name">' + data[i].modelName + '</span>'+
'</a>'+
'<p class="esc-des">'+
'<span class="esc-km">' + data[i].licheng + '万公里</span>'+
'<span class="esc-price">' + data[i].price + '万元</span>'+                           
'</p>'+
'</li>';
priceArray.push(data[i].price);
}
};
document.getElementById("j-esc-list").innerHTML = html;
//最低报价
var minPrice = Math.min.apply(null,priceArray);
var artInfFenqi = document.getElementById("artInfFenqi");
var a = document.createElement("a");
a.className = "price-gz s-blue";
a.innerHTML = "二手车"+ minPrice +"万元起>>";        
a.href = "//price.pcauto.com.cn/price/api/v1/cooperation/second_hand_car/load/?source=5&url=www.guazi.com/www/";
a.target="_blank";
artInfFenqi.appendChild(a)
}
function getEscData() {        
pc.getScript("//price.pcauto.com.cn/price/api/v1/cooperation/guazi/local_second_data/?serialGroupId=10720&regionId="+ _areaId +"&from=1&code=GBK&callback=getEscList")
}
