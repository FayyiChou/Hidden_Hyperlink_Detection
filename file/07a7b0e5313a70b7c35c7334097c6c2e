//精彩活动
function loadactivityDate(city){
if(!city) {
return;
}
var city = city;
var ipMap = {'香港':'广州'};
var flag = ipMap[city] ? true : false;
if(flag) {
city = ipMap[data.city];
}
if(city in activityDate){
var cu = activityDate[city], box = pagePub.$$("Jipactivity");
var len = cu.length/2;
var shtml="<strong>精彩活动：</strong>";
for(var i=0;i<len;i++){
var a = i*2+1,b = i*2;
shtml += '<a target="_blank" href="'+ cu[b] +'">'+cu[a]+'</a>';
}
box.innerHTML = shtml;
}
}
