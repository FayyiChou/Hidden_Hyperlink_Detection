function quotationData(data) {
if (data) {
var data = data.result;
var dataLen = data.length > 1 ? 1 : data.length ;
for (var i = 0; i < dataLen; i++) {
var list = data[i].data;
var len = list.length;
if (len > 0) {
var serialName = data[i].serialGroupName;
var html = "";
html += '<div class="sTit"> <strong class="mark"><a href="'+data[i].depreciateUrl+'" target="_blank">' + serialName + '  最低报价</a></strong> <span class="subMark"><a href="' + data[i].depreciateUrl + '" target="_blank">更多降价信息>></a> </span> </div>';
html += '<ul class="clearfix">';
for (var j = 0; j < len; j++) {
var isShowRenzIcon = list[j].isRenZhen;
var isShow24Icon = list[j].isShow24Icon;
var isShowSellIcon = list[j].namelistRange;
var carNum = list[j].hasXianche;
var phone = list[j]._400Phone;
var showRenzIcon = "",
show24Icon = "",
showSellIcon = "";
if (isShowRenzIcon == 1) {
showRenzIcon = '<img src="//www1.pcauto.com.cn/2013/price2013/s_renz.png" title="太平洋汽车网认证电话，请放心拨打" />';
phone = "<i><strong class='red'>"+phone+"</strong></i>";
phone = phone.replace("转","</strong>转<strong class='red'>");
}else{
phone = "<i>"+phone+"</i>";
};
if (isShow24Icon == 1) {
show24Icon = '<img src="//www1.pcauto.com.cn/2013/price2013/s_24h.png" title="24小时服务热线" />';
}
if (isShowSellIcon == 0) {
showSellIcon = '<img src="//www1.pcauto.com.cn/2013/price2013/s_sbs.png" title="出售本市" />';
} else if (isShowSellIcon == 1) {
showSellIcon = '<img src="//www1.pcauto.com.cn/2013/price2013/s_sbsh.png" title="出售本省" />';
} else if (isShowSellIcon == 2) {
showSellIcon = '<img src="//www1.pcauto.com.cn/2013/price2013/s_sqg.png" title="出售全国" />';
}
if(j == (len-1)){
html += '<li class="last clearfix">';
}else{
html += '<li class="clearfix">';
}
html += '<div class="colA">';
html += '<p class="disc"><a href="' + list[j].articleUrl + '#ad=4235" target="_blank">' + list[j].modelName + '  降' + list[j].discount + '万</a></p>';
html += '<p class="addr"><a href="//price.pcauto.com.cn/' + list[j].dealerId + '/#ad=4236" target="_blank">' + list[j].shortName + '</a>'+ ' ' +list[j].regionName + ' ' + phone + ' ' + showRenzIcon + ' ' + show24Icon + ' ' + showSellIcon + '</p>';
html += '</div>';
html += '<div class="colB">';
html += '  <p class="pri"><strong><a href="' + list[j].articleUrl + '#ad=4237" class="red" target="_blank">' + (parseFloat(list[j].price) - parseFloat(list[j].discount)).toFixed(2) + '万</a></strong></p>';
html += ' <p class="oldPri">' + list[j].price + '万</p>';
html += '</div>';
html += '<div class="colC">';
html += '  <p><a href="' + list[j].articleUrl + '#ad=4237" class="cut" target="_blank">降' + list[j].discount + '万</a></p>';
html += ' <p class="count">' + carNum + '</p>';
html += '</div>';
html += '<div class="colD"> <a href="' + list[j].enquiryUrl + '#ad=4311" class="askPri" target="_blank">询底价</a> </div>';
html += '</li>';
}
html += '</ul>';
if(document.getElementById("Jreduce" + i)){
document.getElementById("Jreduce" + i).innerHTML = html;
};
}
}
}
}
