<script>
;(function() {
var jumpFlag = false; // 跳转标志
/**
* 自动显示
* @param  {boolean} setFlag 设置自动显示
*/
function autoShow(setFlag) {
// IE7 及以下不支持 localStorage
if (!window.localStorage) return;
var key = 'pcauto_push_show';
var timeStr = localStorage.getItem(key);
var curTime = +new Date();
if (!timeStr || timeStr <= curTime) {
// 首次访问或访问记录已失效
if (true == setFlag) {
// 设置有效期后默认显示
var time = curTime + 6 * 3600 * 1000; // 有效期6h
localStorage.setItem(key, time);
} else {
// 立即执行默认显示
togPush(true);
}
}
}
/**
* 显示／隐藏推送弹层
* @param  {boolean} showFlag true: 显示｜false: 隐藏｜其他：切换显示
*/
function togPush(showFlag) {
var wrapElm = document.getElementById('JpushWrap');
if (!wrapElm) return;
var value = '';
if (true == showFlag) {
value = 'block';
} else if (false == showFlag) {
value = 'none';
} else {
value = !wrapElm.clientHeight ? 'block' : 'none';
}
if ('none' == value) {
autoShow(true);
}
wrapElm.style.display = value;
}
// 绑定切换按钮的点击事件
function bindTog() {
var linkElm = document.getElementById('JpushLink');
linkElm && (linkElm.onclick = function() {
if (jumpFlag) {
togPush(false);
jumpFlag = false;
return true;
}
togPush();
return false;
});
}
// 绑定内容的点击事件
function bindJump() {
var conElm = document.getElementById('JpushCon');
var linkElm = document.getElementById('JpushLink');
conElm && (conElm.onclick = function() {
jumpFlag = true;
linkElm && linkElm.click();
});
}
// 绑定关闭按钮的点击事件
function bindClose() {
var closeElm = document.getElementById('JpushClose');
closeElm && (closeElm.onclick = function() {
togPush(false);
});
}
// 初始化
function initPush() {
autoShow();
bindTog();
bindJump();
bindClose();
}
if(3==3){
document.getElementById('Jpush').className="push pushhide";
}
/**
* 加载数据
* @param  {string}   url          资源url
* @param  {function} fn           回调函数
* @param  {string}   callbackName 回调函数名
* @param  {string}   charset      资源编码
*/
function loadData(url, fn, callbackName, charset) {
if ('function' == typeof fn && callbackName) window[callbackName] = fn;
var ohead = document.getElementsByTagName('head')[0];
var oscript = document.createElement('script');
var url = url + (-1 == url.indexOf('?') ? '?' : '&') + 'callback=' + callbackName;
oscript.src = url;
if(charset) oscript.charset = charset;
ohead.appendChild(oscript);
oscript.onload = oscript.onreadystatechange = function() {
var f = oscript.readyState;
if (f && f != 'loaded' && f != 'complete') return;
oscript.onload = oscript.onreadystatechange = null;
ohead.removeChild(oscript);
if (window[callbackName]) {
// 处理 IE6~8 下删除 window 命名空间下的属性或方法会抛出异常
try {
delete window[callbackName];
} catch(e) {
window[callbackName] = undefined;
}
}
};
}
// 获取精彩推送
window.getPush = function(sid, rid) {
if (!sid || !rid) return;
var intf = 'http://price.pcauto.com.cn/dealer/interface/6_0/dealer_top_news_json.jsp?rid=' + rid + '&sid=' + sid + '&showNoDiscount=1&showSgPhoto=1&showNewsContent=1';
loadData(intf, function(data) {
var dataArr = data;
var pushElm = document.getElementById('Jpush');
var conElm = document.getElementById('JpushCon');
if (!dataArr.length || !conElm ||!pushElm) return;
var item = dataArr[0];
var html =
'<div class="push-pic">' +
'<img src="' + item.sgPhoto + '" width="100%" /><span class="push-pic-tag">行情</span>' +
'</div>' +
'<p>' + item.dealerNewsName + '</p>' +
'<p class="push-des">' + item.content.slice(0, 23) + '...</p>';
var linkElm = document.getElementById('JpushLink');
linkElm && (linkElm.href = item.marketUrl + '#ad=8466');
conElm.innerHTML = html;
pushElm.style.display = 'block';
initPush();
}, 'showPush', 'gbk');
};
})();
</script>
