function __showMallDataBlock(data) {
var JmallBox = document.getElementById('JmallBox');
var normalRows = data.normalRows;
var otherRows = data.otherRows;
var total = data.total;
var showItems = normalRows
// var showItems = normalRows.concat(otherRows);
if (total > 0) {
var _html = '<p class="modTit"><a href="//mall.pcauto.com.cn/r1/#ad=5324" target="_blank" class="mark"><strong>雷克萨斯NX 汽车商城活动</strong></a><a href="//mall.pcauto.com.cn/r1/#ad=5324" target="_blank" class="subMark gray">其他车系活动>></a></p><div class="mod-tb" style="max-height:242px; overflow:hidden;">';
var num = data.effectTotal - 1;
var mod = '';
for (var i = 0; i < showItems.length; i++) {
_html += showLis(showItems[i]);
}
_html += '</div>';
JmallBox.innerHTML = _html;
var div = JmallBox.getElementsByTagName("div")[0];
if(div.innerHTML == ""){
JmallBox.style.display = "none"   ;
}else{
JmallBox.style.display = "block";
gctBtnClick();
calTime();
}
} else {
JmallBox.style.display = "none";
}
}
function showLis(mall){
var tag = mall.type;
var cons="";
var imgObjBg;
if(mall.photo){
imgObjBg = '<img src="'+ mall.photo +'" alt="" width="100" height="84"/>'
}else{
imgObjBg = '<img src="//www1.pcauto.com.cn/wap/20160622white/img/bg.jpg" alt="" width="100" height="84"/>'
}
switch(tag){
case 0://超级购车团
var endtime = mall.onlineEndTime;
endtime = endtime.replace(/-/g,"/");
cons += '<a class="btn-gct" data-infor="'+mall.title+'|'+ mall.qrCodeImg +'|'+ mall.brands[0].name +'"><p class="modSum clearfix"><i class="icon">超级购车团<em></em></i><span>'+ mall.title +'</span></p><div class="m-mall-wrap"><p class="pic"><img src="'+mall.brands[0].logo96+'" alt="" width="63" height="63" class="img-logo"/>'+ imgObjBg +'<span>'+ mall.brands[0].name+'</span></p><p class="txt"><span>参团人数：</span><i class="red">'+mall.countDesc+'</i></p><p class="txt count-time" endtime = '+ endtime +'>离报名结束还有<i class="red">2</i>天<i class="red">5</i>时<i class="red">8</i>分</p><span class="mall-btn">我要参团</span></div></a>';
break;
case 1://购车券
cons += '<a href="'+ mall.url +'#ad=5324"><p class="modSum clearfix"><i class="icon">购车券<em></em></i><span>'+ mall.title +'</span></p><div class="m-mall-wrap"><p class="pic"><img src="'+ mall.photo +'" alt="" width="100" height="84"/><span>'+mall.sgName+'</span></p><p class="txt"><span>兑换内容：</span><i class="red">'+ mall.quanWorth +'</i></p><p class="txt"><em class="red">' +mall.stockCountDesc+'</em></p><span class="mall-btn">立即领取购车券</span></div></a>';
break;
case 2://团购
var endtime = mall.endTime;
endtime = endtime.replace(/-/g,"/");
cons += '<a href="'+ mall.url +'#ad=5324"><p class="modSum clearfix"><i class="icon">团购<em></em></i><span>'+ mall.title +'</span></p><div class="m-mall-wrap"><p class="pic"><img src="'+ mall.brandLogo96 +'" alt="" width="63" height="63" class="img-logo"/>'+ imgObjBg +'<span>'+mall.brandName+'</span></p><p class="txt"><span>参团人数：</span><i class="red">'+ mall.orderCountDesc +'</i></p><p class="txt count-time" endtime = '+ endtime +'>离报名结束还有<i class="red">2</i>天<i class="red">5</i>时<i class="red">8</i>分</p><span class="mall-btn">低价买车</span></div></a>';
break;
case 3://平行进口车
cons += '<a href='+ mall.pcUrl +'#ad=5324"><p class="modSum clearfix"><i class="icon">平行进口车<em></em></i><span>'+ mall.name +'</span></p><div class="m-mall-wrap"><p class="pic"><img src="'+ mall.photo +'" alt="" width="100" height="84"/><span>'+mall.name+'</span></p><p class="txt"><span>商城价：</span><i class="red">'+ mall.price +'</i>万起</p><p class="txt red"><span class="label label1"></span>'+ mall.discountPrice +'</p><span class="mall-btn">查看详情</span></div></a>';
break;
}
return cons;
}
function depreciate() {
if (_areaId) {
pc.getScript('//price.pcauto.com.cn/interface/serial_groups_depreciate.jsp?sid=10720&rid=' + _areaId + '&hasOther=1&callback=quotationData');
pc.getScript('//mall.pcauto.com.cn/autoMall/interface/outerInterface/list_super_activity_gct_importcar_json.jsp?showMax=2&serialGroupId=10720&regionId='+ _areaId +'&callback=__showMallDataBlock');
}
}
/*弹窗 S*/
function gctBtnClick() {
var Jclose = pc.getElem('#Jclose');
Jclose.onclick = function() {
closePop("#JpopWechat");
}
var btnGcts = pc.getElems(".btn-gct");
for (var i = 0, len = btnGcts.length; i < len; i++) {
btnGcts[i].onclick = function() {
var infor = this.getAttribute("data-infor");
var data = infor.split("|");
showPop("#JpopWechat", data);
}
}
}
//打开弹窗
function showPop(popElem, data) {
var bgElem = pc.getElem("#JbgMark"),
popElem = pc.getElem(popElem);
var tit = pc.getElem(".m-tit"),
img = popElem.getElementsByTagName("img")[0],
brand = popElem.getElementsByTagName("span")[0];
tit.innerHTML = data[0];
img.src = data[1];
brand.innerHTML = "参与品牌:" + data[2];
bgElem.className = "bg-mark";
popElem.className = "pop-wechat";
setMaskH();
}
//关闭弹窗
function closePop(popElem){
var bgElem = pc.getElem("#JbgMark"),
popElem = pc.getElem(popElem);
bgElem.className="bg-mark s-dn";
popElem.className="pop-wechat s-dn";
}
//设置遮罩层高度
function setMaskH(){
var bgElem = document.getElementById("JbgMark");
if(!window.XMLHttpRequest) {
var _scrollHeight = Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
bgElem.style.height = _scrollHeight + "px";
}
}
/*弹窗 E*/
/*倒计时 S*/
function calTime() {
if (!pc.getElems(".count-time")) {
return;
}
var countTimes = pc.getElems(".count-time"); //获取
pc.each(countTimes,function(elem,index){
var endtime = elem.getAttribute("endtime");
getTime(endtime,elem);
})
}
/**
* 活动倒计时
* @param  {string} expiry endtine
* @param  {object} obj    element
*/
function getTime(expiry,obj) {
if(expiry == "" || !obj){return;}
var now = new Date();
var expiry = expiry;
var a = now.getFullYear() + '.' + (now.getMonth()+1) + '.' + now.getDate() +' ' + now.getHours() + ':' + now.getMinutes() + ':' + now.getSeconds(); 
var b = expiry;
var startTime = new Date(a);
var endTime = new Date(b);
if ( endTime <= now) {
obj.innerHTML = "<i class='s-red'>0</i>天<i class='s-red'>0</i>时<i class='s-red'>0</i>分";
} else {
var days = (endTime - now) / 1000 / 60 / 60 / 24;
var daysRound =    checkTime(Math.floor(days));
var hours = (endTime - now) / 1000 / 60 / 60 - (24 * daysRound);
var hoursRound = checkTime(Math.floor(hours));
var minutes = (endTime - now) / 1000 / 60 - (24 * 60 * daysRound) - (60 * hoursRound);
var minutesRound = checkTime(Math.floor(minutes));
var seconds = (endTime - now) / 1000 - (24 * 60 * 60 * daysRound) - (60 * 60 * hoursRound) - (60 * minutesRound);
var secondsRound = checkTime(Math.round(seconds));
obj.innerHTML = "离报名结束还有<i class='red'>" + daysRound + "</i>天<i class='red'>" + hoursRound + "</i>时<i class='red'>" + minutesRound + "</i>分";
function _getTime(expiry){
return function(){
getTime(expiry,obj)
}
}
newtime = window.setTimeout(_getTime(expiry,obj), 1000);
}
}
function checkTime(n){
if(n > 0){
if(n <= 9){
n = "0" + n;
}
return String(n);
}else{
return "00";
}
}
