pagePub.$$("J-comment-num").innerHTML=data.total;
pagePub.$$("J-comment-num").style.display="block";
}
pagePub.$$("JcmtCount").innerHTML=shtml;
pagePub.$$("JcmtCount").style.display="block";
}
