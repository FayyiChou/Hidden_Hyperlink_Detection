function reviews(data) {
var dataList = data.dealers, dataLen = dataList.length;
dataLen = dataLen > 3 ? 3 : dataLen;
if (!dataLen) return;
var html = '<p class="modTit"><a href="//price.pcauto.com.cn/sg10720/market/" target="_blank" class="mark"><strong>雷克萨斯NX 经销商</strong></a><a href="//price.pcauto.com.cn/sg10720/market/" target="_blank" class="subMark gray">更多优惠信息>></a></p>' + '<ul class="list list-reseller">';
var ITEM_TMPL = 
'<li>' +
'<p><i class="list-item">{aq}：</i><a href="//price.pcauto.com.cn/{id}/" target="_blank" class="s-blue">{shortName}</a> {areaName}<i class="icon-ctfc"></i></p>' +
'<p><i class="list-item">报价：</i><span class="fl">{priceRange}　<i class="s-green">{discountRange}</i></span><a href="//price.pcauto.com.cn/{id}/order0-sg10720.html#ad=8347" target="_blank" class="list-btn">询底价</a></p>' +
'</li>';
for (var i = 0; i < dataLen; i++) {
var item = dataList[i];
var priceRange = !item.priceRange || '0' == item.priceRange ? '暂无报价' : item.priceRange;
var discountRange = '';
var newsLink = item.newsLink;
if (item.discountRange && '0' != item.discountRange) {
discountRange = item.discountRange;
if (discountRange.indexOf('-')>0 && discountRange.split('-').length) {
// 只显示最高降价幅度
discountRange = discountRange.split('-')[1];
}
if(newsLink){
discountRange = '<a href="' + item.newsLink + '" target="_blank">↓降' + discountRange + '</a>';
}
else{
discountRange = '↓降' + discountRange;
}
}
html += ITEM_TMPL.replace('{aq}', item.aq).replace(/{id}/g, item.id).replace('{shortName}', item.shortName).replace('{areaName}', item.areaName).replace('{priceRange}', priceRange).replace('{discountRange}', discountRange);
}
html += '</ul>'; 
var reviewsMod = document.getElementById("Jreseller");
var reviewsModB = document.getElementById("JresellerB");
reviewsMod.style.display = "block";
reviewsModB.style.display = "block";
reviewsMod.innerHTML = reviewsModB.innerHTML = html;
}
