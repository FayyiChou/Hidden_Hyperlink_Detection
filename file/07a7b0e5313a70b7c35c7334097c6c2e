<script type="text/javascript">
/**
* 根据IP定位，重写相关经销商入口url
* @param id <a>标签ID
* @param serialGroupId 车系ID
*/
function IPLocationForDealer(id,serialGroupId){
var areaId = 1;
if(typeof(getPCautoArea)=='function' ){
var areaArry = getPCautoArea();
areaId = areaArry[0];
}else{
areaId = 1;
}
if(areaId != null && serialGroupId > 0){
document.getElementById(id).href = PRICEROOT + '/shangjia/c'+areaId+'/sg'+serialGroupId+'/';
}
}
</script>
<script>
//加关注
var carId = "";	
//var sid = "";
var PASSPORTROOT = "http://my.pcauto.com.cn";
var PRICEROOT = "http://price.pcauto.com.cn";
var accountId = "";
var login_user_cookies_key = "login_user_cookies_key";
if(document.getElementById("dealerUrl")){
IPLocationForDealer("dealerUrl",sid);
}
var _hasSignIn = hasSignIn();
function hasCookie(key) {
return (new RegExp('(?:^|;\\s*)' + encodeURIComponent(key).replace(/[\-\.\+\*]/g, '\$&') + '\\s*\\=')).test(document.cookie);
}
function hasSignIn() {
return hasCookie('common_session_id') && hasCookie('cmu');
}
function getScript(url, cb) {
var s = document.createElement('script');
s.src = url;
if(cb) {
s.onload = s.onreadystatechange = function() {
var st=s.readyState; 
if(st&&st!="loaded"&&st!="complete") return;
s.onload=s.onreadystatechange=null;
cb.call(null);
};
}
document.getElementsByTagName('head')[0].appendChild(s);
}
if(_hasSignIn) {
getScript(PASSPORTROOT + "/intf/getLogedUser.jsp?callback=__setLoginUser");
getScript(PASSPORTROOT + "/intf/getCars.jsp?act=getCars&callback=__showGetCars");
}
function __setLoginUser(data) {
if(data) {
if(data.id>0) {	accountId = data.id;}
}
}
function __showGetCars(data) {
if(data) {
for(var i = 0; i < data.length; i++) {
var carSeriesId = data[i].carSeriesId;
if(carSeriesId == sid) {
carId = data[i].id;
var onInterest = document.getElementById("onInterest");
var interest = document.getElementById("interest");
onInterest.style.display = 'block';
interest.style.display = 'none';
break;
} 
}
}
}
function addInterest() {
if(!accountId) {
url = PASSPORTROOT + "/passport/login.jsp?return=" + window.location.href;
window.open(url,"_self");
} else {
try {
