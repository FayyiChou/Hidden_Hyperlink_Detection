if(data.code == 1) {
carId = data.carId;
var onInterest = document.getElementById("onInterest");
var interest = document.getElementById("interest");
onInterest.style.display = 'block';
interest.style.display = 'none';
}else if(data.code=-1){
alert("不要太贪心喔,这里只能添加20款！");
}
}, {
params: {
series : sid,
act : 'save',
windowname : '1',
callback : 'windowname'
}
}); 
t.send(); 
}catch(h) {
alert("数据提交失败 :" + h.description);
};
}
}
function delInterest() {
