<script type="text/javascript">
function getEscListPosAdd(d) {
var jptIds = new Array();
var html = '';
if (d == undefined) {
document.getElementById("others-about-less-pos").style.display = "none";
return false;
}
var data = d.rows;
if(data.length==0){
document.getElementById("others-about-less-box").style.display = "none";
return false;
}
for (var i = 0; i < data.length; i++) {
jptIds.push(data[i].jptId);
var gPri;
if(data[i].discount == 0){
gPri = data[i].minPrice;
}
else{
gPri = '↓'+data[i].discount;
}
html += '<li>'
+' <p class="psm-a">'
+' <a href="//price.pcauto.com.cn/sg'+ data[i].jptSgId +'/" target="_blank" class="blue">【'+data[i].brandName+' '+data[i].serialGroupName+'】</a>'
+' <a target="_blank" href="//price.pcauto.com.cn/market/'+data[i].dealerId+'-'+data[i].newsId+'.html?jpt='+data[i].jptId+'"><span>'+ data[i].newsText+'</span><em>'+gPri+'万</em></a></p>'
+' <p class="psm-b"><span>'+data[i].name+'</span></p>'
+' </li>'
};
document.getElementById("others-about-less-pos").innerHTML = html;
traceLog('jptTrace',2,jptIds);
}
