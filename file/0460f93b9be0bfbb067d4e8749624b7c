		<li><span><a href="javascript:showDetail(#{item.col},#{item.id});">#{item.title.substr(0,15)}</a></span><span class="date"><a href="javascript:showDetail(#{item.col},#{item.id});">#{item.ctime.substr(5,6)}</a></span></li> 
	}
</SCRIPT>
<SCRIPT id="cp6" type="text/template">
	for (var i = 0; i < this.length; i++) {
		var item = this[i];
		<a href="javascript:showDetail(#{item.col},#{item.id});">#{item.topic.substr(0,20)}...</a> 
	}
</SCRIPT>
<script type="text/javascript">

$(document).ready(function () {
	zjgjs.sso.init();
	getData(3, 97,1);//早晚评报告col92 subcol3
	getData(1, 97,1);//早晚评早评col92 subcol1
	getData(2, 97,1);//早晚评晚评col92 subcol2
	
	getDatacollege(1, 96,4);//证金学院基本概念col92 subcol2
	getDatacollege(2, 96,4);//证金学院基本概念col92 subcol2
	getDatacollege(3, 96,4);//证金学院基本概念col92 subcol2
	getDatacollege(4, 96,4);//证金学院基本概念col92 subcol2
	getDatacollege(5, 96,6);//证金学院基本概念col92 subcol2
	
	getDatanotice()//交易公告col90 subcol3
	getVedio()
	$(".specialservice p").bind('click',function(){
		//$(this).css({background:"#258dcd url(images/gywm-title_sy.png) center left no-repeat",'border-bottom':'1px dashed #d0d0d0'})
		$(this).next(".specialservice ul").slideDown(300);	
       	//$(this).parent().siblings().find("p").css({backgroundImage:"url(images/gywm-title2_sy.png)",'background-color':'#258dcd','border-bottom':'1px solid #d0d0d0'});
		$(this).siblings().next(".specialservice ul").slideUp(300);
	});
	
});

function getData(subcol, col,num) {
  $.getJSON(dataHost + "/quote/news.jsp?callback=?",{
		action:"list",
		col:col,
		subcol:subcol,
		size: num
	}, function(data) {
		$("#list"+subcol).html(AceTemplate.format('tp'+subcol, data.items));
		//$("#calendar"+subcol).html(AceTemplate.format('cp'+subcol, data.items));
	})
}


function getDatacollege(subcol, col,num) {
  $.getJSON(dataHost + "/quote/news.jsp?callback=?",{
		action:"list",
		col:col,
		subcol:subcol,
		size: num
	}, function(data) {
		//$("#list"+subcol).html(AceTemplate.format('tp'+subcol, data.items));
		$("#calendar"+subcol).html(AceTemplate.format('cp'+subcol, data.items));
	})
}
function getDatanotice() {
  $.getJSON(dataHost + "/quote/news.jsp?callback=?",{
	  action:"list",
	 	col: 95,
	 	subcol: 3,
	 	size: 3
	}, function(data) {
		$("#notice").html(AceTemplate.format('cp6', data.items));
		
	})
}
function showDetail(col, idx) {
	if(col!='98'){
	window.open('/detail.html?col='+col+'&id='+idx);	
		}
	if (col=='98'){
	window.open('/detail_flash.html?col='+col+'&id='+idx);		
		}
	
}
function getVedio(){
	$.ajax({
		type : "get",
		async:false,
		url:"http://vd.jrj.com/front/out/index_sp_list.jsp?columnId=Colu1462773451256121,Colu1462773544316127,Colu1462777437095130",
		dataType : "jsonp",
		jsonp: "callback",//服务端用于接收callback调用的function名的参数
		jsonpCallback:"callback",//callback的function名称
		success:function(jsons){			
			var json=jsons.rows;
			var li='';
			for(var i=0;i<3;i++){
				if(json[i]){
					li=li+'<li class="mb10"><div class="pr over"><a target="_blank" href="'+json[i].url+'"><img width="203" height="126" src="'+json[i].videoImage+'"></a></div><p class="title pr" style="height:42px;"><a href="'+json[i].url+'" target="_blank" style=" font-size: 14px;float:left;">'+json[i].videoName+'</a><span style="margin-left:5px; float:left;">主讲：'+json[i].speakerName+'</span></p><p>'+json[i].videoBrief.substring(0,35)+'</p><div class="index_vedio_tab pa"><div class="index_vedio_tabl pa"></div><div class="index_vedio_tabr pa"></div>'+json[i].columnName+'</div></li>';
				}
			}
			
			$('#spjj_lists_new').html(li);
		},
		error:function(XMLHttpRequest, textStatus, errorThrown){
			XMLHttpRequest;
			textStatus;
			errorThrown;
		}
	})
	
	
}
</script>

</body>
</html>
