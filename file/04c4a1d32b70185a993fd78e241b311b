<script>
jQuery( document ).ready(function() {
   jQuery(".region-navbar-links").hide();
});

function topmenu_click()
    {
    if (jQuery(".region-navbar-links:first").is(":hidden")) {
		jQuery(".region-navbar-links:first .block-block").css('top','0');
                jQuery(".region-navbar-links:first .block-block").css('margin-bottom','0');
		jQuery(".region-navbar-links:first").slideDown("slow");
		
      } else {
        jQuery(".region-navbar-links:first").slideUp();
      }
    }
function topmenu_search()
    {
    if (jQuery(".search_form").eq(0).css('display')=='none')
        {
        jQuery(".search_form").eq(0).css('display','block');
        }
    else
        {
        jQuery(".search_form").eq(0).css('display','none');
        }
    }
