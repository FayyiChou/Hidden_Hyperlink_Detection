<script type="text/javascript">
	// 获取一条推荐玩乐数据
	function getOneTuijianWanle() {
		cQuery.jsonp('http://bus.ctrip.com/index.php?param=/data/tuijianWanlePrice&callback=getOneTuijianWanleCallback&DepartCity=&ArriveCity=&DepartDate=2017-04-01&RecommandFlag=2&t=' + new Date().getTime(), {onload: function(){
		}});
	}
	// 获取推荐玩乐数据回调函数
	function getOneTuijianWanleCallback(data) {
		if (data['code'] != 1) {
			return;
		}
		data = data['return'];
		recordDataShowCounts(data);
		var wanle={};
		if(data['product_type'] == '交通接驳'){wanle['icon']='i_shuttle';}
		else{wanle['icon']='i_vact';}
		wanle['title']=data['title'];
		wanle['product_type']=data['product_type'];
		wanle['product_id']=data['product_id'];
		wanle['price']=data['price'];
		wanle['url']=data['url'];
		var html = cQuery.tmpl.render(cQuery('#tuijian_wanle_tmp').html(), wanle);
		jQuery('#tuijian_wanle').html(html).show();
		jQuery('#tuijian_wanle_html').html(html);
	}
	// 记录玩乐展示次数
	function recordDataShowCounts(data){
		cQuery.jsonp('http://bus.ctrip.com/index.php?param=/data/recordDataCounts&title='+data['product_id']+'&message=wanleshownums&type=PC&name=bus.sms.kongwj.data.select.count&t=' + new Date().getTime(), {onload: function(){
		}});
	}
	// 记录玩乐展示次数
	function recordDataCounts(el){
		var url = jQuery(el).attr('data-url');
		var title = jQuery(el).attr('data-title');
		cQuery.jsonp('http://bus.ctrip.com/index.php?param=/data/recordDataCounts&title='+title+'&message=wanleclicknums&type=PC&name=bus.sms.kongwj.dataclick.select.count&callback=jumpWanleUrl&url=' + url, {onload: function(){
			}});
	}
	function jumpWanleUrl(data){
		if (data['code'] != 1) {
			return;
		}
		window.open(data['url']);
	}

	//getOneTuijianWanle();
</script>
