<script type="text/javascript">

	//获取推荐火车票数据
	function getOneTuijianTrain() {
		cQuery.jsonp('http://bus.ctrip.com/index.php?param=/data/tuijianTrainPrice&callback=getOneTuijianTrainListCallback&DepartCity=&ArriveCity=&DepartDate=2017-04-01&RecommandFlag=1&t=' + new Date().getTime(), {onload: function(){
		}});
	}

	// 获取推荐火车票数据回调函数
	function getOneTuijianTrainListCallback(data) {
		if (data['code'] != 1) {
			return;
		}
        data=data['return'];
        var train={};
        train['from']=data['from'];
        train['to']=data['to'];
        train['price']=data['price'];
        train['day']='1';
        train['frompinyin']=data['frompinyin'];
        train['topinyin']=data['topinyin'];
        var html = cQuery.tmpl.render(cQuery('#tuijian_train_list_tmp').html(), train);
        jQuery('#tuijian_train_tip').show();
        //jQuery('#tuijian_train_tip').after(html);
        jQuery('#bool_tuijian_train_list').val(1);
        //jQuery('#tuijian_train_tr').show();
		jQuery('#tuijian_train').html(html);
		jQuery('#no_bus_recom').show();
	}

    getOneTuijianTrain();

