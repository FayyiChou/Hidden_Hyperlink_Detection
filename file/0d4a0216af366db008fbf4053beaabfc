	/* 工具函数 */
	Date.prototype.format = function(format){ 
		var o = { 
		"M+" : this.getMonth()+1, // month
		"d+" : this.getDate(), // day
		"h+" : this.getHours(), // hour
		"m+" : this.getMinutes(), // minute
		"s+" : this.getSeconds(), // second
		"q+" : Math.floor((this.getMonth()+3)/3), // quarter
		"S" : this.getMilliseconds() // millisecond
		} 
		if(/(y+)/.test(format)) 
		format = format.replace(RegExp.$1, (this.getFullYear()+"").substr(4 - RegExp.$1.length)); 
		for(var k in o) { 
			if(new RegExp("("+ k +")").test(format))
				format = format.replace(RegExp.$1, RegExp.$1.length==1 ? o[k] : ("00"+ o[k]).substr((""+ o[k]).length)); 
		} 
		return format; 
	}
	
	var commentBar = document.querySelector("section.comment");
	var url = "http://comment.17k.com/userComment/getUserComment.action";
	//点击
	Q.jsonp(url, {"userId":36436520, "pageNum": 1,"pageCount":50}, function(data) {
		var page = data.page;
		var result = page.result;
		var html = new Array();
		var topic = null;
		for(var i=0;i<result.length;i++){
			topic = result[i];
			if(topic.stateId  == 1){
				html.push("<dl>");
				html.push("<dd><img class='icon' src='"+topic.marks.headImgUrl+"' alt='"+topic.marks.nikeName+"' /></dd>");
				html.push("<dd>");
				html.push("<div class='name'><span>"+topic.marks.nikeName+"</span><span>"+new Date(topic.creationDate).format("yyyy-MM-dd hh:mm:ss")+"</span></div>");
				var content = topic.summary.replace(/<\/?[^>]*>/g,'');
				if(content.length>80)
					content = content.substring(0, 60)+"……";
				html.push("<p><a href='http://h5.17k.com/reply/"+topic.id+"'>"+content+"</a></p>");
				html.push("<div class='toolbar'>");
				var replyCount = '';
				if(topic.replyCount>0)
					replyCount = topic.replyCount
			    html.push("<a  href='/book/"+topic.resourceId+".html'>《"+topic.marks.bookName+"》</a>"); 
				html.push("<a href='http://h5.17k.com/reply/"+topic.id+"'>回复 "+replyCount+"</a>");
				html.push("</div>");
				html.push("</dd>");
				html.push("</dl>");
			}
		}
		
		if(result.length==0)
			html.push("<section class=\"tips\">我还没有发表评论</section>");
		
		commentBar.innerHTML=html.join('');
		
	});
	
	</script>
