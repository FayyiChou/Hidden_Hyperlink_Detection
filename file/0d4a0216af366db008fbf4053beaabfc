<script type="text/javascript">
var wordCount =  (function(){
	var w = window.innerWidth, t = 13;
	if(w > 360) {
		t = 26;
	} else if (w > 768) {
		t = 35;
	} else if(w >= 1024) {
		t = 38;
	}
	return t * 3 - 5;
}())

var formatWord = function(text){
	if(text.length>wordCount)
		text = text.substr(0,wordCount)+"...";
	return text;
}
</script>
