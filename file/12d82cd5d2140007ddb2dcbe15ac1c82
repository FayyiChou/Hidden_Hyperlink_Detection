<script type="text/javascript">
	$(function(){
		$('.login-remember-btn').click(function(){
			if ($('.login-remember-active-btn').is(":visible"))
			{
				$('.login-remember-active-btn').hide();
				$('#login-remember-value').val("0");

				$('.login-submit').addClass('disable');
			}
			else
			{
				$('.login-remember-active-btn').show();
				$('#login-remember-value').val("1");

				$('.login-submit').removeClass('disable');
			}
		});
		
		$('.login-submit').click(function(){
			var username = $.trim($('#register-username').val());
			var password = $.trim($('#register-password').val());
			var password2 = $.trim($('#register-password2').val());
			var verify_code = $.trim($('#register-verifycode').val());
			var phone = $.trim($('#register-phone').val());

			var province = $.trim($('#register-province').val());
			var city = $.trim($('#register-city').val());
			var area = $.trim($('#register-area').val());

			if ($('.login-submit').hasClass('disable'))
			{
				return;
			}
			
			if (username == "")
			{
				$('#register-username').addClass("error");
				$('#register-username-error').html('用户名不能为空');
				$('#register-username-error').show();
				return;
			}

			if (username.indexOf(":") > 0 || username.indexOf("：") > 0)
			{
				$('#register-username').addClass("error");
				$('#register-username-error').html('用户名不能包含冒号":"');
				$('#register-username-error').show();
				return;
			}
			
			if (password == "")
			{
				$('#register-password').addClass("error");
				$('#register-password-error').html('密码不能为空');
				$('#register-password-error').show();
				return;
			}

			if (password2 == "")
			{
				$('#register-password2').addClass("error");
				$('#register-password2-error').html('密码不能为空');
				$('#register-password2-error').show();
				return;
			}

			if (password != password2)
			{
				$('#register-password2').addClass("error");
				$('#register-password2-error').html('两次密码输入不一样，请重新输入');
				$('#register-password2-error').show();
				return;
			}

			if (phone == "")
			{
				$('#register-phone').addClass("error");
				$('#register-phone-error').html('手机号不能为空');
				$('#register-phone-error').show();
				return;
			}

			if (province == "" || city == "")
			{
				$('#register-province').addClass("error");
				$('#register-city').addClass("error");
				$('#register-area').addClass("error");
				
				$('#register-address-error').html('请选择正确的地址');
				$('#register-address-error').show();
				return;
			}

			$('.login-submit').addClass("disable");

			
			//var url_top = "";
			/* var url;
			if (url_top.indexOf("?") > 0)
			{
			    url = url_top + "&username="+encodeURIComponent(username);
			}
			else
			{
				url = url_top + "?username="+encodeURIComponent(username);
			}
			var flag = false;
			//开始判断帐号密码对不对
			$.ajax({
				type:"GET",
				url:url,
				dataType:'json',
				cache:false,
				error:function(XMLHttpRequest, textStatus, errorThrown){
					$('#register-username').addClass("error");
					$('#register-username-error').html('连接服务器失败');
					$('#register-username-error').show();

					$('.login-submit').removeClass("disable");
					
					return;
				},
				success:function(data){
					if (data == "2")
					{
						$('#register-username').addClass("error");
						$('#register-username-error').html('该用户名已经存在');
						$('#register-username-error').show();

						$('.login-submit').removeClass("disable");
						
						return;
					}
					else
					{
						flag = true;
						
					}
				}		
			}); */
			$('#model_error').html('');
			$.ajax({
				type:"POST",
				url:'/register.html',
				dataType:'json',
				data:$('#register-form').serialize(),
				success:function(result){ 
					if(result)
					{	console.log(result);
						if(result['success'])
						{
							$('.login-error').text('');
							window.location.href = result['url'];
						}
						if(result['errmsg'])
						{
							$('.login-submit').removeClass("disable");
							// 显示错误信息
							if(result['errmsg'].length > 0)
							{	
								$('.login-error').text('');
								$('.login-error').show();
								$('#register-username-error').show();
								for(var i=0; i<result['errmsg'].length; i++)
								{
									$('#register-'+result['errmsg'][i]['name']+'-error').text(result['errmsg'][i]['errinfo']);
								}
							}
							//$('#model_error').html(result['errmsg']);
						}
					}
				},
				error:function(){
					$('.login-submit').removeClass("disable");
				}
			})
		});
	});
</script>

