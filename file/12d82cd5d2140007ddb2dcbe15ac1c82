<script src="https://static.geetest.com/static/tools/gt.js"></script>
<script>

var InterValObj; //timer变量，控制时间
var count = 60; //倒计时
var curCount;//当前剩余秒数
var sms_obj;

$(function(){

	/* gt滑动验证 start */
	var handlerPopup = function (captchaObj) {
		// 成功的回调
		captchaObj.onSuccess(function () {
			var validate = captchaObj.getValidate();
			$.ajax({
				url: "/common/verifycaptcha", // 进行二次验证
				type: "post",
				dataType: "json",
				data: {
					type: "pc",
					geetest_challenge: validate.geetest_challenge,
					geetest_validate: validate.geetest_validate,
					geetest_seccode: validate.geetest_seccode
				},
				success: function (data) {
					if (data && (data.status === "success")) {
						// 执行发短信
						sendSms(sms_obj);
					} else {
						$('#register-phone-error').html('验证错误!');
					}
				}
			});
		});
		$(".js_send_phonecode").click(function () {

			sms_obj = this;
			var phone = $('#register-phone').val();
			$('#register-phone').parents('.login-item').find('.login-error').text('');
			var flag = true;
			var p = /^(13[0-9]|14[0-9]|15[0-9]|17[0-9]|18[0-9])\d{8}$/i;

			if(phone == "")
			{
				flag = false;
				$('#register-phone').addClass("error");
				$('#register-phone-error').html('请输入手机号码');
				$('#register-phone-error').show();
			}
			if(phone != "" && !p.test(phone))
			{
				flag = false;
				$('#register-phone').addClass("error");
				$('#register-phone-error').html('手机号码格式不正确');
				$('#register-phone-error').show();
			}

			if(flag){
				captchaObj.show();
			}
		});
		// 将验证码加到id为captcha的元素里
		captchaObj.appendTo("#popup-captcha");
		// 更多接口参考：http://www.geetest.com/install/sections/idx-client-sdk.html
	};
	// 验证开始需要向网站主后台获取id，challenge，success（是否启用failback）
	$.ajax({
		//url: "../web/StartCaptchaServlet.php?type=pc&t=" + (new Date()).getTime(), // 加随机数防止缓存
		url: "/common/startcaptcha?type=pc&t=" + (new Date()).getTime(), // 进行二次验证
		type: "get",
		dataType: "json",
		success: function (data) {
			// 使用initGeetest接口
			// 参数1：配置参数
			// 参数2：回调，回调的第一个参数验证码对象，之后可以使用它做appendTo之类的事件
			window.initGeetest({
				gt: data.gt,
				challenge: data.challenge,
				product: "popup", // 产品形式，包括：float，embed，popup。注意只对PC版验证码有效
				offline: !data.success // 表示用户后台检测极验服务器是否宕机，一般不需要关注
				// 更多配置参数请参见：http://www.geetest.com/install/sections/idx-client-sdk.html#config
			}, handlerPopup);
		}
	});
	/* gt滑动验证 end */
})

function sendSms(curObj)
{
	var phone = $.trim($('#register-phone').val());
	var stype = $(curObj).attr('stype');

	curCount = count;
	$('.js_send_phonecode').addClass('send-phonecode-btn-dis');
	$(curObj).val("倒计时"+ curCount +"秒");
	$('.js_send_phonecode').parents('.login-item').find('.login-error').text('');
	$('.js_send_phonecode').attr("disabled", true);

	// 启动计时器，1秒执行一次
	//InterValObj = window.setInterval("SetRemainTime(curObj)",1000);
	InterValObj = window.setInterval(function(){
		SetRemainTime(curObj);
	},1000);

	$.ajax({
		type:"POST",
		url:"/regverifyphonecode.html",
		data:{'phone':phone, 'stype':stype,'timestamp':'1491082002', 'signature':'697366C67F76132F9DE38B697C8AD892'},
		dataType:'json',
		cache:false,
		success:function(result){
			if(result){
				if(result['success'] == 1){
					
				}else{
					$('#register-phone').addClass("error");
					$('#register-phone-error').html(result['notice_msg']);
					$('#register-phone-error').show();

					curCount = 0;
				}
			}else{
				curCount = 0;
				$('.js_send_phonecode').parents('.login-item').find('.login-error').text('验证码发送失败，请重试');
			}
		},
		error:function(XMLHttpRequest, textStatus, errorThrown){
			curCount = 0;
			$('.js_send_phonecode').parents('.login-item').find('.login-error').text('验证码发送失败，请重试');
		},
	});
}

function SetRemainTime(curObj)
{
	if(curCount == 0){
		window.clearInterval(InterValObj);  // 停止计时器
		$('.js_send_phonecode').removeClass('send-phonecode-btn-dis');
		$('.js_send_phonecode').attr("disabled", false);
		$('.js_send_phonecode').show();
		$(curObj).hide();

		var stype = $(curObj).attr('stype');
		if(stype == 2)
		{
			$(curObj).val('短信获取验证码');
		}
		else if(stype == 3)
		{
			$(curObj).val('语音获取验证码');
		}
	}
	else{
		curCount--;
		$(curObj).val("倒计时"+ curCount +"秒");
	}
}


</script>	
