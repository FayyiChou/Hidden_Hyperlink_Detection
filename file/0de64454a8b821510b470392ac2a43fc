				 	<script type='text/javascript'>
				placevideo();
				function placevideo()
				{
					b = 1920;  m = 1240;  mm = 340;					
					if($(window).width() < b)
					{
						if($(window).width() > m) 
						{	sx = Math.ceil((b - $(window).width()) / 2); $('#bg_vid').css({ right: '-' + sx + 'px' });	}
						else
						{	$('#bg_vid').css({ right: '-' + mm + 'px' }); }
						
					}
					else 						
						$('#bg_vid').css({ right: 0 });					    
					
				}				
				$(window).resize(function(){					
					placevideo();
				});				
</script> 
		
