	<SCRIPT language=javascript>
function doQuit(){
	if(myon){
		userlogin.username.value=""; 
		userlogin.password.value="";
	}
}

function maillogin(loginform){
         if(loginform.username.value==""){
                   alert("请输入您的邮箱帐号");
                   loginform.username.focus();
                   return false;
         }
         if(loginform.password.value==""){
                   alert("请输入您的邮箱密码");
                   loginform.password.focus();
                   return false;
         }
        if(loginform.password.value.length<5){
            alert("您的密码位数太少了!!!\n为了您邮箱安全请登录后马上修改密码.");
        }
                   loginform.action="http://mail.dl.gov.cn/office/login_submit.jsp" ;
				   loginform.submit(); 


}
 
function weblogin(loginform){ 
loginform.action = "user.asp?action=loginSubmit"; 
loginform.submit(); 
} 

function webreg(loginform){ 
loginform.action = "user.asp?action=register"; 
} 

</SCRIPT>
