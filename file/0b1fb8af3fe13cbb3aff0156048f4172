var in_ios_ifm=function(){
        var reback=true;
        try{
            //跨域parent.location.href会出错
            reback=location.href!=parent.location.href&&/IPAD|IPHONE/ig.test(navigator.userAgent.toUpperCase());
        }catch(e){
            reback=reback&&/IPAD|IPHONE/ig.test(navigator.userAgent.toUpperCase());
        }
        return reback;
    };
