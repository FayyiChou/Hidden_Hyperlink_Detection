jQuery(".dh ul li").hover(function(){
	if(jQuery(this).attr("class")!="no_border"){
	jQuery(this).addClass("b_a_t_li");
	jQuery(this).find("a.b_a_t").addClass("b_a_t_d");}
},function(){
	if(jQuery(this).attr("class")!="no_border"){
	jQuery(this).removeClass("b_a_t_li");
	jQuery(this).find("a.b_a_t").removeClass("b_a_t_d");}
});
</script>
