<script type="text/javascript">
if(jslink !=0){
var showImgs={};
//创建节点
	showImgs.phonePhoto=document.getElementsByClassName('phonePhoto')[0];
	var newNode = document.createElement("div");
	newNode.innerHTML='<div class="showBox" id="showBox"><span class="loadPic"></span><span class="size"></span><div class="showMore"><span>浏览完整图集<em></em></span></div></div>';
	showImgs.phonePhoto.parentNode.insertBefore(newNode,showImgs.phonePhoto);
//所有节点
	showImgs.wrap=document.getElementsByClassName('wrap')[0];
	showImgs.click=document.getElementsByClassName('showBox')[0];
	showImgs.loadPic=showImgs.click.getElementsByClassName('loadPic')[0];
	showImgs.loadSize=showImgs.click.getElementsByClassName('size')[0];
	showImgs.show=document.getElementsByClassName('showImgs')[0];
	showImgs.con=document.getElementsByClassName('content')[0];
	showImgs.close=showImgs.show.getElementsByClassName('closeIcon')[0];
	showImgs.add=showImgs.show.getElementsByClassName('addM')[0];
	showImgs.pageNum=showImgs.show.getElementsByClassName('pagenun')[0];
	showImgs.imgSize=albumData.pic.length;
	showImgs.num=5;
	showImgs.height='';
	showImgs.top='';
//初始化
	showImgs.loadPic.innerHTML='<img src='+albumData.pic[0].picture_path+'600_'+albumData.pic[0].picture_name+' />';
	showImgs.loadSize.innerHTML='1/'+showImgs.imgSize;
	showImgs.pageNum.innerHTML='1/'+showImgs.imgSize;
//事件
	showImgs.click.addEventListener('click',function(){
		showImgs.wrap.style.display='none';
		showImgs.show.className='showImgs on';
		showImgs.top=document.body.scrollTop;
		document.body.scrollTop=0;
		var temp='';
		for(var j=0;j<showImgs.num;j+=1){
			temp+='<span><img src='+albumData.pic[j].picture_path+'600_'+albumData.pic[j].picture_name+' /></span>';
		}
		showImgs.con.innerHTML=temp;
		showImgs.pageNum.innerHTML=j+'/'+showImgs.imgSize;
	})
	showImgs.close.addEventListener('click',function(){
		showImgs.wrap.style.display='block';
		showImgs.show.className='showImgs';
		document.body.scrollTop=showImgs.top;
	})
	showImgs.add.addEventListener('click',function(){
		if(showImgs.num>=showImgs.imgSize){
		}else{
			var temp='';
			showImgs.num+=5;
			for(var j=0;j<showImgs.num&&j<showImgs.imgSize;j+=1){
				temp+='<span><img src='+albumData.pic[j].picture_path+'600_'+albumData.pic[j].picture_name+' /></span>';
			}
			showImgs.con.innerHTML=temp;
			showImgs.num=showImgs.num>=showImgs.imgSize?showImgs.imgSize:showImgs.num;
			showImgs.add.innerHTML=showImgs.num+'/'+showImgs.imgSize;
			showImgs.pageNum.innerHTML=showImgs.num+'/'+showImgs.imgSize;
			if(showImgs.num>=showImgs.imgSize){
				this.innerHTML='已阅读完全部图片';
			}
		}
	})
}
</script>



