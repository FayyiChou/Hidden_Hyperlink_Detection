    function __fn() {
        var tempW = 750;
        var objiframe = window.parent.document.getElementsByTagName('iframe')[0];
        var winwidth = window.innerWidth > 0 ? window.innerWidth : objiframe.clientWidth;
        var size = parseInt(winwidth * 100 / tempW);
        var html = document.getElementsByTagName('html').item(0);
        html.style.fontSize = size + "px";
    }
    __fn();
    window.onresize = function () { __fn(); }
</script>
