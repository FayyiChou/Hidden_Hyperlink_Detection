<script type="text/javascript">
	$('#username').focus(function(){
		if($(this).val() == '账号') {
			$(this).val('');
		}
	});
	$('#username').blur(function(){
		if($(this).val() == '') {
			$(this).val('账号');
		}
	});
	$('#verifycode').focus(function(){
		if($(this).val() == '验证码') {
			$(this).val('');
		}
	});
	$("#showPass").focus(function(){
		$(this).hide();
		$(this).siblings('input[name=password]').show().focus();
	});
	$('#showPass').siblings('input[name=password]').blur(function(){
		if($(this).val() == '') {
			$(this).hide();
			$('#showPass').show();
		}
	});
	$(function(){
		var appId = window.location.host;
		$('#loginCodeImg').attr('src', 'http://auth.youzu.com/code/getCaptchaImage?theme=FourLength&'+ Date.parse(new Date())+ '&appId='+ appId +'');
		// 刷新验证码
		// refreshCaptcha();
		// 手动刷新验证码
		$('#loginCodeImg').live('click', function(){
			$('#loginCodeImg').attr('src', 'http://auth.youzu.com/code/getCaptchaImage?theme=FourLength&' +Date.parse(new Date()) +'&appId='+ appId +'');
			return false;
		});
		// 检查用户输入的验证码
		$('.UserLoginKuang_bg .checkCode').live('click', function(){
			var verifycode = $('input[name=verifycode]').val();
			if(!verifycode) {
				return alert('验证码不能为空！');
			}
			var url_check = 'http://auth.youzu.com/user/checkCaptcha';
			$.ajax({
				url: url_check,
				data: {'captcha':verifycode,'appId':window.location.host},
				dataType: 'jsonp',
				jsonp: 'callback',
				success: function(data) {
					if (data.retCode == '000'){
						// $('input[name=captcha_token]').val(data.captchaToken);
						$('#loginCode').hide();
					}else{
						$('#loginCodeImg').attr('src', 'http://auth.youzu.com/code/getCaptchaImage?theme=FourLength&'+ Date.parse(new Date()) +'&appId='+ appId +'');
						return alert('您输入的验证码有误！');
					}
				}
			})
		});
	});
</script>			<!-- /登入器 -->
