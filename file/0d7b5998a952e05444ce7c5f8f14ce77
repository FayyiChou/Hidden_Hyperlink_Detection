		var defaults = {
			'gameId':'146',
			'isNew':true,
			'webUrl':'http://dm.youzu.com/',
			'loginCallback':comLoginCallBack,
			'passportHost': 'http://passport.youzu.com/',
			'loginTextCss': {'highLight': '#c7a659', 'text': '#ada296', 'linkText': '#d2be8a', 'linkHText': '#ada296'},
			'cssOriginal':0
		};
		function comLoginCallBack(data) {
			if (data.status == 1) {
				if (!$.cookie('uuzu_UNICKNAME')) return false;
				typeof objNav != 'undefined' && objNav.initUserStatus();
			} else if (data.status == 2) {
				$.cookie("uuzu_UNICKNAME", null, {path: '/', expires: -1, domain: 'youzu.com'});
				$.cookie("uuzu_UAUTH", null, {path: '/', expires: -1, domain: 'youzu.com'});
				$.cookie("rServer", null, {path: '/', expires: -1, domain: 'youzu.com'});
				typeof objNav != 'undefined' && objNav.initUserStatus();
			}
		}
		$('body').commonLogin(defaults);
		/* 服务器相关 */
		var i = 1,
			_Url = "http://dm.youzu.com",
			param = [];
		ddm.set({"version":3,"isNew":true,"newUrl":_Url+"/site/gameServerList","gameId":146,testServerId:['10000'],'apiUrl':'http://api.youzu.com/'});
		$('#keyServerInfo').find('li').each(function(){
			var name = $(this).data('name');
			param[i] = name;
			i++;
		});
		ddm.init('content', param);
		$(function(){
			/*服务器列表，是否已登录*/
			$('#content1 a,#content2 a,#content3 a,#content4').live('click', function(){
				if(!$.cookie("uuzu_UNICKNAME")) {
					$(this).removeAttr('target');
					com_login.showLogin("'"+window.location.href+"'");
					return false;
				} else {
					$(this).attr('target', '_blank');
				}
			});
		});
	</script>

