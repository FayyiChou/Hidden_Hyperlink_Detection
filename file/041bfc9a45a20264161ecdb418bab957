   <script type="text/javascript">
    $(function () {
        $('#hd .p_info').mouseenter(function () {
            var name = $(this);
            var ul = $(this).find('.down_list');
            var time = setTimeout(function () {
                name.find('.link').stop().animate({ 'opacity': '0.6' }, 100);
                ul.fadeIn();
            }, 100);
            $(this).mouseleave(function () {
                clearTimeout(time);
                name.find('.link').stop().animate({ 'opacity': '1' }, 100);
                ul.fadeOut();
            });
        });

        $('#hd .t_nav .nav li').hover(function () {
            $(this).addClass('hover');
            $(this).find('.slide').show().stop().animate({ 'opacity': '0.9' }, 600);

        }, function () {
            $(this).removeClass('hover');

            $(this).find('.slide').stop().animate({ 'opacity': '0' }, 600, function () {
                $(this).hide()
            });
        });
        $(window).scroll(function () {
            var top = document.documentElement.scrollTop || document.body.scrollTop;

            if (top > 0) {
                if (typeof document.body.style.maxHeight === "undefined") {
                    $('#hd .t_outer').css({ 'position': 'absolute', 'top': top, 'left': '0' });
                } else {
                    $('#hd .t_outer').css({ 'position': 'fixed', 'top': '0', 'left': '0' });
                }
            } else {
                $('#hd .t_outer').css({ 'position': 'static' });
            }

        });

    });
</script> 
