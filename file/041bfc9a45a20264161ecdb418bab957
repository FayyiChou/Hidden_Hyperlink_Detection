   <script type="text/javascript">
$(function () {
	
	$('.beauty_list .content .pic_box').hover(function () {
		var timeLeft = 0;
		var leg = $(this).find('li').length;
		var sum = 0;
		for (i = 0; i < leg; i++) {
			var liWidth = $($(this).find('li').get(i)).width() + 10;
			sum += liWidth;
		};

		$(this).find('ul').css('width', sum);

		rollStatus($(this).find('ul'));

		function rollStatus(box) {
			function checkStatus() {
				var offset = parseInt(box.position().left);
				if (offset < 0) {
					box.parent().find('.prev').unbind('click').one('click', leftClick);
				} if (offset > -(sum - 676)) {
					box.parent().find('.next').unbind('click').one('click', rightClick);
				};
				if (offset == 0) {
					box.parent().find('.prev').hide();
				} else {
					box.parent().find('.prev').show();
				}
				if (offset <= -(sum - 676)) {
					box.parent().find('.next').hide().unbind();
				} else {
					box.parent().find('.next').show();
				}
			};
			function leftClick() {
				var left = box.position().left;
				if (-left < 676) {
					box.animate({ 'left': '+=' + (-left) }, 1500, function () {
						checkStatus();
					});
					var left = 0;
				} else {
					box.animate({ 'left': '+=' + 676 }, 1500, function () {
						checkStatus();
					});
				}
			};
			function rightClick() {
				var left = box.position().left - 676;
				if ((sum + left) < 676) {
					box.animate({ 'left': '-=' + (sum + left) }, 1500, function () {
						checkStatus();
					});
				} else {
					box.animate({ 'left': '-=' + 676 }, 1500, function () {
						checkStatus();
					});
				};
			};
			checkStatus(box);
		};
	}, function () {
		$('.next').hide();
		$('.prev').hide();
	});
	
	$('.beauty_list .content .title .t').each(function(){
		var t_height = $(this).height();
		if(t_height > 24){
			$(this).next().css('margin-top','-24px');
		}
	});

	$('.people_list li').hover(function () {
		$(this).addClass('hover');
	}, function () {
		$(this).removeClass('hover');
	});

	$('.people_list .head_pic').hover(function () {
		var uid = $(this).find('.data_pop').attr('uid');
		var t = $.trim($(this).find('.data_pop').html());
		
		if(t==''){
			$.ajax({
				type:"get",
				url:'/home/showuserinfo',
				data:{"uid":uid},
				dataType:"html",
				success:function(obj){
					$('#data_pop_'+uid).html(obj).show();
					$('#data_pop_'+uid+' .b a').click(function(event){
						event.stopPropagation();
					});
				}
			});
		}else{
			$(this).find('.data_pop').show();
		}
	}, function () {
		$(this).find('.data_pop').hide();		
	});

});




//关注
function followuser(flag,fid){
	var login_user_uid = "0";
	if(login_user_uid <=0){
		loginPop("http://www.soyoung.com/search?q=%E9%BB%84%E5%86%88%E5%86%B0%E5%A6%B9%E6%9C%8D%E5%8A%A1%20320083337");
		return false;
	}
	$.ajax({
        type:"post",
        url:'/home/followuser',
        data:{"uid":login_user_uid,'fid':fid,'flag':flag},
        dataType:"json",
        success:function(result){
            if(result.code == 200){
				alert('关注、取消失败');return false;
            }
            if(result.code == 100){
                if(flag ==1){
                	$('#userid_'+fid).html("<span id=userid_"+fid+"><a class='btn_3' onclick='followuser("+2+","+fid+")'>"+"已关注"+"</a></span>");
                	$('#user_ID_'+fid).html("<span id=userID_"+fid+"><a class='btn_3' onclick='followuser("+2+","+fid+")'>"+"已关注"+"</a></span>");
					$('.people_list li .button a,.people_list .data_pop .b a').click(function(event){
						event.stopPropagation();
					});
    	            return false;
                }else if(flag ==2){
                	$('#userid_'+fid).html("<span id=userid_"+fid+"><a onclick='followuser("+1+","+fid+")'>"+"加关注"+"</a></span>");
                	$('#user_ID_'+fid).html("<span id=userID_"+fid+"><a onclick='followuser("+1+","+fid+")'>"+"加关注"+"</a></span>");
					$('.people_list li .button a,.people_list .data_pop .b a').click(function(event){
						event.stopPropagation();
					});
    	            return false;
                }
                
            }
	    }
	});
}
</script> 
