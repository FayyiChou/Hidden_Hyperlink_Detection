	function DelAnswer(id)
	{
		var con = confirm('你确认删除该答案么？');
		if(con){
			var response = PageClass.DelAnswer(id);
			document.getElementById("span_" + response.value ).innerHTML = "<tr><td colspan='5'>答案删除成功。</td></tr>";
		}
		return;
	}

	function DelComment(id)
	{
		var con = confirm('你确认删除该留言么？');
		if(con){
			var response = PageClass.DelComment(id);
			document.getElementById("span_" + response.value ).innerHTML = "<tr><td colspan='5'>留言删除成功。</td></tr>";
		}
		return;
	}

	function EditAnswer(id)
	{
		document.location.href = "/question/answer_deal.aspx?id=" + id;
	}

	function doAnswer()
	{
		var ID = document.getElementById("lbContentID").value;
		var Content = document.getElementById("txtAnswer").value;
		var strComment = document.getElementById("Comment_new").innerHTML;
		if(Content == ""){
			alert("回答不能为空！");
			return;
		}
		var response = PageClass.InsertAnswer(ID,Content,strComment,answer_Callback);

	}

	function ctlent(eventobject)
	{
		if(eventobject.ctrlKey && eventobject.keyCode==13){
			var ID = document.getElementById("lbContentID").value;
			var Content = document.getElementById("txtAnswer").value;
			var strComment = document.getElementById("Comment_new").innerHTML;
			if(Content == ""){
				alert("回答不能为空！");
				return;
			}
			var response = PageClass.InsertAnswer(ID,Content,strComment,answer_Callback);
		}
	}

	function answer_Callback(response) {
	    if (response.value == "-10") {
	        window.location = "/logout_notice/";
	        return;
	    }

		document.getElementById("Answer_new").innerHTML = response.value;
		document.getElementById("txtAnswer").value = "";
	}

	function doComment()
	{
		var ID = document.getElementById("lbContentID").value;
		var Content = document.getElementById("txtContent").value;
		var strComment = document.getElementById("Comment_new").innerHTML;
		if(Content == ""){
			alert("评论不能为空！");
			return;
		}
        var response = PageClass.InsertComment(ID, Content, strComment);
        if (response.value=="-10") {
            window.location = "/logout_notice/";
            return;
        }
		document.getElementById("Comment_new").innerHTML = response.value;
		document.getElementById("txtContent").value = "";
	}

	function ctlent2(eventobject)
	{
		if(eventobject.ctrlKey && eventobject.keyCode==13){
			var ID = document.getElementById("lbContentID").value;
			var Content = document.getElementById("txtContent").value;
			var strComment = document.getElementById("Comment_new").innerHTML;
			if(Content == ""){
				alert("评论不能为空！");
				return;
			}
			var response = PageClass.InsertComment(ID,Content,strComment);
			document.getElementById("Comment_new").innerHTML = response.value;
			document.getElementById("txtContent").value = "";
		}
	}

	function Report(id)
	{
		PageClass.Report(id);
		document.getElementById("span_report").innerHTML = "感谢你的提醒，社区建设需要每一个人的力量！";
	}

	 function doStar(id,status)
	 {
			var response = PageClass.PageSetStar(id,status);
			$('#span_star').html(response.value);
	 }

	function Deal(codeID,action)
	{
		var response = PageClass.Deal(codeID,action);
		if(response.value == 1)
			document.getElementById("span_report").innerHTML = "操作成功";
		else
			document.getElementById("span_report").innerHTML = response.value;
	}

	function bestAnswer(qid,aid)
	{
		document.location.href = "/question/bestanswer.aspx?qid="+ qid +"&aid=" + aid;
	}
	function bestAnswer2(qid,aid)
	{
		document.location.href = "/question/bestanswer2.aspx?qid="+ qid +"&aid=" + aid;
	}

	function Digg(id,action)
	{
        window.hjquicklogin.doLogin({source : 'nbulo'});return false;
		var userID = "-1";
		if(userID == "-1")
		{
			document.location.href="http://passport.yeshj.com/?url=" + location.href;
			return;
		}	else{
			PageClass.PageDigg(id,action,Digg_callback);
		}

	}

	function Digg_callback(response)
	{
		//alert(response.value);
		var ret = response.value.split(",");
		var id = ret[0];
		var action = ret[1];
		var vCount = ret[2];
		if(action == "agree"){
			if (vCount == 0){
				$("#tips_" + id).html("<div class=WarningTips>你已经投过票了！</div>");
			}	else{
				$("#agree_" + id).html(vCount);
				$("#tips_" + id).html("<div class=CompleteTips>投票已成功，谢谢你的参与！</div>");
			}

		}else if(action == "anti"){
			if (vCount == 0){
				$("#tips_" + id).html("<div class=WarningTips>你已经投过票了！</div>");
			}	else{
				//$("#anti_" + id).html(vCount);
				$("#tips_" + id).html("<div class=CompleteTips>投票已成功，谢谢你的参与！</div>");
			}
		}
		setTimeout("HideMsg("+ id +");", 3000);
	}

	function HideMsg(id){
		$("#tips_" + id).html("");
	}

	$(function(){
		var oHead = document.getElementsByTagName('HEAD').item(0);
		var oScript = document.createElement("script");
		oScript.type="text/javascript";
		oScript.src="http://v1.jiathis.com/code/jia.js";
		oHead.appendChild(oScript);
		window.articleYellowTag.init('bulo');
	});
	
	//显示成功窗
	function ShowSuccess(content,autoClose)
	{
		$("#winmedal-content").removeClass(); 
		$("#winmedal-content").addClass("Success-content");
		$("#winmedal-content").html("<img style='float:left; margin:4px 15px 0px 0px;' src='http://t.yeshj.com/images/ico_right_answer.gif' /> <span class='big gray'>"+content+"</span>");
		$("#winmedal-box").show(300);
		
		if(autoClose)
		{
			window.setTimeout(
			  function() {
					closeWinmedal();
		  }, 1500); 
		}
		return false;
	}

	function closeWinmedal()
	{
		$("#winmedal-content").removeClass(); 
		$("#winmedal-box").hide(100);
	}

</script>
