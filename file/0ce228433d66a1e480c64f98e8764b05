<script>
//    if (!$.browser.msie) {
        var parSlideIndex_d2_1479_2a278_PSlider_=0;
        var parSlidePlay_d2_1479_2a278_PSlider_=true;
        var parSlideCount_d2_1479_2a278_PSlider_=0;
        var parSlideInterval_d2_1479_2a278_PSlider_=1000;
        var parSliderTimeId_d2_1479_2a278_PSlider_;
        var sliderMode_d2_1479_2a278_PSlider_=1;
        var parSlideDelay_d2_1479_2a278_PSlider_=4;
        function parViewSlide_d2_1479_2a278_PSlider_() //初始化
        {
		    parSlideCount_d2_1479_2a278_PSlider_ = 0;
		    try
		    {
			    pa_d2_1479_2a278_PSlider_.filters[0].Stop();
                //如果当前动画为随机则触发随机切换的效果
                if(sliderMode_d2_1479_2a278_PSlider_==10)
                {
                   pa_d2_1479_2a278_PSlider_.style.filter=changeFilter();
                }           
			    pa_d2_1479_2a278_PSlider_.filters[0].Apply();
		    }
		    catch(e) {}
            var i;
            for (i = 0; i < 3; i++)
            {
                var dv = document.getElementById('pa_d2_1479_2a278_PSlider__img' + i);
                if(dv!=null)
                {
                    if (i == parSlideIndex_d2_1479_2a278_PSlider_)
                    {
                        dv.style.display = '';
                    }
                    else
                    {
                        dv.style.display = 'none';
                    }
                }
            }
            try
            {
			    pa_d2_1479_2a278_PSlider_.filters[0].Play();
           
		    }
		    catch(e) {}
	        parSlideIndex_d2_1479_2a278_PSlider_++;
            parSlideIndex_d2_1479_2a278_PSlider_%= 3;
        }
        
        function parSetTimer_d2_1479_2a278_PSlider_() //定时
        {
		    if (parSlidePlay_d2_1479_2a278_PSlider_)
		    {
		       parSliderTimeId_d2_1479_2a278_PSlider_=setTimeout('parViewOnTime_d2_1479_2a278_PSlider_();', parSlideInterval_d2_1479_2a278_PSlider_);
            }
        }
        //随机切换对应的动画效果
        function changeFilter()        
        {
            var vNum;
            vNum = Math.random();
            vNum = Math.round(vNum * 10);
            //保存动画相关参数
            var filtestr;
            switch (vNum)
            {
                case 1:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.Fade(duration=1.0,overlap=1.0)';
                        break;
                    }
                case 2:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.GradientWipe(duration=2,gradientSize=0.25,motion=forward )';
                        break;
                    }
                case 3:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.Pixelate(,enabled=false,duration=2,maxSquare=25)';
                        break;
                    }
                case 4:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.Stretch(duration=2,stretchStyle=PUSH)';
                        break;
                    }
                case 5:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.Wheel(duration=2,spokes=16)';
                        break;
                    }
                case 6:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.RandomDissolve(duration=2)';
                        break;
                    }
                case 7:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.Spiral(duration=2,gridSizeX=50,gridSizeY=50)';
                        break;
                    }
                case 8:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.Slide(duration=2,bands=1,slideStyle=SWAP)';
                        break;
                    }
                case 9:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.RadialWipe(duration=2,wipeStyle=CLOCK)';
                        break;
                    }
                case 10:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.RevealTrans(enabled = true, transition = 23)';
                        break;
                    }
                default:
                    {
                        filtestr = 'progid:DXImageTransform.Microsoft.Fade(duration=1.0,overlap=1.0)';
                        break;
                    }
                }
            return filtestr;
        }

        function changeIntervale_d2_1479_2a278_PSlider_(time)
        {
            parSlideInterval_d2_1479_2a278_PSlider_ = time;
        }
        
        function parViewOnTime_d2_1479_2a278_PSlider_() //增加时间
        {
		    parSlideCount_d2_1479_2a278_PSlider_++;
            
		    if (parSlidePlay_d2_1479_2a278_PSlider_)
		    {
			    if (parSlideCount_d2_1479_2a278_PSlider_ >= parSlideDelay_d2_1479_2a278_PSlider_)
			    {
				    parViewSlide_d2_1479_2a278_PSlider_();
			    }
			    parSetTimer_d2_1479_2a278_PSlider_();
		    }
        }
        
        function parPlaySlide_d2_1479_2a278_PSlider_() //构造函数
        {
		    var btnPAPlayStop_d2_1479_2a278_PSlider_ = document.getElementById('btnPAPlayStop_d2_1479_2a278_PSlider_');
		    btnPAPlayStop_d2_1479_2a278_PSlider_.src='../res/imgs/slideStop.gif';
		    btnPAPlayStop_d2_1479_2a278_PSlider_.onclick=parStopSlide_d2_1479_2a278_PSlider_;
		    parSlidePlay_d2_1479_2a278_PSlider_=true;
		    parViewSlide_d2_1479_2a278_PSlider_();
		    parSetTimer_d2_1479_2a278_PSlider_();
        }
        
        function parStopSlide_d2_1479_2a278_PSlider_() //停止
        {
		    parSlidePlay_d2_1479_2a278_PSlider_=false;
		    var btnPAPlayStop_d2_1479_2a278_PSlider_ = document.getElementById('btnPAPlayStop_d2_1479_2a278_PSlider_');
		    btnPAPlayStop_d2_1479_2a278_PSlider_.src='../res/imgs/slidePlay.gif';
		    btnPAPlayStop_d2_1479_2a278_PSlider_.onclick=parPlaySlide_d2_1479_2a278_PSlider_;
        }
        
        function parNextSlide_d2_1479_2a278_PSlider_() //NEXT
        {
           
		    parSlideIndex_d2_1479_2a278_PSlider_%=3;
		    parViewSlide_d2_1479_2a278_PSlider_();
        }
        
        function parPrevSlide_d2_1479_2a278_PSlider_() //向前
        {
		    parSlideIndex_d2_1479_2a278_PSlider_-=2;
		    while (parSlideIndex_d2_1479_2a278_PSlider_<0)
		    {
			    parSlideIndex_d2_1479_2a278_PSlider_+=3;
		    }
		    parViewSlide_d2_1479_2a278_PSlider_();
        }
      
        parNextSlide_d2_1479_2a278_PSlider_();
