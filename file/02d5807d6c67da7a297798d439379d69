<script>
var request_url='http://www.qdutsoft.com/';
function sendRequest(_this){
	var formObj=$("#messageFrom");
	var username=formObj.find("input[name='username']").val();
	var contact=formObj.find("input[name='contact']").val();
	var content=formObj.find("textarea[name='content']").val();
	
	
	var data={username:username,contact:contact,content:content};
	if(Utils.isEmpty(username)){
		alert("请输入姓名");
		return;
	}
	
	if(Utils.isEmpty(contact)){
		alert("请输入联系方式");
		return;
	}
	if(Utils.isEmpty(content)){
		alert("请输入内容");
		return;
	}

	var success=function(res){
		if(res.status==1){
			$(_this).attr("disabled",'disabled');
		}
		alert(res.info);
	};
	ajaxRequest(request_url,{data:data},success);
}

</script>		

