	<script>
		var case_id='index';
		bannerSize();
		navDown();
		navSwitch();
		$(function() {
$("#down_arrow").on("click", function (a) {
        	a.preventDefault();
            var f = $(this).data("anc");
            var stop = $('#'+f).offset().top;
            var delay = 1000;
            $('body,html').animate({scrollTop: stop}, delay);
    		});
			$('.flexslider').flexslider({
				animation: "fade",
     			directionNav: false,
			});
	    var wow = new WOW({
		    boxClass: 'wow',
		    animateClass: 'animated',
		    offset: 0,
		    mobile: true,
		    live: true
		});
		wow.init();
	    (function(){
			var canvas=document.getElementById('canvas'),
			circlesCreated=false;
			function onScroll() {
				if (!circlesCreated && elementInViewport(canvas)) {
					circlesCreated = true;
					startanimation();
				}
			}
			function elementInViewport(el) {
			var rect = el.getBoundingClientRect();
		    return (
		      rect.top  >= 0 &&
		      rect.left >= 0 &&
		      rect.top  <= (window.innerHeight || document.documentElement.clientHeight)
		    );
		  }
			function startanimation() {
				$('.circular').ClassyLoader({
			                percentage: 100,
			                showText:false,
			                speed: 5,
			                diameter: 80,
			                lineColor: 'rgba(50,174,172,1)',
			                remainingLineColor: 'rgba(200,200,200,0.4)',
			                lineWidth: 10
			            });
			        $('.circular-tow').ClassyLoader({
			                percentage: 98,
			                showText:false,
			                speed: 5,
			                diameter: 80,
			                lineColor: 'rgba(94,167,218,1)',
			                remainingLineColor: 'rgba(200,200,200,0.4)',
			                lineWidth: 10
			        	});
			    	$('.circular-three').ClassyLoader({
			                percentage: 96,
			                showText:false,
			                speed: 5,
			                diameter: 80,
			                lineColor: 'rgba(40,177,99,1)',
			                remainingLineColor: 'rgba(200,200,200,0.4)',
			                lineWidth: 10
			        	});
			    	$('.circular-four').ClassyLoader({
			                percentage: 80,
			                showText:false,
			                speed: 5,
			                diameter: 80,
			                lineColor: 'rgba(60,209,199,1)',
			                remainingLineColor: 'rgba(200,200,200,0.4)',
			                lineWidth: 10
			        	});
			}
			window.onscroll = onScroll;
		})();
			$(".link_weixin_li").mouseenter(function(){
			$(".link_weixin_ewm img").stop().animate({
		    "height": "129px"
		  }, 300);
		}).mouseleave(function(){
			 $(".link_weixin_ewm img").stop().animate({
			    "height": "0"
			  }, 300)
		})
		});
		$("#service .services .wedoit").hover(function(a){
        $(this).addClass("put");
        $('hr', this).addClass("put");
        $('h5', this).addClass("put");
	    },function (){
	        $(this).removeClass("put");
	        $('hr',this).removeClass("put");
	        $('h1', this).removeClass("put");
	        $('p', this).removeClass("put");
	        $('h5', this).removeClass("put");
	    });
		</script>

