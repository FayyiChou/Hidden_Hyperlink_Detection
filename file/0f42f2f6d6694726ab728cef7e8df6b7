<script onerror="window.gMonitor &amp;&amp; window.gMonitor.resourceError(this.src, 'js')" type="text/javascript" src="http://t5.zbjimg.com/t5s/output/pkg/common/js/global-bf0717b0c273551f.js"></script>

<script onerror="window.gMonitor &amp;&amp; window.gMonitor.resourceError(this.src, 'js')" type="text/javascript" src="http://t5.zbjimg.com/t5s/output/common/js/count-eafd7a6e309b69f8.js"></script>

<script>
    //隐藏global。js生成的dom元素
    function del(selector){
        var obj = $(selector);
        if(obj.length > 0){
            obj.remove();
        }
    }
    del(".ui-poptipnoc");
    del(".ui-tools-bottom");
    del(".ui-select");
    del(".task-88-dialog-box");
    del(".webim");
    del(".image");

    function getCookie(c_name) {
        if (document.cookie.length > 0) {
            c_start = document.cookie.indexOf(c_name + "=");
            if (c_start != -1) {
                c_start = c_start + c_name.length + 1;
                c_end = document.cookie.indexOf(";", c_start);
                if (c_end == -1) c_end = document.cookie.length;
                return decodeURI(document.cookie.substring(c_start, c_end));
            }
        }
        return "";
    }

    function SensMsg(msg,time,isKeep,isMobile) {
        if (!(this instanceof arguments.callee)) {
            return new arguments.callee();
        }
        this.msg = msg || '';
        this.time = time || 3000;   //弹窗停留时间
        this.ele = null;           //弹窗的父容器
        this.isKeep = isKeep || false;  //弹窗结束后是否保留dom
        this.isMobile = isMobile || false;
        this.init();
    }

    SensMsg.prototype = {
        constructor: SensMsg,
        preCssText: "max-width: 200px;background: rgba(0,0,0,.6);border-radius: 5px;" +
        "text-align: center;word-break: break-all;padding: 0 10px;position: absolute;left: 50%;" +
        "transform: translate(-50%,50%) translate3d(0,0,0); -webkit-transform: translate(-50%,50%) translate3d(0,0,0);" +
        "color: #fff;font-size: 14px;transition: transform .5s; -webkit-transition: transform .5s;z-index: 9999;" +
        "height: 0;visibility: hidden;",
        nextCssText: "max-width: 200px;background: rgba(0,0,0,.6);border-radius: 5px;" +
        "text-align: center;word-break: break-all;padding: 5px 10px;position: absolute;left: 50%;" +
        "transform: translate(-50%,-50%) translate3d(0,0,0); -webkit-transform: translate(-50%,-50%) translate3d(0,0,0);" +
        "color: #fff;font-size: 14px;transition: transform .5s; -webkit-transition: transform .5s;z-index: 9999;" +
        "height: auto;visibility: visible;",
        init: function () {
            _obj = this;
            var _ele = document.createElement('div');
            this.ele = _ele;
            _ele.innerHTML = '<div class="sensmsg-block">' + _obj.msg + '</div>';

            document.body.appendChild(_ele);

        },
        show: function () {
            var _obj = this;
            if (_obj.ele) {
                var _mainEle = _obj.ele.childNodes[0];
                _mainEle.style.cssText = _obj.preCssText;
                _mainEle.clientWidth; //强制触发并完成重绘
                _mainEle.style.cssText = _obj.nextCssText;
                if(this.isMobile){
                    _mainEle.style.top = screen.availHeight/4 + document.body.scrollTop + 'px'; //手机端位置靠上，避免键盘遮挡
                }
                else{
                    _mainEle.style.top = screen.availHeight/2 + document.body.scrollTop + 'px';
                }
                setTimeout(function () {
                    _obj.isKeep ? _mainEle.style.cssText = _obj.preCssText : document.body.removeChild(_obj.ele);
                }, _obj.time);
            }
        }
    };

    function checkTel(val) {
        val = $.trim(val);
        return /^1[345789]\d{9}$/.test(val);
    }

    $(function () {
        var $jPubIptTel = $('.j-pub-input-tel');
        var $jPubIptNeed = $('.j-pub-input-need');
        var $jPubIptDesc = $('.j-pub-input-desc');
        var $jPubBlockMenu = $('.j-pub-block-menu');
        var sendFirst = true;
        var $filter1 = $('.j-filter-block1');
        var $filter2 = $('.j-filter-block2');
        var $popBox = $('.pub-box-popup');
        var $pubBlockDesc = $('.pub-block-desc');
        var $pubBtnBottom = $('.goto-block');
        var $topBlock = $('.top-block');
        var posPub = $topBlock.height() * 0.26;
        var other_id = '1577';
        var cate_id = '1213';
        var phone = '';
        var isLogin = false;

        $('.pub-btn-back').click(function () {
            $filter2.hide();
            $popBox.hide();
            sendFirst = false;
        });

        $filter1.click(function () {
            $filter1.hide();
            $jPubBlockMenu.hide();
            $pubBlockDesc.hide();
        });

        $('.j-pub-btn-popup').click(function () {
            var _need = $jPubIptNeed.val().trim();
            if (_need.length < 1) {
                $jPubIptNeed.focus();
                setTimeout(function () {
                    new SensMsg('请选择您的需求',3000,false,true).show();
                },100);
                return false;
            }
            if($jPubIptDesc.val().trim().length<1){
                $jPubIptDesc.focus();
                setTimeout(function () {
                    new SensMsg('请填写您的具体需求',3000,false,true).show();
                },100);
                return false;
            }
            if (_need.length<2 || _need.length>30){
                $jPubIptNeed.focus();
                setTimeout(function () {
                    new SensMsg('需求字数请控制在2~30个字',3000,false,true).show();
                },100);
                return false;
            }
            if(isLogin){
                send();
            }
            else{
                if (!checkTel($jPubIptTel.val())) {
                    $jPubIptTel.focus();
                    setTimeout(function () {
                        new SensMsg('请输入正确的手机号', 3000, false, true).show();
                    },100);
                    return false;
                }
                sendFirst && $('.employ-dialog-modifyphone-verify').click();
                $filter2.show();
                $popBox.show();
            }
        });

        $jPubIptNeed.focus(function () {
            $filter1.show();
            $jPubBlockMenu.show();
        });

        var timeout = null;
        var sTop = $topBlock.height();
        var $window = $(window);
        $window.scroll(function () {
            if (timeout !== null) {
                clearTimeout(timeout);
            }
            timeout = setTimeout(function () {
                tempT = $window.scrollTop();
                tempT > sTop ? $pubBtnBottom.fadeIn(300) : $pubBtnBottom.stop(true, true).hide();
            }, 200);
        });

        $jPubBlockMenu.on('click','div.cate',function () {
            var _$this = $(this);
            var _need = _$this.text();
            $jPubBlockMenu.hide();
            $jPubIptNeed.val(_need);
            $filter1.hide();
            cate_id = _$this.attr('data-cate');
        });

        $(".j-top").click(function () {
           $("html,body").stop().animate({scrollTop : 0},1000)
        });

        $(".j-cate").click(function () {
            var _$this = $(this);
            $jPubIptNeed.val(_$this.attr("title"));
            cate_id = _$this.attr('data-cate');
            document.body.scrollTop = posPub;
            $jPubIptTel.focus();
        });

        $('.hot-block').on('click','div',function () {
            var _$this = $(this);
            $jPubIptNeed.val(_$this.text());
            cate_id = _$this.attr('data-cate');
            document.body.scrollTop = posPub;
            $jPubIptTel.focus();
        });



        function send() {

            seajs.use(['common:components/refer-statistics/refer-statistics'], function (ref) {
                var _objRefer = ref.getStatisticsData('object');
                var _pmcode = _objRefer['pmcode'] || '';
                var _first_page = _objRefer['first_page'] || '';
                var _referer = _objRefer['referer'] || '';
                if(cate_id == other_id){
                    var _title = $jPubIptDesc.val().trim();
                }
                $.ajax({
                    url: "/c/IntegratedMarket",
                    type: "post",
                    dataType: "json",
                    data: {
                        pub_page: location.href,
                        referer: _referer,
                        first_page: _first_page,
                        pmcode: _pmcode,
                        title: _title || '我需要' + $jPubIptNeed.val().trim(),
                        content: $jPubIptDesc.val().trim(),
                        phone: phone || $jPubIptTel.val().trim(),
                        budget: '',
                        token: $(".token").text(),
                        other_id: 1582,
                        cate_id: cate_id
                    }
                }).done(function (res) {
                    if(res.success){
                        new SensMsg("需求发布成功，页面跳转至需求详情页",3000,false,true).show();
                        setTimeout(function () {
                            res.success ? (location.href = 'http://m.' + ZBJInfo.baseURI + '/task/' + res.data.task_id) : location.reload();
                        }, 1000);
                    }else{
                        new SensMsg(res.data.msg,3000,false,true).show();
                        setTimeout(function () {
                            location.reload();
                        }, 1000);
                    }
                });
            });
        }

        seajs.use('common:components/quick-login/quick-login', function (QuickLogin) {
            if (getCookie('userid')) {
                $.ajax({
                    type: 'GET',
                    url: 'http://task.' + window.ZBJInfo.baseURI + '/api/gettelbyuserid',
                    dataType: 'jsonp',
                    jsonp: 'jsonpcallback'
                }).done(function (res) {
                    if (res.state == 1) {
                        $('.zbj-form-item').hide();
                        isLogin = true;
                        phone = res.msg.tel;
                    }
                    else {
                        login = new QuickLogin({
                            element: $('#diy-contact'),
                            url: window.location.href,
                            userIntention: 1,
                            wayType: 13,
                            classPrefix: "employ-dialog",
                            tpl: false,
                            mode: 0
                        });

                        login.on('error', function(msg) {
                            setTimeout(function () {
                                new SensMsg(msg,3000,false,true).show();
                            },100);
                        });
                    }
                });
            }
            else {
                login = new QuickLogin({
                    element: $('#diy-contact'),
                    url: window.location.href,
                    userIntention: 1,
                    wayType: 13,
                    classPrefix: "employ-dialog",
                    tpl: false,
                    mode: 0
                });

                login.on('error', function(msg) {
                    setTimeout(function () {
                        new SensMsg(msg,3000,false,true).show();
                    },100);
                });
            }

            $('.pub-btn-sure').click(function () {
                if(isLogin){
                    send();
                }
                else{
                    login.login(function () {
                        send();
                    });
                }
            });
        });
    });
    window.onload = function() {
        //隐藏global。js生成的dom元素
        function del(selector){
            var obj = $(selector);
            if(obj.length > 0){
                obj.remove();
            }
        }
        del(".ui-poptipnoc");
        del(".ui-tools-bottom");
        del(".ui-select");
        del(".task-88-dialog-box");
        del(".webim");
        del(".image");
    };
</script>
