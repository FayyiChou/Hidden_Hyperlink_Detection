<script type="text/javascript">
(function(K, $){
$(function(){
    var dateTime = new Date();
    var difference = dateTime.getTime() - 1479071768*1000;	
    setInterval(function(){
      $("[remaintime]").each(function(){
        var obj = $(this);
        var endTime = new Date(parseInt(obj.attr('remaintime')) * 1000);
        var nowTime = new Date();
        var nMS=endTime.getTime() - nowTime.getTime() + difference;
        var myD=Math.floor(nMS/(1000 * 60 * 60 * 24));
        var myH=Math.floor(nMS/(1000*60*60)) % 24;
        var myM=Math.floor(nMS/(1000*60)) % 60;
        var myS=Math.floor(nMS/1000) % 60;
        var myMS=Math.floor(nMS/100) % 10;
        if(myD>= 0){
			var str = myD+"天"+myH+"小时"+myM+"分"+myS+"."+myMS+"秒";
        }else{
			var str = "真遗憾您来晚了，抢购已经结束。";	
		}
		obj.html(str);
      });
    }, 100);
});
})(window.KT, window.jQuery);
</script>
<script type="text/javascript">
$(document).ready(function(){
	$(".opatyBtn").mouseover(function(){
		$(this).find('span').css('display','block');
	}).mouseout(function(){
		$(this).find('span').css('display','none');
	});
	
	
	$(".in_mary_time span.nohq").click(function(){
		if($(this).hasClass('current')){
			$("#marry_date").attr("disabled", false);
			$(this).removeClass('current');
			$(this).find('i').hide();
		}else{
			$("#marry_date").attr("disabled", "disabled");
			$(this).addClass('current');
			$(this).find('i').show();
		}
		
	});

	$(".in_pub_paihang li").mouseover(function(){
		var index=$(this).index();
			$(".in_pub_paihang li").each(function(a){
				if(a==index){
					$(this).find(".in_pub_ph_hover").show();
					$(this).find('.in_pub_ph_list').addClass("big");
					}	else{
						
					$(this).find(".in_pub_ph_hover").hide();
					$(this).find('.in_pub_ph_list').removeClass("big");
					}
			});
	});
	$(".in_pub_paihang li").eq(0).mouseover();
	
	$(".in_pub_hy_paihang li").mouseover(function(){
		var index=$(this).index();
			$(".in_pub_hy_paihang li").each(function(a){
				if(a==index){
					$(this).find(".in_pub_hy_hover").show();
					$(this).find('.in_pub_hy_list').addClass("big");
					}	else{
						
					$(this).find(".in_pub_hy_hover").hide();
					$(this).find('.in_pub_hy_list').removeClass("big");
					}
			});
	});
	$(".in_pub_hy_paihang li").eq(0).mouseover();
	
	$(".in_service_menu ul.in_service_choose li").mouseover(function(){
		var index = $(this).index();
		$(".in_service_menu ul.in_service_choose li").removeClass("on").eq(index).addClass("on");
		$(".in_service_menu ul.index_ser_limenu").hide().eq(index).show();
		}).eq(0).mouseover();
	});
</script>
