    $(function () {
        // 显示错误信息
        var error = '';
        if (error !== '') {
            dialogPop('错误', '<div style="min-width:200px"></div>', '', '', '关闭', true)
        }

        // 验证码图像点击更新
        $('#captchaImg').click(function () {
            $.get('/p2puser/common/get-captcha-img', function (data) {
                $('#captchaImg').attr('src','/p2puser' +  data);
            })
        })

        // 网页打开时生成验证码
        $('#captchaImg').trigger('click');

