<script type="text/javascript">
var speed=40;
var seter=document.getElementById("seter");
var seter2=document.getElementById("seter2");
var seter1=document.getElementById("seter1");
seter2.innerHTML=seter1.innerHTML;
function Marquee(){
 if(seter2.offsetTop-seter.scrollTop<=0)
  seter.scrollTop-=seter1.offsetHeight;
 else{
  seter.scrollTop++;
 }
}
var MyMar=setInterval(Marquee,speed);
seter.onmouseover=function() {clearInterval(MyMar)}
seter.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
</script>

<script type="text/javascript">

       var scrolltimes=25;
      var demo = document.getElementById("wrap");
      var a = document.getElementById("a");
      var b = document.getElementById("b");
      b.innerHTML = a.innerHTML;
      function scrolls(){
        if(b.offsetWidth - demo.scrollLeft <=5){
         demo.scrollLeft =0;
        }
        else{
         demo.scrollLeft++;
        }
      }
    
	 var OverSet1=setInterval(scrolls,scrolltimes);
	 demo.onmouseover=function() {clearInterval(OverSet1)}
	 demo.onmouseout=function() {OverSet1=setInterval(scrolls,scrolltimes)}
</script>


