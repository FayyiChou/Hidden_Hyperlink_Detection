<script type="text/javascript">
$(".MainNav li").mouseenter(function(){
   $(this).find(".DropDown").css("display","block");
   $(this).siblings().find(".DropDown").css("display","none");
}).mouseleave(function(){
   $("#nav"+navID).addClass("onnav").siblings().removeClass("onnav");
});
   $(".DropDown").mouseover(function(){
      $(this).parent().addClass("onnav");
   }).mouseleave(function(){
      $(this).parent().removeClass("onnav");
      $(this).css("display","none");
   })
</script>

