        // 库存状态
        function loadStockStatus(itemId, rcode) {
            AF.ajax({
                url: '//subject.m.autohome.com.cn/api/mall_getlist.ashx?itemIds=' + itemId,
                dataType: 'json',
                cache: false,
                success: function (re) {
                    var button = AF.$('buyButton_' + rcode);
                    var pvid = document.location.hash;
                    if (!re.result) {
                        return;
                    }
                    if (re.result.result.hitlist[0]) {
					  if (re.result.result.hitlist[0].stock_status > 0) {
                          button.href = '//m.mall.autohome.com.cn/detail/' + itemId + '-' + condition.cityId + '-2.html'+pvid;
						}
                    } else {
                        button.className = 'info-button gray';
                        button.innerHTML = '已售罄';
                    }
                }
            });
        }
        function bindStockStatus() {
            var item = null;
            var items = document.getElementsByName('itemId');
            for (var i = 0, len = items.length; i < len; i++) {
                item = items[i];
                loadStockStatus(item.innerHTML, item.getAttribute('rcode'));
            }
        }
        bindStockStatus();
    </script><script type="text/javascript">
            //滑动效果的导航封装
            function nav(option) {
                var that = this;
                that.$btnShow = option.$btnShow;
                that.$pop = option.$pop;
                that.$pop.data("nav", this);
                that.$close = option.$close;
                that.callback = option.callback;
                that.overflowbody = option.overflowbody;
                that.moveTimer = null;
                that.hideTimer = null;
                that.iscrollInit = false;
                that.init();
            }
            nav.prototype = {
                init: function () {
                    var that = this;
                    if (that.$btnShow) {
                        that.$btnShow.click(function () {
                            that.show();
                            return false;
                        });
                    }
                    if (that.$close) {
                        that.$close.click(function () {
                            that.hide();
                            return false;
                        });
                    }
                    that.$pop.on("touchmove", function (e) { e.preventDefault(); });
                    $(window).on('resize', function () {
                        if (that.$pop.hasClass("move")) {
                            that.navResize();
                        }
                    });

                },
                navResize: function () {
                    var that = this;
                    that.callback && that.callback();
                    setTimeout(function () {
                        var $filterContent = $(".filter-content", that.$pop);
                        $filterContent.css({ "height": $(window).height() - 43 });
                        if (that.iscrollInit == false) {
                            that.scroll = new IScroll($filterContent[0], {
                                click: true
                            });
                            that.iscrollInit = true;
                        }
                        that.scroll.refresh();
                    }, 100);
                    if (that.overflowbody) {
                        $("html,body").css({ "height": $(window).height(), "overflow": "hidden" });
                    }
                }, show: function () {
                    var that = this;
                    clearTimeout(that.hideTimer);
                    that.$pop.removeClass("fn-hide");
                    that.moveTimer = setTimeout(function () {
                        that.$pop.addClass("move");
                        that.navResize();
                    }, 20);
                }, hide: function () {
                    var that = this;
                    clearTimeout(that.moveTimer);
                    that.$pop.removeClass("move");
                    that.hideTimer = setTimeout(function () {
                        that.$pop.addClass("fn-hide");
                    }, 500);
                    if (that.overflowbody) {
                        $("html,body").css({ "height": "auto", "overflow": "auto" });
                    }
                }
            }
            //筛选浮层
            var iscrollInit = false;
            var cityScroll = null;
            new nav({ $btnShow: $("#filter-btn-city"), $pop: $("#filter-city"), $close: $("#filter-city .filter-header span"), overflowbody: false });
            new nav({ $btnShow: $("#filter-btn-brand"), $pop: $("#filter-brand"), $close: $("#filter-brand .filter-header span"), overflowbody: false });
            new nav({ $btnShow: $("#filter-btn-price"), $pop: $("#filter-price"), $close: $("#filter-price .filter-header span"), overflowbody: false });
            new nav({ $btnShow: $("#filter-btn-level"), $pop: $("#filter-level"), $close: $("#filter-level .filter-header span"), overflowbody: false });
        </script><style>
.filter-content {text-align:left;}
.sell-today {border-bottom:0;}
