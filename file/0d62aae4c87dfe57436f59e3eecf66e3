<script type="text/javascript">
    $(".mainnav li").hover(function () {
        $(".mainnav li").find("a").removeClass("cur");
        $(this).find("a:first").addClass("cur");
        $(this).find(".subnav").show();
    }, function () {
        $(this).find("a:first").removeClass("cur");
        $(this).find(".subnav").hide();
    });
</script>

    
