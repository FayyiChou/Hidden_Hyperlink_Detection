  <script type="text/javascript">
    $(function() {
        $('.inp-txt').focus(function() {
            var keyval = $('.inp-txt').val();
            if (keyval == '请输入关键字') {
                $('.inp-txt').val('');
            }
        })
        $('.sea-ul li').hover(function() { $(this).addClass('on'); }, function() { $(this).removeClass('on'); })
        $('.inp-txt').focusout(function() {
            var keyval = $('.inp-txt').val();
            if (keyval == '') {
                $('.inp-txt').val('请输入关键字');
            }
        })
        $('.mkey').click(function() {
            $('.sea-ul').show();
            $('.sea-ul li').click(function() {
                var keyval = $(this).text();
                $('.mkey').text(keyval);
                if ("电力中心" == keyval) {
                    $('#TypeID').val("1");
                } else if ("网络通信" == keyval) {
                    $('#TypeID').val("2");
                } else if ("煤炭煤矿" == keyval) {
                    $('#TypeID').val("3");
                } else if ("石油化工" == keyval) {
                    $('#TypeID').val("4");
                } else if ("水利环境" == keyval) {
                    $('#TypeID').val("5");
                } else if ("燃气热电" == keyval) {
                    $('#TypeID').val("6");
                } else if ("变更公告" == keyval) {
                    $('#TypeID').val("7");
                } else if ("中标公告" == keyval) {
                    $('#TypeID').val("11");
                } else {
                    $('#TypeID').val("0");
                }
                $('.sea-ul').hide();
            })
        })
        $('.submit-btn').click(function() {
            var mval = $('.mkey').text();
            var keyval = $('.inp-txt').val();
        })
        $(document).mouseup(function() {
            $('.sea-ul').hide();
        });

        $("#guanjianci").mousemove(function() {
            $("#guanjianci").click();
        })

        $("#zhaobiao").mousemove(function() {
            $("#zhaobiao").click();
        })


        $("#guanjianci").click(function() {
            $(".zbf1").hide();
            $(".zbf2").show();
            $(this).attr("class", "dh_bg");
            $("#zhaobiao").attr("class", "");
        })

        $("#zhaobiao").click(function() {
            $(".zbf1").show();
            $(".zbf2").hide();
            $(this).attr("class", "dh_bg");
            $("#guanjianci").attr("class", "");
        })


    })

    function ReMenSearch(titleS) {
        $("#txtKey").val(titleS);



    }

    function tiaozhun() {
        if (document.getElementById('txtKey').value == '') return false;

        var keyval = $('.mkey').text();



        if ("招标信息" == keyval) {

            alert("]" + $('#txtKey').val().replace(" ", "") + "[");

            location = "/so/" + $('#txtKey').val().replace(" ", "") + "/";

        } else if ("中标信息" == keyval) {

        } else {

            location = "/so/" + $('#txtKey').val().replace(" ", "") + "/";

        }
    }
</script> 
