    $(function () {
        $.ajax({
            type: "POST",
            url: "/tools/submit_ajax.ashx?action=user_check_login",
            dataType: "json",
            timeout: 20000,
            success: function (data, textStatus) {
                if (data.status == 1) {
                    $("#user-pannel").show();
                    $("#login-pannel").hide();
                    $("#menu").prepend('<li><a href="/user/center/exit.html">退出</a></li>');
                    $("#menu").prepend('<li><a href="/user/center/index.html">' + data.nickname + '</a></li>');
                    $("#users").attr("type-data", data.id);
                    if (data.avatar == "") {
                        $("#user-avatar").prop("src", "/templates/3gqctt3.0/images/webnews19.png");
                    } else {
                        $("#user-avatar").prop("src", data.avatar);
                    }
                } else {
                    $("#menu").prepend('<li><a href="/register.html">注册新帐号</a></li>');
                    $("#menu").prepend('<li><a href="/login.html"><img src="/images/logo250.png" style="border-radius:25px;width:50px;" /></a></li>');
                    $("#menu").prepend('<li><a   class="login-weibo" href="/api/oauth/sina/index.aspx"><img style="border-radius:25px;width:50px;" src="/images/login_2x.png" /></a></li>');
                    $("#menu").prepend('<li><a   class="login-qq" href="/api/oauth/qq/index.aspx"><img style="border-radius:25px;width:50px;" src="/images/login_2x_fc109e51.png" /></a></li>');
                }
            }
        });
        $("#user-avatar").click(function () {
            $("#user-pannel .nav-drop-down").fadeToggle();
        });
    });

