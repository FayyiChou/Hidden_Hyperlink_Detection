    <script type="text/javascript">
        var tv, idx;
        $(function () {
            idx = ($(".tv_item").length)-1;
            tv = setInterval(Switching, 2000);
            $(".tv_button").bind("mouseenter", function () {
                //停止
                clearInterval(tv);
            });
            $(".tv_button").bind("mouseleave", function () {
                //开始
                tv = setInterval(Switching, 2000);
            });
            $(".tv_button").bind("click", function () {
                //点击
                idx =$(".tv_item").length-parseInt($(this).text()) ;
                $(".tv_item").each(function (i, d) {
                    if (i == idx) {
                        $(d).removeClass("hide");
                        return;
                    }
                    $(d).addClass("hide");
                });
                $(".tv_button").each(function (i, d) {
                    $(d).removeClass("tv_button_current");

                });
                $(this).addClass("tv_button_current");

            });
            $(".tv_item").bind("mouseenter", function () {
                //停止
                clearInterval(tv);
            });
            $(".tv_item").bind("mouseleave", function () {
                //开始
                tv = setInterval(Switching, 2000);
            });
           //对联广告
          coupltes (); 

          var loadt = window.setInterval(function () {
            $("#bigpic").show();
            $("#bgdiv").animate({ "height": "300px" }, 1000, function () {               
                var i = 3;
                var timer = window.setInterval(function () {
                    if (i <= 0) {
                        $("#bgdiv").animate({ "height": "40px" }, 1500, function () {
                            $("#bigpic").attr("src", "http://www.cdzk.com/upload/Content_Publish/0000000000530/min-io.gif").css("height", "40px");
                        })
                        window.clearInterval(timer);
                    }
                    i--;
                }, 1000);
            });
            window.clearInterval(loadt);
        }, 2000);

        });
 
        function Switching() {
            idx--;
             if(idx<0){
                 idx=$(".tv_item").length-1;
             }
            
            $(".tv_item").each(function (i, d) {
                if (i == idx) {
                    $(d).removeClass("hide");

                    return;
                }
                $(d).addClass("hide");
            });
            $(".tv_button").each(function (i, d) {

                if (i == idx) {
                    $(d).addClass("tv_button_current");

                    return;
                }
                $(d).removeClass("tv_button_current");
            });
        } 

