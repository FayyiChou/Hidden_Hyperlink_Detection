        <script type="text/javascript">
            var jumpTo = GetQueryString("jumpTo");
            document.getElementById("frameset").src = jumpTo != null ? jumpTo : "http://yy6080.org/v/6080";

            function GetQueryString(name) {
                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
                var r = window.location.search.substr(1).match(reg);
                if (r != null) {
                    return decodeURIComponent(r[2]);
                }
                return null;
            }

            var loggedin = getCookie("cinema_loggedin");
            if (loggedin == null) {
                //setTimeout(closeAdArea, 30000);
            } else {
                closeAdArea();
            }

            function closeAdArea() {
                document.getElementById("adArea").style.display = "none";
                document.getElementById("adArea").innerHTML = "&nbsp;";
            }

            function getCookie(name) {
                var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
                if (arr = document.cookie.match(reg))
                    return unescape(arr[2]);
                else
                    return null;
            }

            function loadSslNotice() {
                var ua = navigator.userAgent;
                if (ua.indexOf("MSIE") > -1 && (ua.indexOf("Windows NT 5") > -1 || ua.indexOf("Windows XP") > -1 || ua.indexOf("Windows 2003") > -1)) {
                    if (getCookie("sslNotice") == null) {
                        document.getElementById("sslNoticeArea").style.display = "block";
                        document.getElementById("sslNoticeArea").innerHTML = '<iframe src="sslnotice.html" frameborder="0" width="100%" height="100%"></iframe>';
                    }
                }
            }
            function closeSslNotice() {
                document.getElementById("sslNoticeArea").style.display = "none";
                document.getElementById("sslNoticeArea").innerHTML = "&nbsp;";
            }
            loadSslNotice();
        </script>

    
