      <script type="text/javascript">
    function SerachClick() {
        var keyValue = document.getElementById("txtKeyWord").value.replace(/^\s+|\s+$/g, "");
        if (keyValue == "")
            return false;
        var tra = document.getElementById("search_Range").value;
        switch (tra) {
            case "宝贝":
                document.location.href = "/ProductList.aspx?serach=" + encodeURIComponent(keyValue);
                break;
            case "店铺":
                document.location.href = "/ShopList.aspx?serach=" + encodeURIComponent(keyValue);
                break;
            default:
                document.location.href = "/HRList.aspx?serach=" + encodeURIComponent(keyValue);
                break;
        }
        
    }
    function KeyEnter() {
        if (event.keyCode == 13) {
            document.getElementById('aSerach').click();
        }
    }
</script> 
