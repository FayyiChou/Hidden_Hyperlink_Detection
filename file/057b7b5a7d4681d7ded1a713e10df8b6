        function trim(value) {
            return value.replace(/(^\s*)|(\s*$)/g, "");
        }
        function opera(x, y) {
            var re = /^[1-9]\d*$/;
            var temp = document.getElementById(x).value;
            var rs = new Number(temp);
            if (!re.test(temp)) {
                alert('请输入整数');
                document.getElementById(x).focus();
                return false;
            }
            if (y) {
                document.getElementById(x).value = rs + 1;
            }
            else {
                if (rs > 1) document.getElementById(x).value = rs - 1;
                else {
                    alert('购买数量应大于0');
                    document.getElementById(x).focus();
                    return false;
                }
            }
        }
        function addcheck() {
            var re = /^[1-9]\d*$/;
            var state = document.getElementById("state").innerHTML;
            if (state == "0") {
                alert("抱歉，此商品已售出或者缺货，您暂时不能下单");
                return false;
            }
            var temp = document.getElementById("buynum").value;
            if (!re.test(temp)) {
                alert('请输入整数');
                document.getElementById("buynum").focus();
                return false;
            }
            if (temp > state) {
                alert('库存不足，您需要减少购买量');
                return false;
            }
        }
    </script> 
