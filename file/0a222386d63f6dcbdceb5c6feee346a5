    <script>
        seajs.use([], function(){
            var search = location.search,
            param, href,
            paramObj = {},
            $desktop = $('.desktop-regis-page'),
            $site = $('.login-page')
            $a = $('.desktop-regis-page .no-account a');

            if(search){
                param = search.substring(1);
                $(param.split('&')).each(function(){
                    var v = this.split('=');

                    paramObj[v[0]] = v[1];
                });
            }
            if(paramObj.from === 'desktop'){
                $desktop.show();
                $site.remove();
            }else{
                $site.show();
                $desktop.remove();
            }
            href = $a.attr('href', ($a.attr('href') + '&from=desktop'));
        });
    </script>

