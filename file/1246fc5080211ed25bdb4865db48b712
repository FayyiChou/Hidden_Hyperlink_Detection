	var domain, _domain = window.location.host.split('.');
	var len = _domain.length;
	if (len > 2){
		domain = _domain[len-2] + '.' + _domain[len-1];
	} else {
		domain = _domain.join('.');
	}
	document.getElementById("domain").innerHTML = domain;
	var url ="http://escrow.ename.com/escrow/escr/"+domain+"?f=guoqi";
	
