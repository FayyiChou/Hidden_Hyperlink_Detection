	}else{
		$("#end_date_"+id).html(daysold+"天"+(hrsold<10?'0'+hrsold:hrsold)+"小 时"+(minsold<10?'0'+minsold:minsold)+"分"+(seconds<10?'0'+seconds:seconds)+"秒."+ms2);
	}
	}
}
$(".com-time").each(function() {
        var reg = /^[0-9]+$/;
        var times = $(this).attr('data-time');
		var id = $(this).attr('data-id');
        if (reg.test(times)) {
            show_date_time(times, 3, id);
        }
	});
$(".times").each(function() {
        var reg = /^[0-9]+$/;
        var times = $(this).attr('data-time');
        if (reg.test(times)) {
            show_date_time(times, 3, 0);
        }
	});;
</script><div class="main pr mt25 clear"><ul class="goods-list clear"><li class="" gtype="1" id="13795"><div class="list-good buy"><div class="good-pic"><a isconvert="1" data-itemid="37869882880" class="pic-img" target="_blank" rel="nofollow"><img alt="2016夏季新款女装中长款连衣裙修身OL无袖立领职业大码夏装裙子" src="http://img01.taobaocdn.com/bao/uploaded/i1/TB1AchALVXXXXbxaXXXXXXXXXXX_!!0-item_pic.jpg_310x310.jpg" /></a></div><h3 class="good-title"><a href="/item-index-id-13795.html" target="_blank">2016夏季新款女装中长款连衣裙修身OL无袖立领职业大码夏装裙子</a><div class="icon-all" style="display:none;"></div><span class="sold">已售<em>135</em></span></h3><div class="good-price"><span class="price-current" ><em>￥</em>219.0</span><span class="des-other"><strong></strong><p><span class="price-old"><em>￥</em>399.0</span><span class="discount">(<em>5.5</em>折)</span></p></span><div class="btn buy m-buy"><i><em class="m-icon"></em><span>天猫</span></i></div></div><a class="y-like my-like" data-pid="37869882880" href="javascript:;" title="加入收藏"><i class="like-ico"><span class="heart_left"></span><span class="heart_right"></span></i></a></div></li><li class="" gtype="1" id="39522"><div class="list-good buy"><div class="good-pic"><a isconvert="1" data-itemid="37736326574" class="pic-img" target="_blank" rel="nofollow"><img alt="职业女裙套装2016夏新款修身职业女装时尚面试正装ol职场女连衣裙" src="http://img01.taobaocdn.com/bao/uploaded/i1/T1RvsnFGRdXXXXXXXX_!!0-item_pic.jpg_310x310.jpg" /></a></div><h3 class="good-title"><a href="/item-index-id-39522.html" target="_blank">职业女裙套装2016夏新款修身职业女装时尚面试正装ol职场女连衣裙</a><div class="icon-all" style="display:none;"></div><span class="sold">已售<em>81</em></span></h3><div class="good-price"><span class="price-current" ><em>￥</em>198.0</span><span class="des-other"><strong></strong><p><span class="price-old"><em>￥</em>895.0</span><span class="discount">(<em>2.2</em>折)</span></p></span><div class="btn buy m-buy"><i><em class="m-icon"></em><span>天猫</span></i></div></div><a class="y-like my-like" data-pid="37736326574" href="jav