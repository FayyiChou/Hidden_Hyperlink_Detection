//Set the id names of your tablinkg (without a number at the end)
var tablinkg_idnameb = new Array("tablinkg");
//Set the id name of your tabcontentgarea (without a number at the end)
var tabcontentg_idnameb = new Array("tabcontentg");
//Set the number of your tabs
var tabcountf = new Array("2");
//Set the Tab wich should load at start (In this Example:Tab 2 visible on load)
var loadtabsf = new Array("1");
//Set the Number of the Menu which should autochange (if you dont't want to have a change menu set it to 0)
var autochangemenuf = 1;
//the speed in seconds when the tabs should change
var changespeedf = 15;
//should the autochange stop if the user hover over a tab from the autochangemenuf? 0=no 1=yes
var stoponhoverf = 1;
//END MENU SETTINGS

/*Swich EasyTabs Functions - no need to edit something here*/

	function easytabsf(menunrf, active) {
		if (menunrf == autochangemenuf) {
			currenttabb = active;
		}
		if ((menunrf == autochangemenuf) && (stoponhoverf == 1)) {
			stop_autochange();
		} else if ((menunrf == autochangemenuf) && (stoponhoverf == 0)) {
			counter = 0;
		}
		menunrf = menunrf - 1;
		for (i = 1; i <= tabcountf[menunrf]; i++) {
			document.getElementById(tablinkg_idnameb[menunrf] + i).className = 'tab' + i;
			document.getElementById(tabcontentg_idnameb[menunrf] + i).style.display = 'none';
		}
		document.getElementById(tablinkg_idnameb[menunrf] + active).className = 'tab' + active + ' tabactive';
		document.getElementById(tabcontentg_idnameb[menunrf] + active).style.display = 'block';
	}
	var timer;
	counter = 0;
	var totaltabs = tabcountf[autochangemenuf - 1];
	var currenttabb = loadtabsf[autochangemenuf - 1];
	function start_autochange() {
		counter = counter + 1;
		timer = setTimeout("start_autochange()", 1000);
		if (counter == changespeedf + 1) {
			currenttabb++;
			if (currenttabb > totaltabs) {
				currenttabb = 1;
			}
			easytabsf(autochangemenuf, currenttabb);
			restart_autochange();
		}
	}
	function restart_autochange() {
		clearTimeout(timer);
		counter = 0;
		start_autochange();
	}
	function stop_autochange() {
		clearTimeout(timer);
		counter = 0;
	}

	function demsg() {
		var menucountb = loadtabsf.length;
		var a = 0;
		var b = 1;
		do {
			easytabsf(b, loadtabsf[a]);
			a++;
			b++;
		} while (b <= menucountb);
		if (autochangemenuf != 0) {
			start_autochange();
		}
		
		$("#friendtable").show();
	}

	if (document.all) {
		window.attachEvent('onload', demsg);		
	}

	else {
		window.addEventListener('load', demsg, false);//firefox
	}
