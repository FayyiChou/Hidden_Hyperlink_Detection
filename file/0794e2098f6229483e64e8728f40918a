  jQuery(".searcharea ul li a").click(function() {
    jQuery(".searcharea ul li a").removeClass("selected");
    jQuery(this).addClass("selected");
  });

  jQuery("#searchWord").click(function() {
    if (jQuery("#searchWord").val() == "请输入搜索内容") {
      jQuery(this).val("");
    }
    jQuery(this).css("color", "#000");
  });

  jQuery("#searchWord").blur(function() {
    jQuery("#seaWord").val(jQuery("#searchWord").val());
    if (jQuery("#searchWord").val() == "") {
      jQuery("#searchWord").val("请输入搜索内容");
      jQuery(this).css("color", "silver");
    }
  });
  jQuery("#searchBtn").click(function() {
    submitFormse();
  });
  jQuery("#searchWord").keydown(function(event) {
    if (event.keyCode == '13') {
      jQuery("#seaWord").val(jQuery("#searchWord").val());
      submitFormse();
    }
  });

});

function submitFormse() {
  var selse = jQuery(".searcharea ul li a[class=selected]").attr("val");
  var actionse = "";
  var keyWord = jQuery("#seaWord").val();
  if (keyWord == "") {
    jQuery("#searchWord").val("");
    jQuery("#searchWord").focus();
    return false;
  }
  if (selse == "1") {
    actionse = "/web/marketing/steel?searchString="+keyWord;
  } else if (selse == "2") {
    actionse = "/web/purchase/jpxx?searchString="+keyWord;
  }
  else if (selse == "3") {
    actionse = "/web/guest/3?queryString=" + keyWord;
  }
  
 // window.location.href = actionse; //本窗口打开
  window.open(actionse); //新窗口打开
}

function validateUrl(urlList, key) {
  for (var i = 0; i < urlList.length; i++) {
    if (urlList[i] == key) {
      return true;
    }
  }
  return false;
}


function portal_normal_Xmlhttp() { //
  var xmlhttp;
  try {
    xmlhttp = new ActiveXObject("msxml2.XMLHTTP");
  } catch (e) {
    try {
      xmlhttp = new ActiveXObject("microsoft.XMLHTTP");
    } catch (e) {
      try {
        xmlhttp = new XMLHttpRequest();
      } catch (e) {}
    }
  }
  return xmlhttp;
}

