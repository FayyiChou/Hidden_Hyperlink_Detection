<script type="text/javascript">
    ShowInfo(C.Gsn("Ms1"),C.Gsn("Cs1"));

    window.onscroll= function(){  FiXBar();  }
    function FiXBar(){
        var BarWrp = C.G("ShrBar"),
            Bar = C.Cls("AddShrBar","div",BarWrp)[0],
            Art = C.Cls("Art")[0],
            BarTop = BarWrp.offsetTop,
            ScrTop=document.body.scrollTop || document.documentElement.scrollTop;/*document.body.scrollTop for chrome*/

        if(BarTop-ScrTop<=0){

            if (Art.offsetHeight - ScrTop > 282) {
                Bar.style.display = "block";
                C.AddClass(Bar,"ShrBarFx");
            } else {
                Bar.style.display = "none";
            }
        }else{
            C.DelClass(Bar,"ShrBarFx");
        };
    }
</script>
