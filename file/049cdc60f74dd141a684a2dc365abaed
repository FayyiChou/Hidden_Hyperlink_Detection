var speed=30
picture2.innerHTML=picture1.innerHTML
function Marquee1(){
if(picture2.offsetWidth-picture.scrollLeft<=0)
picture.scrollLeft-=picture1.offsetWidth
else{
picture.scrollLeft++
} }
var MyMar1=setInterval(Marquee1,speed)
picture.onmouseover=function() {clearInterval(MyMar1)}
picture.onmouseout=function() {MyMar1=setInterval(Marquee1,speed)}
