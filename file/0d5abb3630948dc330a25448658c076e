<script type="text/javascript">
    function logonSubmit() {
        var userName = et.dom.byId("iusername").value.replace(/(\s)+/, "");
        var userPwd = et.dom.byId("iuserpwd").value;
        var remember = et.dom.byId("remember");
        var rememberme = remember.checked ? "1" : "0";
        userName = et.user.utility.CToH(userName);
        userPwd = et.user.utility.CToH(userPwd);
        if (userName == "" || userName == "请输入用户名或验证的手机号") {
            $("#J_mobilePhone_msg").removeClass('fn-hide');
            return false;
        }
        if (userPwd == "" || userPwd == "密码") {
            $("#J_password_msg").removeClass('fn-hide');
            return false;
        }
        var otherParam = "";
        var urlParam = et.user.utility.getUrlParams();
        if (urlParam.UrlMap != null && urlParam.UrlMap.length != 0) {
            if (urlParam.UrlMap.item("forward"))
                otherParam += "&forward=" + encodeURI(urlParam.UrlMap.item("forward"));
            var fromurl = urlParam.UrlMap.item("fromurl");

            if (typeof(fromurl) != "undefined") {
                otherParam += "&fromurl=" + encodeURI(fromurl);
            }
        }

        et.user.utility.signOn(userName, userPwd, rememberme, logonSubmitCallBack, otherParam);
        return false;
    }

    function logonSubmitCallBack(type, json, http) {
        if (json.success) {
            /*携带的邀请码是非201的，跳转到学习中心激活卡课程*/
            if(hasSpecialCode ()){
                location.href = "http://www.jd100.com/studycenter/#/courses/study/";
                return;
            }

            if (!json.fromurl || location.href == json.fromurl) {
                location.href = etWebSiteUrl;
                return;
            }

            location.href = json.fromurl;
            return;
        }
        else {
            //判断用户是否有一周之内不再显示的cookie
            if($.cookie('NOPROMPTINSEVENDAY')== undefined){
                if(json.code === 400) { //没有绑定手机号
                    location.href = json.bindurl;
                    return;
                } else if(json.code === 403) {
                    location.href = json.bindurl;
                    return;
                } else {
                    $("#J_logoin_msg").removeClass('fn-hide');
                    return;
                }
            }else{
                setUserlogonStatus();
            }
        }
    }

    //绑定微信页勾选7天不显示，则设置登录状态，直接登录
    function setUserlogonStatus(){
        $.ajax({
            type: 'POST',
            url: etWebSiteUrl + '/service/usercenter/',
            data : 'do=setuserlogonstatus',
            dataType : 'json',
            success : function (data) {
                if(data.success){
                    /*携带的邀请码是非201的，跳转到学习中心激活卡课程*/
                    if(hasSpecialCode ()){
                        location.href = "http://www.jd100.com/studycenter/#/courses/study/";
                        return;
                    }
                    location.href = decodeURIComponent($("#J_fromurl").val());
                }
            }
        });
    }

    //携带邀请码非201
    function hasSpecialCode () {
        var $specialCode = $('#J_special_code').val();
        return Boolean($specialCode && $specialCode != "201");
    }

    function hasPlaceholderSupport() {
        return 'placeholder' in document.createElement('input');
    }

    function IEPlaceholder() {
        var isSupport = hasPlaceholderSupport();
        if(!isSupport) {
            $(".j_logonInputText").removeClass("fn-hide");

            $(".j_logonInput").bind("keyup keydown keypress", function(){
                if($(this).val()) {
                    $(this).siblings(".j_logonInputText").addClass("fn-hide");
                    return;
                }
                $(this).siblings(".j_logonInputText").removeClass("fn-hide");
            });

            $(".j_logonInputText").click(function() {
                $(this).siblings(".j_logonInput").focus();
            });

            $(".j_logonInput").on("blur", function () {
                var $self = $(this);
                if(!($self.val())) {
                    $self.siblings(".j_logonInputText").removeClass("fn-hide");
                }
            });
        }
    }

    $(document).ready(function () {
        $(".j_logonInput").on("focus", function () {
            var $self = $(this);
            $self.siblings(".pass_label").addClass("pass_label_focus").parent().next(".j_formCheckMsg").addClass("fn-hide");
            $("#J_logoin_msg").addClass("fn-hide");
        }).on("blur", function () {
            var $self = $(this);
            $self.siblings(".pass_label").removeClass("pass_label_focus");
        });

        IEPlaceholder();
    });
</script>

