<script>
function getClassNode(node,class1) 
{
    var p = document.getElementById(node).childNodes, g = [], c = class1;
	for(var i = 0,pl = p.length;i<pl;i++){
	    if(p[i].className === c) g.push(p[i]);
	}
	return g;
}
function change_sort(item,elem,obj,cat_id)
{
    elem = elem.toUpperCase();
    var itemObj = document.getElementById(item);
    var elems = itemObj.getElementsByTagName(elem);
	
    var itemObj1 = getClassNode('product_'+cat_id,'cat_goods_in');
	
	
	
    for (var i=0,l=elems.length; i<l; i++)
    {
        elems[i].className = 'a1';
		itemObj1[i].style.display='none'
		
		if(elems[i]==obj)
		{
		   itemObj1[i].style.display='block'
		}
		
    }
	
	
	obj.className='a1_c';
	
}
</script>
