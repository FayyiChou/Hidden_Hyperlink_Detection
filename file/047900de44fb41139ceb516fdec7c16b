<script type="text/javascript">
var SYS_CTX='';
var FILE_SERVER='http://www.hrstage.com/chj_txt_root/file_root/';
$(document).ready(function(){

  /** 最新招聘会 */
  if($("#zxzph").length==1){
	  $.post("/index/xczph.action", {"page.pageSize" : 4},function(data) {
			$.each(data, function(i, item) {
				var href = SYS_CTX + "/jobmeeting/xczph_info.action?id=" + item.id;
				$("#zxzph").append("<p><a href='"+href+"' title='"+item.title+"' target='_blank'>"+(i+1)+"、"+item.title+"</a></p>");
			});
		}, "json");
  }
  /** 最新培训班 */
  if($("#zxpxb").length==1){
	  $.post("/index/course.action", {"page.pageSize" : 4},function(data) {
			$.each(data, function(i, item) {
				var href = SYS_CTX + "/train/courseclass.action?id=" + item.id;
				$("#zxpxb").append("<p><a href='"+href+"' title='"+item.title+"' target='_blank'>"+(i+1)+"、"+item.title+"</a></p>");
			});
	  }, "json");
  }
  /** 最新新闻动态 */
  if($("#zxxwdt").length==1){
  	$.post("/index/zxxwdt.action",function(data) {
		$.each(data, function(i, item) {
			var href = SYS_CTX + "/templet/article/"+item.id+".html";
			$("#zxxwdt").append("<p><a href='"+href+"' title='"+item.title+"' target='_blank'>"+(i+1)+"、"+item.title+"</a></p>");
		});
	}, "json");
  }

  /** 不显示在线需求图片 */
  $("img.onlinerequire").remove();
});
</script>

