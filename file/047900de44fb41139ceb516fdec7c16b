<script type="text/javascript">
var SEARCH_PARAM={
		"job":{"action":""+"/frontjob/searchjob.action","name":"keyword"},
		"resume":{"action":""+"/frontjob/resumesearch.action","name":"keywords"}
};

$(document).ready(function() {
	var tag=$("input[name='searchTag']").val();
	if(tag){
		$("#searchform").attr("action",SEARCH_PARAM[tag]["action"]);
		$("#searchform #header_searchform").attr("name",SEARCH_PARAM[tag]["name"]);
		$("#searchform .search_option[tag='"+tag+"']").addClass("selected");
	}
	
	var _this = $('#header_searchform');
    _this.blur(function () {
        if ($(this).val() == "") {
            $(this).val("请输入关键词");
        }
    }).focus(function () {
        if ($(this).val() == "请输入关键词") {
            $(this).val("");
        }
    });
    $("#searchform").submit( function() {
        if(_this.val()=='请输入关键词'){
        	_this.val("");
    	}
    });


    $("#searchBtn").click(function() {
		$("#searchPart").toggleClass("hover");
     });

    $("#searchform .search_option").click(function() {
        var $_parent=$(this).parent();
        $_parent.find(".search_option").removeClass("selected");
        $(this).addClass("selected");
        $("#searchform").attr("action", SEARCH_PARAM[$(this).attr("tag")]["action"]);
        $("#searchform #header_searchform").attr("name",SEARCH_PARAM[$(this).attr("tag")]["name"]);
    });
    
    /**绝对定位和相对定位 */
    if($(window).width()>1200){
  	  $("img.onlinerequire").css("right",($(window).width()-947)/2-120);
  	  $(window).resize(function() {
  			$("img.onlinerequire").css("right",($(window).width()-947)/2-120);
  		});
   	}
    
  	//计算返回顶部按钮位置
	$(".backToTop").css("right",($(window).width()-947)/2-35);
    $(window).resize(function() {
		$(".backToTop").css("right",($(window).width()-947)/2-35);
   	});
    
  	//'返回顶部按钮'在顶部，就不显示，否则显示
    $(window).scroll(function() {
		if ($(document).scrollTop() > 0) {
		    $(".backToTop").show();
		}else{
			$(".backToTop").hide();
		}
    });

    //返回顶部
    $(".backToTop").click(function(){
    	$("html,body").animate({"scrollTop":"0"},"fast");
    });

   //微信二维码-0.5秒后显示（隐藏） 
    $("#gfwenxin").hover(function(){
    	setTimeout(function(){$("#weixin img.erweima").show();},500);
    },function(){
    	setTimeout(function(){$("#weixin img.erweima").hide();},500);
    });
    
 $("#toHypxzx").click(function(){
    	ssoOut('http://pst.hrstage.com/member/memHome');
    });
    
});

//至培训平台
function ssoOut(url){
	var _url = SYS_CTX+"/sso_out.action?url=http://pst.hrstage.com/sso/{code}?url="+url;
	window.location.href = _url;
}

//漕河泾与领港的跳转 
function sso(url){
	var _url = url+"?_jsessionid_="+'75465B8F5707B0EC34A0725D52B476E8';
	window.location.href = _url;
}

</script>
