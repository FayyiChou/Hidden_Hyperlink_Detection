<!-- 漂浮站点广告 -->













<script type="text/javascript">
	var dirX =1,dirY =1;
	var posX =0,posY =0;
	$(document).ready(function(){
		 var iIntervalID = "";
		if ($("#float_icon").length == 1) {
		    document.getElementById("float_icon").style.top =0;
		    document.getElementById("float_icon").style.left =0;
		    float_icon.style.visibility ="visible";
		    iIntervalID = window.setInterval("moveIcon()",60); 
	  	}
		
		$(".closespan2").live("click",function () {
			  if ($(this).parents("#float_icon")) {
				  $(this).parents("#float_icon").remove();
			  }
		  });
		
		$("#float_icon").live("mouseover",function () {
			  window.clearInterval(iIntervalID);
		  });
		$("#float_icon").live("mouseout",function () {
			 iIntervalID = window.setInterval("moveIcon()",60); 
		  });
	});
	
	function moveIcon() {
		  posX +=(1 *dirX);
		  posY +=(1 *dirY);
		  $("#float_icon").css("top",posY);
		  $("#float_icon").css("left",posX);
		  if(posX < 1 ||posX + document.getElementById("float_icon").offsetWidth+5 > document.body.clientWidth){
		      dirX =-dirX;
		  }
		  if(posY < 1 ||posY + document.getElementById("float_icon").offsetHeight+30 > document.body.clientHeight){
		      dirY =-dirY;
		  } 
	}
</script>

<div>
	<!-- 漂浮站点广告 -->
	
</div>



