  <script>
//选项卡
$('div.tab_menu li').live('click', function(){
    var $div_li = $("div.tab_menu li");
    $(this).addClass("selected").siblings().removeClass("selected");
    var index = $div_li.index(this);

    $("div.tab_box > div").eq(index).show().siblings().hide();
});

$('#close_login_window').live('click', function() {
    $.closeDOMWindow();
});

$('#login_username').live('focus', function(){
    var txt_value = $(this).val();
    if (txt_value == this.defaultValue) {
        $(this).val('').addClass('rightColor').removeClass('tip-text');
    } else if (txt_value != ''){
        $(this).addClass('rightColor').removeClass('tip-text');
    }
});

$('#login_username').live('blur', function(){
    var txt_value = $(this).val();
    if (txt_value == '') {
        $(this).val(this.defaultValue).addClass('tip-text').removeClass('rightColor');
    } else if (txt_value) {
        $(this).addClass('tip-text').removeClass('rightColor');
    }
});

$('#login_password').live('focus', function(){
    var txt_value = $(this).val();
    if (txt_value == this.defaultValue){
        $(this).val('').addClass('rightColor').removeClass('tip-text');
    }else if(txt_value){
        $(this).addClass('rightColor').removeClass('tip-text');
    }
});

$('#login_password').live('blur', function(){
    var txt_value = $(this).val();
    if (txt_value == '') {
        $(this).val(this.defaultValue).addClass('tip-text').removeClass('rightColor');
    } else if (txt_value) {
        $(this).addClass('tip-text').removeClass('rightColor');
    }
});

$('#login_submit').live('click', function(){
    $tip = '用户名 / 邮箱 / 手机号';
    $login_username = $('#login_username');
    if (($login_username.val().length == 0) || ($login_username.val() == $tip)) {
        $login_username.focus();
        return false;
    }

    $login_password = $('#login_password');
    if ($login_password.val().length == 0) {
        $login_password.focus();
        return false;
    }
    $.ajax({
        type:'post',
        url:'/cgi-bin/ajax/ajax_login.php',
        data:$('#login_form').serialize(),
        success:function(e){
            if(e.error){
                $('#login_usernameTip').show();
                $('#login_usernameTip').addClass('onError');
                $('#login_usernameTip').html(e.data.notice);
            }else{
                window.location.reload();
            }
        }
    });
    return false;
});

var username_valid = false;
var password_valid = false;
var password2_valid = false;

$('#username').live('focus', function(){
    var tip = '4-20个字符(汉字、字母、数字、下划线)';
    $('#usernameTip').show();
    $('#usernameTip').removeClass().addClass('onFocus');
    $('#usernameTip').html(tip);
});

$('#username').live('blur', function(){
    var val = $(this).val();
    var len = val.replace(/[^\x00-\xff]/g,"xx").length;
    var tip = '';
    if (len < 4 || len > 20) {
        tip = "用户名长度不正确，请确认";
        $('#usernameTip').show();
        $('#usernameTip').removeClass().addClass('onError');
        $('#usernameTip').html(tip);
        return false;
    }
    if (/^1[3458][0-9]{9,9}$/.test(val)) {
        tip = "手机号不能作为用户名，请更改";
        $('#usernameTip').show();
        $('#usernameTip').removeClass().addClass('onError');
        $('#usernameTip').html(tip);
        return false;
    }
    var reg = /^[\w|\u4E00-\u9FA5]*$/;
    if (!reg.test(val)) {
        tip = "用户名格式不正确，请确认";
        $('#usernameTip').show();
        $('#usernameTip').removeClass().addClass('onError');
        $('#usernameTip').html(tip);
        return false;
    }
    tip = '';
    $('#usernameTip').show();
    $('#usernameTip').removeClass().addClass('onCorrect');
    $('#usernameTip').html(tip);
    username_valid = true;
    return true;
});

$('#password').live('focus', function(){
    var tip = '请输入6-20个不连续，不重复的字符';
    $('#passwordTip').show();
    $('#passwordTip').removeClass().addClass('onFocus');
    $('#passwordTip').html(tip);
});

$('#password').live('blur', function(){
    var val = $(this).val();
    var len = val.replace(/[^\x00-\xff]/g,"xx").length;
    var tip = '';
    if (len < 6) {
        tip = "您输入的管理密码长度不正确";
        $('#passwordTip').show();
        $('#passwordTip').removeClass().addClass('onError');
        $('#passwordTip').html(tip);
        return false;
    }

    var cc = 0;
    var c_step = 0;
    for (var i = 0; i < len; ++i) {
        if (val.charCodeAt(i) == val.charCodeAt(0) ) {
            ++cc;
        }
        if (i > 0 && val.charCodeAt(i) == (val.charCodeAt(i-1)+1)) {
            ++c_step;
        }
    }
    if (cc == len || c_step == len-1) {
        tip = "您输入的管理密码格式不正确";
        $('#passwordTip').show();
        $('#passwordTip').removeClass().addClass('onError');
        $('#passwordTip').html(tip);
        return false;
    }

    var reg = /^[-_+=~`a-zA-Z0-9!@#$%^&*<>:;.,\S\|\'\"\\?\\/\\(\\)\\{\\}\\[\\]\\<\\>\\\\]{6,}$/;
    if (reg.test(val)) {
        tip = "您输入的管理密码格式不正确";
        $('#passwordTip').show();
        $('#passwordTip').removeClass().addClass('onError');
        $('#passwordTip').html(tip);
        return false;
    }
    tip = '';
    $('#passwordTip').show();
    $('#passwordTip').removeClass().addClass('onCorrect');
    $('#passwordTip').html(tip);

    password_valid = true;
    return true;
});

$('#password2').live('focus', function(){
    var tip = '请再次输密码';
    $('#password2Tip').show();
    $('#password2Tip').removeClass().addClass('onFocus');
    $('#password2Tip').html(tip);
});

$('#password2').live('blur', function(){
    $password = $('#password');
    $password2 = $('#password2');
    if ($password.val() != $password2.val()) {
        var tip = '两次密码不一致，请确认';
        $('#password2Tip').show();
        $('#password2Tip').removeClass().addClass('onError');
        $('#password2Tip').html(tip);
        return false;
    }
    tip = '';
    $('#password2Tip').show();
    $('#password2Tip').removeClass().addClass('onCorrect');
    $('#password2Tip').html(tip);

    password2_valid = true;
    return true;
});

$("#affirm").live('click', function() {
    if ($(this).attr('checked') != 'checked') {
        $("#affirmTip").attr('class','onError')
        $("#affirmTip").html("必须要接受列表网服务协议哦！");
        return false;
    } else {
        $("#affirmTip").removeAttr('class')
        $("#affirmTip").html("");
        return true;
    }
});

$('#reg_submit').live('click', function(){
    if ($('#affirm').attr('checked') != 'checked') {
        return false;
    }
    $('#username').blur();
    if (!username_valid) {
        return false;
    }
    $('#password').blur();
    if (!password_valid) {
        return false;
    }
    $('#password2').blur();
    if (!password2_valid) {
        return false;
    }
    $.ajax({
        type:'post',
        url:'/cgi-bin/ajax/ajax_register.php',
        data:$('#reg_form').serialize(),
        success:function(e){
            if(e.error){
                $('#usernameTip').show();
                $('#usernameTip').addClass('onError');
                $('#usernameTip').html(e.data.notice);
            } else{
                window.location.reload();
            }
        }
    });
    return false;
});
</script> 
