    $('.manage').hover(function(){
        $(".manage-div").show();
        $(".manage-span").addClass("over");
    },function(){
        $(".manage-div").hide();
        $(".manage-span").removeClass("over");
    });

    function ntabs(tabObj, obj) {
        var tabList = document.getElementById(tabObj).getElementsByTagName("li");
        for (i = 0; i < tabList.length; i++) {
            if (tabList[i].id == obj.id) {
                document.getElementById(tabObj + "_tit" + i).className = "selected";
                document.getElementById(tabObj + "_con" + i).style.display = "block";
            } else {
                document.getElementById(tabObj + "_tit" + i).className = "";
                document.getElementById(tabObj + "_con" + i).style.display = "none";
            }
        }
    }
