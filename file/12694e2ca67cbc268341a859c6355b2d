<div id='main' role='main'>
	
	
	<!-- Begin main content -->
	

<script>
	$(document).ready(function () {
		$('#load-more-issues').click(function () {

			var self = this;
			var $self = $(self);

			if ($self.hasClass('loading')) {
				return false;
			}

			var htmlContent = $self.html();
			var currentIssue = $(self).data('index');

			$self.addClass('loading');
			$self.html('<span class="loading">' + htmlContent + '</span>');

			$.ajax({
				url: $(self).attr('data-loadurl'),
				action: 'POST',
				contentType: 'application/json',
				data: { 'index': currentIssue },
				success: function (d) {
					$('#issue-container').append(d);
					$self.removeClass('loading');
					$self.html(htmlContent);
					currentIssue++;
					$(self).data('index', currentIssue);
					if (currentIssue >= $(self).attr('data-issuecount')) {
						$(self).parent().remove();
					}
				}
			});
		});
	});
</script>

