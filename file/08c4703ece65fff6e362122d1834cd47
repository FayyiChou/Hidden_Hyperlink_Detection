    <script type="text/javascript">
        //排队领礼包流程
        function packageStep(step){
            $.get(reurl('share/run.html?act=packageStep'),{inajax:1,step:step},function(data){
                if(data.indexOf('msg:') == 0){
                    alert_box(data.replace('msg:',''));
                }else{
                    $('.epet-gift').html(data);
                    $('.epet-gift').show();
                }
            }); 
        }
        //关闭窗口
        function tipClose(){
            $('.tip-cont').hide();
            $('.zindex-cont').hide();
            $(".epet-tip").hide();
            $(".epet-gift").hide();
        }
    </script>
