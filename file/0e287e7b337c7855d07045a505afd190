  <script>
        $(function () {
			//加载更多
			var pageNo = 2;
			var getMoreTips = "显示更多";
			var noMoreData = "没有更多内容了";
			var buttonLock = 0;//点击锁
			$('.getMore').click(function() {
					if(buttonLock) return false;
					buttonLock = 1;//锁住
					var url = "/news/detail";
					var dataObj = {id:"5401", pageNo:pageNo};
					$('.getMore a span').html("加载中...");
					$.post(url,dataObj,function(data){
						//alert(data);
						//alert(pageNo);
						if($.trim(data) != '') {
							$('.MKlistBox').append(data);
							$('.getMore a span').html(getMoreTips);
							buttonLock = 0;//解锁
							pageNo++;
// 							//加载图片
// 							$(".replace1").lazyload({
								//placeholder: ""
// 							});
							
						} else {
							$('.getMore a span').html(noMoreData);
						}
						
					}, 'html');		
			});
			
			
			
        })
    </script> 
