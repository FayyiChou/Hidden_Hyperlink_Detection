                            $(function () {
                                $(".shop-icon-box1").click(function () {
                                    $(this).hide();
                                    $(this).parent().find(".shop-icon-box2").show();
                                    $(this).parent().parent().find(".shop-content").show();
                                });
                                $(".shop-icon-box2").click(function () {
                                    $(this).hide();
                                    $(this).parent().find(".shop-icon-box1").show();
                                    $(this).parent().parent().find(".shop-content").hide();
