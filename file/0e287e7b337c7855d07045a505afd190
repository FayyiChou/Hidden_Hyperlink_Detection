  <script>
	$(function () {
		var merchantMenuBg = 0;
		var leftAreaShow = 0;
		var moveLength = $(document.body).width() - 60;
        $(".wap-menu").css({
            "transform": "translateX(-100%)"
        });
		$(".wap-menu-icon").click(function () {
			if (leftAreaShow == 0) {
				leftAreaShow = 1;
				$(".wap-header").css({
					"transform": "translateX(calc(100% - 60px))"
				});
				$(".content").css({
					"transform": "translateX(calc(100% - 60px))"
				});
				$(".wap-menu").css({
					"transform": "translateX(0)"
				});
			} else {
				leftAreaShow = 0;
				$(".wap-header").css({
					"transform": "translateX(0px)"
				});
				$(".content").css({
					"transform": "translateX(0px)"
				});
				$(".wap-menu").css({
					"transform": "translateX(-100%)"
				});
			}
		});
		$(".content").click(function () {
			if (leftAreaShow != 0) {
				event.preventDefault();
				leftAreaShow = 0;
				$(".wap-header").css({
					"transform": "translateX(0px)"
				});
				$(".content").css({
					"transform": "translateX(0px)"
				});
				$(".wap-menu").css({
					"transform": "translateX(-100%)"
				});
			}
		});
		$(".sub-menu-btn").click(function () {
			if (merchantMenuBg == 0) {
				merchantMenuBg = 1;
				$(".merchant-menu").css({
					"background": "#1a1a1a"
				});
			} else {
				merchantMenuBg = 0;
				$(".merchant-menu").css({
					"background": "#000"
				});
			}
			$(".wap-header-sub-menu").slideToggle();
		});
		$(".search-input").keydown(function (event) {
			var keyword = $(this).val();
			if (event.which == "13") {
				location.href = "/search/index/keyword/" + keyword;
			}
		});
	});
</script> 
