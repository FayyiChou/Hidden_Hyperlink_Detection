    <script type="text/javascript" src="//static.lecai.com/js/jquery.validate.min.js?v=201703311745"></script>
    <script type="text/javascript">
    var parentRedirect;
    $(document).ready(function() {
        $('#verify').one('focus',function(){
            $.captcha('captcha');
            $('#captcha_info').hide();
            $('#captcha_span').show();
        });
        var validator = $('#login_form').bind('invalid-form.validate', function() {
            }).validate({
            errorElement: 'span',
            errorPlacement: function(error, element) {
                if ($('#login_error .error').length == 0) {
                    error.appendTo($('#login_error'));
                }
            },
            errorClass: 'error',
            validClass: 'valid',
            highlight: function(element, errorClass, validClass) {},
            unhighlight: function(element, errorClass, validClass) {},
            onkeyup: function(element) {},
            success: function(label) {
                label.empty().removeClass(this.errorClass).addClass(this.validClass);
            },
            submitHandler: function(form) {
                $(form).ajaxSubmit({
                    success: function(jobj) {
                        var obj = $.ajaxResultHandler(jobj, {
                            successCloseOverlay: false,
                            ignoreRedirect: true
                        });
                        
                        if (obj && obj.success) {
                            $(form).resetForm();
                            initAjaxLoginAfter(obj);
                        }
                        else {
                            $.captcha('captcha');
                            $('#need_captcha').show();
                            window.require_captcha = true;
                        }
                    }
                });
            },
            rules: {
                username: {
                    required: true
                },
                passwd: {
                    required: true
                },
                verify: {
                    required: true,
                    remote: {
                        url: '/captcha_verify.php',
                        validFunction: function(jobj) {
                            var obj = $.ajaxResultHandler(jobj);
                            if (obj && obj.success) {
                                return obj.data.result === true;
                            }
                            return false;
                        }
                    }
                }
            },
            messages: {
                username: {
                    required: '请输入用户名',
                    remote: '用户名不存在'
                },
                passwd: {
                    required: '请输入登录密码'
                },
                verify: {
                    required: '请输入验证码',
                    remote: '验证码输入不正确'
                }
            }
        });
        //user prelogin
        (function(){
            var pre_login_callback = function(obj){
                if(obj.require_captcha) {
                    $('#need_captcha').show();
                    $.captcha('verify');
                    validator.settings.rules.verify.required = true;
                }else {
                    $('#need_captcha').hide();
                    //取消对于验证码的验证
                    validator.settings.rules.verify.required = false;
                }
            };
            var no_need_to_request_callback = function(tag) {
                if(tag) {
                    $('#need_captcha').show();
                    validator.settings.rules.verify.required = true;
                }else {
                    $('#need_captcha').hide();
                    validator.settings.rules.verify.required = false;
                }
            }
            $('#username').unbind().bind('focus' , function(){
                prelogin({
                    'callback' : pre_login_callback,
                    'no_need_to_request_callback' :  no_need_to_request_callback
                });
            });
            $('#passwd').bind('focus' , function(){
                prelogin({
                    'callback' : pre_login_callback,
                    'no_need_to_request_callback' :  no_need_to_request_callback
                });
            });
        })();
        $('#username').focus();

        //登录切换
        $('.login_left .login_title_ul li').menus({
            'classes' : {'on' : 'on' , 'off' : ''},
            'default_selected_index' :0,
            'hover_switch' : false,
            'switch_success' :  function($menu) {
               var login_name = $menu.attr('login_name');
               $('.login_left .login_box').hide();
               $('#login_' + login_name).show();
            }
        }); 
        var is_ie6 = G.browser.msie && (G.browser.version == 6);
        var jQuery_hidden_for_ie6;
        $('.other_box').hover(function () {
            if (is_ie6) {
                jQuery_hidden_for_ie6 = jQuery('select:visible').css('visibility', 'hidden');
            }
            $('.other_box').addClass('other_box_on');
            $('.other_text').addClass('other_text_on');
            $('#other_list').show();
        }, function () {
            $('.other_box').removeClass('other_box_on');
            $('.other_text').removeClass('other_text_on');
            $('#other_list').hide();
            if (is_ie6) {
                jQuery_hidden_for_ie6.css('visibility', 'visible');
            }
        });
    });
    </script>
