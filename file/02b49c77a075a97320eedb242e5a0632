<script>
//边框变色
var imgDom=document.getElementById("cpxs").getElementsByTagName("img");
var tabDom=document.getElementById("cpxs").getElementsByTagName("table");
//var tabDom=document.getElementById("tab");
for(var i=0;i<tabDom.length;i++){
	(function(i){
		imgDom[i].onmouseover=function (){
			tabDom[i].className='cpbgover';
		}
		imgDom[i].onmouseout=function (){
			tabDom[i].className='tjcp_table';
		}
	}(i))
}
</script>
