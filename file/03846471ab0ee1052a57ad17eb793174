<script>
	$(".site-category").bind('click', function(){
		var site_group =  $(this).parent(".site-group");
		var is_active = site_group.hasClass("active");
		$(".site-group").removeClass("active");
		if(is_active){
			site_group.removeClass("active");
		}
		else{
			site_group.addClass("active");
		}
	});
	
	$(".footer-item.tnc").bind('click', function(){
		showConfirmation(
			'服務條款與細則',
			function(){
				window.open('http://hk.le.com/assets//resource/pdf/tnc/tc/Terms_and_Conditions_HK.pdf', '_blank');
			},
			function(){
				window.open('http://hk.le.com/assets//resource/pdf/tnc/tc/Terms_and_Conditions_Macau.pdf', '_blank');
			}
		);
	});
	
	$(".footer-item.pp").bind('click', function(){
		showConfirmation(
			'私隱政策',
			function(){
				window.open('http://hk.le.com/assets//resource/pdf/privacy_policy/tc/Privacy_Policy_HK.pdf', '_blank');
			},
			function(){
				window.open('http://hk.le.com/assets//resource/pdf/privacy_policy/tc/Privacy_Policy_Macau.pdf', '_blank');
			}
		);
	});
	
	function showConfirmation(title, confirm, deny){
		$.jAlert({
			'type':'confirm',
			'closeOnClick': true,
			'closeBtn': false,
			'confirmQuestion': title,
			'confirmBtnText': '香港版本',
			'denyBtnText': '澳門版本',
			'showAnimation': 'fadeInDown',
			'hideAnimation': 'fadeOutUp',
			'onConfirm': function(){
				if(confirm){
					confirm();
				}
			},
			'onDeny': function(){
				if(deny){
					deny();
				}
			}
		});
	}
	
	//http://hk.le.com/assets//resource/pdf/return_policy/tc.pdf	//http://hk.le.com/assets//resource/pdf/privacy_policy/tc.pdf</script>
