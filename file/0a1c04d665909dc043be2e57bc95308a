<script type="text/javascript">
$(function(){
	$("#KinSlideshow").KinSlideshow();
});
	$(function(){
		var title=$(".nav-ul li");
		var content=$(".content");
		title.mouseover(function(){
			var dd=title.index(this);
			$(this).addClass("li-hover").siblings().removeClass("li-hover");
			content.hide();
			$(content.get(dd)).show();
			return false;
		}).eq(0).trigger("mouseover");
	});

    $(function () {
        var title2 = $(".tempul li");
        var content2 = $(".tempcontent");
        title2.mouseover(function () {
            var dd = title2.index(this);

if(dd==0 || dd=="0")
{
document.getElementById("templi0").className="li-nava";
document.getElementById("templi1").className="li-navb";
}
else
{
document.getElementById("templi0").className="li-navb";
document.getElementById("templi1").className="li-nava";
}

            content2.hide();
            $(content2.get(dd)).show();
            return false;
        }).eq(0).trigger("mouseover");
    });


$(function () {
        var title2 = $(".sub-ul li");
        var content2 = $(".sub-ulcontent");
        title2.mouseover(function () {
            var dd = title2.index(this);

if(dd==0 || dd=="0")
{
document.getElementById("sub-ulli1").className="sub-no";
document.getElementById("sub-ulli0").className="sub-hover";
}
else
{
document.getElementById("sub-ulli0").className="sub-no";
document.getElementById("sub-ulli1").className="sub-hover";
}

            content2.hide();
            $(content2.get(dd)).show();
            return false;
        }).eq(0).trigger("mouseover");
    });

</script>
