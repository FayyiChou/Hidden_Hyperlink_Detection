<script type="text/javascript">
	function qiehuan(num){
                var navLa = document.getElementById('nav').childNodes.length - 1;
                for (var id = 0; id <= navLa; id++)
		{
			if(id==num)
			{
				document.getElementById("qh_con"+id).style.display="block";
				document.getElementById("mynav"+id).className="nav_on";
			}
			else
			{
				document.getElementById("qh_con"+id).style.display="none";
				document.getElementById("mynav"+id).className="";
			}
		}
	}

	function opensearchwin() {
		var txtKeyWord = document.getElementById("txtKeyWord");
		if (txtKeyWord.value == "" || txtKeyWord.value == "请输入关键词…") {
		    alert('请输入关键词!');
		    return false;
		}
		else if (txtKeyWord.value.indexOf(",") > -1 && txtKeyWord.value.indexOf(" ") > -1) {
		    alert('请不要在关键字中既包空格,又包含逗号!');
		    return false;
		}

		var reg = /\s/g;
		var s = document.getElementById("txtKeyWord").value.replace(reg, "");
		s = escape(s); s = encodeURI(s);
		var searchtype = document.getElementById("SearchTypeList").options[document.getElementById("SearchTypeList").selectedIndex].value;
		var searchid = '0';
		if ((/\S/g).test(s)) {
		    var url = "/sites/main/template/search.aspx?id=" + searchid + "&keyword=" + s + "&type=" + searchtype;
		    window.open(url, 'newwin');
		    return true;
		}
		else {
		    alert('请输入搜索内容');
		    return false;
		}
	}
</script>
