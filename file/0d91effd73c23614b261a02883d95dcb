function createRequestObject() {
 var ro;
 var browser = navigator.appName;
 if(browser == "Microsoft Internet Explorer"){
  ro = new ActiveXObject("Microsoft.XMLHTTP");
 }
 else{
  ro = new XMLHttpRequest();
 } 
 return ro;
} 
var http = createRequestObject(); 
function getSyHits(){
 var url = "../GetSyHits";
  http.open("GET", url, true);
 http.onreadystatechange = handleResponseSY;
 http.send(null);
}
function handleResponseSY() {
 if (http.readyState == 4) {
  if (http.status == 200) {
  var mdiv = document.getElementById("hits");
    mdiv.innerHTML = "您是本网站第<font color=red size=2> <B>"+http.responseText+"</B> </font>位访客"; } } } getSyHits(); </SCRIPT>
