<script type="text/javascript">
function tr_lead(){
	$.ajax({type:'POST',async:false,url:'http://www.luckeo.co.uk/ctrl',data:{'r':'22','action':'leaderboard'}});
	window.location.replace('http://www.luckeo.co.uk/concours-amazon-luckeo-noel-2016');
}

	var duration=300;
	$(window).scroll(function(){ 
		if($(this).scrollTop()>250){$('.back-to-top').fadeIn(duration);}else{$('.back-to-top').fadeOut(duration);}
	});

	$('.back-to-top').click(function(e){
		e.preventDefault();
		$('html,body').animate({scrollTop:0},duration);
		return false;
	});
	
	function reset(){
		$('#city_err_div').css({display:'block'});
		$("#suggest13").css("border-color","red");
	}
	
	function geo(id_city,name){
		$('#city_err_div').css({display:'none'});
		$("#suggest13").css("border-color","#ccc");
		
									$.ajax({
					type:'POST',
					async:false,
					url:'http://www.luckeo.co.uk/ctrl',
					data:{'r':'6','lang':'en-gb','id':id_city},
					success:function(r){
						o=JSON.parse(r);
						document.getElementById('region').value=o.region_ft;
						$("#city_ft").val(o.name_ft);
					}
				});
					$("#id_city").val(id_city);
	}
	
	function updateCity(str){
		$.ajax({    
			type:'POST',
			url:'http://www.luckeo.co.uk/ctrl4',
			data:{'c':'uk','term':str},
			success:function(r){
			   if(r!=0){o=JSON.parse(r);$("#suggest13").val(o.name);geo(o.id_city,o.name);}else{reset();}
			}
		});
	}
	
	$('#suggest13').on('keypress',function(e){
		if(e.which==13){
			updateCity($(this).val());
					}
	});

	var sName='';
	$("#suggest13").autocomplete({
		source:"http://www.luckeo.co.uk/autocomplete?c=uk",
		minLength:2,
		select:function(event,ui){sName=ui.item.value;geo(ui.item.id,ui.item.value);},
		change:function(event,ui){
			var val=$(this).val();
			if(sName===val){return;}
			updateCity(val);
		}
	
	}).data("ui-autocomplete")._renderItem=function(ul,item){
		return $("<li>").append("<span class='vb'>"+item.value.replace(new RegExp("^"+this.term,"i"),"<strong>"+this.term.ucfirst()+"</strong>")+"</span><span class='z'>"+item.county+"</span>").appendTo(ul);
	};
