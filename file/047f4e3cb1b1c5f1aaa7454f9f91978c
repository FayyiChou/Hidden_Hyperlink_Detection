			function priceUpd(){
				
				var arr={'':[10,20,50,100,200,500,1000],'cars':[1000,2000,5000,10000,26000,40000],'motos':[1000,2000,3000,4000,5000,6000,10000],'motorhomes':[2000,4000,8000,12000,20000,30000,50000],'other-vehicles':[2000,3000,5000,6000,7000,10000,20000],'auto-parts':[100,200,500,700,1000,2000],'moto-parts':[100,200,500,700,1000,2000],'caravan-accessories':[100,200,500,700,1000,2000],'nautica':[1000,2000,5000,7000,10000,20000,25000,30000,50000,60000],'nautica-accessories':[100,200,500,700,1000,2000],'estate-sales':[30000,60000,80000,120000,140000,160000,200000,260000,400000],'for-rent':[200,300,400,500,600,700,800,900,1000,1200],'holiday-rentals':[200,300,400,500,600,700,800,900,1000,1200],'colocation':[200,300,400,500,600,700,800,900,1000,1200],'lessons':[10,15,20,25,30,40,50,75,100],'work-equipment':[100,200,500,700,1000,2000,5000,10000],'medical-equipment':[100,200,400,600,1000,1400],'computer':[50,100,200,300,400,500,1000,1500],'video-games-consoles':[25,50,100,200,300,400],'audio-video':[25,50,100,200,300,400],'photography':[25,50,100,200,300,400,600,1000],'telephony':[25,50,100,200,300,400,600],'furniture':[25,50,100,200,300,400],'appliances-household':[25,50,100,200,300,400,500],'tableware':[25,50,100,200,300,400,500],'art-antique':[25,50,100,200,300,400,500,1000,1500,2000],'decoration':[25,50,100,200,300,400],'bedding':[25,50,100,200,300,400],'luminaire':[25,50,100,200,300,400],'clothes':[5,10,15,20,25,40,50],'diy':[25,50,100,200,300,400],'garden':[25,50,100,200,300,400,1000],'shoes':[25,50,100,200,300,400],'accessories-luggage':[25,50,100,200,300,400],'jewelry-watches':[25,50,100,200,300,400],'baby-accessories':[25,50,100,200,300,400],'pets':[25,50,100,200,300,400],'tickets':[25,50,100,200,300,400],'music-movies':[25,50,100,200,300,400],'books-magazines':[5,10,25,50,100],'musical-instruments':[25,50,100,200,300,400],'sports':[25,50,100,200,300,400],'bikes':[25,50,100,200,300,400],'games-toys':[10,25,50,100,200,300],'gastronomy':[25,50,100,200,300,400],'wine':[25,50,100,