	function optMmr(email){
		var domain=email.replace(/.*@/,"").split('.')[0];a=["alice","aol","cegetel","clubinternet","free","gmx","icloud","neuf","sfr","tiscali","wanadoo","outlook"];		for(i=0;i<a.length;++i){
			if(domain.indexOf(a[i])>-1){
				$('#yahoo_w').css({display:'none'});
				for(k=0;k<a.length;k++){$('#'+a[k]+'_w').css({display:'none'});}
				$('#'+a[i]+'_w').css({display:'block'});break;
			}
		}
	}

	function invite(){
		var json='[';
		$(".pr").each(function(){
			is_consent=0;
			if($(this).hasClass("b2")){is_consent=1;}
			json=json+'{"id":"'+$(this).attr('id')+'","is_consent":"'+is_consent+'"},';
		});
		json = json.substring(0,json.length-1)+']';
		
		$.ajax({
			type:'POST',
			url:'http://www.luckeo.co.uk/ctrl',
			data:{'r':'14','json':json,'id_import':document.getElementById('storedData').innerHTML},
			success:function(){succ();}
		});
	}
	
	function succ(){
		window.onbeforeunload=null;closeModal('invite_modal');	}
	
		
	function makeFrame(domId,url) { 
		ifrm = document.createElement("IFRAME"); 
		ifrm.setAttribute("src", url);
		ifrm.setAttribute("id", domId);
		ifrm.setAttribute("style", "display:none;");         
		ifrm.style.width = 1+"px"; 
		ifrm.style.height = 1+"px"; 
		document.body.appendChild(ifrm); 
	} 
	
	function mmr(type){
		
		$('.fr2,.invite_status').css({display:'none'});
		$('#step2_1').css({display:'none'});
		
		var url='http://www.luckeo.co.uk/invite?type='+type+'&p=en-gb';
		if(type=='live' || type=='outlook'){url='https://login.live.com/oauth20_authorize.srf?response_type=code&client_id=000000004C169B1B&scope=wl.basic+wl.contacts_emails+wl.emails&redirect_uri=http%3A%2F%2Fwww.luckeo.co.uk%2Flive';}
		else if(type=='gmail'){url='https://accounts.google.com/o/oauth2/auth?response_type=code&client_id=256250691699-hte4kg7shkbgpng2bhplfeake18kej0p.apps.googleusercontent.com&scope=email+profile+https%3A%2F%2Fwww.googleapis.com%2Fauth%2Fcontacts.readonly&redirect_uri=http%3A%2F%2Fwww.luckeo.co.uk%2Fgmail';}
		else if(type=='yahoo'){url='https://api.login.yahoo.com/oauth2/request_auth?response_type=code&client_id=dj0yJmk9cGVhTTN6eEdTTU03JmQ9WVdrOWQyUnBjVmRDTm5FbWNHbzlNQS0tJnM9Y29uc3VtZXJzZWNyZXQmeD1iMQ--&redirect_uri=http%3A%2F%2Fwww.luckeo.co.uk%2Fyahoo';}
		
		if(type=='gmail'){makeFrame('googleLogoutIFrame','https://accounts.google.com/logout');}
		
		var win=window.open(url,'','resizable=no,width=440,height=600');
				
		if(type=='gmail'){$('.fr1').css({display:'none'});$('.fr0').css({display:'block'});}
		else{$('.fr1').css({display:'block'});}	
	
		var pollTimer = window.setInterval(function(){
			if(win.closed !== false){
				window.clearInterval(pollTimer);
				var limit=11;
				
				$.ajax({
					type:'POST',
					url:'http://www.luckeo.co.uk/ctrl3',
					data:{'id_import':document.getElementById('storedData').innerHTML,'limit':limit,'term':"Others"},
					success:function(data){
						if(data==1){
							$('.fr1,.invite_status,#step2_1').css({display:'block'});
							$('.fr2').css({display:'none'});
						}else{
							$('.fr0').css({display:'none'});
							
							$('.fr1,.invite_status').css({display:'none'});
							$('.fr2').css({display:'block'});
							$('.profiles_content').html(data);
															if(document.getElementById('email') != null){
									document.getElementById('what2do').innerHTML = "Votre annonce est presque en ligne...";
									document.getElementById('what2do_fsp').innerHTML = "Cliquez sur le bouton pour confirmer votre annonce.";
								}else{
									document.getElementById('what2do').innerHTML = "";
									document.getElementById('what2do_fsp').innerHTML = "";
								}
													}
					}
				});	
			}
		},100);
		$.ajax({type:'POST',url:'http://www.luckeo.co.uk/ctrl',data:{'r':'22','action':'import','type':type}});
	}
	
	
		$('#btn_inv_continue').click(function(){
		$.ajax({type:'POST',async:false,url:'http://www.luckeo.co.uk/ctrl',data:{'r':'22','action':'continue'}});
				$('#inv_step1,.invite_status').css({display:'none'});
		$('#inv_step2').css({display:'block'});
				return false;
	});
	$('#btn_inv_no2').click(function(){
		$.ajax({type:'POST',async:false,url:'http://www.luckeo.co.uk/ctrl',data:{'r':'22','action':'close'}});
		succ();
		return false;
	});
	



		$("#email_signin_header,#pwd_signin_header").on("keydown",function(e){if(e.which == 13){signin($("#email_signin_header").val(),$("#pwd_signin_header").val());}});
		$("#btn_signin_header").on("click",function(){signin($("#email_signin_header").val(),$("#pwd_signin_header").val());});
		
		function signin(email,pwd){
			var err=0;
			if(email!="" && pwd!=""){
				$.ajax({
					type:'POST',
					async:false,
					url:'http://www.luckeo.co.uk/ctrl',
					data:{'email':email,'pwd':pwd,'r':'15'},
					success:function(r){
						if(r!=0){
							o=JSON.parse(r);
															window.location.replace("http://www.luckeo.co.uk/myaccount");
													}else{err=1;}
					}
				});
			}else{err=1;}
			if(err==1){window.location.replace("http://www.luckeo.co.uk/signup?err_login=1");			}
		}
		
