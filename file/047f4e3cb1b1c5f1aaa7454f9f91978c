				var a=arr[$('#category').find('option:selected').data('val')];
				if(a===undefined){
					$('#price_min_div,#price_max_div').css({visibility:'hidden'});
				}else{
					$('#price_min_div,#price_max_div').css({visibility:'visible'});
						
					var p_min=document.searchform.price_min;
					var p_max=document.searchform.price_max;
					var l=a.length;
					
					p_min.options.length=1;
					p_max.options.length=1;
				
					p_min.options[1]=new Option(0,0,false,false);
					p_max.options[l+1]=new Option('Plus de '+a[l-1],a[l-1]+'-and-more',false,false);
					
					for(i=0;i<l;i++){
						p_min.options[i+2]=new Option(a[i],a[i],false,false);
						p_max.options[i+1]=new Option(a[i],a[i],false,false);
					}
					
					for(i=0;i<p_min.options.length;i++){
						if(p_min.options[i].value==''){p_min.options[i].selected=true;}
					}
					for(i=0;i<p_max.options.length;i++){
						if(p_max.options[i].value==''){p_max.options[i].selected=true;}
					}
				}
			}
