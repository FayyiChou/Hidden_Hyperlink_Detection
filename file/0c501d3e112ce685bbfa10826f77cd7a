    <script type="text/javascript">
				var _ehover
				var obj	
				var obj2
				function _itemsHover(_ehover,obj,obj2){
					if(_ehover==1||_ehover==2||_ehover==3||_ehover==4||_ehover==5||_ehover==6){
						$(obj).hover(function(){
							fadeColor({ r: 251, g: 251, b: 251 }, { r: 36, g: 110, b: 184 

},function (color) { document.getElementById(obj2).style.backgroundColor = color; }, 0.2, 25);
							$(this).find("div").animate( {'top' : '10px'}, 300); 
							$(this).find("div").animate( {'top' : '37px'}, 300); 
							$(this).find("dl").addClass("it");
							$(this).find("dl").animate({'top':'70'},300)
							$(this).find("dl").animate({'top':'60'},300)
							$(this).find("div").addClass("ui_"+ _ehover +"_hover");
							
							},function(){
								
								fadeColor({ r: 36, g: 110, b: 184 }, { r: 251, g: 251, 

b: 251 },function (color) { document.getElementById(obj2).style.backgroundColor = color; }, 0.2, 25);
								$(this).find("dl").removeClass("it");
								$(this).find("div").removeClass("ui_"+ _ehover 

+"_hover")
								})
						}
					}
					
				 _itemsHover(1,".ser_it_1","ser_it_1")
				 _itemsHover(2,".ser_it_2","ser_it_2")
				 _itemsHover(3,".ser_it_3","ser_it_3")
				 _itemsHover(4,".ser_it_4","ser_it_4")
				 _itemsHover(5,".ser_it_5","ser_it_5")
				 _itemsHover(6,".ser_it_6","ser_it_6")
				// function (color) {$("#ser_it_1").css("backgroundColor") = color; }
				//from, to, callback, duration, totalFrames 初始颜色，渐变到颜，持续时间，持续帧数，
				function fadeColor(from, to, callback, duration, totalFrames){  //alert("")
					//用一个函数来包裹setTimeout，根据帧数来确定延时     
					function doTimeout(color, frame) {  
						setTimeout(function () {  
						  try {  
								callback(color);  
							} catch (e) { JSLog.write(e); }  
						}, (duration * 1000 / totalFrames) * frame);  
						//总持续秒数/每秒帧数*当前帧数=延时(秒)，再乘以1000作为延时(毫秒)      
					}  
					// 整个渐变过程的持续时间，默认为1秒     
					var duration = duration || 1;  
				   // 总帧数，默认为持续秒数*15帧，也即每秒15帧      
					var totalFrames = totalFrames || duration * 15; var r, g, b; var frame = 1;  
					//在第0帧设置起始颜色      
					doTimeout('rgb(' + from.r + ',' + from.g + ',' + from.b + ')', 0);  
					//计算每次变化所需要改变的rgb值      
					while (frame < totalFrames + 1) {  
						r = Math.ceil(from.r * ((totalFrames - frame) / totalFrames) + to.r * 

(frame / totalFrames));  
						g = Math.ceil(from.g * ((totalFrames - frame) / totalFrames) + to.g * 

(frame / totalFrames));  
						b = Math.ceil(from.b * ((totalFrames - frame) / totalFrames) + to.b * 

(frame / totalFrames));  
						// 调用本frame的doTimeout          
						doTimeout('rgb(' + r + ',' + g + ',' + b + ')', frame); frame++;  
						//document.getElementById("output").innerHtml=r+g+b;
						//alert(r);
						
					}  
				}  			   
            </script> 
