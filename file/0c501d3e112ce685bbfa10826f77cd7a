  <script type="text/javascript">
	  // 导航菜单交互
	  $(document).ready(function () {	
		  // 鼠标悬停
		  $(".nav li").hover(
			  function () {
				  //鼠标悬停子菜单
				 // $("ul", this).fadeIn();
				 $("ul",this).stop().slideDown();
				 $("a",this).eq(0).addClass("on");
				 
			  }, 
			  // 鼠标移除
			  function () {
				  $("ul", this).stop().slideUp();
				  $("a",this).eq(0).removeClass("on");
			  }
		  );
	  });
	  </script> 
