		<script type="text/javascript">
			var imageUrlPath = 'http://htm4.jobui.com/';		//图片域名
			var uiNowSite = "site_index";		//统计标签
			//jQuery('.j-keywordLenovo').lenovoPrompt({top:45});	//关键词提示
		</script>
		<script type="text/javascript">
		/*头部导航小列表显示与隐藏 start*/
		$(".j-Hnav-title").hover(function() {
			$(this).children('.Hnav-wrap-small').show();
		},function () {
			$(this).children('.Hnav-wrap-small').hide();
		});	
		/*头部导航小列表显示与隐藏 end*/
		/*热门行业切换 start*/
		$(".j-switch-industry").mouseover(function() {
			var $this = $(this),
				isSelect = $this.children('span'),
				isDownPoint = $this.children('em'),
				dataType = $this.attr('data-type'),
				$allIndustry = $(".j-all-industry"),
				otherIndustry = $(".j-switch-industry").children('span'),
				otherDownPoint = $(".j-switch-industry").children('em');
			otherIndustry.removeClass('isSelected');
			otherDownPoint.remove();
			isSelect.addClass('isSelected');
			$this.append('<em>♦</em>');
			$allIndustry.addClass('dn');
			$('.j-industry-'+dataType).removeClass('dn');
		});
		/*热门行业切换 end*/
		/*更多行业筛选 start*/
		$("#myTab a").click(function(e){
		    e.preventDefault();
		    $(this).tab();
		});
		jQuery.fn.tab=function(){
			if(this.parent('li').hasClass('selected')) return;
			var $ul=this.parents('ul');
			var $lis=$ul.find('li');
			var $tabs=$ul.find('[data-toggle="tab"]');
			var idArray=new Array();
			$tabs.each(function(){
				idArray.push(jQuery(this).attr('href').match(/#[\w-]+/)[0]);
			});
			$lis.removeClass('selected');
			this.parents('li').addClass('selected');
			for(var key in idArray){
				if(this.attr('href').match(/#[\w-]+/)[0]==idArray[key]){
					jQuery(idArray[key]).show();
					continue;
				}
				jQuery(idArray[key]).hide();
			}
		}
		$(document).ready(function() { 
			$(".j-switch-box").click(function(e){
		    	e.stopPropagation();
			});
			$(document).click(function(e){
				$(".j-switch-box").hide();
				e.stopPropagation();
			});
		    //更多地区冒泡
			$(".j-more-industry").click(function(e) {
				$(".j-switch-box").toggle();
				e.stopPropagation();
			});
		});
		/*更多行业筛选 end*/
		</script>
	</div>
	 
