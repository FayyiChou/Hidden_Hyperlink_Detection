    <script src="http://static2.qy.zhantai.com/scripts/jquery.min.js"></script>
    <script src="http://static2.qy.zhantai.com/scripts/distpicker.js"></script>
    <script src="http://static2.qy.zhantai.com/scripts/uri.min.js"></script>

    <script>

        $(document).ready(function () {

            if ($('.pkg:checked').attr('data')) {
                $('#amount').html('￥' + $('#total').val() * $('.pkg:checked').attr('data'));
            }

            var uri = URI.parse(window.location.href);
            var query = URI.parseQuery(uri.query);

            
            $('.pkg').each(function () {
                $(this).on('click', function () {
                    $('#unit-price').val($('.pkg:checked').attr('data'));
                    $('#amount').html('￥' + $('#total').val() * $('.pkg:checked').attr('data'));
                })
            })

            $('#submit').on('click', function () {

                if (!checkForm()) {
                    return false;
                }

                $('#submit').hide();
                $('#submit-message').show();
                $('#submit-message span').html('正在提交中....');

                var product = $('.pkg:checked').val();

                
                
                
                
                
                
                
                

                var url = uri.hostname;

                if (uri.path) {
                    url = url + uri.path;
                }

                if (uri.query) {
                    url = url + '?' + uri.query;
                }

                var o = {
                    'OrderCreateForm[product]': product,
                    'OrderCreateForm[total]': $('#total').val(),
                    'OrderCreateForm[contact]': $('#name').val(),
                    'OrderCreateForm[phone]': $('#mobile').val(),
                    'OrderCreateForm[province]': $('#province').val(),
                    'OrderCreateForm[city]': $('#city').val(),
                    'OrderCreateForm[county]': $('#county').val(),
                    'OrderCreateForm[address]': $('#address').val(),
                    'OrderCreateForm[remarks]': $('#remarks').val(),
                    'OrderCreateForm[page_id]': $('#page-id').val(),
                    'OrderCreateForm[unit_price]': $('#unit-price').val(),
                    'OrderCreateForm[product_catalog]': $('#product-catalog').val(),
                    'OrderCreateForm[customer_id]': $('#customer-id').val(),
                    'OrderCreateForm[referer]': document.referrer || '',
                    'OrderCreateForm[url]': url,
                    'OrderCreateForm[ad_plan]': query.gzid,
                }

                $.post('/submit', o, function (result) {
                    if (!result.code) {
                        $('#submit').hide();
                        $('#submit-message span').html('恭喜您，订单已经提交成功！');
                        $('.submit-success').show();
                        $('.submit-success').html('<ul>' +
                            '<li>订单号 : ' + result.sn + '</li>' +
                            '<li>产品 : ' + product + '</li>' +
                            '<li>姓名 : ' + $('#name').val() + '</li>' +
                            '<li>电话 : ' + $('#mobile').val() + '</li>' +
                            '<li>地址 : ' + $('#province').val() + $('#city').val() + $('#county').val() + $('#address').val() + '</li>' +
                            '<li>付款方式 : 货到付款</li>' +
                            '</ul>');
                    } else {
                        alert('非常抱歉，订单提交失败！请联系客服')
                        $('#submit').show();
                        $('#submit-message').hide();
                    }
                })
            })

            $('#subtract').on('click', function () {
                var total = $('#total').val();
                if (total <= 1) {
                    $('#total').val(1)
                } else {
                    $('#total').val(parseInt(total) - 1);
                }
                $('#amount').html('￥' + $('#total').val() * $('.pkg:checked').attr('data'));
            })

            $('#plus').on('click', function () {
                var total = parseInt($('#total').val());
                if (total >= 99) {
                    $('#total').val(99);
                } else {
                    $('#total').val(total + 1);
                }
                $('#amount').html('￥' + $('#total').val() * $('.pkg:checked').attr('data'));
            })

            $('#total').on('keydown blur', function () {
                if (!$('#total').val()) {
                    $('#total').val(1);
                }

                if ($('#total').val() >= 99) {
                    $('#total').val(99);
                }
                $('#total').val(parseInt($('#total').val()));
                $('#amount').html('￥' + $('#total').val() * $('.pkg:checked').attr('data'));
            })
        })

        $(function () {

            function scollDown(id, time) {
                var liHeight = $("#" + id + " ul li").height();
                var time = time || 2500;
                setInterval(function () {
                    $("#" + id + " ul").prepend($("#" + id + " ul li:last").css("height", "0px").animate({
                        height: liHeight + "px"
                    }, "slow"));
                }, time);
            }

            scollDown("pingjia", 3000);
            scollDown("fahuo", 3000);

        });

        function checkForm () {

            var pkg = false;
            $('.pkg').each(function () {
                if ($(this).prop('checked')) {
                    pkg = true;
                    return false;
                }
            })

            if (!pkg) {
                alert('请选择要购买的产品');
                return false;
            }

            
            
            
            
            
            
            
            
            if (!$('#name').val()) {
                alert('请填写姓名');
                return false;
            }

            var namePattern = "^[\u4e00-\u9fa5]{2,6}$";

            if (!RegExp(namePattern).test($('#name').val())) {
                alert('您输入的姓名格式不正确');
                return false;
            }

            if (!$('#mobile').val()) {
                alert('请填写手机号码');
                return false;
            }

            var mobilePattern = "^0?(13|15|17|18|14)[0-9]{9}$";

            if (!RegExp(mobilePattern).test($('#mobile').val())) {
                alert('您输入的手机号码格式不正确');
                return false;
            }

            if (!$('#province').val()) {
                alert('请选择省');
                return false;
            }

            if (!$('#city').val()) {
                alert('请选择市');
                return false;
            }

            if (!$('#address').val()) {
                alert('请填写地址');
                return false;
            }

            return true;
        }

        var $__G_Time = {};
        var $__ms_Count = {};
        var $__G_Timer_Count = 0;
        var $__ms_use = true;

        if (typeof(__Timer_lesser_auto_accuracy) == 'undefined') {
            __Timer_lesser_auto_accuracy = false;
        }

        $(document).ready(function () {
            addTimeLesser(1,125421)
            if (__Timer_lesser_auto_accuracy && $__G_Timer_Count >= __Timer_lesser_worker_max) {
                $__ms_use = false;
            }
            for (id in $__G_Time) {
                showtime(id, $__G_Time[id]-1);
            }
        });

        function addTimeLesser(id, time) {
            $__G_Time['remainTime_'+id] = time;
            $__G_Timer_Count ++;
        }

        function showtime(id, time, msid) {
            var msC = $__ms_Count[id];
            if (msC == undefined) msC = 0;
            if ($__ms_use && msC > 0 && msid != '') {
                $('#'+msid).text('.'+msC);
                msC --;
                $__ms_Count[id] = msC;
                setTimeout(function(){showtime(id, time, msid)}, 100);
                return;
            }
            $__ms_Count[id] = 9;
            if (time <= 0) {
                $('#' + id).html('<span>活动已经结束</span>');
                return;
            }
            var timeUnits = {
                'day': { 'name': '天', 'count': 86400 },
                'hour': { 'name': '小时', 'count': 3600 },
                'minute': { 'name': '分', 'count': 60 },
                'second': { 'name': '秒', 'count': 1 }
            };
            var string = '';
            var iLess = time;
            for (ix in timeUnits) {
                var unit = timeUnits[ix];
                var cc = Math.floor(iLess / unit.count);
                var ccString = cc < 10 ? '0'+cc.toString() : cc.toString();
                string += '<span >' + ccString + '</span>' + unit.name;
                iLess -= cc * unit.count;
            }
            $('#' + id).html(string);
            setTimeout(function(){showtime(id, time - 1, msid)}, $__ms_use ? 100 : 1000);
        }
    </script>


</body></html>