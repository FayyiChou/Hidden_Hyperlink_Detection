  <script>
 function  showmorematerail(){
	 if( jQuery('#morematerialflag').val()==1){
	  jQuery('.materialmore').show();
	  jQuery('#morematerialflag').val(2);
		jQuery('#morematerialimg').attr('src',TTOOLS_PIC_SERVER_URL+'cms/jpk/images/up_660.gif');

	 }else{
		  jQuery('.materialmore').hide();
		  jQuery('#morematerialflag').val(1);
			jQuery('#morematerialimg').attr('src',TTOOLS_PIC_SERVER_URL+'cms/jpk/images/down_660.gif');

	 }
  }
  </script>
