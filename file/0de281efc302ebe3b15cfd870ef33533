<script>
var timeObj=null;
function showSectionNode(obj,courseId,id){
	
	jQuery('.flo_list').height(225);
	clearTimeout(timeObj);
	if(myMap.get(id+'')!=null&&myMap.get(id+'')!=''){
		     jQuery('.flo_list').html(myMap.get(id+''));
		    jQuery(".flo_list").css("left",jQuery(obj).offset().left+100);
		    jQuery(".flo_list").css("top",jQuery(obj).offset().top+jQuery(obj).outerHeight()+10);
		    jQuery(".flo_list").show('normal');
	}else{
	jQuery.ajax({
		type : "POST",
		url : "/jpk/getSectionNode.action?courseId="+courseId+"&characId="+id,
		data : null,
		success : function(data) {
		fillinthemap(id,data);
	    jQuery('.flo_list').html(data);
	    jQuery(".flo_list").css("left",jQuery(obj).offset().left+100);
	    jQuery(".flo_list").css("top",jQuery(obj).offset().top+jQuery(obj).outerHeight()+10);
	    jQuery(".flo_list").show('normal');
		},
		dataType : "html",
	});
	}
}

function mouseOutCharacter(){
	timeObj=setTimeout('sectionDivHide()',2000) ;
}
function mouseOutSection(){
	timeObj=setTimeout('sectionDivHide()',1000) ;
}
function mouseOverSection(){
	clearTimeout(timeObj);
}
function sectionDivHide(){
	 clearTimeout(timeObj);
	  jQuery(".flo_list").hide('normal');
	  
}

function showmoreknowledge(){
	if(jQuery('#moreknowledgeswitch_val').val()==1){
	jQuery('#courseKnowledgeDiv').css('overflow','visible');
	jQuery('#moreknowledgeswitch_val').val(2);
	jQuery('#moreknowledgeswitch').html('收起<<');
	}
	else{
		jQuery('#courseKnowledgeDiv').css('overflow','hidden');
		jQuery('#moreknowledgeswitch_val').val(1);
		jQuery('#moreknowledgeswitch').html('更多>>');
	}
}


function showmoreskilltype(){
	if(jQuery('#moreskilltypeswitch_val').val()==1){
	jQuery('#courseSkillTypeDiv').css('overflow','visible');
	jQuery('#moreskilltypeswitch_val').val(2);
	jQuery('#moreskilltypeswitch').html('收起<<');
	}
	else{
		jQuery('#courseSkillTypeDiv').css('overflow','hidden');
		jQuery('#moreskilltypeswitch_val').val(1);
		jQuery('#moreskilltypeswitch').html('更多>>');
	}
}
function moreCharacter(){
	if(jQuery('#morecharacterwitch_val').val()==1){
	jQuery('.morecharacters').show();
	jQuery('#morecharacterwitch_val').val(2);
	jQuery('#downbar').attr('src',TTOOLS_PIC_SERVER_URL+'cms/jpk/images/down_16up.png');
	}
	else{
		jQuery('.morecharacters').hide();
		jQuery('#morecharacterwitch_val').val(1);
		jQuery('#downbar').attr('src',TTOOLS_PIC_SERVER_URL+'cms/jpk/images/down_16.png');
	}
}




var myMap = new classMap();
function fillinthemap(id, str){
	myMap.put(id+'',str);
}
function delfromthemap(id){
	myMap.remove(id);
}	
function clearthemap(){
	myMap = new classMap();
}

</script>

	<script>
	/**
	 * 详细页面的头部加载
	 */
	function loadSubTop2(){
		var shequUrl ='http://www.icourses.cn/';
		$.post(shequUrl+"ajaxCmsHead.action",{flag:'headmini'},function(data){
			$("#importhead_div").html(data);
	    },"html");
	}
	loadSubTop2();

	jQuery(".mask ul img").error(function(){
		   jQuery(this).attr('src','http://static1.icourses.cn/' + 'sns/images/head/user/50');
	});
	jQuery(".div4 img").error(function(){
			   jQuery(this).attr('src','http://static1.icourses.cn/'  + '/cms/jpk/images/default/bookcover.jpg');
	
		});
	jQuery(".div2 img").error(function(){
		   jQuery(this).attr('src','http://static1.icourses.cn/'  + '/cms/jpk/images/default/120.jpg');

	});
	</script>
	
