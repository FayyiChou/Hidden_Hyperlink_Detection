<script type="text/javascript">
$(function() {
    newtab("#tab1");
    newtab("#tab2");
    newtab("#tab3");
    newtab("#tab4");
    newtab("#tab5");
    newtab("#tab6");
    function newtab(id) {
        $(id + " .tab_head li").each(function(j) {
											  
                if ($(this).hasClass("hover")) {
                    $(this).find("img").attr("src",$(this).find("img").attr("srca"));
                }
            $(this).mouseover(function() {
				$(id + " .tab_head li img").each(function(){
								$(this).attr("src",$(this).attr("srcn"))	;															  
															  
					})
                    $(this).find("img").attr("src",$(this).find("img").attr("srca"));
					$(id + " .tab_body .tab_list").removeClass("tabnow");
                    $(id + " .tab_body .tab_list").eq(j).addClass("tabnow")
            })
        })
    }
})
