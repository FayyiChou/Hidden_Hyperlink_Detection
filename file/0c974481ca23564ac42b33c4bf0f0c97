    <script type="text/javascript"> 
setInterval(function() {
    with(new Date) {
        var sj1 = (getYear() > 1900 ? getYear() : getYear() + 1900) + "年" + (getMonth() + 1) + "月" + getDate() + "日";
        var sj2 = "星期" + "日一二三四五六".charAt(getDay());
        var sj3 = (getHours() < 10 ? "0" + getHours() : getHours());
        var sj4 = (getMinutes() < 10 ? "0" + getMinutes() : getMinutes());
        var sj5 = (getSeconds() < 10 ? "0" + getSeconds() : getSeconds())
    }
    document.getElementById("t_date").innerHTML = sj1;
    document.getElementById("t_week").innerHTML = sj2;
    document.getElementById("t_hour").innerHTML = sj3;
    document.getElementById("t_minutes").innerHTML = sj4;
    document.getElementById("t_second").innerHTML = sj5
},
1000);
</script>
