<script>
    (function (global, undefined) {
        var oUserInfo = oCookie.fGet('_user', true);
        if (!oUserInfo) return;
        oUserInfo = JSON.parse(oUserInfo);
        var nick = oUserInfo['nick'] || '';
        $("#email").val(nick);
    })(window);
    var LoninObj = {
        bef: function () {
            $email = $("input[name='uName']");
            var email = $email.val();
            var isEmail = email.indexOf('@') >= 0;
            $pass = $("input[name='pwd']");
            var pass = $pass.val();
            $vCode = $("input[name='vCode']");
            var vCode = $vCode.val();
            if (email == "" || email == $email.attr('placeholder')) {
                this.showTips("请输入邮箱/手机号");
                $email.focus();
                return false;
            }
            if (isEmail && !/^[\w\.]+([-]\w+)*@[A-Za-z0-9-_]+[\.][A-Za-z0-9-_]/.test(email)) {
                isEmail = false;
                this.showTips("邮箱/手机号格式不正确");
                $email.focus();
                return false;
            }
            //add
            if (!isEmail && !/^1[3|4|5|7|8][0-9]\d{8}$/.test(email)) {
                this.showTips("邮箱/手机号格式不正确");
                $email.focus();
                return false;
            }
            if (pass == "" || pass == $pass.attr('placeholder')) {
                this.showTips("请输入密码");
                $pass.focus();
                return false;
            }
            if (!/^.{6,20}$/.test(pass)) {
                this.showTips("密码长度要求6-20位");
                $pass.focus();
                return false;
            }
            if (vCode == "" || vCode == $vCode.attr('placeholder')) {
                this.showTips("请输入验证码");
                $vCode.focus();
                return false;
            }
            $("#loginBtn").val("登录中...").attr("disabled", true);
            return true;
        },
        success: function (msg) {
            $("#csstishi").removeClass("tishi").addClass("tishisuccess");
            //$("#tishi").html("登录成功，正在为您跳转....");
            window.top.location.href = msg.Data;
        },
        error: function (msg) {
            $("#loginBtn").val("登录").attr("disabled", false);
            this.showTips(msg.Msg);
            //$("#tishi").html(msg.Msg);
            if (msg.code == 1) {
                $("#login_img").attr("src", "/RL/ChangeVcode?type=login&t=" + new Date().valueOf())
            }
        },
        showTips: function (msg) {
            $("#tips").show();
            $("#tipsmsg").html(msg);
        },
        hideTips: function () {
            $("#tips").hide();
        }
    };
    $("#login_img").click(function () {
        $(this).attr("src", "/RL/ChangeVcode?type=login&t=" + new Date().valueOf());
    });
    $("#login_img_refresh").click(function () {
        $("#login_img").attr("src", "/RL/ChangeVcode?type=login&t=" + new Date().valueOf());
    })
    $("#login_form").ajaxForm({
        beforeSubmit: function () {
            return LoninObj.bef();
        },
        success: function (r) {
            if (r.code == 200)
                LoninObj.success(r);
            else
                LoninObj.error(r);
        }
    });
</script>

