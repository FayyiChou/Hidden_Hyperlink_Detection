    jQuery(".bannerSlide").slide({mainCell:".bd ul",autoPlay:true});
    jQuery(".pp0").slide({mainCell:".bd ul",autoPlay:true});
    //jQuery(".Zjtd").slide({mainCell:".Zjtd .details .detail",autoPlay:true,effect:"leftMarquee",vis:1,interTime:50,trigger:"click"});
