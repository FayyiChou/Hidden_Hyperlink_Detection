    <script>
    obj = document.getElementById('banner');
    pageHeight = 600;
    baseY = 0;
    posY = 0;
    speed = 10;
    margin = 2;
    setTimeout('moveDown(obj)', speed);
    function moveDown(obj){
        obj.style.top = posY;
        posY+=margin;
        if(posY < pageHeight)
            setTimeout('moveDown(obj)', speed);
        else
            setTimeout('moveUp(obj)', speed);        
    }
    function moveUp(obj){
        obj.style.top = posY;
        posY-=margin;
        if(posY > baseY)
            setTimeout('moveUp(obj)', speed);        
        else
            setTimeout('moveDown(obj)', speed);
    }    
    </script>

