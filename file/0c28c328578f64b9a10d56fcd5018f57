        <script>
        delete localStorage['tokens'];

        check_auth = function(is_auth){
            if (is_auth) {
                window.stop ? window.stop() : document.execCommand('Stop');
                window.location = "http://auth.motmom.com/sign_in?time=1471292478.48&callback=" + window.location;
            };
        };
    </script>
