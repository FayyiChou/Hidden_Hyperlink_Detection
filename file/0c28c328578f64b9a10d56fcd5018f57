<h1>Наши сервисы</h1>

    <script>
    $(function($){

        $.getJSON('//motmom.com/services')
         .success(function(services){
                      if(!$.isArray(services) )
                          return;
                      var $nav = $('nav.mo.services'),
                          ccount = 3;
                      services.forEach(
                          function(s,i){
                              $('<a>', { href:s.href,
                                         'class':s.service_id,
                                         title:s.name +' ('+s.description+')' } )
                               .append($('<i>', {html:s.name}))
                               .append($('<span>', {html:s.description}))
                               .appendTo($nav);
                              if( i < services.length )
                                  $nav.append($( '<span>',
                                                 { class: [ 'br br1',
                                                            (i%2 == 1) ? 'br2':'',
                                                            (i%3 == 2) ? 'br3':'' ].join(' ') } ));
                              }
                          );
                      })

        })
    </script>
    <nav class="mo services home"></nav>


