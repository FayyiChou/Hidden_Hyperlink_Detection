function isChromeBrowser(){
	var Sys={};
	var ua=navigator.userAgent.toLowerCase();
	var s;
	(s=ua.match(/chrome\/([\d.]+)/))?Sys.chrome=s[1]:0;

	if(Sys.chrome){
		return true;
	}else {
		return false;
	}
}

