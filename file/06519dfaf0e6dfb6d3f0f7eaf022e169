     var mysize = $(".index_news_deta").size();
     var mypage = 0;
     if (mysize % 4 == 0) {
         mypage = mysize / 4;
     } else {
         mypage = parseInt(mysize / 4) + 1;
     }

     for (var i = 1; i < mypage; i++) {
         $(".index_news ul").append("<li></li>");
         //find找到   slice()区间第5个到第8个 
         $(".index_news ul li").eq(0).find(".index_news_deta").slice(4, 8).prependTo($(".index_news ul li:eq(" + i + ")"));
     }

