        <script>

					$(function(){

						$(".item7").hover(

							function(){

								var that=this;	

								item7Timer=setTimeout(function(){

									$(that).find("div").show();

									$(that).find("h2").fadeOut(1);

									$(that).find("dl").fadeIn(1);

								},100);

							},

							function(){

								var that=this;	

								clearTimeout(item7Timer);			

								$(that).find("dl").fadeOut(1);

								$(that).find("div").hide();

								$(that).find("h2").fadeIn(1);

							}

						)	

