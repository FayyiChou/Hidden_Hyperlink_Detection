jq(this).attr("src", img_s);
})
 jq(".tcdPageCode").createPage({
pageCount:231,
current:index,
backFn:function(p){
console.log(p);
}
});
} 

 },
 error: function (jxhr, msg, err) {
//		alert(err);
 }
 });

}	
</script>

