var speedbbb=30 
demobbb2.innerHTML=demobbb1.innerHTML 
function Marqueebbb(){ 
if(demobbb2.offsetWidth-demobbb.scrollLeft<=0) 
demobbb.scrollLeft-=demobbb1.offsetWidth 
else{ 
demobbb.scrollLeft++ 
} 
