<script type="text/html" id="pager-template">
<% var max = curr + 2;
   max < 5 ? max = 5 : 0;
   max > pages ? max = pages : 0;
   var min = max - 5;
   min < 1 ? min = 1 : 0;
%><%
  if(pages > 1){
%><div class="clearfix text-center ib mt10 mb10">
    <%if(curr > 1){%>
    <div class="pull-left ml5 mr5 pager-unit" value="1" pager-unit>|&lt;</div>
    <div class="pull-left ml5 mr5 pager-unit" value="<%= curr-1 %>" pager-unit>&lt;</div>
    <%}%>
    <%for(var i=min; i<=max; i++){%>
    <div class="pull-left ml5 mr5 pager-unit <%if(curr == i){%>active<%}%>" value="<%= i %>" pager-unit><%= i %></div>
    <%}%>
    <%if(curr < pages){%>
    <div class="pull-left ml5 mr5 pager-unit" value="<%= curr+1 %>" pager-unit>&gt;</div>
    <div class="pull-left ml5 mr5 pager-unit" value="<%= pages %>" pager-unit>&gt;|</div>
    <%}%>
</div>
<%}%>
</script>

