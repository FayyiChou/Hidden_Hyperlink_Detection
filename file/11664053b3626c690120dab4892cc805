<script type="text/javascript">
    $(function(){
        $(".classList").hover(function(){
            $(this).addClass("hoverList");
        },function(){
            $(this).removeClass("hoverList");
        });

        $(".classList li").click(function(){
            if ($(this).hasClass('changeText')){
                return false;
            }
            var text = '';
            var type = $(this).find('a').attr('type');
            switch (type)
            {
                case '1':
                    text = '请输入商家关键词';
                    break;
                case '2':
                    text = '请输入商品关键词';
                    break;
                default:
                    type = '1';
                    text = '请输入商家关键词';
                    break;
            }
            $('.changeText').text($(this).text());
            $('form input[name=key]').attr('placeholder', text);
            $('form input[name=type]').val(type);
        })
    })
</script>
