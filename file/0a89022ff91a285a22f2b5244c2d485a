<script type="text/javascript">
jQuery(function () {
	var key_to_encode = new RSAKeyPair("010001", "", "978C0A92D2173439707498F0944AA476B1B62595877DD6FA87F6E2AC6DCB3D0BF0B82857439C99B5091192BC134889DFF60C562EC54EFBA4FF2F9D55ADBCCEA4A2FBA80CB398ED501280A007C83AF30C3D1A142D6133C63012B90AB26AC60C898FB66EDC3192C3EC4FF66925A64003B72496099F4F09A9FB72A2CF9E4D770C41");
         
	jQuery("#btnLogin").click(function () {
	     //获取token
  
  var tokenCode = "";
	document.cookie = "token=" + tokenCode;    
	var data = "";
    var uid =jQuery("#strusername").val();
    var password = jQuery("#strpassword").val();          
    var service = jQuery("#Service").val();
    var ip="";
    var cors = new XMLHttpRequest();
	if ('withCredentials' in cors) {
		jQuery.ajax({
			type: "get",
			url:"https://passport.fang.com/login.api",                            
			data: { "Uid": escape(uid), "Pwd":escape(encryptedString(key_to_encode,password)), "Service": escape(service), "Ip": escape(ip)},
			xhrFields: {withCredentials: true},
			crossDomain: true,
			success: function (json) {
				
				var mm =json.Message;
				if(mm.indexOf("Err")>-1){alert(json.Tip);
				return;}
				
				
                      	document.cookie =  "new_loginname=" + json.UserName;
                      	document.cookie =  "new_loginid=" + json.UserID;
                      	window.location.reload();
			}
		});
	}else if (typeof XDomainRequest != "undefined") {
		jQuery.ajax({
			type: "get",
			url: "https://passport.fang.com/login.api",                            
			data: { "Uid": escape(uid), "Pwd": escape(encryptedString(key_to_encode,password)), "Service": escape(service), "Ip": escape(ip) },
			xhrFields: { withCredentials: true},
			crossDomain: true,
			success: function (json) {

				var mm =json.Message;
				if(mm.indexOf("Err")>-1){alert(json.Tip);
				return;}
				
				
				    document.cookie = "new_loginname=" + json.UserName;
				    document.cookie =  "new_loginid=" + json.UserID;
				    window.location.reload();
			}
	});
	} else {
		jQuery.ajax({
			type: "get",
			url:  "https://passport.fang.com/login.api",                              
			dataType: "jsonp",
			jsonp: "callback",
			data: { "Uid": escape(uid), "Pwd": escape(encryptedString(key_to_encode,password)), "Service": escape(service), "Ip": escape(IP) },
			xhrFields: {	withCredentials: true				                        },
			crossDomain: true,
			success: function (json) {

				var mm =json.Message;
				if(mm.indexOf("Err")>-1){alert(json.Tip);
				return;}
				   document.cookie =  "new_loginname=" + json.UserName;
				   document.cookie =  "new_loginid=" + json.UserID;
				   window.location.reload();
				// alert(json.UserName + '---' +  json.UserID + '---' + json.Message + '---' + json.Tip + '---' + json.UrlReferrer + '---' +  json.Ip);
			}
		});
	}
});
})
