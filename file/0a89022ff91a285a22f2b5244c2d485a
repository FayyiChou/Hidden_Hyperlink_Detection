<script type="text/javascript" src="http://js.soufunimg.com/homepage/new/fang905bj/new/js/gbk.js?v=111404"></script>
<script type="text/javascript">
_ub.city = '枣庄';//必写，中文
_ub.request('vwg.business,vwn.position,vwn.subway,vwn.belongschool,vwn.avgprice,vwn.fixstatus,vwn.feature', function(){ //（直接把需要的往后添就行）
        _ub.load(2); //选取时间最近的值    
        //以下添加业务逻辑
        var urllocation ='/ajax/transaction.do';
        if(location.hostname.indexOf('www.fang.com')==0){
        	urllocation = 'http://www.fang.com/news/ajax/transaction.do?flag=1&city='+encodeURIComponent(_ub.city);
        }
        var business=_ub['vwg.business'];
        if(business=='E'){//二手房
        	$("#esftrade").css('display','block');
        }else if(business=='H'){//家居
        	$("#jiajutrade").css('display','block');
        }else{//其他业务均返回新房
        	var position =_ub['vwn.position'];//区域
        	var subway =_ub['vwn.subway'];//地铁
        	var school =_ub['vwn.belongschool'];//学校
        	var prelprice =_ub['vwn.avgprice'];//平均价格
        	var fixstatus =_ub['vwn.fixstatus'];//装修
        	var feature =_ub['vwn.feature'];//热门
        	$.ajax({
    			url:urllocation,
    			type:'post',
    	        dataType:'html',
    	        data:{
    	        	"flag":"1",//详情页
    	        	"city":_ub.city,
    				"subway":subway,
    				"position":position,
    	        	'school':school,
    	        	'totalprice':prelprice,
    	        	'fixstatus':fixstatus,
    	        	'feature':feature,
    	        	'newsId':'24824235',
    	        	
    	        },
    	        success:function(data){
    	        	$("#transactionId").empty();
    	        	$("#transactionId").append(data);
    	        	$("#xftrade").css('display','block');
    	        }
    		}); 
        }
        console.log("==============="+_ub['vwg.business']); // 使用_ub['编号']的形式来获取，如： _ub['vwn.avgprice']
        

}); 
</script>
