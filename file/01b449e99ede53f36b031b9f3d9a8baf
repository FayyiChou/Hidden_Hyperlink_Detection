  <script>
function btnCalc_onClick()
{	
	if (!check_form()) return false;
	$('#act').val('calc');

	var data=$("#calcForm").serialize();
	$.getJSON('/gongjijin.php?',data, function(json){
		if (json.msg)
		{
			$('#lblMsg_Loan').html(json.msg);
			return false;
		}
		
		if (json.data)
		{
			var arr=json.data.split('|');
			for (i=0;i<arr.length;i++)
			{
				$('#intput_'+i).val(arr[i]);
			}
		}

		return true;
	});
}

function btnReset_onClick()
{	
	$('#loan').val('');
	$('#years').val(20);
	$('#months').val('240');
	$('#rate').val('3.25');

	for (i=0;i<=10;i++)
	{
		$('#intput_'+i).val('');
	}
}

function years_onChange(obj)
{
	var years=$(obj).val();
	$('#months').val(years*12);
}

function report_benxi()
{	
	if (!check_form()) return false;
	$('#act').val('benxi');
	$("#calcForm").submit();
}

function report_benjin()
{	
	if (!check_form()) return false;
	$('#act').val('benjin');
	$("#calcForm").submit();
}

function check_form()
{
	$('#lblMsg_Loan').html('');
	var loan=$('#loan').val();
	if (''==loan)
	{
		$('#loan').focus();
		$('#lblMsg_Loan').html('请输入正确的贷款金额');
		return false;
	}

	return true;
}
</script> 
