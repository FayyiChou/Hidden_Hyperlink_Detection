	var rootUrl  = "http://fitness.39.net/tzgj/";
	var fb = (fatnum)*9;
	var change_token = "0";	
	var pb = (proteinnum)*4;
	var cb = (carbnum-dbnum)*4;
	var ffb = parseInt(fb*1000/(fb+pb+cb));//脂肪百分比
	var ppb = parseInt(pb*1000/(fb+pb+cb));//蛋白质百分比
	var ccb = 1000- ffb - ppb;
	var sport_equel = '["228","480","600","280"]';
	var uweight = '60';	
	$(document).ready(function() {
			if(hadStore){
					$("#food_show_store").click(function(){
								fit_tool.cancelStore(foodId,'food','food_show_store');
					}).text("取消收藏")
			}else{
					$("#food_show_store").click(function(){
							fit_tool.addStore(foodId,'food','food_show_store');
					}).text("收藏我");
			}			
			var ffb = parseInt(fb*1000/(fb+pb+cb));//脂肪百分比
			var ppb = parseInt(pb*1000/(fb+pb+cb));//蛋白质百分比
			var ccb = 1000- ffb - ppb;	
			chart = new Highcharts.Chart({
					chart: {
							renderTo: 'container',
							plotBackgroundColor: null,
							plotBorderWidth: null,
							plotShadow: false
					},
					 credits: {
			            text: '',
			            href: ''
			        },
					title: {
							text: ''
					},
					tooltip: {
							formatter: function() {
									return '<b>'+ this.point.name +'</b>: '+ this.percentage.toString().substr(0,5) +' %';
							}
					},
					plotOptions: {
							pie: {
									allowPointSelect: true,
									cursor: 'pointer',
									dataLabels: {
											enabled: false,
											color: '#000000',
											connectorColor: '#000000',
											formatter: function() {
													return '<b>'+ this.point.name +'</b>: '+ this.percentage.toString().substr(0,5) +' %';
											}
									}
							}
					},

					series: [{
							type: 'pie',
							name: 'Browser share',
							data: [

									['脂肪',   ffb],

									['蛋白质',  ppb],

									{
										 name: '碳水化合物',
											y: ccb,
											sliced: true,
											selected: true
									}
							]
					}]
			});
			//单位换算
			var danwei = ['<option selected="selected" value="100">每100克</option>'];
			for(var i in unit){
				 danwei.push('<option value="'+ unit[i].keNum +'">每' + unit[i].fnum + unit[i].funit  +'</option>');
			}
			$("#yycfdw").html(danwei.join("")).find("option").eq(selectPos).attr("selected","selected")
			$("#yycfdw").change(function(i){
				  var index = $(this).get(0).selectedIndex;
					var keVal = $(this).val();
					var fid = $("#fid").val();
					var newHref = "/tzgj/fit/foodshow?fid="+fid+"&ke="+keVal+"&pos="+index;
					//var newHref = href.indexOf('ke=') == -1 ? href+'&ke='+keVal : href.substring(0,href.indexOf('ke='))+'ke='+keVal;
					//newHref = newHref.indexOf('pos=') == -1 ? newHref+'&pos='+index : newHref.substring(0,newHref.indexOf('pos='))+'&pos='+index;
					window.location.href= newHref;
			});
			setTimeout(function(){
				 $("#container").find("#highcharts-0").css("margin-left","-30px").css("width",'296px');
			},100);
			//获取后台设置的广告信息
			fit_tool.getAdSense('food');
    });
</script>


