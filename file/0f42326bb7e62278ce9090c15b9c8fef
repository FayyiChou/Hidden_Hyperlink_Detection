var GradCon='<div class="mt10 w606 Doc_Grad pr bc"><div class="Doc_addicon User_newbg pa"></div><div class="Toucon"><div class="mt5 mr10 fr"><a href="javascript:void(0)" title="关闭" class="GradBtn User_newbg addClose db cp"></a></div>'+GradList+GradForm+'</div></div>';
$('.Doc_Grad').remove();
if(obv.html()=='已评价'){
alert('您已评价过');
return;
}
Parent.after(GradCon);
e.preventDefault();
var flag=true;
$('.GradPut input').click(function(){flag=false;$('.GradPut input').removeClass('puton');$(this).addClass('puton');});
$('.GradPut input').hover(function(){$(this).addClass('current')},function(){$(this).removeClass('current')})
$('.GradText').bind({
focus: function(){
$(this).val() === '请对医生的劳动做出评价' || $.trim($(this).val()) === '' ?  $(this).val(' ') : 0;
},
blur: function(){
$(this).val() === '请对医生的劳动做出评价' || $.trim($(this).val()) === '' ?  $(this).val('请对医生的劳动做出评价') : 0;
}
});
$('.GradBtn').click(function(){
$('.Doc_Grad').remove();
})
$('.GradForm').submit(function(e){
var self = $(this),
value = $.trim(self.find('.GradText').val());
e.preventDefault();
if(!$('.GradPut').hasClass('none')){
if(flag){
alert('请选择评价理由');
return;
}
}
if($('.GradNum').val()==''){
alert('请进行评价');
return;
}
self.find('button').ajaxStart(function(){
$(this).html('提交中...');
});
if(self.find('button').html() == '提交'){
$.ajax({
url: 'http://api.club.xywy.com/ques_grade.php',
type: 'get',
data: {qid: obv.attr('rel').split("&")[0].split("=")[1] , rid : obv.attr('rel').split("&")[1].split("=")[1],uid : obv.attr('rel').split("&")[2].split("=")[1],GradNum:$('.GradNum').val(),reason:$('.reason').val(),ts_type:$('.puton').val()},
dataType: 'jsonp',
success: function(data){
if(data.result == 1){
    //alert(data.msg);
    club_alert(data.code);
var Numtxt=$('.GradNum').val();
var GradeNum='<span class="GraDSmall"><b></b><i></i></span>';
obv.after(GradeNum);
var Num=$('.GraDSmall b');
$('.GraDSmall i').html(Numtxt+'分');
if(Numtxt==1){Num.attr('class','w10');}else if(Numtxt==1.5){Num.attr('class','w15');}else if(Numtxt==2){
Num.attr('class','w20');}else if(Numtxt==2.5){Num.attr('class','w25');}else if(Numtxt==3){
Num.attr('class','w30');}else if(Numtxt==3.5){Num.attr('class','w35');}else if(Numtxt==4){Num.attr('class','w40');}else if(Numtxt==4.5){Num.attr('class','w45')}else if(Numtxt==5){Num.attr('class','w50')};
$('.Doc_Grad').remove();
//obv.html('已评价');
obv.replaceWith('<span class="graydeep">已评价</span>');
flag=true;
}else{
self.find('button').html('提交');
alert(data.msg);
}
}
});
}
});
$('.GradList li i').mouseenter(function(){
var Index=$(this).index();
var PIndex=$(this).parent('li').index();
$('.GradList li').attr('class','');
$('.GradList li:lt('+PIndex+')').addClass('current');
if(Index==0){
$(this).parent('li').removeClass('current').addClass('on');				
}else{
$(this).parent('li').removeClass('on').addClass('current');
}
if(PIndex==0){
$(this).parent('li').removeClass('on').addClass('current');
}
})			
var PcIndex;
var cIndex;		
$('.GradList li i').click(function(){
cIndex=$(this).index();
PcIndex=$(this).parent('li').index();
$('.GradList li').attr('class','');
$('.GradList li:lt('+PcIndex+')').addClass('clcurrent');
if(PcIndex==0){
$('.GradList li:eq(0)').removeClass('clon').addClass('clcurrent');
$('.GradNum').val(1);
$('#GradNums').html('1分');
}else{
if(cIndex==0){
$(this).parent('li').removeClass('clcurrent').addClass('clon');
$('.GradNum').val(PcIndex+0.5);
$('#GradNums').html((PcIndex+0.5)+'分');				
}else{
$(this).parent('li').removeClass('clon').addClass('clcurrent');
$('.GradNum').val(PcIndex+1);
$('#GradNums').html((PcIndex+1)+'分');
}
}
if(PcIndex<2){
$('.GradPut').removeClass('none');
}else{
$('.GradPut').addClass('none');
}
})
$('.GradList li i').mouseleave(function(){
$('.GradList li').attr('class','');
$('.GradList li:lt('+PcIndex+')').addClass('clcurrent');
if(cIndex==0){
$('.GradList li:eq('+PcIndex+')').removeClass('clcurrent').addClass('clon');				
}else{
$('.GradList li:eq('+PcIndex+')').removeClass('clon').addClass('clcurrent');
}
if(PcIndex==0){
$('.GradList li:eq(0)').removeClass('clon').addClass('clcurrent');
}
})
})
//评论功能完		
</script>

