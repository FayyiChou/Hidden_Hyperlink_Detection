<script type="text/javascript" src="http://static.wkimg.com/common/lib/jquery/1.8.3/jquery.min.js"></script>
<script>
    $.fn.extend({
        loopScroll: function(opts){

            var defaultOptions = {
                    amount          : 1, //每次的滚动个数, 默认是1
                    speed           : 300, //滚动速度常数, 默认是200
                    time            : 3000, //每次滚动间隔时间, 默认为3秒
                    prevButton      : '.roll_lbtn', //向前滚动按钮, id或者class
                    nextButton      : '.roll_rbtn', //向后滚动按钮, id或者class
                    operate         :  function(){},
                    vertical        : false, //是否垂直滚动, 默认不垂直滚动
                    auto            : true //是否自动滚动
                },
                options = $.extend(defaultOptions, opts);

            return this.each(function(key, obj){
                var scrollBox = $(obj).find('ul:first'),
                    item = $(obj).find('li'),
                    length = item.length,
                    pro = options.vertical === true ? 'height' : 'width',
                    index = 0,
                    prevAllow = 1,
                    nextAllow = 1,
                    target,
                    maxIndex ,
                    variation,
                    listSize,
                    wrapSize,
                    dir,
                    fn,
                    timer;

                $(obj).css(pro, 9999);

                if(options.vertical === true){
                    listSize = item.eq(0).outerHeight(true) * length;
                    dir = 'top';
                    wrapSize = $(obj).parent().height();
                }
                else{
                    listSize = item.eq(0).outerWidth(true) * length;
                    dir = 'left';
                    wrapSize = $(obj).parent().width();
                }

                variation = parseInt(listSize / length) * options.amount;

                maxIndex  = parseInt(listSize / variation);

                if(listSize <= wrapSize){
                    return;
                }

                scrollBox.clone(true).insertAfter(scrollBox);

                fn = {
                    clearTimer: function(){
                        clearInterval(timer);
                        timer = null;
                    },
                    moveTo: function(n){
                        if(n > maxIndex){
                            index = n = 1;
                            $(obj).css(dir, 0);
                        }
                        else if(n < 0){
                            index = n = maxIndex - 1;
                            $(obj).css(dir, - variation * maxIndex);
                        }

                        target = - variation * n;
                        this.anim();
                    },
                    anim: function(){
                        var data = {};
                        data[dir] = target;
                        $(obj).animate(data, options.speed, function(){
                            prevAllow = nextAllow = 1;
                            options.operate(index, maxIndex);
                        });
                    },
                    auto: function(){
                        var self = this;
                        timer = setInterval(function(){
                            self.moveTo(++index);
                        }, options.time);
                    },
                    start: function(){
                        this.clearTimer();
                        if(options.auto === true){
                            this.auto();
                        }
                    }
                };

                $(obj).bind({
                    mouseover: function(){
                        fn.clearTimer();
                    },
                    mouseout: function(){
                        fn.start();
                    }
                });

                $(document).on({
                    click: function(event){
                        var target = $(event.target);
                        if(target.closest(options.prevButton).length > 0){
                            if(prevAllow){
                                prevAllow = 0;
                                fn.moveTo(--index);
                            }
                        }
                        else if(target.closest(options.nextButton).length > 0){
                            if(nextAllow){
                                nextAllow = 0;
                                fn.moveTo(++index);
                            }
                        }

                        fn.clearTimer();
                    },
                    mouseover: function(){
                        fn.clearTimer();
                    },
                    mouseout: function(){
                        fn.start();
                    }
                }, options.prevButton + ',' + options.nextButton);

                fn.start();

            });

        }
    });
	$(function(){
		var qaHeight = parseInt($('.question_new_Conle').outerHeight()), //左侧的高度
		rHeight = parseInt($('.mainRight').outerHeight()),
		rTop = parseInt($('.mainRight').offset().top);
		$(window).on('scroll',function(){
			var pageScrollTop = $(window).scrollTop(),
				ggTop = pageScrollTop - rTop,
				lastGgTop = qaHeight+rTop-357;
			if(qaHeight - rHeight < 357){
				return false;
			}
			if(pageScrollTop > rHeight+rTop-40 && pageScrollTop < lastGgTop-30){
				$('#b_if_rame').css({
					position: 'absolute',
						top: ggTop+30,
						left:0
				});
			}else if(pageScrollTop >= lastGgTop-30){
				$('#b_if_rame').css({
					position: 'absolute',
						top: lastGgTop-rTop-10,
						left:0
				});
			}else{
				$('#b_if_rame').css({
					position: 'static',
						top: 0,
						left:0
				});

			}
		});
	});
    $('.doc-online-scroll-list').loopScroll({
        prevButton: '.doc-scroll-up-btn',
        nextButton: '.doc-scroll-down-btn',
        vertical: true,
        auto: true
    });

    </script>

