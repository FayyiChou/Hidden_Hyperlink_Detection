function page_xywy_iframe_show_json_w(page_xywy_div)
{
document.domain="xywy.com";
var theurl = "http://page.xywy.com/gk?a=brands_custom_wenzi&k="+subject;
var div = '.'+page_xywy_div;
$.ajax({
type: "GET",
url: theurl,
dataType: "jsonp",
jsonpCallback: "xywyback",
success: function(json){
if(json.result==1){
var html_str = json.content;
$(div).html(html_str);
return true;
}else{
$('.ggwz').html($('#ulxgwtg').html());
}
}
});
}
page_xywy_iframe_show_json_w('ggwz');
</script>


