$('.Grade').click(function(e){
var obv = $(this);
var Parent=obv.parents('.ballc');
if(Parent.siblings('.Doc_add').get(0)){
Parent.siblings('.Doc_add').remove();
}
if(Parent.next('.Doc_Grad').get(0)){
Parent.next('.Doc_Grad').remove();
return;
}
var GradList='<ul class="GradTxt clearfix pt15"><li>很不满意</li><li>不满意</li><li>一般</li><li>满意</li><li>非常满意</li></ul><ul class="GradList clearfix"><li><i></i><i class="w78"></i></li><li><i></i><i class="w78"></i></li><li><i></i><i class="w78"></i></li><li><i></i><i class="w78"></i></li><li><i></i><i class="w38"></i></li></ul>';
