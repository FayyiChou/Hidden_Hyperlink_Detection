    new Image().src="http://stat-z.xywy.com/e.png?pagevisit=pvclub_quiz_doctor_online&chkarea=%E5%8C%97%E4%BA%AC&chkword=%E5%B0%BF%E9%A2%91&chkwordid=332&fromtype=1&tx=&custom_id=8640&fromurl="+fromurl+"&clientID="+loginID+"&time=" + new Date().getTime()+"time";
</script>
