lock = false;
window.clearInterval(run);
}
else if (lock == false) {
lock = true;
run = setInterval("chgImg(1)", delay);
}
}

function setdefault() {
  for (i=0;i<=ImgLength;i++) {
    if (document.images["img"+i].height > document.images["img"+i].width) { document.images["img"+i].width=67; }
  }
}
