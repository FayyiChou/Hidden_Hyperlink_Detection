<script type="text/javascript">
$(function(){
$(".bg_setup").click(function(){
	/*$(".bg_change").unbind();  */
	if($(".bg_change").css("display")=="none"){$(".bg_change").stop().fadeIn("fast");
}
else{$(".bg_change").stop().fadeOut("fast");
}
});
	
	$(".bg_change_list img").each(function(i){
										   var imgurl="bg_main_"+(i+1)+".jpg";
										   $(this).click(function(){ $("body").css("background","url(images/"+imgurl+") 0 center");
										   $.cookie('agwtheme',imgurl,{expires:1000,path:'/'});return false;
			 });
		});
	$(".bg_close").click(function(){$(".bg_change").stop().fadeOut("fast");});
	$(".bg_change").hover(function(){$(this).stop();},function(){$(this).stop().delay(1000).fadeOut("fast");});
	
	if($.cookie('agwtheme')!=null&&$.cookie('agwtheme')!='undefined')
		{var imgurl=$.cookie('agwtheme');
		$("body").css("background","url(images/"+imgurl+") 0 center");};	
		   });


</script></div>
