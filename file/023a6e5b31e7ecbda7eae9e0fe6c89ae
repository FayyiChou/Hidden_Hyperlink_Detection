<script type="text/javascript">
$(window).bind('resize.resethtml', function(){
	if($(window).width()/$(window).height()<1.8){
		$('html').addClass('html_zhengfangxing');
	} else {
		$('html').removeClass('html_zhengfangxing');
	}
	
	if($(window).width()<1004){
		$('#wrap_all').width(1000);
		$('#fix_top').width(1000);
	}
	else{
		
		$('#wrap_all').width('100%');
		$('#fix_top').width('100%');
		
		}
		
		if($(window).height()<650){
		$('#wrap_all').height(640);
		$('#fix_top').height(115);
		
	}
	else{
		
		$('#wrap_all').height('auto');
		$('#fix_top').height('20%');
		
		}
});
$(window).trigger('resize.resethtml');

</script>
<script language="javascript" type="text/javascript"> 
<!------function changeBgImg(option){ document.getElementById("bg_body").style.backgroundImage = "url("+option.options[option.selectedIndex].value+")"; } -->
 function changeBgImg(option){ 
    $('body').css({'background':'url('+option.options[option.selectedIndex].value+')'});
  } 

</script>
<script type="text/javascript">

  $(function(){   
      var w=100+"%";
      var n=$('.roll_ul li').size();
      $('.roll_ul').width(100*n+"%");
      $('.roll_ul li').width(100/n+"%");
      var t=0;
      $('#banner_next').bind('click',function(){
        t++;
        $('#banner_prev').show();
        var f=$('.roll_ul').css('left');
        $('.roll_ul').animate({'left':'-=100%'},300);     
        if(t==n-1){
          $('#banner_next').hide()
        }
        $('.scroll_nav span').eq(t).siblings().removeClass('current');
        $('.scroll_nav span').eq(t).addClass('current');
      })
      $('#banner_prev').click(function(){
        t--;
        $('#banner_next').show();
        var l=$('.roll_ul').css('left');
        $('.roll_ul').animate({'left':'+=100%'},300)  
        if(t==0){
          $('#banner_prev').hide()
        }
        $('.scroll_nav span').eq(t).siblings().removeClass('current');
        $('.scroll_nav span').eq(t).addClass('current');
      })
      $('.scroll_nav span').each(function(i){
        $(this).click(function(){
          i=$(this).index();
          t=i;
          $('#banner_prev').show();
          
          if(i==0){
            $('#banner_prev').hide();
            $('#banner_next').show(); 
          }
           else if(i==n-1){
            $('#banner_next').hide(); 
            $('#banner_prev').show(); 
          }
          else {$('#banner_next').show();$('#banner_prev').show();}
          var cd=-100*i+"%";
          $('.roll_ul').animate({'left':cd},500); 
          $(this).siblings().removeClass('current');
          $(this).addClass('current')
        })
      })    
  })
</script>
