<script type="text/javascript">
$(document).ready(function() {
	$("#x1").xMarquee(); //停气公告滚动
	$('.flexslider').flexslider({directionNav: true, pauseOnAction: false}); //焦点图切换效果
	$('.newsImg_box').flexslider({directionNav: true, pauseOnAction: false}); //图片新闻
	$('.service_box .service ul li:last').css('margin-right','0'); //我们的服务最后一个'li'的'margin-right'等于'0'
	$('#links').click(function(){$('#links + ul').slideToggle('slow');}); //网站友情链接点击展示效果
	$('.demo1').Tabs(); //选项卡效果
	$('.xMarquee2').Yislide({
		linb : 212,      //单元宽度
		lishow : 6,      //显示出来的个数
		speed : 3000,    //单元移动速度
		linumto : 1      //单元移动个数
	});
	$("#focus").hover(function(){$("#focus-prev,#focus-next").show();},function(){$("#focus-prev,#focus-next").hide();});
	$("#focus").slide({ 
		mainCell:"#focus-bar-box ul",
		targetCell:"#focus-title a",
		titCell:"#focus-num a",
		effect:"left",
		easing:"easeInOutCirc",
		autoPlay:true,
		delayTime:400
	})
	

});
</script>

 












