<script type="text/javascript">
//开始对比
function startCompare(modelId){
var checklist = document.getElementsByName ("modelcompares");
var param = '';
for(var i=0;i<checklist.length;i++){
if(checklist[i].checked == 1){
param += checklist[i].value+",";
}
}
var url = "http://m.pcauto.com.cn/auto/sg14143/m70720/config.html?mIds="+modelId+","+param.substring(0,param.length-1);
window.open(url);
}
//选车型
function select_Model(modelId){
var checklist = document.getElementsByName ("modelcompares");
var flag = false;
for(var i=0;i<checklist.length;i++){
if(checklist[i].checked == 1){
flag = true;
}
}
if(flag == true){
$('#JbtnCtrst2').attr("disabled","");
$('#JbtnCtrst2').attr("onclick","startCompare("+modelId+");");
} else {
$('#JbtnCtrst2').attr("disabled","disabled");
$('#JbtnCtrst2').attr("onclick","");
}
}
</script>

