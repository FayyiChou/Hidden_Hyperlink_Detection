var headerTitleNav = footFn.getElem(".header-title-nav");
if (!headerTitleNav) return;
var canScroll = headerTitleNav.scrollWidth - headerTitleNav.offsetWidth;
if (canScroll > 0) {
headerTitleNav.scrollLeft = canScroll;
}
})();
