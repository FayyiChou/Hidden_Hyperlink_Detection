(function() {
window.addEventListener("load", function() {
var Jfoot = footFn.getId("Jfoot");
var Jbody = footFn.getElem("body");
if (!Jfoot) return;
if (!Jbody) return;
function fixFoot() {
var webHeight = document.documentElement.clientHeight,
JbodyHeight = Jbody.clientHeight,
JfootHeight = Jfoot.clientHeight;
if (Jfoot.style.position != "absolute") {
if (JbodyHeight <= webHeight) {
Jfoot.setAttribute("style", "width:100%;position:absolute;bottom:0;left:0;");
};
} else {
if (JbodyHeight + JfootHeight >= webHeight) {
Jfoot.setAttribute("style", "position:static;");
};
};
}
fixFoot();
setInterval(function() {
fixFoot();
}, 1000);
}, false);
})();
