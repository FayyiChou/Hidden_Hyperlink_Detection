<script type="text/javascript">
function setCookie(name, value, Days){
var exp = new Date();    //new Date("December 31, 9998");
exp.setTime(exp.getTime() + Days*24*60*60*1000);
document.cookie = name + "="+ encodeURIComponent(value) + ";path=/;expires=" + exp.toGMTString();
}
function getCookie(name){ //读取时，忽略键名大小写
var reg = new RegExp(["(?:^| )", name, "=([^;]*)"].join(""),"i"),
arr = document.cookie.match(reg);
return arr ? decodeURIComponent(arr[1]) : null;
}
var key = "browsing_history";
function addsg(sgId,sgname, price) {
var str = getCookie(key);
var brand = "{\"id\":" + sgId + ", \"name\":\"" + sgname + "\", \"price\":\"" + price + "\"}";
//alert(str);
if(str == "" || str == null) {
str = brand;
} else {
var arry = str.split("|");
if(arry.length <= 5) {
for(var i=0;i<arry.length;i++){
var json = eval("(" + arry[i] + ")");
if(json.id == sgId){
return;
}
}
if(arry.length == 5) {
str = arry[1] + "|" + arry[2] + "|" + arry[3] + "|" + arry[4] + "|" + brand;
}else{
str = str + "|" + brand;
}
}
}
setCookie(key, str, 1);
}
window.addEventListener("load",function (){
var history = getCookie(key);
var historyTag = document.getElementById("browsing_history");
if(history == "" || history == null) {
//historyTag.style.display = "none";
}else{
var arry = history.split("|");
var html = "<ul class='m-txts m-txts1'>";
for(var i = arry.length-1; 0<=i; i--) {
var json = eval("(" + arry[i] + ")");
html = html + "<li><a href='http://m.pcauto.com.cn/auto/sg" + json.id + "/'><span>" + json.name + "</span><span class='s-red'>" + json.price + "</span></a><a class='s-blue' href='http://m.pcauto.com.cn/auto/sg"+json.id+"/discount.html'>查优惠<i class='u-arr-r'></i></a></li>";
}
html += "</ul>";
historyTag.innerHTML = html;
}
addsg(14143, "荣威RX5", "9.98-18.68万");
},false);
</script>
