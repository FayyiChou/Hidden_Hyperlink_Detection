var showfristTopsale=true;
function renderSale(dealer){
var salers=dealer.sales;
if(!salers || salers.length==0) return "";
var sales='<div class="m-counselor">';
var clas=showfristTopsale?'m-tog active':'m-tog';
sales+='<p class="'+clas+'">选择购车顾问，尊享一对一服务<i class="u-tog"></i></p>';
var display=showfristTopsale?'block':'none';
sales+='<div class="m-counselor-wrap">';
showfristTopsale=false;
for(var i=0;i<salers.length;i++){
var s=salers[i];
sales+='<div class="m-member">';
sales+='<a href="'+root+'/auto/'+dealer.id+'/sale_order'+s.id+'.html'+'" class="m-member-info">';
var img= !s.headUrl || s.headUrl==''?'http://www1.pcauto.com.cn/20130428/houtai/images/defaultHeadIcon.jpg':s.headUrl;
sales+='<img src="'+img+'" width="40" height="48" />';
sales+='<p>'+s.name+'<i class="s-red">最近为'+s.orderNum+'人提供咨询</i></p>';
sales+='</a>';
sales+='<div class="m-member-btns">';
sales+='<a href="tel:'+s.poc400Phone+'" class="s-grey9" onclick="setSalePhone400Count();"><i class="u-ico-call"></i>致电</a>';
sales+='<a href="'+root+'/auto/'+dealer.id+'/sale_detail'+s.id+'_sg'+sgid+'.html#ad=5765" class="s-grey9"><i class="u-ico-consult"></i>询价</a>';
sales+='</div>';
sales+='</div>';
}
sales+='</div></div>';
return sales;
}
