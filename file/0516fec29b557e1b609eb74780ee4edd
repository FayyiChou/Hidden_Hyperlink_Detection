<!-- AB 测试S -->
<script type="text/javascript">
var curScheme;
(function(){
var cookie,time
abTest = {
setCookie: function(name, value) {
document.cookie = name + "=" + value + "; path=/; domain=" + window.location.hostname.replace(/\w+((\.\w+)+)/, "$1") + "; expires=" + new Date(+new Date() + 31 * 24 * 3600 * 1000).toGMTString()
},
getCookie: function(name) {
return document.cookie.replace(new RegExp(".*(?:^|; )" + name + "=([^;]*).*|.*"), "$1")
},
getCurTime:function(){
time = new Date().getSeconds();
},
// 根据访问时间戳的秒数位分配方案，并用cookie记录方案
scheme:function(){
abTest.getCurTime();
if(time >= 0 && time <30){
curScheme = 0;//购车优惠
}else if(time >= 30 && time <60){
curScheme = 1;//置换购车
}
abTest.setCookie("abTest1",curScheme);
}
}
if(abTest.getCookie("abTest1") == ""){
// 不存在cookie,按时间规则分配一个方案
abTest.scheme();
}else{
// 存在cookie,根据cookie的值露出方案
curScheme = abTest.getCookie("abTest1");
}
})();
</script>
