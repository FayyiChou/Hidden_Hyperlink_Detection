var brandId="365";
var serialId="14143";
var modelId="70720";
function getSecondHandCar(areaId){
$.ajax({//车型导航
url: 'http://price.pcauto.com.cn/price/api/v1/cooperation/guazi/local_second_data/',
data: {
serialGroupId: serialId,
regionId: areaId,
from: 2,
orderIndex : 3//价格降序
},
type: 'get',
dataType: 'jsonp',
cache: true,
success: function (data2) {
var ele2 = document.getElementById('guaziScd4ModelN');
if( ele2){
ele2.href = "http://price.pcauto.com.cn/price/api/v1/cooperation/second_hand_car/load/?from=2&source=6&url="+data2.url;
}
}});
$.ajax({//车型底部二手车列表
url: 'http://m.pcauto.com.cn/auto/api/hcs/serialGroup/secondHandCar',
data: {
serialGroupId: serialId,
regionId: areaId,
from: 2,
orderIndex : 1//上架时间降序
},
type: 'get',
success: function (data) {
if($.trim(data) == '{}'){
}else{
$("#guaziList").html(data);
}
}});
}
var _provinceId=0;
function getJinrong(rid) {
var jinRongLink = "http://m.jr.pcauto.com.cn/modelPayment/r" + rid + "/sg" + serialId + "-m70720/?callback=dkgcFunc";
loadScript(jinRongLink);
}
function getActity () {
locationPub.doAfterLocate(function(locateData) {
var url = "http://play9.pcauto.com.cn/auto150923/action/getActivity.jsp?"
+ "isWap=1&brandId=" + brandId + "&serialId=" + serialId + "&modelId=" + modelId + "&cityId="+locateData.cityId+"&callback=displayData";
loadScript(url);
});
}
