//getJinrong(rid);
var region=rname;
if(region == "上海市" || region == "杭州市" || region == "北京市" || region == "天津市" || region == "成都市" || region == "重庆市" || region == "深圳市" || region == "东莞市" || region == "南京市" || region == "苏州市" || region == "广州市" || region == "佛山市" || region == "武汉市"){
$("#secondcarLink").attr('href','http://www1.pcauto.com.cn/zt/pcauto/160708/PAICHE/index.html#p1/#ad=8266');
$("#secondcarLink").empty().append('<i class="tag tag3">工具</i>旧车多卖20%，个人直卖不压价</a>');
$("#secondcarLink").parent().attr("style","display: block");
}else{
