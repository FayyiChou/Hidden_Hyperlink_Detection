}
}
/**
* 汽车商城活动
* @param data
* @private
*/
function __showMallDataBlock(data){
if(data != null && data.normalRowsTotal > 0){
var mallNum = data.normalRowsTotal;
var malls = data.normalRows;
if(malls != undefined && malls != null){
var shtml = '';
if(mallNum>1){//有多条,随机出一条
var art = getArrayItems(data.normalRows, 1);//随机
shtml = getMallArticleHtml(art[0]);
}else{//只有一条，出一条
shtml = getMallArticleHtml(malls[0]);
}
$("#malls").html(shtml);
$("#malls").show();
}else{
$("#malls").hide();
}
}else{
$("#malls").hide();
if(true){
loadScript("http://mall.pcauto.com.cn/autoMall/interface/gct/tuangou_prediction_count_by_serialgroup.jsp?sgId=14143&days=60&callback=huodongList2");
}
}
}
