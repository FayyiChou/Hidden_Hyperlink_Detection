// 操作cookie
function getCookie(name){ //读取时，忽略键名大小写
var reg = new RegExp(["(?:^| )", name, "=([^;]*)"].join(""),"i"),
arr = document.cookie.match(reg);
return arr ? decodeURIComponent(arr[1]) : null;
}
//一键询价
function oneAsk(dealerId,rid){
//添加计数器
var img = new Image;
img.src = "http://count.pcauto.com.cn/count.php?channel=7871&screen="+screen.width+"*"+screen.height+"&refer="+encodeURIComponent(document.referrer)+"&anticache="+new Date().getTime()+"&url="+encodeURIComponent(location.href);
var url = "http://m.pcauto.com.cn/auto/api/hcs/dealer/order";
var referrer = document.referrer;
var refererType = 1;
var cookie = getCookieMes('pcsuv');
var ip = $("#ip").val();
if (referrer == '') {
referrer = window.location.href;
refererType = 2;
}
$.ajax({
type: 'POST',
url: url,
data: {
reqResource:'wapOrder2013',
domainName: 'DealerCommOrder',
serialGroupId: sgid,
modelId: mid,
regionId: rid,
dealerId:dealerId,
name: userName,
phone: phone,
wap: 1,
referer: referrer,
refererType: refererType,
origin: 2,
cookie: cookie,
ip: ip,
securityKey:securityKey
},
success: function (data) {
if (data) {
if (data != 'ok') {
var result = eval('(' + data + ')');
if (result && !result.err) {
window.location.href = 'http://m.pcauto.com.cn/wap_dazzle/2_0/ask_success.jsp?type=0&back_url=' + referrer + '&oid=' + result.id + '&modelId='+result.modelId+'&dealerId='+result.dealerId+'&t='+ result.t +'&phone='+ result.phone;
//core.showTopTip("一键询价成功");
} else {
core.showTopTip("一键询价失败");
}
} else {
core.showTopTip("一键询价成功");
}
} else {
core.showTopTip("一键询价失败");
}
core.hideTopTip();
},
error: function () {
core.showTopTip("一键询价失败");
core.hideTopTip();
}
});
$('#Jpop-on-key').hide();
$("#oneAsk_"+dealerId).html('已询价');
$("#oneAsk_"+dealerId).attr("onclick","return false;");
$("#oneAsk_"+dealerId).attr("class","ask-price-btn ask-price-btn-disable");
}
function getCookieMes(name){
var cooks = document.cookie.split(";");
for(var i=0;i<cooks.length;i++){
var str = cooks[i].split('=');
var cName = $.trim(str[0]);
if(cName==name){
return unescape(str[1]);
}
}
return '';
}
function ipInfo(data){
if(data){
$("#ip").val(data.ip);
}
}
function getClientIp(){
loadScript('http://price.pcauto.com.cn/wap_dazzle/common/getRequestIp.jsp?callback=ipInfo');
}
