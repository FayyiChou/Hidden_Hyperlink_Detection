<script>
//经销商模块
function getDealerHtmlList(rid, sgid, mid, aq, pageNum, pageSize){
var url = 'http://m.pcauto.com.cn/wap_dazzle/dealer/dealer_ajax.jsp';
$.ajax({type: 'GET',dataType:'jsonp',url: url,data: {rid:rid, sgid:sgid, mid:mid, aq:aq, pageNum:pageNum, pageSize:pageSize},
jsonp: 'callback',
jsonpCallback:'success',
success: function (dealerJson) {
render('4s', dealerJson.fs,rid);
render('zh', dealerJson.zh,rid);
$("#swipe_4S").trigger("click");
swipeTab(document.getElementById("JtabShop")); // 数据处理完成后再初始化 swipe 滑动效果
initTog(document.getElementById("JtabShop")); // 经销商咨询顾问展开或收起
},
cache: true
});
}
function render(applyId, dealers,rid) {
var html = '';
if(dealers.message != null) {
html = '<div class="divTip">'+dealers.message+'</div>';
}
if(dealers.size > 0){
html = html + '<ul class="m-list-shop" id="yh-list">';
$.each(dealers.data, function (index, dealer) {
var str = '<li id="dealer_'+dealer.id+'">';
if(dealer.vip != -1){
str += '<a href="http://m.pcauto.com.cn/auto/' + dealer.id +'/" class="m-shop-info">'
+ '<p>' + dealer.name + '</p>'
+ '<p><span class="s-red">' + dealer.price +'万</span>';
var maxDiscount = dealer.maxDiscount;
if(maxDiscount != null && maxDiscount !='' && maxDiscount !='0.0万'){
str +='<span class="s-green">最高降'+maxDiscount+'</span>';
}
str += '</p>'
}else {
str += '<a href="http://m.pcauto.com.cn/auto/' + dealer.id + '/" class="m-shop-info">'
+ '<p>' + dealer.name + '</p>';
}
//str += '<p class="m-des-buy"><span id="dealeryh1_'+dealer.id+'" name="dealeryh1"></span><span id="dealeryh2_'+dealer.id+'" name="dealeryh2"></span></p>';
if(dealer.address==null || dealer.address=="" ){
str += '<p class="s-grey9"><i class="u-position"></i>暂无</p>';
}else{
str += '<p class="s-grey9"><i class="u-position"></i>' + dealer.address + '</p>';
}
str += '<i class="u-arr-r"></i></a>';
/* if(dealer.dnid != ""){
str += '<p class="cuxiao">促销：<a href="http://m.pcauto.com.cn/auto/market/' + dealer.id + '-' + dealer.dnid + '.html" class="blue">' + dealer.msg + '</a></p>';
} */
if(dealer.phone!="" || dealer.poc_400_phone!="" ){
if(dealer.is_400 == 1) {
str += '<p class="u-btn-gp"><a href="tel:' + dealer.poc_400_phone +'" class="u-btn-call" onclick="traceLog(\'callClick\',1,\''+dealer.poc_400_phone.replace(/-/g,'').replace('转','-')+'\');setPhone400Count();">电话咨询</a>';
} else{
str += '<p class="u-btn-gp"><a href="tel:' + dealer.phone +'" class="u-btn-call" onclick="traceLog(\'callClick\',1,\''+dealer.phone.replace(/-/g,'').replace('转','-')+'\')">电话咨询</a>';
}
}else{
str += '<p class="infoA"> <a href="#" class="u-btn-call u-btn-call2">电话咨询</a>';
}
if(!b){
if(dealer.qbPrice > 0){
str += '<a href="http://m.pcauto.com.cn/auto/ask-c' + rid + '-sg' + sgid + '-m' + mid + '-d' + dealer.id + '.html#ad=4322" class="u-btn-consult u-btn-consult2">询底价</a>';
}else{
str += '<a href="http://m.pcauto.com.cn/auto/ask-c' + rid + '-sg' + sgid + '-m' + mid + '-d' + dealer.id + '.html#ad=4322" class="u-btn-consult">询底价</a>';
}
}else{
str += '<a href="javascript:;" onclick="showAsk('+dealer.id+','+rid+');" class="u-btn-consult" id="oneAsk_'+dealer.id+'" >一键询价</a>';
}
str += '</p>';
//str +=renderSale(dealer);
str+='</li>';
html = html + str;
});
html = html + "</ul><a class='u-btn-more' href='javascript:moreDealerForward()'>更多经销商<i class='u-arr-r'></i></a>";
$('#' + applyId).html(html);
}else{
$('#' + applyId).html('<p class="u-none">暂无经销商</p>');
}
/*getJinrong2(rid);//分期
getDealerPrivilege(rid);//行情*/
}
//分期
function getJinrong2(rid) {
var jinRongLink = "http://m.jr.pcauto.com.cn/modelPayment/r" + rid + "/sg14143-m70720/";
$.ajax({
type: 'GET',
url: jinRongLink,
dataType: 'jsonp',
jsonpCallback: 'dkgcFunc2',
cache: true
});
}
function dkgcFunc2 (data) {
locationPub.doAfterLocate(function(locateData) {
var rid = locateData.cityId;
if (data != null && data.msg == '成功') {
$('span').each(function(){//经销商列表分期
if($(this).attr('name')=="dealeryh1"){
this.innerHTML = data.plan;
}
});
}
});
}
//经销商行情
function getDealerPrivilege(_areaId){
var dealerLink = "http://price.pcauto.com.cn/dealer/interface/get_replace_gift.jsp?rId="+_areaId+"&mId=70720";
$.ajax({
type: 'GET',
url: dealerLink,
dataType: 'jsonp',
jsonpCallback: 'getDealerPrivilegeCb',
cache: true
});
}
function getDealerPrivilegeCb(data){
$('span').each(function(){
if($(this).attr('name')=="dealeryh2"){
var did = this.id.substr(10);
if(data.result == true){//有行情
for(var item in data.row){
var data_did = data.row[item].dealerId;
if(data_did == did){
$('#dealeryh2_'+did).html('置换补贴'+data.row[item].discount+'万');//置换
}
}
}
}
})
}
