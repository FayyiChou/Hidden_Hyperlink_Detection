}
//}
});
}
function displayData(data){
if (data!=null && data.activity.length>0 && data.code == 1){
var obj = data.activity[0];
var activityName=obj.activityName;
var activityUrlWap=obj.activityUrlWap;
$("#displayActivity a[id=activityLink]").attr("href",activityUrlWap);
$("#displayActivity a[id=activityLink]").html(activityName);
$("#displayActivity").attr("style","display: block");
}
}
//车型的行情信息（降幅 ）
locationPub.doAfterLocate(function(locateData) {
getModelNewsByDiscount(locateData.cityId);
});
function getModelNewsByDiscount(rid) {
$.ajax({
url:"http://price.pcauto.com.cn/dealer/interface/app/market_json.jsp",
type: 'GET',
data:{
areaId:rid,
serialId:14143,
modelId:70720,
orderBy:'newest',
pageNo:1,
pageSize:10
},
dataType:'jsonp',
cache: true,
success: function(rs) {
$("#dealertotal").html(rs.total);
}
});
}
function getPingxing(){
$.ajax({
type: 'GET',
url: 'http://mall.pcauto.com.cn/import/intf/mall/1.0/hotSerialGroup/list.do',
data: {},
dataType: 'jsonp',
jsonpCallback: '_setPingxingList',
cache: true
});
}
function _setPingxingList(data){
var html = "";
if(data != null && data.rows.length > 0){
for(var i=0;i<data.rows.length;i++){
html += '<li><a href="/mall/import/sg'+data.rows[i].id+'/"><img alt="'+data.rows[i].name+'" src="http://www1.pcauto.com.cn/piclib/sogouvrsg/'+data.rows[i].id+'.jpg" class="s-place" /><i class="stit">'+data.rows[i].name+'</i><i class="pri">￥'+data.rows[i].price+'万起</i></a></li>';
}
if(html != ''){
$("#Jpingxingwrap").html(html);
scroll_X("Jpingxing","Jpingxingwrap",2.26,"li");
} else {
$("#JtabPingxing").hide();
}
}
}
//全城砍价
if( (1 == 1) || (1 == 2) ){//在售或停产在售
var curCityId;
locationPub.doAfterLocate(function(locateData) {
curCityId = locateData.cityId;
getPrivilegeOrBargain(curCityId);//优惠或砍价
});
$('#whole_city_bargain').attr("href","http://pocket.pcauto.com.cn/buyer/wap/bargain/m70720/#ad=5614");
} else {//置灰处理
$('#bargain_privilege').html('暂不支持砍价服务');
$('#whole_city_bargain').attr("disabled","disabled");
}
//优惠或砍价
function getPrivilegeOrBargain(curAreaId){
$.ajax({
type: 'GET',
url: "http://price.pcauto.com.cn/dealer/interface/price/getLowestModelPrice.jsp?rId="+curAreaId+"&mId=70720&method=local",
dataType: 'jsonp',
jsonpCallback: 'getPrivilege',
cache: true
});
}
//优惠
function getPrivilege(data){
if( (data.price.PRICE >0) && (data.price.OFFICE_PRICE > data.price.PRICE) ){//有优惠
var reducePrice = data.price.OFFICE_PRICE - data.price.PRICE;
$('#bargain_privilege').html('优惠'+Math.round(reducePrice*10000)+'元起');
} else {//没优惠,出砍价人数
$.ajax({
type: 'GET',
url: "http://price.pcauto.com.cn/price/api/v1/serialgroup/bargain_success_count/14143",
dataType: 'jsonp',
jsonpCallback: 'getSuccessCount',
cache: true
});
}
}
//砍价人数
function getSuccessCount(data){
if($('#whole_city_bargain').attr("disabled")!="disabled"){
$('#bargain_privilege').html(data.num+'人砍价成功');
}
}
</script>
<script type="text/javascript">
//for Toutiao
if (window.location.search.indexOf('FromTouTiao') != -1) {
sessionStorage['fromtoutiaoCookie'] = 'seted';
} else {
sessionStorage['fromtoutiaoCookie'] = 'unseted';
}
//控制某些模块隐藏
if(sessionStorage['fromtoutiaoCookie'] == 'seted'){
if(document.querySelector('.header-bg') != null){
document.querySelector('.header-bg').style.display = 'none';
}
if(document.querySelector('.m-footplate') != null){
document.querySelector('.m-footplate').style.display = 'none';
}
if(document.querySelector('.menu') != null){
document.querySelector('.menu').style.display = 'none';
}
if(document.querySelector('.m-footer') != null){
document.querySelector('.m-footer').style.display = 'none';
}
if(document.querySelector('.nav') != null){
document.querySelector('.nav').style.display = 'none';
}
if(document.querySelector('#JtgBtn1601') != null){
document.querySelector('#JtgBtn1601').style.display = 'none';
}
if(document.querySelector('#model_navigation') != null){
document.querySelector('#model_navigation').style.display = 'none';
}
if(document.querySelector('#m_crumb') != null){
document.querySelector('#m_crumb').style.display = 'none';
}
}
