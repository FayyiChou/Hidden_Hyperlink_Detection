  <script>
    function article_flex(){
        var elem = document.getElementById('jieshao');
        var flex = document.getElementById('flex');
        var contentHeighth = elem.offsetHeight;
        var maxHeight = 60;
        if (contentHeighth > maxHeight){
            elem.style.height = maxHeight +'px';
            flex.style.display ='inline-block';
            flex.onclick = function(){
                if(flex.className == 'less'){
                    elem.style.height = 'auto';
                    flex.className = 'more'
                    flex.innerText = '收起∧';
                }
                else{
                    elem.style.height = maxHeight + 'px';
                    flex.className = 'less'
                    flex.innerText = '浏览全部∨';
                }
            }
        }
    }
    article_flex();
</script> 
  <script type="text/javascript">
    $(function(){
        $("#dianping_submit").click(function(){

            var dianpuid = $(".dianpuid").val();
            var userid = $.cookie('_i') ? $.cookie('_i') : 0;
            var star = $(".star").val();
            var content = $("#content").val();
            var forbid_dianping = $(".forbid_dianping").val();
            var svalue = "0 " + (star-5)*15 + "px";


//            if(!userid){
//                alert("请先登录，再评价");
//                return false;
//            }

            if(forbid_dianping==1) {
                layer.alert("不能对自己的店铺进行点评");
            }
            if (forbid_dianping==2) {
                layer.alert("您今天点评的太多了，明天再来吧");
            }
            if (forbid_dianping==3) {
                layer.alert("您已经点评过此店铺了");
            }
            if (forbid_dianping != 0) {
                return false;
            }

            if(star == 0){
                $(".startip").html("<span style='color:#f00;'>请给店铺评分</span>");
                return false;
            }

            if($.trim(content) == ''){
                $(".dianpingtip").html("<span style='color:#f00;'>请给店铺点评</span>");
                return false;
            }

            $.ajax({
                type:'post',
                dataType: 'json',
                url:"http://shanghai.liebiao.com/article/dianpudpinsert/",
                data: {dianpuid:dianpuid,userid:userid,star:star,content:content},
                success: function(data) {

                    if(data.error == 0){
                        layer.alert("点评成功");

                        var username = getCookie('_u') ? getCookie('_u') : "匿名";
                        var lihtml= "";
                        lihtml = lihtml + "<li><div class='produce'>";
                        lihtml = lihtml + "<p class='produce-title'>";
                        lihtml = lihtml + "<span class='user-name'>" + username + "</span>";
                        lihtml = lihtml + "<i style='background-position:" + svalue + "'></i>";
                        lihtml = lihtml + "<span class='day'>" + data.date + "</span>";
                        lihtml = lihtml + "</p>";
                        lihtml = lihtml + "<p class='word'>" + content + "</p>";

                        lihtml = lihtml + "</div></li>";
                        $(".pingjia li").first().before(lihtml);
                        $(".pingjia li").eq(2).nextAll().hide();
                        $(".pingjia li").last().show();
                        $("#content").val("");
                        $(".forbid_dianping").val("3");
                        $(".in-left i").css("background-position", "0 -75px");
                        $(".star").val("0");
                    } else {

                    }
                }
            });

            return false;
        });

        $("#content").keyup(function(){
            $(".dianpingtip").html("");
        });

        $(".in-left i span strong").click(function(){
            var myindex = $(this).index();
            var posy = "0 0px";
            var star = 5;
            var startip = "";

            switch(myindex){
                case 0 :
                    posy = "0 -61px";
                    startip = "1分：非常不满意";
                    star = 1;
                    break;
                case 1 :
                    posy = "0 -45px";
                    startip = "2分：比较不满意";
                    star = 2;
                    break;
                case 2 :
                    posy = "0 -30px";
                    startip = "3分：一般";
                    star = 3;
                    break;
                case 3 :
                    posy = "0 -15px";
                    startip = "4分：比较满意";
                    star = 4;
                    break;
                default :
                    posy = "0 0px";
                    startip = "5分：非常满意";
                    star = 5;
                    break;
            }

            $(".in-left i").css({"background-position":posy});
            $(".startip").html(startip);
            $(".star").val(star);

        });

        var numli = $(".pingjia li").size();
        var isshow = 0;

        if(numli > 4)
        {
            $(".pingjia li").eq(2).nextAll().hide();
            $(".pingjia li").last().show();

            $(".pingjia li.more").click(function(){
                $(".pingjia li").eq(2).nextAll().toggle();
                $(".pingjia li").last().show();

                if(isshow == 0){
                    $(".pingjia li.more a").html("收起∧");
                    isshow = 1;
                } else {
                    var mtop = $(".dianping").offset().top+38;
                    $("html,body").scrollTop(mtop);
                    $(".pingjia li.more a").html("更多∨");
                    isshow = 0;
                }
            });
        }
        else
        {
            $(".pingjia li.more").css({"display":"none"});
        }
    });

