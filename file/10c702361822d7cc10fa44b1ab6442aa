    $.ajax({
        type:'get',
        url:"http://shanghai.liebiao.com/ajax/postclicks/?postid=316419855",
        cache:false,
        dataType:'json',
        success: function(json) {
            if (!json.error) {
                $('#post_visitors').html(json.msg.postclicks);
