<script>
		var lastTime=1;
		var allData=[];
		function _getNowTask(){
			var data={};
			if (lastTime==1){
				data = {frist:1};
			}else
				data = {lastTime:lastTime};
				
			$.post('/ajax/getNowTask.php',data,function(request){
				lastTime = request.nowLookTime;
				_addData(request.data);
			},'json');
		}
		
		function _addData(newData){
			var count=0;
			var str,newStr;
			if (newData!=null){
				str=_renderData(allData,false);
				newStr = _renderData(newData,true);
				$('.ajax_i li').remove();
				$('.c_info .bt2').after(str);
				$('.c_info .bt2').after(newStr);
				$("[info='new']").fadeIn(2000);
				if (allData.length==0)
					allData = newData;
				else{
					for(var i=newData.length-1;i>=0;i--)
						allData.unshift(newData[i]);
				}
				allData = allData.slice(0,7);
			}else{
				str=_renderData(allData,false);
				$('.ajax_i li').remove();
				$('.c_info .bt2').after(str);
			}
		}
		
		function _renderData(Data,type){
			var str='',info='';
			if (type){
				info='info="new" style="display:none"';
			}
			for(var key in Data){
				str +="<li "+info+" ><a href='/user/info/?username="+encodeURI(Data[key].susername)+"' class='lanse' target='_blank'>"+Data[key].susername+"</a><span style='float:left'>  发布任务获得信誉 <strong class='chengse'>+1</strong></span><span class='date'>"
				if (lastTime-Data[key].ctimestamp>3000)
					 str+='';
				 else if (lastTime-Data[key].ctimestamp<60)
					 str+=lastTime-Data[key].ctimestamp+3+'秒前';
				 else
					 str+=parseInt((lastTime-Data[key].ctimestamp)/60)+'分钟前';
				 str+= '</span></li>';
			}
			return str;
		}
		//setInterval(_getNowTask,3000);
		

		
	$(document).ready(         
        function(){
			$("#m_banner").hover(function(){
				$("#btn_prev,#btn_next").fadeIn();
			},function(){
				$("#btn_prev,#btn_next").fadeOut();
			})
			dragBln = false;
			$("#slider").touchSlider({
				flexible : true,
				speed : 400,
				btn_prev : $("#btn_prev"),
				btn_next : $("#btn_next"),
				paging : $("#slider_nav a"),
				counter : function (e) {
					$("#slider_nav a").removeClass("activeSlide").eq(e.current-1).addClass("activeSlide");
				}
			});
			$("#slider").bind("mousedown", function() {
				dragBln = false;
			})
			$("#slider").bind("dragstart", function() {
				dragBln = true;
			})
			$("#slider a").click(function() {
				if(dragBln) {
					return false;
				}
			})
			timer = setInterval(function() { $("#btn_next").click();}, 10000);
			$("#m_banner").hover(function() {
				clearInterval(timer);
			}, function() {
				timer = setInterval(function() { $("#btn_next").click();}, 10000);
			})
			$("#m_banner").bind("touchstart", function() {
				clearInterval(timer);
			}).bind("touchend", function() {
				timer = setInterval(function() { $("#btn_next").click();}, 10000);
			})
		});


		</script>
        
