<script >
$(window).scroll( function(){
	var z_userinfo;
	var h = $(this).scrollTop(),o=$('._bottom_bg,._bottom');
	if (h>=160 && z_userinfo==null ){
		o.show();
	}else
		o.hide();
});
$('._c_cl').click(function(){
	$('._bottom_bg,._bottom').hide();
})
