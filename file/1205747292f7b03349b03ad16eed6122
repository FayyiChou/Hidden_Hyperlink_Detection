	function changeBarBg(index)
	{
		var subMenu = document.getElementById("subMenuId"+index);
		var menu = document.getElementById("menuId"+index);
		for(var i=1;i<=5;i++)
		{
			document.getElementById("subMenuId"+i).style.display="none";
			document.getElementById("menuId"+i).style.backgroundImage="url(../../images/xwk/"+i+".gif)";
		}
		menu.style.backgroundImage="url(../../images/xwk/1"+index+".gif)";
		subMenu.style.display="";
	}
	function changeBarBgds(index)
