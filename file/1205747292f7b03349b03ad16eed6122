	 function check_search() {
	 	var txt_search = document.getElementById("head1_txt_search");
	 	if (txt_search.value == "")
	 	{
	 		alert("请输入新闻标题！");
	 		txt_search.focus();
	 		return false;
	 	} else
	 	{
	 		window.open("../news/news.aspx?Search=" + document.getElementById("head1_txt_search").value);
		 }
          }
         
