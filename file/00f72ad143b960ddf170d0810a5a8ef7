<script language="JavaScript">
            var w33imgUrl = new Array();
            var w33imgLink = new Array();
            var w33imgTitle = new Array();

            var w33count = 0;
            var w33running = false;
            var w33index = 0;
            var w33nextAdTimer;
            var w33k = 0;
            function w33nextAd(num) {
                if (num >= 0 && num < w33count - 1) {
                    w33index = num;
                    clearTimeout(w33nextAdTimer);
                } else {
                    num = w33index;
                }
                if (w33index > w33count - 1) {
                    w33index = 0;
                }
                if (!w33running) {
                    w33running = true;

                } else if (document.all) {
                    w33imgShow.filters.revealTrans.Transition = 50;
                    w33imgShow.filters.revealTrans.apply();
                    w33imgShow.filters.revealTrans.play();
                }
                document.getElementById('w33imgShow').src = w33imgUrl[w33index];
                if (w33imgTitle[w33index] != '') {
                    document.getElementById('w33imgTitle').innerHTML = '<a href="javascript:w33openWindow();" title="' + w33imgTitle[w33index] + '">' + w33imgTitle[w33index] + '</a>';
                }
                document.getElementById('w33imgNumber').innerHTML = "";
                w33k = 0;
                for (var i = 0; i < w33count; i++) {
                    w33k++;
                    if (i != w33index) {
                        document.getElementById('w33imgNumber').innerHTML += '<a href="javascript:w33nextAd(' + i + ')" class="nomal">' + w33k + '</a>';
                    } else {
                        document.getElementById('w33imgNumber').innerHTML += '<a href="javascript:w33nextAd(' + i + ')" class="current">' + w33k + '</a>';
                    }
                }

                w33index++;
                if (w33count > 1) {
                    w33nextAdTimer = setTimeout("w33nextAd(-1)", 4000);
                }
            }

            function w33openWindow() {
                var jumpUrl = w33imgLink[w33index - 1];
                if (jumpUrl && jumpUrl != '') {
                    window.open(jumpUrl);
                }
            }

            $().ready(function() {
                try {
                    var objs = w33imgJsons;
                    w33count = objs.length;
                    if (w33count == 0) {
                        document.getElementById('w33imgShow').style.display = "none";
                        document.getElementById('w33imgNumber').style.display = "none";
                        document.getElementById('w33imgTitle').style.display = "none";
                        return;
                    }
                    for (var i = 0; i < objs.length; i++) {
                        w33imgUrl[i] = objs[i].src;
                        w33imgLink[i] = objs[i].url;
                        w33imgTitle[i] = objs[i].title;
                    }
                    w33nextAd(-1);
                } catch (e) {

                }
            });
