<script type="text/javascript" language="javascript">
            function Check(obj){
                var frm = document.getElementById(obj);
                var inputArr = frm.getElementsByTagName("input");
                for (var i = 0; i < inputArr.length; i++) {
                    if(inputArr[i].getAttribute("checkexpession") == null || inputArr[i].getAttribute("err") == null){
                        continue;
                    }
                    if (inputArr[i].type == "text" && inputArr[i].getAttribute("checkexpession").toLowerCase() == "notnull" && (inputArr[i].value == null || inputArr[i].value == "" || inputArr[i].value == inputArr[i].getAttribute("err"))) {
                        inputArr[i].value = inputArr[i].getAttribute("err");
						return false;
                    }
                }
				return true;
            }
 </script>
