<script type="text/javascript" language="javascript">
     function keydown() 
        { 
            if(event.keyCode==13) 
            {
                var linkurl = '';
                var keyword = document.getElementById('essearchtbn_SearchTB').value
                var target = '_self';
                if(target == "")
                {
                    target = "_blank";
                }
		        document.getElementById("akeyword").target=target;
                var akey = document.getElementById("akeyword");
                if (linkurl == "")
                {
		            akey.href='Search.aspx?KeyWord='+ keyword;
                }
                else
                {
                    akey.href="'"+linkurl+"?KeyWord="+ keyword+"'";
                }
                akey.click();
            }
         }
