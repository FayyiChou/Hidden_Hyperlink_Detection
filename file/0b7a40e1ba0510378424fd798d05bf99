<script type="text/javascript" language="javascript">
    function JsMethod(data, id) {//data,{key:value,key:value};id,value
        if(data == null || data == "")
        {
            return;
        }
        data.jsMethod  = 'JsMethod';
        $("#"+id).html("<span style=\"color:blue;font-size:12px;\">数据加载中...</span>");
        $.ajax({
            type: "GET",
            url: "http%3a%2f%2fwww.hhxjy.net%2findex.aspx",
             data: data,
             dataType: "html",
             async:true,
             cache:false,
             success: function(msg){
                 if(msg.indexOf("DOCTYPE html PUBLIC") > -1)
                 {
                     $("#"+id).html("<span style=\"color:red;font-size:12px;\">没有更多数据</span>");
                 }else{
                     $("#"+id).html(msg);
                 }
             }
        });
     }
     //---用于投票-------
    function consumePoint (obj){
        $.ajax({
            type: "POST",
            url: "/ESControls/Templated/ConsumePoint.ashx",
            dataType: "text",
            data: "ID=" + obj + "&Code=" + $("#txtCode").val(),
            async:true,
            cache:false,
            success: function(msg){
                if(msg.indexOf("成功") == -1){
                    alert(msg);
                }else{
                    alert("投票成功！");
                    window.location.href = window.location.href;
                }
            }
        });
     }
    
    //---用于举报-------
    function reportNum(id){
        $.ajax({
            type: "POST",
            url: "/ESControls/Templated/Report.ashx",
            dataType: "text",
            data: "ID=" + obj + "&Code=" + $("#txtCode").val(),
            async:true,
            cache:false,
            success: function(msg){
                if(msg.indexOf("成功") == -1){
                    alert(msg);
                }else{
                    alert("举报成功！");
                    window.location.href = window.location.href;
                }
            }
        });
    }
</script>

