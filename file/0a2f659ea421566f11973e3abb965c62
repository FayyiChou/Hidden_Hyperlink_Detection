		 <%=n.address%></p></li></script><script type="text/javascript">	var id=23153;
	var mapLng=110.426396;
	var mapLat=20.001754;
	var point = new BMap.Point(mapLng,mapLat);
	//全景
	var panorama = new BMap.Panorama('panorama');
	panorama.setPosition(point);
	//地图标记
	var mp = new BMap.Map('l-map');    			
	mp.centerAndZoom(point, 14);
	mp.enableScrollWheelZoom();
	setOriginal(id);
	// 复杂的自定义覆盖物
    function ComplexCustomOverlay(point, text,message){
      this._point = point;
      this._text = text;
	  this._message = message;
    }
    ComplexCustomOverlay.prototype = new BMap.Overlay();
    ComplexCustomOverlay.prototype.initialize = function(map){
      this._map = map;
      var div = this._div = document.createElement("div");
      div.style.position = "absolute";
      div.style.zIndex = BMap.Overlay.getZIndex(this._point.lat);
      div.style.backgroundColor = "#EE5D5B";
      div.style.border = "1px solid #BC3B3A";
      div.style.color = "white";
      div.style.height = "18px";
      div.style.padding = "2px";
      div.style.lineHeight = "18px";
      div.style.whiteSpace = "nowrap";
      div.style.MozUserSelect = "none";
      div.style.fontSize = "12px"
      var span = this._span = document.createElement("span");
      div.appendChild(span);
      span.appendChild(document.createTextNode(this._text));      
      var that = this;

      var arrow = this._arrow = document.createElement("div");
      arrow.style.background = "url(http://map.baidu.com/fwmap/upload/r/map/fwmap/static/house/images/label.png) no-repeat";
      arrow.style.position = "absolute";
      arrow.style.width = "11px";
      arrow.style.height = "10px";
      arrow.style.top = "22px";
      arrow.style.left = "10px";
      arrow.style.overflow = "hidden";
      div.appendChild(arrow);
       div.onmouseover = function(){
        this.style.backgroundColor = "#6BADCA";
        this.style.borderColor = "#0000ff";
		this.style.zIndex ="99999";
        arrow.style.backgroundPosition = "0px -20px";
      }
      div.onmouseout = function(){
        this.style.backgroundColor = "#EE5D5B";
        this.style.borderColor = "#BC3B3A";
		this.style.zIndex ="0";
        arrow.style.backgroundPosition = "0px 0px";
      } 
	  div.onclick = function(){
		myInfo(that._message);
		
	  }
      mp.getPanes().labelPane.appendChild(div);
      return div;
    }
    ComplexCustomOverlay.prototype.draw = function(){
      var map = this._map;
      var pixel = map.pointToOverlayPixel(this._point);
      this._div.style.left = pixel.x - parseInt(this._arrow.style.left) + "px";
      this._div.style.top  = pixel.y - 30 + "px";
    }
	function setOriginal(id){
		mp.clearOverlays();
		mp.centerAndZoom(point, 14);
		$.post("/index.php?g=home&m=Myditu&a=search", {id:id},
		function(data){
			var obj = data;
			if(obj.status==1){
				var n=obj.data;
				addbiaoji(n);
			}else{
				//alert("无法找到该楼盘");
			}
		});
	}
	
	//添加周围楼盘
	function addMyOverlay(){
		setOriginal(id);
		var bs = mp.getBounds();   //获取可视区域
		var bssw = bs.getSouthWest();   //可视区域左下角
		var bsne = bs.getNorthEast();   //可视区域右上角
		$.post("/index.php?g=home&m=Myditu&a=getBiaozhu", {aLng:bssw.lng,aLat:bssw.lat,bLng:bsne.lng,bLat:bsne.lat},
		function(data){
			var obj = data;
			  window.listDate = obj.data;
			  $("#mylist").empty();
			  $.each(listDate, function(i, n){
					addbiaoji(n);
					buildingList(i,n);
			  });
		  });
	}
	//在地图上显示楼盘点
	function addbiaoji(n){
		var txt = n.title;
		var myCompOverlay = new ComplexCustomOverlay(new BMap.Point(n.mapLng,n.mapLat), txt,n);
		mp.addOverlay(myCompOverlay);
	}
   //显示医院、景点等信息
	function getKeys(myKeys){
		document.getElementById('r-result').style.display = "";
		document.getElementById('r-result1').style.display = "none";
		var myKeys = myKeys;
		var local = new BMap.LocalSearch(mp, {
			renderOptions:{map: mp, panel:"mylist"}
		});
		local.searchInBounds(myKeys, mp.getBounds());
	}
	//返回到首页显示
	function setFirst(){
		document.getElementById('r-result').style.display = "none";
		document.getElementById('r-result1').style.display = "";
		setOriginal(id);
	}
	//弹出信息框
	function myInfo(n){
		var sContent = '<div class="t_box"><div class="boxt"><h3 class="all366C">[<a target="_blank" href="http://xiaoqu.wofang.com/ct_'+n.city+'/">'+n.cityName+'</a>] <a target="_blank" href="http://xiaoqu.wofang.com/'+n.id+'/">'+n.title+'</a></h3></div><div class="boxm"><div class="m1"><div class="txt"><ul><li><em>小区类型：</em>'+n.type+'</li><li>均价：'+n.sprice+'元/㎡</li><li><em>小区环境：</em><i>'+n.around+'</i></li><li><em>小区地址：</em> '+n.address+'</li><li><em>开发商：</em> '+n.developer+'</li></ul></div><div class="pic"><a target="_blank" href="http://xiaoqu.wofang.com/'+n.id+'/"><img src="http://images.wofangwang.com'+n.icon+'" width="151" height="99" alt="那个谁"></a><span class="rexiao_1"></span><p class="all366C"><a target="_blank" href="http://xiaoqu.wofang.com/'+n.id+'/">进入小区详情页>></a></p></div></div><div class="m2"></div></div></div>';
		var opts = {
			offset:new BMap.Size(-5, -28)
		}
		var infoWindow = new BMap.InfoWindow(sContent,opts); // 创建信息窗口对象
		mp.openInfoWindow(infoWindow, new BMap.Point(n.mapLng,n.mapLat)); // 打开信息窗口	
	}
	function buildingList(i,n){
		var data=new Object();
		data.n = n;
		data.i = i;
		var bt=baidu.template;
		var html=bt('newslist',data);
		$("#mylist").append(html);
		document.getElementById('r-result').style.display = "";
		document.getElementById('r-result1').style.display = "none";
	}
	function tomyInfo(i){
		$("#mylist li").removeClass("blue");
		$("#blist_"+i).addClass("blue");
		myInfo(listDate[i]);
	}	
	
</script></div></div></div></div><script type="text/javascript" src="http://statics.wofangwang.com/home/js/lists.js"></script><div class="blank10"></div><div class="seltion w1220"><div class="seltion-cont"><div class="seltion-m"><dl class="selitem selitem-area lh24 clearfix"><dt>区域：</dt><dd class="posrelative"><div class="area clearfix"><a href="http://xiaoqu.wofang.com/" class="a-circle-on cur">不限</a><a href="http://xiaoqu.wofang.com/ct_38/" class="">海口</a><a href="http://xiaoqu.wofang.com/ct_39/" class="">三亚</a><a href="http://xiaoqu.wofang.com/ct_318/" class="">琼海</a><a href="http://xiaoqu.wofang.com/ct_3241/" class="">博鳌</a><a href="http://xiaoqu.wofang.com/ct_322/" class="">儋州</a><a href="http://xiaoqu.wofang.com/ct_330/" class="">陵水</a><a href="http://xiaoqu.wofang.com/ct_317/" class="">文昌</a><a href="http://xiaoqu.wofang.com/ct_324/" class="">澄迈</a><a href="http://xiaoqu.wofang.com/ct_319/" class="">万宁</a><a href="http://xiaoqu.wofang.com/ct_323/" class="">临高</a><a href="http://xiaoqu.wofang.com/ct_325/" class="">定安</a><a href="http://xiaoqu.wofang.com/ct_331/" class="">保亭</a><a href="http://xiaoqu.wofang.com/ct_320/" class="">五指山</a><a href="http://xiaoqu.wofang.com/ct_332/" class="">乐东</a><a href="http://xiaoqu.wofang.com/ct_326/" class="">屯昌</a><a href="http://xiaoqu.wofang.com/ct_321/" class="">东方</a><a href="http://xiaoqu.wofang.com/ct_329/" class="">琼中</a><a href="http://xiaoqu.wofang.com/ct_327/" class="">昌江</a><a href="http://xiaoqu.wofang.com/ct_328/" class="">白沙</a></div></dd><dd class="clear area-son" style="display:none;"><div class="area clearfix"><a href="http://xiaoqu.wofang.com/" class="cur" rel="nofollow">全部</a></div></dd></dl><dl class="selitem lh24 clearfix js-condition"><dt>类型：</dt><dd class="dd-sale-price"><a href="http://xiaoqu.wofang.com/" rel="nofollow" class="cur a-circle-on">不限</a><a href="http://xiaoqu.wofang.com/te_1/" rel="nofollow">普通住宅</a><a href="http://xiaoqu.wofang.com/te_2/" rel="nofollow">写字楼</a><a href="http://xiaoqu.wofan