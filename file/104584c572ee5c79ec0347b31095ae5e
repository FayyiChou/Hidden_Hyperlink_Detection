        return saleCarLink;
    }
    var sfcity = ''; 
       //设置卖车链接
    var SetSaleCarLink1 = function (tagId,pvid) {
        var curArea = getCookie('userarea', 0);
        if (curArea == 110000 || curArea == 310000 || curArea == 120000 || curArea == 500000)//如果为城市为直辖市特殊处理
        {
            curArea = parseInt(curArea) + 100;
        }
        var saleCarLink = loadsolution(curArea) + pvid;
        if (sfcity != '') { 
            if (sfcity.indexOf(',' + curArea + ',') > -1) {
                $("#" + tagId).attr("href", saleCarLink);
            }
        } else {
            $.ajax({
                type: "GET",
                url: '/handler/AuctionSale/getcitys.ashx?cityid=' + curArea,
                dataType: 'json',
                async: true,
                success: function (result) {
                    if (result.code == "0") {
                        sfcity = result.message;
                        var saleCarLink = loadsolution(curArea) + pvid;
                        if (sfcity.indexOf(',' + curArea + ',') > -1) {
                            $("#" + tagId).attr("href", saleCarLink);
                        }
                    }
                }
            });
        }
     
    };
    SetSaleCarLink1("aSaleCar", "#pvareaid=100995");
    SetSaleCarLink1("minihead_caradd", "#pvareaid=101217");
