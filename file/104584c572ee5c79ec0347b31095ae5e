<script>


    function tipError(errid, content) {
        if (content) {
            $('#' + errid).html("<span class='icon-error'><i class='iconfont iconfont-close'>&#xe90c;</i></span>" + content).show();;
        }
        else {
            $('#' + errid).empty();
        }
    };

    function popBottom2Show()
    {        
        var _data = new Object();
        if(objecttype!=0)
        {
            _data.info = infoid+"|1";
        }else
        {
            _data.info = infoid
        }
        _data.eventkey = "c_pc_2sc_cardetail_freecallclick";
        $.ajax({
            url: "//collectionpv.che168.com/collect/page_event.ashx",
            type: "get",
            dataType: "jsonp",
            data: _data                
        })                    
        $("#400Phone").find("a").eq(0).addClass("btn-free-click")
        $("#showSendPhone").show();
    }
    function popBottom2Hide() {
        $("#400Phone").find("a").eq(0).removeClass("btn-free-click")
        $("#showSendPhone").hide();
    } 
    
    var isSumit=true;
    //提交确认电话        
    function clickCallBack() {
        $("#submitCallBack").unbind('click',clickCallBack);
        var userPhone = $("#userPhone").val();
        var infoid =18371128;        
        var LDealerId=$("#car_dealerid").val();
        var SeriesId=$("#car_seriesid").val();
        var SpecId=$("#car_specid").val();
        var linkmanId=$("#car_LinkmanId").val();
        var callbackphoneTime=getCookie('callbackphoneTime');
        var code=$('#phoneCode').val();

        var isValid = true; //通过验证
        if(!ValidMobile(userPhone)) {
            isValid = false;
            $("#submitCallBack").bind('click',clickCallBack);
        }

        //统计埋点
        var _data = new Object();
        _data.info = infoid;
        _data.ref = escape(document.referrer);
        _data.cur=escape(location.href);
        if(LDealerId > 0)
        { _data.eventkey = "c_pc_2sc_cardetail_phone_confirm";

        }else{
            _data.eventkey = "c_pc_2sc_grcardetail_phone_confirm";
        } 
        $.ajax({
            url: "//collectionpv.che168.com/collect/page_event.ashx",
            type: "get",
            dataType: "jsonp",
            data: _data                
        });
        //第一次记录回拨电话时间cookie
        if(callbackphoneTime==null||callbackphoneTime.split('|')[1]!=infoid){
            isSumit=true;
        }else if(callbackphoneTime.split('|')[1]==infoid){
            isSumit=false;
            //10分钟之内不能 2次提交
            $("#submitCallBack").bind('click',clickCallBack);
            var minutes=parseInt((new Date().getTime() - callbackphoneTime.split('|')[0])/(1000*60));
            if(minutes>=10)
            {
                isSumit=true;                
            }else{
                // alert("您的呼叫过于频繁，休息一下再打吧。");     
                tipError('userPhoneErrorTip', '您的呼叫过于频繁，休息一下再打吧。');
            }
        }
        var uname='';
        if(window.memberBindName){
            uname = window.memberBindName;
        }
        if(isValid&&isSumit)//判断用户电话~和10分钟之内的cookie是否有效
        {  
            setCookie('callbackphoneTime', new Date().getTime()+'|'+infoid, { path: '/', expires: new Date(new Date().getTime() + 24 * 60 * 60 * 30), domain:location.hostname });
            //setCookie("callbackphoneTime", new Date().getTime()+'|'+infoid);
            $.post("/Handler/CallbackPhone.ashx", { userphone: userPhone, infoid: infoid ,Dealerid:LDealerId,SeriesId:SeriesId,SpecId:SpecId,linkmanId:linkmanId,url: location.href, ref: document.referrer,code:code,rphone: window.memberBindPhone,ischeck:window.memberBindIscheck,uname:uname }, function (obj) {
                $("#submitCallBack").bind('click',clickCallBack);
                var json=eval("("+obj+")");
                if(json.result==1)
                {                  
                    isSumit=false;       
                    //统计埋点
                    var _data = new Object();
                    _data.info = infoid;
                    _data.ref = escape(document.referrer);
                    _data.cur=escape(location.href);
                    _data.eventkey = "c_pc_2sc_cardetail_sendcallclick";
                    $.ajax({
                        url: "//collectionpv.che168.com/collect/page_event.ashx",
                        type: "get",
                        dataType: "jsonp",
                        data: _data                
                    });
                    Pop.Dialog("free-pop");                    
 
                    $('#phoneCode').val('');
                    $("#userPhone").val(userPhone ); 
                    $("#input_myPhone").val(userPhone );  
                    $("#input_myPhone_ForGuandong").val(userPhone);   
                    var ischeck =0; 
                    var ischeck = window.memberBindIscheck;
                    if(LDealerId > 0){//商家回呼
                        ischeck=1;
                    }else{//个人回呼时如果和上一次手机号一样
                        if(userPhone != window.memberBindPhone){
                            ischeck = 0;
                        }
                    }
                    window.memberBindPhone=userPhone;
                    window.memberBindIscheck=ischeck; 
						
                    if(memberBindIscheck == 0){
                        $("#li_myCode1").show();
                        $("#div_phoneCode").show();
                        $("#li_myCode1_ForGuandong").show();
                    }else{
                        $("#li_myCode1").hide();
                        $("#div_phoneCode").hide();
                        $("#li_myCode1_ForGuandong").hide();
                    }  
                   
                    setCookieNoEnCode('memberPhoneInfo', encodeURIComponent( name + "|" + userPhone+"|"+ischeck), 24 * 60 * 60 * 30, '/', window.location.hostname);

                    return;
                }else
                {
                    tipError('userPhoneErrorTip', json.message);
                    return;
                }
            });            
        }             
    }
    var isSumit_ForGuandong=true;
    //提交确认电话（广东地区（除东莞外）商家询问底价）        
    function clickCallBack_ForGuandong(){    
        $('#yjsure_ForGuandong').unbind('click', clickCallBack_ForGuandong);
        setCookie('callbackphoneTime', new Date().getTime()+'|'+'1233123', { path: '/', expires: new Date(new Date().getTime() + 24 * 60 * 60 * 30), domain:location.hostname });
        var userPhone = $("#input_myPhone_ForGuandong").val();
        var infoid =18371128;        
        var LDealerId=$("#car_dealerid").val();
        var SeriesId=$("#car_seriesid").val();
        var SpecId=$("#car_specid").val();
        var linkmanId=$("#car_LinkmanId").val();
        var callbackphoneTime=getCookie('callbackphoneTime');
        var code=$('#input_myCode_ForGuandong').val();
        var isValid = true; //通过验证

        //统计埋点
        var _data = new Object();
        var _userPhone=userPhone;
        if(!ValidMobile(userPhone)) {
            _userPhone = '';
            isValid = false;
            $('#yjsure_ForGuandong').bind('click', clickCallBack_ForGuandong);
        }
        _data.info = infoid+'|'+_userPhone;
        _data.ref = escape(document.referrer);
        _data.cur=escape(location.href);
        _data.eventkey = "c_pc_2sc_cardetail_gdcallback_submit";
        $.ajax({
            url: "//collectionpv.che168.com/collect/page_event.ashx",
            type: "get",
            dataType: "jsonp",
            data: _data                
        });
        //第一次记录回拨电话时间cookie
        if(callbackphoneTime==null||callbackphoneTime.split('|')[1]!=infoid){
            isSumit_ForGuandong=true;
        }else if(callbackphoneTime.split('|')[1]==infoid){
            $('#yjsure_ForGuandong').bind('click', clickCallBack_ForGuandong);
            isSumit_ForGuandong=false;
            //10分钟之内不能 2次提交
            var minutes=parseInt((new Date().getTime() - callbackphoneTime.split('|')[0])/(1000*60));
            if(minutes>=10)
            {
                isSumit_ForGuandong=true;                
            }else{
                alert("您的呼叫过于频繁，休息一下再打吧。");                
            }
        }
        var uname='';
        if(window.memberBindName){
            uname = window.memberBindName;
        }
        if(isValid&&isSumit_ForGuandong)//判断用户电话~和10分钟之内的cookie是否有效
        {  
            $.post("/Handler/CallbackPhone.ashx", { userphone: userPhone, infoid: infoid ,Dealerid:LDealerId,SeriesId:SeriesId,SpecId:SpecId,linkmanId:linkmanId,url: location.href, ref: document.referrer,code:code,type:1,rphone: window.memberBindPhone,ischeck:window.memberBindIscheck,uname:uname}, function (obj) {
                $('#yjsure_ForGuandong').bind('click', clickCallBack_ForGuandong);
                var json=eval("("+obj+")");
                if(json.result==1)
                {            
                    
                    setCookie('callbackphoneTime', new Date().getTime()+'|'+infoid, { path: '/', expires: new Date(new Date().getTime() + 24 * 60 * 60 * 30), domain:location.hostname   });
                    isSumit_ForGuandong=false;    
                    Pop.Dialog.Hide('askPriceTip_ForGuandong');
                    Pop.Dialog("askPriceSuccess_ForGuandong");     

                    $("#hdphone").val($("#input_myPhone_ForGuandong").val());      
                    $('#input_myCode_ForGuandong').val(''); 
                    $('#phoneCode').val('');
                    $("#userPhone").val(userPhone ); 
                    $("#input_myPhone").val(userPhone );  
                    $("#input_myPhone_ForGuandong").val(userPhone);   
                    var ischeck =0; 
                    var ischeck = window.memberBindIscheck;
                    if(LDealerId > 0){//商家回呼
                        ischeck=1;
                    }else{//个人回呼时如果和上一次手机号一样
                        if(userPhone != window.memberBindPhone){
                            ischeck = 0;
                        }
                    }
                    window.memberBindPhone=userPhone;
                    window.memberBindIscheck=ischeck; 
						
                    if(memberBindIscheck == 0){
                        $("#li_myCode1").show();
                        $("#div_phoneCode").show();
                        $("#li_myCode1_ForGuandong").show();
                    }else{
                        $("#li_myCode1").hide();
                        $("#div_phoneCode").hide();
                        $("#li_myCode1_ForGuandong").hide();
                    }   
                    setCookieNoEnCode('memberPhoneInfo', encodeURIComponent(uname + "|" + userPhone+"|"+ischeck), 24 * 60 * 60 * 30, '/', window.location.hostname);

                    return;
                }else
                {
                    //alert(json.message);   
                    //tipError('userPhoneErrorTip', json.message);
                    Pop.Dialog.Hide('askPriceTip_ForGuandong'); 
                    return;
                }
            });            
        }             
    }    
    //验证用户输入的手机号
    function ValidMobile (phone) {
        var m = $.trim(phone);        
        var reg = new RegExp("^1[3,4,5,6,7,8,9]{1}[0-9]{9}$", "ig");
        if (m == '') {
            alert("请输入您的手机号码");
            return false;
        } else if (!reg.test(m)) {
            alert("请填写正确的手机号");
            
            return false;
        }
        return true;
    };
    var objecttype=0;
    //异地红包取消
    function ClosePop5()
    {
        var _dataRed = new Object();
        _dataRed.info = 18371128;
        if($("#car_dealerid").val()>0)
        {
            _dataRed.eventkey = "c_pc_2sc_cardetail_bjiancehongbao_cancel";//商家
        }
        else
        {
            _dataRed.eventkey = "c_pc_2sc_cardetail_pjiancehongbao_cancel";//个人
        }
        $.ajax({
            url: "//collectionpv.che168.com/collect/page_event.ashx",
            type: "get",
            dataType: "jsonp",
            data: _dataRed                
        });
        Pop.Dialog.Hide('layer5');
    }
</script>
<script type="text/javascript">
    //取用户信息和认证车信息
    var carOwner = function ($) {
        var carName = dtcommon.carinfo.carname;
        var specId = parseInt(dtcommon.carinfo.specid);
        var userId = dtcommon.carinfo.memberid;
        
        //var specId = 7368; 
        //var userId = 10002;
        var getUserInfoUrl = "//cacheapi.che168.com/userinfo/getauthuserinfo.ashx?userids=" + userId;
        
        var topicCount = 3;   // 获取帖子数
        var timeout = 2000; // 接口超时时间，单位：毫秒
        var carOwnerId = "#divCarOwner";
        var carOwnerContentId = "#divCarOwnerContent";
        var acOwnerId = "#ac_owner";
        
        var getUserHead = function (headImageUrl) {
            try
            {
                if (headImageUrl=='' || headImageUrl == null){
                    return '//x.autoimg.cn/space/images/head_120X120.gif';
                }
                if (headImageUrl.indexOf("//") == 0){
                    return headImageUrl;
                }
                headImageUrl = headImageUrl.replace('userheaders', '');
                var domain = "";
                var randArr=[0,1];
                var b = randArr[Math.floor((Math.random()*randArr.length))];
           
                if (headImageUrl.indexOf("autohomecar__") >= 0)
                {
                    b += 2;
                    domain = "//i" + b + ".autoimg.cn/usercenter/";
                }
                else
                {
                    domain = "//i" + b + ".autoimg.cn/album/userheaders";
                }
                return domain + headImageUrl;
            } catch(e)
            {
                return "//x.autoimg.cn/space/images/head_120X120.gif";
            }
        };

        // 获取用户昵称、头像
        var getUserInfo = function () {
            $.ajax({
                type: "GET", cache: true, dataType: "jsonp", url: getUserInfoUrl, timeout: timeout, jsonp: "_callback", jsonpCallback: "getUserInfoCallback",
                success: function (data) {                    
                    //个人信息
                    var content = [];
                    //个人车辆信息
                    var authcar = [];
                    if (data && data.authinfo && data.authinfo.length > 0) {
                        if (data.authinfo[0] && data.authinfo[0].returncode == 0 && data.authinfo[0].result.memberid > 0) {
                    
                            if(data.authinfo[0].result.iscarowner == 1){
                                /* 原有的内容
                                var userInfo = data.authinfo[0].result;
                                var link = "//i.autohome.com.cn/" + userId;
                                content.push('<div class="user-title">');
                                content.push('<i class="level"></i><span class="name">' + $("#car_LinkmanName").val() + '</span><i class="separated">|</i>汽车之家认证车主');
                                content.push('<i class="iconfont drop-down">&#xe916;</i>');
                                content.push('</div>');
                                content.push('<div class="user-dl">');
                                content.push('<div class="user-portrait">');
                                content.push('<a href="');
                                content.push(link);
                                content.push('" target="_blank">');
                                if(userInfo.picture==''){
                                    content.push('<img src="');
                                    content.push('//x.autoimg.cn/space/images/head_120X120.gif');
                                }else
                                {
                                    var headpic = getUserHead(userInfo.picture);
                                    content.push('<img src="');
                                    content.push(headpic);
                                }
                                content.push('" width="30" height="30"></a>');
                                content.push('<span href="');
                                content.push(link);
                                content.push('">');
                                content.push(userInfo.name);
                                content.push('</span>');
                                content.push('<i class="level"></i></div>');
                                if (data.authinfo[1] && data.authinfo[1].returncode == 0 && data.authinfo[1].result.list.length > 0) {
                             
                                    var specList = data.authinfo[1].result.list;
                                    for (var i = 0 ; i < specList.length; i++) {
                                        if (specList[i].status == 2) {
                                            authcar.push('<p>');
                                            authcar.push(specList[i].seriesname);
                                            authcar.push(' ');
                                            authcar.push(specList[i].specname);
                                            authcar.push('<span>认证</span>');
                                            authcar.push('</p>');
                                        }
                                    }
                                }

                                $(carOwnerContentId).html(content.join('') + '' + authcar.join('') + '</div>');
                                */ 

                                content.push('<i class="iconfont icon-renzheng">&#xe68d;</i><span class="name">' + $("#car_LinkmanName").val() + '</span><i class="separated">|</i>汽车之家认证车主');
                                 $(carOwnerContentId).html(content.join(''));

                            }else{
                                  
                                content.push("<div class='user-title'>");
                                content.push("<span class='name'>个人</span>");
                                if($("#car_LinkmanName").val()!=''){
                                    content.push("<i class='separated'>|</i>"+$("#car_LinkmanName").val()+"");
                                }
                                content.push("</div>");

                                $('#divIsAutoCar').html('').html(content.join(''));
                            }
                        }
                    }
                }
            });
        };
                
        getUserInfo();
    };
    //取用户信息和认证车信息
</script>
