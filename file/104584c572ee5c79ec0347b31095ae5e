<script type="text/javascript"> 
    function getCookiedecode (name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return decodeURIComponent(c.substring(nameEQ.length, c.length));
        } return null;
    };
    
    Array.prototype.indexOf = function (item) {
        for (var i = 0; i < this.length; i++) {
            if (this[i] == item) return i;
        }
        return -1;
    };
    /*获取用户提示信息*/
    function GetUserTip() {
        $.getScript("/Handler/index_v1/UserTip.ashx?cd=sltuser&callback=UserTipCallBack"); 
    };
    /*用户提示信息返回处理*/
    function UserTipCallBack(callData) {
        if (callData.error == 1) {
            if (callData.message.allmessage > 0) {
                $('#spanNotice').html('系统通知（<span class="reda">' + callData.message.allmessage + '</span> ）');
                var content = [];
                if (callData.message.waitpayordercount > 0) {
                    content.push('<a target="_blank" onclick="recordOrderTipClick();" href="//i.che168.com/autonomous/orderlist/">您有<span id="carstatecount" class="reda">');
                    content.push(callData.message.waitpayordercount);
                    content.push('</span>个预约定单未支付</a>');
                }

                if (callData.message.carstatecount > 0) {
                    content.push('<a target="_blank" href="//i.che168.com/notice/?t=1&amp;ref=status">网站卖的车有<span id="carstatecount" class="reda">');
                    content.push(callData.message.carstatecount);
                    content.push('</span>个新动态</a>');
                }

                if (callData.message.pricecount > 0) {
                    content.push('<a target="_blank" href="//i.che168.com/notice/?t=6&amp;ref=price">网站卖的车有<span id="pricecount" class="reda">');
                    content.push(callData.message.pricecount);
                    content.push('</span>个新报价</a>');
                }

                if (callData.message.qacount > 0) {
                    content.push('<a target="_blank" href="//i.che168.com/notice/?t=9&amp;ref=qa">我的问答有<span id="qacount" class="reda">');
                    content.push(callData.message.qacount);
                    content.push('</span>个新回复</a>');
                }
                if (callData.message.commentreturn > 0) {
                    content.push('<a target="_blank" href="//i.che168.com/notice/?t=11&amp;ref=qa">我的评论有<span id="qacount" class="reda">');
                    content.push(callData.message.commentreturn);
                    content.push('</span>个新回复</a>');
                }
                if (callData.message.commentdelete > 0) {
                    content.push('<a target="_blank" href="//i.che168.com/notice/?t=12&amp;ref=qa">删除评论有<span id="qacount" class="reda">');
                    content.push(callData.message.commentdelete);
                    content.push('</span>个新回复</a>');
                }
                $('#divTipContent').html(content.join(''));
                $('#usernote').show();
                $('#icon').show();
                var sltArea = new TipHover($('#usertip'), $('#usertip_msg'));
                sltArea.init();
            }
        }
    };

    /*退出登录*/
    function loginOff() {
        $('#nologin').show();
        $('#nologin1').show();
        $('#usertip').hide();
        $('#userinfo').hide();
        if ($('#rightNologin')) {
            $('#rightNologin').show();
            $('#loginUserName').hide();
            $('#loginUserBlock').hide();
            $('#personalLogin1').parent().show();
            $('#dealerlogin1').parent().show();
        }
    }

    /*个人退出*/
    function personalLoginOff() {
        $('#hrefsellcar').attr("href", "//i.che168.com/freecar/#pvareaid=100986");
        //$('#mybuycar').attr("href", "//i.che168.com/freecar/#pvareaid=100986");
        $('#mysave').attr("href", "//i.che168.com/freecar/#pvareaid=100986");

        var logoutChe168Url = "//account.che168.com/login/LogOut?Jsonbackvar=isLogOff&amp;date=" + new Date();
        var logoutAutoUrl = "//account.autohome.com.cn/login/LogOutJson?backvar=isAutoLogOff&amp;date=" + new Date();
        $.getScript(logoutChe168Url, function () {
            $.getScript(logoutAutoUrl, function () {
                loginOff();
                if (typeof CommentOut == "function") {
                    CommentOut()
                }
                setCookie('clubUserShow', '', { path: '/', expires: new Date(new Date().getTime() - 24 * 60 * 60 * 365 * 1000), domain: '.che168.com' });
                setCookie('pcpopclub', '', { path: '/', expires: new Date(new Date().getTime() - 24 * 60 * 60 * 365 * 1000), domain: '.che168.com' });
                setCookie('sessionLogin', '', { path: '/', expires: new Date(new Date().getTime() - 24 * 60 * 60 * 365 * 1000), domain: '.che168.com' });
                 
            });
        });
    };
    /*商家退出*/
    function dealerLoginOff() {
        $.getScript('//www.che168.com//Handler/LoginOut.ashx?async=1', function () {
            $('#personalmini').show();
            $('#dealermini').hide();
            loginOff();
            if (typeof CommentOut == "function") {
                CommentOut()
            }
            setCookie('2scDealerInfo', '', { path: '/', expires: new Date(new Date().getTime() - 24 * 60 * 60 * 365 * 1000), domain: '.che168.com' });
            setCookie('2scDealerId', '', { path: '/', expires: new Date(new Date().getTime() - 24 * 60 * 60 * 365 * 1000), domain: '.che168.com' });
        });
    };

    /*判断登录状态 loginType: 0 未登录，1 个人用户，2 商家用户登录*/
    var che168User = { loginType: 0, memberId: 0, dealerId: 0, userName: '' };
    function UserInfoReader() {
        var dealerId = getCookie('2scDealerId');
        if (dealerId && dealerId > 0) {
            che168User.dealerId = dealerId;
            che168User.userName = getCookiedecode('2scDealerName');
            che168User.loginType = 2;
        } else {
            var clubusershow = getCookiedecode('clubUserShow');
            if (clubusershow && clubusershow.length > 10) {
                var usersplit = clubusershow.split('|');
                if (usersplit.length < 8) { return; }
                che168User.memberId = usersplit[0];
                che168User.userName = usersplit[3];
                che168User.loginType = 1;
            } else {
                che168User.loginType = 0;
            }
        }
        if (che168User.loginType == 1) {
            $('#dealermini').hide();
            $('#personalmini').show();
            $('#usertip').show();
            $('#usename').html(che168User.userName);
            $('#nologin').hide();
            $('#userinfo').show();
            $('#hrefsellcar').attr("href", "//i.che168.com/car/auctionmanager/#pvareaid=101342");
            //$('#mybuycar').attr("href", "//i.che168.com/collection/");
            $('#mysave').attr("href", "//i.che168.com/collection/");
            //$.getScript("//i.che168.com/handler/GetUserOrderManager.ashx?userid=" + che168User.memberId + "&_callback= setUserBooking")

            GetUserTip();
        }
        else if (che168User.loginType == 2) {
            $('#dealermini').show();
            $('#personalmini').hide();
            $('#dealername').html(che168User.userName);
        } 
        $('#dealerLogin').attr("href", $('#dealerLogin').attr("href") + '?backurl=' + location.href);
    };
    function setUserBooking(data) {

        if (data.result.Item1 == 1) {
            var url = ""
            if (data.result.Item2 == "authentication") {
                url = "//i.che168.com/authentication/orderlist/";
            }
            else if (data.result.Item2 == "autonomous") {
                url = "//i.che168.com/autonomous/orderlist/";
            }
            if (url) {
                //$('#mybook').style.display = "block";
                $('#mybook').show();
                $('#mybook').attr("href", url);
            }

        }
        else {
            $('#mybook').hide();

        }
    }
    if (document.location.href.indexOf('sourcename=app.ipad') == -1) {
        UserInfoReader();
    }
    var SetSaleCarLink = function (tagId) {
        var curArea = getCookie('userarea');
        if (curArea == 0) curArea = getCookie('area');
        curArea = parseInt(curArea / 100) * 100;
        /* 添加置换信息 2015年2月27日
        var saleCarLink = "//i.che168.com/car/add/#pvareaid=100492";
         北京地区则跳转到卖车渠道选择页
        if (loadBlock(curArea)) {
            saleCarLink = "//www.che168.com/sell/1.html#pvareaid=100492";
        }*/
        var saleCarLink = '/sell/1.html#pvareaid=100492';
        if ($('#'+tagId)) {
            $('#' + tagId).attr("href",saleCarLink);
        }
    };

    /*鼠标划过事件 显示和隐藏层事件*/
    var TipHover =function (tgerDiv, showDiv, tgerDivClass, showDivClass, timerSeconds) {
        if (tgerDiv.size() > 0 && showDiv.size() > 0) {
            var curTgerDivClass = tgerDiv.attr("class");
            var curshowDivClas = showDiv.attr("class");
            this.init = function () { 
                tgerDiv.bind("mousemove", tipMove);
                showDiv.bind("mousemove", tip_msgMove);
                tgerDiv.bind("mouseout", tipOut);
                showDiv.bind("mouseout", tip_msgOut);

            };
            /*鼠标划过移出 用户信息提示*/
            var isuserMove = true;
            var showtimer = null;
            var tipMove = function () {
                if (showtimer != null) return;
                showtimer = setTimeout(function () {
                    showDiv.show();
                    if (tgerDivClass) tgerDiv.attr("class", tgerDivClass);
                    if (showDivClass) showDiv.attr("class", showDivClass); 
                    showtimer = null;
                }
                , 150);
                if (timer != null) { clearTimeout(timer); timer = null; }
            };
            var timer = null;
            var tipOut = function () {
                if (!showDiv.is(":visible")) { clearTimeout(showtimer); showtimer = null; return; }
                if (timer != null) { clearTimeout(timer); }
                timer = setTimeout(function () {
                    if (isuserMove) {
                        showDiv.hide();
                        tgerDiv.attr("class", curTgerDivClass);
                        showDiv.attr("class", curshowDivClas); 
                        clearTimeout(timer);
                        timer = null;
                        clearTimeout(showtimer);
                        showtimer = null;
                    }
                }, 100);
            };
            var tip_msgMove = function () {
                isuserMove = false;
            };
            var tip_msgOut = function () {
                isuserMove = true;
                tipOut();
            };
            this.init();
        }
    };
     

    // 商家登录
    if (che168User.loginType === 2) {
        var qrcodeTip = new TipHover($('#qrcode2'), $('#qrcodephone2'), 'moreli-title moreli-active');
        qrcodeTip.init();
    } else {
        var mycarhome = new TipHover($('#mycarhome'), $('#mycarhomepanel'));
        mycarhome.init();
    }

