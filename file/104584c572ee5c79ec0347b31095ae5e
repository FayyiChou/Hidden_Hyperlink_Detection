    function searchCityEvt(e) {
        var id = "txtCity";
        var searchCity = $("#"+id);
        
        if (searchCity.val() == document.getElementById(id).defaultTip)
        { searchCity.val('');}

        //处理键盘的“上”、“下”、“回车”键
        e = window.event || e;
        var keyCode = e.keyCode || e.which || e.charCode; 
        var content = [];
        var isHave = false;
        var name, py;
        var i = 0;
        var url = location.href;
        var end = url.indexOf("#");
        if (end > -1) {
            url = url.substring(0, end);
        }
        var areaPy =  $("#py").val().toLowerCase();
        var searchFullCityUrl = "";
        var searchFullCityId = 0;

        var liArr = $("#ulSearchCityResult a");
        var liCount = liArr.length;
        var curli=null;
        var index=-1;
        for (; index < liArr.length; index++) {
            curli = liArr.eq(index);
            if (!curli.hasClass('current')) {
                curli = null;
                continue;
            }
            break;
        } 
        switch (keyCode) {
            // 回车
            case 13: 
                if (curli==null && liCount > 0){
                    curli=liArr.eq(0); 
                }
                if (document.all) {
                    curli[0].click();
                } else if (document.createEvent) {
                    var ev = document.createEvent('MouseEvents');
                    ev.initEvent('click', false, true);
                    curli[0].dispatchEvent(ev);
                }
                return;
                // 向上
            case 38:
                if (curli != null) {
                    index--;
                    curli.removeClass('current');
                }
                else {
                    index = liCount - 1;
                }
                if (index < 0)
                    index = liCount - 1;
                curli = liArr.eq(index);
                curli.addClass('current'); 
                return;
                // 向下
            case 40: 
                if (curli != null) {
                    index++;
                    curli.removeClass('current');
                }
                else {
                    index = 0;
                }
                if (index >= liCount)
                    index = 0;   
                curli=liArr.eq(index);
                curli.addClass("current");
                return;
        }

        
        if (searchCity.val()) {
            var value = searchCity.val().toLowerCase();
            for (i = 0; i < cityArray.length; i++) {
                name = cityArray[i]["name"];
                py = cityArray[i]["py"];
                if (name.indexOf(value)==0 || py.toLowerCase().indexOf(value)==0) {
                    isHave = true; 
                    if (url.toLowerCase().indexOf(areaPy) > -1 && url.indexOf('.com/sell/')==-1) {
                        content.push('<a pid="'+cityArray[i]["id"]+'" onclick="SetAreaCookie(this)" href="' + url.replace(areaPy, py)+'">' + name + '</a>');
                    } else {
                        content.push('<a pid="'+cityArray[i]["id"]+'" onclick="SetAreaCookie(this)" href=\"//www.che168.com/'+py+'/">' + name + '</a>');
                    }
                } 
                // 城市名称或拼音完全匹配，记录跳转的url
                if (name === value || py === value) {
                    searchFullCityId = cityArray[i]["id"];
                    if (url.toLowerCase().indexOf(areaPy) > -1 && url.indexOf('.com/sell/') == -1) {
                        searchFullCityUrl = url.replace(areaPy, py);
                    } else {
                        searchFullCityUrl = "//www.che168.com/" + py + "/";
                    }
                }
            }
        } else {
            isHave = true;
            for (i = 0; i < 10; i++) {
                name = cityArray[i]["name"];
                py = cityArray[i]["py"];
                if (url.toLowerCase().indexOf(areaPy) > -1 && url.indexOf('.com/sell/')==-1) {
                    content.push('<a  pid="'+cityArray[i]["id"]+'" onclick="SetAreaCookie(this)" href="' + url.replace(areaPy, py) +'">' + name + '</a>');
                } 
                else {
                    content.push('<a pid="'+cityArray[i]["id"]+'" onclick="SetAreaCookie(this)" href="//www.che168.com/'+py+'/">' + name + '</a>');
                }
            }
        }
        
        if (isHave) {
            var result = content.join('');
            $("#divSearchCity").show();
            //C.$("divSearchHave").style.display = "block";
            //C.$("divSearchNo").style.display = "none";
            //alert(result);
            $("#ulSearchCityResult").html(result);
        } else {
            $("#divSearchCity").hide();
            // C.$("divSearchHave").style.display = "none";
            // C.$("divSearchNo").style.display = "block";
            $("#ulSearchCityResult").html("");
        }

    };

   
    // 搜索城市
    function searchCity() {
        var id = "txtCity";
        var searchCity = $("#"+id); 
        var searchCityY =document.getElementById(id);
        searchCityY.defaultTip= "请输入城市名"; 
        if (searchCity.val() === '') {
            searchCity.val(searchCityY.defaultTip);
        }

        $("#txtCity").bind('keyup', searchCityEvt);
        $("#txtCity").bind('focus', searchCityEvt);
        $("#txtCity").bind('blur', function() {
            if(document.activeElement.id!='divSearchCity'){
                setTimeout(function() {
                    $("#divSearchCity").hide();
                }, 200);
                if (searchCity.val() === '') {
                    searchCity.val(searchCityY.defaultTip);
                }
            }
        });
    }

    searchCity();
    var indexsearchCity = -1;
    


    /*输出广告使用的当前城市的ID*/
    var ad_Province=0,ad_City=0,cityCode=0,ProvinceAreaID=0;
    var areaId=getCookie('userarea');
    var isShowTransfer=false;
    //代办过户判断北京
    if(areaId=='110000')
    {
        //document.getElementById('isTransfer').style.display='block';                
    }
    
    if(areaId && !isNaN(areaId) && areaId.length==6 ){
        var cotyKey=areaId;
        if(areaId.substring(2,areaId.length)=="0000"){//直辖市获取省会省市ID
            cotyKey=parseInt(areaId)+100;
            if(cotyKey!=110100 && cotyKey!=310100 && cotyKey!=120100 && cotyKey!=500100){
                ad_Province=cityJson[cotyKey].pid;           
                ProvinceAreaID=cityJson[cotyKey].opid;
            }else{                      
                ad_City=cityJson[cotyKey].cid;
                ad_Province=cityJson[cotyKey].pid;
                cityCode=cityJson[cotyKey].ocid;
                ProvinceAreaID=cityJson[cotyKey].opid;
            }
        }else{
            ad_City=cityJson[areaId].cid;
            ad_Province=cityJson[areaId].pid;
            cityCode=cityJson[areaId].ocid;
            ProvinceAreaID=cityJson[areaId].opid;
        }
    }

    /* 如果是全国（或者只选择省没选择市）则给城市特殊值 100100 */
    if (ad_City === 0) {
        ad_City = 100100;
    }

    function loadTipHide()
    {
        if(!getCookie('city-tipHide')&&'False'=='True')
        {
            document.getElementById("city-tip").style.display="block";            
        }
    }
    function onHideGpsDIV()
    {
        document.getElementById("city-tip").style.display="none";        
        setCookie('city-tipHide', "1", { path: '/', expires: new Date(new Date().getTime() + 24 * 60 * 60 * 30), domain: '.che168.com' }); 
    }
    loadTipHide();

    //论坛导航统计埋点
    $("#a_club").bind('click', function () {  
        $.loadJS('//collectionpv.che168.com/collect/page_event.ashx?eventkey=c_pc_carforum_home_forum&ref='+document.referrer+'&cur='+location.href, function () { });
    }); 
    SetSaleCarLink1("wymcTab","#pvareaid=102406");
    SetSaleCarLink1("a_sale","#pvareaid=100663");
</script>
 
     

