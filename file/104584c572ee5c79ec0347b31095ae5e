    /*设置用户地区cookie*/
    function SetAreaCookie(obj) {
        var userArea = obj.getAttribute('pid');
        setCookie('userarea', userArea, { path: '/', expires: new Date(new Date().getTime() + 24 * 60 * 60 * 30), domain: '.che168.com' }); 
    };

