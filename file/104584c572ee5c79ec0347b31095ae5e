<script>
    /*车源浏览记录*/
    function isie6() {
        var browser = navigator.appName
        var b_version = navigator.appVersion
        var version = b_version.split(";");
        if (version[1]) {
            var trim_Version = version[1].replace(/[ ]/g, "");
            if (browser == "Microsoft Internet Explorer" && trim_Version == "MSIE6.0") {
                return true;
            }
        }
        return false;
    }

    var loadRightCarInfo = function (cookieVal) {
        var requestUrl = '/handler/index_v3/GetHistory.ashx?';
        if (isie6()) { //处理IE6中文乱码，区分不同链接，获取不同cdn返回内容
            requestUrl += 'ie=6&';
        }
        if (UsedCarBrowseHistoryCarHTML != '') {
            $('#viewHistory').html('<ul>' + UsedCarBrowseHistoryCarHTML + '</ul>');
            $("#viewHistory").show();
            $("#div_noHistory").hide();
            return;
        }        
        var cid = (getCookie("userarea") == null || getCookie("userarea") == '') ? 0 : getCookie("userarea");
        requestUrl += 'action=carinfo&infoids=' + cookieVal + '&areaid=' + cid;
        if (cookieVal && cookieVal.length > 7) {
            $.ajax({
                url: requestUrl+="&v=2017032020123",
                dataType: 'text',
                success: function (result) {
                    try
                    {
                        data = eval('(' + result + ')');
                        if (data && data.returncode == '1' && parseInt(data.totalcount, 10) > 0) {
                            var content = $('#viewHistory');
                            var _html = '';
                            for (var i = 0; i < data.listcar.length; i++) {
                                var item = data.listcar[i];
                                var regdate = item.date;
                                var html = [
                           '<li  class="no-tag">',
                           '<a target="_blank"  name="track" eventkey="c_pc_righttool_history" href="' + item.url + '">',
                           '<img alt="" src="' + item.pic + '"   class="photo"/>',
                           '<div class="list-photo-info">',
                           '<h3>' + item.carname + '</h3>',
                           '<div class="time">' + item.mileage + '万公里／' + regdate + '／' + item.cityname + '</div>',
                           '<div class="price"><em><b>' + item.price + '</b>万元</em></div>',
                           '</div>',
                           '</a>',
                           '<a class="btn-similar" href="' + item.likeUrl + '" target="_blank">找相似</a></li>'
                                ].join('');
                                _html += html;
                            }
                            content.html('<ul>' + _html + '</ul>');
                            UsedCarBrowseHistoryCarHTML = _html;
                            $("#viewHistory").show(); 
                            $("#div_noHistory").hide(); 
                        } else { 
                            $("div_noHistory").show();;
                            $("viewHistory").hide();
                        }
                    } catch (e) { }
                }
            });
        }
    };
    var UsedCarBrowseHistoryCarHTML = '';
    /*车源浏览记录*/ 
