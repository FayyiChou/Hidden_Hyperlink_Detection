</head><script type="text/javascript">
$(function(){
	$("svg .pref").click(function(){
		var pref = $(this).attr("pref");
		$("select[name=select_pref]").val(pref);
		$("form[name=form]").submit();
	});
	
	var toDoubleDigits = function(num) {
		num += "";
		if (num.length === 1) {
			num = "0" + num;
		}
		return num;     
	};
	
	$("input[name=radio_date]").change(function(){
		var $date = $(this).val();
		var now = new Date();
		var nYear		 = now.getFullYear();
		var nMonth	 = now.getMonth() + 1;
		var nDate	 = now.getDate();
		
		switch( $date ){
			case 'today':
				break;
			
			case 'tomorrow':
				var day = new Date(nYear, nMonth, nDate + 1);
				nYear		 = day.getFullYear();
				nMonth	 = day.getMonth();
				nDate	 =day.getDate();
				break;
				
			case 'day_after_tomorrow':
				var day = new Date(nYear, nMonth, nDate + 2);
				nYear			 = day.getFullYear();
				nMonth		 = day.getMonth();
				nDate	 		= day.getDate();
				break;
		}
		
		nMonth	 = toDoubleDigits( nMonth );
		nDate 		= toDoubleDigits( nDate );
		
		if( $date == 'clear' ) {
			$(".datepicker").val('');
		}else{
			$(".datepicker").val( nYear +"-"+ nMonth +"-"+ nDate );
		}
	});
	
	$( ".top-datepicker" ).datepicker({
		dateFormat : 'yy-mm-dd',
		showMonthAfterYear : true,
		monthNames: ['1月', '2月', '3月', '4月', '5月', '6月', '7月', '8月', '9月', '10月', '11月', '12月'],
		dayNames: ['日', '月', '火', '水', '木', '金', '土'],
		dayNamesMin: ['日', '月', '火', '水', '木', '金', '土'],
		onSelect: function(){
			$("input[name=radio_date]").prop('checked',false);
		}
	});
	
	$("form[name=form] button").click(function(){
		if( $("[name=keyword]").val() == '' && $("[name=select_pref]").val() == '' &&
			 $("[name=age]").val() == '' && $("[name=date]").val() == ''){
			location.href = "/area/";
		} else{
			$("form[name=form]").submit();
		}
	});
});
</script>
