function drop_mouseover(pos){
 try{window.clearTimeout(timer);}catch(e){}
}
function drop_mouseout(pos){
 var posSel=document.getElementById(pos+"Sel").style.display;
 if(posSel=="block"){
  timer = setTimeout("drop_hide('"+pos+"')", 100);
 }
}
function drop_hide(pos){
 document.getElementById(pos+"Sel").style.display="none";
}
var searchTip="输入课程名称";
function search_show(pos,searchType,href){
  searchTip=href.innerText;
  if(searchTip=="课程"){searchTip="输入课程名称";}
  if(searchTip=="学校"){searchTip="输入学校名称或地区";}
  if(searchTip=="新闻"){searchTip="输入新闻关键字";}
  if(searchTip=="知识"){searchTip="输入问题关键字";}
  if(document.getElementById("Key").value=="输入课程名称" 
     || document.getElementById("Key").value=="输入学校名称或地区"
     || document.getElementById("Key").value=="输入新闻关键字"
     || document.getElementById("Key").value=="输入问题关键字"){
    document.getElementById("Key").value=searchTip;
  }
  var links=document.getElementById('searchadiv').getElementsByTagName('a');
  for(var i=0;i<links.length;i++){
    links[i].className='titleN';
  }
  href.className='titleY';
    document.getElementById(pos+"SearchType").value=searchType;
    document.getElementById(pos+"Sel").style.display="none";
    document.getElementById(pos+"Slected").innerHTML=href.innerHTML;
    var sE = document.getElementById("searchExtend");
    if(sE != undefined  &&  searchType == "bar"){
     sE.style.display="block";
    }else if(sE != undefined){
     sE.style.display="none";
    }
 try{window.clearTimeout(timer);}catch(e){}
 return false;
}
function dosearch($p){
  if (document.getElementById('Key').value==''||document.getElementById('Key').value==searchTip){
   alert(searchTip+"!");
   document.getElementById('Key').focus();
   return false;
  }
  document.getElementById('headSearchClass').value=$p;
  return true;
}
