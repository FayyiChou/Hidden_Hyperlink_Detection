        function Search(UserID) {
            txtStockCode = $.trim($('#ajaxinput').val());                
//            txtUserName = $.trim($('#txtUserName').val());     
//            txtStrategyName = $.trim($('#txtStrategyName').val());
//            ddlStrategistType = $('#ddlStrategistType').val();                       
//            ddlOrderDirection = $('#ddlOrderDirection').val();  
//            ddlDealPeriod = $('#ddlDealPeriod').val();     
            var ResultContentlength=0;                    
            var ResultContent1length=0;    
 
            $('#peiguleftResultContent1').hide();                        
            $('#peiguleftResultContent').show();  
            $('#ResultContent').html("正在加载数据。。。");
            $('#ResultContent1').html("正在加载数据。。。");            
            $.get
            (
                '/DataProvider/MatchingHistory.aspx',
                { 'StockCode':txtStockCode,
                  //'UserName':txtUserName,
                  //'StrategyName':txtStrategyName,
                  //'StrategistType':ddlStrategistType,
                  //'OrderDirection':ddlOrderDirection,
                  //'DealPeriod':ddlDealPeriod,
                  'random': Math.random() },
                    function (data) {
                        $('#ResultContent').html(data);
                        ResultContentlength=$('#ResultContent').html().length;
                        //alert(ResultContentlength+ResultContent1length);
                        if(((ResultContentlength+ResultContent1length)==3526)||((ResultContentlength+ResultContent1length)==2526)||((ResultContentlength+ResultContent1length)==3528))  
                        {
                            $('#peiguleftResultContent').hide();                        
                            $('#peiguleftResultContent1').show();                                      
                           // $('#peiguleftResultContent1').html('1');
                        }
                    }
             )
             
            $.get
            (
                '/DataProvider/MatchingOptional.aspx',
                { 'StockCode':txtStockCode,
                  //'UserName':txtUserName,
                  //'StrategyName':txtStrategyName,
                  //'StrategistType':ddlStrategistType,
                  //'OrderDirection':ddlOrderDirection,
                  //'DealPeriod':ddlDealPeriod,
                  'random': Math.random() },
                    function (data) {
                        $('#ResultContent1').html(data);
                        ResultContent1length=$('#ResultContent1').html().length;     
                        //alert(ResultContentlength+ResultContent1length);                                                                   
                        if(((ResultContentlength+ResultContent1length)==3526)||((ResultContentlength+ResultContent1length)==2526)||((ResultContentlength+ResultContent1length)==3528))  
                        {
                            $('#peiguleftResultContent').hide();                        
                            $('#peiguleftResultContent1').show();                                      
                           // $('#peiguleftResultContent1').html('2');
                        }
                        //alert($('#ResultContent1').html().length);                        
                    }
             )    
             getStockQuote_Name('ajaxinput','');   
             //alert(ResultContentlength+ResultContent1length);

        }         
        
//获得股票名称
 function getStockQuote_Name(StockCode_InputName,actionPrice_InputName)
 {
     document.getElementById("MatchingHistoryStockName").innerHTML='';
     document.getElementById("MatchingOptionalStockName").innerHTML='';
     document.getElementById("MatchingHistoryStockCode").innerHTML='';
     document.getElementById("MatchingOptionalStockCode").innerHTML='';
     var _ajaxinput = document.getElementById(StockCode_InputName);
     if(!myfrm_isStockCode(_ajaxinput)) 
     return;
     var prefix = _ajaxinput.value.substring(0,2)
     if(prefix != '60' && prefix != '00' && prefix != '30')
     {
        alert('请输入A股代码')
        return
     }
     var parameter="StockCode="+_ajaxinput.value+"&action=getStockName";
    var ajax = new Ajax("../ajax_StockUserDelegate.aspx",parameter,false,false,null,"GetStockQuote_Price_onCallback");
    GetStockQuote_Price_onCallback = function(res)
    {
        if(res != null && res != "")
        {       
            var html=unescape(res);
            document.getElementById("MatchingHistoryStockName").innerHTML='在'+html;
            document.getElementById("MatchingOptionalStockName").innerHTML=''+html;
            document.getElementById("MatchingHistoryStockCode").innerHTML='（'+_ajaxinput.value+'）';
            document.getElementById("MatchingOptionalStockCode").innerHTML='（'+_ajaxinput.value+'）';



            
//            var html_1,html_2;
//            if(html.indexOf('||||||')>-1)
//            {
//                document.getElementById(actionPrice_InputName+"_1").innerHTML="";
//                document.getElementById(actionPrice_InputName+"_2").innerHTML="";
//            
//                html_1=html.substring(0,html.indexOf('||||||'));
//                
//                var stockName = LTrim(html_1.substring(1,html_1.indexOf('】')))
//                var market = getMarket(_ajaxinput.value)
//                var fullCode = convert6to8HexunVer(_ajaxinput.value)
//                
//                //document.domain = "hexun.com"
//                if(parent.document.getElementById('ifrm') != null)
//                {
//                    parent.document.getElementById('ifrm').src = 'http://flashquote.stock.hexun.com/flash/100ff/dhqt2011.html?c=' + _ajaxinput.value + '&m=' + market +'&p=' + fullCode +'&n=' + EncodeUtf8(stockName)
//                }
//                
//                document.getElementById("stockName_info").innerHTML="&nbsp;"+html_1;
//                
//                html_2=html.substring(html.indexOf('||||||')+6,html.length);
//                document.getElementById(actionPrice_InputName+"_2").innerHTML="&nbsp;"+html_2;
//            }
            return;
        }
        res = null;
        GetStockQuote_Price_onCallback = null;
    }
    ajax.send();
    ajax = null;
 }
        
        
              
