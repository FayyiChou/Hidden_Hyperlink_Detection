    <script>
function GetTagCssData(TgList){

    var TagCssData = eval(TgList);
	
	var pthName = window.location.pathname;
	
	pthName = pthName.replace("/","").toLowerCase();//转换小写.	 
    for(var i = 0; i<= TagCssData.length; i++)
    {         
         if(TagCssData[i]&&TagCssData[i].value!="no")
         {            
            document.getElementById(TagCssData[i].value).className = ""; 
         }         
    }   	
	if(pthName =='/' || pthName ==''){
	    document.getElementById("sy").className = "hover";return;
	}else{   	    
	    if(pthName.indexOf("h_list")!=-1){
	        document.getElementById("zx").className = "hover";return;
	    }
	    if(pthName.indexOf("news")!=-1){
	        document.getElementById("ph").className = "hover";return;
	    }
	    if(pthName.indexOf("h_hxt")!=-1){
	        document.getElementById("hxt").className = "hover";return;
	    }	     
	    if(pthName.indexOf("stratsearch")!=-1){	    
	        
	        document.getElementById("jp").className = "hover";return;
	    }
	    if(pthName.indexOf("mystrats")!=-1){	    
	        
	        document.getElementById("dy").className = "hover";return;
	    }
	    
	}
}
    </script>

