<script type="text/javascript">
//搜索框隐藏与显示提示文字
	jQuery(".search").focus(function(){
			  $(this).css("color","#333333");
			  if($(this).val() ==this.defaultValue){  
                  $(this).val("");           
			  } 
		}).blur(function(){
			 if ($(this).val() == '') {
				$(this).css("color","#e7e7e7");
                $(this).val(this.defaultValue);
				
             }
		});
		

</script>











