    <script type="text/javascript">
    
    
    //alert(window.location.pathname.replace("/",""));
    function Login()
    {
        var url = window.location.href;
        window.location.href = "http://c.hexun.com/Login.aspx?gourl=" + encodeURIComponent(url);
    }
    
    function Reg()
    {
        var url = window.location.href;        
        window.location.href = "http://c.hexun.com/reguser.aspx?gourl="+encodeURIComponent(url); // url             
    }
    
    function Quit()
	{
	    var url = window.location.href;
		window.location.href = 'http://c.hexun.com/Quit.aspx?gourl=' +encodeURIComponent(url);		
	}
	
	function GoToProfile()
	{
	    window.location.href = "../profile.aspx";
	}
    </script>

