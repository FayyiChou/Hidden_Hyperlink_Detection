		<link rel="stylesheet" type="text/css" href="http://tihov.fitdev.ru/countdown/counter.css" />
<script type="text/javascript" src="http://tihov.fitdev.ru/countdown/flipcounter.min.js"></script>
<script src="http://tihov.fitdev.ru/countdown/moment.min.js"></script>
<script src="http://tihov.fitdev.ru/countdown/ru.js"></script>

<script>
var timer;
function declOfNum(number, titles) {
	cases = [2, 0, 1, 1, 1, 2];
	return titles[(number % 100 > 4 && number % 100 < 20) ? 2 : cases[(number % 10 < 5) ? number % 10 : 5]];
}
$(function() {
	moment.lang('ru');
	var $message = $('.countdown');
//'', 'YYYYMMDDhhmmss'
	var start = moment('20160726180000','YYYYMMDDhhmmss');
	//var start = moment('20130810130000', 'YYYYMMDDhhmmss');
	var diff = start.diff(moment('20170219073538', 'YYYYMMDDhhmmss'), 'seconds');
	var days = Math.floor(diff / 86400);
	if (days < 10) $('#d0').show();
	var delta = (days * 86400);
	var hours = Math.floor((diff - delta) / 3600);
	if (hours < 10) $('#h0').show();
	delta += hours * 3600;
	var minutes = Math.floor((diff - delta) / 60);
	if (minutes < 10) $('#m0').show();
	delta += minutes * 60;
	var seconds = diff - delta;
	if (seconds < 10) $('#s0').show();
	var days = new flipCounter('days', {value: days, inc: -1, auto: false});
	var hours = new flipCounter('hours', {value: hours, inc: -1, auto: false});
	var mins = new flipCounter('minutes', {value: minutes, inc: -1, auto: false});
	var secs = new flipCounter('seconds', {value: seconds, inc: -1, auto: false});
	new flipCounter('d0', {value: 0, auto: false});
	new flipCounter('h0', {value: 0, auto: false});
	new flipCounter('m0', {value: 0, auto: false});
	new flipCounter('s0', {value: 0, auto: false});
	var countdown = function() {
		if (secs.getValue() == 10)
			$('#s0').show();
		if (secs.getValue() == 0) {
			$('#s0').hide();
			secs.setValue(60);
			mins.step();
		}
		secs.step();
		if (mins.getValue() < 10)
			$('#m0').show();
		if (mins.getValue() == 0) {
			$('#m0').hide();
			mins.setValue(59);
			hours.step();
		}
		if (hours.getValue() < 10)
			$('#h0').show();
		if (hours.getValue() == 0 && days.getValue() > 0) {
			$('#h0').hide();
			hours.setValue(23);
			days.step();
		}
		if (days.getValue() < 10)
			$('#d0').show();
//		if (days.getValue() == 0) {
//			$('#d0').hide();
//		}
//		clearInterval(timer);
		$('.days.time-text').html(declOfNum(days.getValue(), ['День', 'Дня', 'Дней']));
		$('.hours.time-text').html(declOfNum(hours.getValue(), ['Час', 'Часа', 'Часов']));
		$('.minutes.time-text').html(declOfNum(mins.getValue(), ['Минута', 'Минуты', 'Минут']));
		$('.seconds.time-text').html(declOfNum(secs.getValue(), ['Секунда', 'Секунды', 'Секунд']));
	};
	setTimeout(function() {
		countdown();
		timer = setInterval(countdown, 1000);
	}, 1000);
});
</script>
