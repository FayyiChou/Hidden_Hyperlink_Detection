        <script>
  var speed=30
  Marquee_7_3.innerHTML=Marquee_7_2.innerHTML
  function Marquee(){
  if(Marquee_7_3.offsetWidth-Marquee_7_1.scrollLeft<=0)
  Marquee_7_1.scrollLeft-=Marquee_7_2.offsetWidth
  else{
  Marquee_7_1.scrollLeft++
  }
  }
  var MyMar=setInterval(Marquee,speed)
  Marquee_7_1.onmouseover=function() {clearInterval(MyMar)}
  Marquee_7_1.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
  </script>
