                                <script type="text/jscript">
								$(document).ready(function(e) {
								 var in_out_time = 800; //渐显时间
								 var scroll_Interval_time = 2000; //滚动间隔时间
								 var scroll_time = 1000; //滚动动画时间
								   
								 addli(); 
								 
								 function addli(){
								  $(".myd_2 .scroll_List").css("top",0);
								  $(".myd_2 .scroll_List li:last").clone().prependTo(".myd_2 .scroll_List");
								  $(".myd_2 .scroll_List li:first").css("opacity",0);
								  $(".myd_2 .scroll_List li:first").animate({opacity:1},in_out_time);
								  setTimeout(delLi_last,scroll_Interval_time) ;
								 }
								 
								 function delLi_last(){
								  $(".myd_2 .scroll_List li:last").detach(); 
								  $(".myd_2 .scroll_List").animate({top:55},scroll_time,addli);
								 }
								 
								});
								</script>
                            <div class="myd_2">
								<ul class="scroll_List">
																		<li>
										<span class="myd_2_title" ><a href="http://www.fjzxtravel.com/index.php?ctl=tours&act=view&id=46">定高北土楼群（承启楼）一日游 (天…</a></span>
										<span class="myd_2_user_info" >——— 用户jia*** 2016-12-18预订</span>
									</li>
																		
								</ul>
                            </div>
