       <script>
function hdme(id)
{
	if(id=="0")
	{   
	    document.getElementById("hdme_3").style.display="none";
		document.getElementById("hdme_2").style.display="none";
		document.getElementById("hdme_1").style.display="block";
		document.getElementById("hdme").className="hdm1";
	  
    }
	else if(id=="1")
	{
		document.getElementById("hdme_3").style.display="none";
		document.getElementById("hdme_2").style.display="block";
		document.getElementById("hdme_1").style.display="none";
		document.getElementById("hdme").className="hdm2";
	}
	else
	{
		document.getElementById("hdme_3").style.display="block";
		document.getElementById("hdme_2").style.display="none";
		document.getElementById("hdme_1").style.display="none";
		document.getElementById("hdme").className="hdm3";
	}
}
