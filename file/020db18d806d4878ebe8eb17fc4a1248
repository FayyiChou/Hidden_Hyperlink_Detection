    var moveStyle
    var rand = parseInt(Math.random() * 4)
    switch (rand) {
        case 0: moveStyle = "left"; break;
        case 1: moveStyle = "right"; break;
        case 2: moveStyle = "down"; break;
        case 3: moveStyle = "up"; break;
    }
    $(function () {
        $("#KinSlideshow1").KinSlideshow({ moveStyle: moveStyle });
    })
    onload = function () {
        ChangeImage(1);
    }
    function ChangeImage(type) {
        if (type == 1) {
            document.getElementById("checkCodeImg").src = document.getElementById("checkCodeImg").src + '?';
        }
        else if (type == 2) {
            document.getElementById("SoCheckCodeImg").src = document.getElementById("SoCheckCodeImg").src + '?';
        }
    }

    function SwitchLoginAndQuery(clickObject) {
        if (clickObject.id == "divLogin") {
            document.getElementById('divLoginContent').style.display = "";
            document.getElementById('divQueryContent').style.display = "none";
        }
        else if (clickObject.id == "divQuerySo") {
            document.getElementById('divLoginContent').style.display = "none";
            document.getElementById('divQueryContent').style.display = "";
        }
    }

    function btnYouke_click() {
        document.getElementById("txtUserName").value = "YOUKE";
        document.getElementById("txtPassword").value = "123456";
        document.getElementById("IsYouke").value = "1";
    }
    
</script>
