<script type="text/javascript">
//滚动标题开始
function AutoScroll(obj){
	$(obj).find("ul:first").animate({
		marginTop:"-28px"
	},500,function(){
		$(this).css({marginTop:"0px"}).find("li:first").appendTo(this);
	});
};
setInterval('AutoScroll("#scr_title")',3000);
//tab
function tab(name, c, n) {
for (i = 1; i <= n; i++) {
	var menu = document.getElementById(name + i);
	var more = document.getElementById("m-"+name + i);
	var con = document.getElementById("con-" + name + i);
	if ((menu != null) && (con != null)&&(more != null)) {
		menu.className = i == c ? "hover" : "";
		con.style.display = i == c ? "block" : "none";
		more.style.display = i == c ? "inline" : "none";
	}
	if ((menu != null) && (con != null)&&(more==null)) {
		menu.className = i == c ? "hover" : "";
		con.style.display = i == c ? "block" : "none";
	}
};
};
</script>
<script type="text/javascript">
    //头部下拉广告
    $(function(){
        $("div.slide_ad a.before").mouseenter(function(){
            $(this).removeClass("before").addClass("on");
        }).mouseleave(function(){
            $(this).removeClass("on").addClass("before");
        }).click(function(){
            $(".bg_ad").animate({height:"150px"},500);
            $(this).hide().siblings(".after").show();
        })
        $("div.slide_ad a.after").mouseenter(function(){
            $(this).removeClass("after").addClass("on");
        }).mouseleave(function(){
            $(this).removeClass("on").addClass("after");
        }).click(function(){
            $(".bg_ad").animate({height:"0px"},500);
            $(this).hide().siblings(".before").show();
        })
        function slidedw(){
            
            $(".bg_ad").animate({height:"150px"},500);
            $("div.slide_ad a.before").hide().siblings(".after").show();
        }
        function slideup(){
    
            $(".bg_ad").animate({height:"0px"},500);
            $("div.slide_ad a.after").hide().siblings(".before").show();
        
        }
        setTimeout(slidedw,100);
        var autoSlide = setInterval(slideup,4000);
        autoSlide;
        $(".bg_ad").mouseover(function(){
                                clearInterval(autoSlide);
                               });
        $(".bg_ad").mouseout(function(){
                                autoSlide=setInterval(slideup,10000);
                               })
    })
    
</script>
