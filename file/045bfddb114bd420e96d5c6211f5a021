<script type="text/javascript" src="http://mon.changyou.com/jquery-a.js"></script> 
<script type="text/javascript">
// validate before submit
function validate(){
	var cn = document.getElementById("cn");
	var password = document.getElementById("password");
	var confirmPwd = document.getElementById("confirmPwd");
	var superPwd = document.getElementById("superPwd");
	var confirmSuperPwd = document.getElementById("confirmSuperPwd");
	var securityEmail = document.getElementById("securityEmail");
	var sn = document.getElementById("sn");
	var certNumber = document.getElementById("certNumber");
	var securityQuestion = document.getElementById("question");
	var securityAnswer = document.getElementById("securityAnswer");
	var phone = document.getElementById("phone");
	var valiCode = document.getElementById("valiCode");
	
	var flag = true;
	if(!cnValidate(cn)) flag = false;
	if(!passwordValidate(password)) flag = false;
	if(!confirmPwdValidate(confirmPwd)) flag = false;
	if(!superPwdValidate(superPwd)) flag = false;
	if(!confirmSuperPwdValidate(confirmSuperPwd)) flag = false;
	if(!securityEmailValidate(securityEmail)) flag = false;
	if(!snValidate(sn)) flag = false;
	if(!certNumberValidate(certNumber)) flag = false;
	if(!questionValidate(securityQuestion)) flag = false;
	if(!securityAnswerValidate(securityAnswer)) flag = false;
	if(!phoneValidate(phone)) flag = false;
	if(!valiCodeValidate(valiCode)) flag = false;
	
	
			
	if(flag){
		//2013-07-01 刀剑快注加广告监控
        var jiankongFlag = "";
        var cookies = document.cookie.split(';'); 
        for (var i = 0; i < cookies.length; i++) { 
            var cookie = $.trim(cookies[i]); 
            if (cookie.substring(0, 7) == 'tongji=') { 
            	jiankongFlag = 1;
            	$("#tongji").val("normal," + cookie.substring(7, cookie.length));
            } 
        } 
        if(1 != jiankongFlag){
            $("#tongji").val("normal,");
        }
        return true;
    } else {
        return false;
    }
	    

}
</script> 
