<script language="javascript">
var cnFlag = false;
var banEmails=['boevents@contact.sohu.com','daojian@contact.sohu.com','tlevents@contact.sohu.com','tlbb@contact.sohu.com','service@ko.sohu.com','jianxian@contact.sohu.com','ldj@contact.sohu.com','tousu@contact.sohu.com','service@sohu-inc.com','kuncheng@sohu-inc.com','fengchunyang@sohu-inc.com','brucetang@sohu-inc.com','xuecongma@sohu-inc.com','fionayun@sohu-inc.com','kingdeng@sohu-inc.com','daphnedu@sohu-inc.com','candychen@sohu-inc.com','jessicazhao@sohu-inc.com','gamejob@vip.sohu.com','famouswei@sohu-inc.com','taowang@sohu-inc.com','jackhong@sohu-inc.com','yeli@sohu-inc.com','feverfeng@sohu-inc.com','stormcui@sohu-inc.com','davidchen@sohu-inc.com','candychen@sohu-inc.com','alex@sohu-inc.com','poetryliu@sohu-inc.com','zhouzhifei@sohu-inc.com','weiqing_ch@cyou.com','liye@cyou.com','huangyongfeng@cyou.com','cuiwei@cyou.com','wangtao@cyou.com','hongxiaojian@cyou.com','chenfeng_sc@cyou.com','alex@cyou.com','chenfeng@cyou.com','liutie@cyou.com','zhouzhifei@cyou.com'];

	function isInBanEmails(str){
		for(var i=0;i<banEmails.length;i++){
			if(banEmails[i]==str){
				return true;
			}
		}
		return false;
	}
var PatternsDict=new Object();
	PatternsDict.emailPat= /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/;
String.prototype.Len=function(){return this.replace(/[^\x00-\xff]/g,"**").length;};	
function IsDigit(cCheck) { return (('0'<=cCheck) && (cCheck<='9')); }
function IsAlpha(cCheck) { return ((('a'<=cCheck) && (cCheck<='z')) || (('A'<=cCheck) && (cCheck<='Z'))) }
function showTip(obj, msg){
	var obj = obj.id;
	document.getElementById(obj+"Tip").innerHTML = msg;
}
function showErr(obj, msg){
	var obj = obj.id;
	document.getElementById(obj).style.backgroundColor = "#FFD1D1";
	document.getElementById(obj+"Tip").innerHTML = msg;
}

// validate ok
function validateOK(obj){
	var obj = obj.id;
	document.getElementById(obj).style.backgroundColor = "#FFBC3D";
	document.getElementById(obj+"Tip").innerHTML = "";
}
function isChn(str) {
	var reg = /^[\u4E00-\u9FA5]+$/;
	if (!reg.test(str)) {
		return false;
	}
	return true;
}
function isEn(str) {
	for (i = 0; i < str.length; i++) {
		if (IsAlpha(str.charAt(i))) {
			return true;
		}
	}
	return false;
}

function cnValidate(obj)
{
    strcn = obj.value;
    if (strcn == "")
    {
        showErr(obj,"请输入您的用户名");
        return false;
    }

    if (strcn.length < 5||strcn.length > 16)
    {
        showErr(obj,"用户名长度不符，需5-16位。");
        return false;
    }
  
    for (nIndex=0; nIndex<strcn.length; nIndex++)
    {
        cCheck = strcn.charAt(nIndex);
        if (nIndex==0 && cCheck =='_')
        {
            showErr(obj,"首字符必须为字母或数字，字母不区分大小写。");
            return false;
        }

        if (!(IsDigit(cCheck) || IsAlpha(cCheck) || cCheck=='_' ))
        {
           showErr(obj,"用户名只能使用英文字母、数字以及_组成，字母不区分大小写。");
           return false;
        }
    }
    if (strcn.substring(0,3).toLowerCase() == 'ux_') {
        showErr(obj,"用户名不能以ux_开头。");
        return false;
    }
    send_request('/reg/checkcn.jsp?cn=' + strcn + '&t=' + new Date());
    validateOK(obj);
    return cnFlag;
}

 function send_request(url) {//初始化、指定处理函数、发送请求的函数
		http_request = false;
		//开始初始化XMLHttpRequest 对象
		if(window.XMLHttpRequest) {            //Mozilla 浏览器
			http_request = new XMLHttpRequest();
			if (http_request.overrideMimeType) { //设置MiME 类别
				http_request.overrideMimeType("text/xml");
			}
		}else if (window.ActiveXObject) {     // IE 浏览器
			try {	
				http_request = new ActiveXObject("Msxml2.XMLHTTP");
			} 
			catch (e) {
				try {
					http_request = new ActiveXObject("Microsoft.XMLHTTP");
				} catch (e) {}
			}
		}
		if (!http_request) { // 异常，创建对象实例失败
			window.alert("不能创建XMLHttpRequest 对象实例.");
			return false;
		}
		http_request.onreadystatechange = processRequest;
		http_request.open("GET", url, true);// 确定发送请求的方式和URL 以及是否同步执行下段代码
		http_request.send(null);
	}

	function processRequest() {	
		var obj = document.getElementById("cn");						
		if (http_request.readyState == 4) { 	// 判断对象状态
			if (http_request.status == 200) {   // 信息已经成功返回，开始处理信息
				var msg = http_request.responseText;
				//用户名可以使用
				if (msg.indexOf("ok") != -1) {
	                //ok
					  cnFlag = true;
					  document.getElementById(obj.id).style.backgroundColor = "#FFBC3D";
					  document.getElementById(obj.id+"Tip").innerHTML = "恭喜，该用户名可用";
					
	    			}
				else{
	                //用户名不可用
					if (msg.indexOf("0") != -1) {
	                    //为空
	                    cnFlag = false;
						showErr(obj,"请输入用户名");
						return;
					} else if (msg.indexOf("-1") != -1) {
	                    //非5--16位
	                    cnFlag = false;
	                    showErr(obj,"用户名长度不符,需5-16位");
						return;                  
					} else if (msg.indexOf("-2") != -1) {
	                    //首字符非字母或数字
	                    cnFlag = false;
						showErr(obj,"首字符必须为字母或数字，字母不区分大小写。");
						return;                
					} else if (msg.indexOf("-3") != -1) {
	                    //用户名只能使用英文字母、数字以及_组成
	                    cnFlag = false;
						showErr(obj,"用户名只能使用英文字母、数字以及_组成，字母不区分大小写。");
						return;
					} else if (msg.indexOf("-6") != -1) {
						//用户名不能以ux_开头
	                    cnFlag = false;
						showErr(obj,"用户名不能以ux_开头");
						return;
				    }else if (msg.indexOf("-5") != -1) {
	                    //已被注册
	                      cnFlag = false;                 
				       showErr(obj,"用户名已经被注册,请更改用户名");
				        return;
					} else {
	                    //异常
				      	showErr(obj,"验证用户名时异常错误");
				        cnFlag = true;
				        return;
					}
				}
			} 
			else { //页面不正常
                 showErr(obj,"请求有异常，无法验证用户名");
		        return;	
			}
		}
}
function GetRequest() {

   var url = location.search; //获取url中"?"符后的字串
   var theRequest = new Object();

   if (url.indexOf("?") != -1) {

      var str = url.substr(1);

      strs = str.split("&");

      for(var i = 0; i < strs.length; i ++) {

         theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);

      }

   }
	
   return theRequest;
}

function passwordValidate(obj)
{
	 strpasswd = obj.value;
	 strcn = document.getElementById("cn").value;
	    if (strpasswd == "")  
	    {  
	        showErr(obj,"请输入密码");
	        return false;  
	    }
	    if (strpasswd.length < 4||strpasswd.length > 16)
	    {
	        showErr(obj,"游戏密码长度不符，长度为4-16位");
	        return false;
	    }
	    for (pwIndex=0; pwIndex<strpasswd.length; pwIndex++)  
	    {  
	        cCheck = strpasswd.charAt(pwIndex);
	        if ( cCheck==' ' || cCheck=='\'' || cCheck=='"' || cCheck==',')  
	        {  
	            showErr(obj,"用户密码不能使用空格,逗号,单双引号");
	            return false;  
	        }  
	    }
	    if (strpasswd == strcn)  
	    {  
	        showErr(obj,"用户密码不能与用户名相同");
	        obj.value="";
	        document.getElementById("confirmPwd").value="";
	        return false;  
	    }
	    validateOK(obj);
	    return true;
}
function confirmPwdValidate(obj)
{
	 strconfirmPwd = obj.value;
	 strpwd = document.getElementById("password").value;
	    if (strconfirmPwd == "")  
	    {  
	        showErr(obj,"请确认密码");  
	        return false;  
	    }
	    if (strpwd != strconfirmPwd)
	    {
	        showErr(obj,"两次输入的密码不一致");
	        document.getElementById("password").value="";
	        obj.value="";
	        return false;
	    }
	    validateOK(obj);
	    return true;
	    
}
function superPwdValidate(obj)
{
	  strsuperPwd = obj.value;
	  strpasswd = document.getElementById("password").value;
	  strcn = document.getElementById("cn").value;
	    if (strsuperPwd == "")  
	    {  
	        showErr(obj,"请输入超级密码"); 
	        return false;  
	    }
	    if (strsuperPwd.length<8||strsuperPwd.length>16)
	    {
	        showErr(obj,"超级密码长度不符，长度为8-16位");
	        return false;
	    }
	    if(strsuperPwd == strpasswd)
	    {
	    	showErr(obj,"超级密码不能与密码相同");
	    	obj.value="";
	        return false;
	    }
	    for(nIndex=0;nIndex<strsuperPwd.length;nIndex++)
	    {
	        cCheck = strsuperPwd.charAt(nIndex);
	        if(!(IsDigit(cCheck) || IsAlpha(cCheck)))
	        {
	        	showErr(obj,"超级密码必须为字母或数字");
	        	return false;
	        }
	    }
	    if (strsuperPwd == strcn)  
	    {  
	        showErr(obj,"超级密码不能与用户名相同");
	        obj.value="";
	        return false;
	    }
	    validateOK(obj);
	    return true;
}
function confirmSuperPwdValidate(obj)
{
	strconfirmSuperPwd = obj.value;
	strsuperpwd = document.getElementById("superPwd").value;
	    if (strconfirmSuperPwd == "")  
	    {  
	        showErr(obj,"请重复超级密码");
	        return false;  
	    }
	    if (strsuperpwd != strconfirmSuperPwd)
	    {
	        showErr(obj,"两次输入的超级密码不一致，请重新输入");
	        document.getElementById("superPwd").value="";
	        obj.value=""; 
	        return false;
	    }
	    validateOK(obj);
	    return true;
}
function securityEmailValidate(obj)
{	    
	stremail = obj.value;
	    if (stremail == "")
	    {
	        showErr(obj,"请输入您的邮箱");
	        return false;
	    }
	    if(isInBanEmails(stremail.toLowerCase())) {
	        showErr(obj,"请输入您经常使用的邮箱");
	        return false;
	    }
	    if(stremail.length>50)
	    {
	    	showErr(obj,"邮箱的长度不正确,请重新输入");
	        return false;
	    }
	    var thePat=PatternsDict.emailPat;
	    var gotIt=thePat.exec(obj.value);
	    if(!gotIt)
	    {
	        showErr(obj,"请输入正确的邮箱");
	        return false;
	    }
	    validateOK(obj);
	    return true;
}	
function snValidate(obj)
{
	
	var check_name = checkIsChinese(obj.value);
	    if (check_name != "true"){
			showErr(obj,check_name);
			return false;
		}
		validateOK(obj);
		return true;
		
}
function certNumberValidate(obj)
{
	var check_result = checkIdcard(obj.value);
		if (check_result != "true"){
		showErr(obj,check_result);
		return false;	
		}	
		validateOK(obj);
		return true;
}
function questionValidate(obj)
{
	
	var strquestion = obj.value;
	
		if (strquestion == 99) {
		showErr(obj,"请选择密码提示问题");
		return false;
		}
		validateOK(obj);
		return true;	
}
function securityAnswerValidate(obj)
{
	var stranswer = obj.value;	
	var strquestion = document.getElementById("question").value;
	if (stranswer == "") {
		showErr(obj,"请输入密码提示答案");
		return false;
	}
	if (parseInt(strquestion) >= 8 && parseInt(strquestion) <= 15) {
		for (i = 0; i < stranswer.length; i++) {
			if (stranswer.charAt(i) < "0" || stranswer.charAt(i) > "9") {
				showErr(obj,"请填写8位数字，举例20090514");
				return false;
			}
		}
		if (stranswer.Len() != 8) {
			showErr(obj,"请填写8位数字，举例20090514");
			return false;
		}
	} else {
		if (stranswer.Len() < 2 || stranswer.Len() > 38 || (!isChn(stranswer) && !isEn(stranswer))) {
			showErr(obj,"请填写1-19个中文或2-38个英文");
			return false;
		}
	}
	validateOK(obj);
	return true;
		
}
function phoneValidate(obj)
{
	var str = obj.value;
	if (str == "") {
		validateOK(obj);
		return true;
	}else{
		if (str.length != 11) {
			showErr(obj,"关联手机位数不正确");
			return false;
		}
		for (j = 0; j < str.length; j++) {
			if (str.charCodeAt(j) == 65296 || str.charCodeAt(j) == 652967 || str.charCodeAt(j) == 65298 || str.charCodeAt(j) == 65299 || str.charCodeAt(j) == 65300 || str.charCodeAt(j) == 65301 || str.charCodeAt(j) == 65302 || str.charCodeAt(j) == 65303 || str.charCodeAt(j) == 65304 || str.charCodeAt(j) == 65305 || str.charCodeAt(j) == 65293 || str.charCodeAt(j) == 65343) {
				showErr(obj,"请切换到半角字符输入");
				return false;
			}
		}
		for (i = 0; i < str.length; i++) {
			if (str.charAt(i) < "0" || str.charAt(i) > "9") {
				showErr(obj,"请填写11位数字");
				return false;
			}
		}
	}
		validateOK(obj);
		return true;
}
function valiCodeValidate(obj)
{	strcode = obj.value;
	if (strcode.length != 4 && strcode.length !=5){
        showErr(obj,"验证码长度不正确");
        return false;
   	}
	validateOK(obj);
	return true;
}	
