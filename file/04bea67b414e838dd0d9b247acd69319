<script>
$(document).ready(function(){
	//选择用户 圆圈蓝灰切换
	$('.son_title').click(function(){
		$('.son_title').removeClass('blue').addClass('gray');
		$(this).addClass('blue');
	})
		
	//选择 当前获取焦点的input 其父级div边框变色, 普通用户变蓝其他用户变灰
	$('input').focus(function(){
		$(this).parent('div').css('border','1px solid #24acfa');
		$('.pt_user .son_title').addClass('blue');
		$('.qt_user .son_title').removeClass('blue').addClass('gray');
	});
	$('input').blur(function(){
		$(this).parent('div').css('border','1px solid #e3e3e3');
	});
			
	//鼠标在a01上时，其他用户变蓝普通用户变灰;input父级div边框变灰并失去焦点;请插入卡字样变蓝;字变蓝	
	$('.qt_user .a01').hover(function(){
		$('.qt_user .son_title').addClass('blue');
		$('.pt_user .son_title').removeClass('blue').addClass('gray');
		$('input').parent('div').css('border','1px solid #e3e3e3');
		$('input').blur();
		$('.insert').eq($('.qt_user .a01').index(this)).css('color','#f72525');
		$(this).css('color','#24acfa');
	},function(){
		$('.insert').css('color','#aaa');	
		$(this).css('color','#666');	
	});
	
})
</script>

