<script type="text/template" id="tmplHotelTitle">
<{{if IsOptional}}div{{else}}h3{{/if}} class="htl_tit" data-hotel="title">
    <a title="${HotelName}" class="foreign_hotel" href="javaScript:;" {{if IsOptional}}class="unclick_title"{{else}}onclick="ProductDetail.show({position:'${GLON}|${GLAT}', htlName:'${HotelName}',hotelId:'${Hotel}',masterHotelId:'${MasterHotelID}',cityId:'${City}'});"{{/if}}>${HotelName}</a>
   {{if IsStarLicence && Star > 0}}
   <span class="hotel_stars0${Star}" title="${StarDesc}"></span>
   {{else}}
   <span class="{{if parseInt(CustomerEval) == CustomerEval}}hotel_diamond{{else}}hotel_halfdiamond{{/if}}0${Math.ceil(CustomerEval)}" title="${StarDesc}"></span>
   {{/if}}
   {{if IsOptional}}
   {{if Score>0 && NoVoters>0}}
	<div class="v1_score">
		<p><span class="bignum unclick_title">${Score}</span>/5分 </p>
        {{if RecommendPercent>0}}<p><span class="unclick_title">${parseInt(RecommendPercent*100)}%用户推荐</span></p>{{/if}}
		<p><a href="javaScript:;" class="unclick_title" >${NoVoters}位住客点评</a></p>
	</div>
   {{/if}}
   {{else}}
   {{if Score>0 && NoVoters>0}}
   <a title="${HotelName}" class="htl_pj unclick_title" href="javaScript:;">
        <dfn>${Score}</dfn>/5分
    </a>
     {{if RecommendPercent>0}}<a class="htl_pj">${parseInt(RecommendPercent*100)}%用户推荐</a>{{/if}}
   <a title="" href="javascript:;"  class="htl_pj htl_pj_blue" 
