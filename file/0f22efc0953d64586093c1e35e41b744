	var Index = 0;	
	function ChangeTag(thisobj)
	{
		var ArrConDiv = new Array();
		var Atags = thisobj.parentNode.getElementsByTagName("a");		
		for(var i=0;i<Atags.length;i++)
		{
			if(Atags[i] == thisobj)
			{
				Index = i;
				Atags[i].className = "here";	
			}
			else
			{
				Atags[i].className = "";	
			}
		}		
		var DivTags =  thisobj.parentNode.parentNode.getElementsByTagName("div");
		
		for(var j=0;j<DivTags.length;j++)
		{
			if(DivTags[j].className.indexOf("gk-sort") != -1)
			{
				ArrConDiv.push(DivTags[j]);
			}
		}
		for(var k=0;k<ArrConDiv.length;k++)
		{
			if(k == Index)
			{
				ArrConDiv[k].style.display = "block";	
			}
			else
			{
				ArrConDiv[k].style.display = "none";		
			}
		}
	}
</script>

