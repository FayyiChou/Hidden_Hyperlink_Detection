  f.appendChild(input);
  f.appendChild(input2);
  f.appendChild(input3);
  document.body.appendChild(f);
  f.submit();
  return false;
}
function search4(key) {
	var bor = document.getElementById("bor");
	bor.value = key;
	search1();
}
function search2(key) {
	var bor = document.getElementById("bor");
	bor.value = key;
	var radioForm = document.getElementById("radioForm");
	var i = 0, selected = 0;
	for (; i < radioForm.rad.length; i++) {
		if (radioForm.rad[i].checked) {
			selected = i;
			break;
		}
	}
	if (selected == 0) {
		search1();
	}
	else {
		search3();
	}
}
function search3(){ 
  var ctitle=document.getElementById("bor").value; 
  var f=document.createElement("form");
