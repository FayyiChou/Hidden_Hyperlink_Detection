<script type="text/javascript">
$(document).ready(function(){
	$("#client-infos li").hover(function(){
        $(this).find(".shadow,.qrcode").fadeIn();
    },function(){
       	$(this).find(".shadow,.qrcode").fadeOut();
    })
	var b_length = $('.banner_imgs div').length;
	for (var i = 1; i <= b_length; i++) {
		var _li = $('<li value-index="' + i + '"></li>').appendTo('.banner_btns .carousel-indicators');
		if (i == 1) _li.addClass('active');
	}
	banners_show();
	
	$('#client-infos ul').carouFredSel({
		prev:'#client-infos .ct-prev',
		next:'#client-infos .ct-next',
		scroll:{ duration:600, pauseOnHover:true }
	});
	
	$('#banner_B3 .-hand').css({ right:0, bottom:-10 });
	setTimeout(function(){
		$('#banner_B1 .-hand').animate({ right:0, bottom:-10 }, 600);
	}, 500);
	if ($(window).width() > 1100){
		$('#banner_B3 .-hand2').css({ right:170, bottom:-10 });
		setTimeout(function(){
			$('#banner_B1 .-hand2').animate({ right:170, bottom:-10 }, 600);
		}, 400);
	}
	
	// todo others
	$('.introducing .block').mouseenter(function(){ $(this).addClass('animate'); });
	$('.introducing .block').mouseleave(function(){ $(this).removeClass('animate'); });
});
var banners_show = function(){
	var crtIdx = 1, maxIdx = $('.banner_imgs>div').length;
	var show = function(idx, clk){
		$('#banners').animate({ 'opacity':0.1 });
		$('#banner_B' + crtIdx).animate({ 'margin-right':-3000 }, function(){
			if (clk !== true){
				if (++idx > maxIdx) idx = 1;
			}
			$('#banners').removeClass('B1 B2 B3 B4 B5 B6 B7 B8 B9').addClass('B' + idx);
			$('.banner_btns .carousel-indicators li').removeClass('active').eq(idx - 1).addClass('active');
			$('#banners').animate({ 'opacity':1 });
			$('#banner_B' + idx).animate({ 'margin-right':0 }).show();
			$(this).hide();
			crtIdx = idx;
		});
	};
	var speed = 1000 * 8;
	var bTimer = setInterval(function(){ show(crtIdx); }, speed);
	$('.banner_btns .carousel-indicators li').click(function(){
		if ($(this).hasClass('active')) return;
		clearInterval(bTimer);
		crtIdx = show(~~$(this).attr('value-index'), true);
		bTimer = setInterval(function(){ show(crtIdx); }, speed);
	});
};

//公告新闻
$(document).ready(function(){
	var wh = $(window).height() - 60;
	//$('.section :eq(0)').height(wh);
	/*
	$('#onepage').onepage_scroll({ 
		sectionContainer:'.section', updateURL:false, loop:false, easing:'ease',
		beforeMove:function(idx){},
		afterMove:function(idx){}
	});
	*/
	
	var uupp = 0, uupp2 = $('.-updatelog li').length;
	setInterval(function(){
		uupp++;
		if (uupp >= uupp2) uupp = 0;
		$('.-updatelog').animate({ 'margin-top':uupp*-24 }, 600);
	}, 6000);
	
	$('.fs-customer-2').css({ opacity:0 });
	$('.fs-dongtai-2').css({ left:-500 });
	$('.fs-shenpi-1').css({ right:230, opacity:0 });
	$('.fs-shenpi-2').css({ right:-30, opacity:0 });
	$('.fs-self-design-1').css({ opacity:0, bottom:70 });
	var fs = $('.fs');
	var fs0_top = fs.eq(0).offset().top;
	var fs1_top = fs.eq(1).offset().top;
	var fs2_top = fs.eq(2).offset().top;
	var fs3_top = fs.eq(3).offset().top;
	$(window).scroll(function(){
		var wtop = $(window).scrollTop();
		if (fs0_top > 0 && wtop > fs0_top - 300) {
			fs0_top = 0;
			fs_customer();
		} else if (fs1_top > 0 && wtop > fs1_top - 300) {
			fs1_top = 0;
			fs_dongtai();
		} else if (fs2_top > 0 && wtop > fs2_top - 300) {
			fs2_top = 0;
			fs_shenpi();
		} else if (fs3_top > 0 && wtop > fs3_top - 300) {
			fs3_top = 0;
			fs_self_design();
		}
	});
});
var fs_customer = function(){
	var fs2 = $('.fs-customer-2').css({ opacity:0 });
	setTimeout(function(){
		fs2.animate({ right:-90, opacity:1 }, 1000);
	}, 0);
};
var fs_dongtai = function(){
	var fs2 = $('.fs-dongtai-2').css({ left:-500 });
	setTimeout(function(){
		fs2.animate({ left:225 }, 1000);
	}, 0);
};
var fs_shenpi = function(){
	var fs1 = $('.fs-shenpi-1').css({ right:230, opacity:0 });
	var fs2 = $('.fs-shenpi-2').css({ right:-30, opacity:0 });
	setTimeout(function(){
		fs1.animate({ right:200, opacity:1 }, 1000);
		fs2.animate({ right:0, opacity:1 }, 1000);
	}, 0);
};
var fs_self_design = function(){
	var fs1 = $('.fs-self-design-1');
	setTimeout(function(){
		fs1.animate({ bottom:110, opacity:1 }, 1000);
	}, 0);
};
</script>

