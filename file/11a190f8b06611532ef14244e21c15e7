	<script type="text/javascript">	
		$(function(){
			var date = new Date();
			$("#curDate").html("今天是"+date.formatterDate());
		});
		
		function toLinkAnswer(tid){
			window.open("wlAnswer.htm?tid="+tid);
		}

		function qgMenu(obj){
			var value = $(obj).val();
			if(value != -1){
				window.open($(obj).val());
			}
		}

		var diag;
		function zOpen(type){
			var title = "";
			if(type == "bz"){
				title = "部长信箱";
			}else if(type == "ts"){
				title = "投诉信箱";
			}
			$("#ttag").val(type);
			
			diag = new Dialog("Diag2");
			diag.Width = 500;
			diag.Height = 400;
			diag.Title = title;
			diag.innerElementId="forlogin";
			diag.OKEvent = zAlert;//点击确定后调用的方法
			diag.show();
		}

		function zAlert(){
			var url = "complainAdd.htm";
			$.post(url, $("#myform").serialize(), function(data){
				if(data.success){
					Dialog.alert(data.message);
					diag.close();
				}
			}, "JSON");
		}
		
	</script>
