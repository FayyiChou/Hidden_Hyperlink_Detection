  <script type="text/javascript">
 function show_b(obj){
  if(obj.value == ''){
  obj.value = '请输入关键字';
  obj.style.color = '#666666'; 
  }
  }
  function show_f(obj){
  if(obj.value == '请输入关键字'){
  obj.value = '';
  obj.style = '';
  }
  }
  var re = true;
  function perceive_video(type_id){
  var video_id = parseInt(19054);
  var type_id  = parseInt(type_id);
  if(video_id && type_id){
  if(re){
  $.post("ajax.php",{video_id:video_id,type_id:type_id,act:'perceive'},
  	function(data){
  	re = false;
  //eval("var data='("+data+")'");
switch(data.type)
{
case 'succ':
$('#ajax_num'+type_id).html(data.data);
break;
case 'error':
alert('error');
break;
default:
break;
}  
  	},'json'
  );
  }
  
  }
  }
  function change_video(type,obj){
  obj.style.background ='#cccccc';
  if(type == 'video_album'){
var  oth = 'video_tags'; 
$("#tags_td").css('background','#dfdfdf');
  }else{
var oth  = 'video_album';
$("#album_td").css('background','#dfdfdf');
  }
  $("#"+type).show();
  $("#"+oth).hide();
  }
  var page_size  = 5;
  var album_page = 1;
  var album_max = 0;
  var album_num = 0;
  //ajax专辑分页
  function album(aa){
  if(aa == '-'){
  if(album_page > 1){
  album_page = album_page - 1;
  }else{
  return false;
  }
  }else if(aa == '+'){
  if(album_page < album_max){
  album_page = album_page +1;
  }else{
  return false;
  }
  
  }
  for(var i=0;i<album_num;i++){
  $("#album_"+i).hide();
  var begin = (album_page-1)*page_size;
  var end   = (album_page-1)*page_size+page_size;
  if(i >= begin && i<end){
  $("#album_"+i).show();
  }
  }
  }
  var tags_page = 1;
  var tags_max = 5;
  var tags_num = 24;
  //ajax专辑分页
  function tags(aa){
  if(aa == '-'){
  if(tags_page > 1){
  tags_page = tags_page - 1;
  }else{
  return false;
  }
  }else if(aa == '+'){
  if(tags_page < tags_max){
  tags_page = tags_page +1;
  }else{
  return false;
  }
  
  }
  for(var i=0;i<tags_num;i++){
  $("#tags_"+i).hide();
  var begin = (tags_page-1)*page_size;
  var end   = (tags_page-1)*page_size+page_size;
  if(i >= begin && i<end){
  $("#tags_"+i).show();
  }
  }
  }
</script> 
