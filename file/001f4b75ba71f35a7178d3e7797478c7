<script type="text/javascript">
    $(function () {
        var time = 0;
        function CountDown() {
            time++;
            $('.ui-list-invest-button-CountDown').find('.CountDown').show(function (e) {
                var nid = $(this).attr('nid');
                var endTime = $(this).attr('verify');
                var NowTime = $(this).attr('nowtime');
                var z = endTime - NowTime;
                if (time >= 300 && z > 0) {
                    window.location.reload()
                }
                GetRTime(endTime, NowTime, this, nid);
                endTime--;
                $(this).attr('verify', endTime)
            })
        }
        function GetRTime(EndTime, NowTime, obj, nid) {
            var t = EndTime - NowTime;
            var h = 0;
            var m = 0;
            var s = 0;
            if(t > 3600){
                $(obj).find('#wait').show();
                $(obj).find('#countDownTime').hide();
            }else if(t > 0 && t < 3600) {
                $(obj).find('#wait').hide();
                $(obj).find('#countDownTime').show();
                h = Math.floor(t / 60 / 60);
                m = Math.floor((t - h * 3600) / 60);
                s = Math.floor((t - h * 3600 - m * 60));
                if (h < 10) {
                    $(obj).find('.t_h').html('0' + h)
                } else {
                    $(obj).find('.t_h').html(h)
                }
                if (m < 10) {
                    $(obj).find('.t_m').html('0' + m)
                } else {
                    $(obj).find('.t_m').html(m)
                }
                if (s < 10) {
                    $(obj).find('.t_s').html('0' + s)
                } else {
                    $(obj).find('.t_s').html(s)
                }
            }else if(t < 0) {
                $(obj).parent().attr('style', '');
                $(obj).parent().attr('href', '/?invest&article_id=' + nid);
                $(obj).text('马上投资');
                var pid = $(obj).parent().attr('pid');
                $("#" + pid).attr('href', '/?invest&article_id=' + nid);
                clearInterval(CountDown)
            }
        }
        setInterval(CountDown, 1000);


        $('div.invest-ico').hover(function () {
            var tip = $(this).attr('tip')

            if (tip != '') {
                $('#bubble_tooltip_content_' + $(this).attr('key')).text(tip);
                $('#bubble_tooltip_' + $(this).attr('key')).fadeIn(200);
            } else {
                $('.bubble_tooltip' + $(this).attr('key')).css('display', 'none');
            }
        }, function () {
            $('#bubble_tooltip_' + $(this).attr('key')).fadeOut();
        });

        $('.safe-tip').hover(function () {
            var tip = $(this).attr('tip');
            var key = $(this).attr('key');
            if (tip != '') {
                $('#bubble_tooltip_' + $(this).attr('key')).show();
                $('#bubble_tooltip_content_' + $(this).attr('key')).text(tip);
                dr = $(this).parent('p').find(".safe-tip").index(this) + 1;
                if (dr == 1)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"18px"});}
                else if (dr == 2)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"48px"});}
                else if(dr == 3)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"78px"});}
                else if(dr == 4)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"110px"});}
                else if(dr == 5)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"138px"});}
                else if(dr == 6)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"171px"});}
                else if(dr == 7)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"200px"});}
                else if(dr == 8)
                {$('#bubble_tooltip_' + $(this).attr('key')).css({"right":"230px"});}
            }
        }, function () {
            $('#bubble_tooltip_' + $(this).attr('key')).hide();
        });

        $('.invest-ico-new, .invest-ico-vip,.invest-ico-app').hover(function () {
            var tip = $(this).attr('tip');
            var key = $(this).attr('key');
            var pid = ($('#p_'+ $(this).attr('pid')).width() + 20) + "px";
            $('#bubble_tooltip_' + $(this).attr('key')+'_'+$(this).attr('pid')).show();
            $('#bubble_tooltip_content_' + $(this).attr('key')+'_'+$(this).attr('pid')).text(tip);
            $('#bubble_tooltip_' + $(this).attr('key')+'_'+$(this).attr('pid')).css({"left":"55px"});
            $('.bubble_top').css({"left":pid});
        }, function () {
            $('#bubble_tooltip_' + $(this).attr('key')+'_'+$(this).attr('pid')).hide();
            $('.bubble_top').css({"left":"20px"});
        });
    });

</script>

