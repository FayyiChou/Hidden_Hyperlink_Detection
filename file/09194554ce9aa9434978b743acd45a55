		<script>$(function(){

            var floor=$(".user_floor"),
                leave_btn=$(".user_leave_area ul li"),
                body=$("html,body");

            leave_btn.bind("click",function(){
                if($(this).hasClass("music_btn"))return false;
                var _index=$(this).index();
                var top=floor.eq(_index).offset().top;
                body.stop().animate({"scrollTop":top});

            });
            var oLeave=$(".user_leave_area"),
                win=$(window);

            win.bind("scroll",function(){
                if(win.scrollTop()>800){
                    oLeave.css("display","block");
                }else{
                    oLeave.css("display","none");
                }
            });
            var rule=$(".user_rule"),
                text=rule.find(".area_text"),
                ruleBtn=rule.find(".rule_btn"),
                ruleBtnImg=ruleBtn.find("div");

            ruleBtn.attr("height","on");

            ruleBtn.bind("click",function(){

                if($(this).attr("height")=="on"){
                    $(this).attr("height","off");
                    text.stop().animate({"height":886});
                    ruleBtnImg.css("display","none");
                    ruleBtnImg.eq(1).css("display","block");
                }else if($(this).attr("height")=="off"){
                    $(this).attr("height","on");
                    text.stop().animate({"height":100});
                    ruleBtnImg.css("display","none");
                    ruleBtnImg.eq(0).css("display","block");
                }
            });

        })</script>
