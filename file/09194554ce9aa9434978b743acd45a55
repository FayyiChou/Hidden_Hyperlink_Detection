<!-- 活动页面参数向下传递 -->
<script>
    (function() {
        var jHref = $("a[href*='//'], area[href*='//']"), search = location.search;
        jHref.each(function() {
            var _this = $(this);
            var href = _this.attr("href"), h = href, anchor = '';
            if (href.indexOf('#') != -1) {
                href = href.substring(0, href.indexOf('#'));
                anchor = h.substring(h.indexOf('#'));
            }

            var arrUrl = href.split('?');
            if(arrUrl.length === 1) {
                _this.attr("href", href + search + anchor);
            } else {
                _this.attr("href", href + "&" + search.substring(1) + anchor);
            }
        });
    })()
</script>							 </div>
