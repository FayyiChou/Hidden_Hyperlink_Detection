    function bindEventForTraffic(_city){
    var _ps = $("start_name");var _ep = $("end_name");
    if(_ps){attachEventListener(_ps,'keyup',function(evt){suggestRoadAddrInCity(evt,_city);},false);attachEventListener(_ps,'keydown',function(evt){responseInput(evt,this);},false);}
    if(_ep){attachEventListener(_ep,'keyup',function(evt){suggestRoadAddrInCity(evt,_city);},false);attachEventListener(_ep,'keydown',function(evt){responseInput(evt,this);},false);}
    }
    DomReady.add(function(){bindEventForTraffic('包头');});
    //设置当前登录的用户ID
    AB.set('curUid','');
