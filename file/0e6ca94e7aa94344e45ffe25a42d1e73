<script>

function display_yes(num){
  if(num==1){
	  if(document.getElementById('linklink1').style.display=='none'){
		document.getElementById('linklink1').style.display='block';
		document.getElementById('linklink2').style.display='none';
		document.getElementById('linklink3').style.display='none';
		return;
	  }else if(document.getElementById('linklink1').style.display=='block'){
		document.getElementById('linklink1').style.display='none';
		document.getElementById('linklink2').style.display='none';
		document.getElementById('linklink3').style.display='none';
		return;
	  } 
  }

  if(num==2){
	  if(document.getElementById('linklink2').style.display=='none'){
		document.getElementById('linklink2').style.display='block';
		document.getElementById('linklink1').style.display='none';
		document.getElementById('linklink3').style.display='none';
		return;
	  }else if(document.getElementById('linklink2').style.display=='block'){
		document.getElementById('linklink2').style.display='none';
		document.getElementById('linklink1').style.display='none';
		document.getElementById('linklink3').style.display='none';
		return;
	  } 
  }

  if(num==3){
	  if(document.getElementById('linklink3').style.display=='none'){
		document.getElementById('linklink3').style.display='block';
		document.getElementById('linklink1').style.display='none';
		document.getElementById('linklink2').style.display='none';
		return;
	  }else if(document.getElementById('linklink3').style.display=='block'){
		document.getElementById('linklink3').style.display='none';
		document.getElementById('linklink1').style.display='none';
		document.getElementById('linklink2').style.display='none';
		return;
	  } 
  } 
}


</script>
