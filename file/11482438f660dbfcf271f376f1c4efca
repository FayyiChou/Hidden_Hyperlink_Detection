<script langurage="javascript">
function isBlank( s )
{
  var len=s.length;
  for( i = 0; i < len; i ++ )
  {
    if( s.charAt(i) != " " )
    return false;
  }
  return true;
}
function checkS(){
var f = document.Sform;
if((isBlank(f.keyword.value)||f.keyword.value=="关键字"))
{ alert("请输入搜索关键字!");
	f.keyword.focus();
    return false;
  }
 return true;
  }
</script>
