<script type="text/javascript">
var ci = 5;
for(var ei=0;ei<ci;ei++){
	$("#xd_sub_d"+ei).hide();
}
function showSub(cid,e,cname){
	var as = new Array();
	as["关于我们"] = [-80,450];
	as["产品系列"] = [-360,650];
	as["新闻资讯"] = [-240,450];
	as["诚聘精英"] = [-320,450];
	as["服务支持"] = [-400,450];
	var position = $("#xd_sub_"+cid).position();
	$("#subm_bg").css("padding",0);
	$("#subm_bg").css("margin-top",8);
	$("#subm_bg").css("width",as[cname][1]);
	//
	$("#subm_bg").css("height",30);
	$("#subm_bg").css("line-height","30px");
	$("#subm_bg").css("text-align","left");
	$("#subm_bg").css("left",position.left+as[cname][0]);
	$("#subm_bg").css("top",position.top+20);
	$("#subm_bg").css("position","absolute");
	$("#subm_bg").html($("#xd_sub_d"+cid).html());
	//$("#subm_bg").css("background","#F8F8F8");
	//$("#menu_bot").css("border-top","1px solid #595757");
	$("#subm_bg").css("border-top","1px dotted #595757");
	$("#subm_bg").show();
	//$('#subm_bg').animate({opacity:0.3},1);
}
$("body").click(function(){
	$("#subm_bg").hide();
});
</script>
