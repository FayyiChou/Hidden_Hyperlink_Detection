<script>
	$(function(){
		var timers=[];
		$(".banner_ico img").mouseover(function(){
			for(var timer=timers.shift();timer;timer=timers.shift()){
				clearTimeout(timer);
			}
			$(".banner_ico img").css("border-color", "#cbcbcb");
			$ico=$(this);
			$ico.css("border-color", "#ffffff");
			$("#banner").fadeOut("slow", function(){
				var cpos = $(".banner_ico img").index($ico);
				$(this).attr("src", pics[(cpos >= pics.length)?0:cpos]).fadeIn("slow", function(){
					timers.push(setTimeout(function(){
						$(".banner_ico img").eq(($(".banner_ico img").index($ico)+1)%$(".banner_ico img").size()).mouseover();
					}, 5000));
				});
			});
		}).eq(0).mouseover();
	});
</script>
