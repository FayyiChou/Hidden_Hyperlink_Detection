		},function(){
			$(this).find('.weixin2').slideUp(200);
			}
	)
	$('.weibo').hover(function(){
			$(this).find('.weibo2').slideDown(500);
		},function(){
			$(this).find('.weibo2').slideUp(200);
			}
	)
});

