  <script type="text/javascript">

function getid(id)

{

   return document.getElementById(id);

}

var anndelay = 3000;

var anncount = 0;

var annheight = 24;

var annst = 0;

function announcementScroll()

{

   if( ! annst)

   {

      getid('announcementbody').innerHTML += getid('announcementbody').innerHTML;

      getid('announcementbody').scrollTop = 0;

      if(getid('announcementbody').scrollHeight > annheight * 3)

      {

         annst = setTimeout('announcementScroll()', anndelay);

      }

      else

      {

         getid('announcement').onmouseover = getid('announcement').onmouseout = null;

      }

      return;

   }

   if(anncount == annheight)

   {

      if(getid('announcementbody').scrollHeight - annheight <= getid('announcementbody').scrollTop)

      {

         getid('announcementbody').scrollTop = getid('announcementbody').scrollHeight / 2 - annheight;

      }

      anncount = 0;

      annst = setTimeout('announcementScroll()', anndelay);

   }

   else

   {

      getid('announcementbody').scrollTop ++ ;

      anncount ++ ;

      annst = setTimeout('announcementScroll()', 10);

   }

}

announcementScroll();

</script>
