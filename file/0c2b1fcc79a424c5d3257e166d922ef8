<script language="javascript">
$(document).ready(function(){	
	//开始加载的时候 3秒隐藏掉 恢复原样
/*	var weixin = setInterval(function(){
		$('.weixin_code .close_code').click();
		clearInterval(weixin);
	},1000); */
	//点击关闭按钮
	$('.weixin_code .close_code').click(function(){
		$('.weixin_code').hide();
		$('.weixin_close_open').animate({width:"41px"},300);
	});
	//点击open
	$('.weixin_close_open').click(function(){
		$('.weixin_close_open').animate({width:"0px"},300);
		$('.weixin_code').show();
	});
});
