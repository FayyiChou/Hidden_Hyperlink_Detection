//入口banner
function lh(){
var tinerrk = null;

$('.robg').animate({'height':'100%'},1000)
$('.rk_banner').animate({'height':'600px'},1000)
tinerrk = setTimeout(function(){
   $('.robg').animate({'height':'0'},500)
    $('.rk_banner').animate({'height':'0'},500)
$('.close_p').css({'display':'none'})
 },3000)
$('.close_p').css({'display':'block'})
$('.close_p').click(function(){
$('.robg').animate({'height':'0'},500)
    $('.rk_banner').animate({'height':'0'},500)
$('.close_p').css({'display':'none'})
})
}
lh()



