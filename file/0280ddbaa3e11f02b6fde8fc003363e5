<script>

         

   
                  
//首页下拉
   var index;
   $('.nav .xl_div').hover(function(e) {
      $('.fcd').css('display','block')
        index = $(this).index();
        $('.fcd .f_box').css('display','none')
      $('.fcd .f_box').eq(index-1).css('display','block').siblings().css('display','none')
      
    },function(){
        
        $('.fcd .f_box').css('display','none')
        $('.fcd').css('display','none')
    });
        $('.fcd').hover(function(){
          $('.fcd .f_box').css('display','none')
           $(this).css('display','block')
            $('.fcd .f_box').eq(index-1).css('display','block').siblings().css('display','none')
       },function(){
             $('.fcd .f_box').css('display','none')
           $(this).css('display','none')
       })
 
