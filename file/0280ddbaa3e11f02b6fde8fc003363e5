window.onload = function(){

        var timer0 = null;
	var num0 = 0;
	gun0 = 0;
	var oLi = $('.sygd ul li').eq(0).clone();
	$('.sygd ul').append(oLi);
	var iLen0 = $('.sygd ul li').length;
	var oMain = document.getElementById('mian')
	var oHeight = document.documentElement.clientHeight;
	var oWidth = document.documentElement.clientWidth;
	var height = parseInt(oHeight - 204 + 'px')
	oMain.style.width = oWidth + 'px'
	oMain.style.height = oHeight - 204 + 'px'
	$('.sygd').css('width',oWidth + 'px')
	$('.sygd').css('height',oHeight - 204 + 'px')
	$('.sygd ul li').css('width',oWidth + 'px')
	$('.sygd ul li').css('height',oHeight - 204 + 'px')
	$('.sygd ul li img').css('width',oWidth + 'px')
	$('.sygd ul li img').css('height',oHeight - 204 + 'px')
	$('.sygd ul').css('height',height*4)
	for(var i=0; i< (iLen0-1);i++){
		var iLI = document.createElement('li');
		$('.gd_d ol').append(iLI);
	}
	$('.gd_d ol li').eq(0).addClass('current1')
    timer0 = setInterval(function(){
		num0-=height;
		gun0++;
		if(gun0 > (iLen0-2)){
			gun0 = 0
		}
		if(num0 < -height*(iLen0-1)){
			num0 = -height;
			$('.sygd ul').css('top',-height)
		}
		$('.gd_d ol li').eq(gun0).addClass('current1').siblings().removeClass('current1')
		$('.sygd ul').css('top',num0)
	},4000)
	$('.gd_d ol li').click(function(e) {
                 clearInterval(timer0)
		 var index0 = $(this).index()
		 $('.sygd ul').css('top',-index0*height)
		 $(this).addClass('current1').siblings().removeClass('current1')
		 num0 = -index0*height
		 gun0 = index0
               timer0 = setInterval(function(){
		num0-=height;
		gun0++;
		if(gun0 > (iLen0-2)){
			gun0 = 0
		}
		if(num0 < -height*(iLen0-1)){
			num0 = -height;
			$('.sygd ul').css('top',-height)
		}
		$('.gd_d ol li').eq(gun0).addClass('current1').siblings().removeClass('current1')
		$('.sygd ul').css('top',num0)
	    },4000)
        });


}	
       


