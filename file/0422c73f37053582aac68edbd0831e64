<script language="javascript">
$(window).bind('scroll',goscroll);
$(window).bind('resize',goscroll);
function goscroll(){
	setTimeout(function(){
		$('#page-header').each(function(){
			var scroH = $(window).scrollTop();
			if(scroH>144){
				$(this).css({'position':'fixed','top':'-99px','z-index':1000});
				$('#header-logo').hide();
				$('#headtop').addClass('hpic');
			}else {
				$(this).css({'position':'absolute','top':'0px'});
				$('#header-logo').show();
				$('#headtop').removeClass('hpic');
			}
		});
	},50);
}
</script>

		
