<script language="javascript">
 function showRdKfAd(){
   var isClose=$.cookie("rdAd1");
   if(isClose=="hide11"){
      $("#floatc").hide();
   }else{
      $("#floatc").show();
   }
}
 function showRdDoAd(){
   var issClose=$.cookie("rdAd2");
   if(issClose=="hide22"){
      $("#floatdown").hide();
   }else{
      $("#floatdown").show();
   }
}
$("#floatclose").click(function(){
  $("#floatc").hide();
  $.cookie("rdAd1", "hide11",{expires: 7200});
});
moveTips("floatc",100,5,"right");
showRdKfAd();
</script>


 
