        jQuery(document).ready(function () {
            jQuery(".mtcomment,.mainright,.leftbox").find("a").bind("click", function () {
                var location = jQuery(this).attr("href");
                var newsid = '24735629';
                var regx = /^((https|http)?:\/\/)[^\s].+\.fang\.com\/?$/;
                var flag = regx.test(location);
                if ((flag || location.indexOf("/chushou/") > -1 || location.indexOf("/shou/") > -1 || location.indexOf("/chuzu/") > -1 || location.indexOf("/zu/") > -1 || location.indexOf("/house") > -1 || location.indexOf("/house1") > -1 || location.indexOf("/villa") > -1 || location.indexOf("/school") > -1 || location.indexOf("/bus") > -1) && location.indexOf("/newsecond") == -1)
                {
                    jQuery(this).attr("href", location + "?ztzh_uuid=pc_" + newsid);
                }
            });
            var url = "http://u.soufun.cn/qrcode.php?url=" + encodeURIComponent("https://m.fang.com/news/TJ/13202_24735629.html?src=pcesf") + "&type=news&resize=132&logo=" + encodeURIComponent("http://img.soufun.com/news/image/newscenter/20100811/images/ersflogo.jpg");
            jQuery("#esfQRcode").attr("src", url);
            var newsid = jQuery("#newsId").val();
            if (newsid) {
                jQuery("#zancon").click(function () {
                    jQuery.ajax({
                        type: "get",
                        url: "/EsfHouse/Ajax/News/NewsZanCai.aspx?flag=1&newsid=" + newsid,
                        dataType: "json",
                        success: function (userData) {
                            var data = eval(userData);
                            if (data) {
                                jQuery("#zan").html("(" + data[0].zancount + ")");
                            }
                        },
                        error: function () {

                        }
                    });
                    jQuery("#zancon").unbind("click");
                });
                jQuery("#caicon").click(function () {
                    jQuery.ajax({
                        type: "get",
                        url: "/EsfHouse/Ajax/News/NewsZanCai.aspx?flag=-1&newsid=" + newsid,
                        dataType: "json",
                        success: function (userData) {
                            var data = eval(userData);
                            if (data) {
                                jQuery("#cai").html("(" + data[0].caicount + ")");
                            }
                        },
                        error: function () {

                        }
                    });
                    jQuery("#caicon").unbind("click");
                });
            }
            
            if (jQuery("#titimg").width() > 650) {
                jQuery("#titimg").css("width", 650);
            }
            else {
              
            }
        });
