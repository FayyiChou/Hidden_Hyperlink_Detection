    function changeWindowWidth() {
        var w = document.documentElement.clientWidth; //获取页面可见高度
        if (Number(w) < 1200) {
            document.getElementById("floatDiv").style.display = 'none';
        }
        else {
            document.getElementById("floatDiv").style.display = '';
        }
    }
    jQuery(function () { changeWindowWidth() });
    jQuery(window).bind("resize", function () { changeWindowWidth(); });
    function showDbox(index) {
        var obj = document.getElementById("dbox_" + index);
        obj.style.display = '';
    }
    function hiddenDbox(index) {
        var obj = document.getElementById("dbox_" + index);
        obj.style.display = 'none';
    }
    jQuery(window).scroll(function () {
        var fold = jQuery(window).height() + jQuery(window).scrollTop();
        var w = document.documentElement.clientHeight; //获取页面可见高度
        if (fold > w) {
            jQuery("#floatingup").css("display", "");
        }
        else {
            jQuery("#floatingup").css("display", "none");
        }
    })
</script>
