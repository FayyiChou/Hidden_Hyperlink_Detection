    // JavaScript Document
    $(function(){
        var i=0;
        var j=$(".turnimgwrap ul").size()-1;
        function littleshowbotr1()
        {
            i++;
            if(i>j) i=0;
            $(".turnimgwrap ul").eq(i).show().siblings().hide();
        }
        var action=setInterval(littleshowbotr1,3000);
        $(".rightbtn").click(
            function() {
                clearInterval(action);
                i = i + 1;
                if (i > 1) {
                    i = 0;
                }
                $(".turnimgwrap ul").eq(i).show().siblings().hide();
                return false;
            });
        $(".leftbtn").click(
            function() {
                clearInterval(action);

                function turnlast() {
                    i = i - 1;
                    if (i < 0) {
                        i = 1;
                    }
                    $(".turnimgwrap ul").eq(i).show().siblings().hide();
                }

                turnlast();
                return false;
            });
        $(".leftbtn").hover(function() {
            $(".leftbtn").css("background", "url(http://esf.js.soufunimg.com/esf/esf/online/secondhouse/old/secondhouse/image/esfnew/images/fang/news/images/leftyes.jpg) no-repeat");
