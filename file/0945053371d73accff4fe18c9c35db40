var lunbo_count = 0;
var lunbo_stop = false;
var i=0;
function lunbo()
{
 if(lunbo_count<=1) return;
 if(lunbo_stop){
 setTimeout("lunbo()" , 3000);
 return;
 }
 i++;
 $('.tl_lunbo li').hide();
 $('.tl_lunbo li').eq(i).show();
 if(i+1>lunbo_count-1){
 i=-1;
 }
 setTimeout("lunbo()" , 5000);
}
$(function(){
 lunbo_count=$('.tl_lunbo li').size();
 if(lunbo_count>1) {
 setTimeout("lunbo()" , 2000);
 }
 $(".tl_lunbo").hover(function(){lunbo_stop=true;},function(){lunbo_stop=false;});
});
</script>
