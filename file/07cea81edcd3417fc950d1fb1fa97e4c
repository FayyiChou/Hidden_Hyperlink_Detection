function change_url(url)
{
  var q0 = '';
  var q1 = encodeURIComponent(document.qform.q.value);
  if (q0 != q1)
    url = url.replace(q0, q1);
  window.location.href = url;
}

function change_cp(cp)
{ 
  return; 
  change_url('' + cp);
}

function change_gl(gl)
{
  return;
  change_url('' + gl);
}

function change_f(f)
{
  if (document.qform.q.value != '' && f != '_all')
  {
    change_url('' + f);
  }
}

function change_s(s)
{
  return;
  change_url('' + s); 
}

window.onload = function()
{
  document.qform.q.focus();
}
</script>
