    <script>

$(function(){

	$('#kanfang').ajaxForm({

		beforeSubmit: checkForm, // 此方法主要是提交前执行的方法，根据需要设置

		success: complete, // 这是提交后的方法

		dataType: 'json'

	});

	function checkForm(){

		if( '' == $.trim($('#house_enroll_name').val())){

			alert('姓名不能为空');

				$('#house_enroll_name').focus(); 

			return false;

		}

       if( '' == $.trim($('#house_enroll_phone').val())){

			alert('电话号码不能为空');

				$('#house_enroll_phone').focus(); 

			return false;

		}
		
	 if (!$("#house_enroll_phone").val().match(/^(((13[0-9]{1})|(15[0-9]{1})|(17[0-9]{1})|(18[0-9]{1}))+\d{8})$/)) { 

	      alert('联系电话格式不正确,请重新输入');

			$('#house_enroll_phone').focus(); 

			return false;

		}
		
	if( '' == $.trim($('#house_enroll_kanfanid').val())){

			alert('看房地产不能为空');

				$('#house_enroll_kanfanid').focus(); 

			return false;

		}


 }

	function complete(data){

		if(data.status==1){

			alert('看房团添加成功，等待管理员回复');

			window.location.href="http://www.364000.com";

			return false;

		}else{

			$('#kanfang').html(data.info).show();

			return false;	

		}

	}
});
</script> 
