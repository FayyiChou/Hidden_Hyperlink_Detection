        isIE()
        function isIE(){
            if (window.navigator.userAgent.indexOf("MSIE")>=1){
                var aa1=document.getElementById("link")
                aa1.href="http://www.21rv.com/Public/Home/css/ie.css"
            }

            else{
                console.log("bbbbbbb")
            }

        }
