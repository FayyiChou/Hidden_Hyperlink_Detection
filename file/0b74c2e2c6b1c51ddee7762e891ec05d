  <script type="text/javascript">
    //导航高亮
    $('.header-nav').find('a[href="/home/news/index.html"]').closest('li').addClass('curr');

    // 加入收藏
    $("#addShoucang").click(function() {
        $.post('/home/article/collection.html', {
            id: $(this).attr('data-value')
        }, function(data) {
            alert(data.info);
        });

    });

    // 评论发布
    $("#comment").click(function() {
        if ($("textarea").val() == "") {
            alert("不能为空");
            return;
        };
        // $("form").submit();
        $.ajax({
                url: "/home/article/add/id/27316.html",
                type: 'POST',
                dataType: 'text',
                data: {
                    'content': $('[name=content]').val(),
                    'sid': $('[name=sid]').val(),
                    'uid': $('[name=uid]').val()
                },
            })
            .success(function(e) {
                $('[name=content]').val('');
                alert("评论已提交");
            })
            .fail(function(e) {})
            .always(function(e) {});


    });

    $(".s1").one("click", function(e) {
        var nice = $(e.currentTarget);
        $.ajax({
                url: "/home/article/nice.html",
                type: 'POST',
                dataType: 'text',
                data: {
                    'id': $(e.currentTarget).attr('data')
                },
            })
            .success(function(e) {
                var i = nice.parent().text();
                nice.parent().find('span').text(parseInt(i) + 1);
            })
            .fail(function(e) {})
            .always(function(e) {});
    });

    // 点击率
    var id = 27316;
    $.post("/home/article/click.html",{id:id},function(data){});
    </script>  
