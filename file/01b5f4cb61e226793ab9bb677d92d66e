          cb = (function() {
              var u = window.navigator.userAgent.toLocaleLowerCase(),
              msie = /(msie) ([\d.]+)/,
              chrome = /(chrome)\/([\d.]+)/,
              firefox = /(firefox)\/([\d.]+)/,
