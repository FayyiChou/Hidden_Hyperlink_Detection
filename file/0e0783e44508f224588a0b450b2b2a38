<script type="text/javascript">
$.dialog.setConfig({'base':'http://www.huimg.cn/lib/dialog', skin:'xiaobaike'});
$(document).ready(function(){
cas_synlogin('http://passport.baike.com/loginInfo.jsp?requestType=statusAndTime', "http://passport.baike.com/login.do?site_ource=xiaobaike_page&service=http%3A%2F%2Ftuzi.baike.com%2F%3Fautologin%3Dyes");
});
$.getScript("http://www.huimg.cn/xiaobaike/js/baike_search.js");
var XBK_CLICK_OBJECTIDS = ['ad_banner_606'];
setTimeout(function(){
$.getScript("http://stat.baike.com/js/webstat_manage_xbk.js");
}, 2000);

//选中当前导航
(function ($) {
var urlArr = location.href.split ('/');
var lastpath = urlArr.pop ();
var lastbutonepath = urlArr.pop ();
var navArr = ['index','?m=photo','category-0.html|article','wenda|question','?m=category|category-','?m=article&a=rss','app'];
for(var i=0;i<navArr.length ;i++ ){
var temp = navArr[i].split ('|');
for(var n=0;n < temp.length ;n++ ){
if(lastpath.indexOf(temp[n]) == 0){
$('#nav li').removeClass('current').eq(i).addClass('current');
return;
}
if(i==4){
var reg1 = /category-([1-9][0-9]+).html/;
var reg2 = /category-([0-9]+)-([0-9]+).html/;
if(reg1.test (lastpath)){
$('#nav li').removeClass('current').eq(4).addClass('current');
return;
}
if(reg2.test (lastpath)){
$('#nav li').removeClass('current').eq(4).addClass('current');
return;
}
}
}
}
if(lastbutonepath.indexOf('wendatag') == 0){
$('#nav li').removeClass('current').eq(3).addClass('current');
return;
}
$('#nav li').removeClass('current').eq(0).addClass('current');

setTimeout(function(){
if($("#banner_ad").size() && $("#banner_ad img").height() ){
$("#banner_ad").height( $("#banner_ad img").height() );
}
}, 1000);
})(jQuery);

// 显示创建文章按钮
var article_button = '<div class="t_0"><p>点击以下按钮创建文章</p><div class="btn_cjwz">';
if($("#username").length>0) {
article_button += '<a href="http://www1.baike.com/memcp.php?m=article&a=create&baike_id=606 target="_blank">创建文章</a>';
}else {
article_button += '创建文章';
}
article_button += '</div>';
if($("#username").length==0) {
article_button += '<p>请先<a href="/?m=user&a=login">登录</a></p>';
}
article_button += '</div>';

$("div.column h2:contains('创建文章') + div.content").html(article_button);


</script>
