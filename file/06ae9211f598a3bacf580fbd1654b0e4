(function(){
/*
获取meta[keywords]中的第一个关键字 ，如果为空，取文章标题中的第一个_前的字符串，eg：锚点文章
*/
var _keywords=$('meta[name="keywords"]').attr('content');
_keywords=_keywords.split(';',1)[0];
/*
获取文章标题中的第一个_前的字符串
*/
var getTitle=function(){
var _title=document.title;
_title=_title.split('_',1)[0];
return _title;
}
_keywords=!!_keywords?_keywords:getTitle();
$.ajax({
type:"GET",
url:"http://kuaiwen.pcbaby.com.cn/api/baike/listhotwords.do?keyword="+encodeURIComponent(_keywords),
success:function(data){
var html='',li = data.hotwords,liLen = li.length;
html+='<ul class="jh-titles clearfix">';
for(var i=0;i<liLen;i++){
html+='<li class="aPointLink"><a href="'+li[i].url+'" title="'+li[i].title+'">'+li[i].title+'</a></li>';
}
html+='</ul>';
$('#JajaxJuhe').html(html);
},
error:function(data){
console.log(data.msg);
}
});
