<script type="text/javascript">
	 $(function(){
		$('#qh_menu div').click(function(){
			var liId = $('#num').val();
			if(liId==0){
				$('#0s').show();
				$('#1s').hide();
				$('#num').val(1);
				$('#0').css('border-top','3px solid #82b327');
				$('#1').css('border-top','3px solid #727172');
			}else{
				$('#1s').show();
				$('#0s').hide();
				$('#num').val(0);
				$('#1').css('border-top','3px solid #82b327');
				$('#0').css('border-top','3px solid #727172');
			}
		});
	})
	function show_img(url){
		document.getElementById('banner').style.background='url('+url+')';
		document.getElementById('banner').style.backgroundPosition='center';
		document.getElementById('banner').style.backgroundRepeat='no-repeat';
//		i=0;
//		time();
		}
		
