$(document).ready(function () { 
var is_open = false; 
var close_handel = null; 
$("#change, .xiala_bg").mouseover(function () { 
if (close_handel != null) { 
clearTimeout(close_handel); 
} 
if (!is_open) { 
$(".xiala_bg").slideDown(500); 
} 
}).mouseout(function () { 
close_handel = setTimeout(function () { $(".xiala_bg").slideUp(500); }, 500); 

}); 
}); 
