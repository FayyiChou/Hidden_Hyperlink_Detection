<script type="text/javascript">
 var loginpw = 0;
 function login(){
   datatpl= '<div>邮箱 / 手机：<input style="width:170px;" id="Email" onKeyDown="if (window.event.keyCode==13) updata();" name="111" type="text" /></div>';
   datatpl+= '<div>密 码：<input class="span3" id="password" onKeyDown="if (window.event.keyCode==13) updata();" name="111" type="password" /></div>';
   datatpl+= '<div>验证码：<input class="span2" id="code" onKeyDown="if (window.event.keyCode==13) updata();" name="111" type="text" /><span style="margin-left:10px"><img id="codeimg" src="/login/_vcode.html" alt="看不清楚,点击图片刷新" onClick="autoclick()"/></span></div>';
   datatpl+= '<div style="float:left"><span><a href="/qqlink/oauth/login.php"><img src="../../qqlink/images/qqlogin.png" /></a></span><input style="margin-top:-18px;margin-left:10px;" class="btn btn btn-success span2" name="11" onclick="updata()" type="button" value="登陆" /></div>';
     art.dialog({
        lock: true,
        opacity: 0.87,
        content: datatpl
    });
 }
 
 function updata(){
    var formdata = {
      'id' : $('#id').val(),
      'Email' : $('#Email').val(),
      'password' : $('#password').val(),
      'url' : $('#url').val(),
      'code' : $('#code').val()
    };
    if(loginpw == 0){
      // 用$.post发送数据
      $.post('/login/checkdata.html', formdata, function(result){
            $('#resulttable').append(result);
            var json = eval("("+result+")");
          art.dialog.tips("<font>"+json.msg+"</font><a href='javascript:yd('"+json.url+"')'><br /><h4 align='center'  class='label label-success'>"+json.tips+"</h4></a>",2);
            if(json.url != 0){
               loginpw = 1;
               setTimeout("yd('"+json.url+"')",2000); 
            }
            else
            {
              autoclick(); // 自动刷新验证码
            }
      });
    }
}

// 模拟点击验证码
function autoclick(){
  var codeimg = document.getElementById("codeimg");
  codeimg.src="/index.php?c=login&a=_vcode&" + Math.random(1);
}

function yd(url)
{
   window.location.reload(); 
}
</script><div id="main_banner">
