<script type="text/javascript" src="http://www.wopeng.net/tpl/js/jquery-ui-1.10.1.custom.js"></script> 
<script>

  jqs(function(){
    jqs("#aFloatTools_Show").click(function(){
      jqs('#divFloatToolsView').animate({width:'show',opacity:'show'},100,function(){$('#divFloatToolsView').show();});
      jqs('#aFloatTools_Show').hide();
      jqs('#aFloatTools_Hide').show();        
    });
    jqs("#aFloatTools_Hide").click(function(){
      jqs('#divFloatToolsView').animate({width:'hide', opacity:'hide'},100,function(){$('#divFloatToolsView').hide();});
      jqs('#aFloatTools_Show').show();
      jqs('#aFloatTools_Hide').hide();  
    });
  });
  
jqs(function() {
    var ddd = jqs("#ind_cen05").tabs({ active: 3 });
});
jqs(document).ready(function () {
    // 菜单1
    jqs("#poi_hot li,#wanfa_pic li:first").hover(function () {
        var $this = $(this);
        $this.find('div.intro_info').show();
        Tab('b','bx',5,3);

    }, function () {
        var $this = $(this);
        $this.find('div.intro_info').hide();
    })
