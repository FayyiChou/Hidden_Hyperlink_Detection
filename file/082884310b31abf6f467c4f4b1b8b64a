<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<script src="js/jquery-1.11.3.js"></script>
<!-- Include all compiled plugins (below), or include individual files as needed -->
<script src="js/bootstrap.js"></script>
<script>
    $(".main_cont_btn").click(function () {
        var name = $(":input[name='name']").val();
        var phone = $(":input[name='phone']").val();

        if (name == '' || name == undefined) {
            alert('请填写大名');
            return false;
        }
        if (phone == '' || phone == undefined) {
            alert('请填写手机');
            return false;
        }

        var name_preg = /^[\u4e00-\u9fa5]+$/;
        var phone_preg = /^[1][3-8]\d{9}/;
        if (!name_preg.test(name)) {
            alert('请填写真实姓名');
            return false;
        }

        if (!phone_preg.test(phone)) {
            alert('手机号码有误');
            return false;
        }

        var formData = {"name": name, 'phone': phone};
        $.ajax({
                    'type': 'post',
                    'url': '/index.php?m=Home&c=index&a=apply', 'data'
        :
        formData,
                'dataType'
        :
        'json',
                'success'
        :
        function (data) {
            if (data.status == 200) {
                $(".main_cont_btn_show").css({
                    display: "block"

                });
                $(".main_cont_btn_show").hide(3000);
                //alert(data.msg);
            } else {
                showWin(data.msg);
                //alert(data.msg);
            }
        }
    })
        ;

        function showWin(msg) {
            alert(msg);
        }

    })
</script>
