<script type="text/javascript">
jq(function(){
	jq.each(jq(".news-list-pic"),function(i,obj)
		{
			if(jq(obj).attr("src")=="")
			{
				jq(obj).parent().parent().css("display","none");
				jq(obj).parent().parent().next("div").css("width","100%");
			}

		})
});

</script>

