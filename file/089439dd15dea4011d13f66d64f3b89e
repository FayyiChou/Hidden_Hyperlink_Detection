<!-- tab切换 -->
<script>
$(function(){
var t_li = $(".qxdt .qxdt-area li")
var c_li = $(".qxdt .gqcz-content li")
t_li.hover(function(){
var i = t_li.index($(this));
function way(){
t_li.removeClass("cur").eq(i).addClass("cur");
c_li.hide().eq(i).show();
}
timer=setTimeout(way,500);
},function(){
clearTimeout(timer);
});
});
</script>

