<script>
    function cc(){

        setTimeout(function(){

            //START banner 初始化
            /*var js_jquery=document.getElementById("jqueryjs");*/
            var js_home=document.getElementById("hnjs");
            var imgobj=document.getElementsByTagName("img");

            var bannerimg=document.getElementsByName("js_bannerimg1");
            /* var changelist=document.getElementsByName("js_banner_h");*/

            var banner_h=bannerimg[0].offsetHeight;
            var bannerimgbox=document.getElementById("hnbannerbox")
            var changelistli=bannerimgbox.getElementsByTagName("li")
            /*var bannerbtnbox=document.getElementById("bannerbtnbox");*/
            for(i=0;i<changelistli.length;i++){
                changelistli[i].style.height=banner_h-2+"px";
                changelistli[i].getElementsByTagName("div")[1].style.height=banner_h+"px";
                changelistli[i].getElementsByTagName("div")[0].getElementsByTagName("a")[0].style.height=banner_h+"px";
                changelistli[i].getElementsByTagName("img")[0].style.height=banner_h+"px";
            }

            bannerimgbox.parentNode.style.height=banner_h-2+"px";
            /* bannerbtnbox.getElementsByTagName("a")[0].style.height=banner_h+"px";
             bannerbtnbox.getElementsByTagName("a")[1].style.height=banner_h+"px";*/

            //END banner 初始化
            //START 产品线互动tab 自适应个数
            var interaction_tab=document.getElementById("interaction_tab")
            var tab_list=interaction_tab.getElementsByTagName("li");
            var tab_i=tab_list.length;
            var tab_width=1/tab_i*100+"%";
            var line=document.getElementById("interaction_tab_line");
            line.style.width=tab_width;
            for(i=0;i<tab_i;i++){
                tab_list[i].style.width=tab_width;
            }

            //END 产品线互动tab 自适应个数


            for(i=0;i<imgobj.length;i++){
                var imgurl=imgobj[i].getAttribute("url");
                if(imgurl!=null&&typeof(imgurl)!="undefined"){
                    imgobj[i].setAttribute("src",imgurl);
                }
            }

            /*js_home.src="images/hn.js"*/

            function loadScript(src,callback){
                var script = document.createElement("script")
                script.type = "text/javascript";
                if (script.readyState){ //IE
                    script.onreadystatechange = function(){
                        if (script.readyState == "loaded" ||
                                script.readyState == "complete"){
                            script.onreadystatechange = null;
                            callback();
                        }
                    };
                } else { //Others: Firefox, Safari, Chrome, and Opera
                    script.onload = function(){
                        callback();
                    };
                }
                script.src = src;
                document.body.appendChild(script);
            }

            loadScript("http://image.haier.com/cn/images/hn_jQuery.1.8.3.min.js",function(){
                loadScript("http://image.haier.com/cn/images/jquery.menu-aim.js",function(){
                    loadScript("http://image.haier.com/cn/images/jquery.cookie.js",function(){
                        loadScript("http://image.haier.com/cn/images/hn.js",function(){
                            loadScript("http://image.haier.com/cn/images/hn_index.js",function(){});
                            loadScript("http://image.haier.com/cn/images/hn_module.js",function(){});
                            loadScript("http://image.haier.com/cn/images/hn_login_trs_haier.js",function(){});
                        });

                    }) ;
                }) ;


            })


            /*setTimeout(function(){
             //轮播效果


             //END 产品轮播模块

             },2000)*/

        },1000);

    }
    window.onload=cc;
    var turnFlag = true;

</script>
