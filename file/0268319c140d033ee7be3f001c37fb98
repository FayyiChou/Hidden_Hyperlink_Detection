<script type="text/javascript">

function onchange1()
{
   var c = escape(HX.$("sshy").value)
   window.open("/hyyb.aspx?hname="+c);  
}
function ReturnFilter()
{
    var rd;
   var radioListObj= document.getElementsByName("search_type");
    for(var i=0;i<radioListObj.length;i++)
    {
        if(radioListObj[i].checked) 
        {
           rd = radioListObj[i].value;
        }
    }  
        
    if(rd=="1")
    {
        if(HX.$("multiTxt").value == "" || HX.$("multiTxt").value == "研究机构")
	    {
	        HX.$("multiTxt").value = '';
	    }
	    window.open("/search.aspx?yjjg="+escape(HX.$("multiTxt").value)+"&sshy="+escape(HX.$("sshy").value));
    }
    else if(rd=="2")
    {
        if(HX.$("multiTxt").value == "" || HX.$("multiTxt").value == "分析师")
	    {
	        HX.$("multiTxt").value = '';
	    }
	    window.open("/search.aspx?bgzz="+escape(HX.$("multiTxt").value)+"&sshy="+escape(HX.$("sshy").value));
    }
    else if(rd=="3")
    {
        if(HX.$("multiTxt").value == "" || HX.$("multiTxt").value == "标题")
	    {
	        HX.$("multiTxt").value = '';
	    }
	    window.open("/search.aspx?bgbt="+escape(HX.$("multiTxt").value)+"&sshy="+escape(HX.$("sshy").value));
    }
    
    else if(rd=="0")
    {
         if(HX.$("multiTxt").value == "" || HX.$("multiTxt").value == "输入名称或缩写")
	     {
	        HX.$("multiTxt").value = '';
	        alert("请输入名称或缩写");
	        return false;
    	 }
    	 window.open("/yb_"+escape(HX.$("multiTxt").value)+".shtml");

    	
    }
}
</script>

