                function GetHyData(blist)
                {
                	var hyinfoarray = eval(blist);
                	var _obj = document.getElementById("sshy");
	                _obj.length = 0;
	                if(hyinfoarray == null) return;

	                _obj.length = hyinfoarray.length + 1;
	                _obj.options[0] =new Option("行业分类",0);
	        
	                for(var i=1;i<=hyinfoarray.length;i++)
	                {
	                	_obj.options[i] =new Option(hyinfoarray[i-1].name,hyinfoarray[i-1].value);
	                }
                }
