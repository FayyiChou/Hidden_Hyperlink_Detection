function quxiaojihuo(goods_id){
          $.ajax({
          method:"get",
          url:"/api/pmph/book!deljihuo.do?goods_id="+goods_id,
          dataType:"json",
          success:function(data){
                  location.reload();
          },
          error:function(){
              alert("取消失败");
          }
      });
        
}
