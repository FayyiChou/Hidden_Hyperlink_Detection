var chapterHeight=$("#chapter").height();
function autoheight(){
	var winwidth = $(window).width();
	var rightheight = $(".tab-content").height();
	var nowLeftHeight=$("#chapter").height();
	if(winwidth > 800){
		$("#chapter").css("height", "605px");
		$("#chapter ul").height(605);
		$("#chapter").css("border-bottom", "1px solid rgb(229, 229, 229)");
		$("#chapter ul").css("overflow-y", "auto");
		/* if(rightheight < nowLeftHeight && nowLeftHeight > 485){
			$("#chapter").height(nowLeftHeight - 75);
			$("#chapter ul").height(nowLeftHeight - 75);
		} */
		/* if(rightheight > nowLeftHeight&&rightheight > 476 || nowLeftHeight > 400){
			$("#chapter").height(rightheight - 75);
			$("#chapter ul").height(rightheight - 75);
		} */
	}
}
autoheight();
/* $(".affix-warp #reslist").resize(function(){
	autoheight();
}); */
var oldHeight=0;
setInterval(function(){
	if(oldHeight==0||oldHeight!=$("#reslist").height()){
		oldHeight=$("#reslist").height();	
		autoheight();
	}
},500);

