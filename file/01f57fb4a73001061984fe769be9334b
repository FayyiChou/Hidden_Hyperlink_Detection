<script type="text/javascript">    
    function checkKeyCode(){
		var keyCode = $("#keyCode").val();
		if(keyCode.trim().length==0){
			$.alert("激活码不能为空");
			return false;		
		}
		if(keyCode.trim().length!=12){
			$.alert("激活码长度不正确");
			return false;	
		}
		var  num =  checkKeyCodeExsit(keyCode);
		if(1!=num){
			return false;
		}
	}
    function unsubmit(){
        var keyword=$("#keyword").val();
        if(keyword==null||keyword==""||$.trim(keyword)==""||keyword=="请输入书名、作者名或ISBN号"){
       	 alert("请输入搜索条件");
       	 return false;
        }
      //  $("#form_search_condtion").submit();
      }

</script>
