      <script>
        $(function(){
          var titles=$("#chapter").find(".title");
          function setSales(){
            if($(window).width()< 800){
            $(".book-details-intro .cat-bookitem ul.sales").css("position","static");
            $.each(titles,function(i,item){
             	var a_title_couent=$(item).parent("a").attr("title").split("[")[0];
							$(item).html(a_title_couent.length>45?a_title_couent.substring(0,45)+"...":a_title_couent);
            });
            }else{
               $.each(titles,function(i,item){
                  var a_title_couent=$(item).parent("a").attr("title").split("[")[0];
        	  console.log(a_title_couent.length);
									$(item).html(a_title_couent.length>8?a_title_couent.substring(0,8)+"...":a_title_couent);
               });
               $(".book-details-intro .cat-bookitem ul.sales").css("position","absolute");
            }
          }
          setSales();
          $(window).resize(function(){
             setSales();
          });
          $(".breadcrumb").css("background-color","#FFFFFF");//当前位置导航背景颜色
          $(".list-details-header").find("#h3").css({
            "background-color":"#f5f5f5",
            "padding-left":"10px"
          }).append('<a href="javascript:void(0);" class="openOrCloseBtn openOrCloseBtn1" title="关闭/开起搜索条件">'+
                    '<span id="msn" class="glyphicon glyphicon-chevron-up" style="float: right; color:#E5BC7D; line-height: 40px; padding-right: 12px;"></span><\/a>');
          $(".list-details-header").find("#h4").css({
            "background-color":"#f5f5f5",
            "padding-left":"10px"
          })/*.append('<a href="javascript:void(0);" class="openOrCloseBtn openOrCloseBtn2" title="关闭/开起搜索条件">'+
          		'<span id="msn" class="glyphicon glyphicon-chevron-up" style="float: right; color:#E5BC7D; line-height: 40px; padding-right: 12px;"></span></a>')*/;
          $(".intro-more").find(".inner ").removeClass("hide").prev().remove();
          $(".openOrCloseBtn").eq(0).attr("title","关闭/开起图书详细信息");
          $(".openOrCloseBtn").live("click",function(){
            var index=$(this).parent("h3").index($(".list-details-header").find("h3"));
            var divClass=index==0?"book-details-intro":"affix-selector";
            var _this=this;
            $("."+divClass).find(":input:checkbox").toggle(100);
            $("."+divClass).slideToggle(100,function(){
              var span=$(_this).find("span");
              if(span.attr("class").indexOf("glyphicon-chevron-up")>=0){
								span.removeClass("glyphicon-chevron-up").addClass("glyphicon-chevron-down");
              }else{
								span.removeClass("glyphicon-chevron-down").addClass("glyphicon-chevron-up");
              }
             });
           });
           $(".openOrCloseBtn").eq(1).trigger("click");
        });
        var  count=0;
        var counum=0;
        $(function(){
        	$(".list-details-header").find(".openOrCloseBtn1").click(function(){
            count =count+1;
            if(count%2!=0){
              $(".list-details-header").find("#sp1").html("展开");
            }else{
              $(".list-details-header").find("#sp1").html("收起");
            }
          });
          $(".list-details-header").find(".openOrCloseBtn2").click(function(){
            counum =counum+1;
            if(counum%2!=0){
            	$(".list-details-header").find("#sp2").html("关闭筛选条件");
            }else{
            	$(".list-details-header").find("#sp2").html("展开筛选条件");
            }
          });
          $("#sp1").click(function(){
          	$(".list-details-header").find(".openOrCloseBtn1").click();
          });
          $("#sp2").click(function(){
            $(".list-details-header").find(".openOrCloseBtn2").click();
          });
        });
		</script>
