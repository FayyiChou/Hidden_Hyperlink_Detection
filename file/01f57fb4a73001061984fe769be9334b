      <script>
        $(function(){
          function taggleActiveTab(li){
            var windowWidth=$(window).width();
            if(windowWidth > 800){
              $("[role='tablist']").find("li").show();
              return ;
            }
            var activeLi=li;
            if(!li){
               activeLi=$("[role='tablist']").find(".active");
            }
            activeLi.siblings("li").show();
            activeLi.hide();
          }
          
          $(window).resize(function(){
              taggleActiveTab();
          });
          taggleActiveTab();
          $("[role='presentation']").click(function(){
              taggleActiveTab($(this));
          });
  /*      //默认显示列表视图
          $(".liebiaolist ").addClass("active");
          $(".tuxinglist ").removeClass("active");
  				$("[role='tablist']").find("li").eq(1).find("a").trigger("click"); */
        });
                
</script> 
