<script>
	function turnpage(page){
		var list=DataList("list1");
      	var pageindex=parseInt(list.getParam("_ZVING_PAGEINDEX"));
		var total=parseInt(list.getParam("_ZVING_PAGETOTAL"));
		var size=parseInt(list.getParam("_ZVING_SIZE"));
		var pagecount=parseInt((total-1)/size+1);
		list.resumePagebar=function(){
			updatapagebar();
		}
		if(page=="prev"){
            if(pageindex<=0){
              return;
            }
			DataList('list1').previousPage();
			DataList('list2').previousPage();
		}else if(page=="next"){
            if(pageindex+1>=pagecount){
              return;
            }
			DataList('list1').nextPage();
			DataList('list2').nextPage();
		}else if(!isNaN(page)){
			DataList('list1').gotoPage(parseInt(page));
			DataList('list2').gotoPage(parseInt(page));
		}else{
			return;
		}
	}

	function updatapagebar(){
		var list=DataList("list1");
		var pageindex=parseInt(list.getParam("_ZVING_PAGEINDEX"));
		var total=parseInt(list.getParam("_ZVING_PAGETOTAL"));
		var size=parseInt(list.getParam("_ZVING_SIZE"));
		var pagecount=parseInt((total-1)/size+1);
		var from=pageindex>1?pageindex-1:1;
		var to=(pageindex+4<pagecount)?((pageindex+4)<6?6:(pageindex+4)):(pagecount);
		var html="<li><a href=\"javascript:turnpage('prev');\">«</a></li>";
		var temp="<li class=\"{classname}\"><a href=\"javascript:turnpage('{page-1}');\" >{page}</a></li>";
		for(var i=from;i<=to;i++){
			if(i==pageindex+1){
				html+=temp.replace("{classname}","active").replace("{page-1}",i-1).replace("{page}",i);
			}else{
				html+=temp.replace("{classname}","unactive").replace("{page-1}",i-1).replace("{page}",i);
			}
		}
		html+="<li><a href=\"javascript:turnpage('next');\">»</a></li>"
		$("ul.pagination").html(html);
	}
	function filterresource(id){
		var list1=DataList("list1");
		var list2=DataList("list2");
		list1.resumePagebar=function(){
			updatapagebar();
		}
		var mothod1=list1.getParam("_ZVING_METHOD");
		var mothod2=list1.getParam("_ZVING_METHOD");
		mothod1=mothod1.replace(/&articleid=\d*/,"&articleid="+id);
		mothod1=mothod1.replace(/&resname=.*/,"&resname=");
		mothod2=mothod2.replace(/&articleid=\d*/,"&articleid="+id);
		mothod2=mothod2.replace(/&resname=.*/,"&resname=");
		list1.setParam("_ZVING_METHOD",mothod1);
		list2.setParam("_ZVING_METHOD",mothod2);
		list1.setParam("_ZVING_PAGEINDEX",0);
		list2.setParam("_ZVING_PAGEINDEX",0);
		list1.loadData();
		list2.loadData();
	}
	//检索
	function subFind(){
		//获取表单中的值
		var dc = Form.getData("res_form", '', 'json');
		var resname = $V("res_name",dc);
		var list1=DataList("list1");
		var list2=DataList("list2");
		list1.resumePagebar=function(){
			updatapagebar();
		}
		var mothod1=list1.getParam("_ZVING_METHOD");
		var mothod2=list1.getParam("_ZVING_METHOD");
		mothod1=mothod1.replace(/&resname=.*/,"&resname="+resname);
		mothod1=mothod1.replace(/&articleid=\d*/,"&articleid=");
		mothod2=mothod2.replace(/&resname=.*/,"&resname="+resname);
		mothod2=mothod2.replace(/&articleid=\d*/,"&articleid=");
		list1.setParam("_ZVING_METHOD",mothod1);
		list1.setParam("resname",resname);
		list2.setParam("_ZVING_METHOD",mothod2);
		list1.setParam("_ZVING_PAGEINDEX",0);
		list2.setParam("_ZVING_PAGEINDEX",0);
		list1.loadData();
		list2.loadData();
	}
	$(function(){
		$("#chapter li.piece a").each(function(){
			var $el=$(this);
			$el.click(function(){
				$li=$el.parent("li");
				if($li.hasClass("seclected")){
					return;
				}
              	var id=$el.attr("pieceid")||"";
				filterresource(id);
				$("#chapter li.piece.seclected").removeClass("seclected");
				$li.addClass("seclected")
			});
		});
	});
 function addFavorite(bookid,siteid,type){
	    if(!ssoLogin){
	        $.alert("您尚未登录，不能收藏。");
	        return;
	    }
	    if(!bookid||!siteid||!type){
	       return;
	    }
			//$.Loading.show("正在收藏...");
	    $.ajax({
	        url : 'http://zengzhi.ipmph.com/zhbooks/MemberFavorites/add1?ID='+bookid+'&SiteID='+siteid+'&FavType='+type,
	        dataType : 'json',
	        success : function(req) {
	            if(req._ZVING_STATUS==1){
	                $.alert(req._ZVING_MESSAGE);
	            }else{
	                $.alert(req._ZVING_MESSAGE);
	            }
	        }
	    });
	}
 function cancleFavorite(bookid){
   $.ajax({
       method:"get",
     url:"http://zengzhi.ipmph.com/zhbooks/MemberFavorites/cancel?SiteId=35&ftype=Goods&fid="+bookid,
       dataType:"json",
       success:function(data){
      	 $.alert("取消成功!");
      	 location.reload();
       },
       error:function(){
           $.alert("取消失败");
         location.reload();
       }
   });
 }
  $("#reslist div[role='tabpanel']").click(function(event){
    var $el=$(event.target);
    if($el.is("img")){
    	$el=$el.parent();
    }
    if($el.is("a")&&$el.attr("isbn")){
      if (!ssoLogin) {
        $el.attr("target","_self");
       $.alert("您尚未登录，不能浏览资源。");
       return false;
     } 
      if(book&&(!book.activated)){
         $.alert("您尚未激活该图书，请先激活。");
        return false;
      }
    }
  });
  
