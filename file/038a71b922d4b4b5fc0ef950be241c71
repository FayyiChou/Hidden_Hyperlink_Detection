<script type="text/javascript">

    $('.dtBox .dtTab span').mouseover(function () {
       var reURL;
        var sid = $(this).attr("ref");
        if (sid == "0001,0032,0033") {
            reURL = "/kentedt.htm";
        }
        else if(sid == "0001,0032,0034")
        {
            reURL = "/xingy.htm";
        }
       else if(sid == "0001,0032,0035")
        {
            reURL = "/zuixijx.htm";
        }
        $(".newMore").attr("href", reURL);
        $(this).addClass("cur").siblings().removeClass("cur");
        //$("#curUrl2").attr("href", $(this).find("a").attr("href"));
        $(".dtBox .dtCont").eq($('.dtBox .dtTab span').index(this)).show().siblings(".dtCont").hide();
    });
 function reURL(url) {
        window.location.href = url;
    }
</script>

        
        

