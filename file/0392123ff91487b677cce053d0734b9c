<script type="text/javascript">
	
	var scrollWrapW,myScroll;
	var rollDirection = 0; //0为左 1为右
	var speed = 2000;
	function __scrollBox(){
	 if (rollDirection == 0){
		 scrollWrapW = -($('#scroll2 li').eq(0).outerWidth(true) + $('#scroll2 li').eq(1).outerWidth(true)) + 'px';
		 $('#scroll2').animate({left:scrollWrapW}, {
		   duration:200,
		   complete:function(){
			 $('#scroll2').append($('#scroll2 li:first')).append($('#scroll2 li:first')).append($('#scroll2 li:first'));
			 $('#scroll2').css("left","0");
			}
		  });
	 }else{
		var last = $('#scroll2 li').length - 1;
		var last2 = $('#scroll2 li').length - 2;
		scrollWrapW = -($('#scroll2 li').eq(last).outerWidth(true) + $('#scroll2 li').eq(last2).outerWidth(true)) + 'px';
		$('#scroll2').animate({left:0}, {
		   duration:200,
		   complete:function(){
			   $('#scroll2').prepend($('#scroll2 li:last')).prepend($('#scroll2 li:last')).prepend($('#scroll2 li:last'));
			   $('#scroll2').css("left",scrollWrapW);
			}
		  });
	 }
	}
	
	$(document).ready(function(){
	/* roll control */
	myScroll = setInterval(__scrollBox,speed);
	$('#scroll2').hover(function(){
	  clearInterval(myScroll);							 
	},function(){
	  myScroll = setInterval(__scrollBox,speed);	
	});
	/* left control */
	$('.left').click(function(){
	  rollDirection = 0;				  
	});
	$('.right').click(function(){
	  rollDirection = 1;				  
	});
	})
	
	$( document ).ready( function ()
			{
				$( '#qq' ).scrollFollow(
				{
				speed: 1000,
				offset: 200,
				killSwitch: 'exampleLink',
				onText: 'Disable Follow',
				offText: 'Enable Follow'
				}
				);
			}
		);
		
	function showsub( ids )
	{		
		for( var i = 1; i<=6; i++)
		{
			if (i==ids)
			{
				if( document.getElementById("sn_"+i).style.display=="none")
				{
					document.getElementById("sn_"+i).style.display = "";
				}
				else if (document.getElementById("sn_"+i).style.display == "")
				{
					document.getElementById("sn_"+i).style.display="none";
				}
			}
			else
			{
				document.getElementById("sn_"+i).style.display="none"
			}
		}
	}
	
</script>

