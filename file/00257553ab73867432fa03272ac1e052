<script type="text/javascript">
    $("#gb_footer").load("../common/html/gb_footer.html");

   
    var in_input = $('#search_bottom_input');
    var in_txt = in_input.val();
    $(in_input).focusin(function (){
        var in_input = $('#search_bottom_input');
        var in_txt = in_input.val();
        if(in_txt==="" || in_txt==="搜索其它作品试试"){
           in_input.val('');
           in_input.css('color','#000');
        }
    });
    $(in_input).focusout(function (){
        var in_input = $('#search_bottom_input');
        var in_txt = in_input.val();
        if(in_txt==="" || in_txt==="搜索其它作品试试"){
           in_input.val('搜索其它作品试试');
           in_input.css('color','#999999');
        }else{
            in_input.css('color','#000000');
        }
    });

    $('.in_btn').click(function (){
        var in_input = $('#search_bottom_input');
        var in_txt = in_input.val();
        if(in_txt==="" || in_txt==="搜索其它作品试试" ){
            in_input.css('color','red');
            return false;
        }
    });
</script>
