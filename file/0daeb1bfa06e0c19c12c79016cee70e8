<script type="text/javascript">
    $(".xinNav>ul>li").hover(function(){
        $(".xinTran").stop().animate({left:$(this).position().left+33,opacity:1},{duration:800,easing:'easeOutExpo'})
        $(".xinSubNav",this).stop().animate({height:210},{duration:1200,easing:'easeOutExpo'})
    },function(){
        $(".xinTran").stop().animate({left:33,opacity:0},{duration:800,easing:'easeOutExpo'})
        $(".xinSubNav",this).stop().animate({height:0},{duration:200,easing:'easeOutExpo'})
    })
</script>
