<script>
    $(function(){
        $("#content").height($(window).height());
         $("#content").css("overflow","hidden");
        $(".footerVideo").click(function(){
            $(".videoWrap").css("display","block");
			$(".videolightbox").css("display","block");
			$("#myvideo").html('<video src="flv/sjgzpt.mp4"  width="650px" height="500px" controls="controls"> Your browser does not support the video tag.</video>');
        })
        $(".videoWrap,.videoClose").click(function(){
            $(".videoWrap").css("display","none");
			$(".videolightbox").css("display","none");
			$("#myvideo").html('');
            //$(".vi").html("")
        })
    })
</script>
<script>
	var targetNo = 0;
	$("#left").click(function(){
	    var _h = $(window).width();
		if(0<targetNo&&targetNo<=4){
		targetNo=targetNo-1;
        if($("#right img").attr("src")=="themes/pingtang/images/righth.svg")
		{
		   $("#right img").attr("src","themes/pingtang/images/right.svg");
		}
	    $("body,html").stop().animate({scrollLeft:targetNo*_h},{duration:800,easing:'easeOutExpo'});
        $('#content').attr('data-fanye', targetNo);
		}
		if(targetNo==0){
	     $("#left img").attr("src","themes/pingtang/images/lefth.svg");
		}		
	
	})
	$("#right").click(function(){
	    var _h = $(window).width();
		if(0<=targetNo&&targetNo<4){
		targetNo=targetNo+1;
        if($("#left img").attr("src")=="themes/pingtang/images/lefth.svg")
		{
		   $("#left img").attr("src","themes/pingtang/images/left.svg");
		}
	    $("body,html").stop().animate({scrollLeft:targetNo*_h},{duration:800,easing:'easeOutExpo'});
        $('#content').attr('data-fanye', targetNo);
		}
		if(targetNo==4){
	     $("#right img").attr("src","themes/pingtang/images/righth.svg");
		}
		
	
	})
</script>
<script type="text/javascript">
    var _h = $(window).width();
	$("#content div").css("height",$(window).height());
	$("#content").css("width",5*$(window).width());
    flashNum = 0;
    $(window).resize(function(){
        _h = $(window).width();
        $(".contentBox").css({"width":_h})
    })
    $(".friendLinks").click(function(){
        $(this).toggleClass("linkCur");
        $(".friendsLInks").slideToggle();
    })
    $(".contentBox").css({"width":_h})
    $('.annList').myScroll({visible:1});
    $(".xinNav>ul>li").hover(function(){
        $(".xinTran").stop().animate({left:$(this).position().left+33,opacity:1},{duration:800,easing:'easeOutExpo'})
        $(".xinSubNav",this).stop().animate({height:210},{duration:1200,easing:'easeOutExpo'})
    },function(){
        $(".xinTran").stop().animate({left:33,opacity:0},{duration:800,easing:'easeOutExpo'})
        $(".xinSubNav",this).stop().animate({height:0},{duration:200,easing:'easeOutExpo'})
    })
    $("#rightNav").hover(function(){
        $(this).stop().animate({right:0},{duration:800,easing:'easeOutExpo'});
    },function(){
        $(this).stop().animate({right:-590},{duration:800,easing:'easeOutExpo'});
    })
    var up = false;
    var targetNo = 0;
    var explorer = window.navigator.userAgent ;
    var time;
    //time=setInterval("timeMove()",5000)
    function timeMove(){
        targetNo = targetNo++>3?0:targetNo;
        scrollFunc();
    }
    
    var glpds = 0;
    var scrollFunc=function(e){ 
        if(!$("body,html").is(":animated") && !$("#footer").is(":animated")){
            var content = parseInt($('#content').attr('data-fanye'));
            ee=e || window.event; 
            if(e.wheelDelta){//IE/Opera/Chrome 
                glpds = e.wheelDelta; 
            }else if(e.detail){//Firefox 
                glpds = e.detail; 
            } 
            if(glpds == 120){
                if(content > 0 && content <= 5){
                    content = content-1;
                }
            }else if(glpds == -120){
                if(content >= 0 && content < 5){
                    content = content+1; 
                }else if(content == 5){
                    content = 5;
                }
            }
           if(content == 1){
                //$("#footer").stop().animate({bottom:0},{duration:800,easing:'easeOutExpo'});
            }
        }else{
            e=e||window.event;
            if (e&&e.preventDefault){
                e.preventDefault();
                e.stopPropagation();
            }else{ 
                e.returnvalue=false;  
                return false;     
            }
        }
		
    } 
    /*注册事件*/ 
    if(document.addEventListener){ 
        document.addEventListener('DOMMouseScroll',scrollFunc,false); 
    }//W3C 
    document.onmousewheel=scrollFunc;
        
    $(".footerIcon").click(function(){
        if(up){
            $(".footerIcon").addClass("Cur");
            $("#footer").stop().animate({bottom:-275},{duration:800,easing:'easeOutExpo'})
        }else{
            $(".footerIcon").removeClass("Cur");
            $("#footer").stop().animate({bottom:0},{duration:800,easing:'easeOutExpo'})
        }
    })
    $(".audioUrl").click(function(){
        if(flashNum ==0){
            $(".flashContent").load("/swf1.html");
        }else if(flashNum ==1){
            $(".flashContent").load("/swf2.html");
        }else if(flashNum ==2){
            $(".flashContent").load("/swf3.html");
        }else if(flashNum ==3){
            $(".flashContent").load("/swf4.html");
        }else if(flashNum ==4){
            $(".flashContent").load("/swf5.html");
        }
    })
    
    function scrollFunc1(){
        if (explorer.indexOf("MSIE") >= 0 || explorer.indexOf("Firefox") >= 0) {
            document.documentElement.scrollTop = targetNo*_h;
            if(document.documentElement.scrollTop==0){
                $(".navBar li").eq(0).addClass("barCur").siblings().removeClass("barCur");
            }
        }else{
            $("body,html").animate({scrollTop:targetNo*_h},{duration:800,easing:'easeOutExpo'})
        }
    }
    $(".footerIcon").click(function(){
        if(up){
            $(this).addClass("Cur");
            $("#footer").stop().animate({bottom:-275},{duration:800,easing:'easeOutExpo'});
            $("#footerNews").fadeIn();
        }else{
            $(this).removeClass("Cur");
            $("#footer").stop().animate({bottom:0},{duration:800,easing:'easeOutExpo'});
              $("#footerNews").fadeOut();
        }
        up = !up;
    })
    $(".navBar li").click(function(){
        targetNo = $(this).index();
        $(this).addClass("barCur").siblings().removeClass("barCur");
		if(targetNo>4){}
        if(targetNo<5)
		var pdc = parseInt($(this).attr('data-pdc'));
		if(pdc==5 ||pdc ==1||pdc==2||pdc==3)
		{
        $("body,html").stop().animate({scrollTop:targetNo*_h},{duration:800,easing:'easeOutExpo'});
        $('#content').attr('data-fanye', pdc);
		}
    })
    $(".chatLinks li").hover(function(){
        $(".chatBg").stop().animate({left:85*$(this).index()},{duration:600,easing:'easeOutExpo'})
    })
    $(".dcbBtn,.dcbBtns").click(function(){
        $("#dcb").fadeIn();
    })
    $(".surveyClose").click(function(){
        $("#dcb").fadeOut();
    })
    var xxT = $(".footerNewsBoxList ul li").length;
    var xxN = 0;
    var xxTime = setInterval(function(){
        xxN = ++xxN>xxT-1?0:xxN;
        $(".footerNewsBoxList ul").stop().animate({left:-240*xxN},800);
        $(".footerNewsBoxList ol li").eq(xxN).addClass("xxCru").siblings().removeClass("xxCru");
    },5000)
    $(".footerNewsBoxList ol li").hover(function(){
        xxN = $(this).index();
        $(".footerNewsBoxList ul").stop().animate({left:-240*xxN},800)
        $(".footerNewsBoxList ol li").eq(xxN).addClass("xxCru").siblings().removeClass("xxCru");
    })
    $(".app").hover(function(){
        $(".google").show();
    },function(){
        $(".google").hide();
    });
    $(".p2").css("background-position","-50px 0px");
    
    $(function(){ 
        $("body,html").animate({scrollTop:0});
        setTimeout(function(){
            $("#rightNav").stop().animate({right:-590},{duration:800,easing:'easeOutExpo'})
        },1000);
    }); 
</script>
