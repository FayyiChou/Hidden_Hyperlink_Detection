              scrtime = setInterval(function () {
                  var $ul = $("#weibo_list ul");
                  var liHeight = $ul.find("li:last").height();
                  $ul.animate({ marginTop: liHeight + 40 + "px" }, 1000, function () {

                      $ul.find("li:last").prependTo($ul)
                      $ul.find("li:first").hide();
