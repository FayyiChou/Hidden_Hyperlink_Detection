 function showRdKfAd(){
   var isClose=$.cookie("rdAd1");
   if(isClose=="hide11"){
      $("#floatc").hide();
   }else{
      $("#floatc").show();
   }
}
 function showRdDoAd(){
   var issClose=$.cookie("rdAd2");
   if(issClose=="hide22"){
      $("#floatdown").hide();
   }else{
      $("#floatdown").show();
   }
}
