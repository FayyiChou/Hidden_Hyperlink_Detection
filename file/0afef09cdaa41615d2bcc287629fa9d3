<script type="text/javascript">
    $(function () {
        //点击弹出和隐藏第三方登录js
        var num=0;
        $('.otherLogin .click_sf').click(function(){
            if(num==0){
                $('.otherLogin .sf_container').css('display','block');
                        
                num++;
            }else{
                $('.otherLogin>div').css('display','none');
                num=0;
            }
                
        })
        var vOption = {
            onkeyup: false,
            rules: {
                LoginName:{required:true},
                Password: { required: true }
            },
            messages: {
                LoginName: {required: "请输入账号"},
                Password: { required: "请输入密码" }
            }
            , errorElement: "div"
        };
        window.formvalidator = $("#appform").validate(vOption);

        $("#loginbutton").click(function () {
            if (formvalidator.form()) {
                //alert("submit");
                document.getElementById('appform').submit();
            }
            else {
                formvalidator.focusInvalid();
                return false;
            }
        });

        $("#LoginName,#Password,#CheckCode").keyup(function (ev) {
            ev = ev || window.event;
            var code = (ev.keyCode || ev.which);
            if (code == 10 || code == 13) {
                $("#loginbutton").click();
            }
        });
    });
</script>
