                <script type="text/javascript">
                    (function () {
                        var inputArray = ["uid", "domain", "locale", "useSSL", "saveUsername"];

                        (function () {
                            for (var i = 0; i < inputArray.length; i++) {
                                var input = getElement(inputArray[i]);
                                if (input == null) {
                                    continue;
                                }

                                if ((input.type != "text") || (getValue(input) == "")) {
                                    var cookieValue = getCookie(input.name);
                                    if (cookieValue) {
                                        setValue(input, cookieValue);
                                    }
                                }

                            }

                        })();

                        function getElement(name) {
                            return document.getElementsByName(name)[0];
                        }

                        function getValue(input) {
                            if (input.type == "checkbox") {
                                return input.checked ? "1" : "0";
                            } else {
                                return input.value;
                            }
                        }

                        function setValue(input, value) {
                            if (input.type == "checkbox") {
                                input.checked = (value == "1");
                            } else {
                                input.value = value;
                            }
                        }

                    })();


                    var showLanguages = function(show) {
                        var e = jQ(".languages");
                        if (e.css("display") == 'none' && show) {
                            e.css("display", "block");
                        }
                        else {
                            e.css("display", "none");
                        }
                    };
                    jQ(".languageOptions").hover(function(){
                        var arrowSrc = jQ("#arrow img").attr("src");
                        jQ("#arrow img").attr("src", arrowSrc.replace(".png", "_hover.png"));
                    }, function(){
                        var arrowSrc = jQ("#arrow img").attr("src");
                        jQ("#arrow img").attr("src", arrowSrc.replace("_hover.png", ".png"));
                    }).click(showLanguages.bind({}, true)).blur(showLanguages.bind({}, false));

                    jQ.fn.allTimeChange = function(callback){
                        callback.call(this);
                        setTimeout(arguments.callee.bind(this, callback), 100);
                        return this;
                    };
                    var isPlaceholderSupported = "placeholder" in document.createElement("input");
                    jQ(".inptr .blank input").focus(function(){
                        if(this.id != "domain"){
                            this.parentNode.style.border="1px solid #64abdb";
                        }

                        if(!isPlaceholderSupported && (jQ.trim(this.value) == jQ.trim(jQ(this).attr("placeholder")) || !jQ.trim(this.value)) ) {
                            this.value = "";
                            this.style.color = "#000";
                            if(this.type=="password"){
                                var pwdPlaceholder = document.getElementById("pwdPlaceholder");
                                if(!pwdPlaceholder){
                                    var _this = jQ(this);
                                    pwdPlaceholder = document.createElement("div");
                                    pwdPlaceholder.id = "pwdPlaceholder";
                                    pwdPlaceholder.appendChild(document.createTextNode(_this.attr("placeholder")));
                                    pwdPlaceholder.style.top = "0";
                                    pwdPlaceholder.style.left = parseInt(_this.position().left) + "px"; //ie9????position().left??????????????????px
                                    pwdPlaceholder.style.color = "#999";
                                    pwdPlaceholder.style.backgroundColor = "#FFFFFF";
                                    pwdPlaceholder.style.position = "absolute";
                                    pwdPlaceholder.style.height = _this.height() + "px";
                                    pwdPlaceholder.style.lineHeight = _this.height() + "px";
                                    pwdPlaceholder.style.fontSize = _this.css("font-size");
                                    pwdPlaceholder.style.fontWeight = _this.css("font-weight");
                                    pwdPlaceholder.onclick = function(){
                                        _this.focus();
                                    }
                                    this.style.backgroundColor = "transparent";
                                    _this.before(pwdPlaceholder);
                                }
                                pwdPlaceholder.style.display = "none";
                            }
                        }
                        showDel.bind(this)();
                    }).blur(function(){
                        this.parentNode.style.border='1px solid #ffffff';
                        if(!isPlaceholderSupported && !jQ.trim(this.value)) {
                            if(this.type=="password"){
                                var pwdPlaceholder = document.getElementById("pwdPlaceholder");
                                if(pwdPlaceholder){
                                    pwdPlaceholder.style.display = "block";
                                }
                            }
                            else {
                                this.value = jQ.trim(jQ(this).attr("placeholder"));
                                this.style.color = "#999";
                            }
                        }
                    }).keydown(showDel).next().click(function(){
                        jQ(this).prev().focus().val('');
                        showDel.bind(jQ(this).prev()[0])();

                    });


                    jQ("#logArea .options #savePwd,#SSL").click(function(){
                        var e = jQ(this).children(".unChecked, .checked");
                        if(e.hasClass('checked')) {
                            e.removeClass("checked").addClass("unChecked");
                            if(e.hasClass('rememberPwd')) {
                                document.getElementById("saveUsername").checked = false;
                                setCookie("saveUsername", 0);
                            }
                            else {
                                document.getElementsByName("useSSL")[0].checked = false;
                                setCookie("useSSL", 0);
                            }
                        }
                        else {
                            e.removeClass("unChecked").addClass("checked");
                            if(e.hasClass('rememberPwd')) {
                                document.getElementById("saveUsername").checked = true;
                                setCookie("saveUsername", 1);
                            }
                            else {
                                document.getElementsByName("useSSL")[0].checked = true;
                                setCookie("useSSL", 1);
                            }
                        }
                    });

                    jQ(".button").click(function(){
                        jQ(this).parents('form').submit();
                    });

                    jQ(".affected").hover(function(){
                        this._to = setTimeout((function() {
                            var ele = jQ(this);
                            var img = ele.children("img");
                            ele.children(".word, .wordForIE").css("color", "#3598db");
                            ele.children(".download").css("display", "block");
                            if (document.documentMode > 8) {
                                ele.css("display", "block");
                                var download = ele.children(".download");
                                var downloadWidth = download.width();
                                download.css("left", (250 - downloadWidth) / 2);
                            }
                        }).bind(this), 100);
                    }, function(){
                        clearTimeout(this._to);
                        var ele = jQ(this);
                        var img = ele.children("img");
                        ele.children(".word, .wordForIE").css("color", "#698a9b");
                        ele.children(".download").css("display", "none");
                    });


                    jQ(".languages .items .item").mousedown(function(){
                        var lang = this.id;
                        jQ("#languageName").text(languages[lang]);
                        document.getElementById("locale").value = lang;
                        if(languages[lang] !== undefined){
                            changeLocale(lang);
                        }
                    });

                    jQ("#uid").change(function(){
                        if(document.getElementById('userLoginTab').className == "active"){
                            window.uid = this.value;
                        }
                        else {
                            window.adminid = this.value;
                        }
                    });

                    jQ(document.body).click(function () {
                        fadeOutElement(jQ(".domainPanel").get(0));
                    });

                    if (!jQ(".below_submit.options").children().length) {
                        jQ(".below_submit.options").remove();
                    }

                    if (SYS_CONST.twoFactorAuthMeta.dynamic2) {
                        jQ(".j-twofactor-verify-wrap").html(secondAuthPage);
                        twoFactorVerify.initVerify({
                            authData: SYS_CONST.twoFactorAuthMeta,
                            $authWrap: jQ('.j-twofactor-verify-wrap'),
                            $loginForm: jQ('#loginForm'),
                            $userLoginTab: jQ('#userLoginTab'),
                            $adminLoginTab: jQ('#adminLoginTab')
                        });
                    }
                </script>

            
        
    

</body></html>