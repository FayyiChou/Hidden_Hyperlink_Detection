                    if (!Function.prototype.bind) {
                        Function.prototype.bind = function(oThis) {
                            if (typeof this !== 'function') {
                                // closest thing possible to the ECMAScript 5
                                // internal IsCallable function
                                throw new TypeError('Function.prototype.bind - what is trying to be bound is not callable');
                            }

                            var aArgs   = Array.prototype.slice.call(arguments, 1),
                                fToBind = this,
                                fNOP    = function() {},
                                fBound  = function() {
                                    return fToBind.apply(this instanceof fNOP
                                                    ? this
                                                    : oThis,
                                            aArgs.concat(Array.prototype.slice.call(arguments)));
                                };

                            fNOP.prototype = this.prototype;
                            fBound.prototype = new fNOP();

                            return fBound;
                        };
                    }


                    function initInput(){
                        ipt_arr= ["uid", "domain", "password","verifyCode"];
                        var input;
                        for (var i= 0; i < ipt_arr.length; i++) {
                            input = document.getElementById(ipt_arr[i]) || document.getElementsByName(ipt_arr[i])[0];
                            if (input == null) {
                                continue;
                            }
                            if(!("placeholder" in document.createElement("input"))){ //??????????placeholder????????
                                input.focus();
                            }
                        }
                    }

                    var isLowerThanIE8andCompatible = function(){
                        var ua = navigator.userAgent;
                        var regular = "MSIE ";
                        var rIndex = ua.indexOf(regular);
                        var IEIndex = rIndex>-1?(rIndex+regular.length):0;
                        var IEVersion = IEIndex?parseFloat(navigator.userAgent.slice(IEIndex, IEIndex+3)):0;
                        return IEVersion >= 8.0?false:(ua.indexOf("compatible")>-1);
                    };

                    function switchLoginForm(isAdmin) {
                        this.className = '';
                        var userLoginTab = document.getElementById('userLoginTab');
                        var adminLoginTab = document.getElementById('adminLoginTab');
                        var form = jQ("#loginForm");
                        if (form.hasClass('inTwoFactorVerify')) {
                            return false;
                        }
                        if (isAdmin) {
                            userLoginTab.className = 'inactiveUser';
                            adminLoginTab.className = 'active';
                            document.forms[0].action = "http://c1.icoremail.mt:9900/webadmin/login.jsp";
                            document.getElementById('webmailIndexUrl').value = location.href;
                            if(!jQ.trim(jQ(".Error").text())){
                                jQ('.Error').text(decodeURIComponent(''));
                            }
                        } else {
                            userLoginTab.className = 'active';
                            adminLoginTab.className = 'inactiveAdmin';
                            document.forms[0].action = "/coremail/index.jsp";
                        }
                        jQ(".Error").text("");
                    }
                    var doResize = function(){
                        var HeadHeight = jQ(".Head").height();
                        var MainHeight = jQ("#main").height();
                        var footerHeight = jQ('.footer').height();
                        var wHeight = window.innerHeight;
                        var bHeight = document.body.clientHeight;
                        var sHeight = document.body.scrollHeight;
                        var mainHeight = Math.max(wHeight || 0, bHeight || 0, sHeight || 0);

                        if(HeadHeight + MainHeight + footerHeight + 10 < mainHeight){
                            jQ(".contact").css("margin-bottom", (50+mainHeight-HeadHeight-MainHeight-footerHeight)+"px");
                            if(isLowerThanIE8andCompatible()){
                                document.body.style.overflowY = "hidden";
                            }
                        }
                        else {
                            document.body.height = HeadHeight + MainHeight + footerHeight + 10;
                        }
                    };

                    var showDel = function(){
                        var delEle = jQ(this).next();
                        if(jQ.trim(jQ(this).val())){
                            if(isLowerThanIE8andCompatible()){
                                delEle.css({
                                    display: "block",
                                    top: "-34px"
                                })
                            }
                            else delEle.css("display", "block");
                        }
                        else {
                            delEle.css("display", "none");
                        }
                    };

                    window.onload = function(){
                        doResize();
                        if(ajc_locale == 'ja'){
                            jQ("#logArea .options").css("width","228px");
                        }
                        var cookieValue = getCookie("locale");
                        if (cookieValue) {
                            if (cookieValue.indexOf("en_") == -1
                                        && cookieValue.indexOf("zh_") == -1
                                    && cookieValue.indexOf("ja") == -1) {
                                changeLocale("zh_CN");
                            }
                        }

                        initInput();
                        showDel.call(document.getElementById('uid'));
                        showDel.call(document.getElementById('password'));

                        if(isLowerThanIE8andCompatible()){
                            jQ(".footer .versionPanel .content .version .word").removeClass("word").addClass("wordForIE");
                            jQ(".download").each(function(){
                                var top =  parseInt(jQ(this).css("top"));
                                jQ(this).css("top", top+24);
                            })
                        }
                        if(jQ.trim(document.getElementById("uid").value)){
                            jQ("#password").focus();
                        }
                        else {
                            jQ("#uid").focus();
                        }

                        document.getElementById("savePwd").children[0].className = "rememberPwd " + (document.getElementById("saveUsername").checked?"checked":"unChecked");
                        if(document.getElementById("SSL")) {
                            document.getElementById("SSL").children[1].className = "SSL " + (document.getElementsByName("useSSL")[0].checked?"checked":"unChecked");
                        }
                        for(var lang in languages){
                            if(lang == document.getElementById("locale").value){
                                jQ("#languageName").text(languages[lang]);
                                break;
                            }
                        }

                        (document.getElementById("vcImage") || {}).onclick = function() {
                            this.src = this.src.replace(/&rand=[\w\-\.]+/, '&rand=' + Math.random());
                        };
                        (document.getElementById("vcHint") || {}).href = "javascript:document.getElementById('vcImage').onclick();";

                        if(jQ("#logArea").height() < jQ("#loginForm").height()){

                            jQ(".MainFooter").addClass("hidden");

                        }

                        
                    };

                    window.onresize = doResize;

                    document.onkeydown = function(e){
                        e = e || window.event;
                        if(e.keyCode == 13){
                            jQ("#loginForm").submit();
                        }
                    }
                    function indexLocaleSelect(){
                        var html = "<div class=\"arrow\"></div><div class=\"items\">";
                        
                        
                        
                        
                        
                        html = html +
                               "<div class=\"item\" onmousedown=\"changeLocale('zh_CN', '简体中文'); return false;\">简体中文</div>";
                        
                        
                        
                        html = html +
                               "<div class=\"item\" onmousedown=\"changeLocale('zh_TW', '繁體中文'); return false;\">繁體中文</div>";
                        
                        
                        
                        html = html +
                               "<div class=\"item\" onmousedown=\"changeLocale('en_US', 'English'); return false;\">English</div>";
                        
                        
                        
                        html = html +
                               "<div class=\"item\" onmousedown=\"changeLocale('ja', '日本語'); return false;\">日本語</div>";
                        

                        html = html + "</div>";
                        return html;
                    }
