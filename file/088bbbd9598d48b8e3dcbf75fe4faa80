<script>
!(function(){
    //火焰插件
    var i =0;
    function change(){
        if(i<5){
            i++;
            if(i==4){i=0}
            $(".hot-content").children().attr({"class":"hot"+i});
        }
        setTimeout( function(){ change(); }, 150 );
    }
    change();

})();


