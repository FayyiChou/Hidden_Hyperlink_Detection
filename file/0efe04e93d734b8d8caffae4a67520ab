    <script src="http://assets.dwstatic.com/mobile/src/js/main/seajs/sea-lego.js"></script>
    <script>
        seajs.config({
            base: "http://assets.dwstatic.com/project/mobi-topic/1.0/js/",
            alias: {
              'header': 'module/header.js',
            }
        });
        seajs.use(['header','zepto'],function(header,$){
            var listData;
            $.ajaxJSONP({
              url: 'http://zx.duowan.com/s/dataJSON/newArtList.json?callback=jsonp1',
                dataType: 'jsonp',
              	success: function(result){
                    listData = result.data;
                },
                complete:function(){
                	$('#addMore').click(function(){
                        var itemNum = $('[name="tagLink"]').length;
                        var addPart = '',
                            addNum = 0;
                        if(itemNum + 15 >= listData.length){
                            addNum = listData.length;
                            $('#addMore').hide();
                        }else{
                            addNum = itemNum + 15;
                        }
                        for(var i = itemNum ; i < addNum ; i++){
                            addPart = addPart + '<a href="'+ listData[i].link +'" name="tagLink"><li>'+ listData[i].title  +'</li></a>';
                        }
                        $('.ch-list-sec').append(addPart);
                    });
                    $('#addMore').click(); 
                }
            })
            
        }); 
    </script>
