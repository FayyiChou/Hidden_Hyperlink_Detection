    <script>
    function topostpage(obj){
        var postmsg = $('#needmessage').val();
        postmsg = postmsg.replace('<','');
        postmsg = postmsg.replace('>','');
        postmsg = postmsg.replace("'",'');
    
        var url = obj.href+"&postmsg="+encodeURIComponent(postmsg);
        location=url;
        return false;
    }
    </script> 
