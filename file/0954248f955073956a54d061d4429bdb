  <script type="text/javascript">
   var currentpid = 0;
$('.favbtn').on('click', function() {
var obj = $(this);
$.ajax({
type:'POST',
url:obj.attr('href') + '&handlekey=favbtn&inajax=1',
data:{'favoritesubmit':'true', 'formhash':'9bdbe3a6'},
dataType:'xml',
})
.success(function(s) {
popup.open(s.lastChild.firstChild.nodeValue);
evalscript(s.lastChild.firstChild.nodeValue);
})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
});
return false;
});


     $(function(){
        $('.authi').click(function(){
           $('.popup2').fadeOut();
        })
        $('.firstmsg').click(function(){
           $('.popup2').fadeOut();
        })

        $(".postmsg").click(function(){



            var pop = $('.popup2');
pop.fadeIn();
pop.css('top',$(this).parent().offset().top);
var pid = $(this).attr('pid');

            var support = $(this).attr('support')?$(this).attr('support'):0;
            var against = $(this).attr('against')?$(this).attr('against'):0;
            pop.attr('pid',pid);
            currentpid = pid;
// 支持
            var digged = buryed =  0;
            if($(this).hasClass('digged')){
      digged =1;
}
            if($(this).hasClass('buryed')){
      buryed =1;
}

pop.find("[action='digg']").attr('href','forum.php?mod=misc&action=postreview&do=support&tid=874234&pid='+pid+'&hash=9bdbe3a6').html((digged?'已':'')+'支持['+support+']');
//反对
pop.find("[action='bury']").attr('href','forum.php?mod=misc&action=postreview&do=against&tid=874234&pid='+pid+'&hash=9bdbe3a6').html((buryed?'已':'')+'反对['+against+']');
//回复
pop.find("[action='recomment']").attr('href','forum.php?mod=post&action=reply&fid=371&tid=874234&repquote='+pid+'&extra=&page=1');


});
        $('.control >a').click(function(){
            if($('.control').hasClass('digged') || $('.control').hasClass('buryed')){
                alert('您已参与过');
                return false;
            }

            var cname =$(this).attr('_action')+'ed';
            var currlink = $(this);

            $.ajax({
    			type:'POST',
    			url:currlink.attr('href'),
    			data:{'inajax':'1'},
    			dataType:'xml',
    		})

    		.success(function(s) {
                $('.control').addClass(cname);
                currlink.addClass(cname);
                currlink.html(parseInt(currlink.html())+1);
                return false;
            })
            .error(function() {
    			alert('请检查网络');

    		});
            return false;
        });
        $('.ajaxbtn').click(function(){

            var url = $(this).attr('href');
            if($('#pm_'+currentpid).hasClass('digged') || $('#pm_'+currentpid).hasClass('buryed')){
                alert('您已参与过');
                $('.popup2').hide();
                return false;
            }

            var cname =$(this).attr('action')+'ed';

           	$.ajax({
    			type:'POST',
    			url:url,
    			data:{'inajax':'1'},
    			dataType:'xml',
    		})
    		.success(function(s) {
                var pid = $('.popup2').attr('pid');

                $('#pm_'+pid).addClass(cname);
                if(cname == 'digged'){
                    var support = $('#pm_'+pid).attr('support');
                    support = support?parseInt(support):0;

                    $('#pm_'+pid).attr('support',support+1);
                    $('#em_'+pid).html(parseInt($('#em_'+pid).html())+1) ;
                    $('#em_'+pid).addClass(cname);
                }else{
                    var against = $('#pm_'+pid).attr('against');
                    against = against?parseInt(against):0;

                     $('#pm_'+pid).attr('against',against+1);
                }
                $('.popup2').hide();

    		})
    		.error(function() {
    			alert('请检查网络');
    		});
            return false;
            });
    });

</script> 
