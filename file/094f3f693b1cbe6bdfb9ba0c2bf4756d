<script>
$(document).ready(function(){
	$('#overlay').click(function(){
		$('#equinix_popup').css('display','none');
		$(this).hide();
	});
});
</script>
<script type="text/javascript">
function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
    return "";
}

function checkCookie(cookieName) {
	cookie = getCookie(cookieName);
	if (cookie != "true") {
			$('#equinix_popup').css('display','block');
			$('#overlay').css('display','block');
	}
}

$(document).ready(function(e) {
   $('#equinix_popup').appendTo('body');
   checkCookie('equinix_popup');

   $('.equinix_close').click(function(){
       document.cookie="equinix_popup=true";
       $('#equinix_popup').css('display','none');
       $('#overlay').css('display','none');
   });

$('.equinix_closeText').click(function(e){
   e.preventDefault();
       document.cookie="equinix_popup=true";
       $('#equinix_popup').css('display','none');
       $('#overlay').css('display','none');
});

});

</script>


