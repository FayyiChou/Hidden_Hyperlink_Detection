Captcha" id="captcha-3811-0-10"><div class="FormContentLeft"><label for="SKFCE_10_UserInput"><span class="formCaptcha">Please enter code </span></label></div><div class="FormContentRight"><input type="hidden" name="SKFCE_10_ElementID" id="SKFCE_10_ElementID" value="10" /><img src="/RandomImage.aspx?id=55591" alt="Captcha Test Image" /><br/> <input type="hidden" name="SKFCE_10_CaptchaID" id="SKFCE_10_CaptchaID" value="55591" /><input type="text" name="SKFCE_10_UserInput" id="SKFCE_10_UserInput" value="" data-name="CAPTCHA" /></div></div><input type="hidden" name="AttachIDString" id="AttachIDString" value="nothing" /><input type="hidden" name="AttachFilenames" id="AttachFilenames" value="" /></div><div class="FormContentBoth"><input type="submit" name="PostEnquiry" id="PostEnquiry" value="SEND" class="SubmitButtonElement" /></div></div></form>
			<script>

				var errorMessages = [	
										'Your input for textbox Title must not be blank.',
										'Your input for textbox "First Name" must not be blank.',
										'Your input for textbox "Last Name" must not be blank.',
										'Your input for textbox "Company" must not be blank.',
										'Your input for textbox "Email" must not be blank.',
										'Your input for textbox "Tel No" must not be blank.',
										'Your input for textarea "Enquiry" must not be blank.',
										'Enter the captcha code.'
									];
				
				// URI Parse function. Split value pairs by "&".
				function querySt(ji) {
					hu = window.location.search.substring(1);
					gy = hu.split("&");
					for (i=0;i<gy.length;i++) {
						ft = gy[i].split("=");
						if (ft[0] == ji) {
							return ft[1];
						}
					}
				}
				
				// Set variables based on specific URL value names that we're looking for (ignoring the rest).
				var param1 = querySt("utm_source");
				var param2 = querySt("utm_medium");
				var param3 = querySt("utm_campaign");
			
				// Check if the URL parameters exist. All params are required.
				if(param1 === undefined || param2 === undefined || param3 === undefined) {
					var paramsDefined = false;
				}
				// If they do exist, set the switch to true.
				else { 
					var paramsDefined = true;
				}
				
				
				$(document).ready(function(){
					if($('.formValidationError').length > 0){
						$('ul.nav li a').eq($('ul.nav li').length - 1).trigger('click');
					}
					if(paramsDefined === true) {
						var paramCookie1 = $.cookie("param1", param1, {path: '/'});
						var paramCookie2 = $.cookie("param2", param2, {path: '/'});
						var paramCookie3 = $.cookie("param3", param3, {path: '/'});
					}
					
					// If the cookies already exist, then let's try and populate the fields in the form.
					// Cookie values are Decoded from their ASCII characters.
					// All cookies must exist.
					if($.cookie("param1") !== null && $.cookie("param2") !== null && $.cookie("param3") !== null) {
						
						$("input[value=param1]").val($.URLDecode($.cookie("param1")));
						$("input[value=param2]").val($.URLDecode($.cookie("param2")));
						$("input[value=param3]").val($.URLDecode($.cookie("param3")));
				
					}
					
					else {
					
						$("input[value=param1]").val("");
						$("input[value=param2]").val("");
						$("input[value=param3]").val("");
						
					}
				
					if($('.subTnx').length > 0) {
						$('.reqInfo, .pleaseComplete').remove();
						$('.frame').eq(4).css('min-height', 0);
						$('ul.nav li a').eq(positions.length-1).trigger('click');
					}

					$('.req').each(function(i){
						$(this).parent().parent().find('.FormContentLeft').prepend('<div class="errorText">'+ errorMessages[i] +'</div>');
					});
					
					$('.send').click(function(e){
						$('.req').each(function(i){
							if($(this).val() == ""){
								showError($(this));
							}else{
								hideError($(this));
							}
							/* for email*/
							if(i ==  4 && $(this).val() != ""){
								if(!validateEmail($(this).val())){
									showError($(this));
								}else{
									hideError($(this));
								}
							}
						});
						
						if($('.req').hasClass('error')){
							e.preventDefault();
						}else{
							var isSafari = (navigator.userAgent.toLowerCase().indexOf('safari') !== -1 && navigator.userAgent.toLowerCase().indexOf('chrome') === -1) && typeof window.ontouchstart === 'undefined';
							if(!isSafari){
								$('#Sitekit_Form_3811').submit();
							}
						}
					});
				});
				
				function validateEmail(email) { 
					var re = /^([a-zA-Z0-9])(([a-zA-Z0-9])*([\._\+-])*([a-zA-Z0-9]))*@(([a-zA-Z0-9\-])+(\.))+([a-zA-Z]{2,4})+$/;
					return re.test(email);
				} 
				
				function showError(element) { 
					element.addClass('error');
					element.parent().parent().find('.errorText').show();
				} 
				
				function hideError(element) { 
					element.removeClass('error');
					element.parent().parent().find('.errorText').hide();
				} 


			</script>
		  </div>
        </div>
