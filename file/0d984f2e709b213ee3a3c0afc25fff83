    function goTop() {

        $(".float").stop().animate({
            "top": ($(window).height() + $(window).scrollTop() - ($(window).height() + $(".float").height()) / 2) < 790 ? 790 :
     $(window).height() + $(window).scrollTop() - ($(window).height() + $(".float").height()) / 2
        }, 600);
    }
</script>
