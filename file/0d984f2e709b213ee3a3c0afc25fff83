   function checkForm()
{
	if($('#uname').val()=='姓名')
	{
		alert('提示：请输入姓名！');
        $('#uname').focus();
		return false;	
	}
	
	if($('#utele').val()=='联系电话')
	{
		alert('提示：请输入您的联系电话！');
		$('#utele').focus();
		return false;	
	}
	
