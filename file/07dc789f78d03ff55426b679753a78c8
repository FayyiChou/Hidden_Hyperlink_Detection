<script type="text/javascript">
$(document).ready(function(){
	$("#selCity").hide();
	$("#selCounty").hide();

	$("#change-city").click(function(){
		$("#cityinfobar").parent().css('height', '64px');
		$("#cityinfobar").hide();
		$(".weatheropt").hide();
		$("#select-city").show();

		return false;
    });

	$("#cancel-change").click(function(){
		return cancelChange();
    });

	$("#do-change").click(function(){
		var host = $("#selCity option:selected").attr("py");
		var cityPy = $("#selCounty option:selected").attr("py");
		if (cityPy == "") {
			cityPy = host;
			var ishot = $("#selCity option:selected").attr("ishot");
		} else {
			var ishot = $("#selCounty option:selected").attr("ishot");
		}

		if (host == null || host == '') {
			return cancelChange();
		}

		if (ishot==0) {
			var gourl='http://' + host + '.tianqi.com/'+cityPy+'/';
		} else {
			var gourl='http://'+cityPy+'.tianqi.com';
		}
		SetCookie('cityPy', cityPy);
		window.location.href=gourl;

		return false;
    });
});

function cancelChange()
{
	$("#cityinfobar").show();
	$(".weatheropt").show();
	$("#select-city").hide();
	$("#cityinfobar").parent().css('height', '32px');

	return false;
}

function changeProvince($obj)
{
	if ($obj.val() == '') {
		$("#selCity").hide();
		$("#selCounty").hide();
		return false;
	}

	loadJS("/weather.php?a=getZoneInfo&type=1&pid="+$obj.val()+"&callback=getDzcityFun");
}

function changeCity($obj)
{
	if ($obj.val() == '') {
		$("#selCounty").hide();
		return false;
	}

	loadJS("/weather.php?a=getZoneInfo&type=2&cid="+$obj.val()+"&pid="+$('#selProvince').val()+"&callback=getDzzoneFun");
}

function getDzcityFun(json)
{
	var str='';
	str += "<option ishot='0' py='' value=''>请选择市</option>";
	for(var key in json[0].py)
	{
		str += "<option ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
	}
	$("#selCity").html(str);
	$("#selCity").show();
	$("#selCounty").hide();
}

function getDzzoneFun(json)
{
	str = '';
	//str += "<option ishot='0' py='' value=''>请选择县</option>";
	for(var key in json[0].py)
	{
		str += "<option ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
	}
	$("#selCounty").html(str);
	$("#selCounty").show();
}
</script>		</div>
