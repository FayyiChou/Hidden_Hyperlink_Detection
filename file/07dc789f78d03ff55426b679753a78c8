<script language="javascript">
function set(id)
{
	if (id == '1') {
		$("#zone").selectbox();
		return;
	} else if(id == '2') {
		return;
	} else if(id == '3') {
		SetCookie('cityPy','');
		return;
	}

	var host = $("#city option:selected").attr("py");
	var cityPy = $("#zone option:selected").attr("py");
	if (cityPy == "") {
		cityPy = host;
		var ishot = $("#city option:selected").attr("ishot");
	} else {
		var ishot = $("#zone option:selected").attr("ishot");
	}

	if (id == '4') {
		if (cityPy == "") {
			alert('请选择县后再设置');
		} else {
			SetCookie('cityPy', cityPy);
			alert("设置成功！");
		}
		return;
	}

	if (cityPy != '') {
		if (ishot==0) {
			var gourl='http://' + host + '.tianqi.com/'+cityPy+'/';
		} else {
			var gourl='http://'+cityPy+'.tianqi.com';
		}
		//$(id).attr("href",gourl);
		window.location.href=gourl;
	}
}
function getcity(p)
{
	loadJS("http://www.tianqi.com/weather.php?a=getZoneInfo&type=1&pid="+p+"&callback=getcityFun");
}
function getzone(p)
{	
	if($id('province').value==''){
		str = '';
		str += "<option ishot='0' py='' value=''>请选择县</option>";
		$("#zone").html(str);
		$("#zone").selectbox();
	}else{
		loadJS("http://www.tianqi.com/weather.php?a=getZoneInfo&type=2&cid="+p.value+"&pid="+$id('province').value+"&callback=getzoneFun");
	}
}
function getcityFun(json)
{
	var str='';
	str += "<option ishot='0' py='' value=''>请选择市</option>";
	for(var key in json[0].py)
	{
		//if(json[0].defval==key) str += "<option selected ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
		//else str += "<option ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
		str += "<option ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
	}
	$("#city").html(str);
	str = '';
	str += "<option ishot='0' py='' value=''>请选择县</option>";
	/*for(var key in json[1].py)
	{
		if(json[1].defval==key) str += "<option selected ishot='"+json[1].ishot[key]+"' py='"+json[1].py[key]+"' value='"+key+"'>"+json[1].value[key]+"</option>";
		else str += "<option ishot='"+json[1].ishot[key]+"' py='"+json[1].py[key]+"' value='"+key+"'>"+json[1].value[key]+"</option>";
	}*/
	$("#zone").html(str);
	$("#city").selectbox();
	$("#zone").selectbox();
}
function getzoneFun(json)
{
	str = '';
	str += "<option ishot='0' py='' value=''>请选择县</option>";
	for(var key in json[0].py)
	{
		//if(json[0].defval==key) str += "<option selected ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
		//else str += "<option ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
		str += "<option ishot='"+json[0].ishot[key]+"' py='"+json[0].py[key]+"' value='"+key+"'>"+json[0].value[key]+"</option>";
	}
	$("#zone").html(str);
	$("#zone").selectbox();
	set('go');
}
function setgzcity(c){
    var citygz = getCookie('citygz');
    if (citygz != '') {
        c = c + ',' + citygz;
    }
    SetCookie('citygz', c);
}
</script>
