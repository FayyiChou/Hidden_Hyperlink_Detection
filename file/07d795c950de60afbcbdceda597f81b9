    function ding() {
        var aid = jQuery("#aid").val();
        jQuery.getJSON("/tlbb0713.ashx?r=" + Math.random(), { "action": "ding", "id": aid }, function (datas) {
            if (datas[0].state) {
                alert("您已经评价过这篇文章了");
            } else {
                jQuery(".ding_a b").html(datas[0].num);
            }
        });
    }
    function cai() {        
