			<script>
        function autosubmit(){//事件触发函数   
        	if(event.keyCode==13){   
        		to_search();
        	}
        }
        	function to_search(){
				var tit=$("#searchT").val();
				if(tit.length==0||tit.length<1){
					alert("检索关键字不能留空！");
					return;
					}
				document.location.href="/front/second_search.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2t2t1e="+encodeURI(tit);
            }
        	function changeImg(o,n){
        		$("#"+o+"").css("display","none");
        		$("#"+n+"").css("display","block");
        	}
        	function backImage(o,obj){
        	}
        </script>
