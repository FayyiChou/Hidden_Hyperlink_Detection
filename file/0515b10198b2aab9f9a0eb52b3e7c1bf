  <script>
              $(document).ready(function(){
              $.ajax({
		            type:"POST",url:"/viewVote",
		            dataType:"html",
		            data:"",
		            beforeSend:function(XMLHttpRequest) {
		            	$("#showVoteMsg").html("<img src=/news/css/ajax-loader.gif>正在加载投票信息..");
		                },
		           	 success:function(msgv) {
				       if(msgv==""){$("#showVoteMsg").html("没有发起投票信息!");}else{
				    	   $("#playVote").html(msgv);
						    }
		                },
		           	complete:function(XMLHttpRequest,textStatus) {},
