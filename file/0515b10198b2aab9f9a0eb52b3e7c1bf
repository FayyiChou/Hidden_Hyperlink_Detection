<script language="JavaScript" type="text/JavaScript">
function go_to(id,name){
	if(id=="4028812a28aa85100128aa8606260001"){
		window.open("/front/"+name+".html");
		return;
	}
	window.open("/front/"+name+"/"+id+".html");
}
function go_second(id,name,idTip){
	if(id=="4028812a28aa85100128aa87aabe0009"){
		window.open("/front/institutions.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2="+id+"&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc="+idTip+"");
	}else if(id=="4028812a28aa85100128aa880f06000c"){
		window.open("/front/rdzt.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2="+id+"&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc="+idTip+"");
	}else if(id=="4028812a28aa85100128aa89cee60018"||id=="4028812a28b32b680128b36b2311000d"||id=="402881a62950b0240129514b64780002"){
		window.open("/front/second_img.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2="+id+"&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc="+idTip+"");
	}else if(id=="4028812a28b32b680128b4691e4b0015"){
		window.open("/front/vote_second.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2="+id+"&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc="+idTip+"");
	}else if(id=="4028812a28b32b680128b468f3c00014"){
		//县长信箱
		window.open("/front/leavemsg/gzhd_xian_List.jsp?uId=47&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2=4028812a28b32b680128b4696a670017&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc=4028812a28aa85100128aa86c62a0006");
	}else if(id=="4028812a28b32b680128b4696a670017"){
		//互动大厅
		window.open("/front/leavemsg/gzhd_List.jsp?uId=&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2=4028812a28b32b680128b4696a670017&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc=4028812a28aa85100128aa86c62a0006");
	}else if(id=="4028812a28b5174d0128b66051620001"||id=="4028812a28b5174d0128b6607e6e0002"){
		//网上办事
		window.open("/front/second_bs.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2="+id+"&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc="+idTip+"");
	}else if(id=="4028812a28aa85100128aa8760470007"){
		window.open("/front/third/999145_"+idTip+".html");
	}else if(id=="4028812a28aa85100128aa89994f0016"){
		window.open("http://dbxzjc.maoming.gov.cn/banshigongs/");
	}
	else if(id=="A028812a28aa85100128aa8945340013"){
		window.open("/zwgk/index.jsp");
	}
	//http://localhost/dborg/front/second.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2=4028812a28aa85100128aa8760470007&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc=4028812a28aa85100128aa8606260001
	else{
		window.open("/front/second.jsp?265e4bd629300c55e158eff8aa47649bd6224e4266b86dc2="+id+"&265e4bd629300c55e158eff8aa47649bd6224e4266b86dc="+idTip+"");
	}
}


</script>
<script>
function addCookie(){　 // 加入收藏夹
if (document.all){
window.external.addFavorite('http://dianbai.gov.cn/', '电白县人民政府--政务公开');
}else if (window.sidebar){
window.sidebar.addPanel('电白县人民政府--政务公开', 'http://dianbai.gov.cn/', "");
}
}
function setHomepage(){　 // 设置首页
if (document.all){
document.body.style.behavior = 'url(#default#homepage)';
document.body.setHomePage('http://dianbai.gov.cn/');
}else if (window.sidebar){
if (window.netscape){
try {
netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
}catch (e) {
alert("该操作被浏览器拒绝，如果想启用该功能，请在地址栏内输入 about:config ,然后将项为signed.applets.codebase_principal_support的值改为true");
}
}
var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
prefs.setCharPref('browser.startup.homepage', 'http://dianbai.gov.cn/');
}
}
</script>
