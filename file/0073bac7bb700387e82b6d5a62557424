			document.body.className = fontSize;
			if (fontButton = document.querySelector('.font')) {
				fontButton.innerHTML = fontName;
				fontButton.className = fontSize + ' font';
				fontButton.addEventListener('tap', function () {
					fontSize = 'f2';
					switch(this.innerHTML) {
						case '\u5C0F': fontName = '\u4E2D'; fontSize = 'f2'; break;
						case '\u4E2D': fontName = '\u5927'; fontSize = 'f3'; break;
						case '\u5927': fontName = '\u5C0F'; fontSize = 'f1'; break;
					}
					fontButton.innerHTML = fontName;
					try { localStorage.setItem('font-size', fontSize); } catch (error) {}
					fontButton.className = (document.body.className = fontSize) + ' font';
				});
			}
