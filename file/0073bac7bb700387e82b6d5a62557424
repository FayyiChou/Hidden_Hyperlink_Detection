	function tgStatis(url,event,describe,gmine_ad){
		var ip_ck = document.cookie.match(/ip_ck=([^;$]+)/);
  		 ip_ck = ip_ck && ip_ck[1] ? ip_ck[1] : '######IP_CK#####';
		if(typeof(event) == 'undefined'){ 
		event = 'app_wenzhang_tuiguangb_tu'; 
		}else{ 
		url = url.replace('####time####',Date.parse(new Date())); 
		} 
		var img = document.createElement('img');
		img.style.cssText = 'position:absolute;z-index:-1;pointer-events:none;opacity:0;';
		tgUrl = 'http://pvtest.zol.com.cn/images/pvevents.gif?t='+Date.parse(new Date)+'&event='+event+'&plan_des='+describe+'&ip_ck='+ip_ck+'&url='+url;
		tgUrl += gmine_ad ? '&adcnt='+gmine_ad : '';
		img.setAttribute("src",tgUrl); 
		document.body.appendChild(img);
		//window.location.href=url; 
	}
	
	
	scrollTgStatis(document.querySelector('#relate_pic'),'wap_article_page_nice_pic_rec_show','zol-0-0-0');
	scrollTgStatis(document.querySelector('#wonder-read'),'wap_article_page_nice_article_rec_show','zol-0-0-0');
	
	function scrollTgStatis(suggestList,event,describe){
		if(!suggestList)return;
		window.addEventListener('scroll',function(){
			var range = window.innerHeight - 100;
			var position = suggestList.getBoundingClientRect().top;
			if(position < window.innerHeight && position > range){
				if(suggestList.hadPost) return;
				tgStatis && tgStatis('/article/3714430.html',event,describe);
				suggestList.hadPost = 1;
			}else{
				suggestList.hadPost = 0;
			}
		})
	}
	
	document.querySelectorAll('.gmine_ad a').forEach(function(a){
        if(!a) return;
        a.addEventListener('tap',function(){
            var _div = this.parentNode;
    		var gmine_id       = _div.getAttribute("gmine");
    		var gmine_type     = _div.getAttribute("data-type");
    		var gmine_position = _div.parentNode.getAttribute("data-id");
    		var gmine_ad_str   = '0'+gmine_type+'_'+gmine_position+'_'+gmine_id;
    		tgStatis('/article/3714430.html','wap','',gmine_ad_str);
    	})
    });
