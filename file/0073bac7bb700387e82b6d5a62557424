<script>
    function tgStat(url,event){
        if(typeof(event) == 'undefined'){
            event = 'app_wenzhang_tuiguangb_tu';
        }else{
            url = url.replace('####time####',Date.parse(new Date()));
        }
        var img  = document.getElementById("tgPv");
        tgUrl      = 'http://pvtest.zol.com.cn/images/pvevents.gif?t='+Date.parse(new Date)+'&event='+event+'&ip_ck=f3be47446ed33ff769fb0ec1442078df====&url='+url;
        img.setAttribute("src",tgUrl);
        window.location.href=url;
    }
</script>

<script>
    (function(){
        // platform 值 1android 2iphone 3 ipad 0全部
        var userAgent = navigator.userAgent, replaceA = document.querySelectorAll('.replace-url'), promotion = document.querySelectorAll('.promotion'), appInfo = document.querySelectorAll('#appInfo a');
        if(/iphone/i.test(userAgent)){
            if(appInfo){
                for(var i=0; i<appInfo.length;i++){
                    appInfo[i].href = appInfo[i].href.replace(/android/i,'touch/iphone');
                }
            }
            replaceUrl(replaceA,2);
            displayPromotion(promotion,2);
        }else if(/android/i.test(userAgent)){
            replaceUrl(replaceA,1);
            displayPromotion(promotion,1);
        }else if(/ipad/i.test(userAgent)){
            replaceUrl(replaceA,4);
            if(appInfo){
                for(var i=0; i<appInfo.length;i++){
                    appInfo[i].href = appInfo[i].href.replace(/android/i,'touch/iphone');
                }
            }
        }
        function replaceUrl(replaceUrl,os){
            var platform;
            if(!replaceUrl || replaceUrl.length <= 0) return ;
            for(var i=0; i < replaceUrl.length; i++){
                platform = replaceUrl[i] && replaceUrl[i].getAttribute('data');
                if(platform == 0 || (platform & os) == os){
                    replaceUrl[i].href = replaceUrl[i].getAttribute('data-href');
                    replaceUrl[i].style.color = '#1062BD';
                }
            }	
        }
        // 推广链接
        function displayPromotion(promotion,os){
            var platform;
            if(!promotion || promotion.length <= 0) return ;
            for(var i=0; i < promotion.length; i++){
                platform = promotion[i] && promotion[i].getAttribute('data-os');
                if(platform == 0 || (platform & os) == os){
                    promotion[i].style.display = '';
                }
            }
        }
    })();
</script>

