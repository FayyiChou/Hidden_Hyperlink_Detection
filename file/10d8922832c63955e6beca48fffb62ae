  var speed91=25//速度数值越大速度越慢
  document.getElementById("demo662").innerHTML=document.getElementById("demo661").innerHTML
  function Marquee91(){
  if(document.getElementById("demo662").offsetWidth-document.getElementById("demo66").scrollLeft<=0)
  document.getElementById("demo66").scrollLeft-=document.getElementById("demo661").offsetWidth
  else{
  document.getElementById("demo66").scrollLeft++
  }
  }
  var MyMar91=setInterval(Marquee91,speed91)
  document.getElementById("demo66").onmouseover=function () {clearInterval(MyMar91)}
  document.getElementById("demo66").onmouseout=function () {MyMar91=setInterval(Marquee91,speed91)}
  </SCRIPT>
  
  
  
  
  
