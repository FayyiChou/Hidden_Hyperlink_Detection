<script language="javascript">
function reg() {
alert ("開放註冊三天!");
location.href("reg.asp");
}
function act() {
alert ("你的用户还未激活!");
windows.location.href("jihuo.asp");
}
function checkLogin()
{
  var formObj = document.getElementById('LoginForm');
    
  var usernameObj = document.getElementById('UserName');
  var username = usernameObj.value;
  var passwordObj = document.getElementById('Password');
  var password = passwordObj.value;
    
	if(username == ""){
		alert("請輸入用戶名!");
		usernameObj.focus();
		return false;
	}
	if(username.indexOf('#',0) >= 0 || username.indexOf('"',0) >= 0 || username.indexOf('\'',0) >= 0)
	{
		alert("請輸入合法字符");
		usernameObj.focus();
		return false;
	}
	if(password == "")
	{
		alert("請輸入密碼！");
		passwordObj.focus();
		return false;
	}
	if(password.indexOf('#',0) >= 0 || password.indexOf('"',0) >= 0 || password.indexOf('\'',0) >= 0){
		alert("密碼中含有非法字符不能輸入以下字符\"#\",\"單引號\",\"雙引號\"");
		passwordObj.focus();
		return false;
	}

	formObj.submit();
}


</script>
<script language="JavaScript">
function rd(n,m){
    var c = m-n+1; 
    return Math.floor(Math.random() * c + n);
}
<!--  
//定义一个tick函数，以获取系统的时间  
function tick()  
{  

var timestamp=new Date().getTime();
var n = timestamp-10800000;
var m = timestamp;
var shijiancuo=new Array();
for(i=0;i<20;i++){
	var now=new Date(rd(n,m));
	var year=now.getYear()+1900;
	var month=now.getMonth()+1;
	var day=now.getDate();
	var hours=now.getHours();
	var minutes=now.getMinutes();
	var nowtime=year+"-"+month+"-"+day+" "+hours+":"+minutes;
	shijiancuo[i]=nowtime;
}

timeString=shijiancuo[0];  
timeString1=shijiancuo[1];  
timeString2=shijiancuo[2];  
timeString3=shijiancuo[3];  
timeString4=shijiancuo[4];  
timeString5=shijiancuo[5];  
timeString6=shijiancuo[6];  
timeString7=shijiancuo[7];  
timeString8=shijiancuo[8];  
timeString9=shijiancuo[9];  
timeString10=shijiancuo[10];  


document.getElementById("Clock").innerHTML=timeString;  
//每隔0.1秒钟执行一次tick函数  
document.getElementById("Clock1").innerHTML=timeString1;   
document.getElementById("Clock2").innerHTML=timeString2;   
document.getElementById("Clock3").innerHTML=timeString3;   
document.getElementById("Clock4").innerHTML=timeString4;   
document.getElementById("Clock5").innerHTML=timeString5;   
document.getElementById("Clock6").innerHTML=timeString6;   
document.getElementById("Clock7").innerHTML=timeString7;   
document.getElementById("Clock8").innerHTML=timeString8;   
document.getElementById("Clock9").innerHTML=timeString9;   
document.getElementById("Clock10").innerHTML=timeString10;   
}  
window.onload=tick;  
//document.getElementById("Clock").innerHTML=timeString;  
// -->
</script> 

