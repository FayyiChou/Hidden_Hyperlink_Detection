    <script>
        $(function () {

            //添加本页到历史记录
            xiaoxue.browsingHistory.add();

            //获取并更新浏览量
            $.ajax({
                type: "post",
                dataType: "json",
                async: true,
                url: "/q.ashx",
                data: { action: "GetUpdateFeatureClicks", id: 1839 },
                success: function (d) {
                    isSuccess = !d.errNo;
                    if (d.errNo > 0) {
                        alert(d.errMsg);
                        return;
                    };
                    $("#featureClicks").html(d.data.ViewedCount);
                },
                error: function () {
                    alert('获取点击量错误：' + arguments[1]);
                }
            });
        });

        //复选框
        $(function(){

            //全选
            $('.CbxAll').on('click', function(){
                $(".zt_zlxq [type='checkbox']").prop('checked', $(this).prop('checked'));
                UpdateCheckMessage();
            });

            //单元全选
            $('.CbxUnit').on('click', function(){
                var p = $(this).prop('name').replace("CbxUnit", "");
                $('.SoftInUnit' + p).prop('checked', $(this).prop('checked'));

                if ($('.SingleSoft:checked').length ==$('.SingleSoft').length) {
                    $('.CbxAll').prop('checked', true);
                }else {
                    $('.CbxAll').prop('checked', false);
                }

                
                UpdateCheckMessage();
            });

            //单选
            $('.SingleSoft').on('click', function(){
                             
                if ($('.SingleSoft:checked').length ==$('.SingleSoft').length) {
                    $('.CbxAll').prop('checked', true);
                }else {
                    $('.CbxAll').prop('checked', false);
                }

                var unit = $(this).attr('unit');

                if ($('.SoftInUnit'+ unit + ':checked').length == $('.SoftInUnit'+ unit).length) {
                    $("[name='CbxUnit" +unit + "']").prop('checked', true);
                }else {
                    $("[name='CbxUnit" +unit + "']").prop('checked', false);
                }
                
                UpdateCheckMessage();
            });

            //更新选中信息
            function UpdateCheckMessage(){

                var totalPoint = 0;

                var totalMoney = 0;

                var totalCount = 0;

                var totalSize = 0;

                var mfCount = 0;

                var ptCount = 0;

                var jpCount = 0;

                var wxtCount = 0;
                
                $(".SingleSoft:checked").each(function(){

                    var leavel = $(this).siblings('p').first().find("[name='SoftLeavel']").val();
                   
                    if (leavel == 1) {
                        mfCount++;
                    } else if (leavel == 2) {
                        ptCount++;
                        totalPoint += Number.parseInt($(this).attr('SoftPoint'));
                    }else if (leavel == 3) {
                        jpCount++;
                        totalMoney += Number.parseFloat($(this).attr('SoftMoney'));
                    }else if (leavel == 4) {
                        wxtCount++;
                    }

                    totalSize +=  Number.parseFloat( $(this).attr('SoftSize') );
                    totalCount++;

                });

                var msg="";

                if (totalCount > 0) {
                    
                    msg = "共选中 " + totalCount +" 个资料(" +  (totalSize>=1024 ? Math.round(totalSize/1024*100)/100  +"MB" : totalSize + "KB") + ") ";

                    if (totalPoint > 0 ) {
                        msg += "需要 " + totalPoint + "普通点 ";
                    }

                    if (totalMoney > 0 ) {
                        msg += "需要 " + totalMoney + "元储值 ";
                    }

                    if (wxtCount > 0) {
                        msg += wxtCount + "个网校通资料,仅网校通用户可下载 ";
                    }

                }

                $('.CheckMsg').html(msg);
            }

            $('#downloadALL').on('click', function(){

                xiaoxue.goDownload("/download.aspx?featureid=1839&title=" + "%e3%80%90%e6%95%b4%e5%bc%a0%e4%b8%93%e8%be%91%e3%80%91%e3%80%90%e7%b2%be%e5%93%81%e3%80%91%e5%b0%8f%e5%ad%a6%e8%8b%b1%e8%af%ad%e5%8f%a5%e5%bc%8f%e8%ae%ad%e7%bb%83%e8%90%a5");
            });

            $('.download_all').on('click', function(){

                var softIds="";

                $(".SingleSoft:checked").each(function(){
                    softIds += $(this).val() +",";
                });

                if (softIds.length > 0) {
                    xiaoxue.goDownload("/download.aspx?featureid=1839&softid=" + softIds);
                }else {
                    alert("请勾选要下载的资料");
                }
                
            });
        });

        

    </script>

