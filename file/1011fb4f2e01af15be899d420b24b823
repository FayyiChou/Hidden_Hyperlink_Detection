<script language="javascript">
function tabItem(tanme,tid,tnum)
{
	for (var t=0;t<=tnum;t++)
	{
		if (t==tid){
			document.getElementById(tanme+"_"+t).className = "nav_hover";
			document.getElementById(tanme+"_div_"+t).style.display = "block";
		}
		else{
			document.getElementById(tanme+"_"+t).className = "nav";
			document.getElementById(tanme+"_div_"+t).style.display = "none";
		}
	}
}

function searchurl(){
	var kwd=document.soform.keyword.value;
	kwd=encodeURIComponent(kwd);
	var strgourl="";
	var sobj=document.getElementsByName("gourl");
	for (var s=0;s<sobj.length;s++){
		if(sobj[s].checked == true){
			strgourl=sobj[s].value;
		}
	}
	if (strgourl == "baidu"){
		window.location.href="http://www.baidu.com/s?wd="+kwd+"&ie=utf-8";
	}else{
		window.location.href="search.asp?keyword="+kwd;
	}
	return false;
}
</script>
