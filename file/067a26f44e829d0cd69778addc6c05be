<script type="text/javascript">
function addCookie(){　 // 加入收藏夹
if (document.all){
window.external.addFavorite('http://www.mxsc.com.cn/', '民祥建材网');
}else if (window.sidebar){
window.sidebar.addPanel('民祥建材网', 'http://www.mxsc.com.cn/', "");
}
}
function addCookie2(){
try{
window.external.AddFavorite('http://www.mxsc.com.cn/','民祥建材网'); 
}catch(e){
(window.sidebar)?window.sidebar.addPanel('民祥建材网','http://www.mxsc.com.cn/',''):alert('请使用按键 Ctrl+d，收藏民祥建材网'); 
}
}
function setHomepage(){　 // 设置首页
if (document.all){
document.body.style.behavior = 'url(#default#homepage)';
document.body.setHomePage('http://www.mxsc.com.cn/');
}else if (window.sidebar){
if (window.netscape){
try {
netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
}catch (e) {
alert("该操作被浏览器拒绝，如果想启用该功能，请在地址栏内输入 about:config,然后将项 signed.applets.codebase_principal_support 值该为true");
}
}
var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
prefs.setCharPref('browser.startup.homepage', 'http://www.mxsc.com.cn/');
}
} 
</script>
