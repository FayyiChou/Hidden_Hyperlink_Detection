<script>

document.getElementById('wangzhiSearchBtn').onclick = function (){

  var key = document.getElementById('wangzhiSearchKey').value;

  var url = 'http://domain.knet.cn/web/domain/index.html?keyword=' + encodeURIComponent(key);

