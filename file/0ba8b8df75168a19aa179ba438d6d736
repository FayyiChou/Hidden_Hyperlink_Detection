<script type="text/javascript">

var k = 0;

var myVideo = $("#myVideo").get(0);

var myImg = $("#myImg");

$("#myVideo,#myImg").click(function(){

  if($(this).attr("id")=="myImg"){

     $(this).hide();

     myVideo.play();

     k = 1;

  }else{

   if(k == 0){

    myVideo.play();

    myImg.hide();

    k = 1;

  }else if(k ==1 ){

    myVideo.pause();

    myImg.show();

    k = 0;

  }

}

  

});



</script>







