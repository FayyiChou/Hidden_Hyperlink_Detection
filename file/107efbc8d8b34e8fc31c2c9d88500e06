$().ready(function(e) {
	var h = 60,j = $('#JS-content-absstract'),z = $('#zhedie');
	if(j.height()>h){
		j.css({
			'height':h + 'px',
			'overflow':'hidden'
		});
		z.html('展开').attr('to',1);
	}else{
		z.hide();
	}
	$('[to="1"]').live('click',function(data){
		j.css('height','auto');
		z.html('收起').attr('to',0);
	});
	$('[to="0"]').live('click',function(data){
		j.css('height',h + 'px');
		z.html('展开').attr('to',1);
	});
});
