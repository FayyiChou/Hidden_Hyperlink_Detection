	<script type="text/javascript">
		$(function () {
			//右侧侧附加选择层
			(function () {
				$('[data-action=popup-car]').rightSwipe({
					clickEnd: function (b) {
						var $leftPopup = this;
						if (b) {
							var $back = $('.' + $leftPopup.attr('data-back'))
							$back.touches({ touchstart: function (ev) { ev.preventDefault(); }, touchmove: function (ev) { ev.preventDefault(); } });
							var $swipeLeft = $leftPopup.find('.swipeLeft');

							$leftPopup.myScroll = new IScroll($swipeLeft[0], {
								probeType: 1,
								snap: 'dd',
								momentum: true,
								click: true
							});
						} else {
							$leftPopup.myScroll.scrollTo(0, 0, 0, false);
						}
					}
				});
			})();
		});
	</script>
	<script type="text/javascript">
		$(function () {
			$.ajax({
				url: "http://api.car.bitauto.com/CarInfo/GetCarDataJson.ashx",
				type: "get",
				dataType: "jsonp",
				data:{action:"car",pid:4937},
				async:false,
				cache:true,
				jsonpCallback:"fetchCarData",
				success:function(result){
					if (typeof result == 'undefined') {
						return;
					} else {
						if(result.CarList){
							var carModelArray = new Array();
							var carId = 123290;
							carModelArray.push("<dl class=\"tt-list absolute\">");
							for(var year in  result.CarList)
							{
								carModelArray.push("<dt><span>"+ year.replace('s','') + "款</span></dt>");
								for(var j=0;j<result.CarList[year].length;j++){
									var priceShow = result.CarList[year][j].ReferPrice.length>0&&result.CarList[year][j].ReferPrice>0?result.CarList[year][j].ReferPrice+"万":"暂无";
									if(result.CarList[year][j].CarId==carId){
										carModelArray.push("<dd class=\"current\">");
									}else{
										carModelArray.push("<dd>");
									}
									carModelArray.push("<a href=/car/"+result.CarList[year][j].CarId+ ">");
									carModelArray.push("<p>"+result.CarList[year][j].CarName+ "</p>" );
									carModelArray.push("<strong>"+priceShow+"</strong>");
									carModelArray.push("</a></dd>");
								}
							}
							carModelArray.push("</dl>");
							var carModelString = carModelArray.join("");
							$("#popCarList").html(carModelString);
						}
					}
				}
			});
		});
	</script>
