<script type="text/javascript">
$(document).ready(function() {
if(350==1){
$("#channel_001").addClass("on"); 
}
    $("#channel_00350").addClass("cur"); 
	$('.nav_t li, .menulist').mouseenter(function() {
		var id = this.id.replace(/channel_00|box/, '');
		oN(id);
	}).mouseleave(function() {
		var id = this.id.replace(/channel_00|box/, '');
		oFF(id);
	});
});
function oN(tar){
	var id = '' + 350;
	document.getElementById('box' + tar).style.display = 'block';
	if(tar != id){
		document.getElementById("box" + id).style.display = 'none';
	}
}
function oFF(tar){
	var id = '' + 350;
	document.getElementById('box' + tar).style.display = 'none';
	document.getElementById("box" + id).style.display = 'block';
}
</script>
