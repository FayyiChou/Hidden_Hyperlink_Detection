        $(".lnavh").hide();
        $(".lnav").hover(
                function(){
                    $(this).children(".lnavi").hide();
                    $(this).children(".lnavh").show();
                },
                function(){
                    $(this).children(".lnavh").hide();
                    $(this).children(".lnavi").show();
                }
        );
    });
