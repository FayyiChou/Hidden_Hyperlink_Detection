var sUserAgent = navigator.userAgent;
        var bIsAndroid = sUserAgent.toLowerCase().match(/android/i) == "android";
        function scroll_functionz_ipad(){
            if( m2o_stream_trigger && ($(document).scrollTop() + $(window).height() >= $(document).height()*0.9) ) {
                m2o_stream();
            }
        }

                                                function scroll_function(){
                                                if( m2o_stream_trigger && ($(document).scrollTop() + $(window).height() >= $(document).height()) ) {
                                                m2o_stream();
                                                }
                                                }


                                                $(window).scroll(function(){
                                                clearTimeout(m2o_stream_timer);
                                                m2o_stream_timer = setTimeout(function(){
                                                    
                if (bIsAndroid || 'standalone' in navigator && !navigator.standalone && (/iphone|ipod|ipad|Android/gi).test(navigator.platform) && (/Safari/i).test(navigator.appVersion)) {
                    scroll_functionz_ipad();
                }                
                                                    
                                                scroll_function();
                                                }, 500);
                                                });
