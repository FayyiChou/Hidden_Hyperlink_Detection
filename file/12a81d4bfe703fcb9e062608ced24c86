  <script type="text/javascript">
$(function(){
		tabpage("cl-item","cr-item","cl-item-active");
		var titles=$('.cate-name');
		var contents=$('.cate-block-content');
		titles.hover(function(){
			var curIndex=titles.index(this);
			$(contents[curIndex]).show();
		},function(){
			var curIndex2=titles.index(this);
			$(contents[curIndex2]).hide();
		});
		$("#qbg-search").focus(function(){
			if($(this).val()=='一键办公管家'){
				$(this).val('');
			}
		});
		$("#qbg-search").blur(function(){
			if($(this).val()==''){
				$(this).val('一键办公管家');
			}
		});
		$("#btn-search").click(function(){
			$("#searchForm").submit();
		});
		
		$("#ECS_CARTINFO").hover(function(){
			$('#cart-list').show();
			$('.cart-link').addClass('cart-link-active');
		},function(){
			$('#cart-list').hide();
			$('.cart-link').removeClass('cart-link-active');
		});		
		
		$(".cate-down2").hover(function(){
			$(this).addClass('cate-down-color');
			$("#category").show();
		},function(){
			$("#category").hide();
			$(this).removeClass('cate-down-color');
		});
		$('.cri4-block').hover(function(){
			var curIndex=$('.cri4-block').index(this);
			$($('.cri4-list')[curIndex]).show();
			$($('.cbc-space4')[curIndex]).show();
		},function(){
			var curIndex=$('.cri4-block').index(this);
			$($('.cri4-list')[curIndex]).hide();
			$($('.cbc-space4')[curIndex]).hide();
		});
		$(".cate-bottom").hover(function(){
			$('.brand-cate-content').show();
		},function(){
			$('.brand-cate-content').hide();
		});
});
function tabpage(titleClass,contentClass,activeClass){
	$("."+titleClass).mouseover(function(){
		var titles=$('.'+titleClass);
		var contents=$('.'+contentClass);
		var curIndex=titles.index(this);
		$('.'+activeClass).removeClass(activeClass);
		$(this).addClass(activeClass);
		contents.hide();
		$(contents[curIndex]).show();
	});
}
</script>
