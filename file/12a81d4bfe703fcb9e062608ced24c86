       <script type="text/javascript">
$(document).ready(function(){
	var i_num=$(".pic_s_list").find("li").size();
	if(i_num>4){
		var max_top=-104*(i_num-4);
	}else{
		var max_top=0;
	}
	
	$("#goprev").click(function(){
		if(max_top==0){return;}
		var m_t=$(".pic_s_0").css("marginTop");
		var m_t_l=m_t.length;
		var m_t_n=parseInt(m_t.substr(0,(m_t_l-2)));
		if(m_t_n<0){
			var to_t=(m_t_n+104)+"px";
			$(".pic_s_0").css("marginTop",to_t);
		}
	});
	$("#gonext").click(function(){
		if(max_top==0){return;}
		var m_t=$(".pic_s_0").css("marginTop");
		var m_t_l=m_t.length;
		var m_t_n=parseInt(m_t.substr(0,(m_t_l-2)));
		if(m_t_n>max_top){
			var to_t=(m_t_n-104)+"px";
			$(".pic_s_0").css("marginTop",to_t).delay(1000);
		}
	});
	$(".pic_s_list li").mouseover(function(){
		$(".pic_s_list li").removeClass("pic_s_on");
		$(this).addClass("pic_s_on");
		var b_p=$(this).find("img").attr("img_data");
		$(".pic_b_list a").attr("href",b_p);
		$("#showbigpic").attr("src",b_p);
	});
});
</script> 
