  <script type=text/javascript>
function check_form(name,action)
{
var x ;
var mycars =[1,2,4,5,6];
for ( x in mycars)
{
chgClassName('right4_menu_'+mycars[x],'0');
} 
if (action=="news")
{	
document.getElementById("mod").value="portal";
document.search_form.action="search.php?searchsubmit=yes";
} else if (action=="bbs")
{	document.getElementById("mod").value="forum";
document.search_form.action="search.php?searchsubmit=yes";
} else if (action=="shop")
{	
document.search_form.action="http://www.haofs.com/shop/search.asp";
} else if (action=="house")
{
document.search_form.action="http://www.haofs.com/house/list.asp";
} else if (action=="job")
{
document.search_form.action="http://www.haofs.com/job/list.asp";
}
chgClassName('right4_menu_'+name,'on');
GetObj('searchtarget').value = name;
}
 </script>
