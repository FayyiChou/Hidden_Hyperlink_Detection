<script language="javascript">
var h = document.body.clientHeight;
var h1 = document.getElementById("pctop").offsetHeight + 3;
var h2 = document.getElementById("rightCnt").clientHeight + 20;

if(h-200 > h1 ){
document.getElementById("pctop").style.height = h-200 + "px";
document.getElementById("rightCnt").style.height = h-200 + "px";
}
else{
document.getElementById("pctop").style.height = 630 + "px";
document.getElementById("rightCnt").style.height = 610 + "px";
}

var h1 = document.getElementById("pctop").offsetHeight + 3;
var h2 = document.getElementById("rightCnt").clientHeight + 20;

function adjustHeight(){
if(h1>h2){
document.getElementById("rightCnt").style.height = h1 - 20 + "px";
}
else{
document.getElementById("pctop").style.height = h2 - 3 + "px"
}
}
adjustHeight();

</script>


