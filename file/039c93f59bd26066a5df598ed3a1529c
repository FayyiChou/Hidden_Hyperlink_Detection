	<script language="javascript">
  function switchmodTag3(modtag,modcontent,modk) {
    for(f=10; f <15; f++) {
      if (f==modk) {
        document.getElementById(modtag+f).className="menuOn";document.getElementById(modcontent+f).className="slidingList";}
      else {
        document.getElementById(modtag+f).className="menuNo";document.getElementById(modcontent+f).className="slidingList_none";}
    }
  }
    </script>
     
