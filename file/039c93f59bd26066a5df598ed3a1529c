	<script language="javascript">
  function switchmodTag17(modtag,modcontent,modk) {
    for(h=70; h <76; h++) {
      if (h==modk) {
        document.getElementById(modtag+h).className="menuOn";document.getElementById(modcontent+h).className="slidingList";}
      else {
        document.getElementById(modtag+h).className="menuNo";document.getElementById(modcontent+h).className="slidingList_none";}
    }
  }
    </script>
    
    
    
	     
