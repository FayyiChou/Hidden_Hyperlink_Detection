  function switchmodTag9(modtag,modcontent,modk) {
    for(m=20; m <27; m++) {
      if (m==modk) {
        document.getElementById(modtag+m).className="menuOn";document.getElementById(modcontent+m).className="slidingList";}
      else {
        document.getElementById(modtag+m).className="menuNo";document.getElementById(modcontent+m).className="slidingList_none";}
    }
  }
    </script>

