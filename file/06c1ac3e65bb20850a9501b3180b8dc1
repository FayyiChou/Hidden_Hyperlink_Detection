                                $(function () {
                                    var rollH = $(".upscroll_box").height();
                                    var listH = $(".upscroll_box .tb_list").height();
                                    //alert(listH);
                                    if (listH > rollH) {
                                        var martop = 0;
                                        $(".upscroll_box .tb_list").clone().attr("class", "").appendTo(".upscroll_box");
                                        function rollText() {
                                            martop++;
                                            if (martop > listH) {
                                                martop = 0;
                                                $(".upscroll_box .tb_list").css("margin-top", -martop);
                                            }
                                            else {
                                                $(".upscroll_box .tb_list").css("margin-top", -martop);
                                            }
                                        }
                                        var int = setInterval(rollText, 50);
                                        $(".upscroll_box").hover(
                            function () {
                                clearInterval(int);
                            },
                            function () {
                                int = setInterval(rollText, 50);
                            }
                        );
                                    }
                                });
