    function $TAB(x) {
        return document.getElementById(x);
    }
    function tab_show(a, b, c, d) {
        x = a,max_i = b,tn = c,tc = d;
        for (var i = 1; i <= max_i; i++) {
            $TAB(tn + i).className = $TAB(tc + i).className = "";
        }
        $TAB(tn + x).className = $TAB(tc + x).className = "on";
    }
    (function() {
        $(".j-morebtn").click(function(ev) {
            $(this).hide();
            $(".j-morebox").show();
            return false;
        });
        $(".j-retractbtn").click(function(ev) {
            $(".j-morebox").hide();
            $(".j-morebtn").show();
            return false;
        });
    })();


