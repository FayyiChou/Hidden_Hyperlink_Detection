<script type="text/javascript">
vali = {captcha: 0, email: 0, pwd: 0};
$('#email').bind('blur', function(){
    var pLen = $(this).val().length;
    if(pLen < 6 || pLen > 30) return validateMsg('email', '请输入邮箱', 0);
     var email = $(this).val();
    $.post("/user/checkIp",{email:$(this).val()},
		function(d){
                if(d.status==1){
                    $.post("/user/issetPhone",{email:email},function(re) { 
                        if(re.status == 2){
                            $("#authCode").attr('name', 'authCode');
                            $('#loginPhoneCode').show();
                            $("#birthcheck").hide();
                            $('#birthmsg').text('');
                            $('.dengluu').css('margin-left', '200px');
                            $('.dengluu').attr("type","button");
                            $('.gettfirdLogin').css('margin-left', '300px;');
                        }else{
                            $("#birthcheck").show();
                            $('#loginPhoneCode').hide();
                            $('#birthmsg').html('系统监测到您的账号本次登录IP和上次不同，为了保障您的账户资产安全，请输入您在元宝网预留的身份证上的出生日期；如还未实名认证，请联系客服认证。');
                        }
                },'json');
		}
    }, 'json');
    return validateMsg('email', '', vali.email = 1);
});
$('#pwd').bind('blur', function(){
    var pLen = $(this).val().length;
    if(pLen < 6 || pLen > 20) return validateMsg('pwd', '密码长度在6-20个字符之间', 0);
    return validateMsg('pwd', '', vali.pwd = 1);
});
    vali.captcha = 1;
$("h4 input.gettfirdLogin").on("click",function(){
    location.href="https://creditid.taiyiyun.com/web/ThirdPartCustomer/Login/?Appkey=7AFBA915C0DC4F8580211EE11C5CD5F0&redirect_uri=https%3A%2F%2Fwww.yuanbao.com%2Fuser%2Flogin&State=22d5907b-40cd4c56-61d53e59-6aa5c920&Sign=65ABDB783EB8E63B87D89BF3D6AAB964";
    // alert('暂停服务，维护中');
});
</script>
