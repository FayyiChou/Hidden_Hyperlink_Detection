			function ewm_show (thisdom,i_scr,idom) {
				$(thisdom).parents("li").find("."+idom).fadeIn();
				$(thisdom).attr("src",i_scr);
			}
			function ewm_hide (thisdom,i_scr,idom) {
				$(thisdom).parents("li").find("."+idom).fadeOut();
				$(thisdom).attr("src",i_scr)
			}
