    <script type="text/javascript" language="javascript">
         var speed = 40
         ccdemo2.innerHTML = ccdemo1.innerHTML
         function Marquee() {
             if (ccdemo2.offsetWidth - ccdemo.scrollLeft <= 0)
                 ccdemo.scrollLeft -= ccdemo1.offsetWidth
             else {
                 ccdemo.scrollLeft++
             }
         }
         var MyMar = setInterval(Marquee, speed)
         ccdemo.onmouseover = function () { clearInterval(MyMar) }
         ccdemo.onmouseout = function () { MyMar = setInterval(Marquee, speed) }
    </script> 
    <script type="text/javascript" language="javascript">
        var speed = 40
        cdemo2.innerHTML = cdemo1.innerHTML
        function Marquee() {
            if (cdemo2.offsetWidth - cdemo.scrollLeft <= 0)
                cdemo.scrollLeft -= cdemo1.offsetWidth
            else {
                cdemo.scrollLeft++
            }
        }
        var MyMar = setInterval(Marquee, speed)
        cdemo.onmouseover = function () { clearInterval(MyMar) }
        cdemo.onmouseout = function () { MyMar = setInterval(Marquee, speed) }
    </script>
