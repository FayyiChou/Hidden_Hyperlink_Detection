			var val = "msg";
			var re = new RegExp("" + val + "=([^&?]*)", "ig");
			var msg=((uri.match(re)) ? (uri.match(re)[0].substr(val.length + 1)) : null);
			if(msg!=null && msg!='' && msg=='LimitLogin'){
				dialog('提示信息',"您的帐号在其他地点登录，请重新登录",1);
			}

			$(".i-q-txt").mouseenter(function(){
				$(this).show();
				$(this).parents(".i-teach-wrap").find(".i-teach-pic-bg").show();
			});
			
			$(".JSTD-list-wrap .i-teach-pic a").mouseenter(function(){
				$(this).css("color","#e94e60");
				$(".i-teach-wrap").find(".i-q-txt").hide();
				$(".i-teach-wrap").find(".i-teach-pic-bg").hide();
				$(this).parent().siblings(".i-q-txt").show();
				$(this).parents(".i-teach-wrap").find(".i-teach-pic-bg").show();
			});
			$(".JSTD-list-wrap .i-teach-pic a").mouseleave(function(){
				$(this).css("color","#242424");
				$(this).parent().siblings(".i-q-txt").hide();
				$(this).parents(".i-teach-wrap").find(".i-teach-pic-bg").hide();
			});
			
			$(".JSTD-list-wrap .txtOf.tac a").mouseenter(function(){
				$(this).css("color","#e94e60");
				
				$(this).parent().siblings(".i-q-txt").show();
				$(this).parents(".i-teach-wrap").find(".i-teach-pic-bg").show();
			});
			$(".JSTD-list-wrap .txtOf.tac a").mouseleave(function(){
				$(this).css("color","#242424");
				$(this).parent().siblings(".i-q-txt").hide();
				$(this).parents(".i-teach-wrap").find(".i-teach-pic-bg").hide();
			});
			
			$(".swiper-slide").css("background","none");
			
		});
		
		var pageD = 1;
		//教师团队-左按钮
		function courseListCutLeftD(selector,selectorWrap,dotWrap,num){
			var list = $(selector);
			var w = $(selectorWrap).width();
			var page_count = Math.ceil(list.find("li").length / num);
			
			if(!list.is(":animated")){
				if(pageD != 1){
					list.animate({left:'+=' + w},"slow");
					pageD--;
					$(dotWrap).find(".current").prev().addClass("current").end().removeClass("current");
				}else{
					return;
				}
			}
		}

		//教师团队-右按钮
		function courseListCutD(selector,selectorWrap,dotWrap,num){
			
				var list = $(selector);
				var w = $(selectorWrap).width();
				var page_count = Math.ceil(list.find("li").length / num);
				
					if(!list.is(":animated")){
						if(pageD == page_count){
							/*list.animate({left:'0px'},"slow");
							page = 1;*/
							return;
						}else{
							list.animate({left:'-=' + w},"slow");
							pageD++;
							$(dotWrap).find(".current").next().addClass("current").end().removeClass("current");
						
						}
					}

		}
		
	</script>
