            <script type="text/javascript">
				$(".n_til1 span a:first").addClass("cur");
				$(".block_cont_1:second").css("display","none");
                function ShowCheckNew2(src) {
                    $(src).parent().children().removeClass("cur");
                    $(src).addClass("cur");
                    var Index = $(src).attr("id");
                    Index = Index.substring(3, Index.length);
                    var ShowIndex = "#shz" + Index
                    $(src).parent().parent().parent().children(".block_cont_1").hide();
                    $(src).parent().parent().parent().find(ShowIndex).show();

                    var ShowIndex1 = "#shzz" + Index
                    $(src).parent().parent().children(".fr").hide();
                    $(src).parent().parent().find(ShowIndex1).show();

                }
                var CheckIndex = "0";
                var SwichID = "#index_dongtai";

                $(SwichID).find("#chz" + CheckIndex).addClass("cur");
                $(SwichID).find("#shz" + CheckIndex).show();
                $(SwichID).find("#shzz" + CheckIndex).show();				
            </script>

