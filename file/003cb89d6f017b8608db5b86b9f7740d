<script type="text/javascript">
	// 为了捕获一些异常信息 监听全局的error函数  先尝试是否有效
	function globalSendErrorLog(msg, url, line, column, error) {
		var baseArr = [];
		baseArr.push('errorMsg=' + msg);
		baseArr.push('url=' + url);
		baseArr.push('lineNo=' + (line ? line : 'no_line'));
		baseArr.push('columnNo=' + (column ? column : 'no_column'));
		baseArr.push('errorDetail=' + error);
		var img = new Image();
		img.src = '/log/errorLog?' + baseArr.join('&');
	}
	window.onerror = function(msg, url, lineNo, columnNo, error) {
		globalSendErrorLog(msg, url, lineNo, columnNo, error);
	}
</script>	<link rel="stylesheet" href="/static/page/login/login_146ce4f.css">
