  <script type="text/javascript">
function Verifycomment(){
	var $tcomment=$("#tareaComment").val();
	if ( $tcomment=='' || $tcomment.length<2 )
	{
		alert('请正确填写评论内容:)');
		return false;
	}else{
		$("#commentform").submit();
	}
}
//切换游戏
function regame(ngamep,rep){
	ngamep=ngamep<1?1:ngamep;
	$.ajax({
		   type: "GET",
		   url: "/Xinwen/newingame",
		   async: false,
		   data: "p="+ngamep+"",
		   dataType:"json",
		   success: function(ret){
			 if(ret.status==1){
				 if(rep==1){
				 	ngamep=parseInt(ngamep)+1;
				 	$("#ngamep").val(ngamep);
				 }
				 info = eval(ret.list)
				 var strhtml='';
				  for(var i=0; i<info.length; i++)  
				  {  
					strhtml+='<div class="nigame"><div class="nigame-left">';
					strhtml+='<a href="http://www.daogame.cn/game-'+info[i].id+'.html" target="_blank"><img src="http://img.daogame.cn'+info[i].thumb+'" alt="" width="70" height="70" /></a></div>';
					strhtml+='<div class="nigame-right"><h3><a href="http://www.daogame.cn/game-'+info[i].id+'.html" target="_blank">'+info[i].title+'</a></h3>';
					strhtml+='<span><a href="http://www.daogame.cn/game-'+info[i].id+'.html" target="_blank">'+info[i].mods+'</a></span></div>';
					strhtml+='<div class="clear"></div></div>';
				     
				  }  
				  $("#newingame-cont").html(strhtml);
			 }
			 
		   }
	});
}
$(document).ready(function(){
	//自动切换
	var ngamep=$("#ngamep").val();
	regame(ngamep,0);
	//点击切换；
	$("#nextp").click(function(){
		var ngamep=$("#ngamep").val();
		ngamep=parseInt(ngamep)+1;
		regame(ngamep,1);
	});
});

</script>  
