function adSwipe(m,e){var f=function(){};var u=function(C){setTimeout(C||f,0)};var B={addEventListener:!!window.addEventListener,touch:("ontouchstart" in window)||window.DocumentTouch&&document instanceof DocumentTouch,transitions:(function(C){var E=["transitionProperty","WebkitTransition","MozTransition","OTransition","msTransition"];for(var D in E){if(C.style[E[D]]!==undefined){return true}}return false})(document.createElement("swipe"))};if(!m){return}var c=m.children[0];var s,d,r,g;e=e||{};var k=parseInt(e.startSlide,10)||0;var v=e.speed||300;e.continuous=e.continuous!==undefined?e.continuous:true;function n(){s=c.children;g=s.length;if(s.length<2){e.continuous=false}if(B.transitions&&e.continuous&&s.length<3){c.appendChild(s[0].cloneNode(true));c.appendChild(c.children[1].cloneNode(true));s=c.children}d=new Array(s.length);r=m.getBoundingClientRect().width||m.offsetWidth;c.style.width=(s.length*r)+"px";var D=s.length;while(D--){var C=s[D];C.style.width=r+"px";C.setAttribute("data-index",D);if(B.transitions){C.style.left=(D*-r)+"px";q(D,k>D?-r:(k<D?r:0),0)}}if(e.continuous&&B.transitions){q(i(k-1),-r,0);q(i(k+1),r,0)}if(!B.transitions){c.style.left=(k*-r)+"px"}m.style.visibility="visible"}function o(){if(e.continuous){a(k-1)}else{if(k){a(k-1)}}}function p(){if(e.continuous){a(k+1)}else{if(k<s.length-1){a(k+1)}}}function i(C){return(s.length+(C%s.length))%s.length}function a(G,D){if(k==G){return}if(B.transitions){var F=Math.abs(k-G)/(k-G);if(e.continuous){var C=F;F=-d[i(G)]/r;if(F!==C){G=-F*s.length+G}}var E=Math.abs(k-G)-1;while(E--){q(i((G>k?G:k)-E-1),r*F,0)}G=i(G);q(k,r*F,D||v);q(G,0,D||v);if(e.continuous){q(i(G-F),-(r*F),0)}}else{G=i(G);j(k*-r,G*-r,D||v)}k=G;u(e.callback&&e.callback(k,s[k]))}function q(C,E,D){l(C,E,D);d[C]=E}function l(D,G,F){var C=s[D];var E=C&&C.style;if(!E){return}E.webkitTransitionDuration=E.MozTransitionDuration=E.msTransitionDuration=E.OTransitionDuration=E.transitionDuration=F+"ms";E.webkitTransform="translate("+G+"px,0)"+"translateZ(0)";E.msTransform=E.MozTransform=E.OTransform="translateX("+G+"px)"}function j(G,F,C){if(!C){c.style.left=F+"px";return}var E=+new Date;var D=setInterval(function(){var H=+new Date-E;if(H>C){c.style.left=F+"px";if(A){x()}e.transitionEnd&&e.transitionEnd.call(event,k,s[k]);clearInterval(D);return}c.style.left=(((F-G)*(Math.floor((H/C)*100)/100))+G)+"px"},4)}var A=e.auto||0;var w;function x(){w=setTimeout(p,A)}function t(){clearTimeout(w)}var h={};var y={};var z;var b={handleEvent:function(C){switch(C.type){case"touchstart":this.start(C);break;case"touchmove":this.move(C);break;case"touchend":u(this.end(C));break;case"webkitTransitionEnd":case"msTransitionEnd":case"oTransitionEnd":case"otransitionend":case"transitionend":u(this.transitionEnd(C));break;case"resize":u(n);break}if(e.stopPropagation){C.stopPropagation()}},start:function(C){var D=C.touches[0];h={x:D.pageX,y:D.pageY,time:+new Date};z=undefined;y={};c.addEventListener("touchmove",this,false);c.addEventListener("touchend",this,false)},move:function(C){if(C.touches.length>1||C.scale&&C.scale!==1){return}if(s.length<2){return}if(e.disableScroll){C.preventDefault()}var D=C.touches[0];y={x:D.pageX-h.x,y:D.pageY-h.y};if(typeof z=="undefined"){z=!!(z||Math.abs(y.x)<Math.abs(y.y))}if(!z){C.preventDefault();t();if(e.continuous){l(i(k-1),y.x+d[i(k-1)],0);l(k,y.x+d[k],0);l(i(k+1),y.x+d[i(k+1)],0)}else{y.x=y.x/((!k&&y.x>0||k==s.length-1&&y.x<0)?(Math.abs(y.x)/r+1):1);l(k-1,y.x+d[k-1],0);l(k,y.x+d[k],0);l(k+1,y.x+d[k+1],0)}}},end:function(E){var G=+new Date-h.time;var D=Number(G)<250&&Math.abs(y.x)>20||Math.abs(y.x)>r/2;var C=!k&&y.x>0||k==s.length-1&&y.x<0;if(e.continuous){C=false}var F=y.x<0;if(!z){if(D&&!C){if(F){if(e.continuous){q(i(k-1),-r,0);q(i(k+2),r,0)}else{q(k-1,-r,0)}q(k,d[k]-r,v);q(i(k+1),d[i(k+1)]-r,v);k=i(k+1)}else{if(e.continuous){q(i(k+1),r,0);q(i(k-2),-r,0)}else{q(k+1,r,0)}q(k,d[k]+r,v);q(i(k-1),d[i(k-1)]+r,v);k=i(k-1)}e.callback&&e.callback(k,s[k])}else{if(e.continuous){q(i(k-1),-r,v);q(k,0,v);q(i(k+1),r,v)}else{q(k-1,-r,v);q(k,0,v);q(k+1,r,v)}}}c.removeEventListener("touchmove",b,false);c.removeEventListener("touchend",b,false)},transitionEnd:function(C){if(parseInt(C.target.getAttribute("data-index"),10)==k){if(A){x()}e.transitionEnd&&e.transitionEnd.call(C,k,s[k])}}};n();if(A){x()}if(B.addEventListener){if(B.touch){c.addEventListener("touchstart",b,false)}if(B.transitions){c.addEventListener("webkitTransitionEnd",b,false);c.addEventListener("msTransitionEnd",b,false);c.addEventListener("oTransitionEnd",b,false);c.addEventListener("otransitionend",b,false);c.addEventListener("transitionend",b,false)}window.addEventListener("resize",b,false)}else{window.onresize=function(){n()}}return{setup:function(){n()},slide:function(D,C){t();a(D,C)},prev:function(){t();o()},next:function(){t();p()},stop:function(){t()},getPos:function(){return k},getNumSlides:function(){return g},kill:function(){t();c.style.width="";c.style.left="";var D=s.length;while(D--){var C=s[D];C.style.width="";C.style.left="";
if(B.transitions){l(D,0,0)}}if(B.addEventListener){c.removeEventListener("touchstart",b,false);c.removeEventListener("webkitTransitionEnd",b,false);c.removeEventListener("msTransitionEnd",b,false);c.removeEventListener("oTransitionEnd",b,false);c.removeEventListener("otransitionend",b,false);c.removeEventListener("transitionend",b,false);window.removeEventListener("resize",b,false)}else{window.onresize=null}}}}if(window.jQuery||window.Zepto){(function(a){a.fn.Swipe=function(b){return this.each(function(){a(this).data("Swipe",new Swipe(a(this)[0],b))})}})(window.jQuery||window.Zepto)};
//幻灯结束
function SerBanner(banTypeId){
	require(['sogougps'],function(sogouGps){
	var ele=$("#topad-box"),
		_close=ele.find(".btn-close"),
		_ul = ele.find(".pic-list"),
		_pageBtn = ele.find(".page-btn"),
		isShow=false,//是否可显示
		//globalCity = global_city?global_city.split("市")[0]:'',
		city = global_city||'',
		city2 = city.split("市")[0]||'';
	var storageKey = "fwAds_"+banTypeId,
		nowTime = +new Date(),
		frVal = null;
	_close.click(function(){
		isShow=false;
		ele.hide();
		if(window.localStorage){
			var storageVal = {"close":true,"time":+new Date()};
			localStorage.setItem(storageKey,JSON.stringify(storageVal));
		}
	});
	
	var sogou1 = window.sogou = window.sogou || {};
	sogou1.head = sogou1.head || {};
	sogou1.head.ads=sogou1.head.ads||{};
	sogou1.head.ads.show=function(){
		
		if(isShow){
			ele.show();
		}
	};
	sogou1.head.ads.hide=function(){
		
		ele.hide();
	};
	//城市检查
	function checkCity(item){
		var citys = item.citys;
		if(citys&&citys.length>0&&city){
			return citys.indexOf(city)!=-1||citys.indexOf(city2)!=-1? true : false;
		}
		return true;
	}
	//月份计算0~11，但数据是按1~12进行月份设置，所以要进行减1换算
	function timeForm(time){
		if(typeof time == "string"){
			var timeArr = time.split(/[,，]/),
				y = timeArr && timeArr[0]?timeArr[0]:0,
				m = timeArr && timeArr[1]?timeArr[1]:0,
				d = timeArr && timeArr[2]?timeArr[2]:0,
				hh = timeArr && timeArr[3]?timeArr[3]:0,
				mm = timeArr && timeArr[4]?timeArr[4]:0,
				ss = timeArr && timeArr[5]?timeArr[5]:0;
			m = m>0?m-1:0;
			time = +new Date(y,m,d,hh,mm,ss);
		}else{
			time = new Date(time);
			var m = time.getMonth()>0?time.getMonth()-1:0;
			time.setMonth(m);
			time = +new Date(time);
		}
		return time;
	}
	//时间检查
	function checkTime(item){
		var daterange = item.daterange,
			startT = daterange.starttime,
			endT = daterange.endtime,
			itemShow = item.close?false:true;
		if(itemShow&&daterange&&daterange.endtime){
			var startTime=timeForm(startT),
				endTime=timeForm(endT);
			itemShow=nowTime>=startTime&&nowTime<=endTime;
			return itemShow;
		}
		return itemShow;
	}
	//判断关闭banner后，是否符合展现流程-时间有效性
	function allowShowAdsTime(closeTime,item){
		var daterange = item.daterange;
		if(daterange && daterange.starttime){
			var startT = daterange.starttime,
				endT = daterange.endtime,
				startTime=timeForm(startT),
				endTime=0;
			if(startTime>closeTime && startTime<=nowTime){
				if(endT){
					endTime=timeForm(endT);
					return nowTime<=endTime
				}				
				return true;
			}else{
				return false;
			}
		}
		return false;
	}
	//获取对应服务的广告图片
	function showAds(indexdata,closeTime){
		var ads=indexdata.ads,
			imgList = [],
			validImgs = [],
			closeAll = true,
			closeTime = closeTime||0,
			hasNewAd = false;
		for(var a=0,aln=ads.length;a<aln;a++){
			if(ads[a].fwid==banTypeId){
				imgList = ads[a].list;
				closeAll = ads[a].closeAll;
				frVal = ads[a].fr;
				break;
			}
		}
		for(var i=0,iln=imgList.length;i<iln;i++){
			var item = imgList[i];
			if(checkCity(item)&&checkTime(item)){
				validImgs.push(item);
				if(!hasNewAd && closeTime){
					hasNewAd = allowShowAdsTime(closeTime,item);
				}
			}
		}
		if(validImgs.length&&!closeAll&&(!closeTime||(closeTime && hasNewAd))){//!closeTime||(closeTime && hasNewAd)判断是否有新banner
			if(window.localStorage && localStorage.getItem(storageKey)){//删除当前服务banner的localStorage
				localStorage.removeItem(storageKey);
			}
			renderList(validImgs);//构建图片列表			
			isShow=true;
			ele.show();
			adSwipe(ele[0], {
				auto: 3000,//开始自动幻灯片（以毫秒为单位幻灯片之间的时间）
				continuous: true,//创建一个无限的循环（当滑动到所有动画结束时是否循环滑动）
				disableScroll: true,//当滚动滚动条时是否停止幻灯片滚动
				stopPropagation: true,
				callback: function(index) {
					ele.find('._pageBtn a').eq(index%validImgs.length).addClass("cur").siblings("a").removeClass("cur");
				}
			});
		}else if(closeAll){
			isShow=false;
		} 
	}
	//拼图片列表结构
	function renderList(list){
		var ulStr = '',
			pagesStr = '';
		for(var i=0,ln=list.length;i<ln;i++){
			var item = list[i],
				url = item.url;
			url=setSearchKey(url,"fr",frVal);//为banner设置来源
			ulStr += '<li style="position:relative;float:left;"><a href="'+url+'">\
						<img src="'+item.imgurl+'" />\
					</a></li>';
			if(i==0){
				pagesStr += '<a href="javascript:void(0)" class="cur"></a>';
			}else{
				pagesStr += '<a href="javascript:void(0)"></a>';
			}
		}
		_ul.append(ulStr);
		_pageBtn.append(pagesStr);
		if(list.length<2){
			_pageBtn.hide();
		}
	}
	
	//为地址添加search字段
	function setSearchKey(url,key,val){
		if(url){
			var key = key=="fr"?(~url.indexOf("http")?"sogou_fr":"fr"):key;
			var mark1 = url.indexOf("?"),
				mark2 = url.indexOf("#"),
				searchStr = '',
				hashStr = '',
				val = val||'',
				newUrl = '';
			if(~mark1){
				if(~mark2 && mark2>mark1){
					hashStr = url.substring(mark2);
					searchStr = url.substring(mark1+1,mark2);
				}else{
					searchStr = url.substring(mark1+1);
				}
				var reg = new RegExp("(^|&)"+key+"\\s*=\\s*([^&]*)");
				searchStr=searchStr.replace(reg,'');
				searchStr+="&"+key+"="+val;
				newUrl = url.substring(0,mark1+1)+searchStr+hashStr;
			}else{
				searchStr = "?"+key+"="+val;
				if(~mark2){
					hashStr = url.substring(mark2);
					newUrl = url.substring(0,mark2)+searchStr+hashStr;
				}else{
					newUrl = url+searchStr;
				}
			}
			if(newUrl.indexOf("?&")==mark1){
				newUrl = newUrl.replace("?&","?");
			}
			return newUrl;
		}
		return url;
	}
	
	//判断关闭banner后，是否符合展现流程
	function allowShowAds(key,indexdata){
		var allowAdShow = true,
			closeTime = 0;
		if(window.localStorage && localStorage.getItem(key)){
			var fwAds = JSON.parse(localStorage.getItem(key));
			if(fwAds.close && (fwAds.close==true || fwAds.close=="true")){
				var endTime=fwAds.time+24*3600*1000;
				closeTime = fwAds.time;
				allowAdShow=nowTime>=endTime;
			}
		}
		if(allowAdShow)showAds(indexdata,closeTime);
	}
	function initGps(){
        function sucGps(address, position, data, locationStr){
        	if(data&&(data.status=='ok'||data.city)){
            	if(!(data.city==city || data.city==city2)){
            		city = data.city;
                	if(city.indexOf("市")>0){
                		city2 = city.split("市")[0];
                	}else{
                		city2 = city;
                		city = city+"市"; 
                	}
            	}
            	allowShowAds(storageKey,indexdata);
            }else{
            	errGps()
            }
        }
        // 定位失败后调用
        function errGps(){allowShowAds(storageKey,indexdata);}
        // gps定位
        function gpsInit() {
            sogouGps.fire({
                hasAddressFunc: sucGps,
                noAddressFunc: errGps,
                done: sucGps,
                fail: errGps
            });
        }
        gpsInit();
    };
	
	if(banTypeId!=1&&indexdata&&indexdata.ads){
		//allowShowAds(storageKey,indexdata);
		initGps();
	}
	isShow=(!isShow ? isShow:true);
	})
}
(function(banType){	
	SerBanner(banType);
})('063');
