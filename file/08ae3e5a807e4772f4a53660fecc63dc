"uigs_st="+parseInt((getNowTime()-START_TIME)/1000),"uigs_cl="+uigs_encode(uigs_cl),"uigs_id="+id];if(txt){cl_pbstr.push("txt="+uigs_encode(txt))}if(firstCl){cl_pbstr.push("first_cl="+(getNowTime()-visitTime)/1000);firstCl=false}uigs_pingback(other+cl_pbstr.join("&"))};window.addEventListener("message",function(e){if(e&&e.data&&e.data.type=="pd"){var data=e.data.pd;uigs2PB(data.uigsflag+"&href="+data.dHref,(data.dTxt||"").replace(/<.*?>/g,""),data.uigsflag,data.other?data.other.join(""):"")}},false);function getOS(){var ua=navigator.userAgent;if(ua.indexOf("Windows NT 5.1")!=-1){return"Windows XP"}if(ua.indexOf("Windows NT 6.0")!=-1){return"Windows Vista"}if(ua.indexOf("Windows NT 6.1")!=-1){return"Windows 7"}if(ua.indexOf("iPhone")!=-1){return"iPhone"}if(ua.indexOf("iPad")!=-1){return"iPad"}if(ua.indexOf("Linux")!=-1){var index=ua.indexOf("Android");if(index!=-1){var index2=ua.indexOf("Build");var index1=ua.lastIndexOf(";",index2);var type=ua.slice(index1+1,index2);return type||"unknown"
}else{return"Linux"}}return"unknown"}function getScreenResolution(){var screenW=window.screen.width,screenH=window.screen.height,dpr=window.devicePixelRatio,ua=navigator.userAgent;if(/(iphone)|(ipad)/ig.test(ua)){return screenW*dpr+"*"+screenH*dpr}else{return screenW+"*"+screenH}}function getBrowserName(){var browser=["Sogousearch","MQQBrowser","SogouMobileBrowser","UCBrowser","Firefox","baidubrowser","LieBaoFast","Mb2345Browser","LeBrowser","(opera)|(OPR)","(chrome)|(CriOS)","Safari"],ua=navigator.userAgent;for(var i=0,ln=browser.length;i<ln;i++){var reg=new RegExp(browser[i],"ig");if(reg.test(ua)){if(/(opera)|(OPR)/ig.test(ua)){return"opera"}else{if(/(chrome)|(CriOS)/ig.test(ua)){return"chrome"}}return browser[i]}}return"unknown"}function getNetStatus(){var connection=navigator.connection||navigator.mozConnection||navigator.webkitConnection||null,type=connection?connection.type:"nosupport",states={};if(connection){states[connection.UNKNOWN]="unknown";states[connection.ETHERNET]="ethernet";states[connection.WIFI]="wifi";states[connection.CELL_2G]="2G";states[connection.CELL_3G]="3G";states[connection.CELL_4G]="4G";states[connection.NONE]="none";return states[type]}return"nosupport"}$d.onclick=function(evt){var ret=oldclick(evt);uigs_click(evt);uigs_para.query="";return ret};window.sogou=window.sogou||{};window.sogou.pb=window.sogou.pb||{};window.sogou.pb.pv=uigs_pv})(window);
var detailinit=(function(){
	require(['sogougps'],function(sogouGps){//统一修改定位接口
        var channels = fwdata_channel.channels,
            fwdata = [],
            city = global_city||'',
    		city2 = city.split("市")[0],
            uigs_result = false,
            _cont = $("#cont"),
            frVal = "channel_medical",
            nowTime = window.nowjs||+new Date();
      //为地址添加search字段
        function setSearchKey(url,key,val){
    		if(url){
    			var key = key=="fr"?(~url.indexOf("http")?"sogou_fr":"fr"):key;
    			var mark1 = url.indexOf("?"),
    				mark2 = url.indexOf("#"),
    				searchStr = '',
    				hashStr = '',
    				val = val||'',
    				newUrl = '';
    			if(~mark1){
    				if(~mark2 && mark2>mark1){
    					hashStr = url.substring(mark2);
    					searchStr = url.substring(mark1+1,mark2);
    				}else{
    					searchStr = url.substring(mark1+1);
    				}
    				var reg = new RegExp("(^|&)"+key+"\\s*=\\s*([^&]*)");
    				searchStr=searchStr.replace(reg,'');
    				searchStr+="&"+key+"="+val;
    				newUrl = url.substring(0,mark1+1)+searchStr+hashStr;
    			}else{
    				searchStr = "?"+key+"="+val;
    				if(~mark2){
    					hashStr = url.substring(mark2);
    					newUrl = url.substring(0,mark2)+searchStr+hashStr;
    				}else{
    					newUrl = url+searchStr;
    				}
    			}
    			if(newUrl.indexOf("?&")==mark1){
    				newUrl = newUrl.replace("?&","?");
    			}
    			return newUrl;
    		}
    		return url;
    	}
        //城市信息判断s
        var fuwudata_cache = {},isloadFuwudata=false;
        function findFuwuData(){
                if(!fuwudata||!fuwudata.data||!fuwudata.data.length){return;}
                var obj={};
                for(var i=0;i<fuwudata.data.length;i++){
                        var fwdataItem = fuwudata.data[i],
                                itemId = fwdataItem.id;
                        obj[itemId] = fwdataItem;                       
                }
                isloadFuwudata = true;
                return obj;
        }
        function citycheckById(id){
                if(id instanceof Array){
                        return allCitycheck(id);
                }
                var citys = getFuwuItemCity(id);
                if(citys&&citys.length){
                        return citys.indexOf(city)!=-1||citys.indexOf(city2)!=-1? true : false;
                }
                return true
        }
        function getFuwuItemCity(id){
                !isloadFuwudata?fuwudata_cache=findFuwuData():'';
                return fuwudata_cache[id]?fuwudata_cache[id].city:[];
        }
        function allCitycheck(relateid){
                for(var i=0;i<relateid.length;i++){
                        var citys = getFuwuItemCity(relateid[i]);
                        if(citys&&citys.length){
                                if(citys.indexOf(city)!=-1||citys.indexOf(city2)!=-1)return true;
                        }else{
                                return true;
                        }
                }
                return false;
        }
        function getCityRelateId(relateid,name,id){
                var relateIds = relateid,
                        url = '',
                        cityRelateIdData = [],
                        cityRelateIds = [];
                for(var r=0;r<relateIds.length;r++){
                        if(citycheckById(relateIds[r].id)){
                                cityRelateIdData.push({"id":relateIds[r].id,"url":relateIds[r].url})
                                cityRelateIds.push(relateIds[r].id);
                        }
                }
                if(cityRelateIds.length>1){
                        var name = name||"上门服务",
                                id = id||'';
                        return url = '/f/mserverlist?fr=channel_medical#relateid='+encodeURIComponent(cityRelateIds.join(","))+'&name='+encodeURIComponent(name)+'&id='+id;
                }else if(cityRelateIds.length==1){
                        url = cityRelateIdData[0].url;
                        return url = /^http:\/\//.test(url)?url:'/f/'+url+'?fr=channel_medical';
                }
                return null;
        }
        //城市信息判断e
        //相关服务判断s-当前城市没有该项服务时，展现其它相关服务
        function relateOtherCheck(item){
                if(item.fwid && citycheckById(item.fwid)){
                        return {"item":item};
                }
                var relates = item.relateother;
                if(relates && relates.length){
                        for(var r=0;r<relates.length;r++){
                                if(citycheckById(relates[r].fwid)){
                                        return {"item":item,"relate":relates[r]};
                                }
                        }
                }
                return null;
        }
        //相关服务判断e
        function getfwData(){
                if(channels.length){
                        for(var i=0;i<channels.length;i++){
                                if(channels[i].channel == "medical_health"){
                                        var channelData = channels[i].data;
                                        for(var i2=0;i2<channelData.length;i2++){
                                                var item = channelData[i2];
                                                if(!item.fwid&&!item.relateid&&!item.relateid.length){continue;}
                                                //if(item.fwid && !citycheckById(item.fwid)){continue;}//noteh_20160420
                                                if(item.fwid && !citycheckById(item.fwid) && !(item.relateother && item.relateother.length)){continue;}
                                                if(!item.fwid && item.relateid){
                                                        if(!item.relateid.length){continue;}
                                                        var relateids = [];
                                                        for(var r=0;r<item.relateid.length;r++){
                                                                relateids.push(item.relateid[r].id);
                                                        }
                                                        if(!citycheckById(relateids)){
                                                                continue;
                                                        }
                                                }
                                                fwdata.push(channelData[i2]);
                                        }
                                        fwdata.sort(desc_sort('order'));
                                        renderCont();
                                        //window.sogou.pb.pv("&fwnum="+fwdata.length);//展现统计
                                        if(fwdata.length){uigs_result=true}
                                        break;
                                }
                        }
                }
                window.sogou.pb.pv("&load_time="+((new Date()).getTime()-nowTime)/1000+"&result="+uigs_result);//展现统计
        }
        function desc_sort(filed){
                return function(obj1,obj2){
                        var val1 = parseInt(obj1[filed]),
                                val2 = parseInt(obj2[filed]);
                        return val1<val2?1:(val1>val2?-1:0);
                }
        }
        function renderCont(){
                var str = '',dataln=fwdata.length;
                var group={},cache;
                for(var i=0;i<dataln;i++){
                        var _item = fwdata[i],
                                id = _item.id,
                                style = _item.style,
                                name = _item.name,
                                des = _item.des,
                                promotion = _item.promotion,
                                url = _item.url,
                                classify=_item.classify,
                                itemStr = '';
                        if(_item.relateother){
                                var _otRelate = relateOtherCheck(_item);
                                if(!_otRelate){continue;}
                                if(_otRelate.relate){
                                        var _relate = _otRelate.relate;
                                        des = _relate.des;
                                        promotion = _relate.promotion;
                                        url = _relate.url;
                                }
                        }
                        url = /^http:\/\//.test(url)?url:'/f/'+url;
                        promotion = promotion?'<span class="arrico">'+promotion+'</span>':'';
//                      if(_item.relateid && _item.relateid.length){
//                              //url = 'http://fuwu.wap.sogou.com/f/channel_list.jsp?fr=channel_visit#relateid='+encodeURIComponent(_item.relateid.join(","));
//                              url = '/f/channel_list.jsp?fr=channel_visit#relateid='+encodeURIComponent(_item.relateid.join(","));
//                      }
                        if(_item.relateid && _item.relateid.length){
                                url = getCityRelateId(_item.relateid,name,id)||url;                                             
                        }
                        url = setSearchKey(url,"fr",frVal);//为地址添加来源_addh_20160506
                        cache = group[classify] ? group[classify] : (group[classify] = []);
                        itemStr = '<a href="'+url+'" class="merd-doc '+style+'" id="med_'+id+'">\
                                              <h4 class="merd-tit">'+name+promotion+'</h4>\
                                              <p class="merd-txt">'+des+'</p>\
                                            </a>';
                        cache.push(itemStr);
                }
                for(var classifyName in group){
                        str += '<div class="merdical-seation"><h3 class="merd-tith3">'+classifyName+'</h3><div class="vis-item">'+group[classifyName].join('')+'</div></div>';
                }
                _cont.html(str);
        }
        //getfwData();
        function initGps(){
            // 定位成功后调用
            function sucGps(address, position, data, locationStr){
            	if(data&&(data.status=='ok'||data.city)){
                	if(!(data.city==city || data.city==city2)){
                		city = data.city;
                    	if(city.indexOf("市")>0){
                    		city2 = city.split("市")[0];
                    	}else{
                    		city2 = city;
                    		city = city+"市"; 
                    	}
                	}
                	getfwData();
                }else{
                	errGps()
                }
            }
            // 定位失败后调用
            function errGps(){getfwData();}
            // gps定位
            function gpsInit() {
                sogouGps.fire({
                    hasAddressFunc: sucGps,
                    noAddressFunc: errGps,
                    done: sucGps,
                    fail: errGps
                });
            }
            gpsInit();
        };
    	initGps();
        //展现统计
//    	window.addEventListener("load", function () {
//    		window.sogou.pb.pv("&load_time="+((new Date()).getTime()-nowTime)/1000+"&result="+uigs_result);//展现统计
//        },false);
	})
})();
