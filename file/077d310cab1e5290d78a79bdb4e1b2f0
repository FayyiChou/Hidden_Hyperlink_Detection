$(document).ready(function(){
    var apiUrl = 'http://ku.178.com/api/kugame/get_image';
    //需要获取图片的手游game_id
    var gameids = "";
    if(gameids) {
	    $.getJSON(apiUrl+'?gameids='+gameids+'&callback=?', function(json) {
	        if(0 == json.code) {
	        	$.each(json.result, function(i,item){
		        	console.log(item.cover);
					$('img#img_' + i).attr("src", item.cover);
