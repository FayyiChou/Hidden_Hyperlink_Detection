<script type="text/javascript">
$(function(){
	if(document.all) {
		  
        $('#btn_copy').click(function(){
        	var text = $('#card_info').val();
        	if (window.clipboardData) {
        		  window.clipboardData.clearData();
        		  clipboardData.setData("Text", text);
        		  alert("复制成功！");
        	}

        }); 
   }else{
	   var text = $('#card_info').val();
		var html = '<object type="application/x-shockwave-flash" data="http://cimg.178.com/xin/v4/img/clipboard.swf" width="100%" height="100%" id="clipboard">';
			html +='<param name="wmode" value="transparent">';
			html += '<param name="allowScriptAccess" value="always">';
			html += '<param name="flashvars" id="content" value="content='+text+'">';
		    html += '</object>';
	   $('#btn_copy').html(html);
	}
});

$(document).ready(function(){
    //获取游戏信息
    var apiUrl = 'http://ku.178.com/api/game/getinfo';
    var gameId = "6096";
    if(gameId) {
	    $.getJSON(apiUrl+'?id='+gameId+'&callback=?', function(json){
	        if(json.result) {
	        	var jr = json.result; 
	        	$('.block13 .img').html('<a target="_blank" href="' + jr.url + '"><img src="' + jr.cover + '"></a>');
	        	var str = '<p>类型：'+jr['class']+ '<br> 产地：'+jr.region+'<br> 开发：'+jr.developer+ '<br> 模式：'+
	            		jr.genre+'<br> 题材：'+jr.theme+ '<br>画面：'+jr.view+ '<br> 收费: ' + jr.payment + '<br> </p>';
	           	$('.block13 .dig').html(str); 

	           	var num=$('<div class="num">'+jr.score+'</div>');
	           	ps=parseInt(jr.score);

	           	var star='';
	           	for(var i=0;i<5;i++) {
					var c=i<ps/2?' cur':'';
					star+='<span class="star'+c+'"></span>';
				}
				$('div.score').append(num);
				$('div.score').append(star);
				$('#gameinfo').show();
	        } 
	    });
	    
    } else {
    	$('#gameinfo').hide();
    }
    
});

</script>

