        // cookies are separated by semicolons
        var aCookie = document.cookie.split("; ");
        for (var i = 0; i < aCookie.length; i++) {
            // a name/value pair (a crumb) is separated by an equal sign
            var aCrumb = aCookie[i].split("=");
            if (sName == aCrumb[0])
                return decodeURIComponent(aCrumb[1]);
        }

        // a cookie with the requested name does not exist
        return null;
    }

    /**
     * 存储临时COOKIE（浏览器关闭失效）
     * @param sName
     * @param sValue
     */
    document.setCookieTemporary = function (sName, sValue) {
        var sCookie = sName + "=" + encodeURIComponent(sValue);
        sCookie += "; path=/";
        document.cookie = sCookie;
    }
    /**
     * 移除COOKIE
     * @param sName
     * @param sValue
     */
    document.removeCookie = function (sName) {
        document.cookie = sName + "=; expires=Fri, 31 Dec 1999 23:59:59 GMT;";
    }



    $(function(){
        if (document.getCookie('close_ad') == 1) {
            $('.notice_all').remove();
            $('#notice_exist').remove();
        }else{
            $('.header').prepend("<div class=\"notice_all\" ><a href='http://sale.houxue.com/huodong/chongwu.html#source=xuexiao' target='_blank'><div class=\"notice\"></div></a></div><a id=\"notice_exist\"  href=\"javascript:;\"></a>");
        }
        /**
         * 点击隐藏
         */
        $('#notice_exist').click(function(){
            $('.notice_all').remove();
            $('#notice_exist').remove();
            document.setCookieTemporary('close_ad', 1);
        })
    })


</script> 
