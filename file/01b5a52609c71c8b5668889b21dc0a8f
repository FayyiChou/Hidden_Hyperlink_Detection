        $(window).scroll(function () {
            var hgl = $('.hglbox').offset().top;
            if ($(window).scrollTop() > hgl - 350) hglbox();
        });

        function hglbox() {
            $('.hglbox .pstyle').eq(0).animate({ top: 63, opacity: 1 }, 1200);
            $('.hglbox .pstyle').eq(1).animate({ top: 423, height: 378, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(2).animate({ top: 436, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(3).animate({ top: 235, opacity: 1 }, 1000);
            $('.hglbox .pstyle').eq(4).animate({ top: 249, height: 508, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(5).animate({ top: 259, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(6).animate({ top: 0, opacity: 1 }, 1000);
            $('.hglbox .pstyle').eq(7).animate({ top: 542, height: 378, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(8).animate({ top: 553, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(9).animate({ top: 334, opacity: 1 }, 1000);
            $('.hglbox .pstyle').eq(10).animate({ top: 465, height: 378, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(11).animate({ top: 484, opacity: 1 }, 500);
            $('.hglbox .pstyle').eq(12).animate({ top: 217, opacity: 1 }, 1000);
            $('.hglbox .pstyle').eq(13).animate({ top: 606, left: 111, opacity: 1 }, 3500);
