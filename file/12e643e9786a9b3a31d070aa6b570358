		<script type="text/javascript">
		$(function () {
			//源对象为自动切换的一个数组，即其本身，非其父包装元素
			//如要切换图片本身就以图片组为对象，
			//如要切换层就以层组为对象

			//带前后按钮

			$('#change .a_bigImg').soChange({
				botPrev:'#change .a_last',//按钮，上一个
				botNext:'#change .a_next'//按钮，下一个
			});
			$("#change").css("height",($(window).width()*700/1920));
			$(".changeBox_a1").css("height",($(window).width()*700/1920));
			$(".ditu_search").css("left",($(window).width()/2-325));
			$(".eachBox").css("height",($(window).width()*700/1920));
			$(".a_last").css("top",($(window).width()*700/1920)/2);
			$(".a_next").css("top",($(window).width()*700/1920)/2);
			
			$(window).resize(function(){ 
				$(".eachBox").css("height",($(window).width()*700/1920));
				$(".a_last").css("top",($(window).width()*700/1920)/2);
				$(".a_next").css("top",($(window).width()*700/1920)/2);
				$(".ditu_search").css("left",($(window).width()/2-325));
				$("#change").css("height",($(window).width()*700/1920));
				$(".changeBox_a1").css("height",($(window).width()*700/1920));
			});
			
			$(".ditu_search").css("left",($(window).width()/2-325));
			$(window).resize(function(){
				$(".ditu_search").css("left",($(window).width()/2-325));
			});
		});
				  
		//lei动画效果
		$(".sy-leibie-list li .lei img").hover(
	 		function(){	$(this).addClass('animated flipInY');},
			function(){$(this).removeClass('flipInY');
			 $(this).addClass('animated flipInY');
			}
		);
				 
		//首页背景BANNER图js

		$('.picBackground li').soChange({
			thumbObj:'.picNav li',
			thumbOverEvent:false,// 关闭鼠标经过thumbObj切换事件，只有点击thumbObj时才切换对象
			thumbNowClass:'on', 
			changeTime:7000,//自定义切换时间为4000ms
			slideTime:1000,//平滑过渡间歇为0，意味着直接切换 
			botPrev:'#change_2 .a_last',//按钮，上一个
			botNext:'#change_2 .a_next'//按钮，下一个
		});
		$('.MshudiPic li').soChange({
			thumbObj:'.MshudiMub li', 
			thumbNowClass:'on'
		});
		$(".Mchengyuan").hover(
			function(){$(this).addClass("McyOn");},
			function(){$(this).removeClass("McyOn");}
		);
		$(".MspCon dl").hover(
			function(){$(this).addClass("on");$(this).find(".MspBnt").show();},
			function(){$(this).removeClass("on");$(this).find(".MspBnt").hide();}
		);
		$(".msgcon_1").hover(
			function(){$(this).addClass("on");},
			function(){$(this).removeClass("on");}
		);
		</script>

		<script>
		window.onload = function(){
			scrollPic();
		}
		scrollPic.onload = function ()
        {
            loading (this);
        }
		function scrollPic() {
			var scrollPic = new ScrollPic();
			scrollPic.scrollContId   = "scrollPic"; //内容容器ID
			scrollPic.arrLeftId      = "LeftArr";//左箭头ID
			scrollPic.arrRightId     = "RightArr"; //右箭头ID

			scrollPic.frameWidth     = 1000;//显示框宽度
			scrollPic.pageWidth      = 200; //翻页宽度

			scrollPic.speed          = 10; //移动速度(单位毫秒，越小越快)
			scrollPic.space          = 10; //每次移动像素(单位px，越大越快)
			scrollPic.autoPlay       = false; //自动播放
			scrollPic.autoPlayTime   = 3; //自动播放间隔时间(秒)

			scrollPic.initialize(); //初始化
		}
		</script>
