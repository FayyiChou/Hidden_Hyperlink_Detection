  <script language="JavaScript">
<!--
	if($('#artical_content #nopic').val() != 1){
		$('#artical_content img').LoadImage(true, 500, 500,'http://img1.indiancn.com/statics/images/s_nopic.gif'); 
	}
	
	var dayarray = new Array("星期日", "星期一", "星期二", "星期三", "星期四", "星期五", "星期六")
	var montharray = new Array("01", "02", "03", "04", "05", "06", "07", "08", "09", "10", "11", "12")
	function getthedate() {
		var mydate = new Date()
		var year = mydate.getYear()
		if (year < 1000) year += 1900
		var day = mydate.getDay()
		var month = mydate.getMonth()
		var daym = mydate.getDate()
		if (daym < 10) daym = "0" + daym
		var hours = mydate.getHours()
		var minutes = mydate.getMinutes()
		var seconds = mydate.getSeconds()
		var dn = "上午"
		if (hours >= 12) dn = "下午"
		if (hours > 12) {
			hours = hours - 12
		}
		if (hours == 0) hours = 12
		var dn = ""
		if (minutes <= 9) minutes = "0" + minutes
		if (seconds <= 9) seconds = "0" + seconds
		var cdate = "&nbsp;" + year + "年" + montharray[month] + "月" + daym + "日" + " " + dayarray[day] + " " + dn + " " + hours + ":" + minutes + ":" + seconds
		document.getElementById("h_getDay").innerHTML = cdate
	}
	if (!document.all) getthedate()
	function goforit() {
		if (document.all) setInterval("getthedate()", 1000)
	}
	
	goforit();
   -->
   </script> 
  <script type="text/javascript">
$(document).ready(function(){
	$("#zoomDoc14").click(function(){zoomDoc(14);});
	$("#zoomDoc16").click(function(){zoomDoc(16);});
})

function zoomDoc(size) {
	$('#zoomDoc16').removeClass();
	$('#zoomDoc14').removeClass();
	if(size == 14) {
		$('#zoomDoc16').addClass("icon font_down_disable");
		$('#zoomDoc14').addClass("icon font_up_disable");
	}else {
		$('#zoomDoc16').addClass("icon font_down");
		$('#zoomDoc14').addClass("icon font_up");
	}
	$('#artical_content').attr("style", "font-size: "+size+"px;");
	$('#artical_content').children().attr("style", "font-size: "+size+"px;");  
}

$('#guoneili').hover(function(){
	TabControl('guonei');
});
$('#yindunewsli').hover(function(){
	TabControl('yindunews');
});
$('#wenhuali').hover(function(){
	TabControl('wenhua');
});
$('#lvyouli').hover(function(){
	TabControl('lvyou');
});
function TabControl(TabId) {
	if (TabId!=null) {
		var TabIdArray=['guonei','yindunews','wenhua','lvyou'];	
		for (var i = 0; i < TabIdArray.length; i++) {
			$('#'+TabIdArray[i]+'li').removeClass();
			if (TabIdArray[i] == TabId){
				$('#'+TabIdArray[i]+'li').addClass("current"); 
				$('#'+TabIdArray[i]).attr("style", "display: block;")
			}else{
				$('#'+TabIdArray[i]).attr("style", "display: none;")
			}
		}
	}
}
</script> 
