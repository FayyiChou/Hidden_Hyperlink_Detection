<script type="text/javascript">
    function onmouseZK(num) {
        if (num == 1) {
            document.getElementById("divimg2").style.display = "";
        }
        else if (num == 2) {
            document.getElementById("divimg4").style.display = "";
        }
        else if (num == 3) {
            document.getElementById("divimg6").style.display = "";
        }
    }
    function onmouseTH(num) {
        if (num == 1) {
            document.getElementById("divimg2").style.display = "none";
        }
        else if (num == 2) {
            document.getElementById("divimg4").style.display = "none";
        }
        else if (num == 3) {
            document.getElementById("divimg6").style.display = "none";
        }
    }
</script>
