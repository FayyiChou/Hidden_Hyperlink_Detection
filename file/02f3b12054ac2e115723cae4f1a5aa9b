    jQuery(document).ready(function(){
        jQuery('#nav2 li').mouseover(function(){
            jQuery(this).addClass('active');
            //子菜单显示
            jQuery(this).children('ul').show();
        }).mouseout(function(){
            jQuery(this).removeClass('active');
            jQuery(this).children('.three_nav').hide();
           jQuery(this).children('.three_nav2').hide();
