				                <script type="text/javascript">
			$(function(){
//				loaded = true;
				offsettop = $("#roll_div").offset().top;
				ifset=false;
				$(window).scroll(Add_Data);
			});
			function Add_Data(){
				if($("#roll_div").length){
					if($('#rightbox').height()<$('#leftbox').height()){return;}
					var offset=$("#roll_div").offset().top;
					var scroll=$(window).scrollTop();
					if($("#footerdiv").offset().top<(scroll+$("#roll_div").height())){
						$("#roll_div").css('position','absolute');
						$("#roll_div").css('top',offset);
						//console.log('stop');	 $("#roll_div").removeClass("roll_div2").addClass("roll_div1");
					}else{
						$("#roll_div").css('position','');
						$("#roll_div").css('top',0);
					}
					if(!ifset&&offsettop<scroll){//当向下滚到到评论位置的时候
						  $("#roll_div").removeClass("roll_div1").addClass("roll_div2");
						  ifset=true;
					}else if(ifset&&offsettop>scroll){ 
						 $("#roll_div").removeClass("roll_div2").css('position','');
						 ifset=false;
					}
				}
			}
                </script>
            </div>
