pc.ready(function(){
tops=pc.getElem("#jTop");
var Height = "500";
window.onscroll = function(){
var scrollTop = document.documentElement.scrollTop || document.body.scrollTop; 
if(scrollTop > Height ){
pc.show(tops, 300);
}else{
pc.hide(tops, 0)
}
}
});
light=pc.getElem(".light");
pc.addEvent(light,"click",function(){
pc.getElem(".overlay").style.display="block";
light.style.display="none";
pc.getElem(".lightoff").style.display="block"
});
lightoff=pc.getElem(".lightoff");
pc.addEvent(lightoff,"click",function(){
pc.getElem(".overlay").style.display="none";
lightoff.style.display="none";
pc.getElem(".light").style.display="block"
});
window.fade1 = new pc.tab({
target: pc.getElems('#Jslider2 ul.pics-b'),
effect: 'fade',
