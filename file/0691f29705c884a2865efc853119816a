replceSrc(this.target[this.curPage], "#src1");}
});
/*del-fn*/
function _fn(o){
this.config={
obj:null,
className:"",
type:1
};
for(var c in o) {
this.config[c] = o[c]
};
if(this.config.obj==null){return;}
if(this.config.type==1){
this.change1();
}
if(this.config.type==2){
this.animate1();
}
};
_fn.prototype = {
change1: function(){
var o=this.config;
pc.each(o.obj,function(g,h){
pc.addEvent(g,"mouseenter",function(){pc.addClass(g,o.className);replceSrc(g, "#src1")});
pc.addEvent(g,"mouseleave",function(){pc.removeClass(g,o.className);})
});
},
animate1: function(){
var o=this.config;
pc.each(o.obj, function(g,h){
var covbg=pc.getElem(".popup",g),_top=g.getAttribute("data-height");
pc.addEvent(g, 'mouseenter', function(){pc.end(covbg);pc.animate(covbg, {top:o.top}, 'swing',200, function(){pc.end(covbg)});});
pc.addEvent(g, 'mouseleave', function(){pc.end(covbg);pc.animate(covbg, {top:_top+'px'}, 'swing',200, function(){pc.end(covbg)});});
})
}
}
new _fn({obj: pc.getElems(".control li"),className:"hov",type:1});
new _fn({obj: pc.getElems(".pics-b-265 li"),className:"hov",type:1});
new _fn({obj: pc.getElems(".jincaitable tr"),className:"hov",type:1});
new _fn({obj: pc.getElems("#jWeixin"),className:"weixin-hov",type:1});
meuninner2=  pc.getElem("#barrage_container2");
expand=  pc.getElem("#btn_player_expand");
if(document.getElementById("mod_player").getElementsByTagName("iframe")[0]){
document.getElementById("mod_player").getElementsByTagName("iframe")[0].style.width="1094px";
document.getElementById("mod_player").getElementsByTagName("iframe")[0].style.height="599px";
}    
pc.addEvent(expand, 'click', function(){
pc.end(meuninner2);
if(barrage_container2.style.width=="300px")
{
pc.animate(meuninner2, {width:"14px"}, 'swing',200, function(){pc.end(meuninner2)});
if(document.getElementById("mod_player").getElementsByTagName("iframe")[0]){
document.getElementById("mod_player").getElementsByTagName("iframe")[0].style.width="1094px";
}        
pc.animate(pc.getElem("#mod_player"), {width:"1094px"}, 'swing',200, function(){pc.end(pc.getElem("#mod_player"))});
pc.getElem("#btn_inner").innerText="我要评论";
pc.removeClass(pc.getElem("#mod_barrage"),"open");
}
else{
//pc.animate(meuninner, {width:"290px"}, 'swing',200, function(){pc.end(meuninner)});
pc.animate(meuninner2, {width:"300px"}, 'swing',200, function(){pc.end(meuninner2)});
if(mod_player){
pc.animate(mod_player, {width:"808px"}, 'swing',200, function(){pc.end(mod_player)});
}
if(document.getElementById("mod_player").getElementsByTagName("iframe")[0]){
document.getElementById("mod_player").getElementsByTagName("iframe")[0].style.width="808px";
}
pc.animate(pc.getElem("#mod_player"), {width:"808px"}, 'swing',200, function(){pc.end(pc.getElem("#mod_player"))});
pc.getElem("#btn_inner").innerText="专心看直播";
pc.addClass(pc.getElem("#mod_barrage"),"open");
}
});
