<script type="text/javascript">
            function ShowDate() {
                var day = "";
                var month = "";
                var ampm = "";
                var ampmhour = "";
                var myweekday = "";
                var year = "";
                mydate = new Date();
                ampmhour = mydate.getHours()
                myweekday = mydate.getDay();
                mymonth = mydate.getMonth() + 1;
                myday = mydate.getDate();
                myyear = mydate.getYear();
                year = (myyear > 200) ? myyear : 1900 + myyear;
                if (myweekday == 0)
                    weekday = " 星期日 ";
                else if (myweekday == 1)
                    weekday = " 星期一 ";
                else if (myweekday == 2)
                    weekday = " 星期二 ";
                else if (myweekday == 3)
                    weekday = " 星期三 ";
                else if (myweekday == 4)
                    weekday = " 星期四 ";
                else if (myweekday == 5)
                    weekday = " 星期五 ";
                else if (myweekday == 6)
                    weekday = " 星期六 ";

                if (ampmhour < 6) { ampm = "凌晨好！"; }
                else if (ampmhour < 9) { ampm = "早上好！"; }
                else if (ampmhour < 12) { ampm = "上午好！"; }
                else if (ampmhour < 14) { ampm = "中午好！"; }
                else if (ampmhour < 17) { ampm = "下午好！"; }
                else if (ampmhour < 19) { ampm = "傍晚好！"; }
                else if (ampmhour < 22) { ampm = "晚上好！"; }
                else { ampm = "夜里好！"; }
                document.write(ampm + "  今天是 " + year + "年" + mymonth + "月" + myday + "日  " + weekday);
            }

            function addBookmark() {
                var title = "江西鄱阳湖国家级自然保护区管理局", url = "http://www.poyanglake.org";
                try {
                    if (window.external) {
                        window.external.AddFavorite(url, title);
                    } else if (window.sidebar) {
                        window.sidebar.addPanel(title, url, "");
                    } else {
                        alert("此浏览器不支持自动添加收藏，请用Ctrl + D手动收藏。");
                    }
                } catch (e) {
                    alert("此浏览器不支持自动添加收藏，请用Ctrl + D手动收藏。");
                }
            }
</script>
