<script language=javascript>
    var i = 10;
    var timer = window.setInterval(timerFun, 1000);
    function showAd(){
        $("#top_ads").slideDown("slow");
        $("#jzcbbox").hide();
        i = 10;
        timer= window.setInterval(timerFun, 1000);
    }

    setTimeout(function () {
        $("#top_ads").slideDown("slow");
    }, 2000);

        function timerFun() {
            if (i == 0) {
                setTimeout($("#top_ads").slideUp("slow"), 10000);
                $("#jzcbbox").show();
                $("#timer_str").text();
                clearInterval(timer);
            }
            $("#timer_str").text(i + "秒之后自动关闭");
            i--;
        }

        function closeAd() {
            $("#top_ads").slideUp("slow");
            $("#jzcbbox").show();
            $("#timer_str").text("");
            clearInterval(timer);
        }
</script>
