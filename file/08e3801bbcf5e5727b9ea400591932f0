    <script type="text/javascript">
        //默认图片替换方法 需要早于列表加载
        function defaultImg(obj, src) {
            if (!obj.getAttribute("data-loaded")) {
                var src = src || "http://pic.c-ctrip.com/group/nopic_420280.png";
                obj.setAttribute("data-loaded", true);
                obj.src = src;
            };
        };
    </script>

