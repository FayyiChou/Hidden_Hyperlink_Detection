<script>
    function visUcsList() {
        document.getElementById("divMask").style.display = "";
        
        if (document.getElementById("ucsv3spanList").style.display != "none") {
            document.getElementById("ucsv3spanList").style.display = "none";
        } else {
            document.getElementById("ucsv3spanList").style.display = "";
        }
    }
    function setUcsHidden(id, val) {
        document.getElementById("divMask").style.display = "none";
        document.getElementById("ucsv3spanList").style.display = "none";
        document.getElementById("groupItemType").value = id;
        document.getElementById("ucsv3span").innerHTML = val+"<i></i>";
    }
     function initPage() {
         document.getElementById("divMask").style.display = "none";
         if (document.getElementById("pageNameEnum").value == "25" || document.getElementById("pageNameEnum").value == "19") {
            document.getElementById("Group_Search_Box").style.display = "none";
            document.getElementById("ucsv3span").style.display = "";
            addClass(document.getElementById("divGroupSearch"), "search-input-tarea");
        }else if (document.getElementById("pageNameEnum").value == "17") {
            document.getElementById("Group_Search_Box").style.display = "";
            document.getElementById("ucsv3span").style.display = "";
            addClass(document.getElementById("divGroupSearch"), "search-input-tarea");
        }else {
            document.getElementById("Group_Search_Box").style.display = "";
            removeClass(document.getElementById("divGroupSearch"), "search-input-tarea");
            document.getElementById("ucsv3span").style.display = "none";
        }
    }


    function hasClass(obj, cls) {  
        return obj.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)'));  
    }  
  
    function addClass(obj, cls) {  
        if (!this.hasClass(obj, cls)) obj.className += " " + cls;  
    }  
  
    function removeClass(obj, cls) {  
        if (hasClass(obj, cls)) {  
            var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)');  
            obj.className = obj.className.replace(reg, ' ');  
        }
    }
    function unMask(obj) {
        if (obj == undefined || obj == "") {
            document.getElementById("divMask").style.display = "none";
        } else {
            obj.style.display = "none";
        }

        document.getElementById("ucsv3spanList").style.display = "none";
    }

    window.urlCurrentHost = "tuan.ctrip.com";

    initPage();
</script>
