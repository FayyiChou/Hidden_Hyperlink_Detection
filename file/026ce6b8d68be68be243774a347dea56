    var pre = '/1/1025/157841.html';
    var nex = '/1/1025/161187.html';

    var currentpos, timer;
    function initialize() {
        timer = setInterval("scrollwindow()", 20);
    }
    function sc() {
        clearInterval(timer);
    }
    function scrollwindow() {
        window.scrollBy(0, 1);
    }

    $(function () {
        document.onmousedown = sc;
        document.ondblclick = initialize;

  
        $(window).keydown(function (e) {
            var c = e.keyCode;
            if (c == 37) {
                location.href = pre;
            } else if (c == 39) {
                location.href = nex;
            }
        });
    });
</script>
