		<link type="text/css" href="css/suspension/suspension.css" rel="stylesheet">
		<script type="text/javascript">
			function b() {
				h = $(window).height();
				t = $(document).scrollTop();
				w = $(window).width();
				if(w > 1250 )
				{
				  	$('#download_phone').show();
				}
				else{
				  $('#download_phone').hide();
				}
			}
			$(document).ready(function(e) {
				b();
				$('#gotop').click(function() {
					$(document).scrollTop(0);
				});
				$('#xfplay_info').hover(function() {
					$(this).attr('id', 'xfplay_info_hover');
					$('#xfplay_info_img').show();
				}, function() {
					$(this).attr('id', 'xfplay_info');
					$('#xfplay_info_img').hide();
				});
				$('#code').hover(function() {
					$(this).attr('id', 'code_hover');
					$('#code_img').show();
				}, function() {
					$(this).attr('id', 'code');
					$('#code_img').hide();
				});
				$('#download_phone').hover(function() {
					$(this).attr('id', 'download_phone_hover');
					$('#download_phone_img').show();
				}, function() {
					$(this).attr('id', 'download_phone');
					$('#download_phone_img').hide();
				});
			});
			$(window).scroll(function(e) {
				b();
			});
			window.onresize = function(){
                b();
            }
		</script>
		      
