	<script>
	//图片加载失败时替换Url
    function picNotFind(img) {
        img.src = img.src.replace("/400_", "/500_");
        img.onerror = null;
    }
    //当头像图片加载失败时，调用默认头像替换
    function headPicNotFind(img) {
        img.src = img.src.replace(img.src, "//x.autoimg.cn/app/image/default_userpic.png");
        img.onerror = null;
    }
	//设置回复数
    function setReplyValue(rsp) {
        var item = eval(rsp);

        var replys = item.Replys ? parseInt(item.Replys) : 0;
        var views = item.Views ? parseInt(item.Views) : 0;
        document.getElementById("forum-info-reply").innerHTML = replys;
        document.getElementById("forum-info-view").innerHTML = views;
    }
	//格式化字符串
    String.prototype.format = function (args) {
        var result = this;
        if (arguments.length > 0) {
            if (arguments.length == 1 && typeof (args) == "object") {
                for (var key in args) {
                    if (args[key] != undefined) {
                        var reg = new RegExp("({" + key + "})", "g");
                        result = result.replace(reg, args[key]);
                    }
                }
            }
            else {
                for (var i = 0; i < arguments.length; i++) {
                    if (arguments[i] != undefined) {
                        var reg = new RegExp("({[" + i + "]})", "g");
                        result = result.replace(reg, arguments[i]);
                    }
                }
            }
        }
        return result;
    };
    
    function launchFullScreen(element) {
        if(element.requestFullScreen) {
            element.requestFullScreen();
        } else if(element.mozRequestFullScreen) {
            element.mozRequestFullScreen();
        } else if(element.webkitRequestFullScreen) {
            element.webkitRequestFullScreen();
        }
    }

    function LoadSelfVideo(browser,ispc,screenWidth) {
       
        //自家视频处理
        (function () {                        
            var videos = $("div[name='clubvideo']");
            if (videos.length > 0) {
                var maxwidth = 480;//294;
                var maxheight = 480; //220;
                if (!ispc && !browser.versions.ios) {
                    videos.each(function(i) {
                        var v = jQuery(this), pic = v.attr("v_pic"), pk = v.attr("v_pk"), src = v.attr("v_src"), videotype = "video/mp4",v_width = v.attr("v_w"), v_height =v.attr("v_h");  
                        if(pic=="")
                            pic = "//x.autoimg.cn/club/v1Content/player/v1/background.png";
                        screenWidth =320;                   
                        //android 使用视频宽高设置封面最大宽高值                    
                        maxwidth = v_width > screenWidth ? screenWidth - 20 : v_width;
                        maxheight = maxwidth/ v_width * v_height;
 
                        v.css({ 'width': maxwidth + 'px',  'margin': '15px auto' });//'height': maxheight + 'px',
                        src = src.replace(/(?:\.wm)?\.mp4/ig, ".mp4");                                               
                        var s = "<div style=\"width:"+maxwidth+"px;height:"+maxheight+"px;margin:0 auto;margin-bottom:20px;position: relative;background:url("+pic+");background-size:100% auto;\"><video id =\"video_" + pk + "\" width=\"" + maxwidth + "\" height=\"" + maxheight + "\" src=\"" + src + "\" type=\"" + videotype + "\" controls=\"controls\" poster=\"" + pic + "\" style=\"background-color: #000;\" ,preload:\"none\">    </video></div>";                            
                        v.append(s);
                         
                        //当设备为mi2时给出提示
                        if (navigator.userAgent.indexOf('MI 2') > -1) {
                            v.append('<p class=\"tip\" style=\"font-size: 10px;color: red !important;text-align: center;\">提示:如不能播放，请点击全屏观看</p>');
                        }
                        //当设备是红米给出提示
                        else if (navigator.userAgent.indexOf('Xiaomi-2013022__weibo') > -1) {
                            v.append('<p class=\"tip\" style=\"font-size: 10px;color: red !important;text-align: center;\">提示:为了视频流畅播放，建议在小屏模式下观看</p>');
                        }
                    });
                } else {
                    $.getScript("//x.autoimg.cn/club/v1Content/player/v1/mediaelement-and-player.min.js?10", function() {

                        //ios直接调用video签，android使用A标签实现                                    
                        var css = $("<link>", { "rel": "stylesheet", "type": "text/css", "href": "//x.autoimg.cn/club/v1Content/player/v1/mediaelementplayer.css?10" })[0];
                        document.getElementsByTagName("head")[0].appendChild(css);
                        //小视频灾备开关
                        var videoButton = false;
                        
                        videos.each(function(i) {
                            var v = jQuery(this), pic = v.attr("v_pic"), pk = v.attr("v_pk"), src = v.attr("v_src"), videotype = "video/mp4",v_width = v.attr("v_w"), v_height =v.attr("v_h"); ;
                            if (ispc) {
                                if (src.indexOf('.wm') == -1)
                                    src = src.replace(/(?:\.wm)?\.mp4/ig, ".wm.mp4");

                            } else {
                                if (browser.versions.ios) {
                                    src = src.replace(/(?:\.wm)?\.mp4/ig, ".wm.m3u8");
                                    videotype = "video/m3u8";
                                } else {
                                    src = src.replace(/(?:\.wm)?\.mp4/ig, ".mp4");
                                    videotype = "video/mp4";
                                }
                            }
                            if(videoButton && src !='')
                                src = src.replace('http://v.autoimg.cn/', 'http://v9.autoimg.cn/');
                            if(pic=="")
                                pic = "//x.autoimg.cn/club/v1Content/player/v1/background.png";
                            
                            //pc端直接显示480X480,iOS根据屏幕宽度适配
                            if (!ispc) {
                                maxwidth = v_width > screenWidth ? screenWidth - 20 : v_width;
                                maxheight = maxwidth / v_width * v_height;
                            }

                            v.css({ 'width': maxwidth + 'px', 'height': maxheight + 'px', 'margin': '15px auto' });
                            var s = jQuery("<video />", { width: maxwidth, height: maxheight, id: pk, src: src, poster: pic, type: videotype, controls: "controls", style: "background-color:#000000;width:" + maxwidth + "px;height:" + maxheight + "px"  ,preload:"none"});
                            v.append(s);
                            s.mediaelementplayer({
                                defaultVideoWidth: maxwidth,
                                defaultVideoHeight: maxheight,
                                videoWidth: maxwidth,
                                videoHeight: maxheight,
                                alwaysShowControls: false,
                                iPadUseNativeControls: true,
                                iPhoneUseNativeControls: true,
                                AndroidUseNativeControls: true,
                                success: function(player, node) {
                                    //播放
                                    player.addEventListener('play', function(e) {
                                        jQuery(this).data("mediaelementplayer").enterFullScreen();
                                    }, false);
                                    //异常
                                    player.addEventListener('error', function(e) {
                                        if (e.target && e.target.error.code == 4 && !$(this).data('ErrHandled')) {
                                            e.target.setSrc(e.target.src.replace(/(?:\.wm)?(\.(?:mp4|m3u8))/ig, '$1'));
                                            e.target.play();
                                            $(this).data('ErrHandled', '1');
                                        }
                                    }, false);
                                    //播放完毕
                                    player.addEventListener('ended', function(e) {
                                        $("#mejs-overlay mejs-layer").css("display", "none");
                                    }, false);
                                    //连接意外断开
                                    player.addEventListener('waiting', function(e) {
                                        //jQuery(this).data("mediaelementplayer").pauseVideo();                                                                
                                        $("#mejs-overlay mejs-layer").css("display", "none");
                                    }, false);

                                }
                            });
                        });
                    });
                }
            }
        })();              
    }
	 $(document).ready(function () {
	        //图片懒加载
	        var murl = "//x.autoimg.cn/app/image/tmbg.png";
	        $("img").lazyload({
	            placeholder: murl,
	            //effect      : "fadeIn",
	            threshold: 200
	        });
	        //关闭下面下载图层
	        $("#close-app").click(function() {            
	            $(".open-app-foot").hide();
	        });
	    });

	(function(win, browser) {
        if (win.screen === undefined || win.screen.width < 810) {
            if (browser.versions.iPad  || browser.versions.webKit  || browser.versions.mobile  || browser.versions.ios  || browser.versions.iPhone  || browser.versions.ucweb  || browser.versions.ucSpecial ) {
                //移动端请求           
                //处理图片
                dealImg(window.innerWidth, '700,600,450');
                //处理android和iOS的下载链接
                var ischeckbrower = (browser.versions.safari && browser.versions.ios) || browser.versions.chrome;
                dealDownLoad('club',ischeckbrower, browser.versions.weixinweibo, false, browser.versions.ios, 1, 4, 61301410, '【杰德 老款 改 新款 全LED大灯】广东首例，第一台！', 3104, 'c');
                //处理文章跳转链接,将pc端地址转换为m端地址
                dealArtileLink(4);
                dealVideoImg(window.innerWidth, false);
            } else {
                //非移动端请求
                $(".picbox").removeClass("picbox").addClass("picboxpc");
                dealDownLoad('club',false, browser.versions.weixinweibo, true, false, 1, 4, 61301410, '【杰德 老款 改 新款 全LED大灯】广东首例，第一台！', 3104, 'c');
                dealVideoImg(window.innerWidth, true);                   

            }
        } else {
            //当屏幕宽度<=1080
            var ispc = win.screen.width > 1080;
            if (ispc) {
                $(".picbox").removeClass("picbox").addClass("picboxpc");		                    
                    
            } else {		
                        
                //处理文章跳转链接,将pc端地址转换为m端地址
                dealArtileLink(4);                            
            }
                
            dealDownLoad('club',false, browser.versions.weixinweibo, ispc, false, 1, 4, 61301410, '【杰德 老款 改 新款 全LED大灯】广东首例，第一台！', 3104, 'c');                 
            dealVideoImg(window.innerWidth, ispc);             
        }
        LoadSelfVideo(browser,ispc,win.screen.width);
    })(this, browser);
	</script>
