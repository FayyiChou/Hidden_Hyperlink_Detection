    <div id="myFocus-wrap">
    <div id="myFocus" style="visibility:hidden"><!--焦点图盒子-->
      <div class="loading"><span>请稍候...</span></div><!--载入画面-->
      <ul class="pic"><!--内容列表-->
	  
	    
	  
