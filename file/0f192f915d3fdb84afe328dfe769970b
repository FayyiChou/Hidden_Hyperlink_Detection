  <script type="text/javascript">
var mf=myFocus;//简称
var html=mf.$('myFocus-wrap').innerHTML;//保存原html
function getParam(){//获取全部参数
	return {
		id:'myFocus',
		pattern:'mF_classicHC',  //切换样式mF_tbhuabao,mF_fscreen_tb,mF_51xflash,mF_expo2010,mF_classicHC
		time:3, //切换间隔，单位秒
		trigger:'click', //触发切换模式，click,mouseover
		width:960/1, //宽
		height:230/1,//高
		txtHeight:'default',
		auto:1=='1'?true:false, //自动播放
		autoZoom:1=='1'?true:false, //是否缩小居中图片以适应焦点图大小
		wrap:0=='1'?true:false, //显示自带边框
		index:0/1, //从第一张开始显示
		delay:100/1, //延迟100秒，当为悬停模式时
		//额外参数
		duration:600/1, //运动速度600中,400快,1000慢
		direction:'left', //运动方向,left right top  bottom
		easing:'easeOut', //运动形式:easeOut>快出慢入,easeIn>慢出快入,easeInOut>慢出慢入,swing>摆钟缓动,linear>匀速运动
		less:1/1, //是否无缝,1是,0否
		chip:8/1,//图切片数量(能被图高整除才有效),8,10 16片
		type:4/1, //切片效果:4随机效果1甩头2甩尾3凌乱
		pad:62/1, //图片留边宽度:62中,82宽,32窄,
		txtWidth:68/1,//文字段宽度68默认，38短，108长
		gray:0/1, //非当前图片是否变灰,0否1是
		direct:'one', //切出方向:random随机,one单向(向右)
		turn:'random' //翻牌方向:random随机(单向),left,right,up,down
	};
}
function reHTML(){//还原
	mf.$('myFocus-wrap').innerHTML=html;
	var css=mf.$$('style')[0];
	css.parentNode.removeChild(css);
};
function toggleDisplay(o){//高级设置切换
	if(o.innerHTML=='高级参数设置&gt;&gt;'){mf.$('adv').style.display='block';o.innerHTML='隐藏高级参数设置&gt;&gt;'}
	else{mf.$('adv').style.display='none';o.innerHTML='高级参数设置&gt;&gt;';}
}
function displayParam(){//额外参数显示
	var s=mf.$('pattern');
	var st=mf.pattern[s.value]['cfg'];
	var ps=mf.$$('p','oth'),len=ps.length;
	for(var i=0;i<len;i++) ps[i].style.display='';
	if(st) for(var p in st) mf.$('p-'+p).style.display=mf.$('custom').style.display='inline';
}
function apply(){//应用
	reHTML();//还原
	var p=getParam();//获取参数
	p.waiting=false;//不等待图片加载
	myFocus.set(p,true,function(){
		displayParam();//显示额外参数
	});
}
//开始设置
myFocus.set(getParam(),true,function(){
	displayParam();//显示额外参数
});
//屏蔽IE执行误差
window.onerror=function(){return true};
</script>
</div>
