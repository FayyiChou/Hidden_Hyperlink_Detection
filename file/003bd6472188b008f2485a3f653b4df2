				var speed=35;
				Content2.innerHTML=Content1.innerHTML;
				function Marquee()
				{
					if(pic.scrollLeft>=Content1.scrollWidth)
					{pic.scrollLeft=0;
					}
					else
					{pic.scrollLeft++;
					}
				}
				var Mar=setInterval(Marquee,speed)
				pic.onmouseover=function()
				{clearInterval(Mar)
				}
				pic.onmouseout=function()
				{Mar=setInterval(Marquee,speed)
				}
