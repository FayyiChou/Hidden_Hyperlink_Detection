function tab(objnav,objcon,objpar){
	objnav.hover(function(){
		var _index = $(this).index();
		$(this).addClass("current").siblings().removeClass("current");
		$(this).parents(objpar).find(objcon).eq(_index).show().siblings().hide();
	});	
}
