								jQuery('#logonavcon .menu-item-has-children').on("touchstart", function (e) {
								    "use strict"; //satisfy the code inspectors
								    var link = jQuery(this); //preselect the link
								    if (link.hasClass('hover')) {
								        return true;
								    } else {
								        link.addClass("hover");
								        jQuery('#logonavcon .menu-item-has-children').not(this).removeClass("hover");
								        e.preventDefault();
								        return false; //extra, and to make sure the function has consistent return points
								    }
