<script type="text/javascript">
	$(function(){
		var count = $("#mydiv").find("ul").find("li").length;
		var commonInfo = $("#mydiv").find("ul").find("li").find("#disImg");
		for(var i = 0;i < count;i++){
			var disLength = strlen(commonInfo.eq(i).attr("alt"));
			if(disLength > 9){
				var str = $.trim(commonInfo.eq(i).attr("alt")).substring(0,9);
				commonInfo.eq(i).attr("alt",str);
			}
		}
	})
// 区别中英文字占用的字节
function strlen(str) {
	var str = $.trim(str);
	var len = 0;
	for ( var i = 0; i < str.length; i++) {
		len += str.charCodeAt(i) > 0 && str.charCodeAt(i) < 255 ? 0.5 : 1;
	}
	return len;
}
