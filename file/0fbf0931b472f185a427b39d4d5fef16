<script src="/web/resources/js/footer.js" type="text/javascript"></script><script type="text/javascript">
	function setMainHeight(h){
		var con_h = $(".content-index"),
		main_h = con_h.find(".main").height(),
		side_h = con_h.find(".side").height();
		if(h)side_h-=h;
		con_h.height(Math.max(main_h,side_h));

	} 
setMainHeight();
</script>
