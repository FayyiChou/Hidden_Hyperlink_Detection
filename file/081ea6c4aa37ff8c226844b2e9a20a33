<script type="text/javascript">
    var topictitle = '手拿菜刀砍电线 一路火花带闪电';
    var maxpage = parseInt('14');
    var pageid = parseInt('13');
    if (maxpage > 1) {
        document.onkeyup = function (e) {
            e = e ? e : window.event;
            var tagname = is_ie ? e.srcElement.tagName : e.target.tagName;
            if (tagname == 'INPUT' || tagname == 'TEXTAREA') return;
            actualCode = e.keyCode ? e.keyCode : e.charCode;
            if (pageid < maxpage && actualCode == 39) {
                window.location = 'showtopic-1198478-14.html';
            }
            if (pageid > 1 && actualCode == 37) {
                window.location = 'showtopic-1198478-12.html';
            }
        }
    }
</script>

