<script>
function mycheck(){
	var login=get_login();
	if(login==1){
		alert("请先到会员页面登录或注册!");
		document.form1.action="/mini/user/check/check.php";
		document.form1.submit();
	}else{
		alert("请先到店主页面登录或注册!");
		document.form1.action="/mini/shopstation/manager/login.php";
		document.form1.submit();
	}
}

function get_login(){
	var arr=document.getElementsByName("login");
	for(var i=0;i<arr.length;i++){
		if(arr[i].checked)return arr[i].value;
	}
}

</script>
