function g(o){return document.getElementById(o);}
function HoverLi(n){
//如果有N个标?就将i<=N;
//本功能非常OK,兼容IE7,FF,IE6
	for(var i=1;i<=6;i++)
	{
		g('tb_'+i).className='no';
		g('tbox_0'+i).style.display='none';
	}
		g('tb_'+n).className='on';
		g('tbox_0'+n).style.display='block';
}
function AHoverLi(n){
	for(var i=1;i<=6;i++)
	{		
		g('atb_'+i).className='no';
		g('atbox_0'+i).style.display='none';
	}
	g('atb_'+n).className='on';
	g('atbox_0'+n).style.display='block';	
}
