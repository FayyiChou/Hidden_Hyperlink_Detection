            <SCRIPT>
var Picspeedd=15
div2.innerHTML=div1.innerHTML
function Marqueee(){
	if(div2.offsetWidth-div.scrollLeft<=0)
		div.scrollLeft-=div1.offsetWidth
	else{
		div.scrollLeft++
	}
}
var MyMarr=setInterval(Marqueee,Picspeedd)
div.onmouseover=function() {clearInterval(MyMarr)}
div.onmouseout=function() {MyMarr=setInterval(Marqueee,Picspeedd)}
