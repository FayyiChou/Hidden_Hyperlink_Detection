<script>

//第1个滚动框
var box=document.getElementById("newlist");
var box1=document.getElementById("newlist1");
var box2=document.getElementById("newlist2");
box2.innerHTML=box1.innerHTML;
function marluwei_1()
{
	if($(box1).children('li').length>5){
		if(box2.offsetTop<=box.scrollTop){
			box.scrollTop-=box1.offsetHeight;
		}
		else{
			box.scrollTop++;
		}
	}
}

var timerluwei_1=setInterval(marluwei_1,50);



</script>

