<script type="text/javascript" language="JavaScript">
totop("rollnewsup","rollnewsup1","rollnewsup2",30,"hwcms001")
function $(id){
return document.getElementById(id)
}
function totop(rollnewsup,rollnewsup1,rollnewsup2,speed,flag){
rollnewsup=$(rollnewsup);
rollnewsup1=$(rollnewsup1);
rollnewsup2=$(rollnewsup2);
rollnewsup2.innerHTML=rollnewsup1.innerHTML;
function Marquee(){
if(rollnewsup2.offsetTop-rollnewsup.scrollTop<=0)
rollnewsup.scrollTop-=rollnewsup1.offsetHeight;
else
rollnewsup.scrollTop++;
}
var flag=setInterval(Marquee,speed);
rollnewsup.onmouseover=function (){clearInterval(flag);}
rollnewsup.onmouseout=function (){flag=setInterval(Marquee,speed);}
}
</script>
