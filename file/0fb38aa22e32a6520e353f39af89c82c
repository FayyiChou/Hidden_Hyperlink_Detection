<script>
    var videos = {};

    function getRandCat(items, total) {
        var returnArr = [];
        var arrLength = items.length;
        var indexArr = [];
        var i = 0;
        while (i < total) {
            var index = parseInt(Math.random() * arrLength);
            if ($.inArray(index, indexArr)) {
                i++;
                returnArr.push(items[index]);
            }
        }
        return returnArr;
    }
    function changeTitle() {
        var obj = $("#video-tabs .active");
        var tag = obj.data("tag");
        var cats = getRandCat(videos.source[tag].cats, 9);
        $.each(obj.find(".color-title"), function (k, v) {
            var url = "http://video.php100.com/index.php?tag=" + tag + "&cat_id=" + cats[k].cat_id;
            var html = '<a href="' + url + '" target="_blank">' + cats[k].name.substring(0, 10) + '</a>';
            $(this).html(html);
        })
    }
    $(function () {
        $.get("video.json", {}, function (res) {
            videos.source = res;
            $.each($("#video-tabs .tab-pane"), function (k, v) {
                var tag = $(this).data("tag");
                var cats = getRandCat(videos.source[tag].cats, 9);
                var videoList = getRandCat(videos.source[tag].videos, 3);
                var html = "";
                $.each(videoList, function (k, v) {

                    var url = "http://video.php100.com/detail.php?tag=" + tag + "&video_id=" + v.video_id;
                    html += '<div class="col-md-4">' +
                            '    <div class="video-list-box">' +
                            '        <a href="' + url + '" target="_blank"><img src="' + v.img + '"></a>' +
                            '        <div class="caption">' +
                            '        <a href="' + url + '" target="_blank">' + v.title.substring(0, 20) + '</a>' +
                            '       </div>' +
                            '    </div>' +
                            '</div>'

                })
                $(this).find(".tab-video-list").html(html);
                $.each($(this).find(".color-title"), function (k, v) {
                    var url = "http://video.php100.com/index.php?tag=" + tag + "&cat_id=" + cats[k].cat_id;
                    var html = '<a href="' + url + '" target="_blank">' + cats[k].name.substring(0, 10) + '</a>';
                    $(this).html(html);
                })
            })
        }, "json")

    });
</script>
