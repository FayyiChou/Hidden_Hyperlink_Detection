<script type="text/javascript">
//$.ajax({type:"GET",url:"s.js",dataType:"script"});//加载并执行一个 JS 文件
$(document).ready(function(){
	//your code here
	var cur=0;
	$("#menu ul li").eq(cur).addClass("current"); //为第一个添加当前效果样式
	$("#menu > ul > li > a").hover(function(e){
		if (e.target == this) {
			$(this).parent().addClass("hover");
		}
	},function(e){
		$(this).parent().removeClass("hover");
	});

	$(".product-index > ul > li > a").hover(function(e){
		var pid = ($(this).parent()).attr("id").replace('c_','');
		div = '#c_' + pid;
		sdiv = '#cs_' + pid;
		var x = $('#product-index-1').offset().left;
		var y = $(div).offset().top;
		$(sdiv).css('left', x + 'px');
		$(sdiv).css('top',y + 18 + 'px');
		$(sdiv).show();
	},function(e){
		var pid = ($(this).parent()).attr("id").replace('c_','');
		var sdiv = '#cs_' + pid;
		$(sdiv).hide();
	});

	$(".subCatalog").hover(function(e){
		var pid = $(this).attr("id").replace('cs_','');
		div = '#c_' + pid;
		sdiv = '#cs_' + pid;
		var x = $('#product-index-1').offset().left;
		var y = $(div).offset().top;
		$(sdiv).css('left', x + 'px');
		$(sdiv).css('top',y + 18 + 'px');
		$(sdiv).show();
	},function(e){
		var pid = $(this).attr("id").replace('cs_','');
		var sdiv = '#cs_' + pid;
		$(sdiv).hide();
	});
});
</script>