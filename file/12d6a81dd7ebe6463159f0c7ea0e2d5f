//设置为首页js
function setHomepage(obj){
	try{
		obj.style.behavior='url(#default#homepage)';
		obj.setHomePage(window.location);
	}catch(e){
		if(window.netscape) {
			try {
				netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
			}catch (e) {
				alert("此操作被浏览器拒绝！\n请在浏览器地址栏输入“about:config”并回车\n然后将 [signed.applets.codebase_principal_support]的值设置为'true',双击即可。");
			}
			var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
			prefs.setCharPref('browser.startup.homepage',window.location);
		 }
	}
}


//收藏与回到顶部的js效果
$(function(){
	$(".goTop").hide(); //打开页面隐藏
	setInterval("goTop()",100);
})
function goTop(){
	if(getScrollTop()==0){
		$(".goTop").fadeOut(1000);
	}else{
		$(".goTop").fadeIn(300);
	}
}
function getScrollTop(){//自定义滚动条到顶部的函数
   var scrollTop=0;
   if(document.documentElement&&document.documentElement.scrollTop){
   		scrollTop=document.documentElement.scrollTop;
   }else if(document.body&&document.body.scrollTop){
   		scrollTop=document.body.scrollTop;
   }
   return scrollTop;
}
$(function(){
	$(".content05_right  #line_news li:eq(2)").css("margin-right","0px");
	$("#video_list li:even").css("margin-right","36px");
	$("#links .link_add span:first").css("margin-bottom","14px");
	$("#banner li:last-child").css("background","none");
})
</script>

