<script type="text/javascript">
	// ^^^^^^^^^J^^
	$(window).load(function(){
		var $win = $(window),
			$ad = $('#abgne_float_ad').css('opacity', 0).show(),	// ^^^s^i^϶^^ܳz^^^B^^ܥX^^
			_width = $ad.width(),
			_height = $ad.height(),
			_diffY = 20, _diffX = 20,	// ^Z^^^k^ΤW^^^^Z
			_moveSpeed = 800;	// ^^^ʪ^^t^^
		
		// ^^^^ #abgne_float_ad ^^^ʨ^w^I
		$ad.css({
			top: _diffY,	// ^^^W
			left: $win.width() - _width - _diffX,
			opacity: 1
		});
		
		// ^^^^^^^[^W scroll ^^ resize ^ƥ^
		$win.bind('scroll resize', function(){
			var $this = $(this);
			
			// ^^^^ #abgne_float_ad ^^^^^^
			$ad.stop().animate({
				top: $this.scrollTop() + _diffY,	// ^^^W
				left: $this.scrollLeft() + $this.width() - _width - _diffX
			}, _moveSpeed);
		}).scroll();	// Ĳ^o^@^^ scroll()
		
		// ^^^^^s^i
		$('#abgne_float_ad .abgne_close_ad').click(function(){
			$ad.hide();
		});
	});
</script>

<div id="abgne_float_ad">
