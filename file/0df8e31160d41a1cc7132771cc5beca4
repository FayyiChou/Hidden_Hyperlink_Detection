	<script type="text/html" id="item_tmpl">
		
			<li>
				<@if(view_tag1 == '1'){@><div class="viewTag1"></div><@}@>
			    <div class="pic"><a href="/toTbItem.htm?id=<@:=item_id@>&page=m_tm" ><img class="lazyimg" name="product_img" src="http://egouimg1.qutu.com/m_egou/public/loading_white.png" data-original="<@:=pic_url @>"></a></div>
			    <div class="title"><a href="'/toTbItem.htm?id=<@:=item_id @>&page=m_tm" ><@:=title @></a></div>
			    <div class="money_bg"><span class="sybol">￥</span><span class="money"><@:=discount_price @></span><del>￥<@:=reserve_price @></del><span class="buy"><@:=volume @>人已买</span></div>
			</li> 
		
