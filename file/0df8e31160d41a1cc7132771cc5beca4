<script type="text/javascript">
function Oshow(){
	oNavicon=$('#nav_icon').attr('class');
	if(oNavicon.indexOf("nav_icon1")==-1){
		window.document.body.scrollTop=0;
		setTimeout(function(){
			$('#nav_icon').addClass("nav_icon1");
			$('.nav_list').hide();
			$('.tmh_mlnav_name').show();
			$('.tmh_mlnav_bg').show();
			$("#tmh_mlnav").animate({ top: "0"},500);
			$('#main_bg').show();
			//$('.main').addClass("body_bg");
			$('.main').css({'height':"100%","overflow":'hidden'});
		},100);
	}else{
		Ohide();
	}
}
function Ohide(){
	$('#nav_icon').removeClass("nav_icon1");
	$('.nav_list').show();
	$('.tmh_mlnav_name').hide();
	$("#tmh_mlnav").animate({ top: "-280px",}, 500); 
	setTimeout(function(){$('.tmh_mlnav_bg').hide()},500);
	$('#main_bg').hide();
	$('.main').css({'height':"auto","overflow":'visible'});

}

