	<script type="text/javascript">
		function initTbTop(){
			obj = document.getElementById("tmh_nav"); 
			if (!obj){ 
				return;
			} 
			//获取要定位元素距离浏览器顶部的距离
			var itemCatsH = $("#tmh_nav").offset().top;
			//滚动条事件
			$(window).scroll(function(){
			//获取滚动条的滑动距离
				var scroH = $(this).scrollTop();
				//滚动条的滑动距离大于等于定位元素距离浏览器顶部的距离，就固定，反之就不固定
				if(scroH > itemCatsH){
					$("#tmh_nav").attr("class","tmh_nav tmh_nav_fixed");
					
				}else{
					$("#tmh_nav").attr("class","tmh_nav");
				} 
			}); 
		};
	
		var width = parseInt($("#tbItem .pic img").eq(0).width());
		width = Math.ceil(width/100) * 100;
		width = width == 0 ? 400 : width;
		function setImgSize(obj,attrName){
			if($(obj).length == 0) return false;
			obj.find("img[name='product_img']").each(function(index){
				var src = $(this).attr(attrName);
				if(!src){
					src = $(this).attr('src');
					attrName = "src";
				}
				if(src.indexOf("egou.com") > 0) {
					var subStr = src.substring(src.lastIndexOf('.'));
					var reg = /\.\d{1,3}x\d{1,3}/;
					if(!reg.test(subStr)){
						$(this).attr(attrName,src+'.'+width+'x'+width);
					}
				}else if((src.indexOf("taobaocdn") > 0 ||src.indexOf("tbcdn.cn") > 0 || src.indexOf("alicdn.com") > 0)){
					var subStr = src.substring(src.lastIndexOf('.'));
					var reg = /_\d{1,3}x\d{1,3}/;
					if(!reg.test(subStr)){
						$(this).attr(attrName,src+'_'+width+'x'+width);
					}
				}
				
			}); 
		}
		setImgSize($("#tbItem"),'data-original');
		
		$(document).ready(function() {
			initTbTop();
			$("#tbItem img[name='product_img']").lazyload({effect:"fadeIn",hiddenType:"visibility",failurelimit:50,threshold:400});
		});
		var itemsHeight = $("#tbItem");
		function findTop() {
			var b = 0;
			itemsHeight.each(function(i) {
				var t = $(this);
				t.children().each(function() {
					b = b + $(this).height();
				});
			});
			return b;
		}
		function ScrollLoader(opt) {
		    var loading = false;
		    var last_index = 0;
		    var bottom_height = opt.bottom_height || 100;
		    var jdom = $(document);
		    var jwin = $(window);
		    function load() {
		    	if (loading) {
		       	 	if ($.isFunction(opt.before_load))
			          opt.before_load.apply(opt, [
			                    last_index
			                ]);
		       	 	return;
		    	}
		    	loading = true;
		    	$.ajax({
		    		type: "POST", 
		    		url: opt.url,
		    		data : opt.paras,
		    		cache : false,
		    		dataType : "json",
		    		success : function(data) {
		    			if(data != null){
		    				opt.paras.pageNo++;
		    				var items = data;
			        		if (items.length == 0) {
			                    jwin.off('scroll', onscroll);
			                    loading = false;
			                    return;
			                }
			        		opt.renderer.apply(opt, [items]);
			                loading = false;
		    			}
		    		},
		    		error : function(XMLHttpRequest, textStatus, errorThrown) {
		    			jwin.off('scroll', onscroll);
		    			loading = false;
		    		},
		    		complete:function(){
		    			  if ($.isFunction(opt.after_load))
				                opt.after_load.apply(opt, [last_index]);
					}
		    	});
	    	};
		    function onscroll() {
		    	var totalheight = parseFloat(jwin.height()) + parseFloat(jwin.scrollTop());
		    	var curTop = findTop() / 2;
		    	if (curTop <= totalheight + bottom_height) {
		        	load();
		        }
		    }
		   jwin.scroll(onscroll).trigger('scroll');
		};
		new ScrollLoader({
		    bottom_height: 1000,
		    before_load: function () {
		    	 $('.loading').show();
		    },
		    after_load: function () {
		    	$('.loading').hide();
		    	$("#tbItem img.lazyimg").lazyload({effect:"fadeIn",event:'load',threshold:1000,failure_limit:20});
		    },
		    url:window.ContextPath + '/tm_items_get.htm',
		    paras :{
		    	pageNo : 2,
				c :c,
				pt : pt,
				isHot : isHot,
				cat_lv1_name : catLv1Name,
				cat_lv2_name : catLv2Name,
				chn : chn
		    },
		    renderer: function (items) {
		        $.each(items, function (i, item) {
		        	 var reserve_price = item.reserve_price ? item.reserve_price : 0;
		             item.reserve_price = (reserve_price - reserve_price%10)/100;
		             var discount_price = item.discount_price ? item.discount_price : 0;
		             item.discount_price = (discount_price - discount_price%10)/100;
		        	var tpl = document.getElementById('item_tmpl').innerHTML;
		        	var g = template(tpl,item);
		        	$('#tbItem').append(g);
		        });
		    }
		});  
	</script>

<div class="gmu-media-detect" id="gmu-media-detect0"></div></body></html>