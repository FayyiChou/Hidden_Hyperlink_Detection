        var searchDefaultValue = $('form[name="hsearch"] :text[name="q"]').val();
		$('form[name="hsearch"]').submit(function(){
			if ($(':input[name="q"]').val()==searchDefaultValue){
                alert('请输入查询关键词');
				return false;
			}
		});

        $('form[name="hsearch"] :text[name="q"]').focus(function(){
            if ($(this).val()==searchDefaultValue) {
                $(this).val('').attr('class','');
            }
        }).blur(function(){
            if ($(this).val()=='') {
                $(this).val(searchDefaultValue).attr('class','no');
            }
        });
