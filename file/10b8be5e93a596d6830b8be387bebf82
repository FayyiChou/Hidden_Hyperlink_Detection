<script>
	$(function(){
		$(".title_long_show ul li a").each(function(){
			var len = 0;
			var s = $(this).text();
			var l=$(this).text().length;
			for (var i = 0; i < l; i++) {
			if (s[i].match(/[^\x00-\xff]/ig) != null)
				len += 2;
			else
				len += 1;
			} 
			$(this).css("width",len*6+8+"px");
		});
	})
</script>
