    function initLightbox(){
        new Lightbox({
            fileLoadingImage: 'http://www.metcalfemodels.com/skin/frontend/metcalfe/metcalfe/images/lightbox/loading.gif',
            fileBottomNavCloseImage: 'http://www.metcalfemodels.com/skin/frontend/metcalfe/metcalfe/images/lightbox/closelabel.gif',
            overlayOpacity: 0.8,
            animate: 1,
            resizeSpeed: 7,
            borderSize: 10,
            labelImage: 'Image',
            labelOf: 'of'
        });
    }


    if (Prototype.Browser.IE) {
        Event.observe(window, 'load', function(){ //KB927917 fix
            initLightbox();
        });
    } else {
        document.observe("dom:loaded", function(){
            initLightbox();
        });
    }
