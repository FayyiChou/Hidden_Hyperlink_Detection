//nav鼠标以上替换图片
$(document).ready(function(){
	$("#navUl li").mouseenter(function(){
		   $(this).find(".index").attr("src","/images/4/indexB.png");
	})
	$("#navUl li").mouseleave(function(){
		$(this).find(".index").attr("src","/images/4/index.png");
	})
	$("#navUl li").mouseenter(function(){
		   $(this).find(".service").attr("src","/images/4/serviceB.png");
	})
	$("#navUl li").mouseleave(function(){
		$(this).find(".service").attr("src","/images/4/service.png");
	})
	$("#navUl li").mouseenter(function(){
		   $(this).find(".join").attr("src","/images/4/joinB.png");
	})
	$("#navUl li").mouseleave(function(){
		$(this).find(".join").attr("src","/images/4/join.png");
	})
	$("#navUl li").mouseenter(function(){
		   $(this).find(".public").attr("src","/images/4/publicB.png");
	})
	$("#navUl li").mouseleave(function(){
		$(this).find(".public").attr("src","/images/4/public.png");
	})
	$("#navUl li").mouseenter(function(){
		   $(this).find(".advertise").attr("src","/images/4/advertiseB.png");
	})
	$("#navUl li").mouseleave(function(){
		$(this).find(".advertise").attr("src","/images/4/advertise.png");
	})
	$("#navUl li").mouseenter(function(){
		   $(this).find(".help").attr("src","/images/4/helpB.png");
	})
	$("#navUl li").mouseleave(function(){
		$(this).find(".help").attr("src","/images/4/help.png");
	})
	$("#navUl li").mouseenter(function(){
		$(this).find("a").css("color","#1779dc");
	})
	$("#navUl li").mouseleave(function(){
		$(this).find("a").css("color","white");
	})
})


//tabs

$(document).ready(function(){
		$("#tabs li").bind("mouseenter",function(){
			var index = $(this).index();
			var divs = $("#tabs-body > div");
			$(this).parent().children("li").attr("class","tab-nav");
			$(this).attr("class","tab-nav-action");
			divs.hide();
			divs.eq(index).show();
		});
	});
	

$(document).ready(function(){
		$("#tabsMod li").bind("mouseenter",function(){
			var index = $(this).index();
			var divs = $("#tabs-body-Mod > div");
			$(this).parent().children("li").attr("class","tab-nav");
			$(this).attr("class","tab-nav-action-Mod");
			divs.hide();
			divs.eq(index).show();
		});
	});
	
$(document).ready(function(){
	
	var caidan= $('#content .midContent .secondArea .module .tabs-Mod ul li');
	
	caidan.mouseenter(function(){
		$(this).siblings().children().removeClass('blueLine-action');
		$(this).children().addClass('blueLine-action');//点击菜单选择的没有下划线加上类样式one
		 		
		});   
});

$(document).ready(function(){
		$("#tabsSer li").bind("mouseenter",function(){
			var index = $(this).index();
			var divs = $("#tabs-body-Ser > div");
			$(this).parent().children("li").attr("class","tab-nav");
			$(this).attr("class","tab-nav-action-Mod");
			divs.hide();
			divs.eq(index).show();
		});
	});
	
$(document).ready(function(){
