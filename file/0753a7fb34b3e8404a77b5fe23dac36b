$("#special li h1").mouseover(function(){
	$(this).siblings("ul").slideDown(400);
	$(this).parent().siblings().children("ul").hide();
})
$("#special > li").mouseleave(function(){
		
			$(this).children("ul").hide();
		
		
	});
