  		<script type="text/javascript">

   

    $(function () {

        //页面初始化时执行瀑布流



        var $container = $('#GetMoreBox');

        $container.show();

 

        var boxMaker = {};

		

		 var templateLi ="<p>{1}</p><p style=\"margin-top:11px; text-indent:27px; height:54px; overflow:hidden\">{2}</p><p style=\"float:right\">{3}</p>";



		 //var templateLi ="<p class=\"author\">{1}</p><p class=\"content\">{2}</p><p style=\"float:right\" class=\"imgstr\">{3}</p>";

        //var templateLi = "<a title=\"{1}\" rel=\"{2}\" tip=\"{3},{4}\" href=\"javascript:;\"><b><img width=\"{5}\" height=\"{6}\" alt=\"{1}\" src=\"{7}\"></b><!--<i><img height=\"100%\" alt=\"\" src=\"{8}\"></i><cite>posted by <em>Aviva baby</em> <br><small>{9}</small></cite><tt></tt>--></a>";

        boxMaker.makeBoxes = function (data) {

		

        var boxes = [];

        for (var i = 0; i < data.length; i++) {

            var tempLi = templateLi;

          

            tempLi=tempLi.replace("{1}", data[i]["imgstr"]);

			tempLi=tempLi.replace("{2}", data[i]["jianjie"]);

			tempLi=tempLi.replace("{3}", data[i]["jiastr"]);

          

            var box = document.createElement('div');

            box.setAttribute('class', "zxzx_photo");

			//box.setAttribute('style', ""+data[i]["stylestr"]+"");

            //alert(data[i]["stylestr"]);

            box.innerHTML = tempLi;

            

            boxes.push(box);

        }

		$(".loading").hide();

        return boxes;

    };



        window.pageindex = 4;

		window.last=12;  //分页开始设置

		window.amount=4; //一页显示多少条记录

        window.exectue=false;

		

    function request()

    {   



        $(".no_more").hide();

        $(".loading").show();

        var url = "dataindex.php";

		

       //alert(pageindex); 测试当前第几页

       

        var data = getDatas(url,  pageindex);

		

		last=amount*pageindex; // 改变分页起始序数

		

		pageindex++;

        

        if (!data || data.length == 0) {

            $(".no_more").show();

            $(".loading").hide();

			$(".more").hide();

            window.exectue=true;

            return;

       }

        var $boxes = $(boxMaker.makeBoxes(data));

       



        window.setTimeout(function () {

           

            $container.append($boxes);

         

            var msnry = $container.data('masonry');

           

            msnry.appended($boxes);



            msnry.layout();



            set_ppt();

            window.exectue=false;

        }, 1000);

        

    }



    function getDatas(url, pageindex) {

        var myData;

        $.ajaxSettings.async = false;

        $.post(url, { Action: "post", "last": last,"strcd": '', "amount": amount }, function (data, textStatus) {

            if (textStatus == "success") {

                myData = data;

            }

        },"json");

        return myData;

    }

	

	  



	  $(document).ready(function(){

	  

	  //request();//默认先显示一页

		  

	  $('#getmore').click(function(){

		  request();

		  });

	  })



    });



</script> 

