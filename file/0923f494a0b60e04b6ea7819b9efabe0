			$(".box3_title").heightLine({
				maxWidth:768,
				minWidth:480
			});
			$(".box3_title_fix").heightLine({
				minWidth:0
			});
			$(".box3_desc").heightLine({
				maxWidth:768,
				minWidth:480
			});
			$(".box3_desc_fix").heightLine({
				minWidth:0
			});
			$(".box3_img").heightLine({
				maxWidth:768,
				minWidth:480
			});
			$(".box3_img_fix").heightLine({
				minWidth:0
			});
		});
	</script>
