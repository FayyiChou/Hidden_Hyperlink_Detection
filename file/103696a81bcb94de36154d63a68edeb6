<script>
  $(document).ready(function(){
  var cM = 0;
    $('#sign-in').click(function(){
    $.ajax({
      type:"POST",
      url: "http://www.vapecraft.ru/main/login_in_ajax",
      data: { 
        username: $('input[name=username]').val(),
        password: $('input[name=password]').val()
      },
      dataType: "html", 
      success: function(data_status){
        var obj_status = jQuery.parseJSON(data_status);
        if(obj_status.warning.length){
          showAlert('#sign-inDrop', obj_status.warning, true);
        }else if(obj_status.success.length){
          showAlert('#sign-inDrop', obj_status.success, false, true);
          setTimeout(function() { location.reload(0); }, 800);
        } else{
          showAlert('#sign-inDrop', "Ошибка обмена, повторите попытку", true);
        }   
      }
    }); 
        return false;
    });
$('input[name=password]').keypress(function (e) {
  if (e.which == 13) {
  $('#sign-in').click();
  }
});

function showAlert(parent, message, warning, success){
    var cs = 'alert-info';
    if(parent==undefined){
        return;
    }
    $(parent+' .alert').attr({
        'class':'alert hidden',
        'style':''
    });
    if(message == undefined){
        return;
    }
    if(warning!=undefined && warning){
        cs = 'alert-warning navi-alert';
    }
    if(success!=undefined && success){
        cs = 'alert-success navi-alert';
    }    
    $(parent+' .alert').attr('class','alert hidden '+cs);
    $(parent+' .alert .text').html(''+message+'');
    //$(parent+' .alert').show();
    $(parent+' .alert').removeClass('hidden');
    clearInterval(cM);
    cM = setInterval(function (){
        $(parent+' .alert').hide('fast', function (){
            $(parent+' .alert').attr({
                'class':'alert hidden',
                'style':''
            });
        });
        clearInterval(cM);
    }, 10000);
}   
    $('#save_profile_button').click(function(e){
        var form = $('#save_profile');
        $.post('/main/save_profile/', form.serialize(), function (data){
            if(data.error.length){
                showAlert('#save_profile', data.error, true);
            }else if(data.success.length){
                showAlert('#save_profile', data.success, false);
                setInterval(function (){
                    window.location.href="?";
                }, 3000);
            } else{
                showMessage("Ошибка обмена, повторите попытку", true, false);
            }
        }, 'json');
        e.preventDefault();
        return false;
    }); 
});
</script>
