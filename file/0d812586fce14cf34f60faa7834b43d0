<script type="text/javascript">
	var have_logo = 1;
	var lid = '75';
	var py = 'Q';
	

var logo_view_li = document.getElementById("logo_view_li");
var li = logo_view_li.getElementsByTagName("li");
var logo_view_div = document.getElementById("logo_view_div");
var div = logo_view_div.getElementsByTagName("div");
var mouseover_time;

for(var i = 0; i < li.length; i++){
	li[i].onmouseover = function(){
		var obj = this;
		mouseover_time = setTimeout(function(){
			for(key_li=0 ; key_li<li.length; key_li++) {
				if(li[key_li] == obj){
					li[key_li].className = "li_active";
					div[key_li].style.display = "block";
				}else{
					li[key_li].className = "";
					div[key_li].style.display = "none";
				}
			}
		}, 200);


	}

	li[i].onmouseout = function(){
		clearTimeout(mouseover_time);
	}
	
}



	if(have_logo == 0){
		document.getElementById("li_A").onmouseover();
	}else{
		document.getElementById("li_"+py).onmouseover();
		document.getElementById("a_"+lid).className = "ak_check"; 
		document.getElementById("a_"+lid).target = "_self";
	}
	

</script>

</div>
