 <SCRIPT>
var speed2=25//速度数值越大速度越慢
demmo2.innerHTML=demmo1.innerHTML
function Marqueee(){
if(demmo2.offsetWidth-demmo.scrollLeft<=0)
demmo.scrollLeft-=demmo1.offsetWidth
else{
demmo.scrollLeft++
}
}
var MyMarr=setInterval(Marqueee,speed2)
demmo.onmouseover=function() {clearInterval(MyMarr)}
demmo.onmouseout=function() {MyMarr=setInterval(Marqueee,speed2)}
</SCRIPT>
