var speeddemoL1 = 30;
var demoL1=document.getElementById("demoL1"); 
var demoL1b=document.getElementById("demoL1b"); 
var demoL1e=document.getElementById("demoL1e");
demoL1e.innerHTML=demoL1b.innerHTML;
function MarqueedemoL1()
{
	if(demoL1e.offsetWidth-demoL1.scrollLeft<=0){demoL1.scrollLeft-=demoL1b.offsetWidth;} 
	else{demoL1.scrollLeft++;}
} 
var flagdemoL1=setInterval(MarqueedemoL1,speeddemoL1);
demoL1.onmouseover=function(){clearInterval(flagdemoL1);} 
