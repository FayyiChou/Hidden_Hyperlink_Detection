<script type="text/javascript"> 
$(function(){
	  $('#searchkey').bind('keypress',function(event){
	        if(event.keyCode == "13")    
	        {
	        	searchkey();
	        }
	    });

});
function searchkey(){
	var searchkey=$("#searchkey").val();
	if($.trim(searchkey)==""||$.trim(searchkey)=="请输入关键字"){
		alert("关键字不能为空");
		return;
	}
	window.location.href=encodeURI("../../html/searchkey.shtml?title="+searchkey);
	
}
</script>
