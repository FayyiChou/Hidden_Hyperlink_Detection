<script type="text/template" id="T_Cities">
    <div id="J_Cities" class="cities">
        <span class="citiestitle">支持汉字/拼音/英文字母</span>
        <% var map = ['hot', 'abcdef', 'ghj', 'klmn', 'pqrstw','xyz']; %>
        <ul class="cities_cat clearfix">
            <% for(var i = 0; i < map.length; i++) { %>
                <li
                <% if (i === 0) { %>
                class="current"
                <% } else if (i === map.length - 1) {%>
                class="last"
                <% } %>
                >
            <% if (map[i] === 'hot') { %>
            热门城市
            <% } else {%>
            <%= map[i].toUpperCase() %>
            <% } %>
            </li>
            <% } %>
        </ul>
        <div class="cities_list" id="J_PKGCityList">
            <% for(var i = 0; i < map.length; i++) { %>
            <ul class="cities_list_cat clearfix <% if (i !== 0) { %> hide <% } %><% if (map[i] == "hot") { %> hotlistcity <% } %>">
                <% var group = data[map[i]]; %>
                <% var mapzimu = map[i]; %>
                <% for(var k=0;k<mapzimu.length;k++){%>
                    <%if("hot" == mapzimu && k ==0){%>
                        <% for(var h = 0; h < group.length; h++) { %>
                        <% var item = group[h]; %>
                            <li title="<%= item.cityName %>"><a href="javascript:;" code="<%= item.cityCode %>"><%= item.cityName %></a></li>
                        <% } %>
                    <%}else if(mapzimu !="hot"){%>
                        <span>
                            <li class="firstcode"><%=mapzimu[k].toUpperCase() %></li>
                            <div class="citycodemap">
                                <% for(var h = 0; h < group.length; h++) { %>
                                <% var item = group[h]; %>
                                    <% if(mapzimu[k] == item.cityAbbr.substr(0, 1) ){%>
                                        <li title="<%= item.cityName %>"><a href="javascript:;" code="<%= item.cityCode %>"><%= item.cityName %></a></li>
                                    <%}%>
                                    
                                <% } %>
                            </div>
                        </span>
                        
                    <%}%>
                    

                <% }%>
                
            </ul>
            <% } %>
        </div>
    </div>
</script>

