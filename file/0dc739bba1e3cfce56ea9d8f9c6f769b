            <script>
            	function goTagList(tagName,parentId) {
            		var menuName = getMenuName(MENU_ID);
					var sourceInsStr = encodeURIComponent(menuName+"-"+"详情页右侧大神标签");
            		if(parentId != '0'){
						window.open(CTX+"/list/submenu/36?subTags="+encodeURIComponent(tagName)+"&sourceIns="+sourceInsStr);
					}else{
						window.open(CTX+"/list/submenu/36?rootTags="+encodeURIComponent(tagName)+"&sourceIns="+sourceInsStr);
					}
				}
				var grilDataList;
				function showBeautyList() { 
					var showdata = {};
					showdata.items = [];
			    	if(grilDataList.items.length > 0){	
						var arr = grilDataList.items.sort(function(){return 0.5 - Math.random()});
						showdata.items = arr.slice(0,2);
						document.getElementById('beautyList').innerHTML = template("grilListTemp",showdata);
					}
				} 
	        	$(function() {
	        		try { 
	        			function clothNum(num) {
							if (num > 10000) {
								return (num / 10000).toFixed(1) + "W";
							}
							return num;
						}

						var meiNvmenuId = "2";
						var postData = {};
						if(meiNvmenuId == 36){
							postData.rootMenuId = 0;
						}else{
							postData.subMenuId = 36;
						}
						$.ajax({  
						     type: "get",  
						     url: "http://jtv.icafe8.com/superajax/commendSuperList.do",  
						     data: postData,
						     dataType: "json",
							 success:function(data){
							 	var gameAncharList = "";
							 	var menuName = getMenuName(MENU_ID);
							 	var sourceInsStr = encodeURIComponent(menuName+"-"+"详情页右侧大神列表");
							 	for(var i=0,l=data.length>=5?5:data.length;i<l;i++){
			        			 	gameAncharList += "<li> <a href=\""+CTX+"/detail/"+data[i].roomId+"?sourceIns="+sourceInsStr+"\" target=\"_blank\"> <img src=\""+data[i].anchorHeadImgUrl+"\" alt=\"\">"+
			        			 						"<div class=\"name\">"+data[i].ancharName+"</div>"+
			        			 						"<div class=\"count\">"+clothNum(data[i].adjustLookCount)+"</div>"+
		                                    			"<div class=\"desc\">"+data[i].title+"</div></a></li>"
								 }
								 $("#gameAnchar").append(gameAncharList);
							 }
						}); 						

						var searchUrl="http://jtv.icafe8.com/getOnlineRooms.do?perPage=60&rootMenuId="+meiNvmenuId; 
						$.ajax({  
						     type: "get",  
						     url: searchUrl,  
						     dataType: "json", 
							 success:function(data){
								 grilDataList = data;
								 showBeautyList();
							 }
						}); 

	        		} catch(e) {}
	    		});
	    		</script>
