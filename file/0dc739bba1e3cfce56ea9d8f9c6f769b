			function goAttribute(attrStr,rootMenuId,menuId) {
				var _url = CTX+"/attrList/"+encodeURIComponent(attrStr)+"?";
				if(rootMenuId==0){
					_url += "rootMenuId="+menuId;
				}else{
					_url += "subMenuId="+menuId;
				}
        		window.open(_url);
			}
    	    function addHistoryStore(){
    			var menuName = getMenuName(36);
    			var thisobj = {roomId:"178459",title:"童颜巨？？",videoImgUrl:"http://img2.plures.net/5584328c-9190-4561-90f9-99e7be2194f2",onlineStatus:"0",ancharName:"龟哥户外",lookCount:"160004",menuName:menuName};
    			var thisobjlist = {};
    			var addhistory = store.get("jtvhistory");
    			if(addhistory!=null){
    				thisobjlist = addhistory.val;
    			}
    			thisobjlist[178459] = thisobj;
    			storeWithExpiration.set("jtvhistory",thisobjlist,600000);
    		}
    		function addHistory(){
    			var memberId = $.cookie('memberId');
	    		if(memberId!=null){
					$.post(FRONT_DOMAIN+"/addHistory.do",{memberId:memberId,roomId:178459},function(data,status){});			
				 }
				 var historyRoomIds = store.get('historyRoomIds')||"";
				 if(historyRoomIds != ""){
				 	var subhistoryRoomIds = ","+historyRoomIds+",";
				 	if(subhistoryRoomIds.indexOf(",178459,") >= 0){
				 		historyRoomIds = subhistoryRoomIds.replace(",178459,", ",");
				 	}else{
						var hisarr = str2Arr(historyRoomIds);
				 		if(hisarr.length > 60){
				 			historyRoomIds = historyRoomIds.substring(0,historyRoomIds.lastIndexOf(","));
				 		}
				 		historyRoomIds = ","+historyRoomIds+",";
				 		addHistoryStore();
				 	}
				 	historyRoomIds = 178459 + historyRoomIds.substring(0,historyRoomIds.length-1);
				 }else{
				 	historyRoomIds = 178459;
				 	addHistoryStore();
				 }
				store.set('historyRoomIds', historyRoomIds);
			}
			var addhistoryflag = false;
			function initSaveFistory(){
				setTimeout(function(){
					if(!addhistoryflag){
						var barflag = $.cookie('barerrorflag');
						var inithistoryload = $.cookie('inithistoryload');
						if(barflag==null && inithistoryload==null){
							initSaveFistory();
						}else{
							addHistory();
							addhistoryflag = true;
							return;
						}
					}
				},1000);
			}
