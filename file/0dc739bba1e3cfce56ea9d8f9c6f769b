        	$(function() {
        		try {
        		var addfavorite = store.get("favoriteRoomIds");
    			if(addfavorite!=null){
    				if((","+addfavorite+",").indexOf(",178459,") < 0){
    					$(".concern_btn").show();
						$(".have_concern").hide();
    				}else{
    				    $(".concern_btn").hide();
						$(".have_concern").show();
    				}
    			}
        		var rootMenuId = ROOT_MENU_ID;
        		var rootTagId = "";
        		var subTagId = "";
        		if(MENU_ID != null && MENU_ID != ""){
        			for(var p in MENU_MAP){
					 	for(var i=0,l=MENU_MAP[p].length; i < l;i++){
					 		if(MENU_MAP[p][i].menuId == MENU_ID){
					 			rootMenuId = MENU_MAP[p][i].parentId;
					 			break;
					 		}
					 	}
					 }

        		}
        		var guid = $.cookie('jtvguid');
        		var fromtype = $.cookie('jtvfromtype');
        		var barId = $.cookie('jtvbarId')||"";
        		if(fromtype == "client"){
					fromtype = 1;
				}else if(fromtype == "twolevelpage"){
					fromtype = 2;
				}else if(fromtype == null){
					fromtype = 0;
				}
        		var param = "{barId:\""+barId+"\",rootMenuId:\""+rootMenuId+"\",subMenuId:\""+MENU_ID+"\",rootTagId:\""+rootTagId+"\",subTagId:\""+subTagId+"\",siteId:\""+SITE_ID+"\",roomId:\"178459\",ancharName:\"龟哥户外\",type:\"0\",guid:\""+guid+"\",source:\""+fromtype+"\",sourceExt:\""+getQueryString("sourceExt")+"\",sourceIns:\""+getQueryString("sourceIns")+"\"}";
