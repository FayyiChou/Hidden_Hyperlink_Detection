			function templateHelper(){
				try {
			    template.helper("clothNum",function(num){
			    	if(num>10000){
			    		return (num/10000).toFixed(1)+"万";
			    	}
			    	return num;
			    });
   
			    template.helper("clothSite",function(data){
			    	return window.sitemap[Number(data)].siteName;
			    });
