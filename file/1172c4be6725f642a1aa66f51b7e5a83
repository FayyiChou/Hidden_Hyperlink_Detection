<script type="text/javascript">
        function ShowChecks(n,count){
        for(var i=0;i<count;i++){
        if (i==n){
        document.getElementById('col'+i).style.display='block';
        document.getElementById('curs'+i).className='current';}
        else {
        document.getElementById('col'+i).style.display='none';
        document.getElementById('curs'+i).className='';}
        }
        }
        
       ShowChecks(0,4); 
      

</script>

        

