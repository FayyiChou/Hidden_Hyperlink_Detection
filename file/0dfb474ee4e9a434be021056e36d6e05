			case 2:edit_mode_='design';toolbar_=['tenpay','<label style="float:left;padding-top:3px;"><input style="border:none" type="checkbox" onclick="SwitchDiv(this.checked,\'title_input\');try{gid(\'topic\').focus();}catch(e){}" />标题</label> <label style="float:left;padding-top:3px;"><input style="border:none" type="checkbox" onclick="gid(\'ad\').src=\'savefile.asp?boardid=17\';SwitchDiv(this.checked,\'upload_input\')" /*checked="checked"*/ />上传</label>'];break;
