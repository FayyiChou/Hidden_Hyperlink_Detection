			textarea_id:'body',
			edit_height:'212px',
			edit_mode:edit_mode_,
			toolbar:toolbar_,
			use_ubb:2==Dvbbs_Mode,
			is_show_status:Dvbbs_Mode>0
		};
		function dvloadarea(){
			if ('function'==typeof DvEdit){
				dvtextarea=new DvEdit(dveditconfig);
			}else{setTimeout('dvloadarea()',0)}
		}
		dvloadarea();
		</script> 
       </div> </td> 
     </tr> 
