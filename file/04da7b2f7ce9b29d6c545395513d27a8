function search_key123(){
	var v = document.getElementById("searchinput").value;
	if(v=="" || v=="请输入关键字"){
		$("#searchinput").focus();
		return ;
	}
	window.location.href = "http://www.bbanp.com/list_0/search?title="+v;
}
function onKeysubmitsearchform1(event){

	var keyCode = event.keyCode?event.keyCode:event.which?event.which:event.charCode;
	if (keyCode ==13){
		search_key123();
	}
}
