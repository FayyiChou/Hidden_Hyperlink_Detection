<script>
	var page=1;
	var step=20;
	var doms=1;
	try{
		var html="";
		getList();
	}catch(err){
		//$$("shade").style.display="none";
	}
	
	function getList(){
		//$$("shade").style.display="block";
		try{
			Prepare({part:"anno/list",page:page,step:step},function(data){
				if(data.results.list.length==step){
					$$("loadMore").style.display="block";
					page+=1;
				}else{
					$$("loadMore").style.display="none";
				}
				if(data.results.ret==100&&data.results.list.length!=0){
					for(i=0;i<data.results.list.length;i++){
						html+="<table>"+(doms==1?"":"<tr><td colspan='2' style='padding:0'><hr></hr></td></tr>")+"<tr>"+
								"<td style='width:30px;text-align:center'>&nbsp;</td>"+
								"<td onclick=\"to('/anno-info/qrpfxdvsbfevsaccxvuuraodxssabbbb/"+data.results.list[i].announcementId+"');\"><div class='d1'>"+data.results.list[i].announcementName+"</div><div class='d2'>"+data.results.list[i].createTime+"</div></td>"+
							  "</tr></table>"
						doms++;
					}
				}else if(page==1){
					html = "<table class='minHeight' style='height:400px'><tr><td style='text-align:center;background:#efeff4'><img src='/app/images/null.png' style='width:62px;'><div style='padding:5px;font-size:16px;color:#848484'>暂无公告消息记录</div></td></tr></table>"
				}
				$$("userAnn").innerHTML=html;
				//$$("shade").style.display="none";
			});
		}catch(e){
			//$$("shade").style.display="none";
			Alert("系统忙，请稍后重试！");
		}
	}
	$$("loadMore_btn").onclick = function() {
		getList();
	}

	//设定listtitle
	setListTitle(appid);
		
	
</script></body></html>