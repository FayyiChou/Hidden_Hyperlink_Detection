	window.top.location = "http://www.byitgroup.com/webonstage/";
function webGo()
{
    var lng = document.getElementById("selLang").options[document.getElementById("selLang").options.selectedIndex].value;
    if (lng == "")
    {
        alert("请选择语言");
        return;
    }
    if (lng == "en")
    {
         window.top.location = "http://www.byitgroup.com/webonstage/";
    }
     if (lng == "cn")
    {
       window.top.location = "webonstage/";
    }
}
</script>
