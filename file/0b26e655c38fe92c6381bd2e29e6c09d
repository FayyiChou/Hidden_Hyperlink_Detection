                            <a class=" articlelist1_a_title " target="_blank" href="/97/1b/c2490a235291/page.htm" title="区政协党组班子召开民主生活会" >区政协党组班子召开民主生活会</a>
                        
                    </li>
                </ul>
            </td>
            <td id="endw91"></td>
        </tr>
    </table>
</div>
<script language="javascript">
    var topw91 = document.getElementById("topw91");
    var beginw91 = document.getElementById("beginw91");
    var endw91 = document.getElementById("endw91");
    var tabw91 = document.getElementById("tabw91");
    //复制内容
    endw91.innerHTML = beginw91.innerHTML;
    //调整table的宽度
    tabw91.width = topw91.offsetWidth * 2;
    //循环函数
    function marqueew91() {
        if (endw91.offsetWidth - topw91.scrollLeft <= 0) {
            topw91.scrollLeft -= beginw91.offsetWidth;
        } else {
            topw91.scrollLeft += 1;
        }
    }
    //定时器
    var mymarw91 = setInterval(marqueew91, 150);
    //鼠标移入
    topw91.onmouseover = function() {
        clearInterval(mymarw91);
    };
    //鼠标移出
    topw91.onmouseout = function() {
        mymarw91 = setInterval(marqueew91, 150);
    };
</script>
