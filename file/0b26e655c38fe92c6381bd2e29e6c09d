<script language="JavaScript">
            var w90imgUrl = new Array();
            var w90imgLink = new Array();
            var w90imgTitle = new Array();

            var w90count = 0;
            var w90running = false;
            var w90index = 0;
            var w90nextAdTimer;
            var w90k = 0;
            function w90nextAd(num) {
                if (num >= 0 && num <= w90count - 1) {
                    w90index = num;
                    clearTimeout(w90nextAdTimer);
                } else {
                    num = w90index;
                }
                if (w90index > w90count - 1) {
                    w90index = 0;
                }
                if (!w90running) {
                    w90running = true;

                } else if (document.all) {
                    w90imgShow.filters.revealTrans.Transition = 50;
                    w90imgShow.filters.revealTrans.apply();
                    w90imgShow.filters.revealTrans.play();
                }
                document.getElementById('w90imgShow').src = w90imgUrl[w90index];
                if (w90imgTitle[w90index] != '') {
                    document.getElementById('w90imgTitle').innerHTML = '<a href="javascript:w90openWindow();" title="' + w90imgTitle[w90index] + '">' + w90imgTitle[w90index] + '</a>';
                }
                document.getElementById('w90imgNumber').innerHTML = "";
                w90k = 0;
                for (var i = 0; i < w90count; i++) {
                    w90k++;
                    if (i != w90index) {
                        document.getElementById('w90imgNumber').innerHTML += '<a href="javascript:w90nextAd(' + i + ')" class="nomal">' + w90k + '</a>';
                    } else {
                        document.getElementById('w90imgNumber').innerHTML += '<a href="javascript:w90nextAd(' + i + ')" class="current">' + w90k + '</a>';
                    }
                }

                w90index++;
                if (w90count > 1) {
                    w90nextAdTimer = setTimeout("w90nextAd(-1)", 4000);
                }
            }

            function w90openWindow() {
                var jumpUrl = w90imgLink[w90index - 1];
                if (jumpUrl && jumpUrl != '') {
                    window.open(jumpUrl);
                }
            }

            $().ready(function() {
                try {
                    var objs = w90imgJsons;
                    w90count = objs.length;
                    if (w90count == 0) {
                        document.getElementById('w90imgShow').style.display = "none";
                        document.getElementById('w90imgNumber').style.display = "none";
                        document.getElementById('w90imgTitle').style.display = "none";
                        return;
                    }
                    for (var i = 0; i < objs.length; i++) {
                        w90imgUrl[i] = objs[i].src;
                        w90imgLink[i] = objs[i].url;
                        w90imgTitle[i] = objs[i].title;
                    }
                    w90nextAd(-1);
                } catch (e) {

                }
            });
