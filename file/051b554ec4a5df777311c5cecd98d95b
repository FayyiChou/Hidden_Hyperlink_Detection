<script type="text/javascript"> 
var speed=30;
var demoa = $("#demoa");
var demoa1 = $("#demoa1");
var demoa2 = $("#demoa2");
demoa2.html(demoa1.html());
function Marquee2(){ 
    if(demoa.scrollLeft()>=demoa1.width())
        demoa.scrollLeft(0); 
    else{
        demoa.scrollLeft(demoa.scrollLeft()+1);
    }
} 
var MyMar=setInterval(Marquee2,speed) 
demoa.mouseover(function() {
    clearInterval(MyMar);
} )
demoa.mouseout(function() {
    MyMar=setInterval(Marquee2,speed);
} )

function fun1(){
    alert(demoa.scrollLeft());
}
function fun2(){
    alert(demoa1.width());
}
</script>
<script type="text/javascript">

$(document).ready(function(){
	
	$("#cform").submit(function(){
		
		var flag = false;
		
		if($("#keyword").val() == ""){
			
			alert("请输入关键字！");			
			
		}else{			
			flag = true;
		}
		
		return flag;
	})
})

</script>


<script type="text/javascript">
function hidead()
{document.getElementById("ad").style.display="none";}

function wcshow(){
			
		if(document.getElementById('wczt').style.display=="none"){
			
			document.getElementById('wczt').style.display="block";
			exit();
		}
	  		
		else{
			
			document.getElementById('wczt').style.display="none";
			
		}		
		
	}

</script>

