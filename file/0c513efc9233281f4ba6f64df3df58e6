<script type="text/javascript">
var lastPage='';
var currentPage='/eshare/share';
var end_time,leave_time;
$(document).ready(function() {
	end_time = new Date();
});
document.addEventListener('visibilitychange', function() {
	try
	{
	    if (document.visibilityState == 'hidden') 
	    {
	    	leave_time=new Date();
	    	page=currentPage;
	    	//event|currentPage|pageTitle|fromPage|requestTime|pageStartTime|pageEndTime|pageLeaveTime|userid|usernickname
	    	var msg="PAGE|"+getPageName(currentPage)+"|"+getPageName(lastPage)+"|1491079466531|"+start_time.getTime()+"|"+end_time.getTime()+"|"+leave_time.getTime()+"|0"+"|";
	    	sendData(msg+getExtra(),"PAGE");
	    	try
	    	{
	    		if(page=='/eshare/index2'||page=='/eshare/play2'||page=='/eshare/vlivelist' ||page=='/eshare/vlivelistdetail' ||page=='/eshare/vliveownercenter' ||page=='/eshare/rolllist')
	    		{
	    			 if(!ended&&laststarttime>0)
	    			 {
	    				sendVodData(last_id,last_title,last_url,last_ownerid,last_ownername,(new Date().getTime()-laststarttime)/1000);
	    			 }
	    		}
	    		else if(page=='/eshare/live'||page=='/eshare/playback'||page=='/eshare/programDetail')
	    		{
	    			if(!ended&&last_starttime>0)
	    			{
	    				sendLiveData(last_channelid,last_channelname,last_url,last_islive,(new Date().getTime()-last_starttime)/1000,last_programe_name);
	    			}
	    		}
	    	}
	    	catch(e)
	    	{
	    	}
	    }
	}
	catch(exx)
	{
	}
}); 

function getExtra()
{
	var extra="";
	
	   	
	   
	   	
	   
	   
	   
	   
	   
	   
	   
		extra="|0||20170402044426|"+offset+"|"+$(".share_text").val();
	   
	   
	   
	   
	   
       
 	   
 	   
 	   
 	   
 	  
	   
	   
	     	   
	   
	   	   
	   
	 	
	return extra;
}

function getPageName(page)
{
	if(page=='/eshare/tvchannel')
	{
	   return "直播汇聚页";
	}
	if(page=='/eshare/cctv1')
	{
	   return "CCTV欧洲杯页";
	}
	if(page=='/eshare/goldTime')
	{
	   return "奥运金牌时刻页";
	}
	if(page=='/eshare/index2')
	{
	   return "汇聚页";
	}
	if(page=='/eshare/live'||page=='/eshare/playback')
	{
	   return "直播频道页";
	}
	if(page=='/eshare/liveplay')
	{
	   return "直播活动页";
	}
	if(page=='/eshare/matchResult')
	{
	   return "奥运战况页";
	}
	if(page=='/eshare/play2')
	{
	   return "被分享页";
	}
	if(page=='/eshare/programDetail')
	{
	   return "奥运分项直播";
	}
	if(page=='/eshare/schedule')
	{
	   return "赛程页";
	}
	if(page=='/eshare/share'||page=='/eshare/share2')
	{
	   return "分享页";
	}
	if(page=='/eshare/shareRank')
	{
	   return "排行榜页";
	}
	if(page=='/eshare/user')
	{
	   return "用户订阅页";
	}
	if(page=='timeline')
	{
	   return "微信朋友圈";
	}
	if(page=='groupmessage')
	{
	   return "微信群";
	}
	if(page=='singlemessage')
	{
	   return "微信好友";
	}
	if(page=='/eshare/vliveappoint')
	{
	   return "预约";
	}
	if(page=='/eshare/vlivelist')
	{
	   return "直播汇聚";
	}
	if(page=='/eshare/vlivelistdetail')
	{
	   return "直播详情";
	}
	if(page=='/eshare/vlivemyappoint')
	{
	   return "我的预约";
	}
	if(page=='/eshare/vliveownercenter')
	{
	   return "播客主页";
	}
	if(page=='/eshare/vliveusercenter')
	{
	   return "个人中心";
	}
	if(page=='/eshare/rolllist')
	{
	   return "滚播汇聚页";
	}
	if(page=='/eshare/uploaded')
	{
	   return "已上传视频列表页";
	}
	if(page=='/eshare/uploadNew')
	{
	   return "上传视频页";
	}
	if(page=='/eshare/myshare')
	{
	   return "我的纷享";
	}
	if(page=='/eshare/classify')
	{
	   return "分类页";
	}
	if(page=='/eshare/fivestar10')
	{
	   return "五星足球10周年";
	}
	if(page=='/eshare/lives')
	{
	   return "奉贤两会微直播";
	}
	return "未知";
}


function sendShareData(title,url,channel)
{
	//SHARE|Page|User|Nickname|title|shareurl|channel
	var msg="SHARE|"+getPageName(currentPage)+"|0"+"|"+"|"+title+"|"+url+"|"+channel;
	sendData(msg+getExtra(),"SHARE");
}

function sendCommentData()
{
	//COMMENT|Page|User|Nickname|shareid|sharetitle|ownerid|ownernickname
	var msg="COMMENT|"+getPageName(currentPage)+"|0"+"|"+"||未命名||";
	sendData(msg,"COMMENT");
}

function sendVliveCommentData()
{
	//COMMENT|Page|User|Nickname|shareid|sharetitle|ownerid|ownernickname
	var msg="COMMENT|"+getPageName(currentPage)+"|0"+"|"+"||未命名||";
	sendData(msg,"COMMENT");
}

function sendVodData(shareid,title,url,ownerid,ownername,playtime)
{
	var msg="VOD|"+getPageName(currentPage)+"|0"+"|"+"|"+shareid+"|"+title+"|"+url+"|"+ownerid+"|"+ownername+"|"+playtime;
	sendData(msg,"VOD");
}

function sendLiveData(channelid,channelname,playurl,isLive,playtime,programe_name)
{
	var msg="LIVE|"+getPageName(currentPage)+"|0"+"|"+"|"+channelid+"|"+channelname+"|"+playurl+"|"+isLive+"|"+playtime+"|"+programe_name;
	sendData(msg,"LIVE");
}


function sendVodDataSyn(shareid,title,url,ownerid,ownername,playtime)
{
	var msg="VOD|"+getPageName(currentPage)+"|0"+"|"+"|"+shareid+"|"+title+"|"+url+"|"+ownerid+"|"+ownername+"|"+playtime;
	sendData(msg,"VOD",false);
}

function sendLiveDataSyn(channelid,channelname,playurl,isLive,playtime)
{
	var msg="LIVE|"+getPageName(currentPage)+"|0"+"|"+"|"+channelid+"|"+channelname+"|"+playurl+"|"+isLive+"|"+playtime;
	sendData(msg,"LIVE",false);
}

function sendAdvertisement(url)
{
	var msg="ADVERTISEMENT|"+getPageName(currentPage)+"|0"+"||"+url;
	sendData(msg+getExtra(),"ADVERTISEMENT");
}

function sendData(msg,topic)
{
	sendData(msg,topic,true);
}

function sendData(msg,topic,isasync)
{
	try
	{
		$.ajax({
	        url: "http://tracker.otvcloud.com/collection.gif?appid=900001&subsys="+encodeURIComponent('0_亦纷享')+'&msg='+encodeURIComponent(msg)+'&topic='+topic,
	        async: isasync,
	        dataType: "jsonp",
	        success:function(result){
	            //alert(JSON.stringify(result));
	        },
	        error:function(xhr,status,error){
	            //alert(status);
	        }
	    });
	    
	}
	catch(e)
	{
	}
}


</script>
