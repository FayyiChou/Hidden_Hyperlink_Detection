<script>
var labels=',';
var rollids=",";
function createChannel()
{
	location.href='http://app.ybk.otvcloud.com/eboke/my60secondsUpdate.php?channel=1000&backurl='+encodeURIComponent("http://liveclips.otvcloud.com/eshare/share.php?parm=&playbillid=&channel=0");
}

$(".micro_channel").click(function(){
	if($(".channels").css("display")=="none"){
		$(".channels").slideDown(300);
		$(".select_channel").removeClass("border_radius");
		$(".select_channel").addClass("border_radius_none");
	}else{
		$(".select_channel").removeClass("border_radius_none");
		$(".select_channel").addClass("border_radius");
		$(".channels").slideUp(0.01);
	}
})
$(".channel_item").click(function(){
	
	rollids=$(this).attr('id').replace('live_','');
	$(".select_channel").html($(this).html());
	$(".channels").slideUp(0.01);
})
var labels=',';
var offset='';
$(function(){
	try
	{
		var msg="PAGE|"+getPageName(currentPage)+"|"+getPageName(lastPage)+"|1491079466531|"+start_time.getTime()+"|"+new Date().getTime()+"|0|0"+"|";
		sendData(msg+getExtra(),"PAGE");
	}
	catch(ex1)
	{
	}  
		
	var openid = '';
	var canclick = true;
	var url = '';
	var tempuserid = openid;
	var channel = '0';
	// var text = $(".share_text").val();

	$('.share_time').click(function(){
		$('.share_time').each(function(){
			$(this).removeClass("selected");
		});
		$(this).addClass("selected");
		offset = $(this).attr("time");
	});

	$(".share_cancel").click(function(){
		location.href="live.php?id=0";
	})
	
	
	var height=$(window).height();
	
	$(window).resize(function() {
		
		if($(window).height()>height)
		{
			$('.footer').show();
		}
		else
		{
			$('.footer').hide();
		}
		height=$(window).height();
    });
	    
	function selectTime(time)
	{
		offset=time;
		//console.log(offset);
		$('.share_time').each(function(){
			src=$(this).find('img').attr('src');
			
			if(time==$(this).attr('time'))
			{
				if(src.indexOf('selected')<0)
				{
					src=src.replace('.png','_selected.png');
					$(this).find('img').attr('src',src);
				}
			}
			else
			{
				if(src.indexOf('selected')>0)
				{
					src=src.replace('_selected.png','.png');
					$(this).find('img').attr('src',src);
				}
			}
		});
	}
	
	offset="60";
	
	$('.share_time').each(function(){
		$(this).click(function(){
			selectTime($(this).attr('time'));
		});
	});
	
$(".footer,.share_submit").click(function(){
	
	if(offset=='0')
	{
		 var titlepic="images/default-img.gif";
		 var desc="直播";
		 var param="".split(",");
		 
		 if(!isWeiXin()){
		        if (typeof(bridge)=="undefined") {
					alert("bridge不存在");
		        }else{
		        	var sdata="";
		        	var share_url ="http://liveclips.otvcloud.com/eshare/live.php?id="+param[0];
					sdata+="{";
					sdata+="\"succeeded\":true,";
					sdata+="\"url\":\""+share_url+"\",";
					sdata+="\"text\":\""+desc+"\",";
					sdata+="\"msg\":\"分享成功\",";
					sdata+="\"logo_url\":\""+titlepic+"\"";
					sdata+="}";
					bridge.onResult(sdata);
		        };
		        $('.share_shadow').hide();
		        return false;
		    }
		 
		 
		 try
		 {
		        wxready(titlepic,desc,"http://liveclips.otvcloud.com/eshare/live.php?id="+param[0]+"&channel="+param[2],"实时短视频，尽在亦纷享",'0',channel);
		 }
		 catch(ex)
		 {
		 }
		 $("#sharebg").show();
	}
	else
	{
		canclick=true;
		if(canclick){
			canclick = false;
			/*if(offset==0){
				alert("请选择分享片段时长");
				canclick = true;
				return;
			}*/
			var desc=$(".share_text").val();
			if(desc==''){
				desc="给您分享了一段精彩视频";
			}
			$('.share_shadow').show();
			$.post('php/share_time.php',{
				'url':url,
				'offset':offset,
				'starttime':'20170402044426',
				'text':desc
			},function (data){
				var obj = eval('('+data+')');
				if(obj.result==1){
					var titlepic = obj.titlepic?obj.titlepic:'images/default-img.jpg';
					var shareurl = obj.shareurl;
					$.post('php/review_share.php',{
						'shareurl':encodeURIComponent(shareurl),
						'tempuserid':tempuserid,
						'titlepic':encodeURIComponent(titlepic),
						'channel':channel,
						'text':desc,
						'rollids':rollids,
						'playbill_id':0,
						'offset':offset,
						'id':0,
						'labels':labels,
						'labeltext':$('.label_text').val()
					},function (data){
						var obj = eval('('+data+')');
						if(obj.result==1){
							//调微信分享
							if(!isWeiXin()){
						        if (typeof(bridge)=="undefined") {
									alert("bridge不存在");
						        }else{
						        	var sdata="";
						        	var share_url ="http://liveclips.otvcloud.com/eshare/play2.php?share_id="+obj.shareid;
									sdata+="{";
									sdata+="\"succeeded\":true,";
									sdata+="\"url\":\""+share_url+"\",";
									sdata+="\"text\":\""+desc+"\",";
									sdata+="\"msg\":\"分享成功\",";
									sdata+="\"logo_url\":\""+titlepic+"\"";
									sdata+="}";
									bridge.onResult(sdata);
						        };
						        $('.share_shadow').hide();
								// $("#sharebg").show();
						        return false;
						    }
							try
							{
								wxready(titlepic,obj.text,"http://liveclips.otvcloud.com/eshare/play2.php?share_id="+obj.shareid+"&goback=true","实时短视频，尽在亦纷享",'0',channel);
							}
							catch(ex)
							{
							}
							$('.share_shadow').hide();
							$("#sharebg").show();
							canclick = true;
						}else{
							if (!isWeiXin()) {
								var sdata="";
								sdata+="{";
								sdata+="\"succeeded\":false,";
								sdata+="\"msg\":\""+obj.msg+"\"";
								sdata+="}";
								bridge.onResult(sdata);
							};
							$('.share_shadow').hide();
							alert("分享失败");
							canclick = true;
						}
					});
				}else{
					$('.share_shadow').hide();
					alert("获取分享信息失败");
					canclick = true;
				}
			});
		}
	}
	
	});

	$("#sharebg").click(function(){
		$("#sharebg").hide();
	});
});
function isWeiXin(){
    var ua = window.navigator.userAgent.toLowerCase();
        if(ua.match(/MicroMessenger/i) == 'micromessenger'){
        return true;
    }else{
        return false;
    }
}
</script>


   
</body></html>