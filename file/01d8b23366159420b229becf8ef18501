<script>
	/*搜索框JS效果*/
	var height=document.getElementById("bk-bg");
	height.style.height=document.documentElement.clientHeight+'px';
	function fade(){
		$(".y_buttou_out").hide();
		$(".head-hua").fadeOut(100);
		$(".y-pc-newsearch").show().animate({right:'340px'});	
		$("#bk-bg").show();
		document.body.style.overflow="hidden";
	}
	$(".y-new-sicon02,#bk-bg").click(function(){
		$(".y_buttou_out").show();
		$(".y-pc-newsearch").hide().animate({right:'-560px'});
		$(".head-hua").fadeIn(600);
		$("#bk-bg").hide();
		document.body.style.overflow="visible";
	});
</script>


