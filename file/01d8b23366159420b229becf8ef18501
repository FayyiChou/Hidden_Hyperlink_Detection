        <script>
            var _num =0;
            if(_num == 1){
                show_div();
            }
            function show_div(){ 
            var obj_div=document.getElementById("starlist"); 
            obj_div.style.display=(obj_div.style.display=='none')?'block':'none'; 
            } 
            /**
             * 分类切换
             * 张衡
            */
             $(".category").on("click",function() {
                 /* Act on the event */
                 setPage();
                 var _category_id = $(this).data('category_id');
                 $("#menu0 li").removeClass('hover');
                 var url = $(this).data('url');
                 $(this).addClass('hover');
                 $("#cata_all").removeClass('hover');
                 $(".aorder").attr("data-category_id",_category_id);
                 $.post(url,{typeId:_category_id}, function (msg) {
                  console.log(msg.html);
                    if(msg.count){
                        $(".look-more").show();
                    }else{
                        $(".look-more").hide();
                    }
                    if (msg.status) {
                        $("#main_con_tab").html(msg.html);
                        $("#getmorequestionlist").attr("data-type-id",_category_id);
                        bind_local_comment();
                        callzan();
                        return false;
                    }else{
                         $("#main_con_tab").html('<div style="font-size:3em;padding:2em 0;color: #ccc;text-align: center">没有数据</div>');
                    }
                 });

             });
        /**
         * 排序
         */
        $(".aorder").on("click",function(){
           var _category_id = $(this).attr('data-category_id');
           var url = $(this).data('url');
           var _order_type = $(this).data('order_type');
           $(this).removeClass('paixu_active');
           $(this).siblings().addClass('paixu_active');
            $.post(url,{typeId:_category_id,ordertype:_order_type}, function (msg) {
                    if(msg.count){
                        $(".look-more").show();
                        $("#getmorequestionlist a").html("查看更多...");
                    }else{
                        $(".look-more").hide();
                    }
                    if (msg.status) {
                        $("#main_con_tab").html(msg.html);
                        $("#getmorequestionlist").attr("data-type-id",_category_id);
                        $("#getmorequestionlist").attr("data-ordertype",_order_type);
                        bind_local_comment();
                        callzan();
                        return false;
                    }else{
                         $("#main_con_tab").html('<div style="font-size:3em;padding:2em 0;color: #ccc;text-align: center">没有数据</div>');
                    }
                 });
        })

      var quesionCategory = "Array";
        $(function(){
           if(quesionCategory.length == 0){
            $(".more_guanzhu").click();
            }
        })
      function gotofollow(){
            $.post('/question/index/get_question_follow.html',{},function(data){
                if(data.status == 0){
                     toast.success(data.info);
                }else if(data.status == 1){
                    $("#question_follow").html(data.data);  
                }
            })
        }
        </script> 
        <!-- 导航展开收起 -->
        <script>
          function show(){
            var more=document.getElementById("more");
            var hiden=document.getElementById("hiden");
            if(hiden.style.display=="none"){
              hiden.style.display="block";
              $(".holddown").hide();

            }else{
              hiden.style.display="none";
              $(".holddown").show();
            }
          }
        </script>
        <!-- 导航展开收起 end-->
