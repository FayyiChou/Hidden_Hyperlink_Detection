		function loginSe() {
			//登录
			var password = $("#password").val();

			if (password == null || password == "") {
				alert("密码不能留空!!");
				document.location.reload();
				return false;
			}

			$("#idm").val(22);

			RSAUtils.setMaxDigits(200);
			//setMaxDigits(256);
			var key = new RSAUtils.getKeyPair("10001", "",
					"81a04f765c3aaa96484f693ca9136d35d63844328e686bef793ae8fcb2a9e2aeabb4ca4a2d5e6dfd725277f7ef0f98f29464cc702e5088817a9b8b58a43f961383d717cfad8cb197328f9199727ba6b97029069c01e1360dbd080a146cea970319f148f6ae63f424e71d308ddf1600382fea7f8754d08c9468ce68a90e15c1e9");
			var encrypedPwd = RSAUtils.encryptedString(key, password
					+ "@rpo3maMG");

			var count = 0;
			var isPwd = false;
			while ((isPwd = (encrypedPwd.length != 256))) {
				if (count > 2) {
					alert("密码加密失败,请点击确认后,重新输入.");
					//循环3次,不正确则弹出窗口,提示用户即将刷新页面
					location.reload(true);
					break;
				}
				encrypedPwd = RSAUtils.encryptedString(key, password
						+ "@rpo3maMG");
				count++;
			}

			if (!isPwd) {
				$("#password").val(encrypedPwd);
				$("#idm").val("4" + count);
			}

			return !isPwd;
		}
	</script>




</body></html>