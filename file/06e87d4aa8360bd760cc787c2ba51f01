		function resetUserame(obj) {
			if (obj.value == "") {
				//obj.value="username@gmail.com";
			}
			  if(obj.value.length>10){
				alert("用户名输入长度不能超过10位！");
				obj.value=obj.value.substring(0,10);
			}  
		}

