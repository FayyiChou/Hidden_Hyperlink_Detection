<script type="text/javascript">
function iEsc(){ return false; }
function iRec(){ return true; }
function DisableKeys() {
if(event.ctrlKey || event.shiftKey || event.altKey) {
window.event.returnValue=false;
iEsc();}
}
document.ondragstart=iEsc;
document.onkeydown=DisableKeys;
document.oncontextmenu=iEsc;
if (typeof document.onselectstart !="undefined")
document.onselectstart=iEsc;
else{
document.onmousedown=iEsc;
document.onmouseup=iRec;
}
function DisableRightClick(qsyzDOTnet){
if (window.Event){
if (qsyzDOTnet.which == 2 || qsyzDOTnet.which == 3)
iEsc();}
else
if (event.button == 2 || event.button == 3){
event.cancelBubble = true
event.returnValue = false;
iEsc();}
}
</script>        
    <script>
        jQuery(document).ready(function($) {
            $(".royalSlider").royalSlider({
                width: 1023,
                height: 570,
                keyboardNavEnabled: true,
                loop: true,
                imageScaleMode: 'fill',
                transitionSpeed: 300,
                autoPlay: {
                    enabled: true,
                    pauseOnHover: true,
                    delay: 5000,
                    stopAtAction: false
                }
            });

            var slider = $(".royalSlider").data('royalSlider');

            // set the background image of the first slide
            $('#wrap').css('background-image', 'url(' + slider.currSlide.image + ')')

            // after the slide change
            slider.ev.on('rsAfterSlideChange', function(event) {
                $('#wrap').css('background-image', 'url(' + slider.currSlide.image + ')')
                $('.fade').css('background-color', 'transparent');
            });
            // before slide
            slider.ev.on('rsBeforeMove', function(event, type, userAction) {
                $('.fade').css('background-color', 'rgba(255, 255, 255, 0.8)');
            });
        });
    </script>
