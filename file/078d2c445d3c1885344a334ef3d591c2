<script language="JavaScript" type="text/javascript"> 
var speed=50 
demo5.innerHTML=demo4.innerHTML 
function Marquee(){ 
if(demo5.offsetTop-demoo.scrollTop<=0) 
demoo.scrollTop-=demo4.offsetHeight 
else{ 
demoo.scrollTop++ 
} 
} 
var MyMar=setInterval(Marquee,speed) 
demoo.onmouseover=function() {clearInterval(MyMar)} 
demoo.onmouseout=function() {MyMar=setInterval(Marquee,speed)} 
  </script>
