            <td width="22%" align="center"><script language="javascript" type="text/javascript">
function Time(){ 
	var d1 = new Date();
	var d2 = new Date(2017,5,19);
	var d3 = new Date(1970,1,1);
	var t = (( d2.getTime() - d1.getTime() - d3.getTime() - 8*3600*1000 ) / 1000)/(60*60*24);
	if( t > 0 )
	{
		document.getElementById('mtime').innerHTML = ( t ).toFixed( 0 ) + "";
		setTimeout( 'Time()',100 );
	}
	else
	{
		document.getElementById('mtime').innerHTML = "";
		splash();
	}
}

function splash()
{
	obj = document.getElementById('mtime');
	if( obj.style.display == 'none')
		obj.style.display = 'block';
	else
		obj.style.display = 'none';
	setTimeout('splash()',400 );
}
setTimeout( 'Time()',100 );
            </script>
