!function(){	// 更多文字显示隐藏处理
	function moreinit() {
		var $morezoneys = $('.footer-toper .ys-links');
		var $morezoneot = $('.footer-toper .other-links');
		$morezoneys.mouseenter(function() {
			$morezoneys.addClass('links-zone-hover');
		});
		$morezoneot.mouseenter(function() {
			$morezoneot.addClass('links-zone-hover');
		});
		$morezoneys.mouseleave(function() {
			$morezoneys.removeClass('links-zone-hover');
		});
		$morezoneot.mouseleave(function() {
			$morezoneot.removeClass('links-zone-hover');
		});
	}
	moreinit();
	// 跨站资源统计
	(function (doc) {
		var s = doc.createElement('script');
		s.src = 'http://s0.hao123img.com/res/js/fe/cspalog.js?t=' + (+new Date);
		var parent = doc.getElementsByTagName('script')[0].parentNode;
		parent.appendChild(s);
	})(document);
