		<script type="text/javascript">
			$(document).ready(function(){
				function contentScroll(){
					$('.shBox-scroll').jScrollPane();
					};
					$(function() {
					setTimeout(contentScroll,100);
					$(window).resize(function(){ setTimeout(contentScroll,100)});
				});

				function check_resize_window() {
					var min = 140;
					var max = 1450;
					var min_scroll_horizontal = 1000;
					
					var page_width = $(window).width();

					if (page_width < max) {
						var margin_left = (max - page_width);
					} else {
						var margin_left = 0;
					}
					
					if (page_width < min_scroll_horizontal) {
						$('#wrapper').css('overflow-x', 'auto');
					} else {
						$('#wrapper').css('overflow-x', 'hidden');
					}

					if (margin_left > min) {
						margin_left = min;
					}
					//$('#resize_check').html(page_width+', '+margin_left);
					//$('#background').css('margin-left', '-'+margin_left+'px');
					$('#content').css('background-position', '-'+margin_left+'px 0px');
				}

				check_resize_window();
				$(window).bind('resize', check_resize_window);
				
				$(".elevatezoom").elevateZoom({ 
					zoomType : "lens",  
					lensSize : 200
				});
				
				$('#langCurrent').mouseover(function(){
					$('#languages').css('display', 'block');
				});
				
				$('#languages').mouseleave(function(){
					$('#languages').css('display', 'none');
				});
				
				/*$('a[id^=lang_]').click(function(){
					$('#languages').css('display', 'none');
					var lang = this.id.split('_');
					var url_lang = 'http://'+document.domain+'/'+lang[1];
					//var url_lang = window.location.href+'/'+lang[1];
					//alert(url_lang);
					window.location.href = url_lang;
				});*/
				
				return false;
			});
		</script>

