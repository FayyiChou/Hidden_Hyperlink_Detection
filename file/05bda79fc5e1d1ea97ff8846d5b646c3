var speed=16
demoo2.innerHTML=demoo1.innerHTML
function Marquee1(){
if(demoo2.offsetWidth-demoo.scrollLeft<=0)
demoo.scrollLeft-=demoo1.offsetWidth
else{
demoo.scrollLeft++
}
}
var MyMar1=setInterval(Marquee1,speed)
demoo.onmouseover=function() {clearInterval(MyMar1)}
demoo.onmouseout=function() {MyMar1=setInterval(Marquee1,speed)}
