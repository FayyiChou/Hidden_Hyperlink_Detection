function getExplorer() {
	var explorer = 'Mozilla/5.0 (Windows NT 6.1) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/31.0.1650.48 Safari/537.36' ;
	if(explorer.indexOf("2345Explorer") <0){
		var url = "/usersIpAction/ipFlag.do";
		$.ajax({
			type: "POST",
			async:false,
			url: url,
			success: function(data){
				if(data != "1"){
					document.getElementById("ad").style.display = "block"; 
				}
			}
		});
	}
}

function addUserip(){
	//document.getElementById("ad").style.display = "block"; 
	var url = "/usersIpAction/addUsersIp.do";
	$.ajax({
		type: "POST",
		url: url,
		success: function(data){
				document.getElementById("ad").style.display = "none"; 
		}
	});
}

function doSubmitLogin(){
	try{
		var phone = document.getElementById("loginNameForLogin").value;
		var password = document.getElementById("passWordForLogin").value;
		var yzm = document.getElementById("veriryCode").value;
		if(phone == null || phone==""){
			alert('请输入手机号码!');
			return false;
		}
		if(password == null || password==""){
			alert('请输入正确的密码!');
			return false;
		}
		if(yzm== null || yzm==""){
			alert('请输入验证码!');
			return false;
		}
	}catch(e){
		alert(e);
	} 
	return true;
}

function reloadVerifyImg(){	
	document.getElementById("validCode").src="/imgCode?a="+Math.random();
}

function reloadVerifyRegImg(){
	document.getElementById("validRegCode").src="/regimgCode?a="+Math.random();
}

window.onload = function(){
	if(window.top == window.self){
					
					// 刷新验证码，确保无缓存
					reloadVerifyImg();
					reloadVerifyRegImg();
					document.getElementById("loginNameForLogin").focus();
					
	}else{
		window.parent.location.href="index.jsp";
	}
}
