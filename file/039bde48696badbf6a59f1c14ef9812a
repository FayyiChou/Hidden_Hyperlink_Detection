<!-- 注册 start -->
<script type="text/javascript">

/**
 * 限制只能输入数字
 */
function keyPressNum() {
    var keyCode = event.keyCode;  
    if ((keyCode >= 48 && keyCode <= 57)){
        event.returnValue = true;
    } else {  
        event.returnValue = false;
    }  
}

function checkLoginNamePhone(){
	var loginNameIsPass = false;
	var loginName = document.getElementById("loginName");
	document.getElementById("custSmsCodeTip").innerHTML ="";
	if(loginName.value == null || loginName.value==""){
		loginName.focus();
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("custSmsCodeTip").innerHTML ="手机号不能为空！";
		document.getElementById("loginName").className="tipInput";
		return loginNameIsPass;
	}
	if(loginName.value.length != 11){
		loginName.focus();
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("custSmsCodeTip").innerHTML = "请填写11位手机号！";
		document.getElementById("loginName").className="tipInput";
		return loginNameIsPass;
	}
	
	if(!fucCheckTEL(loginName.value)){
		loginName.focus();
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("custSmsCodeTip").innerHTML = "请输入正确的手机号！";
		document.getElementById("loginName").className="tipInput";
		return loginNameIsPass;
	}
	var url = "/customerAcion/checkIsExist.do?checkName="+loginName.value;
	url=encodeURI(url);
	url=encodeURI(url);
	$.ajax({
		type: "POST",
		async:false,
		url: url,
		data: "checkName="+ escape(loginName.value),
		dataType: 'text',
		contentType: "application/x-www-form-urlencoded; charset=utf-8",
		success: function(data){
			if(data != null && data == "true"){
				loginName.focus();
				disabled = "true";
				document.getElementById("custSmsCodeTip").style.display = "block";
				document.getElementById("custSmsCodeTip").innerHTML = "手机号已经被注册！";
				document.getElementById("loginName").className="tipInput";
				loginNameIsPass = false;
			}else{
				document.getElementById("loginName").className="";
				document.getElementById("custSmsCodeTip").style.display = "block";
				document.getElementById("custSmsCodeTip").innerHTML = "输入正确，该手机号码可以注册";
				loginNameIsPass = true;
			}
		}
	});
	return loginNameIsPass;
}


	//是否同意协议
function checkProtocol(obj){
	if(obj.checked){
		document.getElementById("submitBtn").disabled = "";
	}
	else{
		document.getElementById("submitBtn").disabled = "disabled";
	}
}

// 检查重复密码
function checkPwd2(){
	var passWord = document.getElementById("passWord");
	var passWord2 = document.getElementById("passWord2");
	document.getElementById("custSmsCodeTip").innerHTML = "";
	if(passWord.value != passWord2.value)
	{
		document.getElementById("custSmsCodeTip").innerHTML ="两次输入的密码不一致！";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("passWord2").className="tipInput";
		passWord2.focus();
		return false;
	}
	if(passWord.value.length<6)
	{
		document.getElementById("custSmsCodeTip").innerHTML ="密码长度不能小于6位!";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("passWord2").className="tipInput";
		passWord.focus();
		return false;
	}
	if(passWord.value.length>18)
	{
		document.getElementById("custSmsCodeTip").innerHTML = "密码长度不能大于18位!";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("passWord2").className="tipInput";
		passWord.focus();
		return false;
	}
	document.getElementById("passWord2").className="";
	document.getElementById("custSmsCodeTip").innerHTML = "";
	return true;
}
// 检查密码
function checkPwd(){
	var passWordIsPass = false;
	var loginName = document.getElementById("loginName");
	var passWord = document.getElementById("passWord");
	if(passWord.value==""){
		passWord.focus();
		document.getElementById("custSmsCodeTip").innerHTML = "请输入密码！";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("passWord").className="tipInput";
		return passWordIsPass;
	}else if(passWord.value==loginName.value){
		document.getElementById("custSmsCodeTip").innerHTML = "密码和登录名不能相同！";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("passWord").className="tipInput";
		passWord.focus();
		passWord.value= "";
		return passWordIsPass;
	}
	if(passWord.value.length < 6){
		document.getElementById("custSmsCodeTip").innerHTML = "密码长度必须大于6位！";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("passWord").className="tipInput";
		passWord.focus();
		return passWordIsPass;
	}
	if(passWord.value.length >18){
		document.getElementById("custSmsCodeTip").innerHTML = "密码长度必须小于18位！";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("passWord").className="tipInput";
		passWord.focus();
		return passWordIsPass;
	}
	document.getElementById("passWord").className="";
	document.getElementById("custSmsCodeTip").innerHTML = "";
	return true;
}

/**
*手机号验证
*/
function fucCheckTEL(phone){
	if (phone.search(/^(((13[0-9]{1})|(14[0-9]{1})|(15[0-9]{1})|(17[0-9]{1})|(18[0-9]{1}))+\d{8})$/) != -1){
		return true;
	}else{    
		return false;
	}
}

/**
 * 提交注册
 */
function doSubmit(){
	if(!checkLoginNamePhone()){
		return;
	}
	var custSmsCode = document.getElementById("custSmsCode").value;
	if(custSmsCode == null || "" == custSmsCode){
		document.getElementById("custSmsCodeTip").innerHTML = "请先完成短信验证";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("custSmsCode").className="tipInput";
		return;
	}
	var smsCodeFlag = document.getElementById("smsCodeFlag").value;
	if(smsCodeFlag == "0"){
		document.getElementById("custSmsCodeTip").innerHTML = "请先完成短信验证";
		document.getElementById("custSmsCodeTip").style.display = "block";
		document.getElementById("custSmsCode").className="tipInput";
		return;
	}
	if(!checkPwd()){
		return;
	}
	if(!checkPwd2()){
		return;
	}
	var checkbox = document.getElementById("checkbox");
	if(!checkbox.checked){
		document.getElementById("custSmsCodeTip").innerHTML = "请阅读并同意注册协议！";
		document.getElementById("custSmsCodeTip").style.display = "block";
		return;
	}
	document.getElementById("submitBtn").disabled = "disabled";
	actionFormReg.submit();
}
</script>
<!-- 注册 end -->

<!-- 发送短信 start -->
<script type="text/javascript">
var InterValObj; //timer变量，控制时间  
var count = 60; //间隔函数，1秒执行  
var curCount;//当前剩余秒数  
var code = ""; //验证码  
var codeLength = 6;//验证码长度  

function sendSmsCode(){
   	curCount = count;
    var jbPhone = document.getElementById("loginName").value;
    var jbPhoneTip = document.getElementById("custSmsCodeTip").innerHTML;
    if(jbPhoneTip=="亲,您的手机号码一天内获取验证码次数超过5次"){
    	return "";
    }
    if(jbPhoneTip == "输入正确，该手机号码可以注册"){
    	document.getElementById("custSmsCodeTip").innerHTML = "";  
    }
    if (checkLoginNamePhone() && "" != jbPhone) {
    	var veriryCodeReg = '1234';
		if(veriryCodeReg == null || "" == veriryCodeReg){
			document.getElementById("custSmsCodeTip").innerHTML = "请先输入验证码！";
			return;
		}else{
			document.getElementById("custSmsCodeTip").innerHTML = "输入正确，该手机号码可以注册";
		}
		jbPhoneTip = document.getElementById("custSmsCodeTip").innerHTML;
        if(jbPhoneTip == "输入正确，该手机号码可以注册" || jbPhoneTip == "短信验证码已发到您的手机,请查收"){
            // 产生验证码
            code = ""; 
            for ( var i = 0; i < codeLength; i++) {
                code += parseInt(Math.random() * 9).toString();
            }
           // 向后台发送处理数据  
           $.ajax({
               type: "POST", // 用POST方式传输
               async:false,
               dataType: "text", // 
               url: "/smsAction/sendCode.do", // 目标地址
               data: "jbPhone=" + jbPhone +"&code=" + code+"&unCheckVeriryCode=1&veriryCodeReg="+veriryCodeReg,
               success: function (data){
                   data = parseInt(data, 10);  
                   if(data == 1){  
                	   document.getElementById("custSmsCodeTip").style.display = "block";
                       document.getElementById("custSmsCodeTip").innerHTML = "短信验证码已发到您的手机,请查收";  
                       InterValObj = window.setInterval(SetRemainTime, 1000); // 启动计时器，1秒执行一次
                       //设置button效果，开始计时
			           document.getElementById("getSmsCodeBtn").disabled = "disabled";
			           document.getElementById("getSmsCodeBtn").value = curCount + "秒后可重新获取";
                   }else if(data == 0){ 
                	   document.getElementById("custSmsCodeTip").style.display = "block";
                       document.getElementById("custSmsCodeTip").innerHTML = "短信验证码发送失败,请重新发送";  
                  }else if(data == 2){
                	   document.getElementById("custSmsCodeTip").style.display = "block";
                       document.getElementById("custSmsCodeTip").innerHTML = "亲,您的手机号码一天内获取验证码次数超过5次";  
                  }else if(data == 3){
                	   document.getElementById("custSmsCodeTip").style.display = "block";
                       document.getElementById("custSmsCodeTip").innerHTML = "亲,您的手机号码获取验证码过于频繁！";
                  }else if(data == 4){
                	   document.getElementById("custSmsCodeTip").style.display = "block";
                       document.getElementById("custSmsCodeTip").innerHTML = "请先输入正确的验证码！";
                  }
               }  
           });  
       } 
    }else{
       //document.getElementById("custSmsCodeTip").innerHTML = "请填写正确的手机号";
       document.getElementById("custSmsCodeTip").style.display = "block";
       document.getElementById("loginName").className="tipInput";
    }  
}
 
//timer处理函数  
function SetRemainTime() {  
    if (curCount == 0) {                  
        window.clearInterval(InterValObj);// 停止计时器  
        // 启用按钮  
        document.getElementById("getSmsCodeBtn").disabled = "";
        document.getElementById("getSmsCodeBtn").value =  "重新获取验证码";
       	code = ""; // 清除验证码。如果不清除，过时间后，输入收到的验证码依然有效
    }else{
       curCount--;  
       document.getElementById("getSmsCodeBtn").value = curCount + "秒后可重新获取";
   }  
}  
  
function checkSms(){
    var SmsCheckCodeVal = document.getElementById("custSmsCode").value;
     // 向后台发送处理数据  
     if(SmsCheckCodeVal != null || "" != SmsCheckCodeVal){
     	$.ajax({
	       url : "/smsAction/checkCode.do",
	       data : "smsCheckCode="+SmsCheckCodeVal,
	       type : "POST",   
	       dataType : "text",   
	       success : function(data) {
	            data = parseInt(data, 10);
	             if (data == 1) {  
	               document.getElementById("custSmsCodeTip").innerHTML = "短信验证码正确，请继续"; 
	               document.getElementById("custSmsCodeTip").style.display = "block";
	               document.getElementById("getSmsCodeBtn").disabled = "disabled";
	               document.getElementById("smsCodeFlag").value = "1";
	            } else {
	                document.getElementById("custSmsCodeTip").innerHTML = "短信验证码有误，请核实后重新填写";
	                document.getElementById("custSmsCodeTip").style.display = "block";
	                document.getElementById("custSmsCode").className="tipInput";
	            }
	         }  
	    });
     }
}
</script>
<!-- 发送短信 end -->

<script type="text/javascript">
var error = "";
if(error != null && error != ''){
	alert(error);
}

var success = "";
if((error == null || error == '') && success != null && success != ''){
	alert(success);
}
</script>


