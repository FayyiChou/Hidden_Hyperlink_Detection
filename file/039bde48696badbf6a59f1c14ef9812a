<script type="text/javascript">

	var id=function(o){
		return document.getElementById(o);
	};
	var scroll=function (o){
		var space=id(o).offsetTop;
		id(o).style.top=space+'px';
		void function(){
			var goTo = 0;
			var roll=setInterval(function(){
				var height =document.documentElement.scrollTop+document.body.scrollTop+space-320;
				var top = parseInt(id(o).style.top);
				if(height!= top){
					goTo = height-parseInt((height - top)*0.9);
					id(o).style.top=goTo+'px';
				}
			},100);
		}();
	};
scroll('game');
</script>

<script type="text/javascript">
	function showGameInfo(id){
		if(id == "royal"){
			//皇室战争
			document.getElementById("royal").style.display = "block";
			document.getElementById("royaljiao").style.display = "block";
			document.getElementById("running").style.display = "none";
			document.getElementById("runningsanjiao").style.display = "none";
		}else{
			//奔跑吧兄弟
			document.getElementById("running").style.display = "block";
			document.getElementById("runningsanjiao").style.display = "block";
			document.getElementById("royal").style.display = "none";
			document.getElementById("royaljiao").style.display = "none";
		}
	}
	
	function hiddeGameInfo(id){
		document.getElementById(id).style.display = "none";
	}
</script>

