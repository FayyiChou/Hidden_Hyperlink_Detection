		function chanTxt(i){
			var f 	= $('fastList'),
				t  	= $('talkBox'),
				l 	= f.getElementsByTagName('li');
				var txt = l[i].innerHTML;
				t.value = txt;
		}
	</script>
<script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery(".down_up_btn").toggle(function(){
        jQuery(this).parent().find(".autograph_div").css({height:"auto"});
			  jQuery(this).find("i").css({background:"url(http://oss-attachments.55-img.com/newbbs/images/seeall_btn.png)no-repeat center right"})
       }, function(){
        jQuery(this).parent().find(".autograph_div").css({height:60});
			  jQuery(this).find("i").css({background:"url(http://oss-attachments.55-img.com/newbbs/images/seeall_btn1.png)no-repeat center right"})

    });

    /*
		jQuery(".down_up_btn").click(function() {
			jQuery(".autograph_div").slideToggle();
		});
		*/
		jQuery("#goTop").click(function() {
			jQuery('body,html').animate({
				scrollTop : 0
			}, 300);
		});
		jQuery(window).scroll(function() {
			var scroll_top = jQuery(document).scrollTop();
			if (scroll_top >= 500) {
				jQuery(".stick_top").show();
			} else if (scroll_top <= 500) {
				jQuery(".stick_top").hide();
			}
		});
		var r_item_h = jQuery(".card_content_main_right").height(),
			 jl_top_h = Math.abs(jQuery(".card_content_main_right").offset().top),
			 windowh = jQuery(window).height(),
			 main_h = jQuery(".card_content_main").height(),
		main_top_h = jQuery(".card_content_main").offset().top;

    jQuery(window).scroll(function(){
      var winscrolltop = jQuery("body").scrollTop();
        if(r_item_h+jl_top_h <= windowh){
          if(winscrolltop >= (windowh + jl_top_h) &&  winscrolltop <= (main_h+main_top_h)){
        		jQuery(".card_content_main_right").css({position:"fixed",left:"50%",bottom:"0px",marginLeft:"202px"});
          }else{
						jQuery(".card_content_main_right").css({position:"",marginLeft:"0px"});
					}
        }else{
          if(winscrolltop >= (r_item_h-windowh+jl_top_h)  &&  winscrolltop <= (main_h+main_top_h-windowh)){
		            jQuery(".card_content_main_right").css({position:"fixed",left:"50%",bottom:"0px",marginLeft:"202px"});
		      }else{
				    jQuery(".card_content_main_right").css({position:"",marginLeft:"0px"});
				  }
			 }
		 });

		// for(var ii=1;ii<=20;ii++){
			// jQuery(".user_phone").mouseover(function() {
			// 	jQuery(".user_pop").show();
			//
			// });
			// jQuery(".user_pop").mouseleave(function() {
			// 	jQuery(".user_pop").hide();
			// });

		// }
/*
jQuery(".user_img1").mouseover(function(){
	jQuery(this).find(".user_pop").show();
});
jQuery(".user_img1").mouseleave(function(){
	jQuery(this).find(".user_pop").hide();
});
*/
jQuery(".louzhuinfo").mouseover(function(){

	jQuery(this).find(".user_pop").css({top:"-276px", left:"-156px"});
	jQuery(this).find(".user_pop").show();
});
jQuery(".louzhuinfo").mouseleave(function(){
	jQuery(this).find(".user_pop").hide();
});

jQuery(".user_center_name_f").mouseover(function(){

	jQuery(this).find(".user_pop").css({top:"-275px",left:"-168px"});
	jQuery(this).find(".user_pop").show();

});


jQuery(".user_center_name_f").mouseleave(function(){
jQuery(this).find(".user_pop").hide();
});

		jQuery(".up_down_btn_pop").toggle(function() {
			jQuery(".up_down_btn_pop").removeClass("up1").addClass("down1")
			jQuery(".looks_div").animate({
				height : '120px'
			})
		}, function(){
			jQuery(".up_down_btn_pop").addClass("up1").removeClass("down1")
			jQuery(".looks_div").animate({
				height : '60px'
			})
