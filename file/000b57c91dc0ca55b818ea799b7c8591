<script>
window.onload = function() {

        $("#use1").addClass("choosed");
        $("#use2").removeClass("choosed");
        $("#use3").removeClass("choosed");

        $("#use1").hover(function(){
            $("#use1").addClass("choosed");
            $("#use2").removeClass("choosed");
            $("#use3").removeClass("choosed");
        });
        $("#use2").hover(function(){
            $("#use2").addClass("choosed");
            $("#use1").removeClass("choosed");
            $("#use3").removeClass("choosed");
        });
        $("#use3").hover(function(){
            $("#use3").addClass("choosed");
            $("#use2").removeClass("choosed");
            $("#use1").removeClass("choosed");
        });

        $("#txt").keyup(function () {
            if ($.trim($(this).val()) != '') {
                $("#adv-sub").addClass("logined");
            } else {
                $("#adv-sub").removeClass("logined");
            }
        });


        $("#adv-sub").click(function () {
            $("#s1").hide();
            var txt1 = $.trim($("#txt").val());
            var type = $("#used").val();
            var formhash = $("#formhash").val();
            if (!formhash) {
                alert('请填写内容');
                return false;
            }
            if (txt1 != '') {
                $.ajax({
                    type: 'POST',
                    url: '/do.php?ac=feedback',
                    async: false,
                    data: {'type': type, 'formhash': formhash, 'msg': txt1},
                    success: function (a) {
                        if (a == -1) {
                            alert('请填写内容');
                            return false;
                        } else if (a == -2) {
                            alert('请登录');
                            return false;
                        } else if (a == 1) {
                            $("#s1").show();
                            setTimeout(function () {
                                $(".user-back-box").hide();
                                $("#s1").hide();

                                $("#txt").val('');
                                $("#adv-sub").removeClass("logined");
                                $("#use1").addClass("choosed");
                                $("#use2").removeClass("choosed");
                                $("#use3").removeClass("choosed");
                            }, 3000);
                        } else {
                            return true;
                        }
                    }
                });
            }
        });
        $(".user-closed").click(function(){
            $(".user-back-box").hide();
        })
    };
</script>
