<style>label.error{font:500 12px/14px microsoft YaHei;color:#ff0000;border:none;float:none;margin:0 auto;display:block;width:315px;}.btn-disable{background-color:#cbc9c9;cursor:auto;border-radius:5px;}label#mobile_reg_username-error{display:block;float:none;}</style>
<script type="text/javascript" src="http://img.glr.cn/js/jquery.validate.min.js"></script>
<script type="text/javascript" src="http://img.glr.cn/js/additional-methods.js"></script>
<script>
    jQuery(document).ready(function($){
        $(".validateCodeWrap input").blur(function () {
            var code_val = $(".validateCodeWrap input").val();
            if(code_val == "") {
                alert("请先输入图片验证码");
                $("#identifying_code").css("disabled","true");
                return false;
            }
            var data={"new_code":code_val};
            var url="/source/captcha.php?ac=code";
            $.post(url,data,function(data){
                if(data.flag){
                    /*alert("OK");*/
                    $(".validateCodeWrap p").css("display","none");
                    $(".validateCodeWrap input").css("border","1px solid #ddd");
                    /*$("#mobile_regSubmit").removeAttr("disabled");*/
                }else{
                    /*alert(data.msg);*/
                    $(".validateCodeWrap p").css("display","block");
                    $(".validateCodeWrap input").css("border","1px solid #FF0400");
                    document.getElementById('img').src='./source/captcha.php?'+Math.random();
                    /*$("#mobile_regSubmit").attr("disabled",true);*/
                }
            },"json");
        });
        $.validator.addMethod("registerUsername", function(value, element) {
            var flag=false;
            $.ajax({
                type:"post",
                dataType:"json",
                async:false,
                url:'/do.php?ac=register&op=register_username',
                data:{"username":value},
                success: function(data){
                    if(data.flag){
                        flag=true;
                    }
                }
            });
            return flag;
        }, "该手机号已绑定");
        $("#mobile_regForm").validate({
            errorPlacement: function(error, element) {
                error.insertAfter(element.parent());
            },
            submitHandler:function(){
            	var mobile=/^(((13[0-9]{1})|(15[0-9]{1})|(17[0-9]{1})|(18[0-9]{1}))+\d{8})$/;
                var username=$.trim($("#mobile_reg_username").val());
                if(mobile.test(username)){
                	var data={
                			'type':"mobile",
                			'mobile':username,
                			"code":$("#identifying_code").val(),
                			"password":$("#mobile_reg_password").val(),
                	};
            	}else{
                    var data={
                    		'type':'email',
                    		"email":username,
                    		"password":$("#mobile_reg_password").val(),
                    }
            	}
                $.post("/do.php?ac=register&op=register",data,function(data){
                    if(data.flag){
                        window.location.href="/";
                    }else{
                        alert(data.msg);
                    }
                },"json");

            },
            rules:{
                username:{
                    required:true,
                    Username:true,
                    registerUsername:true,
                },
                psd:{
                    required:true,
                    minlength:6,
                    maxlength:16
                },
                identifying_code:{
                    required:true,
                },
                mobile_ckb:{
                    required:true,
                }

            },
            messages:{
                username:{
                    required:"用户名必填",
                    Username:'请输入正确的手机号或邮箱！',
                    registerUsername:"手机号或者邮箱已经注册，请登录",
                },
                psd:{
                    required:"用户密码必填",
                    minlength:"密码由6-16位字母数字组成！",
                    maxlength:"密码由6-16位字母数字组成！"
                },
                identifying_code:{
                    required:"验证码错误！",
                },
                mobile_ckb:{
                    required:"请接受服务条款！"
                }

            }
        });
        

        //发送验证码倒计时
        var wait = 60; //时间
        $(".btn_send").removeAttr('disabled');
        function time(o, p) {
            if (wait == 0) {
                o.removeAttr("disabled");
                o.val("点击重新发送");
                p.html("如果您在1分钟内没有收到验证码，请检查您填写的手机号码是否正确或重新发送");
                wait = 60;
            } else {
                o.attr("disabled", true);
                o.val(wait + "秒后重新获取");
                wait--;
                setTimeout(function() {
                            time(o, p); //循环调用
                        },
                        1000)
            }
        }
        $(".btn_send").click(function(){
            var code_val = $(".validateCodeWrap input").val();
            if(code_val == "") {
                alert("请先输入图片验证码");
                return false;
            }
            var mobile=$("#mobile_reg_username").val();
            var hash=$("#mobile_regForm").data("hash");
            $.post("/do.php?ac=register&op=send_code",{"mobile":mobile,"hash":hash,"new_code":code_val},function(data){
                if(data.flag == 1){
                    time($(".btn_send"),$(".msg"));
                }else if(data.flag == 2){
                    $(".validateCodeWrap p").css("display","block");
                    document.getElementById('img').src='./source/captcha.php?'+Math.random();
                } else {
                    alert(data.msg);
                }
            },"json");

        })


    })
</script>
 
