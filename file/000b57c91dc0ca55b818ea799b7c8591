<script type="text/javascript" defer src="http://img.glr.cn/2014topu/js/head.js?20170113"></script>
<script>
  function check_login_submit() {
   var falg = 0;
   var username = $('#loginform').find('input[name=username]').val();
   var password = $('#loginform').find('input[name=password]').val();
   var cookietime = $('#loginform').find('input[name=cookietime]').val();
   var span_error = $('#span_error');
   var callback_url=$("#callback_url").val();
   if(username.indexOf(" ")>=0) {
       span_error.html("账户不存在");
       $('#loginform').find('input[name=username]').focus();
       falg = -1;
   }
   if(username.length == 0 || username == '邮箱/用户名/手机号'){
    span_error.html("请输入邮箱或用户名");
    $('#loginform').find('input[name=username]').focus();
    falg = -1;
   }else if(password.length == 0){
    span_error.html("请输入密码");
    $('#loginform').find('input[name=password]').focus();
    falg = -1;
   }else if(falg == 0){
    $('#loginsubmit').val("正在登录...");
    $.ajax({
      type:'POST',
      url: "/do.php?ac=83cab67b810879e7e6e93b3244d4696b&op=ajaxlogin",
      data: {'username':username,'password':password},
      async: false,
      success: function(a){
        var a = parseInt(a);
    	$('#loginsubmit').val("正在登录...");
    	if(a== '-1'){
falg = -1;
$('#loginform').find('input[name=password]').focus();
        $('#span_error').html("账号或密码不正确，请重新输入");
        $('#loginsubmit').val("登录");				
}else if(a== '-2'){
falg = -1;
$('#loginform').find('input[name=password]').focus();
        $('#span_error').html("帐号不存在");
        $('#loginsubmit').val("登录");		
}else{
falg = 1;					
}			
        
        
      }
    });
   }
   if(falg == 0 || falg==-1){
return false;
}else{
return true;
}
  }
</script>
