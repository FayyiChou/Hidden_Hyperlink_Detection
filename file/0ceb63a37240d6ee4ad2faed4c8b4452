        var parSlideIndex_d2_1479_2a248_PSlider_=0;
        var parSlidePlay_d2_1479_2a248_PSlider_=true;
        var parSlideCount_d2_1479_2a248_PSlider_=0;
        var parSlideInterval_d2_1479_2a248_PSlider_=1000;
        var parSliderTimeId_d2_1479_2a248_PSlider_;
        var sliderMode_d2_1479_2a248_PSlider_=1;
        var parSlideDelay_d2_1479_2a248_PSlider_=4;
        function parViewSlide_d2_1479_2a248_PSlider_() //初始化
        {
		    parSlideCount_d2_1479_2a248_PSlider_ = 0;
		    try
		    {
			    pa_d2_1479_2a248_PSlider_.filters[0].Stop();
                //如果当前动画为随机则触发随机切换的效果
                if(sliderMode_d2_1479_2a248_PSlider_==10)
                {
                   pa_d2_1479_2a248_PSlider_.style.filter=changeFilter();
                }           
			    pa_d2_1479_2a248_PSlider_.filters[0].Apply();
		    }
		    catch(e) {}
            var i;
            for (i = 0; i < 1; i++)
            {
                var dv = document.getElementById('pa_d2_1479_2a248_PSlider__img' + i);
                if(dv!=null)
                {
                    if (i == parSlideIndex_d2_1479_2a248_PSlider_)
                    {
                        dv.style.display = '';
                    }
                    else
                    {
                        dv.style.display = 'none';
                    }
                }
            }
