<script>
    var curPage = 1;
    function togglePage(no) {
        $('.photo-group').addClass('hide');
        $('.block').removeClass('active');
        $('.photo' + no).removeClass('hide');
        $('.p' + no).addClass('active');
        curPage = parseInt(no);
    }
    function nextPage() {
        if (curPage !== 4) {
            curPage += 1;
            $('.photo-group').addClass('hide');
            $('.block').removeClass('active');
            $('.photo' + curPage).removeClass('hide');
            $('.p' + curPage).addClass('active');
        }
    }
    function prevPage() {
        if (curPage !== 1) {
            curPage -= 1;
            $('.photo-group').addClass('hide');
            $('.block').removeClass('active');
            $('.photo' + curPage).removeClass('hide');
            $('.p' + curPage).addClass('active');
        }
    }
</script>

