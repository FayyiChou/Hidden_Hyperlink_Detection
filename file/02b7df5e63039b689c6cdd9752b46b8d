function togglepage(){
	$('#header,#content,#footerdiv').hide();
	$('html,body').scrollTop(0);
	$('#read_all').show();
	 
	 $("body").css('background', '#F1F1F1');
	 return false;
	 
}

function togglepageback() {
	$('html,body').scrollTop(0);
	$('#read_all').hide();
	$('#roll_div').hide();
