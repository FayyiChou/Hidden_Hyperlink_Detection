    <script type="text/javascript" language="javascript">
	$(function(){
		btn_header_down();
	})
	function btn_header_down(){
            $("#btn_header").bind("click",function(){
                if($(this).hasClass("btn_header_down")) {
                    $(this).removeClass("btn_header_down");
                    if($(".ad_001").css("display") == "block"){
                        $(".top_box_two").animate({height:"155px"});
                        $(".ad_001,.path").fadeIn(200);
                    }else{
                        $(".top_box_two").animate({height:"195px"});
                        $(".ad_001_big,.path").fadeIn(200);
                    }
                } else {
                    $(this).addClass("btn_header_down");	
                    $(".top_box_two").animate({height:"35px"});
                    $(".ad_001,.path").fadeOut(200);
                    $(".ad_001_big,.path").fadeOut(200);
                }
            })
	 }
    </script>
