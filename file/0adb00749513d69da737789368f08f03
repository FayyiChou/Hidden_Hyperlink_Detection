    window.onscroll = function () {
        t = document.documentElement.scrollTop || document.body.scrollTop;
        if (t >= 300) {
            $(".backtop").show();
        } else if (t < 300) {
            $(".backtop").hide();
        }
    }
    function scrolltop() {
        $("html, body").animate({ scrollTop: 0 }, 1000);
    }
