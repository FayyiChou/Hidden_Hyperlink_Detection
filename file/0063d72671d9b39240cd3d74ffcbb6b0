                <%}else if(model.value ==2){%>
                拍主昵称
                <%}else if(model.value == 0){%>
                已结束
                <%}%>
                <span class="s_icon_down arrow"></span>
            </a>
            <%}else if(model && model.area == 'dianpu'){%>
            <a href="javascript:;" class="option" data-value ="<%=model.value%>">
                <%if(model.value ==0){%>店铺名称<%}else if(model.value ==1){%>店主昵称<%}%>
