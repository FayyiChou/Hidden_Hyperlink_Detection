            <%if(model && model.area == 'shop'){%>
            <a href="javascript:;" class="option" data-value ="<%=model.value%>">
                <%if(model.value ==0){%>在售<%}else if(model.value ==1){%>已售<%}else if(model.value ==2){%>全部<%}%>
                <span class="s_icon_down arrow"></span>
            </a>
            <%}else if(model && model.area == 'pm'){%>
            <a href="javascript:;" class="option" data-value ="<%=model.value%>">
                <%if(model.value ==1){%>
                拍品名称
                <%}else if(model.value ==3){%>
