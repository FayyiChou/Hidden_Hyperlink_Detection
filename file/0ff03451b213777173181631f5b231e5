demo6.scrollTop-=demo61.offsetHeight //demo6跳到最顶端 
else{ 
demo6.scrollTop++ 
}
} 
var MyMar6=setInterval(Marquee6,speed6)//设置定时器 
//鼠标移上时清除定时器达到滚动停止的目的 
demo6.onmouseover=function() {clearInterval(MyMar6)} 
//鼠标移开时重设定时器 
demo6.onmouseout=function(){MyMar6=setInterval(Marquee6,speed6)} 
</SCRIPT>
</DIV>
