<DIV id=demo12></DIV>
<SCRIPT> 
var speed1=30
demo12.innerHTML=demo11.innerHTML //克隆demo11为demo12 
function Marquee1(){ 
//当滚动至demo11与demo12交界时 
if(demo12.offsetTop-demo1.scrollTop<=0) 
demo1.scrollTop-=demo11.offsetHeight //demo1跳到最顶端 
else{ 
demo1.scrollTop++ 
}
} 
var MyMar1=setInterval(Marquee1,speed1)//设置定时器 
//鼠标移上时清除定时器达到滚动停止的目的 
demo1.onmouseover=function() {clearInterval(MyMar1)} 
//鼠标移开时重设定时器 
demo1.onmouseout=function(){MyMar1=setInterval(Marquee1,speed1)} 
</SCRIPT>
</DIV>
