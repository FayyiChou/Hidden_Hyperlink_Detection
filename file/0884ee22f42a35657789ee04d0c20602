var good_list = new Class({
    divSrc:'',
    divSrc2:'',
    initialize: function(div1,div2){
      this.divSrc=div1;
      this.divSrc2=div2;
      
      this.tab = 'ttp_106';
    },
    changeTableing:function(type,div){
      var i;
      for(i=0;i<4;i++){
        if(i==type){
          $(this.divSrc2+i).setStyles({
            'display':''
          });
          
          $('106_more_'+i).setStyles({
            'display':''
          });
          $(this.tab+i).addClass('current');
        }else{
          $(this.divSrc2+i).setStyles({
            'display':'none'
          });
          $('106_more_'+i).setStyles({
            'display':'none'
          });
          $(this.tab+i).removeClass('current');
        }
      }
    }
  }); 


