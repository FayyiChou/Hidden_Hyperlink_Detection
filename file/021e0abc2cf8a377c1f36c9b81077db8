<script type="text/javascript">
	$(function() {
		$('#j_search_btn').click(function() {
			var str = $(this).prev().val();
			checks(str);
		});
	});
	function checks(t) {
		t = t.replace(/(^\s+)|(\s+$)/g, "");
		if (t == "" || t == "请输入关键字") {
			alert('请输入有效内容');
			return false;
		}
		szMsg = "[#_%&'/",
		";:=!^]";
		alertStr = "";
		for (i = 1; i < szMsg.length + 1; i++) {
			if (t.indexOf(szMsg.substring(i - 1, i)) > -1) {
				alertStr = "请勿包含非法字符如[#_%&'/",
				";:=!^]";
				alert(alertStr);
				// break;
				return false;
			}
		}
		var key = document.getElementById("keys").value;
		window.location.href = "/search.html?keyword=" + key;
		return true;
	}
	function gSearch(){
		var keyword = document.getElementById('keys').value;
		if(keyword=='' || keyword=="请输入检索关键词"){ alert("请输入检索关键词"); }
		else{
			window.location = '/search1.html?keyword='+encodeURI(keyword);
		}
	}
</script>

