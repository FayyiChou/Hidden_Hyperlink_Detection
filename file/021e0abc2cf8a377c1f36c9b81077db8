<script type="text/javascript">

        //取图片进行地址转换，主要为了兼容历史数据
        function formatImgUrl(_imgUrl) {
            formatImgUrl(_imgUrl, "347", "249");
        }
        function formatImgUrl(_imgUrl, _width, _height, _imgTitle) {
            if (_width == null || _width == "") {
                _width = 347
            }
            ;
            if (_height == null || _height == "") {
                _height = 249
            }
            ;
            var str = _imgUrl;
            var arr = new Array();
            arr = str.toString().split(";");
            var image = arr[0];
            var s3 = image.substr(0, 2);
            if (s3 == "P0") {
                var httphead = "/protect/";
                var s1 = image.substr(0, 8);
                var s2 = image.substr(0, 10);
                httphead += s1 + "/" + s2 + "/" + image;
                document.write('<img src="' + httphead + '" height="' + _height + '" width="' + _width + '" alt="' + _imgTitle + '" />');
            } else if (s3 == "W0") {
                var httphead = "/webpic/";
                var s1 = image.substr(0, 8);
                var s2 = image.substr(0, 10);
                httphead += s1 + "/" + s2 + "/" + image;
                document.write('<img src="' + httphead + '" height="' + _height + '" width="' + _width + '" alt="' + _imgTitle + '" />');
            } else {
                document.write('<img src="' + _imgUrl + '" height="' + _height + '" width="' + _width + '" alt="' + _imgTitle + '" />');
            }
        }
    </script>

        
