<script>
var vcodeImgSrc = '/smart-web/vcode';

//################
$("input[type='text'],input[type='password']").focus(function(e){
	$(this).siblings("span").css("display","none");
}).next("span").click(function(){
	$(this).prev("input").focus();
});
$("input[type='text'],input[type='password']").blur(
function(e){
	if($(this).val()==""){
		$(this).siblings("span").css("display","block");
	}else{
		$(this).siblings("span").css("display","none");
	}
}
);
//################

function DY_scroll(wraper,prev,next,img,speed,or){ 
	var wraper = $(wraper);
	var prev = $(prev);
	var next = $(next);
	var img = $(img).find('ul');
	var w = img.find('li').outerWidth(true);
	var s = speed;
	
	next.click(function(){
		img.animate({
			'margin-left':-w*2
		},function(){
			img.find('li').eq(0).appendTo(img);
			img.find('li').eq(0).appendTo(img);
			img.css({'margin-left':0});
		});
	});
	
	prev.click(function(){
		img.find('li:last').prependTo(img);
		img.find('li:last').prependTo(img);//写两遍就会执行两次
		img.css({'margin-left':-w*2});
		img.animate({'margin-left':0});
	});
	
	if (or == true)
	{
		ad = setInterval(function(){
			next.click();
		},s*1000);
		
		wraper.hover(function(){
			clearInterval(ad);
		},function(){
			ad = setInterval(function() {
				next.click();
			},s*1000);
		});
	}
}

function DY_scrollFX(wraper,col3Left,col3Right,img,speed,or){ 
	var wraper = $(wraper);
	var col3Left = $(col3Left);
	var col3Right = $(col3Right);
	var img = $(img).find('ul');
	var w = img.find('li').outerWidth(true);
	var s = speed;
	
	col3Right.click(function(){
		img.animate({
			'margin-left':-w
		},function(){
			img.find('li').eq(0).appendTo(img);
			img.css({'margin-left':0});
		});
	});
	
	col3Left.click(function(){
		img.find('li:last').prependTo(img);//写两遍就会执行两次
		img.css({'margin-left':-w});
		img.animate({'margin-left':0});
	});
	
	if (or == true)
	{
		ad = setInterval(function(){
			col3Right.click();
		},s*1000);
		
		wraper.hover(function(){
			clearInterval(ad);
		},function(){
			ad = setInterval(function() {
				col3Right.click();
			},s*1000);
		});
	}
}
		



function switchPanelBtnClick(obj){
	switch($(obj).attr('id')){
	case 'switchRegBtn':
		/*if('CHROME21'=='IE7')	//IE兼容性问题，解决发送验证码按钮错位问题
			$('#phone_validCode').attr('style','margin-bottom:auto;');*/
		$('.login_dl').hide();
		$('.login').hide();
		$('.register_dl').show();
		$('.register').show();
		regSwitch($('#regPhoneBtn'));
		break;
	case 'switchLoginBtn':
		$('.login_dl').show();
		$('.login').show();
		$('.register_dl').hide();
		$('.register').hide();
		break;
	}
}


$(function(){
		$(".downEwm").mouseover(function(){
				$("#ewm").show();
		})
		$("#ewm").mouseout(function(){
			$("#ewm").hide();
		})
	$(".arrowmain li").mouseover(function(){
	   $(this).find('.mengban1').show();
	});
 	$(".arrowmain li").mouseout(function(){
	   $(this).find('.mengban1').hide();
 	});
    
	DY_scroll('.img-scroll','.prev','.next','.img-list',3,false);// true为自动播放，不加此参数或false就默认不自动
	DY_scrollFX('.img-scroll1','.col3Left','.col3Right','.img-list11',3,false);// true为自动播放，不加此参数或false就默认不自动
	loginInit('');	
  	regInit('','','','','');
});

</script>
