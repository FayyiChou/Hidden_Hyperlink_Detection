    //地理位置定位
    var GeoLocationObject = {
        changeCity: function (cityId) {
            location.replace(thisurl.replace("999999999",cityId));
        },
        initAction: function () {
            var executeGPS="";
            //无citycode参数
            if(2109 == -1){
                executeGPS = "True";
                //如果手动选过城市，则不调用GPS
                if(readcookie("selectAreaManual") == "True"){
                    executeGPS = "False";
                }
            }
            else{
                executeGPS = "False";
                setcookie("selectAreaManual","True");
            }
          
            if (executeGPS == "True") {
                nativeGeo.executeGeoLocation();
            }

        }
    }
    GeoLocationObject.initAction();

