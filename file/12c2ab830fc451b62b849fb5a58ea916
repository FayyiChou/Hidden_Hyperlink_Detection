    <script type="text/javascript">
        var thisurl = "/nb1765_c999999999_nc0/";
        var SerialId=1765;
        var CarId=0;
        var CityCode=2109;
        var CityDX=0;
        var DealerPages = {
            PageSize: 10,
            TotalCount:75,
            Pages: 0,
            CurrentNum: 1,
            WillNum: 1,
            PrePage: $("#btnPrePage"),
            NextPage: $("#btnNextPage"),
            PageInfo: $("#divPageInfo"),
            IsRunning:false,
            Init: function () {
                this.Pages = parseInt((this.TotalCount + this.PageSize - 1) / this.PageSize);
                $("#pageSelect").change(function () {
                    if(DealerPages.checkRun()){
                        DealerPages.WillNum = $(this).val();
                        DealerPages.changePage();
                    }
                });
                this.PrePage.click(function () {
                    if(DealerPages.checkRun()){
                        DealerPages.WillNum--;
                        DealerPages.changePage();
                    }
                });
                this.NextPage.click(function () {
                    if(DealerPages.checkRun()){
                        DealerPages.WillNum++;
                        DealerPages.changePage();
                    }
                });
            },
            checkRun:function(){
                if(DealerPages.IsRunning) 
                    return false;
                DealerPages.IsRunning=true;
                return true;
            },
            btnDisable: function () {
                if (this.CurrentNum == 1)
                    this.PrePage.addClass("m-pages-none");
                else
                    this.PrePage.removeClass("m-pages-none");
                if (this.CurrentNum == this.Pages)
                    this.NextPage.addClass("m-pages-none");
                else
                    this.NextPage.removeClass("m-pages-none");
            },
            changePage: function () {
                
                if (this.CurrentNum == this.WillNum || this.WillNum > this.Pages || this.WillNum < 1) {
                    this.WillNum = this.CurrentNum;
                    this.IsRunning=false;
                    return;
                }
                
                $.ajax({
                    type: "post",
                    url: "/ajax/PageGetPromotionInfo.ashx",
                    data: {"serialId": SerialId  ,"carId":  CarId  ,"cityCode":  CityCode  ,"cityDX":  CityDX  ,"pageIndex":  DealerPages.WillNum,"o":'0'},
                    dataType: "json",
                    success: function (msg) {
                        DealerPages.IsRunning=false;
                        if(msg!=null &&msg.reslut != undefined && msg.reslut.length > 0){
                            $("#divShowResult ul").html("");
                            $("#divShowResult ul").html(msg.reslut);
                            DealerPages.CurrentNum = DealerPages.WillNum;
                            DealerPages.PageInfo.html(DealerPages.CurrentNum + "/" + DealerPages.Pages);
                            $("#pageSelect option[value=" + DealerPages.CurrentNum + "]").attr("selected", "selected");
                            DealerPages.btnDisable();
                            $("html,body").animate({ scrollTop: $("#anchor").offset().top }, 300)
                        }
                    },
                    error: function () {
                        DealerPages.IsRunning=false;
                    }
                });       
            }
        }
        DealerPages.Init();

        $('[data-action=popup-order]').rightSwipe({
            clickEnd: function (b) {
                var $leftPopup = this;
                if (b) {
                    var $back = $('.' + $leftPopup.attr('data-back'))
                    $back.touches({ touchstart: function (ev) { ev.preventDefault(); }, touchmove: function (ev) { ev.preventDefault(); } });
                    var $swipeLeft = $leftPopup.find('.swipeLeft');
                    $swipeLeft.touches({ touchmove: function (ev) { ev.preventDefault(); } });

                    var myScroll = new iScroll($swipeLeft[0], {
                        momentum: true,
                        click: true,
                        bounce: false,
                        onBeforScrollStart: function () { }
                    });
                }
                else {
                    $leftPopup.myScroll.scrollTo(0, 0, 0, false);
                }
            }
        });
        $('[data-action=popup-car]').rightSwipe({
            clickEnd: function (b) {
                var $leftPopup = this;
                if (b) {
                    var $back = $('.' + $leftPopup.attr('data-back'))
                    $back.touches({ touchstart: function (ev) { ev.preventDefault(); }, touchmove: function (ev) { ev.preventDefault(); } });
                    var $swipeLeft = $leftPopup.find('.swipeLeft');
                    $swipeLeft.touches({ touchmove: function (ev) { ev.preventDefault(); } });

                    var myScroll = new iScroll($swipeLeft[0], {
                        momentum: true,
                        click: true,
                        bounce: false,
                        onBeforScrollStart: function () { }
                    });
                }
                else {
                    $leftPopup.myScroll.scrollTo(0, 0, 0, false);
                }
            }
        });

        //右侧侧附加选择层
        (function () {
            $('[data-action=popup-city]').rightSwipe({
                clickEnd: function (b) {
                    var $leftPopup = this;
                    if (b) {
                        var $back = $('.' + $leftPopup.attr('data-back'))
                        $back.touches({ touchstart: function (ev) { ev.preventDefault(); }, touchmove: function (ev) { ev.preventDefault(); } });
                        var $swipeLeft = $leftPopup.find('.swipeLeft');

                        $leftPopup.myScroll = new IScroll($swipeLeft[0], {
                            probeType: 1,
                            snap: 'li',
                            momentum: true,
                            click: true
                        });
                    } else {
                        $leftPopup.myScroll.scrollTo(0, 0, 0, false);
                    }
                }
            });

            $('[data-action=popup-city2]').rightSwipe({
                zIndex : 10000000,
                clickEnd: function (b) {

                    var $leftPopup = this;
                    if (b) {
                        var $back = $('.' + $leftPopup.attr('data-back'))
                        $back.touches({ touchstart: function (ev) { ev.preventDefault(); }, touchmove: function (ev) { ev.preventDefault(); } });
                        var $swipeLeft = $leftPopup.find('.swipeLeft');

                        $leftPopup.myScroll = new IScroll($swipeLeft[0], {
                            probeType: 1,
                            snap: 'li',
                            momentum: true,
                            click: true
                        });

                        $leftPopup.find('.sub-return').on('click', function(ev){
                            ev.preventDefault();
                            $leftPopup.children().removeClass('swipeLeft-block');
                            setTimeout(function () { $leftPopup.css('z-index', 0).hide(); }, 200);
                        })
                    
                    } else {
                        $leftPopup.myScroll.scrollTo(0, 0, 0, false);
                    }
                }
            });

          
        })();
        function showCity(pid,pname) {
            if($("#pvc"+pid).length>0){
                $("#childCityUl").html("");
                $("#childCityUl").append($("#pvc"+pid).html());
            }
        }
        function changeCity(pvcId, cityId, cityName) {
            location.href = thisurl.replace("999999999",cityId);
        }
    </script>
