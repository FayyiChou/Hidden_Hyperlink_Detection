            url: path + "/wapData/getCourseList/870", type: "post", async: false, success: function (data) {
                var courseType = data.courseType;
                /*判断是否有权限观看视频*/
                if (courseType != 0 && (!data.skuPrivilege || data.vipExpire || data.dropOut || !checkInstal)) {
                    vipAuth = false;
                }
                if (vipAuth == false) {
                    if (user != null && user != "") {
                        if(courseType == "2"){
                            $("#lsVideo").html('<img src="/resources/wap/course/images/vip_auth_2.png" />');
                        }else{
                            $("#lsVideo").html('<img src="/resources/wap/course/images/vip_auth.png" />');
                        }
                        $("#lsVideo").find("img").click(function () {
                            xiaonengIm(settingid, sku, 'courseDetail');
                        });
                    } else {
                        if(courseType == "2"){
                            $("#lsVideo").html('<img src="/resources/wap/course/images/vip_auth_2.png" onclick="signIn();" />');
                        }else{
                            $("#lsVideo").html('<img src="/resources/wap/course/images/vip_auth.png" onclick="signIn();" />');
                        }
                    }
                }
                document.getElementById("test1").innerHTML = "<div class=\"coursePlay-title borB color3\"></div>";//标题数据
                $(".coursePlay-title").text(data.courseName);
                if (data.skuPrivilege && !data.vipExpire && !data.dropOut && checkInstal) {
                    var orderNum = 0;
                    for (var i = 0; i < data.courses.length; i++) {
                        $("#test1").append("<div class=\"coursePlay-list\">" + "<div class=\"coursePlayList-top borB color3\"></div><ul></ul>" + "</div>")//向#test内容最后添加coursePlay-list
                        $(".coursePlay-list:eq(" + i + ") .coursePlayList-top").text(data.courses[i].chapterName)//向下标为i的coursePlay-list中的coursePlayList-top添加章节数据
                        var lectures = data.courses[i].lectures;
                        if (i < data.courses.length - 1) {
                            $(".coursePlay-list:eq(" + i + ")").after("<div class=\"bgE\" style='margin-top: -1px;'></div>");
                        }
                        for (var j = 0; j < lectures.length; j++) {
                            $(".coursePlay-list:eq(" + i + ") ul").append("<li class=\"color6\"><i class='coursePlayList-left-i'><img src='/resources/wap/course/images/course_nosee_icon.png' class=\"coursePlayList-left\" /></i><p class=\"coursePlayList-right borB\"></p></li>")//向下标为i的coursePlay-list中的ul添加li
                            $(".coursePlay-list:eq(" + i + ") .coursePlayList-right:eq(" + j + ")").text(lectures[j].lectureName)//向下标为i的coursePlay-list中的下标为j的li添加小节数据
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("lsVideoId", lectures[j].lsVideoId);
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("courseId", lectures[j].lectureId);
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("orderNum", orderNum);
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("id", "lecture"+lectures[j].lectureId);
                            if (j == lectures.length - 1) {
                                $(this).css('border-bottom', 'none')
                            }
                            if (lectures[j].isLook == "true") {
                                $(".coursePlay-list:eq(" + i + ") .coursePlayList-left:eq(" + j + ")").attr("src","/resources/wap/course/images/course_already_icon.png");
                                $(".coursePlay-list:eq(" + i + ") .coursePlayList-left:eq(" + j + ")").parent().attr("class","coursePlayList-left-i");
//                                $(".coursePlay-list:eq(" + i + ") .coursePlayList-left:eq(" + j + ")").css({
//                                    "background": "url('/resources/wap/course/images/course_already_icon.png') no-repeat",
//                                    "background-size": "18px",
//                                });
                                $(".coursePlay-list:eq(" + i + ") .coursePlayList-right:eq(" + j + ")").css({'color': '#666'})

                                $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("playType", "yes");
                            } else {
                                $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("playType", "no");
                            }
                            orderNum++;
                        }
                    }
                    $(".coursePlay-list li").eq(0).css("color", "#35d48f");
                    $(".coursePlayList-left").eq(0).attr("src","/resources/wap/course/images/course_play_icon.png");
//                    $(".coursePlayList-left").eq(0).css({
//                        "background": "url('/resources/wap/course/images/course_play_icon.png') no-repeat",
//                        "background-size": "20px"
//                    });
                    studyHistory("28003");
                } else {
                    var orderNum = 0;
                    for (var i = 0; i < data.courses.length; i++) {
                        $("#test1").append("<div class=\"coursePlay-list\">" + "<div class=\"coursePlayList-top borB color3\"></div><ul></ul>" + "</div>")//向#test内容最后添加coursePlay-list
                        $(".coursePlay-list:eq(" + i + ") .coursePlayList-top").text(data.courses[i].chapterName)//向下标为i的coursePlay-list中的coursePlayList-top添加章节数据
                        var lectures = data.courses[i].lectures;
                        for (var j = 0; j < lectures.length; j++) {
                            $(".coursePlay-list:eq(" + i + ") ul").append("<li class=\"color6\"><i><img src='/resources/wap/course/images/course_nosee_icon.png' class=\"coursePlayList-left\" /></i><p class=\"coursePlayList-right borB\"></p></li>")//向下标为i的coursePlay-list中的ul添加li
                            $(".coursePlay-list:eq(" + i + ") .coursePlayList-right:eq(" + j + ")").text(lectures[j].lectureName)//向下标为i的coursePlay-list中的下标为j的li添加小节数据
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("lsVideoId", lectures[j].lsVideoId);
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("courseId", lectures[j].lectureId);
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("orderNum", orderNum);
                            $(".coursePlay-list:eq(" + i + ") li:eq(" + j + ")").attr("id", "lecture"+lectures[j].lectureId);
//                            $(".coursePlay-list:eq(" + i + ") .coursePlayList-left:eq(" + j + ")").css({
//                                "background": "url('/resources/wap/course/images/video_play_lock.png') no-repeat",
//                                "background-size": "18px"
//                            });
                            $(".coursePlay-list:eq(" + i + ") .coursePlayList-left:eq(" + j + ")").attr("src","/resources/wap/course/images/video_play_lock.png");
                            $(".coursePlay-list:eq(" + i + ") .coursePlayList-left:eq(" + j + ")").parent().attr("class","coursePlayList-left-i");
                            orderNum++;
                        }
                    }
                }
            }

        });
        if (vipAuth) {
            $("#test1").find("li").each(function () {
                $(this).click(function () {
                    var lsVideoId = $(this).attr("lsVideoId");
                    var courseLectureId = $(this).attr("courseId");
                    var newUrl = "/wap/coursePlay/870/"+courseLectureId;
                    history.replaceState(null,"", newUrl);
                    $(this).attr("playType", "yes");
                    player.sdk.playNewId({"uu": "c2ygy4wf5p", "vu": lsVideoId,"auto_play":"0"});//切换ID
                    $("#test1").find("li").each(function () {
                        var playType = $(this).attr("playType");
                        $(this).find(".coursePlayList-right").css("color", "#666666");
                        if (playType == "yes") {
//                            $(this).find(".coursePlayList-left").css({
//                                "background": "url('/resources/wap/course/images/course_already_icon.png') no-repeat",
//                                "background-size": "18px"
//                            });
                            $(this).find(".coursePlayList-left").attr("src","/resources/wap/course/images/course_already_icon.png");
                            $(this).find(".coursePlayList-right").css({'color': '#ccc'})
                        } else {
//                            $(this).find(".coursePlayList-left").css({
//                                "background": "url('/resources/wap/course/images/course_nosee_icon.png') no-repeat",
//                                "background-size": "18px"
//                            });
                            $(this).find(".coursePlayList-left").attr("src","/resources/wap/course/images/course_nosee_icon.png");
                            $(this).find(".coursePlayList-left").parent().attr("class","coursePlayList-left-i");

                        }
                    });
                    playback = $(this).attr("orderNum");
                    $(this).find(".coursePlayList-right").css("color", "#35d48f").siblings().css("color", "#666666");
//                    $(this).find(".coursePlayList-left").css({
//                        "background": "url('/resources/wap/course/images/course_play_icon.png') no-repeat",
//                        "background-size": "18px"
//                    });
                    $(this).find(".coursePlayList-left").attr("src","/resources/wap/course/images/course_play_icon.png");
                    studyHistory(courseLectureId);
                });
            });
            if(lectureId!=0) {
                $("#lecture" + lectureId).click();
            }
        }

        window.call = function (type, data) {
            if (type == "playerInit") {
                setInterval(function () {
                    var nowIndex = playback * 1 + 1;
                    var selectLi;
                    var upSelectLi;
                    if (player.sdk.getVideoSetting().duration != null && player.sdk.getVideoSetting().duration != "" && (player.sdk.getVideoSetting().duration == player.sdk.getVideoTime())) {
                        $("#test1").find("li").each(function () {
                            var orderNum = $(this).attr("orderNum");
                            if (orderNum == playback) {
                                upSelectLi = $(this);
                            }
                            if (orderNum == nowIndex) {
                                selectLi = $(this);
                            }
                        });
                        var lsVideoId = selectLi.attr("lsVideoId");
                        var courseLectureId = selectLi.attr("courseId");
                        player.sdk.playNewId({"uu": "c2ygy4wf5p", "vu": lsVideoId,"auto_play":"0"});
                        upSelectLi.attr("playType", "yes");
                        upSelectLi.css("color", "#666666");
                        upSelectLi.find(".coursePlayList-right").css("color", "#666666");
//                        upSelectLi.find(".coursePlayList-left").css({
//                            "background": "url('/resources/wap/course/images/course_already_icon.png') no-repeat",
//                            "background-size": "18px"
//                        });
                        upSelectLi.find(".coursePlayList-left").attr("src","/resources/wap/course/images/course_already_icon.png");
                        selectLi.attr("playType", "yes");
                        selectLi.css("color", "#35d48f");
                        selectLi.find(".coursePlayList-right").css("color", "#35d48f");
//                        selectLi.find(".coursePlayList-left").css({
//                            "background": "url('/resources/wap/course/images/course_play_icon.png') no-repeat",
//                            "background-size": "18px"
//                        });
                        selectLi.find(".coursePlayList-left").attr("src","/resources/wap/course/images/course_play_icon.png') no-repeat");
                        studyHistory(courseLectureId);
                    }
                }, 1000);

            }

        };
    });

    function studyHistory(lectureId){
        if (user != null && user != '') {
            $.ajax({
                url: path + '/wapData/courseStudyHistory/870/'+lectureId,
                type: 'post',
                async: false,
                success: function (data) {

                }
            });
        }
    }
