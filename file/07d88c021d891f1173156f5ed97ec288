<script src="/resources/common/js/cookie/js.cookie.js"></script>

<script>
    var tongjiDomain = "http://www.duia.com/tongji/";
    //设置cookie
    function setCookie(name, value, day) {
        Cookies.set(name, value, { expires: day,domain: 'duia.com',path: '/' });
    }

    //读取cookies
    function getCookie(name) {
        var val= Cookies.get(name);
        if(val==undefined){
            return null;
        }
        return val;
    }

    //删除cookies
    function delCookie(name) {
        Cookies.remove(name);
    }

    function getCharCode(str) {
        var codeArr = new Array();
        for (var i = 0; i < str.length; i++) {
            codeArr.push(str.substring(i, i + 1).charCodeAt());
            if (i != str.length - 1) {
                codeArr.push("_");
            }
        }
        return codeArr.join("");
    }

    //清除cookie
    if (getCookie("cookie_version") == null || getCookie("cookie_version") == "") {
        setCookie("cookie_version", "1", 365);
        delCookie("track");
        delCookie("beginTime");
        delCookie("referrerTrack");
        delCookie("utm_source");
        delCookie("referrer");
    }

    //获取上一个页面地址
    var referrer = getCharCode(document.referrer);
    //获取当前页地址
    var trackUrl = getCharCode(window.location.href);
    var uuid=getCookie("client_id");
    try {
        if (location.href.match(new RegExp(/%[A-Z0-9]{2}\b/g)).length >= 1) {
            trackUrl = getCharCode(decodeURI(window.location.href));
        }
        if (document.referrer.match(new RegExp(/%[A-Z0-9]{2}\b/g)).length >= 1) {
            referrer = getCharCode(decodeURI(document.referrer));
        }
    } catch (e) {

    }

    if (getCookie("referrer") == null || getCookie("referrer") == "") {
        if (document.referrer != null) {
            setCookie("referrer", referrer, 30);
        }
    }

    setCookie("track", trackUrl, 1);
    setCookie("beginTime", new Date().getTime(), 1);
    setCookie("referrerTrack", referrer, 1);

    window.onbeforeunload = onbeforeunload_handler;
    /*跟踪用户信息*/
    function onbeforeunload_handler() {
        if (getCookie("track") != null && getCookie("track") != "") {
            $.ajax({
                url: "/sysTrack/saveTrack",
                type: "get",
                dataType: "text",
                cache: false,
                async: false,
                success: function () {

                }
            });
        }
    }

    /**
     * 小能接入
     */
    function xiaonengIm(settingid, categoryId,key) {
        tongjiXn(key);
        var sid = settingid;
        if (typeof sid != 'undefined' && sid != null && sid != "") {
            NTKF.im_openInPageChat(sid);
            // 添加小能销售推广记录
            var url = path + "/sysKeyPointLog/saveXnSale";
            var datas = {"uuid": sid, "username": NTKF.user.id, "sku": categoryId, "et": "LIVE_SALES_IM"};
            $.post(url, datas, function (data) {
            });
        } else {
            xiaoneng(categoryId, key);
        }
    }

    /**
     * 根据sid进行在线咨询相关展示
     */
    function xiaoneng(sku, key) {
        if (!isNaN(sku)) {
            var cookieKey = sku + "_" + key;
            if (getCookie(cookieKey) == null) {
                var url = path + '/sysDict/xiaoneng';
                var dataes = {sku: sku};
                $.ajax({
                    type: "post",
                    url: url,
                    data: dataes,
                    async: false,
                    dataType: "json",
                    success: function (data) {
                        var val = data[key];
                        NTKF.im_openInPageChat(val);
                        setCookie(cookieKey, val,1);
                    }
                });
                $.ajax({
                    type: "post",
                    url: path + "/sysKeyPointLog/saveXn/" + sku + "/" + key,
                    data: {uuid: NTKF.user.id},
                    async: false,
                    success: function (data) {

                    }
                });
            } else {
                var val = getCookie(cookieKey);
                NTKF.im_openInPageChat(val);
            }
        } else {
            NTKF.im_openInPageChat(sku);
        }
    }

    /*小能key转换*/
    function changeXnKey(key){
        switch (key){
            case "course":
                return "商品列表";
            case "courseDetail":
                return "商品详情";
            case "video":
                return "视频列表";
            case "videoDetail":
                return "视频详情";
            case "homeSku":
                return "sku首页";
            default:
                return "小能咨询";
        }
    }

    /*小能统计*/
    function tongjiXn(publicTitle) {
        publicTitle = changeXnKey(publicTitle);
        globalTongjiParams.source = "WAP";
        globalTongjiParams.trackLiveContactAdd(senderId, liveId, sku, publicTitle);
    }
</script>
