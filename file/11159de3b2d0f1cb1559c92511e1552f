<script> 
//合作方实现newsRender接口
function newsRender(data){
	if (data[0].img == null) {
		var html = '';
		html = getTpl('搜索热词',data);
		$(".wz_nrc").html(html);
	}

}
function getTpl(title, data){
	var html = '',_item = null;
	for(var j=0,l=8;j<l;j++){
		_item = data[j]; 
		html+='<li><a rel="nofollow" href="'+ decodeURIComponent(_item['url']) +'" target="_blank" title="'+_item['title']+'">'+ countByteLength(_item['title'],1) +'</a></li>';
	}
	return html;
}

</script>
