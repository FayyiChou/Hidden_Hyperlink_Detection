     <script type="text/javascript">
function abc(json_str){
	json_str=json_str.pnews;
	var _str = "<ul class=\"jplb\">";
	for(var key in json_str){
		url=decodeURIComponent(json_str[key].url);
		_str += "<li>\
				 <div><a href=\""+url+"\" title=\'"+json_str[key].title+"\' target=\"_blank\"><img src=\""+json_str[key].img+"\" border=\"0\" alt=\'"+json_str[key].title+"\' width=\"138\" height=\"95\"></a></div>\
				 <div><a href=\""+url+"\" title=\'"+json_str[key].title+"\' target=\"_blank\">"+countByteLength(json_str[key].title,.5)+"</a></div>\
				 </li>";
	}
	_str += "</ul>";
	//$("#qh360").html(_str);
}
function countByteLength(str,cnCharByteLen) {
	var byteLen = 0;
	for (var i=0; i<str.length; i++) {
		if ((/[\x00-\xff]/g).test(str.charAt(i)))
			byteLen += 1;
		else
			byteLen += cnCharByteLen;
	}
	if (byteLen>10) byteLen = 10;
	return str.substring(0,byteLen);
}
</script>
    
