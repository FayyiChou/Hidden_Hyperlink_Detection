     	$(function(){
     		var obj = new Image();
     		var loadImage = "http://sta.qinxue.com/images/login.gif";
     		$(".myclassgif").css("cursor","pointer");
     		$(".myclassgif").click(function(){
     			var objSelect = $(this);
     			var picleft = objSelect.offset().left;
     			var pictop = objSelect.offset().top;
     			var picwidth = objSelect.width();
     			var picheight = objSelect.height();
     			var picdiv = '<div class="jqImgLoading" style="top:'+pictop+'px;left:'+picleft+'px;position:absolute;z-index:999999;width:'+picwidth+'px;height:'+picheight+'px;background:url('+loadImage+') no-repeat center;margin:auto;"></div>';
     			objSelect.parent().append(picdiv);
     			//$(this).attr('src', loadImage);
     			obj.src = $(this).attr("_mygif");
     			obj.onload = function(){
     				//$(".jqImgLoading").remove();
     				objSelect.next().css("background","");
     				objSelect.attr('src', obj.src);
     			}
     		});
     		$(".i_list_cd").click(function(){
	     	  $(".i_list_cd_02").attr("style","display:block");
	    	});
	    	$(".i_list_cd_02").mouseover(function(){
	    	  $(".i_list_cd_02").attr("style","display:block");
	    	});
     	});
     	$(document).bind("click",function(event){
	        var e = event || window.event; 
	        var elem = e.srcElement||e.target; 
	        while(elem) 
	        { 
	        	var re = /^i_list_cd i_list_cd[1-9]$/gi;
	        	var strElemCls = elem.className;	        	
	            if(strElemCls == "i_list_cd_02" || re.test(strElemCls) ) 
	            { 
	                return; 
	            }
	            elem = elem.parentNode;
	        } 
	     
	       $('.i_list_cd_02').hide();
	     });
	</script>
    
