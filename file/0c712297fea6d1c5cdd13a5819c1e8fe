    var is_show_new = true;
    var goods_load_url  = 'http://m.coulai.com/index.php?c=group&a=tag&id=341795';
    var href_blank  = true;//是否在新窗口打开链接，默认是
    if( typeof goods_load_call_back != 'function' ){
        goods_load_call_back=function(){};
    }
    //从history状态中读取需要加载的页面数
    var pre_page = (history && history.state && history.state.page) ? history.state.page : 0;

    var goods_load  = GOODS_LOAD.init(
        {
            goods_load_url  : goods_load_url,
            is_show_new     : is_show_new,
            goods_init      : [],
            goods_attr      : GOODS_LOAD.goods_attr.concat('maintitle', 'subtitle', 'timestatus'),
            pre_load_page   : pre_page,is_target_blank : href_blank,
            call_back_fn    : goods_load_call_back,
            page_size       : parseInt("60") ? parseInt("60") : 20
        });
    $(document).on('click','a.goods-a',function (e) {
        history.replaceState({
                page:goods_load.cur_page_num,
                scroll: $(window).scrollTop()
            });
    });
</script> 
