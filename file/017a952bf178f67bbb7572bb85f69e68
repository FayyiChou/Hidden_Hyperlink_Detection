<script type="text/javascript">

$(document).ready(function(){

$(".closeRBox").bind("click", function(){

$(".rZxzx").remove();

$(this).remove();

});

var isshow_stlist = false;

$(".stype .stbtn").bind("click", function(){

var op = $(".stype");

$(".stlist").css("top", op.offset().top + 5 + 'px');

$(".stlist").css("left", op.offset().left + 4 + 'px');

$(".stlist").css("display", "block");

isshow_stlist = true;

return true;

});

$(".stype .stbtn").bind("mouseover", function(){

return isshow_stlist = true;

});

$(".stype .stbtn").bind("mouseout", function(){

return isshow_stlist = false;

});

$(".stlist a").bind("mouseover", function(){

$(".stlist a").removeClass("stfocus");

$(this).addClass("stfocus");

isshow_stlist = true;

return true;

});

$(".stlist a").bind("mouseout", function(){

$(this).removeClass("stfocus");

return isshow_stlist = false;

});

$(".stlist a").bind("click", function(){

$(".stype span").html($(this).text());

$(".stlist").css("display", "none");

$("input[name=type]").val($(this).attr("search-type"));

return true;

});

$("body").bind("click", function(){

if($(".stlist").css("display") == "block" && isshow_stlist == false) {

$(".stlist").css("display", "none");

}

return true;

});

});

</script>

</body>

