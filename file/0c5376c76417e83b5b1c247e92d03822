<script type="text/javascript">
function searchInResult(id){
	var oldQuery = "好似天堂-丈夫去世后的情感生活";
	if(oldQuery!=document.searchForm1.query.value){
		document.searchForm.query.value=oldQuery+' '+document.searchForm1.query.value;
		document.searchForm1.query.value=oldQuery+' '+document.searchForm1.query.value;
	}
	document.searchForm.submit();
	return true;
}

function setHomepage() {
	if (document.all) {
		document.body.style.behavior = 'url(#default#homepage)';
		document.body.setHomePage('http://v.sogou.com');
　　 }
	else if (window.sidebar) {
		if (window.netscape) {
			try {
				netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
			}
			catch (e) {
				alert("该操作被浏览器拒绝，如果想启用该功能，请在地址栏内输入 about:config,然后将项 signed.applets.codebase_principal_support 值该为true");
			}
		}
		var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
		prefs.setCharPref('browser.startup.homepage', 'http://v.sogou.com');
		alert('已设置默认首页为http://v.sogou.com');
	}
} 

