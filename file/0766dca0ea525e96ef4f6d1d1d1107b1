	<script type="text/javascript">
$(function (){
	$('.btn-visible-submit').click(function (){
		_hmt.push(['_trackEvent', '首页留言', '留言', '留言', 1]);
		var copylist = [
			// '_wpnonce',
			// '_wp_http_referer',
			// '_ninja_forms_display_submit',
			// '_form_id',
			// '_PaaPX',
			// '_hp_name',
			'ninja_forms_field_6',
			'ninja_forms_field_7',
			'ninja_forms_field_8',
			'ninja_forms_field_11',
			'ninja_forms_field_9'
		];
		var form_source = $('.contact-form #ninja_forms_form_5'),
			form_target = $('.realform-wraper #ninja_forms_form_5'),
			src, trg, sel;
		for (var i=0; i< copylist.length; i++) {
			sel = 'input[name="'+copylist[i]+'"], textarea[name="'+copylist[i]+'"]';
			src = $(sel, form_source);
			trg = $(sel, form_target);
			if (trg.length == 0)
				continue;

			trg.val( src.val() );
		}

		form_target.submit();
		return false;
	});
});
</script>
