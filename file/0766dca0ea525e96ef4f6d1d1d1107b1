<script type="text/javascript" charset="utf-8">
$(function (){
	var now=0;
	var st=null;
	 var lis=$(".brand-list");
	 var lis_len=lis.size();
	 var btns=$(".solids");
	  btns.each(function(index){
	   $(this).click(function (){
	   	btns.removeClass("active");
	   	$(this).addClass('active');
	     slideRun(index);
	   })
	 })

	  function slideRun(index){

	    lis.eq(index).siblings().css({
	     display:"none"
	    });
	   lis.eq(index).css({
	     display:"block"
	    });
	  }

	  $(function (){
	  	$('.carousel-indicators li').click(function (){
	  		$('#top-banner').carousel($(this).data('slide-to'));
	  	});
	  });
})

</script>
