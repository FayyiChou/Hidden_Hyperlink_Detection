  <script type="text/javascript">
$(function() {
var  = $('nav#menu'),
 = $('html, body');

.mmenu();
.find( 'li > a' ).on(
'click',
function()
{
var href = $(this).attr( 'href' );

//	if the clicked link is linked to an anchor, scroll the page to that anchor 
if ( href.slice( 0, 1 ) == '#' )
{
.one(
'closed.mm',
function()
{
setTimeout(
function()
{
.animate({
scrollTop: $( href ).offset().top
});	
}, 10
);	
}
);						
}
}
);
});
</script> 
