<script type="text/javascript">

var BanTimer;
var BanCont = 0;
function BanGd(){
	
	if(BanCont < 2){
		BanCont++;
	}else{
		BanCont = 0;	
	}
	$(".BanGdNum>a").eq(BanCont).addClass("BanCur").siblings().removeClass("BanCur");
	$(".BanGdImg>a").eq(BanCont).show().siblings().hide();
	
}

$(function(){
	
	BanTimer = setInterval("BanGd()",2000)
	$(".BanGdImg>a").mouseenter(function(){
		clearInterval(BanTimer);
	})
	$(".BanGdNum>a").click(function(){
		clearInterval(BanTimer);
		BanCont =  $(".BanGdNum>a").index(this);
		$(".BanGdNum>a").removeClass("BanCur");
		$(this).addClass("BanCur").siblings().removeClass("BanCur");
		$(".BanGdImg>a").eq(BanCont).show().siblings().hide();
	})
	$(".BanGd").mouseleave(function(){
		clearInterval(BanTimer);
		BanTimer = setInterval("BanGd()",2000)	
	})
	
		
	
})




    </script>
