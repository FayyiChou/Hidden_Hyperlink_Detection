<script type="text/javascript">

document.onmouseup=function(event){
	var e=event ||window.event;
 	var elm=e.srcElement || e.target;
	var target_e = document.getElementById('login_tip_wrap');
	do{ 
	if(elm.id == "login_show" && target_e.style.display=="none" ) {target_e.style.display="block"; return;}
	if(elm.id =="login_tip_wrap") return; 
	elm = elm.parentNode;
	}
	while(elm.parentNode)
	flag_job = false;
	target_e.style.display="none";
	}

</script>
