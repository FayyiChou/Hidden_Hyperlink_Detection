    <script>
function contactOnline() {
    var qrleft = parseInt($('ft').clientWidth + parseInt(fetchOffset($('ft'))['left']));
    if(qrleft + $('scrolltop').clientWidth > document.documentElement.clientWidth) {
      $('scrolltop').style.cssFloat = 'right';
      $('scrolltop').style.left = 'auto';
      $('scrolltop').style.right = 0;
    } else {
      $('scrolltop').style.cssFloat = 'left';
      $('scrolltop').style.left = (qrleft) + 'px';
      $('scrolltop').style.right = 'auto';
    }
    var aTagObject = $('scrolltop').getElementsByTagName("a");
    var aTagLen =  aTagObject.length;

    for(var i = 0; i < aTagLen; i++ ){
      if(aTagObject[i].className != 'contact-online'){
        aTagObject[i].style.display = 'none';
      }
    }

}
_attachEvent(window, 'load', function() { contactOnline(); }, document);
