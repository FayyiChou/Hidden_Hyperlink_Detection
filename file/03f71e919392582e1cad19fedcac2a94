                 <script>

                      var rzdom={
                           id:function(x){
                                if (typeof x == "string") return document.getElementById(x);
                                return x;
                           },
                           addClass:function (obj, cls) {
                             obj.className += " " + cls;
                           },
                           getsub:function(obj){
                              var subnodes=[];
                              if(typeof(obj.children) !="undefined"){
                                  subnodes=obj.children;
                               }
                              else{
                                  for (var i in obj){
                                     if(obj[i].nodeType=1) subnodes.push(obj[i]);
                                  }
                              }
                              return   subnodes;
                           },
                           hasClass: function (obj, cls) {
                                return obj.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)'));
                           },
                           removeClass:function(obj, cls) {
                               if(obj==null || typeof(obj)=="undefined") return null;
                                if (rzdom.hasClass(obj, cls)) {
                                    var reg = new RegExp('(\\s|^)' + cls + '(\\s|$)',"g");
                                    obj.className = obj.className.replace(reg, ' ');
                                }
                            }

                         }
                       var menus= rzdom.id("rz_main_menu");
                       var menulis=rzdom.getsub(menus);
                       var local_url=window.location.href;
                       var gid5=local_url.match(/gid=5$/);
                       var f225=local_url.match(/-225-/);
               var f179180=local_url.match(/-179-/) || local_url.match(/-180-/) || local_url.match(/-257-/);
                       var eachfun=function(objs,cb){ for(var i in objs){ cb(objs[i]); }};
                       eachfun(menulis,function(e){  e.className="";  })
                       if((gid5!=null && gid5!=undefined)   || (f179180!=null && f179180!=undefined) ){
                           menulis[2].className="a";
                       }
                       else if(f225 && f225!=undefined){
                           menulis[1].className="a";
                       }else{
                           local_url=window.location.pathname;
                           if(local_url=="" || local_url=="/"  || local_url=="index.php" ||  window.location.href.match(/forum.php$/)!=null ){
                             menulis[0].className="a";
                           }

                       }


                    </script>
