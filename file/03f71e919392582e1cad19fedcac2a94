<script type="text/javascript">
    var logOutHtml =  ' <a href="http://sso.baofeng.net/api/server/logout?from=i&amp;next_action='+location.href+'">退出</a>';
    var loginHtml  = '<a href="javascript:miniSSOLogin.login()">登录</a> <a href="javascript:miniSSOLogin.register()">注册</a>';

    //登录成功回调方法，处理页面相关显示
    function LoginSuccessCallback(res){
        var flag = res.status;
        if(flag){
            document.getElementById('test').innerHTML =  res.info.username + logOutHtml;
            var curr_url = parent.location.href;
            var index_num = curr_url.indexOf('#');
            index_num = (index_num > 0) ? index_num : curr_url.length;
            curr_url = (curr_url.substring(0, index_num));
            if(BROWSER.ie){

            }
            window.location.href = curr_url;
        }
    }

</script>
