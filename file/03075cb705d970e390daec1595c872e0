	FrontProducts_list01['FrontProducts_list01-1433231806780_init'] = function() {
		jQuery(".specChangeImg").click(function(){
			var data = $(this).metadata();
			var srcStr=data.smallImgUrl;
			var defaultImg=data.defaultImg;
			$(this).parent().find(".specChangeImg").removeClass("click");
			$(this).addClass("click")
			if(srcStr!=""){
				jQuery("#FrontProducts_list01-1433231806780_"+data.id).attr("src",srcStr);
			}else{
				if(defaultImg!=""){
					jQuery("#FrontProducts_list01-1433231806780_"+data.id).attr("src",defaultImg);
				}else{
					jQuery("#FrontProducts_list01-1433231806780_"+data.id).attr("src","/images/common/noimage.jpg");
				}
			}
		});
	};
