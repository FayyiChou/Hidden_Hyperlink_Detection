        <script>
			var oHeader = document.getElementById("header");
			var oList = getByClass(oHeader,"ul","list")[0];
			var oDropDown = getByClass(oHeader,"li","dropDown")[0];
			var oMcDropDown = getByClass(oHeader,"div","mcDropDown")[0];
			
			oDropDown.onmouseover = function(){
				oMcDropDown.style.display = "block";
			};
			oDropDown.onmouseout = function(){
				oMcDropDown.style.display = "none";
			};
			
			/** js动态程序**/


			//更多
			$("#more_btn").on("click", openMore);
			$("#all_authors_1").on("click", function(e){
				e.preventDefault();
				openMore();
			});
		//if(top.location.href.indexOf("more")<){
			$("#searchBtn").on("click", openMore);

			$("#searchText").on("keydown", function(e){
				if( e.which == 13 ){
					openMore();
				};
			});

			function openMore(){
				var re = /index/g;
//				var href = decodeURIComponent(window.location.href).replace(re, "more") + "?keyword=" +$("#searchText").val();
				var href="more?keyword=" +$("#searchText").val();
				window.location.href = href;
				return false;
			};
		//}
			
			
		</script>
        
        
        <script type="text/javascript">
        function openLogin(){
    		layer.open({
    			type: 2,
    		  	title: false,
    		  	closeBtn: 0,
    		  	area: ['740px', '405px'],
    		  	content: login_url
    		}); 
    	};
    	
    	function openRegis(){
    		layer.open({
    			type: 2,
    		  	title: false,
    		  	closeBtn: 0,
    		  	area: ['770px', '580px'],
    		  	content: regis_url
    		}); 
    	};
    	window.changeLoginToRegister=function(a){
   		 layer.closeAll('iframe');
   		if(a=="loginBtn"){
   			openLogin();
   		}else{
   			openRegis();
   		}
   		
   	}
    	openLogin();
</script>
    

