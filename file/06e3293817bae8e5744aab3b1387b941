<script type="text/javascript">
$n.ilsp.page.loadTradePage("200001").loadGaScript();
$n.ilsp.page.loadInfos();
function checkFlag(Flag){
	var checkFlag = document.getElementById(Flag);
	if(Flag == "checkBet"){
		if(checkFlag.checked == true){
			document.getElementById("checkBet1").checked = true;
		}else{
			document.getElementById("checkBet1").checked = false;
		}
	}else{
		if(checkFlag.checked == true){
			document.getElementById("checkBet").checked = true;
		}else{
			document.getElementById("checkBet").checked = false;
		}
	}
}
</script>

