<script type="text/javascript">
var dltgp,issueNum;
$(document).ready(function() {
	dltgp = new GamePage();
	issueNum = $('#issueNum').val();
	dltgp.init('200001',LOT_CONST.GI_DLT_PT,LOT_CONST.PT_SINGLE);	
	dltgp.betTime();
    getDltDiv("pt");//普通
  /*   var userStatus = $("#userStatus").val();
    if(userStatus && parseInt(userStatus)  == 2){
    	showSalearea([{"areaList":["21","23","34","36","42","12","13","45","52","61","65","50","14","15","22","32","33","11","31","35","37","41","43","44","46","51","53","54","62","63","64"],"gameNum":"200001"}]);//销售省份显示，在gpage.js文件中
    	$("#area").parent().show();
    } */ 
    changeDltGame("");
    if(location.href.indexOf("serialnum=") > 0){
    	this.rdmBnArray = $n.ilsp.upc.dltDetail(dltgp);
    }
    
});
$("#change_pt a").click(function(){
	changeDltGame($(this).attr("name"));
});

function changeDltGame(name){
	if(name == ""){
		name="pt";
	}
	var tar = $("#change_pt a[name="+name+"]");
	var val = name;
	var div = ["pt","dt","nt","xy"];
	tar.siblings().removeClass("on").end().addClass("on");	
	for(var i=0;i<div.length;i++){
		if(val==div[i]){
			getDltDiv(val);
			$("#fallwear_"+val).show();
		}else{
			$("#fallwear_"+div[i]).html('');
			$("#fallwear_"+div[i]).css('display','none');
		}
	}
	$("ul#betNumList li").removeAttr("flag");
}

function getDltDiv(dt){
	$.ajax({type:"get",
		url:'/dltDiv.htm?method=dltDiv&pt='+dt,
		dataType:"html",
		cache:false,
		async:false,
		success:function(data){
			var p=$("#fallwear_"+dt);
			p.html(data);
			$("div.home_l_mid").show();
			$("div.home_l_mid01").show();
			$("#betNumList").parent().css("width","");
			$("#betNumList").css("width","545px");
			if(dt == "pt"){
				$('#dltRdm').css('display','');
				$('#dtRdm').css('display','none');
			}else if(dt == "dt"){
				$('#dltRdm').css('display','none');
				$('#dtRdm').css('display','');
				$("#betNumList").parent().css("width","485px");
				$("#betNumList").css("width","465px");
			}else{
				$('#dltRdm').css('display','');
				$('#dtRdm').css('display','none');
			}
			pageInit(dt);
			//因为页面在加载不完全的情况下计算的弹出框高度不整齐，此弹出框加在这里即为了在页面完全加载时计算页面的高度。
		    $n.ilsp.box.testMessageBox_Switch("彩票");
		}
	});
}
function pageInit(val){
	dltgp = new GamePage();
	if(val=="xy"){
		dltgp.init('200001',LOT_CONST.GI_DLT_XY,LOT_CONST.GI_DLT_XY_SX);
	}else if (val=="nt"){
		dltgp.init('200001',LOT_CONST.GI_DLT_NT,LOT_CONST.PT_SINGLE);
	}else if(val=="pt"){
		dltgp.init('200001',LOT_CONST.GI_DLT_PT,LOT_CONST.PT_SINGLE);
		dltgp.bwall.showDltMiss(issueNum,"yl");
	}else if(val=="dt"){
		dltgp.init('200001',LOT_CONST.GI_DLT_DT,LOT_CONST.PT_BASEROUND);
		dltgp.bwall.showDltMiss(issueNum,"yl");
	}
	//玩法切换
	var options = $('ul#betNumList li');
	if(options.length > 0){
		//重新为删除按钮绑定事件
		 var elmli = options.children('div.f_r');
		//初始化删除事件
		elmli.each(function(){
			 $(this).children('a:eq(1)').unbind('click').bind('click',function(){
				dltgp.basket.delElemOption(this);
			}).css('color','');
			 $(this).children('input')[0].disabled = false; 
			
		});
	}
}

</script>

