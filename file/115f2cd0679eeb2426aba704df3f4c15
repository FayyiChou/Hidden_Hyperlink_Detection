	<script>
		$(document).ready(function () {
			//全国 [切换]
			$(".l_show ul li").each(function (i) {
				$(this).attr("l_show", i)
			});

			$(".l_show ul li").bind("click ", function () {
				$(".l_show ul li").removeClass("over");
				$(this).addClass("over");
				$(".show").hide();
				$(".show").eq($(this).attr("l_show")).show();
			});
			$("#Settled").click(function () {
				$("#maskSettled").show();
				$('body').css({"height": "100%", "overflow": "hidden"});
			});
			$("#shutSettled").click(function () {
				$("#maskSettled").hide();
				$('body').css({"height": "", "overflow": ""});
			});
		});
	</script>

