<script type="text/javascript">
var who1;
var who2;
function create_stokke(){
	document.getElementById('appendHere').innerHTML = '<div id="stokke_container"><div id="stokke_video" title="STOKKE"><a href="javascript:close_stokke()" id="stokke_close">[ cerrar ]</a>'
			+'<iframe style="max-width:100%" id="dialog_iframe" scrolling="no" width="650" height="460" title="STOKKE" '
			+'frameborder="0" title="STOKKE" '
			+'src="http://media.stokke.com/tripptrapp/movie.aspx?lang=es' 
			+ '&RETURNSUCCESS=http://kidshome.es/returnStokke.html' /*+ document.getElementById("buy_block").attributes["action"].value*/
			+'"></iframe></div></div>';
}
function close_stokke()
{
	$('#stokke_container').hide();
}
function addToCart()
{
	$('#stokke_container').hide();
	who2.hide();
	who1.show();
	who1.click();
}
$("li.ajax_block_product").each(function(index){
	var imglink = $(this).find("a.product_img_link");
	var imglink_url = imglink.attr('href');
	if(imglink_url.toLowerCase().indexOf("tripp") > -1 && imglink_url.indexOf("trona-") > -1) {
		var addcart = $(this).find('a.ajax_add_to_cart_button');
		if(addcart.length > 0) {
			addcart.hide();
			$(this).find('div.act_box').append('<a class="stokke btn btn-default" href="javascript;" rel="nofollow" title="Añadir al carrito" data-id-product-attribute="0" data-id-product="4430" data-minimal_quantity="1"><div><i class="icon-glyph icon-small icon_btn icon-mar-lr2"></i><span>Añadir al carrito</span></div></a>');
			$(this).find('a.stokke').click(function(event){
				event.preventDefault();
				who1 = addcart;
				who2 = $(this);
				$('#stokke_container').remove();
				create_stokke();
				$('#stokke_container').show();
			});
		}
	}
});
</script>
	
