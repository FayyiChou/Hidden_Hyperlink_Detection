<script>
var roll_image = new Array;
roll_image[0]='apDiv1';
roll_image[1]='apDiv2';
roll_image[2]='apDiv3';
roll_image[3]='apDiv4';
var dj=new Array;
dj[0]='dj1';
dj[1]='dj2';
dj[2]='dj3';
dj[3]='dj4';
var flag=0;
function roll(i)
{
	
	
  for(var j=0;j<roll_image.length;j++){
	  document.getElementById(roll_image[j]).style.visibility='hidden';
	  document.getElementById(dj[j]).style.color='black';
	  document.getElementById(dj[j]).style.backgroundColor='#F3F3F3';
	  }
  document.getElementById(roll_image[i]).style.visibility='visible';
  document.getElementById(dj[i]).style.color='white';
  document.getElementById(dj[i]).style.backgroundColor='#990000';
  flag=i;

}
window.setInterval('sho()',3000);
function sho()
{
  switch(flag)
  {

    case 0:
    {
      document.getElementById(dj[0]).click();    
      flag=1;
      break;
    }  

    case 1:
    {
      document.getElementById(dj[1]).click();    
      flag=2;
      break;
    }  
    
    case 2:
    {
      document.getElementById(dj[2]).click();    
      flag=3;
      break;
    }  
    
    case 3:
    {
      document.getElementById(dj[3]).click();    
      flag=0;
      break;
    }  



  }
  
}

function showTooltip(target,e) {
	  
    var divlabel = document.getElementById(target.id.replace(/^label/,'tooltip'));
    var mousePos = getMousePos(e);
    divlabel.style.left = mousePos.x+"px";
    divlabel.style.top = mousePos.y+"px";
    divlabel.style.display = 'block';
    divlabel.style.display = 'block';
    divlabel.style.width = '280px';
    divlabel.style.border = '1px solid #000';
}  
function hideTooltip(target) { 
	   
    document.getElementById(target.id.replace(/^label/,'tooltip')).style.display = 'none';  
}


function getMousePos(event) {
            var e = event || window.event;
            var scrollX = document.documentElement.scrollLeft || document.body.scrollLeft;
            var scrollY = document.documentElement.scrollTop || document.body.scrollTop;
            var x = e.pageX+15 || e.clientX + scrollX+15;
            var y = e.pageY+15 || e.clientY + scrollY+15;
            return { 'x': x, 'y': y };
        }
function isNews(docDate)
{
    var ddate = docDate.split('-');
    mydate=new Date();
    var year=mydate.getFullYear(); 
    var month=(month="0"+(mydate.getMonth()+1)).substr(month.length-2,2); 
    var day=(day="0"+mydate.getDate()).substr(day.length-2,2); 
    if(ddate[0]==year && ddate[1]==month && (parseInt(day)-parseInt(ddate[2]))<3 )
    {
       document.write("<img src='./images/new.gif' /> ");
    }
}
var int;
var index=0;
var pics;
var len;
function jgShow()
{
   pics= document.getElementById('jg').getElementsByTagName('div');
   len = pics.length;
   autoSwitch();
   int=setInterval("autoSwitch()",5000);
   document.getElementById('jg').onmouseover(function() {
      clearInterval(int);
   });
   document.getElementById('jg').onmouseout(function()  {
      int=setInterval("autoSwitch()",5000);
   });
}
function autoSwitch()
{
   if(index>=len) {index=0;}
   showpic(index);
   index=index+1;
}
function showpic(index)
{
    for(var i=0; i<len; i++) {
      
      if(i == index) {
        pics[i].style.visibility="visible";
      } else {
        pics[i].style.visibility="hidden";
      }
   }
}
window.onload=jgShow;


 var count = 0;
   function wbegtr(num) {
      if(num%4==0 && count==0) {
          document.write("<tr>");
      }
   }
   function wendtr(num) {
      if(num%4==3 && count==4) {
          document.write("</tr>");
      }
   }
   function wtd(content) {
      if(count>=4) {
        count=0;
      }
      document.write(content);
      count++;
   } 
   function showlinks(num,content) {
      num = num-1;
      wbegtr(num);
      wtd(content);
      wendtr(num);
   }
</script>

