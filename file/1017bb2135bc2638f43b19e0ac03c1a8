        $('.zy_nr_xq').mouseover(function() {
            $(this).children('.ys').hide();
            $(this).children('.lk').show();
        });
        $('.zy_nr_xq').mouseout(function() {
            $(this).children('.ys').show();
            $(this).children('.lk').hide();
        });

