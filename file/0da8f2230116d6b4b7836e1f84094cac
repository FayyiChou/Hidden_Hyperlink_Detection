function switchxxk(modtag,modcontent,modk) {
    for(i=0; i <2; i++) {
      if (i==modk) {
        document.getElementById(modtag+i).className="selectTag";document.getElementById(modcontent+i).className="ccenter";
	  } else {
        document.getElementById(modtag+i).className="";document.getElementById(modcontent+i).className="menuNo";
	  }
    }
}
