var left = document.getElementsByTagName('dl');
for(i=0;i<left.length;i++){
if(left[i].className =='pil cl'){
var dt = left[i].getElementsByTagName('dt');			
var dd = left[i].getElementsByTagName('dd');
for(j=0;j<dd.length;j++){
