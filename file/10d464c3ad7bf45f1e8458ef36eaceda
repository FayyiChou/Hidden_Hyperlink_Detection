<script src="//hm.baidu.com/hm.js?0b7b58f14af0700dff3e3f38f9439b2b"></script><script type="text/javascript" src="../global/js/jquery-1.10.2.min.js"></script>
<script type="text/javascript" src="../global/js/function.js"></script>
<script type="text/javascript">
	function hideHtyList(){
		$("#htylist").hide();
	}
	
	$( document ).ready(function() {	    
		//search form
		$('#searchform input:text').focus(function() {
			if($(this).val() == '请输入关键字'){
				$(this).val('');
			}
		}).blur(function() {
			if($(this).val() == ''){
				$(this).val('请输入关键字');
			}
		});
		
		$('#searchform img:eq(3)').css("cursor","pointer").click(function(){
			$("#searchform").attr("action", $('#searchform select').val()+".php");
			$('#searchform').submit();
		});
		
		$('#searchform input:text').on('keydown', function(e) {			
			e.keyCode == 13 && $('#searchform').submit();;
		});
		
		//submenu
		$("#hty").mouseover(function(){
			var currentPosLeft = $(this).offset().left;
			var currentPosTop = $(this).offset().top;			
			$("#htylist").css({
							  "top": currentPosTop+20,
							  "left": currentPosLeft
						});
			$("#htylist").show();
		});
		$("#hty").mouseout(function(){
			timeouthandle = setTimeout(hideHtyList,500);
		});
		
		$("#htylist").mouseover(function(){
			clearTimeout(timeouthandle);
			$("#htylist").show();
		});
		$("#htylist").mouseout(function(){
			hideHtyList();
		});
	});
</script>
