<script>
if ( document.getElementById("pictureContent") ){
	pictureContent=document.getElementById("pictureContent").innerHTML;
	var linkArrays=pictureContent.match(/<a[^>]*>(.+?)<\/a>/img);
	if ( linkArrays!=null ){
		for( var i=0;i<=linkArrays.length-1;i++ ){
			linkArray=linkArrays[i].match(/<a[^>]*>(.+?)<\/a>/i);
			if ( linkArray[1].search(/<img[^<>]*>/i)>-1 ){
				pictureContent=pictureContent.replace(linkArrays[i], linkArray[1]);
			}
		}
		document.getElementById("pictureContent").innerHTML=pictureContent;
		document.getElementById("pictureContent").style.display="";
	}
}
function getElementsByName(tag,eltname){
  var elts=document.getElementsByTagName(tag);
  var count=0;
  var elements=[];
  for(var i=0;i<elts.length;i++){
     if(elts[i].getAttribute("name")==eltname){
        elements[count++]=elts[i];
     }
  }
  return elements;
}
window.onload=function(){
	var wid=$('#pictureContent img').width();
	$(".nph_photo_prev").css('width',wid/2+20);
	$(".nph_photo_next").css('width',wid/2+20);
	$('#bigtosmall').change(function(){
		var maxWidth=screen.width;
		if($(this).attr('checked')){
			if(maxWidth<wid){
				$('#pictureContent img').attr('width',maxWidth);
			}
		}else{
			$('#pictureContent img').attr('width',wid);
		} 
	});
	$('#comic_tip').css({
		position:'absolute',
		left: ($(window).width() - $('#comic_tip').outerWidth())/2,
		top: ($(window).height() - $('#comic_tip').outerHeight())/2 + $(document).scrollTop()
	});
	$('#closeBtn').click(function(){
		$('#comic_tip').hide();
	});


	var pageSelect=document.getElementById("p__select");
	var linkaddress=document.getElementById("nextArtLinkUrl").value;
	var nextPage="";
	if (pageSelect){
		for (var i=0;i<=pageSelect.length;i++){
			if (pageSelect.options[i].selected==true){
				if ( pageSelect.options[i+1] ){
					nextPage=pageSelect.options[i+1].value;
				}
				break;
			}
		}
		if ( nextPage=="" ){
			if ( pageSelect.length>1 ){
				if (linkaddress!="" ){//重置下一话图标和链接
					for(i=0;i <getElementsByName("li","page__next").length;i++){
					  getElementsByName("li","page__next")[i].innerHTML="<a href='"+linkaddress+"' target=\"_blank\">下一话</a>";
					}
					 document.getElementById('next_comic_page').href=linkaddress;
				}else{//重置返回目录图标和链接
					for(i=0;i <getElementsByName("li","page__next").length;i++){
					  getElementsByName("li","page__next")[i].innerHTML="<a href='http://"+window.location.host+"' target=\"_blank\">下一话</a>";
					}

					 document.getElementById('next_comic_page').href='http://'+window.location.host;
					
				}
			}
		}
	}
}

function goNextPage(){
	var pageSelect=document.getElementById("p__select");
	var nextaddress=document.getElementById("nextArtLinkUrl").value;
	var nextPage="";
	if (pageSelect){
		for (var i=0;i<=pageSelect.length;i++){
			if (pageSelect.options[i].selected==true){
				if ( pageSelect.options[i+1] ){
					nextPage=pageSelect.options[i+1].value;
				}
				break;
			}
		}
		if ( nextPage=="" ){
			if ( pageSelect.length>1 ){
				if (nextaddress!="" ){
					 document.getElementById('comic_tip').style.display='block';
                            /*if ( confirm("已经是最后一页了, 是否返回下一话?") ){
                     location.href=nextaddress;
                     }*/ 
				}else{
					if ( confirm("已经是最后一页了, 是否返回首页?") ){
						location.href="/";
					}
				}
			}
		}else{
			location.href=nextPage;
		}
	}
}

function drag(evt) {
	evt = evt || window.event;
	if (document.all && evt.button != 1) {
		return false;
	}
	pictureContent = document.getElementById('pictureContent');
	
	if (pictureContent.addEventListener) {
		pictureContent.addEventListener("mousemove", moveHandler, true);
		pictureContent.addEventListener("mouseup", upHandler, true);
	} else if (pictureContent.attachEvent) {
		pictureContent.setCapture( );
		pictureContent.attachEvent("onmousemove", moveHandler);
		pictureContent.attachEvent("onmouseup", upHandler);
		pictureContent.attachEvent("onlosecapture", upHandler);
	} else {
		var oldmovehandler = pictureContent.onmousemove;
		var olduphandler = pictureContent.onmouseup;
		pictureContent.onmousemove = moveHandler;
		pictureContent.onmouseup = upHandler;
	}
	mouseLeftRelative=evt.screenX;
	mouseTopRelative=evt.screenY;
	if (evt.stopPropagation) evt.stopPropagation( );
	else evt.cancelBubble = true;	
	if (evt.preventDefault) evt.preventDefault( );
	else evt.returnValue = false;	
	if (evt.stopPropagation) evt.stopPropagation( );
	else evt.cancelBubble = true;	
	pictureContent.style.cursor = "move";
	function moveHandler(evt) {
		mouseLeftRelative2=evt.screenX;
		mouseTopRelative2=evt.screenY;
		//pictureContent.scrollLeft-=mouseLeftRelative2-mouseLeftRelative ;
		document.documentElement.scrollLeft-=mouseLeftRelative2-mouseLeftRelative ;
		document.documentElement.scrollTop-=mouseTopRelative2-mouseTopRelative ;
		//window.scrollTo(0, document.documentElement.scrollTop - (mouseTopRelative2-mouseTopRelative));	

		mouseLeftRelative=evt.screenX;
		mouseTopRelative=evt.screenY;
		
		if (evt.stopPropagation) evt.stopPropagation( );
		else evt.cancelBubble = true;
	}
	function upHandler(evt) {
		if (pictureContent.removeEventListener) {
			pictureContent.removeEventListener("mouseup", upHandler, true);
			pictureContent.removeEventListener("mousemove", moveHandler, true);
		} else if (pictureContent.detachEvent) {
			pictureContent.detachEvent("onlosecapture", upHandler);
			pictureContent.detachEvent("onmouseup", upHandler);
			pictureContent.detachEvent("onmousemove", moveHandler);
			pictureContent.releaseCapture( );
		} else {
			pictureContent.onmouseup = olduphandler;
			pictureContent.onmousemove = oldmovehandler;
		}
		if (evt.stopPropagation) evt.stopPropagation( );
		else evt.cancelBubble = true;
	}
}
var pictureContent = document.getElementById('pictureContent');
if ( pictureContent ){
	pictureContent.onmousedown = drag;
}
</script>

 

