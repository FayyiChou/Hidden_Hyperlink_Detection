			var speeds=30
			demo2.innerHTML=demo1.innerHTML
			function Marquees(){
			if(demo2.offsetWidth-demo.scrollLeft<=0)
			demo.scrollLeft-=demo1.offsetWidth
			else{
			demo.scrollLeft++
			}
			}
			var MyMars=setInterval(Marquees,speeds)
			demo.onmouseover=function() {clearInterval(MyMars)}
