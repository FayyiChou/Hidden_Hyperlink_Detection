<script type="text/javascript">


var bgArray = ['#0054D8','#2E9BDE','#C51F35','#CACACA','#FFC100','EAE1D8'];

$('#bannerCarousel').on('slid.bs.carousel', function () {
    var index = $('#bannerCarousel').find('div.active').index();
    index = index%bgArray.length;
    $('#wideHeader').css('background', bgArray[index]);
});

$( document ).ready(function() {
	$('#wideHeader').css('background', bgArray[0]); 
});
 
