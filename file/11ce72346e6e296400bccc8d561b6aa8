<script>
var newsboxList = "newsbox|newsbox1|newsbox2|newsbox3";//第一个样式前导名
var xxjjboxList = "xxjjbox|xxjjbox1|xxjjbox2";
var jyjxboxList = "newsbox|jyjxbox01|jyjxbox02";
var dytdboxList = "newsbox|dytdbox1|dytdbox2|dytdbox3|dytdbox4";
var tdjsboxList = "newsbox|tdjsbox01|tdjsbox02|tdjsbox03";
var linksboxList = "rightlinks|linksbox01|linksbox02|linksbox03";
function divSwitchFun(objName,listObj)
{
	var l = listObj.split("|");
	for(i=1;i<l.length;i++){
		if(l[i]==objName){
			document.getElementById(l[i]).className = l[0]+"2";
			document.getElementById("home"+l[i]).style.display = "block";
		}else{
			document.getElementById(l[i]).className = l[0]+"1";
			document.getElementById("home"+l[i]).style.display = "none";
		}
	}
	//for(i=1;i<l.length;i++){
	//	if(l[i]==objName){
	//		document.getElementById("home"+l[i]).style.display = "block";
	//	}else{
	//		document.getElementById("home"+l[i]).style.display = "none";
	//	}
	//}
}
</script>
