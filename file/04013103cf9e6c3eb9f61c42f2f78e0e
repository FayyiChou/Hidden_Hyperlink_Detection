            <script>
                                var speed1 = 30
                                var tab = document.getElementById("mo");
                                var tab1 = document.getElementById("mo1");
                                var tab2 = document.getElementById("mo2");
                                tab2.innerHTML = tab1.innerHTML
                                function Marquee1() {
                                    if (tab2.offsetWidth - tab.scrollLeft <= 0)
                                        tab.scrollLeft -= tab1.offsetWidth
                                    else {
                                        tab.scrollLeft++
                                    }
                                }
                                var MyMar1 = setInterval(Marquee1, speed1)
                                tab.onmouseover = function () { clearInterval(MyMar1) }
                                tab.onmouseout = function () { MyMar1 = setInterval(Marquee1, speed1) }
                            </script>
