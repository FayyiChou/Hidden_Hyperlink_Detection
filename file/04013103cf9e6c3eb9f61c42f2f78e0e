            <script type="text/javascript">
                                var speed_1001 = 50;
                                var marquePic2 = document.getElementById("marquePic2");
                                var marquePic1 = document.getElementById("marquePic1");
                                var hddemo = document.getElementById("hdxxdemo");

                                marquePic2.innerHTML = marquePic1.innerHTML;
                                function Marquee_hd() {
                                    if ((marquePic2.offsetWidth - hddemo.scrollLeft) <= 0) {
                                        hddemo.scrollLeft -= marquePic1.offsetWidth;
                                    } else {
                                        hddemo.scrollLeft++;
                                    }
                                }
                                var MyMarhd = setInterval(Marquee_hd, speed_1001);
                                hddemo.onmouseover = function () { clearInterval(MyMarhd); }
                                hddemo.onmouseout = function () { MyMarhd = setInterval(Marquee_hd, speed_1001); } 
                            </script></div>
