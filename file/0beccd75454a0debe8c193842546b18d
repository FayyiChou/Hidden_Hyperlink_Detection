    function swap_xwjj(tt){
			switch(tt){
				case"xwjj1":
				document.getElementById("xwjj1").style.display = "block";
				document.getElementById("xwjj2").style.display = "none";
				document.getElementById("xwjj3").style.display = "none";
					break;
				case"xwjj2":
				document.getElementById("xwjj1").style.display = "none";
				document.getElementById("xwjj2").style.display = "block";
				document.getElementById("xwjj3").style.display = "none";
					break;
				case"xwjj3":
				document.getElementById("xwjj1").style.display = "none";
				document.getElementById("xwjj2").style.display = "none";
				document.getElementById("xwjj3").style.display = "block";
					break;
			}
		}
