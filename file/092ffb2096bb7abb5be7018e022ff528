            <script>
        function liSum(wrap,list,pageUL,pageCUR){
            var len = jQuery(wrap).find(list).length;
            var i=1;
            var UL = "<ul class='pagination'>";
            for(i;i<=len;i++){
                if(i==1){
                    UL += "<li class="+pageCUR+"><a href='javascript:void(0);'>1</a></li>";
                }else{
                    UL += "<li><a href='javascript:void(0);'>"+i+"</a></li>";
                }
            }
            UL += "</ul>";
            jQuery(wrap).append(UL);
            return false;
        }

        function fixedopt(){
            if(document.documentElement.clientWidth < 1400 )
            {
                jQuery("#fixed-opt").attr("class","fixed-opt-r");
            }
            else
            {
                jQuery("#fixed-opt").attr("class","fixed-opt-l");
            }
        }

        function goTop()
        {
            $(window).scroll(function(e) {
                if($(window).scrollTop()>100)
                {
                    $(".returnTop,.returnTop2").css('display','block').fadeIn(1000);
                }
                else $(".returnTop,.returnTop2").fadeOut(1000);
            });
        };

        $(document).ready(function(){
            fixedopt();

            goTop();

            $(".returnTop,.returnTop2").click(function(e) {
                $('body,html').animate({scrollTop:0},1000);
            });

            $(".tab-menu li").click(function(e) {
                $('.tab-menu li').removeClass('active');
                $(this).addClass('active');
            });

            /* 输入框默认提示*/
            jQuery(".brand-panel").hover(function(){
                jQuery(".brandmore-panel").show();
            },function(){
                jQuery(".brandmore-panel").hide();
            });
            /* end 输入框默认提示*/

            /* 侧边二维码*/
            jQuery(".fixed-code").click(function(){
                jQuery(".fixed-opcode").show();
            });
            jQuery(".icon-close").click(function(){
                jQuery(".fixed-opcode").hide();
            });
            /* end 侧边二维码*/

            /* 输入框默认提示*/
            jQuery(".sc-sty,.input-sty,.signup-txt").each(function(){

                if(jQuery(this).val()){
                    jQuery(this).siblings(".input-note").hide();
                    jQuery(this).addClass('active');
                }else{
                    jQuery(this).siblings(".input-note").click(function(){
                        jQuery(this).hide().siblings("input").focus();
                    });
                }
                jQuery(this).focus(function(){
                    jQuery(this).siblings(".input-note").hide();
                    jQuery(this).addClass('active');
                });
                jQuery(this).blur(function(){
                    setTimeout(function () {
                        var txt = jQuery(this).val();
                        if(txt){
                            jQuery(this).siblings(".input-note").hide();
                            jQuery(this).addClass('active');
                        }else{
                            jQuery(this).siblings(".input-note").show();
                            jQuery(this).removeClass('active');
                        }
                    },500)

                });
            });
            /* end 输入框默认提示*/

        });
    </script>
