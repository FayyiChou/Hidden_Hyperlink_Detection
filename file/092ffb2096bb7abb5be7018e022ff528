<script>
    $(function () {
        $('#biz_brand a.active').length >0 || $('.other-name').before($('.brandtx-dl > dd >a.active'));

        $('.carsort-option a').click(function () {
            $(this).siblings('.active').removeClass('active')
            $(this).addClass('active');
            var car_model = $('#car_model a.active').attr('data-id');
            var biz_gold = $('#biz_gold a.active').attr('data-id');
            var biz_silver = $('#biz_silver a.active').attr('data-id');
            var price = $('#price a.active').attr('data-id');
            var brand_id = $('#biz_brand a.active').attr('data-id') ? $('#biz_brand a.active').attr('data-id') : $('.brandtx-dl > dd >a.active').attr('data-id');
            brand_id = brand_id ? brand_id : 0;
            window.location.href = '?biz_gold='+biz_gold+'&biz_silver='+biz_silver+'&car_model='+car_model+'&price='+price+'&brand_id='+brand_id;
            return false;
        });
        $('.brandtx-dl > dd >a').click(function () {
            $('.brandtx-dl > dd >a.active').removeClass('active')
            $(this).addClass('active');
            var car_model = $('#car_model a.active').attr('data-id');
            var biz_gold = $('#biz_gold a.active').attr('data-id');
            var biz_silver = $('#biz_silver a.active').attr('data-id');
            var price = $('#price a.active').attr('data-id');
            var brand_id = $('.brandtx-dl > dd >a.active').attr('data-id');
            brand_id = brand_id ? brand_id : 0;
            window.location.href = '?biz_gold='+biz_gold+'&biz_silver='+biz_silver+'&car_model='+car_model+'&price='+price+'&brand_id='+brand_id;
            return false;
        });

        $('.price-sort-btn').click(function () {
            var car_model = $('#car_model a.active').attr('data-id');
            var biz_gold = $('#biz_gold a.active').attr('data-id');
            var biz_silver = $('#biz_silver a.active').attr('data-id');
            var price_start = $('#price_start').val();
            var price_end = $('#price_end').val();
            var brand_id = $('.brandtx-dl > dd > a.active').attr('data-id');
            brand_id = brand_id ? brand_id : 0;
            window.location.href = '?biz_gold='+biz_gold+'&biz_silver='+biz_silver+'&car_model='+car_model+'&price_start='+price_start+'&price_end='+price_end+'&brand_id='+brand_id;
            return false;
        });
    });
    //预约看车
    $('.appoint').click(function () {
        $('#appoint-cover').show();
        $('#appoint-form').attr('action',$(this).attr('data-url'));
    });
    $('#appoint-form').submit(function () {
        var data = $(this).serialize();

        $.ajax({
            url:$('#appoint-form').attr('action'),
            type:'post',
            data:data,
            dataType:'json',
            beforeSend:function () {
                $('#appoint-submit').text('提交中...');
                $('#appoint-submit').attr('disabled',true);
            },
            complete:function () {
                $('#appoint-submit').text('立即预约');
                $('#appoint-submit').attr('disabled',false);
            },
            success:function (res) {
                alert(res.msg);
                if (res.status){
                    $('.pop-cover').hide();
                    document.getElementById('appoint-form').reset();
                }
            },
            error:function () {
                alert('请求错误，请稍后再试')
            }
        })
        return false;
    });
    



    function sort_type(type) {
        var url = '?sort='+type;
        var car_model = $('#car_model a.active').attr('data-id');
        url += car_model ? '&car_model='+car_model : url;
        var biz_gold = $('#biz_gold a.active').attr('data-id');
        url += biz_gold ? '&biz_gold='+biz_gold : url;
        var biz_silver = $('#biz_silver a.active').attr('data-id');
        url += biz_silver ? '&biz_silver='+biz_silver : url;
        var price = $('#price a.active').attr('data-id');
        url += price ? '&price='+price : url;
        var brand_id = $('#biz_brand a.active').attr('data-id') ? $('#biz_brand a.active').attr('data-id') : $('.brandtx-dl > dd >a.active').attr('data-id');
        url += brand_id ? '&brand_id='+brand_id : url;
        window.location.href = url;
        return false;
    }


    function getQueryString(name) {
        var r = window.location.search.substr(1);
        if (r != null) return unescape(r[2]); return null;
    }
</script>

