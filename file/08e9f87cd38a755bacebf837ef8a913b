<script type="text/javascript" src="https://res.cngoldres.com/mobile/cngold/js/foot_info.js"></script><script type="text/javascript" src="https://res.cngoldres.com/web/js/mu.js"></script> 
<script type="text/javascript">
            $(document).ready(function(){
                getQuote();
                setInterval(function(){getQuote();},60*1000);
                //导航更多弹开收起
                $('.top-menu .more').click(function(){
                    $(this).next('.pNav').slideDown();
                    $(this).hide();
                    $(this).parent().find(".amore").show();
                });
                $('.top-menu .back').click(function(){
                    $(this).parent().slideUp();
                    $(this).parent().parent().find(".amore").hide();
                    $('.top-menu .more').show();
                });
            });

            function getQuote(){
                var number = 2;
                var codes = "JO_92233,JO_9753,JO_9754,JO_61813,JO_63737,JO_42760,JO_92271,JO_92272,JO_64084,JO_92237,JO_56315,JO_60892,JO_60369,JO_60378,JO_62037,JO_57206,JO_57207,JO_38740";
                var url="https://api.jijinhao.com/quoteCenter/realTime.htm?codes="+codes;
                $.getScript(url,function(){
                    if(quote_json != null && quote_json != ""){
                        var codeArr = codes.split(',');
                        for(var i in codeArr){
                            var jCode = codeArr[i];
                            var quote = quote_json[jCode];

                            if(quote != undefined) {
                                //最新价
                                var q63Val = quote.q63;
                                q63Val = format(q63Val,number);
                                if(q63Val == "NaN.undefined")q63Val="--";
                                //涨跌值
                                var q70Val = quote.q70;
                                q70Val = format(q70Val,number);
                                if(q70Val == "NaN.undefined")q70Val="--";
                                //涨跌幅
                                var q80Val = quote.q80;
                                q80Val = format(q80Val,number);
                                if(q80Val == "NaN.undefined")q80Val="--";

                                if(q63Val == 0) {
                                    q63Val = "<em>--</em>";
                                    q70Val = "<em>--</em>";
                                    q80Val = "<em>--</em>";
                                } else {
                                    if(q70Val > 0){
                                        q63Val = "<em class='red'>" + q63Val + "↑</em>";
                                        q70Val = "<em class='red'>"+q70Val+"</em>";
                                        q80Val = "<em class='red'>"+q80Val+"%</em>";
                                    } else if(q70Val < 0){
                                        q63Val = "<em class='green'>"+q63Val+"↓</em>";
                                        q70Val = "<em class='green'>"+q70Val+"</em>";
                                        q80Val = "<em class='green'>"+q80Val+"%</em>";
                                    } else if(q70Val == 0){
                                        q63Val = "<em>"+q63Val+"</em>";
                                        q70Val = "<em>"+q70Val+"</em>";
                                        q80Val = "<em>"+q80Val+"%</em>";
                                    }
                                }
                                $("#"+jCode+"_price").html(q63Val);
                                $("#"+jCode+"_updownPercent").html(q70Val);
                                $("#"+jCode+"_updown").html(q80Val);
                            } else {
                                $("#"+jCode+"_price").html("<em>--</em>");
                                $("#"+jCode+"_updownPercent").html("<em>--</em>");
                                $("#"+jCode+"_updown").html("<em>--</em>");
                            }
                        }
                    }
                });
            }

            TouchSlide({
                slideCell:"#picScroll",
                titCell:".hd ul", //开启自动分页 autoPage:true ，此时设置 titCell 为导航元素包裹层
                autoPage:true, //自动分页
                autoPlay:true,//自动运行
                pnLoop:"true", // 前后按钮循环
                switchLoad:"_src" //切换加载，真实图片路径为"_src"
            });
            jQuery(".section-tab").slide({titCell:".hd h2",mainCell:".bd",delayTime:0,trigger:"click"});
            function UlScroll(obj){
                $(obj).find("ul").animate({top:"-67.1875px"},1000,function(){
                    $(this).css({top:"0px"}).find("li:first").appendTo(this);
                });
            }
            setInterval('UlScroll("#scrollKX")',4000);
        </script>


