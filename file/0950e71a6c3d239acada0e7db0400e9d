    <script  type="text/javascript">

    function ShowTime()

    {
		var NowDate = new Date();
		var ShowYear = NowDate.getFullYear();
		var ShowMonth = NowDate.getMonth()+1;
		var ShowDate = NowDate.getDate();
		var h=NowDate.getHours();
		var m=NowDate.getMinutes();
		var s=NowDate.getSeconds();

		var d = NowDate.getDay();
		var dayNames = new Array("星期日", "星期一", "星期二", "星期三", "星期四", "星期五", "星期六");
		document.getElementById('showbox').innerHTML = ShowYear+ '年' + ShowMonth + '月' + ShowDate +'日'+ '　'+dayNames[d] ;
		setTimeout('ShowTime()', 1000);
		
		function Appendzero (obj) { if (obj < 10) return "0" + "" + obj; else return obj; }

		// add a zero in front of numbers<10
		m=checkTime(m)
		s=checkTime(s)
		document.getElementById('showtime').innerHTML= "<span>"+ Appendzero(h)+"</span><span>"+m+"</span><span>"+s+"</span>"
		t=setTimeout('startTime()',500)
		
		function checkTime(i)
		{
		if (i<10) 
		  {i="0" + i}
		  return i
		}
    }

</script>
   	
