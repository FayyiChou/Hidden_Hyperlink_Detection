		  <script>

		  
		  var cursBreads = location.href;
		  
		  function explode( delimiter, string ) { // Split a string by string
    var emptyArray = { 0: '' };
    if ( arguments.length != 2
        || typeof arguments[0] == 'undefined'
        || typeof arguments[1] == 'undefined' )
    {
        return null;
    }
    if ( delimiter === ''
        || delimiter === false
        || delimiter === null )
    {
        return false;
    }
    if ( typeof delimiter == 'function'
        || typeof delimiter == 'object'
        || typeof string == 'function'
        || typeof string == 'object' )
    {
        return emptyArray;
    }
    if ( delimiter === true ) {
        delimiter = '1';
    }
    return string.toString().split ( delimiter.toString() );
}

var explsrt = explode('/', cursBreads);
console.log(explsrt)
if(explsrt[3]=='catalog'){
	var iose = 1;
	$('.breadcrumb a').each(function(){
		if(iose>3){
			$(this).remove();
		}
		iose++;
	})
}


		  
		  
		  $('#edit-submit-1').val('Отправить')
			$('#contact-mail-page').submit(function(){
				var erso = 0;
				if($('#contact-mail-page #edit-name').val()==''){
					$('#contact-mail-page #edit-name').css('border','1px solid red')
					erso=1;
				}
				if($('#contact-mail-page #edit-mail').val()==''){
					$('#contact-mail-page #edit-mail').css('border','1px solid red')
					erso=1;
				}
				if($('#contact-mail-page textarea').val()==''){
					$('#contact-mail-page textarea').css('border','1px solid red')
					erso=1;
				}
				if(erso==0){
					var serials = $(this).serialize();
					
					$.ajax({
					  type: "POST",
					  url: "deds.php",
					  data:serials,
					  success: function(msg){
						if(msg==1){
							$('#contact-mail-page>div').prepend('<p class="oksmess">Заявка отправлена</p><p class="oksmess">Спасибо, наши менеджеры свяжутся с Вами в ближайшее время.</p>')
