<script type = "text/javascript">
/* Скрипт для корзины товаров  */ 
$(document).ready(function() {
	
	$('#node-49 .node_read_more').remove()
	
var cartPrice = $('.cart-block-summary-total .uc-price').text();

var cart = $(".cartys");
var tovar = $(".num-items").text();
if(tovar == 1) {
cart.text(tovar + " товар " + cartPrice);
$(".nazys").text(" товар");

}
else if (tovar > 1 && tovar < 5) {
cart.text(tovar + " товара " + cartPrice);
$(".nazys").text(" товара");
}

else if (tovar >= 5) {
cart.text(tovar + " товаров " + cartPrice);
$(".nazys").text(" товаров");
}

else {
cart.text("Нет товаров");
}


$('.boardsaq').click(function(){
	if($(this).hasClass('actsmne')){
		$(this).removeClass('actsmne')
		$('.callme_formse').slideUp()
	}
	else{
		
		$('.callme_formse').slideDown()
		$(this).addClass('actsmne')
	}
})

$('#webform2004').submit(function(){
	if($(this).find('input[name="phone"]').val()==''){
		$(this).find('input[name="phone"]').css('border','1px solid red')
	}
	else{
		$(this).find('input[name="phone"]').css('border','none');
		var serial = $(this).serialize();
		$.ajax({
		  type: "POST",
		  url: "/feeds.php?type=topFed",
		  data: serial,
		  success: function(msg){
			if(msg==1){
				$('.callme_formse').html('<p class="aiped">Ваша заявка была успешно отправлена. В ближайшее время с вами свяжется наш менеджер.</p>')
			}
		  }
		});
	}
	
	
	return false;
})

$('.closeforma').click(function(){
	$('.blacksrot').fadeOut(200)
})

$('#webforsem200344').submit(function(){
		var flasss = 0;
		if($(this).find('input[name="phone"]').val()==''){
			flasss=1;
			$(this).find('input[name="phone"]').css('border-color','red');	
		}
		else{
			$(this).find('input[name="phone"]').css('border-color','#A9A9A9');	
		}
		if($(this).find('input[name="234"]').val()==''){
			flasss=1;
			$(this).find('input[name="234"]').css('border-color','red');	
		}
		else{
				$(this).find('input[name="234"]').css('border-color','#A9A9A9');	
		}
		if($(this).find('textarea[name="reww"]').val()==''){
			flasss=1;
			$(this).find('textarea[name="reww"]').css('border-color','red');	
		}
		else{
					$(this).find('textarea[name="reww"]').css('border-color','#A9A9A9');	
		}
		
		if(flasss==0){
			var serial = $(this).serialize();
					$.ajax({
		  type: "POST",
		  url: "/feeds.php?type=topFesdd",
		  data: serial,
		  success: function(msg){
			if(msg==1){
				$('.okMesa').fadeIn(100)
			}
		  }
		});
		}
		
	return false;
})




$('.ozvde').click(function(){
	$('.blacksrot').fadeIn(200)
})


});
</script>


