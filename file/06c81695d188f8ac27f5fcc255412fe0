		var pol="https://www.newchinalife.com:443/myxinhua/loginCenter.gsp?legalType=0";
var ins="https://www.newchinalife.com:443/myxinhua/loginCenter.gsp?legalType=1";
  
 
      function check()
      {  
         var fr=document.getElementsByName("legalTypeA"); 
         var tt=1;
         for(var i=0;i<fr.length;i++){
    		var e = fr[i];
			if(e.checked==true)
			{
				tt= e.value;
			}
		 } 
			 if(tt==0)
			 { 
			  	window.location.href=pol;
			 }else{
			   	 window.location.href=ins;
			 }  
     } 
