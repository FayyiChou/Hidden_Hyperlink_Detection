function searchbga() {
	if(document.getElementById("searchValuehead").value=="" ||document.getElementById("searchValuehead").value=="请输入关键字"){
		alert("关键字为必填项");
		document.getElementById("searchValuehead").focus();
		return false;
	}
	document.getElementById("searchValuehead1").value
	 = encodeURI(document.getElementById("searchValuehead").value);
	//searchForm111.action=url;
	document.searchForm111.submit();
}
 
 //防止非法SQL注入
function isWords(obj)
{ 
 //try{
    //防止非法SQL注入
    var objRe = new RegExp(".*;*(use|exec|insert|select|delete|update|master|declare|#).*","i");
    if (objRe.test(obj.value))
    {
     obj.value="";
     alert("有非法标识请重新输入！");
    }
    
    //判断并避免单引号
    if (obj.value.indexOf("'")>-1)
    {
     objRe = /'/g;
     alert("有非法标识请重新输入！");
     obj.value = obj.value.replace(objRe,"");
    }
    
    //判断并避免单>
    if (obj.value.indexOf(">")>-1)
    {
     objRe = />/g;
     alert("有非法标识请重新输入！");
     obj.value = obj.value.replace(objRe,"");
    }
    
    //判断并避免单<
    if (obj.value.indexOf("<")>-1)
    {
     objRe = /</g;
     alert("有非法标识请重新输入！");
     obj.value = obj.value.replace(objRe,"");
    }
  //}catch(e) { alert(e.description); }
 }

//简繁转换时调整div显示及iframe显示
function cnorbig(){
	if(Cookie.Get("l") == "t"){
		if(null!=document.getElementById("f_cn")){
			document.getElementById("f_cn").style.display="none";
		}
		if(null!=document.getElementById("f_big")){
			document.getElementById("f_big").style.display="block";
		}
		if(null!=document.getElementById("c_cn")){
			document.getElementById("c_cn").style.display="none";
		}
		if(null!=document.getElementById("c_big")){
			document.getElementById("c_big").style.display="block";
		}
		//iframe相关繁简
		if(null!=document.getElementById("if_stocksticker")){
			$("#if_stocksticker",parent.document.body).attr("src","http://ir.finet.com.cn/nci/stocksticker.html?lang=zh-hk");
		}
		if(document.getElementById("if_stockinfo")){
			var stockinfopath='';
			
			$("#if_stockinfo",parent.document.body).attr("src",stockinfopath+"lang=zh-hk");
		}
		
	}else if(Cookie.Get("l") == "s"){
		if(null!=document.getElementById("f_cn")){
			document.getElementById("f_cn").style.display="block";
		}
		if(null!=document.getElementById("f_big")){
			document.getElementById("f_big").style.display="none";
		}
		if(null!=document.getElementById("c_cn")){
			document.getElementById("c_cn").style.display="block";
		}
		if(null!=document.getElementById("c_big")){
			document.getElementById("c_big").style.display="none";
		}
		//iframe相关繁简
		if(null!=document.getElementById("if_stocksticker")){
			$("#if_stocksticker",parent.document.body).attr("src","http://ir.finet.com.cn/nci/stocksticker.html?lang=zh-cn");
		}
		if(document.getElementById("if_stockinfo")){
			var stockinfopath='';
			
			$("#if_stockinfo",parent.document.body).attr("src",stockinfopath+"lang=zh-cn");
		}
	}
}
if(false){
$(document).ready(
	function(){
		cnorbig();
	}
);
}
