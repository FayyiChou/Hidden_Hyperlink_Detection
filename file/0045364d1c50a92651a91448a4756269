<script>
$(function() {
	$('.move_up').css('visibility','hidden');
	if ($('.looked_good').size() == 1){$('.move_down').css('visibility','hidden')};

  $('.move_up').click(function(event) {
  	$('.lenta_div').animate(
  		{scrollTop: $('.lenta_div').scrollTop()-200},
  		300,
  		disable_button_up
  	);
  	$('.move_down').css('visibility','visible');
  });

  $('.move_down').click(function(event) {
  	$('.lenta_div').animate(
  		{scrollTop: $('.lenta_div').scrollTop()+200},
  		300,
  		disable_button_down
  	);
  	$('.move_up').css('visibility','visible') ;
  });
  $(".move_up").add(".move_down").hover(
  	function(event) {$(this).css('background-position','50% -30px');},
  	function(event) {$(this).css('background-position','50% 0px');}
  );
})
function disable_button_up () {
	if ($('.lenta_div').scrollTop() <= 2){$('.move_up').css('visibility','hidden')};
}
function disable_button_down () {
  if ($('.lenta_div').scrollTop() >= ($('.looked_good').size()-1) * 200 - 2){$('.move_down').css('visibility','hidden')};
}
</script>
