<script>
$(function() {
	$('.chapters > DIV').one('mouseover', function(){					// íà íàâåäåíèå ìûøè ïîëó÷àåì ìåíþ ñàéòà, íà èêîíêó êîòîðîãî íàâåëèñü - äåëàåì ýòî îäèí ðàç			
		getItemsCommon({'item': $(this).attr('data_num'), 'func_before': 'SetDopLinks'}, 'first_level4site', 'ul_'+$(this).attr('data_num'), '');
	})	.hover(															// ïîêàçûâàåì èëè ñêðûâàåì ìåíþ ïðè íàâåäåíèè íà èêîíêó
		function(event){
			$('.chapters DIV DIV').hide(); 
			var t = $(this).position().top + 50 + $(this).find('DIV').height() - window.innerHeight + 25; // âû÷èñëÿåì, íà ñêîëüêî íå îòêðûâàþùèéñÿ áëîê íå âõîäèò â ýêðàí. (50 - òîï áëîêà ÷àïòåð, 25 - çàïàñ íà âñïëûâàþùóþ õðîìîâñêóþ ñòðîêó)
			$(this).find('DIV').css('top', -Math.max(t, 0)).show(200)
		},
		function(event){
			$('.chapters DIV DIV').hide(); 
		}
	)
})
function SetDopLinks() {
	$('.chapters DIV DIV UL A').not('.dop').each(function(){
		$(this).addClass('dop').attr('href', $(this).attr('href') + '');
	})
}
</script>
