        <script type="text/javascript">
            $(document).ready(function(){
                    jQuery('span[func="recFolding"]').click(function() {
                        var o = jQuery(this);
                        if (o.hasClass('qa-extend')) {
                        jQuery('li[group="2"]').css('display', 'block');
                        o.attr(
                            {
                            'class': 'qa-retract',
                            'title': '收起'
                            }
                            );
                        }
                        else {
                        jQuery('li[group="2"]').css('display', 'none');
                        o.attr(
                            {
                            'class': 'qa-extend',
                            'title': '展开'
                            }
                            );
