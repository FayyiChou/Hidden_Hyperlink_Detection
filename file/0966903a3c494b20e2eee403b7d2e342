<script type="text/javascript">
jQuery.extend({
		loginFun:function(){
			var userName = $("input[name='userName']").val();
			var password = $("input[name='password']").val();
			if(userName==null || userName==''){
				alert("请输入登录名!");
				return false;
			}
			if(!/^[A-Za-z0-9]{6,30}$/.test(userName)){	
				alert("登录名长度必须是6-30位数字或字母");
				return false;
			}
			
			if(password==null || password==''){
				alert("请输入密码!");
				return false;
			}
			
			if(!/^[A-Za-z0-9]{6,30}$/.test(password)){	
				alert("密码长度必须是6-30位数字或字母");
				return false;
			}
			return true;
		},
		doLogin:function(){
			//登录次数
			var times = $("input[name='loginTimes']").val();
			if(times >= 3){
				$("#logImg").hide();
				$("#myLog").show();
				var userName = $("input[name='userName']").val();
				var password = $("input[name='password']").val();
				var dialog = $.dialog({
					title:"安全验证",
					width:550,
					height:320,
					id:"safeCode",
					lock:true,
					opacity:0.4
				});
				$.ajax({
					url:"http://www.xiongxinkm.com:80/front/checkCodeLogin.htm?userName="+userName+"&password="+password+"&loginTimes="+times,
					dataType:"html",
					type:"post",
					success:function(data){
						dialog.content(data);
					}
				});	
			}else{
				if($.loginFun()){
					$("#loginForm").submit();
				}
			}
		}
	});
	$(document).ready(function(){
		//更换验证码
		$("#changeImg").click(function(){
			$(this).attr("src","frontCheckCode.htm?d="+new Date());
		});
		$("#userName").val("输入用户名");
		$("#userName").focus(function(){
			var valss=$(this).val();
			if(valss=="输入用户名"){
				$("#userName").val("");
			}
		});
		$("#userName").blur(function(){
			var valss=$(this).val();
			if(valss==""){
				$("#userName").val("输入用户名");
			}
		});
		$("#loginForm").ajaxForm({
			url : "frontLogin.htm",
			type:"post",
			success : function(data) {
				var code = data.code;
				if (code == 10000) {
					window.location.href = "http://www.xiongxinkm.com:80/front/main.htm";
				}else{
					$("#logImg").hide();
					$("#myLog").show();
					if(data.mess!=undefined){
						if (code == 2001) {
							var userName = $("input[name='userName']").val();
							var password = $("input[name='password']").val();
							var dialog = $.dialog({
								title:"安全验证",
								width:550,
								height:320,
								id:"safeCode",
								lock:true,
								opacity:0.4
							});
							$.ajax({
								url:"http://www.xiongxinkm.com:80/front/safeCodeLogin.htm?userName="+userName+"&password="+password+"&loginTimes="+data.loginTimes,
								dataType:"html",
								type:"post",
								success:function(data){
									dialog.content(data);
								}
							});
						}else{
							$("input[name='loginTimes']").val(parseInt(data.loginTimes)+1);
							alert(data.mess);
							$("#changeImg").attr("src","frontCheckCode.htm?d="+new Date());
						}
					}
				}
			}
	    });
		//登录
		$("input[name='loginBtn']").click(function() {
			if($.loginFun()){
				$("#logImg").show();
				$("#myLog").hide();
				$.doLogin();
			}
		});
		//键盘事件
		document.onkeydown = function(event){
			var e = event||window.event||arguments.callee.caller.arguments[0];
			if(e && e.keyCode==13){
				if($.loginFun()){
					$.doLogin();
				}
			}
		};
	});
</script>
