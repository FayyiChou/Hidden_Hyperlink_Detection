        <script>ZK.ns("ZK.utils.ScrolledPlaceholders");
            ZK.utils.ScrolledPlaceholders = new(function() {
                this.attach = function() {
                    $(window).bind("scroll.scrolledPlaceholders", $.proxy(this.handleScroll, this));
                };
                this.detach = function() {
                    $(window).unbind(".scrolledPlaceholders", $.proxy(this.handleScroll, this));
                };
                this.handleScroll = function() {
                    try {
                        var nodes = $("placeholder");
                        nodes.each(function(index, element) {
                            var node = $(element),
                            type = node.attr("nodetype"),
                            attributes = node.prop("attributes"),
                            replacement = $("<" + type + ">");
                            $.each(attributes,
                            function() {
                                if (this.name != "nodetype") {
                                    replacement.attr(this.name, this.value);
                                }
                            });
                            node.replaceWith(replacement);
                        });
                    } catch(err) {}
                    this.detach();
                };
            })();
            ZK.utils.ScrolledPlaceholders.attach();</script>
