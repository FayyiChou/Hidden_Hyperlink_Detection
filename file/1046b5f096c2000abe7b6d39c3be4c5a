        <script>ZK.ns('ZK.feedComments');
            ZK.feedComments.Peekable = {};
            ZK.feedComments.Peekable.peekableHandler = function() {
                var $ele = $(this);
                var spaceThumbDiv = $ele.find(".zkSpaceThumbnailCtn"),
                fileDiv = $ele.find(".zkCommentFilesDiv"),
                playerDiv = $ele.find(".playerDiv"),
                spaceThumbDivHeight = spaceThumbDiv.length ? spaceThumbDiv.prop("scrollHeight") : ($(".zkSpaceThumbnail").length ? $(".zkSpaceThumbnail").first().prop("scrollHeight") : 0),
                fileHeight = fileDiv.length ? fileDiv.prop("scrollHeight") : 0,
                playerDivHeight = playerDiv.length ? playerDiv.first().prop("scrollHeight") * playerDiv.length: 0,
                toleranceHeight = spaceThumbDivHeight + fileHeight + playerDivHeight;
                var option = toleranceHeight == 0 ? null: {
                    "toleranceHeight": toleranceHeight
                };
                $ele.peekable(option);
            };
            $(".commentBodyContent.comment-peekable:visible").each(ZK.feedComments.Peekable.peekableHandler);
        </script>
