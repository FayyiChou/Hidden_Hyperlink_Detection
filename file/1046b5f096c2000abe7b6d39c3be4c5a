                ZK.spaceActions.AddToIdeabookDialog.init(spaceId);
                ZK.ui.Yamdi.show(ZK.spaceActions.AddToIdeabookDialog, null);
            }
            function showPostToForumForm(spaceId, ownedByPro, isMP, trackingEventSource) {
                ZK.spaceActions.QuestionDialog.init(spaceId, ownedByPro);
                if (trackingEventSource !== undefined) {
                    ZK.spaceActions.QuestionDialog.setTrackingEventSource(trackingEventSource);
                }
                if (isMP === undefined) {
                    isMP = false;
                }
                if (isMP) {
                    ZK.spaceActions.QuestionDialog.setQuestionTopic(ZK.spaceActions.QuestionDialog.PRODUCT_TOPIC);
                } else {
                    ZK.spaceActions.QuestionDialog.setQuestionTopic(ZK.spaceActions.QuestionDialog.SPACE_TOPIC);
                }
                ZK.ui.Yamdi.show(ZK.spaceActions.QuestionDialog, null);
            }
            function showSendSpaceEmailDialog(spaceId) {
                if (ZK.data && ZK.data.Spaces) {
                    var space = ZK.data.Spaces.get(spaceId);
                    if (space) {
                        ZK.dialogs.EmailDialog.prepare({
                            type: ZK.sendEmail.types.SPACE,
                            referenceId: space.id,
                            subject: space.t,
                            message: (space.prod ? ZK.sendEmail.texts.DEFAULT_MESSAGE_PRODUCT: ZK.sendEmail.texts.DEFAULT_MESSAGE_SPACE)
                        });
                        ZK.ui.Yamdi.show(ZK.dialogs.EmailDialog);
                    }
                }
            }
