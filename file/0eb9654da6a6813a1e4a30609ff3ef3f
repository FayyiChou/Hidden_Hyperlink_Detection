				GJ.LogTracker.gjchNew = url_gc;
				GJ.LogTracker.trackPageView();
				GJ.LogTracker.bindSpeedEvent();
				GJ.LogTracker.trackSpeedEvent();
			}
		});
	}
GJ.use(['jquery', 'js/util/storage/storage2.js', 'log_tracker'], function($, Storage){
