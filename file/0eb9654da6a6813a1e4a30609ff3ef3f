<script type="text/javascript">

GJ.use('jquery', function(){

    if ($('#self_sticky').size() <= 0) {
		return false;
	}

    var userInfo = GJ.getCookie('GanjiUserInfo');
    var postType = 0;
    var userId = 676411709;
    var categoryId = 12;
    var tips = '';
    var major_category = '公益活动/志愿者';


//    $("div.service-item").each(function(){
//   	if($(this)==$("div.service-item").last()) {
//    		$("div.service-item").css('border-right','0px');
//    	} else {
//    		$("div.service-item").css('border-right','1px solid #dddddd');
//   	}
//      });

    var itemCount	= $("div.service-item").size();
    var itemWidth	= 699 / itemCount;
    $("div.service-item").each(function(index, el) {
        if (index < itemCount - 1) {
        	$(el).css('border-right','1px solid #dddddd');
        }
        $(el).width(itemWidth);
    });

    if(userInfo){
        var data = GJ.jsonDecode(userInfo);
        if((data.user_id == userId)&&(postType == 0) ) {
            var title = '效果不好？试试赶集推广！';
			$('#self_service_detail_tips_title').html(title);
			$.getJSON('http://www.ganji.com/ajax.php?module=get_advance_info&dir=self_sticky&callback=?', {userid:userId,cate:12,m:128,city:120}, function(ret){
				if (typeof ret != 'object') {
					return false;
				}
				else if (typeof ret.sticky == 'object' && ret.sticky.sticky_price <= 0) {
					return false;
				}
				$.each(ret, function(serviceName, serviceInfo) {
					$.each(serviceInfo, function(serviceKey, serviceValue) {
					    $('.self_' + serviceName + '_detail_tips .' + serviceKey).text(serviceValue);
					    $('#' + serviceKey).text(serviceValue);
					});
				});
                if(!ret.direction || !ret.direction.price) {
                	$('.self_direction_detail_tips').hide();
                }
                if(ret.sticky.sticky_price<=0) {
                	$('.self_sticky_detail_tips').hide();
                }

                if(ret.subSticky.subSticky_price<=0) {
                	$('.self_subSticky_detail_tips').hide();
                }

                $("#_self_service_detail_tips").appendTo('#self_sticky');
                $("#_self_service_detail_tips").show();
                $('.to-top-item').each(function() {
                    topDescMoreHide(this);
                });
                $("#self_sticky").show();
            });
        }
     }
});
</script>
