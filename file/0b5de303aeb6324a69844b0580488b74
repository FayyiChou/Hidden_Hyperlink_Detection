new mainGenreInfo("http://thumb.comic.naver.net", "Update", "story");
function goNstore(url) {
	var nstorePopup = window.open("http://nstore.naver.com" + url, "nstore", "left=0, top=0, menubar=yes, resizable=yes, scrollbars=yes, status=yes, titlebar=yes, toolbar=yes, directories=yes, location=yes");
	nstorePopup.focus();
}
