  <script type="text/javascript">
(function($) {
        var size = $("#m_small").children().size();
        var timer = setInterval(go, 5000);
        var slid = function() {
            $("#m_small .current").removeClass();
            $(this).addClass("current");
            var index = $(this).index();
            var pic = "pic" + index;
            var div = $("." + pic);
            var newdiv = '<div class="picture_box pic' + index + ' ">' + $(div).html() + '</div>';
            $($("#picture_main").children()[0]).before(newdiv);
            $(div).remove();
            clearInterval(timer);
        }
        $("#m_small li").hover(slid);
        $("#m_small li").mouseleave(function() {
            timer = setInterval(go, 5000)
        });
        function go() {
            var i = $("#m_small .current").index();
            $("#m_small .current").removeClass();
            i++;
            i = i % 4;
            $("#m_small .current").removeClass();
            $($("#m_small").children()[i]).addClass("current");
            var pic = "pic" + i;
            var div = $("." + pic);
            var newdiv = '<div class="picture_box pic' + i + ' ">' + $(div).html() + '</div>';
            $($("#picture_main").children()[0]).before(newdiv);
            $(div).remove();
        }
    })(jQuery)
</script>
  <div class="clear"></div>
</div>
