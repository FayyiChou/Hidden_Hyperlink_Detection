	$(".btn-slide").click(function(){
		var index = $(".btn-slide").index(this);
		$('.more-slide').eq(index).slideToggle("fast");
	     if ($(this).hasClass("on")) {
		  $(this).removeClass("on");
		  $(this).addClass("off");
		} else if ($(this).hasClass("off")) {
		  $(this).removeClass("off");
		  $(this).addClass("on");
		}
    });
});
