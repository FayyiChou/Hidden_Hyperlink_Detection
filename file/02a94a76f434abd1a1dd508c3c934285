      <script language="javascript">
      function confirmSubmit(){
          scored = true;
          $('.list-comment-2 select').each(function(i){
              if ($(this).val()==0){
                  scored = false;
              }              
          });
          if (!scored){
              if (confirm("此次评论尚未进行评分，是否确认提交评论？")){
                  document.getElementById("postform").submit();
              }
          }else{
              document.getElementById("postform").submit();
          }
      }
      </script>
