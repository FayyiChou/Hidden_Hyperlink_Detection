    function loadHomeAds () {
        var bigRewardImgBox = ".strItem.bigReward .homeAds";
        var bigRewardImgSelector = ".strItem.bigReward .homeAds .adsImg";
        var bigRewardLinkSelector = ".strItem.bigReward .homeAds a";
        var smallRewardImgBox =".strItem.smallReward .homeAds";
        var smallRewardImgSelector = ".strItem.smallReward .homeAds .adsImg";
        var smallRewardLinkSelector = ".strItem.smallReward .homeAds a";
        $(bigRewardImgBox).hide();
        $(smallRewardImgBox).hide();
        CM.ajax("/banner/ajax/loadHomeBanner.html",{},function (data) {
            if(data.success){
                var bigRewardBannerList = data.data.bigRewardBannerList;
                var smallRewardBannerList = data.data.smallRewardBannerList;
                var bigRewardIntervalMilliseconds = data.data.bigRewardIntervalMilliseconds;
                var smallRewardIntervalMilliseconds = data.data.smallRewardIntervalMilliseconds;
                if(bigRewardBannerList && bigRewardBannerList.length > 0){
                    $(bigRewardImgBox).show();
                    bigRewardAds.showAds(bigRewardBannerList,"bigReward",bigRewardImgSelector,bigRewardLinkSelector,bigRewardIntervalMilliseconds);
                }
                if(smallRewardBannerList && smallRewardBannerList.length > 0){
                    $(smallRewardImgBox).show();
                    $(smallRewardImgSelector).each(function(i){
                    	var __this = $(this);
                    	var index = i%smallRewardBannerList.length;
                    	__this.css("background-image","url(/banner/img/smallReward/" + index + ".html)");
                    })
                    $(smallRewardLinkSelector).each(function(i){
                    	var __this = $(this);
                    	var index = i%smallRewardBannerList.length;
                    	__this.attr("href","/banner/link/smallReward/" + index + ".html");
                    })
                    //smallRewardAds.showAds(smallRewardBannerList,"smallReward",smallRewardImgSelector,smallRewardLinkSelector,smallRewardIntervalMilliseconds);
                }
            }
        },function (data) {},function (data) {});
    }

    var showBatchAdsInterval;

    function loadRaffleCloudAds () {
        var raffleCloudImgBox = ".raffleCloudAds";
        var raffleCloudImgSelector = ".raffleCloudAds .adsImg";
        var raffleCloudLinkSelector = ".raffleCloudAdsLink";
        $(raffleCloudImgBox).hide();
        clearInterval(showBatchAdsInterval);
        $(raffleCloudImgBox).stop(true);
        CM.ajax("/banner/ajax/loadBannerList/raffleCloud.html",{},function (data) {
            if(data.success){
                var raffleCloudBannerList = data.data.bannerList;
                var raffleCloudIntervalMilliseconds = data.data.intervalMilliseconds;
                if(raffleCloudBannerList && raffleCloudBannerList.length > 0){
                    $(raffleCloudImgBox).css({top: '280px'});
                    $(raffleCloudImgBox).css({top: '-=50px'});
                    setTimeout(function () {
                        $(raffleCloudImgBox).show();
                        $(raffleCloudImgBox).animate({top: '+=50px'}, 1000);
                        raffleCloudAds.showAds(raffleCloudBannerList,"raffleCloud",raffleCloudImgSelector,raffleCloudLinkSelector,raffleCloudIntervalMilliseconds);
                        showBatchAdsInterval = setInterval(function() {
                            $(raffleCloudImgBox).animate({top: '-=5px'}, 980);
                            $(raffleCloudImgBox).animate({top: '+=5px'}, 980);
                        },500);
                    },2500);
                }
            }
        },function (data) {},function (data) {});
    }

    function loadCountDownAds () {
    	if($("#hasUserRaffleBefore").val() == 'false'){
    		 return ;
    	}
    	
        if(raffleCloudAds.isRunning()){
           return;
        }
        var countDownImgBox = ".countDownAds";
        var countDownImgSelector = ".countDownAds .adsImg";
        var countDownLinkSelector = ".countDownAds a";
        $(countDownImgBox).hide();
        clearInterval(showBatchAdsInterval);
        $(".raffleCloudAds").stop(true); //close raffleCloud then stop Animate
        CM.ajax("/banner/ajax/loadBannerList/countDown.html",{},function (data) {
            if(data.success){
                var countDownBannerList = data.data.bannerList;
                var countDownIntervalMilliseconds = data.data.intervalMilliseconds;
                if(countDownBannerList && countDownBannerList.length > 0){
                    countDownAds.showAds(countDownBannerList,"countDown",countDownImgSelector,countDownLinkSelector,countDownIntervalMilliseconds);
                    $(".count").addClass("countWithAds");
                    $(".countDownAds").append($(".count"));;
                }
            }
            $(countDownImgBox).show();
        },function (data) {},function (data) {});
    }

    // 关闭弹出层
    function closeDialog(dialogId){
        qb_user.dialogOption.closeWin($("#" + dialogId));
    }
    
    function showRewardDetails(buildingId,buildingIndex){
         createRewardDetailsTable(buildingId,buildingIndex);
    	 qb_user.dialogOption.addShadow($("#prizeBox"));
         qb_user.dialogOption.popWin($("#prizeBox"));
    }
    
    function createRewardDetailsTable(buildingId,buildingIndex) {
        $.fn.dataTableExt.oStdClasses.sPaging = "qb_paginate paging_";
    	$('#rewardDetails').html( '<h2 style="padding-bottom:20px;"><i>第' + buildingIndex + '幢楼全部奖项</i></h2><table cellpadding="0" cellspacing="0" border="0" class="tab-box-border" id="myTable" style="width:560px;"></table>' );
    	$('#myTable').dataTable({
    		'iDisplayLength': 10,
    		'bSortClasses': false,
    		'bSort': false,
    		'sPaginationType': 'full_numbers',
    		"sDom": '<"top"i>rtp<"clear">',
    		'oLanguage': {
    			'sProcessing': '加载数据中...',
    			'sInfo': '当前显示  _START_ 到  _END_ 条记录，总共  _TOTAL_ 条',
    			'sInfoEmpty': '',
    			'oPaginate': {'sFirst': '首页', 'sPrevious': '上一页', 'sNext': '下一页', 'sLast': '末页'},
    			"sEmptyTable": "没有数据"
    		},
    		'aoColumns': [
    			{ 'sTitle': '所在楼层', 'sClass': 'center', 'sWidth': '50%' },
    			{ 'sTitle': '红包金额（钱宝币）', 'sClass': 'center', 'sWidth': '50%' }
    		],
    		'bProcessing': true,
    		'bServerSide': true,
    		'sAjaxSource': '/ajax/rewardDetails.html',
    		'fnServerData': function ( sSource, aoData, fnCallback ) {
    			aoData.push({'name':'buildingId','value':buildingId});
    			
    			CM.ajax(sSource, aoData, function(data){
    				var result = data.data;
    				var aaData = [];
    				var records = result.aaData;
    				for(var i=0; i<records.length; i++){
    					var record = records[i];
    					aaData.push([record.index, '<span class="orange">' + CM.formatNumber(record.reward) + '</span>']);
    				}
    				fnCallback({
    					'sEcho': result.sEcho, 
    					'iTotalRecords': result.itotalRecords, 
    					'iTotalDisplayRecords': result.itotalDisplayRecords, 
    					'aaData': aaData
    				});
    				
    				qb_user.dialogOption.resizeWin($('#prizeBox'));
    			});
    		}
    	});
    	
    }
    
</script>

