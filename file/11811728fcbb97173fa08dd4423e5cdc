            $(".btnPop").toggleClass("on");
            $(".popHead").toggleClass("dn");
        });

        $(".batchRaffle").mouseenter(function () {
            $('.batchRaffle').removeClass('dn');
        }).mouseleave(function () {
            $('.batchRaffle').addClass('dn');
        });

        refreshBatchBtn();

        $("#batchRaffle20").on("click",function() {
            raffle.batch(10);
        });

        $("#batchRaffle50").on("click",function() {
            raffle.batch(50);
        });
        
        $('.input_tip').bind({ 
            focus:function(){ 
                if (this.value == this.defaultValue){ 
                    this.value=""; 
                } 
            }, 
            blur:function(){ 
                if (this.value == ""){ 
                    this.value = this.defaultValue; 
                    $(this).css({"color":'gray'});
                } 
            } 
        }); 

