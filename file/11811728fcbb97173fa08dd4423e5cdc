			{{if value.coupon > 0}}{{value.coupon}}宝券
			{{else}}{{(value.reward/100).toFixed(2).replace(/\B(?=(?:\d{3})+\b)/g, ',')}}元
			{{/if}}
		</li>
        {{/each}}
    </ul>
    <div class="max">
        <p><span class="fl strFloor">{{finalFloor.floorIndex}}层</span><span class="fr">
			{{if finalFloor.coupon > 0}}{{finalFloor.coupon}}宝券
			{{else}}{{(finalFloor.reward/100).toFixed(2).replace(/\B(?=(?:\d{3})+\b)/g, ',')}}元
			{{/if}}
		</span>
        </p>
    </div>

</script>

