<script>

$(function(){

  var dv=$(".menulis div");

  dv.click(function(){

  var aixuexi=$(this);

  if(aixuexi.next("ul").is(":hidden")){
   aixuexi.attr("style","height: 40px;line-height: 40px;text-indent: 30px;background:url(templates/default/images/9_03.jpg) no-repeat scroll 5% 50%; background-color: #c6510b;font-family:微软雅黑; color:#eae9e7; font-size:18px;"); 
   aixuexi.next("ul").show();
   

    }else{
   aixuexi.attr("style","height: 40px;line-height: 40px;text-indent: 30px;background:url(templates/default/images/9.png) no-repeat scroll 5% 50%; background-color: #c6510b;font-family:微软雅黑; color:#eae9e7; font-size:18px;"); 
  aixuexi.next("ul").hide();

}

  })

})

</script>
