</script>

    <script type="text/javascript">
    $(function() {
        var a = 0;
        $("#banner_inner").find("img").each(function(){
            var b = parseInt($(this).css("width"))/window.innerWidth;
            $(this).css("width",parseInt($(this).css("width"))/b+"px");
            $(this).css("height",parseInt($(this).css("height"))/b+"px");
            a = $(this).css("width");
             
        });
         $("#banner").each(function(){
            $(this).css("width",window.innerWidth+"px");
         })


        $('#banner').kxbdSuperMarquee({
            distance: parseInt(a),

            time: 4,
            navId: '#banner_nav',
            direction: 'left'
        });

        //document.getElementById('banner_inner').style.width = window.innerWidth;
        //console.log(document.getElementById('banner_inner').style.width,window.innerWidth);
    });


    
    </script>

		
