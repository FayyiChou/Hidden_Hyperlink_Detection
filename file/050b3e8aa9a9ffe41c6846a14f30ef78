jQuery(function(){
	var modContent = jQuery('.mod-content');
	var navItems = modContent.find('.nav-item');
	var contentItems = modContent.find('.content-item');
	modContent.on('click', '.nav-item', function(){
		var jQuerythis = jQuery(this);
		var index = jQuerythis.index();
		navItems.removeClass('active').eq(index).addClass('active')
		contentItems.hide().eq(index).show()
	})
	modContent.find('.nav-item').eq(0).click();

	//通栏背景
	var thirdCate = modContent.find('.part-third-cate');
	thirdCate.css({
		'min-height': jQuery(this).height()
	})	
	jQuery(window).resize(function(){
		thirdCate.css({
			'min-height': jQuery(this).height()
		})
	});
})
</script></div>
