  <script type="text/javascript">

    function jPlarea(idval) {//显示或隐藏评论框 
      if ($("#pl_" + idval).html() == "") {         
          $("#feeds").load("feeds.html"); 
          $("#pl_" + idval).load("/ws/plpage.aspx?id=" + idval + "&n=" + Math.random());
      } else {
          $("#pl_" + idval).html("");
      }
    }
    function postpl(idval) { //提交评论
        var txt1 = document.getElementById("pltxt_" + idval);
        var uid = document.getElementById("uid_" + idval);
        if (txt1.value == "") {
            alert('请输入评论内容！');
            txt1.focus();
            return;
        }        
        $.ajax({
            type: "POST",
            url: "/Ws/plOk.ashx",
            data: "db=efudb5&tab=newsdata&uid=" + uid.value + "&reid=0&keyid=" + idval + "&txt1=" + txt1.value,
            success: function(msg) {
                if (msg == "true") {
                    txt1.value = "";
                   alert("提交评论完成！");
                } else {
                    alert(msg);
                }
            }
        });
       
    }
	
	function getbeibo(cid) { //备播广告,刷新随机显示，后台缓存10分钟
            $.getJSON("http://zs.efu.com.cn/ws/beibo.ashx?cid=" + cid + "&jsoncallback=?", 
function(data) {
                var len = 0;                            
                $.each(data.Table, function(i, item) {
                    len++;
                });               
                len = len - 1;
                var vi = Math.round(Math.random() * len);//随机数                
                $("#newsdiv1").html(data.Table[vi].adv_code);
				if(vi==len){
					$("#newsdiv2").html(data.Table[vi-1].adv_code);
				}
				else {
					$("#newsdiv2").html(data.Table[vi+1].adv_code);
				}
            });
                      
        }

        $(function() {
        //getbeibo(6);
        });

function getbeibo2(cid1) { //备播广告,刷新随机显示，后台缓存10分钟
            $.getJSON("http://zs.efu.com.cn/ws/beibo.ashx?cid=" + cid1 + "&jsoncallback=?", 
			function(data) {
                var len = 0;                            
                $.each(data.Table, function(i, item) {
                    len++;
                });               
                len = len - 1;
                var vi = Math.round(Math.random() * len);//随机数                
                $("#newsdiv1a").html(data.Table[vi].adv_code);				
            });
                      
        }
		
