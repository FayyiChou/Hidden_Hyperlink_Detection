<script>
  $(function(){
    
    $('.smalllist').find("img.lazy").lazyload({
      effect: "fadeIn",
      placeholder: "/resources/images/blank.gif",
      placeholder2: "/resources/images/loading_duia.gif"
    });
    $('.duiab-mes').find("img.lazy").removeClass("lazy");
    $('.lastSmallList dl').hover(function(){
        $(this).find('.duiab-gly').show();
    },function(){
      $(this).find('.duiab-gly').hide();
    });

    //播放相关
    var mp3List;
    audiojs.events.ready(function() {
      mp3List = audiojs.createAll({
        trackEnded: function() {
          hidePlayImg();
        },
        loadError: function(){
          hidePlayImg();
        }
      });
      $("a.voice_v4").on("click",function(){
        var audio = mp3List[0];
        var url=$(this).attr("f");
        var p=parseInt($(this).attr("p"));
        audio.load(url);
        if(p==0){
          $(this).attr("p",1);
          $(this).find("img.play_img").show();
          audio.play();
        }else{
          $(this).attr("p",0);
          $(this).find("img.play_img").hide();
          audio.pause();
        }
      });
    });

    function hidePlayImg(){
      $("img.play_img").hide();
    };

    $('.lastSmallList').find('a').each(function(){
      var html=$(this).html();
      var have=false;
      while(html.match(/\[#[^\]]*#\]/)){
        have=true;
        html=html.replace(/\[#([^\]]*)#\]/,'<img src="http://tu.duia.com/duibaexpr/$1.png">');
      }
      if(have){
        $(this).html(html);
      }
    })
  });
  $("dl dt").find("p").each(function(){
    var a=$(this).text().trim().length;
   if(a>10){
     $(this).text($(this).text().trim().substring(0,10)+'...')
   }
  })
  //处理帖子中的禁用词
//  dealForbiddenWord();
</script>

