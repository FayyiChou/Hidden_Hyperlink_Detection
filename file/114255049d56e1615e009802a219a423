<script>
    
    //滚动翻页条目数
    var pageSizeSmall=15;
    //每页显示条目数
    var pageSizeBig=45;
    //滚动翻页页码
    var pageNowSmall=1;
    //翻页页码
    var pageNowBig=0;
    //总条目数
    var count=0;
    //分类集合
    var category="";
    //导航id
    var bannerId=0;
    //导航样式
    var styleId="";
    var teacherId=null;
    //类别
    var type="";
    var groupId=$("#groupId").val();
    //分类切换样式
    $(".change-choice ul li").click(function(){
        $("#curTopicId").val("");
        $(this).addClass("on").siblings().removeClass("on");
    });
    //贴子列表切换
    $('#categroy_panel').find('a').bind('click',function(){
        category=$(this).attr('cid');
        type=$(this).attr('type');
        if($(this).attr('ctype')==2){
            category=$('#bannerList').find('.duian-cur:eq(0)').attr('bbdata');
            teacherId=$(this).attr('cid');
        }else{
            teacherId=null;
        }
        bannerId=$('#bannerList').find('.duian-cur:eq(0)').attr('bid');
        styleId=$('#bannerList').find('.duian-cur:eq(0)').attr('styleId');
        if(!$(this).next("div").is(".childCates")){
            $('.childCates').hide();
        }
        if(type=='all'||type=='light'||type=='you'){
            category=$('#bannerList').find('.duian-cur:eq(0)').attr('bbdata');
        }else if (type=='parent'){
            var eles=$(this).next("div").find('a');
            for(var i=0;i<eles.size();i++){
                category+=','+$(eles.get(i)).attr('cid');
            }
        }
        initPage();
    });
    //子类显示效果
    $(".childrenset").hover(function(){
        if($(this).find("div:eq(0)").find("a").size()==0){
            return;
        }
        $(this).find("div:eq(0)").show();
    },function(){
        $(this).find("div:eq(0)").hide();
    });
    //获取请求数据
    function getQueryData(){
        var searchData=$('#bannerList').find('.duian-cur:eq(0)').attr("sdata");
        return {id:groupId,type:type,category:category,bannerId:bannerId,styleId:styleId,searchData:searchData,teacherId:teacherId}
    }
    //初始化分页
    function initPage(){
        initCount();
        loadPage(0);
    }
    //初始化贴子总数
    function initCount(){
        $.ajax({
            type: "post",
            async: false,
            url: path + "/duibaGroup/portal/topicCount",
            dataType: "text",
            data: getQueryData(),
            success: function (result) {
                count=result;
                var option = {next_text: ">", prev_text: "<", link_to: "javascript:void(0)", num_display_entries: 8, num_edge_entries: 2, items_per_page: pageSizeBig, callback: loadPage};
                $("#Pagination").pagination(count, option);
            }
        });
    }
    //加载置顶
    function loadTops(){
        if(pageNowBig!=0||pageNowSmall!=1){
            $('.duiba_list_top:eq(0)').html("");
            return;
        }
        $('.duiba_advert_1').hide();
        var data=getQueryData();

        if(data.styleId == 'simplesp'){
            data.type= "teacherTop";
        }else if(data.type=='all' || data.type=='light'){
            data.type= "all_top";
        }else if(data.type=='you'){
        	data.type= "you_top";
        }else{
            data.type= "top";
        }

        data.itemIndex=0;
        data.pageSize=3;
        $.ajax({
            type: "post",
            async: false,
            url: path + "/duibaGroup/portal/topicList3",
            dataType: "text",
            data:data,
            success: function (html) {
                $('.duiba_list_top:eq(0)').html(html);
                $('.duiba_list_top:eq(0)').find('.lastSmallList').animate({opacity: 1, "margin-top": 0}, 1000,function(){
                    $('.duiba_advert_1').slideDown(500);
                });
            }
        });
    }
    var hotcategory="";
    //加载热贴列表
    function loadHot(){
        if(pageNowBig!=0&&pageNowSmall!=1){
            return;
        }
        var data=getQueryData();
        if(hotcategory==data.category){
            return;
        }
        hotcategory=data.category;
        data.type="hot";
        data.itemIndex=0;
        data.pageSize=6;
        $.ajax({
            type: "post",
            async: true,
            url: path + "/duibaGroup/portal/topicList3",
            dataType: "text",
            data:data,
            success: function (html) {
                $('.duiba_advert_hotlist:eq(0)').html(html);
            }
        });
    }

    $(window).scroll(function () {
        var tar =$(".duiba_user_info:eq(0)");
        var t1 = tar.offset().top+tar.height()-20;
        var top=$(document).height()-($(window).scrollTop()+$("#").height()+$(".foot").height())-12;
        if(top<=0){
            $("#").css({"position": "fixed", "top": top+"px", "width": "240px"});
        }else if ($(window).scrollTop() >= t1) {
           $("#").css({"position": "fixed", "top": "40px", "width": "240px"});
        }
        else {
            $("#").css({"position": "inherit", "top": null, "width": "240px"});
        }
        var tar1=$('.duiba_advert_hotlist');
        if($(window).scrollTop()-(tar1.offset().top+tar1.height())>0){
            $(".duiba_appPanel").addClass("duiba_appfix");
        }else{
            $(".duiba_appPanel").removeClass("duiba_appfix");
        }
    });

    //加载广告位1
    function loadListAdvert(){
        var searchData=$('#bannerList').find('.duian-cur:eq(0)').attr("sdata");
        if((type=='all'||type=='light')&&pageNowSmall==1&&pageNowBig==0&&(searchData==null||searchData=='')){
            $.post(path + "/duibaAppBanner/getBanner", {groupId: $("#groupId").val(), appType: 103}, function (data) {
                if(data==null||data==""||typeof data=="undefined"){
                    return;
                }
                $(".duiba_advert_1").html('<a href="'+data.params+'" target="_blank"><img src="'+data.image+'" style="display:block;width: 100%;max-width:100%;" /></a>');
            });
        }else{
            $(".duiba_advert_1").html("");
        }
    }
    //翻页加载列表
    function loadPage(pageIndex){
        pageNowSmall=1;
        pageNowBig=pageIndex;
        var data=getQueryData();
        data.itemIndex=pageNowBig*pageSizeBig;
        data.pageSize=pageSizeSmall;
        loadTops();
        loadHot();
        loadingData(true);
        loadListAdvert();
        $.ajax({
            type: "post",
            async: true,
            url: path + "/duibaGroup/portal/topicList3",
            dataType: "text",
            data:data,
            success: function (html) {
                showListHtml(function(){
                    $('.duiba_list_content:eq(0)').html(html);
 //                   console.log($('.duiba_list_content:eq(0)').find(".smalllist").size()+"."+$(".duiba_list_top").find(".smalllist").size()+"."+pageIndex)
                    if($('.duiba_list_content:eq(0)').find("#div_no_tips").size()>0&&$(".duiba_list_top").find("dl").size()>0&&pageIndex==0){
                    	$('.duiba_list_content:eq(0)').find("#div_no_tips").hide();
                    }
                    setTimeout(function(){
                        $('#duiba_list_content').scrollPagination();
                    },1000)
                });
                loadingData(false);
            }
        });
        showHidePaginationDiv();
    }
    //滚动加载贴子列表
    function loadScrollPage(){
        if(pageNowSmall==3||pageNowBig*pageSizeBig+pageNowSmall*pageSizeSmall>count){
            return;
        }
        var data=getQueryData();
        data.itemIndex=pageNowBig*pageSizeBig+pageNowSmall*pageSizeSmall;
        data.pageSize=pageSizeSmall;
//        var curTopicId = $("#curTopicId").val();
//        if(curTopicId){
//            data.curTopicId =curTopicId;
//        }
        loadingData(true);
        $.ajax({
            type: "post",
            async: false,
            url: path + "/duibaGroup/portal/topicList3",
            dataType: "text",
            data:data,
            success: function (html) {
                showListHtml(function(){
                    $('.duiba_list_content:eq(0)').append(html);
                    pageNowSmall++;
                    if(pageNowSmall<3){
                        $('#duiba_list_content').scrollPagination();
                    }
                });
                loadingData(false);
                showHidePaginationDiv();
            }
        });
    }
    //滚动触发绑定
    $(function(){
        $('#duiba_list_content').scrollPagination({
            'scrollTarget': $(window),
            'heightOffset': 400,
            'deply': 666,
            'scrollCtrl': 0,
            'beforeLoad': function () {
                this.scrollCtrl=0;
            },
            'nowLoad': function (obj) {
                if(this.scrollCtrl==1){
                    return;
                }
                this.scrollCtrl=1;
                loadScrollPage();
                this.afterLoad(obj);
            },
            'afterLoad': function (obj) {
                this.scrollCtrl=0;
            }
        });
    });

    //显示第一滚动部分
    function showListHtml(func){
        $('#duiba_list_content').find('.lastSmallList').removeClass("lastSmallList");
        func();
        if(pageNowSmall==1){
            var pos = $('.duiab-mes:eq(0)').offset().top - 500;
            $("html,body").animate({scrollTop: pos}, 1000);
        }
        $('#duiba_list_content').find('.lastSmallList').animate({opacity: 1, "margin-top": 0}, 1000);
    }

    //翻页显示隐藏
    function showHidePaginationDiv(){
        if(pageNowSmall==3||(pageNowBig*pageSizeBig+pageNowSmall*pageSizeSmall)>=count&&count>pageSizeBig){
            $("#Pagination").show();
        }else{
            $("#Pagination").hide();
        }
    }
    //数据加载效果
    function loadingData(loading){
        if(loading){
            $("#div_no_tips").remove();
            $('#load_flag').show();
        }else{
            $('#load_flag').hide();
        }
    }
    //点赞效果
    var praiseUp = function ($obj) {
        var obj = $($obj);
        var param_id = obj.attr("d");
        if (param_id == null || param_id == "") {
            return;
        }
        $.ajax({
            type: "post",
            async: true,
            cache:false,
            url: path + "/duibaGroupTopic/praise",
            dataType: "text",
            data: {"topicId": param_id},
            success: function (result) {
                obj.find(".add_db_v4").show();
                obj.find(".add_db_v4").animate({opacity: 0, "margin-top": -20}, 1000);
                obj.attr("d", "").attr("style", "background-position: -2px -185px;");
                obj.find("span").text(obj.find("span").text() * 1 + 1);
            }
        });
    };

    function categoryBar(id){
    	if(id == 'you'){
            $('#categroy_panel').find('a[cid=you]').parent().show();
    	}
        $('#categroy_panel').find('a[cid='+id+']').click();
    }
    $('#categroy_panel').find("li:eq(0)").find('a').click();
</script>
</div>

