<script src="/resources/common3/js/cookie/js.cookie.js"></script>
<script>
    function xiaoneng(sku,key) {
    	var path = $("#path").val();//暂时这么写着
        if(!isNaN(sku)){
        var cookieKey = sku+"_"+key; 
         if(Cookies.get(cookieKey)==null){
        	 var url = path + '/sysDict/xiaoneng';
             var dataes = {sku: sku};
             $.ajax({
         		type: "post",
         		url:url,
         		data:dataes,
         		async:false,
         		dataType:"json",
         		success:function(data){
         			 var val = data[key];
               	     NTKF.im_openInPageChat(val);
                    Cookies.set(cookieKey,val,{path:"/"});
         		}
         	});
             $.ajax({
          		type: "post",
          		url:path+"/sysKeyPointLog/saveXn/"+sku+"/"+key,
          		data:{uuid:NTKF.user.id},
          		async:false,
          		success:function(data){
          		
          		}
          	});
          }else{
        	     var val =  Cookies.get(cookieKey);
        	     NTKF.im_openInPageChat(val);
          }
        }else{
            NTKF.im_openInPageChat(sku);
        }
    }
</script>
