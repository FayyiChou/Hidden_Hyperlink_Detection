<script type="text/javascript">
    var messageRedisUrl='http://message.duia.com'; //勿删除,消息中会用到
    $(function(){
        var curMsgUserId="";
        curMsgUserId=parseInt(curMsgUserId);
        if(curMsgUserId>0){
            setHeadMessage(curMsgUserId,function(data){
                var sysNoticeCount=getSysNoticeCountFromData(data);
                var sysMessageResponseCount=getSysMessageCountFromData(data);
                var classSysNoticeCount=getClassSysNoticeCountFromData(data);
                var duibaMessageCount=getDuibaMessageCountFromData(data);
                var allCount=sysNoticeCount+sysMessageResponseCount+classSysNoticeCount;
                if(allCount>0){
                    $("#msg_mark").show();
                }else{
                    $("#msg_mark").hide();
                }
                if(sysNoticeCount>0 && sysNoticeCount<=99 ){
                    $("#msg_sysNotice").html("系统消息("+sysNoticeCount+")");
                }else if(sysNoticeCount > 99){
                    $("#msg_sysNotice").html("系统消息(99+)");
                }else{
                    $("#msg_sysNotice").html("系统消息");
                }
                if(sysMessageResponseCount>0 && sysMessageResponseCount<=99){
                    $("#msg_res").html("回复("+sysMessageResponseCount+")");
                }else if(sysMessageResponseCount > 99){
                    $("#msg_res").html("回复(99+)");
                }else{
                    $("#msg_res").html("回复");
                }
                if(classSysNoticeCount>0 && classSysNoticeCount<=99){
                    $("#msg_classSysNotice").html("班级通知("+classSysNoticeCount+")");
                }else if(classSysNoticeCount>99){
                    $("#msg_classSysNotice").html("班级通知(99+)");
                }else{
                    $("#msg_classSysNotice").html("班级通知");
                }
                if(duibaMessageCount>0 && duibaMessageCount<=99){
                    $("#msg_duiba").html("论坛回复("+duibaMessageCount+")");
                }else if(duibaMessageCount > 99){
                    $("#msg_duiba").html("论坛回复(99+)");
                }else{
                    $("#msg_duiba").html("论坛回复");
                }
            });
        }


        $(".top-ul .head-mes").hover(function(){
            $(this).css("background","#333");
            $(this).find("a").eq(0).css("color","#fff")
            $(this).find(".mes-list").show();
        },function(){
            $(this).css("background","#333")
            $(this).find("a").eq(0).css("color","#bbb")
            $(this).find(".mes-list").hide();
        })
    });

</script>



