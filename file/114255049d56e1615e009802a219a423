<script>
    $(".dl-wx,.dl-erm").hover(function(){
        $(".dl-erm").stop().show()
    },function(){
        $(".dl-erm").stop().hide()
    })
    function signIn() {
        var returnUrl = encodeURIComponent(window.location);
        window.location.href =document.getElementById("ssoDomain").value+"uc?returnUrl=" + returnUrl;
    }
    function register() {
        var returnUrl = encodeURIComponent(window.location);
        window.location.href = document.getElementById("ssoDomain").value+"uc/goRegister?returnUrl=" + returnUrl;
    }
</script>
