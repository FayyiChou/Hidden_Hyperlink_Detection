<div class="duiab-top-sever" style="margin-top: 18px;"><input type="text" placeholder="搜索" id="duibaSearch"><a id="duibaSearchBtn"></a></div>
<script>
    $(function(){
        $("#duibaSearchBtn").bind('click',function(){
            var val=$("#duibaSearch").val();
                if($.trim(val)==''){
                return;
            }

            $("#bannerList").find("li").removeClass("duian-cur");
            $("#bannerList").find("li#banner_-1").remove();
            $("#bannerList").find("li:last").after('<li id="banner_-1" styleId="full"  type="1" bid="-1" bbdata="-1"  sdata="'+val.replace(/<[^>]+>/g,"")+'"  jsFuncition="findCategroy" class="duian-cur">搜索结果<i></i></li>')
            $("#bannerList").find("li#banner_-1").click(function(){
                bannerActive($(this));
            });
            bannerActive($("#bannerList").find("li#banner_-1"));
        });

        initBanners();

        $('#bannerList').find('li:eq(0)').find("div:eq(0)").css('margin-left','22px');
    });

    //下属分类列表
    function findCategroy(id){
        var e =$("#"+id);
        var groupId=$("#groupId").val();
        var bannerId=e.attr("bid");
        var bannerData=e.attr("bbdata");
        var searchData=e.attr("sdata");
        var styleid=e.attr("styleid");
        var data={groupId:groupId,bannerId:bannerId,bannerData:bannerData,searchData:searchData,styleid:styleid};
            $.post(path+"/duibaGroupBanner/findCategorys",data,function(rdata){
            $('.duiab-m-l').html(rdata);
        });
    }


    //导航处理
    function bannerActive(e){
        if(e.attr("type")=="1"){//js处理
            var fun=e.attr("jsFuncition");
            var id =e.attr("id");
            eval(''+fun+'(\''+id+'\')');
        }else if(e.attr("type")=="2"){//页内请求
            var url = e.attr("href");
            if(url.indexOf("http://")!=0&&url.indexOf("https://")!=0){
                url=path+url;
            }
            $.post(url,null,function(rdata){
                $('.duiab-m-l').html(rdata);
                loadHot();
            });
        }else if(e.attr("type")=="3"){//新开页面

        }
        var tarct= $.trim($('#bannerList').find('.duian-cur:eq(0)').text());
        console.log(tarct)
        if(tarct=="资讯"||tarct=="首页"){
            $('.duiba_appPanel').show();
        }else{
            $('.duiba_appPanel').hide();
        }
    }
    //导航样式效果加载
    function initBanners(){
        $(".duiab-main1 ul li").click(function(){
            if(!$(this).parent().is("a")){
                $(this).addClass("duian-cur").siblings().removeClass("duian-cur");
            }
            if($(this).attr("id")!='banner_-1'){
                $("#bannerList").find("li#banner_-1").remove();
                $("#duibaSearch").val("");
            }
            bannerActive($(this));
        });
    }
</script>

