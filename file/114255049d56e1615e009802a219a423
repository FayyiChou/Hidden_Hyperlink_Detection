	<script type="text/javascript">
		$(function (){
			//加载分类导航
			$.post(path+"/duibaGroupBanner/list",{groupId:$("#groupId").val()},function(date){
				$("#bannerList").html(date);
				$(".duiab-main1 ul li:eq(0)").click();
			});
			//加载当前用户信息
			$.post(path+"/duibaGroupTopic/portal/userInfo",{userCount:5},function(html){
				$(".duiba_user_info:eq(0)").html(html);
			});
			//加载右侧导航
			$.post(path+"/duibaGroupFloat/findDuibaFloats",{groupId:$("#groupId").val()},function(html){
				$("#rightFloatDiv").html(html);
			});

			//加载广告位1
			$.post(path + "/duibaAppBanner/getBanner", {groupId: $("#groupId").val(), appType: 101}, function (data) {
				if(data==null||data==""||typeof data=="undefined"){
					return;
				}
				var html = '<a href="'+data.params+'" target="_blank"><img src="'+data.image+'" style="display:block;width: 100%;" /></a>'
				$(".duiba_advert_2").html(html);
			});
			//加载广告位2
			$.post(path + "/duibaAppBanner/getBanner", {groupId: $("#groupId").val(), appType: 102}, function (data) {
				if(data==null||data==""||typeof data=="undefined"){
					return;
				}
				var html = '<a href="'+data.params+'" target="_blank"><img src="'+data.image+'" style="display:block;width: 100%;" /></a>'
				$(".duiba_advert_3").html(html);
			});

			//设置用户信息
			$("html body").on("click",".btn_push",function(){
				var topicId = $(this).attr("d");
			    window.loginPath = path + '/duibaGroupTopic/portal/' + topicId;
			    var login = '';
			    if (login == '') {
			        signIn();
			        return ;
			    }
			    //设置
			    var styleid = $(".duian-cur").attr("styleid");
			    var url = path + "/duibaGroup/handle";
	            var dataes = {id: topicId,master:'list','styleId':styleid};
	            $.post(url, dataes, function (data) {

	                $("#dialog_push").html(data);
	                $("#dialog_push").shadow({
	                    border: false,
	                    width: 525
	                });
	            });
			});

	    	//关闭弹入窗
		    $("#btn_dialog_del_cancel").click(function () {
		    	$(".web_dialog_shadow").remove();
		        $("#dialog_del").hide();
		    });

			 $("html body").on("click",".btn_del",function(){
				 var topicId = $(this).attr("d");
				 $("#del_id").val(topicId);
			     $("#dialog_del").shadow({
			           border: false,
			           width: 450
			     });
			  });


			    //删除贴子
			  $("#btn_dialog_del_submit").click(function(){
			      var topicId = $("#del_id").val();
			      var url = path + "/duibaGroupTopic/del";
			      $("#btn_dialog_del_cancel").click();
			      //删除
			      $.post(url,{topicId:topicId},function(data){});
			      $("dl").each(function(){
			    	  if($(this).attr("data")==topicId){
			    		  $(this).remove();
			    	  }
			      });
				  $(".duiab-zx li").each(function(){
					  var id = $(this).find(".btn_del").attr("d");
					  if(id == topicId){
						  $(this).remove();
					  }
				  });
			  });

		});

	</script>

</body></html>