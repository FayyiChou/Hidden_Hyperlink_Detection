  $(function(){
    $('.duiba_advert_hotlist:eq(0)').find("img.lazy").lazyload({
      effect: "fadeIn",
      placeholder: "/resources/images/blank.gif",
      placeholder2: "/resources/images/loading_duia.gif"
    });

    $('.duiba_advert_hotlist:eq(0)').find('a').each(function(){
      var html=$(this).html();
      var have=false;
      while(html.match(/\[#[^\]]*#\]/)){
        have=true;
        html=html.replace(/\[#([^\]]*)#\]/,'<img src="http://tu.duia.com/duibaexpr/$1.png">');
      }
      if(have){
        $(this).html(html);
      }
    })
    //处理帖子中的禁用词
//    dealForbiddenWord();
  });
