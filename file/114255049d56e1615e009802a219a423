<script>
  function refreshWindow(){
    window.location.href=window.location.href;
    //$(location).attr("href",$(location).attr("href"));
  }
  function scrollToTop(){
    $('body').animate({scrollTop: '0px'}, 300,function(){
      document.documentElement.scrollTop = 0;
    });
  }
  function setFloatPanel(){
    var domwid=$(window).width();
    var left=(domwid-1100)/2;
    var leftBox=left+1100;
    $(".duiab-xf").css("left",leftBox);
    $(".Wechat_add").css("left","1%");
  }

  $(function(){
    setFloatPanel();
    $(window).resize(function(){setFloatPanel();});
    $(window).scroll(function () {
      if($(window).height()-$(window).scrollTop()<=0){
        $("#scrollTopBtn").show();
      }else{
       //$("#scrollTopBtn").hide();//个别浏览器会出现黑条
      }
    })
    $('#fxbtn_float').hover(function(){
      $('.duiab-fxtc').stop();
      $('.duiab-fxtc').animate({'width':'210px','margin-left':'-210px'},300,function(){
        $('.duiab-fxtc').css('overflow','hidden');
      });
    },function(){
      $('.duiab-fxtc').stop();
      if($('#wxPic').height()>0){
        $('.duiab-fxtc').delay(300);
      }
      $('.duiab-fxtc').animate({'width':'0px','margin-left':'0px'},300,function(){
        $('.duiab-fxtc').css('overflow','hidden');
      });
    });
    $('#showWxPicBtn').hover(function(){
      if($('.duiab-fxtc').width()<210){
        return;
      }
      var url=$(location).attr('href');
      if(url!=$('#wxPic').attr('data')){
        $('#wxPic').html('<div class="wx_sm" style="color: #fff;padding-top: 20px;"><img src="/duiba/getQRCode?url='+url+'">'
        +'<p style="color: #fff;height: 20px; line-height: 20px;">用微信扫描二维码<br>分享至好友和朋友圈</p></div>');
        $('#wxPic').attr('data',url);
      }
      $('.duiab-fxtc').css('overflow','visible');
      $('#wxPic').stop().animate({'height':'230px'},300);
    },function(){
      $('#wxPic').stop().animate({'height':'0px'},300,function(){
        $('.duiab-fxtc').css('overflow','hidden');
      });
    });
    if($('.duiab-xf').find('a:contains("报班"),a:contains("管家")').size()==2){
      $('.duiab-xf').find('a:contains("报班"):eq(0)').remove();
    }
  })


  function rtoolshare(type){
    try{
      share(type);
      return;
    }catch(e){

    }
    var url = window.location.href;
    var title = '【对啊帮】---'+window.document.title;
    var _t = encodeURIComponent(title);
    var _url = encodeURIComponent(url);
    var picurl= $('.duiab-logo').find("img:eq(0)").attr('src');
    if(typeof(picurl) == 'undefined' || picurl == ''){
      picurl = $(".duiab-r-tx").attr("src"); //如果是个人中心抓个人头像
      if(picurl.indexOf("duia.com")==-1){
        picurl = 'http://bang.duia.com/' + picurl;
      }
    }
    var _pic=encodeURIComponent(picurl);
    if(type=="xl"){
      var _appkey = "3105006789";
      var _u = 'http://service.weibo.com/share/share.php?url=' + _url + '&appkey=' + _appkey + '&title=' + _t+ '&pic=' + _pic+"&searchPic=false";
      window.open(_u, '转播到新浪微博', 'width=700, height=680, top=0, left=0, toolbar=no, menubar=no, scrollbars=no, location=yes, resizable=no, status=no');
    }else if(type=="qkj"){
      var _appkey = "100397916";//你从腾讯获得的appkey
      var _site = 'http://www.duia.com';//你的网站地址
      var _u = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?title=' + _t + '&url=' + _url + '&appkey=' + _appkey + '&site=' + _site + '&pics=' + _pic;
      window.open(_u, '转播到QQ空间', 'width=730, height=680, top=100, left=100, toolbar=no, menubar=no, scrollbars=no, location=yes, resizable=no, status=no');
    }
  }

