		$(document).on("ajaxListView", function(e) {
											var $el = $(this);
											if ($el.next().length == 0)
											{
												$.get( $(this).data("url"), function(response) {
													$el.next().empty().remove();
													$el.after(response);
												});
											}
										});

		$(document).on("mouseenter", ".ajaxDropDown", function(e) {
											var $el = $(this);

											var $dropdown = $el.parent().find("ul:first-child");
											if ($dropdown.length == 0)
											{
											$.get( $(this).data("url"), function(response) {
												$dropdown.empty().remove();
												$el.after(response);
											});
											}

										});
	

			
