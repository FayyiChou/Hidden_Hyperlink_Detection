			e.preventDefault();
		});

		$(document).on("click", "a.header-menu-btn", function(e){
			$(".page-content").toggleClass("shift");
		});

		$(document).on("click", ".navigation-item.has-drop > a.navigation-item-link", function(e){
			$(this).parent().toggleClass("active show-dropdown");
			e.preventDefault();

			var $primary = $(this).closest(".navigation-list-primary");
			var level = parseInt($primary.find(".active.show-dropdown").length);

			$primary.addClass("active").css("left", (-1)*$primary.width()*level+"px");
		});

		
		$(document).on("click", "a.navigation-back-btn", function(e){

			var $primary = $(".sidebar .navigation-list-primary");
			var $el = $primary.find(".navigation-item.has-drop.active.show-dropdown:last");
			if ($el.length)
			{
				$primary.addClass("active");
			}
			else
			{
				$primary.removeClass("active");
			}
			//$(".page-content").toggleClass("shift");
			$el.toggleClass("active show-dropdown");

			var level = parseInt($primary.find(".active.show-dropdown").length);
			$primary.css("left", (-1)*$primary.width()*level+"px");

			e.preventDefault();
		});

	

