	<script type="text/javascript" src="/file/assets/6c7685fb/listview/jquery.yiilistview.js"></script>
<script type="text/javascript" src="/file/assets/ead7a81e/script.js"></script>
<script type="text/javascript">
/*<![CDATA[*/

			$("#xefa29bd533b50836cb08af3a6edd53801").flexslider({'prevText':'','nextText':'','animation':'slide'});
		  

			$("#x950840279b9694edaf6e53a44e6a64df1 > .owl-carousel").owlCarousel({'items':4,'navigation':true,'scrollPerPage':true,'navigationText':['<a class=\"prev-btn\" href=\"#\" style=\"display: block;\"><\/a>','<a class=\"next-btn\" href=\"#\" style=\"display: block;\"><\/a>']});
		  
jQuery('#searchBar').xautocomplete({'autoFocus':true,'html':true,'delay':750,'minLength':2,'select':function(event, ui) {
				if (ui.item.url)
					window.location = ui.item.url;
				return false;
			},'response':function (event, ui)
			{
				if (!ui.content.length) {
					var noResult = { value:"", label: "Ничего не найдено" };
					ui.content.push(noResult);
				}
			},'source':function(request, response) {
			$.getJSON("/search/", {
				q: request.term,
				i: ""
			}, response);
		}});

		$.ui.autocomplete.prototype._renderItem = function (ul, item) {

			var terms = this.term.split(" ");
			for(var i in terms)
			{	
				item.label = item.label.replace(new RegExp("(?![^&;]+;)(?!<[^<>]*)(" + $.ui.autocomplete.escapeRegex(terms[i]) + ")(?![^<>]*>)(?![^&;]+;)", "gi"), "<strong>$1</strong>");
			}
			//item.label = item.label.replace(new RegExp("(?![^&;]+;)(?!<[^<>]*)(" + $.ui.autocomplete.escapeRegex(this.term) + ")(?![^<>]*>)(?![^&;]+;)", "gi"), "<strong>$1</strong>");
			return $("<li></li>")
				.data("item.autocomplete", item)
				.append("<a>" + item.label + "</a>")
				.appendTo(ul);
		};

	
