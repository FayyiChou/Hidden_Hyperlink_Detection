$("#nav li").hover(
        function () {
            var u = $(this).find('ul:first');
            if (u.children("li").length > 0) {
                u.css({ visibility: "visible", display: "none" }).show();
            }
        },
        function () {
            $(this).find('ul:first').css({ visibility: "hidden" });
        });
</script>
