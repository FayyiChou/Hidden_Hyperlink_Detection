<!-- jjwxc_index_rich_media_300_250 -->        <script type="text/javascript">$('#hidden_adp').show();
    function show_google_ads() {
        $('div.adp_hidden').each(function() {
            var idp = $(this).attr('id').replace(/adp_h/, 'adp');
            var pos = $("#"+idp).offset();
            if (pos) {
                $(this).css({"left": pos.left+"px", "top": pos.top+"px"});
            }
        });
    }
    show_google_ads();
    $(window).scroll(function() {
        show_google_ads();
    })</script>
