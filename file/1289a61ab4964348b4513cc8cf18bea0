    $(document).ready(function() {
    });
    function slide(int)
    {
        if (int==0&&(typeof(doneTheSelection)=='undefined'))
        {
            doneTheSelection = 1;
            $("#feedbackfalse").slideDown(2500);//窗帘效果展开
        } else if (int==1&&(typeof(doneTheSelection)=='undefined'))
        {
            doneTheSelection = 1;
            $("#feedbacktrue").slideDown(2500);//窗帘效果展开
        }
    }
    $(document).ready(function() {
        $('#submit').click(function() {

            var msg = $('#feedback').val();

            $.post("", {feedback: msg}, function() {
                //$('#content').load("");
                $('#feedback').val('');
            });
        });
    });


