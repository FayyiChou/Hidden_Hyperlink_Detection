var _ua = navigator.userAgent.toLowerCase();
var idBlock = '';
feedbackShort = '41px';
feedbackIntervalID = '';

var browser =
{
	version: (_ua.match( /.+(?:me|ox|on|rv|it|opera|ie)[\/: ]([\d.]+)/ ) || [0,'0'])[1],
	opera: /opera/i.test(_ua),
	msie: (/msie/i.test(_ua) && !/opera/i.test(_ua)),
	msie6: (/msie 6/i.test(_ua) && !/opera/i.test(_ua)),
	msie7: (/msie 7/i.test(_ua) && !/opera/i.test(_ua)),
	msie8: (/msie 8/i.test(_ua) && !/opera/i.test(_ua)),
	msie10: (/msie 10/i.test(_ua) && !/opera/i.test(_ua)),
	mozilla: /firefox/i.test(_ua),
	chrome: /chrome/i.test(_ua),
	safari: (!(/chrome/i.test(_ua)) && /webkit|safari|khtml/i.test(_ua)),
	iphone: /iphone/i.test(_ua),
	ipod: /ipod/i.test(_ua),
	iphone4: /iphone.*OS 4/i.test(_ua),
	ipod4: /ipod.*OS 4/i.test(_ua),
	ipad: /ipad/i.test(_ua),
	//iphoneold: /iphone.*OS 1|iphone.*OS 2|iphone.*OS 3|iphone.*OS 4|iphone.*OS 5|iphone.*OS 6/i.test(_ua),
	iphone7: /iphone.*OS 7/i.test(_ua),
	ipod7: /ipod.*OS 7/i.test(_ua),
	ipad7: /ipad.*OS 7/i.test(_ua),
	android: /Android/i.test(_ua),
	safari_mobile: /iphone|ipod|ipad/i.test(_ua),
	opera_mobile: /opera mini|opera mobi/i.test(_ua),
	mobile: /iphone|ipod|ipad|opera mini|opera mobi|Android|BlackBerry|iPhone|iPad|iPod|IEMobile/i.test(_ua)
};

data =
{
	"full_server_name" : window.location.protocol + "//" + window.location.hostname+"",
	"server_name" : window.location.protocol + "//" + window.location.hostname,
};

globalData = data;


function setCookie (name, value, expires, path, domain, secure)
{
	document.cookie = name + "=" + escape(value) + ((expires) ? "; expires=" + expires : "") + ((path) ? "; path=" + path : "") + ((domain) ? "; domain=" + domain : "") + ((secure) ? "; secure" : "");
}

function changeLang(lang)
{
	if(lang)
	{
		var d = new Date();
		var t = d.getTime(); // UNIX - äàòà, êîëè÷-âî ìèëëèñåêóíä

		var t2 = t + 2592000000;
		d.setTime(t2);
		var t3 = d.toGMTString(),
		cook_number = "";

		setCookie("romart_com_ru_lang", lang, t3, "/");
		location.reload();
	}
}

function showHideFeedback()
{
	//$('.manualsBox div').slideUp("slow");
	// $('.manualsBox div[attr='+num+']').slideDown("slow");

	clearInterval(feedbackIntervalID);
	timeOutFeedback();

	if($('.feedback').css('width') <= '41px')
	{
		var new_width = '696px';
	}
	else
	{
		var new_width = feedbackShort;
	}

	$('.feedback').animate({"width":new_width}, 700);
}

function hideFeedback()
{
	if($('.feedback').css('width') != '41px' && $('.feedback').css('width') != '17px')
	{
		showHideFeedback();
	}
}

function timeOutFeedback()
{
	feedbackIntervalID = setInterval(function(){ hideFeedback(); }, 7000);
	// console.log('timeOutFeedback '+feedbackIntervalID);
}

function closeErrorBubble(elem)
{
	$(elem).prev().remove();
	clearInterval(feedbackIntervalID);
	timeOutFeedback();
}

function makeErrorBubble(params)
{
	/*

		colorClass		: êëàññ ñ öâåòîì òåêñòà â ïóçûðå (âñïëûâàþùåì îêíå),
		msg				: òåêñò ñîîáùåíèÿ,
		left			: îòñòóï ñëåâà,
		top				: îòñòóï ñâåðõó,
		domElement		: domElement - ñåëåêòîð! ýëåìåíòà, ïåðåä êîòîðûì íàäî óñòàíîâèòü ïóçûðü (Bubble), ïðèìåð: #id1
		id				: id ïóçûðÿ (Bubble),
		setTimeout		: false (ïî óìîë÷àíèþ), íàëè÷èå òàéìåðà ñêðûâàíèÿ ïóçûðÿ
		setTimeoutTime	: êîëè÷åñòâî ìèëèñåêóíä òàéìåðà ñêðûâàíèÿ ïóçûðÿ

	*/

	if($('#'+params['id']).length == 0)
	{
		$(params['domElement']).before('<div id="'+params['id']+'" class="errorBubble"></div>');
	}

	$('#'+params['id']).html('<span class="bubbleButton treangleWhite '+params['colorClass']+'" style="display:none;">'+params['msg']+'</span>');
	$('#'+params['id']+' .bubbleButton').fadeIn("slow");

	var errorMsg = $('#'+params['id']+' .bubbleButton');
	var newTop = $(errorMsg).height() + params['top'];
	newTop = -newTop;
	$(errorMsg).css({
			'position' : 'absolute',
			'top' : newTop+'px',
			'left' : params['left']+'px'
		});

	if(params['setTimeout']) // true
	{
		setTimeout(function()
			{
				$('#'+params['id']+' .bubbleButton').fadeOut( "slow", function() {
				   		$(this).parent().remove();
					});
			}, params['setTimeoutTime']);
	}

}

function fadeError(elem)
{
	$(elem).parent().find('.bubbleButton').fadeOut( "slow", function() {
   		$(this).parent().remove();
	});
}


function send_letter()
{
	var error = false;

	var params =
	{
		'left' : 12,
		'setTimeout' : true,
	}


	if($('#name').val() == '')
	{
		params['top'] = -12;
		params['domElement'] = '#name';
		params['id'] = 'error_name';
		params['setTimeoutTime'] = 30000;
		params['colorClass'] = 'redFont';
		params['msg'] = $('#errorSendLetterName').html(); // 'Ïîæàëóéñòà, óêàæèòå âàøå èìÿ!';


		makeErrorBubble(params);

		// alert('Ïîæàëóéñòà, óêàæèòå âàøå èìÿ!');
		var error = true;
	}

	if($('#question').val() == '')
	{
		params['top'] = 4;
		params['domElement'] = '#question';
		params['id'] = 'error_question';
		params['setTimeoutTime'] = 30000;
		params['colorClass'] = 'redFont';
		params['msg'] = $('#errorSendLetterQuestion').html(); // 'Ïîæàëóéñòà, ââåäèòå âàø âîïðîñ!';

		makeErrorBubble(params);

		// alert('Ïîæàëóéñòà, ââåäèòå âàø âîïðîñ!');
		var error = true;
	}




	if(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,4})+$/.test($('#email').val()))
	{

	}
	else
    {
		params['top'] = -12;
		params['domElement'] = '#email';
		params['id'] = 'error_email';
		params['setTimeoutTime'] = 30000;
		params['colorClass'] = 'redFont';
		params['msg'] = $('#errorSendLetterEmail').html(); // 'Íåïðàâèëüíûé E-mail';



		makeErrorBubble(params);

		// alert('Íåïðàâèëüíûé e-mail');
		var error = true;
	}


	if(!error)
	{
		var name = $('#name').val();
		var email = $('#email').val();
		var question = $('#question').val();

		$.ajax(
		{
			url : data["full_server_name"] + "/ajax.php",
			type : "POST",
			dataType : "JSON",
			data :
			{
				name : name,
				email : email,
				question : question,
				type : 'send_letter'
			},
			success : function (e)
			{
				if ( e.ok == 'ok' )
				{
					$('#name').val('');
					$('#email').val('');
					$('#question').val('');

					var params =
					{
						'left' : 272,
						'setTimeout' : true,
					}

					params['top'] = -150;
					params['domElement'] = '#sendButton';
					params['id'] = 'error_sendButton';
					params['setTimeoutTime'] = 4000;
					params['colorClass'] = 'greenFont';
					params['msg'] = e.message;

					makeErrorBubble(params);

					setTimeout(function(){ hideFeedback(); }, 4000);
					// console.log($('#error_nick .bubbleButton').height());

				}
				else
				{
					this.error();
				}
			},
			error : function (){

			}
		});
	}
}

function viewMap()
{
	if($('.yaMapBox').css('display') == 'none')
	{
//		$('#viewMap').parent().append('<div class="yaMapBox"><img src="//api-maps.yandex.ru/services/constructor/1.0/static/?sid=_OFOfDbPEnQEgA9zMhhTj2b3On3hQ5ZM&width=600&height=450" alt=""/></div>');


//		$('#viewMap').parent().append('<div class="yaMapBox"></div>');

		$('.yaMapBox').show('slow');
	}
	else
	{
		$('.yaMapBox').hide('slow'); // , function(){ $('.yaMapBox').remove(); }
	}
}

var langMap = 'ru_RU';
if(langMap == 'en_US')
{
	var markMap = '<div class="stylePlacemark" style="width: 280px;"><div class="imgPlacemark mainSprite" style="background-position: -248px -88px;width: 115px;"></div><div class="textPlacemark"><p style="color: #AC1E40;"><strong>ROMART</strong> <br> company</p><p>Russia, Moscow,<br>Svyatoozerskaya street, d. 2.</p></div></div>';
}
else
{
	var markMap = '<div class="stylePlacemark" style="width: 260px;"><div class="imgPlacemark mainSprite" style="background-position: -366px -88px;"></div><div class="textPlacemark"><p style="color: #AC1E40;">Ãðóïïà êîìïàíèé <br><strong>ÐÎÌÀÐÒ</strong></p><p>Ñâÿòîîçåðñêàÿ óë., 2 <br>Ìîñêâà</p></div></div>';
}

var myMapPopup;

ymaps.ready(init);

//console.log(markMap);

function init () {
	var myMapPopup = new ymaps.Map("mapPopup", {
			center: [55.7124,37.8806], // Ìîñêâà
			zoom: 16
		}),

	// Ñîçäàåì ãåîîáúåêò ñ òèïîì ãåîìåòðèè "Òî÷êà".
		myPlacemark = new ymaps.Placemark([55.7124,37.8806], {
			balloonContent: markMap
		}, {
			preset: 'islands#dotIcon',
			iconColor: '#AC1E40'
		});

		 myMapPopup.geoObjects
			.add(myPlacemark)
}

floating_container =
{
	/*

		content				- ñîäåðæàíèå áëîêà
		close				- êíîïêà çàêðûòèÿ (true / false)
		closeImage			- êíîïêà çàêðûòèÿ èçîáàðåæíèåì ( true / false )
		closeFunc			- ôóíêöèÿ, ñðàáàòûâàþùàÿ ïî íàæàòèþ íà êíîïêó çàêðûòèÿ èçîáàðåæíèåì (ïî óìîë÷àíèþ ïóñòî)
		round				- êðóãëûå óãëû (true / false)
		round-color			- background header è footer
		width				- ñòàòè÷åñêàÿ øèðèíà (ïî óìîë÷àíèþ äèíàìèêà) - öèôðà ( â ïèêñåëÿõ )
		margin_right		- îòñòóï êíîïêè çàêðûòü îò ïðàâà
		padding_bottom		- îòñòóï êíîïêè çàêðûòü îò íèçà
		max-height			- %, åñëè òåêñòà ìåíüøå ýòîé âûñîòû, òî max-height èãíîðèðóåòñÿ è âûñîòà áóäåò ðàâíà êîíòåíòó,
							  åñëè áîëüøå äî äîáàâëÿåòñÿ êîíòåéíåð ñ scroll
		top					- îòñòóï ñâåðõó
		missClick			- ôóíêöèÿ êîòîðàÿ ñðàáàòûâàåò ïðè êëèêå íà "ñåðóþ çîíó" + çàêðûòèÿ îêíà
		missClick_no_destroy- ïàðàìåòð, îáîçíà÷àþùèé äåëàòü ëè destroy ïåðåä ôóíêöèåé missClick. Åñëè åãî íåò, òî äåëàòü
		headerContent		- êîíòåíò êîòîðûé âñòàâëÿåòñÿ â header
		footerContent		- êîíòåíò êîòîðûé âñòàâëÿåòñÿ â footer
		opacity				- true / false (ïî óìîë÷àíèþ true)
		bottom				- îòñòóï ñíèçó â px
		opacity_value		- âåëè÷èíà ïðîçðà÷íîñòè â ïðîöåíòàõ (íàïðèìåð 0.3)
		background_color	- öâåò ôîíà âíóòðåííåãî áëîêà ( ïî óìîë÷àíèþ white )
		opacity_background	- ôîíà âííåøíåãî áëîêà ( ïî óìîë÷àíèþ ïóñòî )
		background_zindex	- z-index ôîíà (íàïðèìåð äëÿ âòîðîãî çàòåìíåíèÿ ïîâåðõ ïåðâîãî, ïî óìîë÷àíèþ = 1)
		window_zindex		- z-index öåíòðàëüíîãî îêíà (ïî óìîë÷àíèþ = 5000)
		floating_for_obj	- îáúåêò îòíîñèòåëüíî êîòîðîãî íàäî âûðîâíèòü floating_container (ïî óìîë÷àíèþ åãî íåò)
		for_obj_left		- îòñóòóï ñëåâà îòíîñèòåëüíî âåðõíåé ëåâîé òî÷êè îáúåêòà floating_for_obj (ïî óìîë÷àíèþ åãî íåò)
		multi_overlay		- åñëè ìíîãî îêîí, òî êàæäîå ñëåäóþùåå áóäåò ïîâåðõ ïðåäûäóùèõ (ïî óìîë÷àíèþ îòñóòñòâóåò)
		for_obj_top		- îòñóòóï ñâåðõó îòíîñèòåëüíî âåðõíåé ëåâîé òî÷êè îáúåêòà floating_for_obj (ïî óìîë÷àíèþ åãî íåò)

		return				- òèï âîçâðàùàþùèõñÿ äàííûõ ( ïî óìîë÷àíèþ id ýëåìåíòîâ ), åñëè óêàçàí "obj" âîçâðàùàåòñÿ:

		{
			"id" : id,
			"overlay" : [object HTML_Element],
			"opacity" : [object HTML_Element],
			"close" : [object HTML_Element],
			"scroll" : [object HTML_Element],
			"footer" : [object HTML_Element]
		}

		________________

		åñëè âûñîòà ñîçäàâàåìîãî êîíòåéíåðà ïîëó÷àåòñÿ áîëüøå âûñîòû ðàáî÷åé îáëàñòè, âûñòàâëÿåòñÿ îòñòóï ñâåðõó - 10px

	*/

	id_list : [],
	msie6_select_fix : [],
	id_list_obj : [], // 

	create : function (param)
	{
		if(param['width'] == 'auto')
		{
			var pxVar = '';
		}
		else
		{
			var pxVar = 'px';
		}

		var closeImage = false;

		do
		{
			var id = Math.floor(Math.random() * 99999);
		}
		while
		(
			document.getElementById('opacity_' + id) ||
			document.getElementById('overlay_' + id) ||
			document.getElementById('scroll_' + id) ||
			document.getElementById('close_overlay_container_' + id) ||
			document.getElementById('footerContent' + id) ||
			document.getElementById('floating_container_' + id)
		)

		floating_container.id_list.push(id);

		var div = document.createElement("div");

		div.id = "floating_container_" + id;

		if ( param['opacity'] === false )
		{
			div.innerHTML = '<div id="overlay_' + id + '" align="left" style="margin: 0px; display:block; position:absolute;background:none;">';
		}
		else
		{
			div.innerHTML = '<div id="opacity_' + id + '" style="overflow-x: hidden; overflow-y: hidden; margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; position: fixed; display: block; *position: absolute;"></div><div id="overlay_' + id + '" align="left" style="margin: 0px; display:block; position: absolute;background:none;"></div>';
		}

		if(param['multi_overlay'])
		{
			if($('.opacity_container').length == 0)
			{
				$('body').children(':first-child').before(div);
			}
			else
			{
				$('.overlay_container:last').parent().after(div);
			}
		}
		else
		{
			document.body.insertBefore(div, document.body.firstChild);
		}


		if ( param['opacity'] !== false && param["missClick"] )
		{
			document.getElementById('opacity_' + id).onclick = function ()
			{
				if(!param["missClick_no_destroy"])
				{
					floating_container.destroy();
				}

				param["missClick"]();
			}
		}

		if(param['opacity_mouseover'])
		{
			document.getElementById('opacity_' + id).onmouseover = function ()
			{
				mouseover_reg = true;
				$('.opacity_container').stop();
				$('.mini_reg_box').css({zIndex : "0", position: "static"});
				$('.opacity_container').animate({ opacity: "0.01" }, 500,  function() {if(mouseover_reg == true){param["opacity_mouseover"]();}else{ $('.opacity_container').animate({ opacity: "0.3" }, 500); }});
				$('.reg_edition_border').remove();
			}
		}

		if ( browser.msie6 )
		{
			var select = document.getElementsByTagName("select");
			var selectLength = select.length;

			for ( var i = 0; i < selectLength; i++ )
			{
				select[i].style.display = 'none';

				floating_container.msie6_select_fix.push(select[i]);
			}
		}

		if ( param['opacity'] !== false )
		{
			document.getElementById('opacity_' + id).className = 'opacity_container';
			document.getElementById('opacity_' + id).style.height = (document.body.scrollHeight + (browser.msie ? 20 : 0)) + 'px';
		}

		if ( param['background_zindex'] )
		{
			document.getElementById('opacity_' + id).style.zIndex = param['background_zindex'];
		}

		if ( param['opacity_value'] !== false )
		{
			document.getElementById('opacity_' + id).style.opacity = param['opacity_value'];
			if(param['opacity_value'] == '0')
			{
				document.getElementById('opacity_' + id).style.filter = 'alpha(opacity=' + param['opacity_value'] + ')';
			}
		}

		if( param['opacity_background'] )
		{
			document.getElementById('opacity_' + id).style.background = param['opacity_background'];
		}

		document.getElementById('overlay_' + id).className = 'overlay_container';

		if ( param['window_zindex'] )
		{
			document.getElementById('overlay_' + id).style.zIndex = param['window_zindex'];
		}

		if(typeof(param['closeFunc']) == 'undefined')
		{
			param['closeFunc'] = 'floating_container.destroy(' + id + ');';
		}

		if ( param['close'] && param['close'] == true )
		{
			document.getElementById('overlay_' + id).innerHTML = floating_container.button_close(id, param['margin_right'], param['padding_bottom'], param['closeFunc']);

			closeImage = true;
		}
		else if ( param['closeImage'] && param["closeImage"] == true )
		{
			document.getElementById('overlay_' + id).innerHTML = '<table cellpadding="0" cellspacing="0" style="width:100%"><tr><td align="left"><div>' + ( param["headerContent"] ? param["headerContent"] : "" ) + '</div></td><td style="padding:3px 3px 0px 0px;" align="right"><img style="cursor:pointer;" id="close_overlay_container_' + id + '" onClick="'+param['closeFunc']+'" src="' + data.full_server_name + '/images/hrest1.gif"></td></tr></table>';

			closeImage = true;
		}

		document.getElementById('overlay_' + id).innerHTML += '<div style="' + (param['width'] ? 'width:' + param['width'] + pxVar+';' : '') + 'background:' + (param['background_color'] ? param['background_color'] : 'white') + ';" id="scroll_' + id + '">' + param['content'] + '</div>';

		if ( param["footerContent"] )
		{
			document.getElementById('overlay_' + id).innerHTML += '<div id="footerContent' + id + '">' + param["footerContent"] + '</div>';
		}

		if ( param['round'] && param['round'] == true )
		{
			if ( param['round-color'] )
			{
				floating_container.round_corners(id, param['round-color']);
			}
			else
			{
				floating_container.round_corners(id);
			}
		}

		if ( param['width'] )
		{
			document.getElementById('overlay_' + id).style.width = param['width'] + pxVar;
		}

		document.getElementById('overlay_' + id).style.display = '';


		if ( param['floating_for_obj'] )
		{
			var for_obj = param['floating_for_obj'];
			var x = findPosX(for_obj);

			document.getElementById('overlay_' + id).style.left = (x - param['width'] + param['for_obj_left']) + 'px';
		}
		else
		{
			document.getElementById('overlay_' + id).style.left = (document.body.clientWidth - document.getElementById('overlay_' + id).clientWidth) / 2 + 'px';
		}
/*
document.getElementById('overlay_' + id).parentNode.style.position = 'relative';
alert(document.getElementById('overlay_' + id).style.left);
*/


		scroll_height = ( browser.msie ) ? document.getElementById('scroll_' + id).parentNode.clientHeight : document.getElementById('scroll_' + id).clientHeight;

		if ( param['max-height'] && param['max-height'] < (scroll_height * 100 / document.documentElement.clientHeight) )
		{
			document.getElementById('scroll_' + id).style.overflowY = 'auto';

			if ( browser.msie )
			{
				document.getElementById('scroll_' + id).style.overflowX = 'hidden';
			}

			document.getElementById('scroll_' + id).style.height = (document.documentElement.clientHeight * param['max-height'] / 100) + 'px';

			if(!browser.msie)
			{
				scroll = true;
			}
		}

		if ( param['floating_for_obj'] )
		{
			var for_obj = param['floating_for_obj'];
			var y = findPosY(for_obj);
			document.getElementById('overlay_' + id).style.top = (y + param['for_obj_top']) + 'px';
		}
		else
		{
 			( !param['top'] ) ? floating_container.set_top(id) : floating_container.set_top(id, param['top']);
		}

		if ( param['bottom'] )
		{
			floating_container.set_bottom(id, param['bottom']);
		}

		if ( param["return"] && param["return"] == "obj" )
		{
			var returnObj =
			{
				"id" : id,
				"overlay" : document.getElementById("overlay_" + id),
				"scroll" :  document.getElementById('scroll_' + id)
			};

			if ( param['opacity'] !== false )
			{
				returnObj["opacity"] = document.getElementById("opacity_" + id);
			}

			if ( closeImage == true )
			{
				returnObj["close"] = document.getElementById('close_overlay_container_' + id);
			}

			if ( param["footerContent"] )
			{
				returnObj["footer"] = document.getElementById("footerContent" + id);
			}

			floating_container.id_list_obj[id] = returnObj;

			return returnObj;
		}
		else
		{
			return id;
		}
	},

	set_bottom : function (id, px)
	{
		if ( (document.documentElement.clientHeight - document.getElementById('overlay_' + id).clientHeight) > 0 )
		{
			scrollTop = (document.getElementsByTagName('body')[0].scrollTop > document.getElementsByTagName('html')[0].scrollTop) ? document.getElementsByTagName('body')[0].scrollTop : document.getElementsByTagName('html')[0].scrollTop;

			document.getElementById('overlay_' + id).style.top = scrollTop + document.documentElement.clientHeight - document.getElementById('overlay_' + id).clientHeight - px + "px";
		}
		else
		{
			/* if ( browser.chrome )
			{
				document.getElementById('overlay_' + id).style.top = (document.body.scrollTop + (px ? px : 10)) + 'px';
			}
			else
			{
				document.getElementById('overlay_' + id).style.top = (document.getElementsByTagName( 'html' )[0].scrollTop + (px ? px : 10)) + 'px';
			} */
		}
	},

	set_top : function (id, px)
	{
		var correction = $('.headerTop').height();
		if(browser.mobile )
		{
			var docScrollTop = $(document).scrollTop() + correction +2;
			var docScrollLeft = $(document).scrollLeft();
//			alert(docScrollTop);
//			alert(docScrollLeft);

//			$('#overlay_' + id).css({ top: docScrollTop, left: docScrollLeft});
			$('#overlay_' + id).css({ top: docScrollTop});
// alert(document.getElementById('overlay_' + id).style.left);
		}
		else
		{
// alert(correction);
			correction = correction +20;
			var winHeight = document.documentElement.clientHeight - correction;

			document.getElementById('overlay_' + id).style.left = (document.body.clientWidth - document.getElementById('overlay_' + id).clientWidth) / 2 + 'px';

			if ( (winHeight - document.getElementById('overlay_' + id).clientHeight) > 0 )
			{
				scrollTop = (document.getElementsByTagName('body')[0].scrollTop > document.getElementsByTagName('html')[0].scrollTop) ? document.getElementsByTagName('body')[0].scrollTop : document.getElementsByTagName('html')[0].scrollTop;

				document.getElementById('overlay_' + id).style.top = ((!px ? ((winHeight - document.getElementById('overlay_' + id).clientHeight + correction) / 2) : 0) + scrollTop + (px ? px : 0)) + 'px';
			}
			else
			{

/*
				if ( browser.chrome )
				{
					document.getElementById('overlay_' + id).style.top = (document.body.scrollTop + (px ? px : 10)) + 'px';
				}
				else
				{
					document.getElementById('overlay_' + id).style.top = (document.getElementsByTagName( 'html' )[0].scrollTop + (px ? px : 10)) + 'px';
				}
*/
					document.getElementById('overlay_' + id).style.top = (document.body.scrollTop + correction + (px ? px : 10)) + 'px';
			}
		}
	},

	round_corners : function (id, color)
	{
		var newVersion = ( color ) ? true : false;
		var color = ( color ) ? color : '#FFFFFF';

		if ( browser.msie || newVersion == false )
		{
			document.getElementById('overlay_' + id).innerHTML = '<div class="rounded-corner-container" style="background:transparent none !important;"><div class="rounded-corner-1" style="background:' + color + ';"></div><div class="rounded-corner-2" style="background:' + color + ';"></div><div class="rounded-corner-3" style="background:' + color + ';"></div><div class="rounded-corner-4" style="background:' + color + ';"></div></div>' + document.getElementById('overlay_' + id).innerHTML + '<div class="rounded-corner-container" style="background:transparent none !important;"><div class="rounded-corner-4" style="background:' + color + ';"></div><div class="rounded-corner-3" style="background:' + color + ';"></div><div class="rounded-corner-2" style="background:' + color + ';"></div><div class="rounded-corner-1" style="background:' + color + ';"></div></div>';
		}
		else
		{
			document.getElementById('overlay_' + id).innerHTML = '<div class="br8" style="background:' + color + ';">' + document.getElementById('overlay_' + id).innerHTML + '</div>';
		}
	},

	button_close : function (id, margin_right, padding_bottom, closeFunc)
	{
		if(!closeFunc)
		{
			var closeFunc = 'floating_container.destroy(' + id + ');'
		}
		return '<div style="background:white;padding:2px 0px ' + (padding_bottom ? padding_bottom : 5) + 'px 0px;text-align: right;"><span style="cursor:pointer;margin:0px ' + (margin_right ? margin_right : 10) + 'px 0px 0px;" id="close_overlay_container_' + id + '" onClick="'+closeFunc+'" class="genmed_b">Çàêðûòü</span></div>';
	},

	destroy : function (id)
	{
		if ( browser.msie6 )
		{
			while ( floating_container.msie6_select_fix.length > 0 )
			{
				var select = floating_container.msie6_select_fix.shift();

				select.style.display = '';
			}
		}

		if ( id )
		{
			if ( document.getElementById("floating_container_" + id) )
			{
				document.getElementById("floating_container_" + id).parentNode.removeChild(document.getElementById("floating_container_" + id));

				id_listL = floating_container.id_list.length;

				for ( i = 0; i < id_listL; i++ )
				{
					if ( floating_container.id_list[i] == id )
					{
						floating_container.id_list.splice(i, 1);

						break;
					}
				}
			}
		}
		else
		{
			id_listL = floating_container.id_list.length;

			for ( i = 0; i < id_listL; i++ )
			{
				id = floating_container.id_list[i];

				if ( document.getElementById("floating_container_" + id) )
				{
					document.getElementById("floating_container_" + id).parentNode.removeChild(document.getElementById("floating_container_" + id));

					id_listL = floating_container.id_list.length;

					for ( i = 0; i < id_listL; i++ )
					{
						if ( floating_container.id_list[i] == id )
						{
							floating_container.id_list.splice(i, 1);

							break;
						}
					}
				}
			}

			floating_container.id_list = Array();
		}
	}
}

function closeDestroy()
{
	tinymce.remove();
	floating_container.destroy();
	window.location.hash = '#close';
}

function popUpWin(contentHtml, width, redirect, dop)
{
	var contWidth = 'auto';

	if(width != '' && typeof(width) != 'undefined')
	{
		var contWidth = width;
	}

	closeDestroy();

	try
	{
		if($('#'+contentHtml).length != 0 )
		{
			contentWin = $('#'+contentHtml).html();
		}
		else
		{
			contentWin = contentHtml;
		}
	}
	catch (e)
	{
		// contentHtml - ýòî òî÷íî html-êîä
		contentWin = contentHtml;
	}


	var boxWindow = '<div style="">'+contentWin+'</div>';

	id = floating_container.create
	({
		'content' : boxWindow,
		'background_color' : 'none',
		'width' : contWidth,
		'missClick' : closeDestroy,
		'opacity_value' : '0.3',
		//'top' : ScrollTop,
		'round' : false
	});
}

checkScrollTop = 0;


if(browser.mobile)
{
	$(document).on('touchstart click', function (event)
	{
		if ($(event.target).closest(".iosSubMenuBox").length) return;

/*
		if ($(event.target).closest('.selectedList:not(.displayNone) .PopupListBoxSelected').length || $(event.target).closest('.pseudoSelect.noBottomBorder').length) return;
*/

		$('li.iosSubMenuBoxHover').removeClass('iosSubMenuBoxHover');
	});
}

$(document).ready(function(){

	if(browser.mobile)
	{
		$('.grayscale').addClass('grayscaleMobile').removeClass('grayscale');
		$('.img-shadow').addClass('img-shadowMobile').removeClass('img-shadow');

		$('.feedback').css('width', '17px');
/*
		$(document).bind('touchstart', function(){
			$('.iosSubMenuBoxHover').removeClass('iosSubMenuBoxHover');
		});
*/
/*
		$('li.iosSubMenuBox').css('border', '1px solid red');
*/
		$('li.iosSubMenuBox').bind('click', function(){ /* touchstart click */

			if($(this).hasClass('iosSubMenuBoxHover'))
			{
				$('li.iosSubMenuBoxHover').removeClass('iosSubMenuBoxHover');
			}
			else
			{
				$('li.iosSubMenuBoxHover').removeClass('iosSubMenuBoxHover');
				$(this).toggleClass('iosSubMenuBoxHover');
			}

		});
	}

	if(window.location.hash == '#expert')
	{
		html = '<div class="whiteBoxExpert">\
				<div class="absBoxExpert"><span class="closeExpertCross cursors" onclick="closeDestroy();">&times;</span></div>\
				<span class="romartBigLogo mainSprite inlineBlock verticalMiddle"></span>\
				<span class="inlineBlock verticalMiddle" style="padding-left:120px; text-align: right;">Ýêñïåðò â îáëàñòè<br>ìàòåðèíñòâà è äåòñòâà</span>\
			</div>\
<div align="center" class="vinousBox"><span>Áëàãîäàðèì çà óñïåøíóþ ïîäïèñêó<br>íà ðàññûëêó ÃÊ ÐÎÌÀÐÒ.<br><br>Â áëèæàéøåå âðåìÿ ñ âàìè ñâÿæåòñÿ<br>ìåíåäæåð äëÿ óòî÷íåíèÿ èíôîðìàöèè</span></div>';

		popUpWin(html, '526', '', '');
	}

	    $(window).scroll(function(){
			if(parseInt($('.feedback').css('width')) <= 41)
			{
		        if($(window).scrollTop() > 30)
				{
					if(parseInt($('.feedback').css('width')) > 17 && checkScrollTop == 0)
					{
						// console.log($(window).scrollTop()+' = ÑÊÐÛÒÜ = '+parseInt($('.feedback').css('width')));
						checkScrollTop = 1;
						$('.feedback').stop().animate({width : '17px'}, 'fast', function(){ checkScrollTop = 0; feedbackShort = '17px'; });
					}
				}
				else if($(window).scrollTop() < 30 && parseInt($('.feedback').css('width')) < 41 && checkScrollTop == 0)
				{
					// console.log($(window).scrollTop()+' = '+parseInt($('.feedback').css('width')));

					checkScrollTop = 1;
					$('.feedback').stop().animate({width : '41px'}, 'fast', function(){ checkScrollTop = 0; feedbackShort = '41px'; });

					// console.log('ñòàëî '+$('.feedback').css('width'));
				}
			}
	    })

	$(window).resize(function(){
		if($('.overlay_container').length != 0)
		{
			var id = $('.overlay_container').attr('id');
			id = id.substring(8);

			floating_container.set_top(id);
		}
	});
});



