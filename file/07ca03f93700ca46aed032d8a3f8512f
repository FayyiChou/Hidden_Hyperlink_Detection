<script type="text/javascript">

	MyIntervalID = '';

$(function()
{
	//Enable swiping...

	$(".slider ul").swipe(
	{
		//Generic swipe handler for all directions

		swipe:function(event, direction, distance, duration, fingerCount, fingerData)
		{

			if(direction == null)
			{
				// alert(direction);
				direction = 'left';
			}
			var directionMirror = (direction == 'left') ? 'right' : 'left';
			slideTo(directionMirror);

//			$(this).text("You swiped " + direction );
		},

		//Default is 75px, set to 0 for demo so any distance triggers swipe

		threshold:0
	});
});


	function sliderJS (obj, sl) // slider function
	{

		clearInterval(MyIntervalID);
		timeOutSlider();

		var ul = $(sl).find("ul");
		var bl = $(sl).find("li.slider"+obj);
		var step = $(bl).width();

//		$(ul).animate({marginLeft: "-"+step*obj}, 500);

		$(ul).animate({left: "-"+step*obj}, 500);
	}

	function slideTo(arg)
	{
		var allElements = $('.nav').children().length;
		var checkedNum = $('.nav .on').index();
		// alert(allElements);
		// alert($('.nav .on').index());

		if(arg == 'right')
		{
			if(checkedNum+1 == allElements)
			{
				$('.nav').children('span:first-child').click();
			}
			else
			{
				$('.nav').children('.on').next().click();
			}
		}
		else if(arg == 'left')
		{
			if(checkedNum == 0)
			{
				$('.nav').children('span:last-child').click();
			}
			else
			{
				$('.nav').children('.on').prev().click();
			}
		}
	}

	function timeOutSlider()
	{
		MyIntervalID = setInterval(function(){slideTo('right'); }, 10000);
	}


	$(document).on("click", ".nav span", function() // slider click navigate
	{
		var sl = $(this).parent().parent().find(".slider");
		$('.nav').find("span").removeClass("on");
		$(this).addClass("on");
		var obj = $(this).attr("data-rel");
		sliderJS(obj, sl);
		return false;
	});

	$(document).ready(function()
	{
/*
		$(".sliderParent").each(function ()
		{
			var obj = $(this);
			$(obj).append("<div class='nav'></div>");

			$(obj).find("li").each(function ()
			{
				$(obj).find(".nav").append("<span data-rel='"+$(this).index()+"'></span>");
				$(this).addClass("slider"+$(this).index());
			});

			$(obj).find("span").first().addClass("on");
		});
*/
		timeOutSlider();
	});

	function selectCat(elem)
	{
		if(!$(elem).hasClass('contentsHover'))
		{
			$('.contentsHover').removeClass('contentsHover');
			$(elem).addClass('contentsHover');
			var num = $(elem).attr('data-cat');
/*
			$('.manualsBox div').slideUp("slow");
			$('.manualsBox div[attr='+num+']').slideDown("slow");
*/
/*
			$('.manualsBox .displayView').removeClass('displayView').addClass('displayNone');
			$('.manualsBox div[data-cat="'+num+'"]').removeClass('displayNone').addClass('displayView');
*/

			$('.manualsBox .displayView').slideUp("fast", function(e){
				$('.manualsBox div[data-cat="'+num+'"]').fadeIn("fast").removeClass('displayNone').addClass('displayView').css('display', '');
			}).removeClass('displayView').addClass('displayNone');



			$('.manualsTextBox .displayView').slideUp("fast", function(e){
				$('.manualsTextBox span[data-cat="'+num+'"]').fadeIn("fast").removeClass('displayNone').addClass('displayView').css('display', '');
			}).removeClass('displayView').addClass('displayNone');

		}
	}


function isIntoView(elem)
{
	var docViewTop = $(window).scrollTop();
    var docViewBottom = docViewTop + $(window).height();
	var elemTop = $(elem).offset().top;
	var elemBottom = elemTop + $(elem).height();

	var t = docViewBottom - 150;
//    console.log(elemTop+'Event Fired'+t);

	return (elemTop <= docViewBottom - 150);
}

function showHideBox()
{
	var element = $('.appearBox');

	$(element).each(function(e){
		if (isIntoView(this))
		{
			$(this).addClass('avatar-scroll-appear').removeClass('avatar-scroll-disappear');
		}
		else
		{
			$(this).addClass('avatar-scroll-disappear').removeClass('avatar-scroll-appear');
		}
	});

}

$(document).ready(function(){
	$(window).scroll(function(){
		showHideBox();
	});
	$(window).resize(function(){
		showHideBox();
	});
	showHideBox();
});



</script>

