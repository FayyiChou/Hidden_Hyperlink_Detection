<script type="text/javascript">
    // 增长动画
    var steps = 100,timeInt = 15;
    var numAnim = new countUp("ent-totalCount",0,1072,0,(steps*timeInt / 1000),{
        separator:'',
                useEasing:true
    });

    var initPic = function() {
                // 饼图动画
        var ctx = document.getElementById("ent-chart").getContext("2d");
        var pieChart = new Chart(ctx);
        var pieData = [
            {value:322, color:'#86B33F'},
            {value:204, color:'#357BA8'},
            {value:546, color:'#DC6220'}
        ];

        var hasStart = false;
        var startAnimite = function () {
            if (hasStart)
                return;

            hasStart = true;
            numAnim.start();
            pieChart.Pie(pieData, {animation: typeof G_vmlCanvasManager === 'undefined'});
        };
        $("#ent-seasonCount,#ent-weekCount,#ent-monthCount").fadeIn(timeInt * steps);

        if(g_center.isMain) {
            $('#ent-total').bind('inview', function () {
                startAnimite();
                $('#ent-total').unbind('inview');
            });
        }else{
            startAnimite();
        }
    }

    if($.browser.msie && parseInt($.browser.version) < 9){
        window.onload = initPic;
    }else{
        initPic();
    }
    $('#header-search-word').focus();

    $('.flexslider').flexslider({
        //animation: "slide",
        prevText : '',
        nextText : ''
    });

    // 新闻点击
    $('.news-list h3 span.fl').bind('click',function(){
        var jThis = $(this);
        if(jThis.hasClass('select'))
            return;

        var id = jThis.attr('data-id');
        jThis.parent().parent().find('.select').removeClass('select');

        jThis.addClass('select');
        $('#news-' + id).addClass('select');
    });
    $('.news-list h3 a').bind('click',function(){
        var jThis = $(this);
        var id = jThis.parent().find('.select').attr('data-id');

        //window.open('/node/news/?cate=' + id);
        window.open('/node/news/?type=index&cate=' + id);
    });
