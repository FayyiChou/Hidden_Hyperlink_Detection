          <script type=text/javascript>
        function cselectTag(showContent,selfObj){
            // 操作标签
            var tag = document.getElementById("ctags").getElementsByTagName("li");
            var taglength = tag.length;
            for(i=0; i<taglength; i++){
                tag[i].className = "";
            }
            selfObj.parentNode.className = "cselectTag";
            // 操作内容
            for(i=0; j=document.getElementById("ctagContent"+i); i++){
                j.style.display = "none";
            }
            document.getElementById(showContent).style.display = "block";
            
            
        }
        </script>
    
