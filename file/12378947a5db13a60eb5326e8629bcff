<script>
$(function(){
  $("input[name='sub']").click(function(){
      var number=$("input[name='number']").val();
      if(number=="" || number=="请填写后11位.例如:20140160000."){
        alert("请填写后11位.例如:20140160000.");
      }else{
        $.ajax({
          type:'post',
          url:'/goods/ajax',
          data:'number='+number,
          dataType:'json',
          success:function(msg){
            if(msg.status==1){
              alert(msg.info);
            }else{
              alert(msg.info);
            }
          }
        })
      }
      
  })
})
</script>
