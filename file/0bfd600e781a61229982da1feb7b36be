</div>
<script>
//使用div时，请保证colee_left2与colee_left1是在同一行上.
var speed=30//速度数值越大速度越慢
var scroll_div2=document.getElementById("scroll_div2");
var scroll_div1=document.getElementById("scroll_div1");
var scroll_div=document.getElementById("scroll_div");
scroll_div2.innerHTML=scroll_div1.innerHTML
function Marquee3(){
if(scroll_div2.offsetWidth-scroll_div.scrollLeft<=0)//offsetWidth 是对象的可见宽度
scroll_div.scrollLeft-=scroll_div.offsetWidth//scrollWidth 是对象的实际内容的宽，不包边线宽度
else{
scroll_div.scrollLeft++
}
}
var MyMar3=setInterval(Marquee3,speed)
scroll_div.onmouseover=function() {clearInterval(MyMar3)}
scroll_div.onmouseout=function() {MyMar3=setInterval(Marquee3,speed)}
</script></DIV></DIV>
