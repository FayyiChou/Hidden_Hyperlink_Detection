					$(this).children('ul').css('top','auto').css('bottom',height + 'px');
				}else{				
					$(this).children('ul').css('top',height+'px').css('bottom','auto');	
				}
				$(this).children('a').css({'font-family':layer944D875BC24EACE29D13867E15DE36C0_getSubMenuHoverCss("font-family",0),'font-size':layer944D875BC24EACE29D13867E15DE36C0_getSubMenuHoverCss("font-size",0),'color':layer944D875BC24EACE29D13867E15DE36C0_getSubMenuHoverCss("color",0),'font-weight':layer944D875BC24EACE29D13867E15DE36C0_getSubMenuHoverCss("font-weight",0),'font-style':layer944D875BC24EACE29D13867E15DE36C0_getSubMenuHoverCss("font-style",0)});
