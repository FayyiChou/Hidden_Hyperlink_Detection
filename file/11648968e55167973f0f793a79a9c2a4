$(document).ready(function(){

	$(".footerbg").delay(100).fadeIn(300);

	$("ul#footera li").find("ul").hide();

	$("ul#footera li").hover(function(){

		$(this).css("background","#000");

		

		$(this).children().css("color","#000");

		$(this).find("a.hide").css("color","#fff");

		$(this).find("ul").show();

