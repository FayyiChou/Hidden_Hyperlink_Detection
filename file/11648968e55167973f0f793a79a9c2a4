//兼容IOS

var isIPad = function(){ return navigator.userAgent.match(/iPad/i) !== null; };

var isIPhone = function(){ return navigator.userAgent.match(/iPhone/i) !== null; };

var isIOS = function(){ return isIPhone() || isIPad(); };



var checkOSForPlayer = function(){

	if(isIOS()){

		$("#video1").css("display", "block");

		$("#banner_img").css("display", "none");

		document.getElementById("video1").src = vo_now;

		

		$("#video2").css("display", "block");

		$("#banner_img2").css("display", "none");

		document.getElementById("video2").src = getCurrentVideoUrl2();

		

		$("#video3").css("display", "block");

		$("#banner_img3").css("display", "none");

		document.getElementById("video3").src = getCurrentVideoUrl3();

	}

}

$().ready(function() {

	checkOSForPlayer();

});

//end

