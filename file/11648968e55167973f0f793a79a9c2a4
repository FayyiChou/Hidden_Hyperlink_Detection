//是点击公用的播放器方法start 

function onImgClick_p(url){

	var $blackGround = $("<div class='blackBackground'></div>");

	$(".blackBackground").css("height",$(window).height());

	$blackGround.appendTo("body").show();

	$(".boxOuter").fadeIn().find(".video").html($(".video01").html());

	vo_now=url;

	onImgClick();

}



var onImgClick = function (){

		firstPlay = true;

		var oSwf = getSwfObject();

		if(oSwf){

			$("#banner_img").css("display", "none");

			$("#banner_div").css("display", "none");

			$("#videoPlayer")[0].style.display = "block";

			oSwf.style.display = "block";

			loadVideo(vo_now);

		}

	}

	function closevideo(){

	

		$(".blackBackground").hide();

		$(".boxOuter").fadeOut().find(".video").html(" ");

	} 

		

//是点击公用的播放器方法 end



//是点击第二个播放器调用的方法

function onImgClick2(){

	firstPlay = true;

	var oSwf = swfobject.getObjectById("videoPlayer2");

	if(oSwf){

		$("#banner_img2").css("display", "none");

		$("#videoPlayer2")[0].style.display = "block";

		oSwf.style.display = "block";

		

		if($("#videoPlayer")[0].style.display == "block")

			swfobject.getObjectById("videoPlayer").stopVideoPlay();

		if($("#videoPlayer3")[0].style.display == "block")

			swfobject.getObjectById("videoPlayer3").stopVideoPlay3();

		try{

		oSwf.load2(getCurrentVideoUrl2());

}catch(e){}

	}

}

//end

//是点击第三个播放器调用的方法

function onImgClick3(){

	firstPlay = true;

	var oSwf = swfobject.getObjectById("videoPlayer3");

	if(oSwf){

		$("#banner_img3").css("display", "none");

		$("#videoPlayer3")[0].style.display = "block";

		oSwf.style.display = "block";

		

		if($("#videoPlayer2")[0].style.display == "block")

			swfobject.getObjectById("videoPlayer2").stopVideoPlay2();

		if($("#videoPlayer")[0].style.display == "block")

			swfobject.getObjectById("videoPlayer").stopVideoPlay();

		try{

		oSwf.load3(getCurrentVideoUrl3());

}catch(e){}

	}

}

//end

