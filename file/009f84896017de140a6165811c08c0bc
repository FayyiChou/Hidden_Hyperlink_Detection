	<script language="javascript">
    	 function time(){
    		var sysTime=new Date();  
    		var startTime=new Date("2009/5/08 10:00");
    		var endTime=new Date("2009/5/11 8:00");
    		if(startTime<sysTime&&sysTime<endTime){
    			 document.getElementById("demoIndexGr").style.display="block";
    		} else {
    			 document.getElementById("demoIndexGr").style.display="none";
			}

    	}
    		time();
	</script>
