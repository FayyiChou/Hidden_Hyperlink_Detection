			<script>
			function bookPolicy(airlineCode,departureAirport,arrivalAirport){
				alert("登陆后您即可预定");
            	if(airlineCode!=null&&departureAirport!=null&&arrivalAirport!=null){
        			window.location.href='/liantuo/manage/newListPolicyFlight.in?airlineCode='+airlineCode+'&departureAirport='+departureAirport+'&arrivalAirport='+arrivalAirport;
            	}else{
            		alert("数据错误！请联系平台客服");
            	}
    		}
			</script>
