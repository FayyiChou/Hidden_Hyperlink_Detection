function setContent(str){  
str = str.replace(/<[^>].*?>/g,"");
str = str.replace(/[&nbsp;]/gi,"");
str = str.replace(/[&nbsp]/gi,"");     
str = str.replace(/[&]/gi,"");   
return str;   
}
