function showVideo(id,f){
	if(f=='y'){
		$.openPopupLayer({url: '/video/videoyk.php',name: 'showVideo',parameters:{id:id}});
	}else{
		$.openPopupLayer({url: '/video/video.php',name: 'showVideo',parameters:{id:id}});
	}
}
$('.prev img').hover(
	function(){
		$(this).attr("src","/images/foot/arrow-prev.gif");
	},
	function(){
		$(this).attr("src","/images/foot/arrow-prev.png");
	}
	
);
$('.next img').hover(
		function(){
			$(this).attr("src","/images/foot/arrow-next.gif");
		},
		function(){
			$(this).attr("src","/images/foot/arrow-next.png");
		}
		
	);
</script>

