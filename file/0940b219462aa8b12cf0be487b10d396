	function chk_update_header_login(obj){	
		Msg="";
		if(obj.xaccount.value == ""){
			Msg+="e-mail帳號 \n";
		}
		if(obj.xpassword.value == ""){
			Msg+="密碼 \n";
		}

		if(Msg!=''){
			alert("下列為必填欄位：\n\n"+Msg);
			return false;
		}
	
		obj.submit();
		return true;
	}
</script>