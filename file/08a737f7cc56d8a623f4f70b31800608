<script type="text/javascript" src="http://static.jfrft.com/muban/js/backtop.js"></script><script type="text/javascript">
$(document).ready(function () {
        var bt = $('#toolBackTop');
        var sw = $(document.body)[0].clientWidth;

        var limitsw = (sw - 1060) / 2 - 230;
        if (limitsw > 0){
                limitsw = parseInt(limitsw);
                bt.css("right",limitsw);
        }

        $(window).scroll(function() {
                var st = $(window).scrollTop();
                if(st > 30){
                        bt.show();
                }else{
                        bt.hide();
                }
        });
})
</script>
<style>
.back-to {bottom:60px;overflow:hidden;position:fixed;right:10px;z-index:999;}
.back-to .back-top:hover {background-position: -50px 0;}
</style>
