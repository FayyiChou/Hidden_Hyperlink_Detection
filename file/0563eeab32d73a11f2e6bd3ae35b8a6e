function SwcAr04( POjbID, ViewID, MaxID )
{
 try
 {
  for( var i=1; i<=MaxID; i++ )
  {
  var k=ViewID+1;
  document.getElementById(POjbID+"Div"+i).style.display = "none";
  document.getElementById(POjbID+i).style.color= "#000";
  document.getElementById(POjbID+ViewID).style.background = "url('http://images1.wenming.cn/web_sc/images/pic_16.jpg')";
  document.getElementById(POjbID+i).style.background = "none";
  }
  document.getElementById(POjbID+"Div"+ViewID).style.display = "";
 document.getElementById(POjbID+ViewID).style.color= "#fff";
 document.getElementById(POjbID+ViewID).style.background = "url('http://images1.wenming.cn/web_sc/images/pic_16.jpg')";
  document.getElementById(POjbID+i).style.background = "none";
 }
 catch(err)
 {}
}
</script>
