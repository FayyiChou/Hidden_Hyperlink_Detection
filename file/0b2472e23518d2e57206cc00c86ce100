<script type="text/javascript">
  (function(){
		var loc= window.location.toString();
		var urlUtilArr = new Array();
		urlUtilArr['cpcd1'] = {splitUrl:'hemai'};
		urlUtilArr['cpcd2'] = {splitUrl:'http://yhd.aicai.com/kaijiang'};
		urlUtilArr['cpcd3'] = {splitUrl:'http://yhd.aicai.com/ggtj'};
		urlUtilArr['cpcd4'] = {splitUrl:'http://live.yhd.aicai.com'};
		urlUtilArr['cpcd5'] = {splitUrl:'http://zst.yhd.aicai.com'};
		urlUtilArr['cpcd6'] = {splitUrl:'/pages/rank'};
		urlUtilArr['cpcd7'] = {splitUrl:'http://yhd.aicai.com/info'};
		for(var tempkey in urlUtilArr){
			var tempSplitUrl = urlUtilArr[tempkey].splitUrl;
			if(tempSplitUrl&&loc.indexOf(tempSplitUrl)>=0){
				document.getElementById(tempkey).className="hot";
				document.getElementById("cpcd0").className="";
				break;
			}
		}
		var timeoutId = -1;
		var tempObj = document.getElementById("headlotteryNavId");
	    var timeoutFn = function(){
	    	jQuery('#headlottery_spanId').attr("class","yhd_ltit");
	    	jQuery('#headlottery_divId').stop(true,true).slideUp(500);
			jQuery("#_icon_down").hide();
			jQuery("#_icon_up").show();
	    };
	    tempObj.onmouseover = function () {
	    	  window.clearTimeout(timeoutId);
	    	  jQuery('#headlottery_spanId').attr("class","yhd_ltit1");
	    	  jQuery('#headlottery_divId').stop(true,true).slideDown(500);
	    	  jQuery("#_icon_up").hide();
			   jQuery("#_icon_down").show();
		};
		  tempObj.onmouseout = function () {
		    	timeoutId = window.setTimeout(timeoutFn,500);
			};
  })();
</script>    
    
