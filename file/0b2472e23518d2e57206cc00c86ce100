	function CNNIC_change(eleId){
		var str= document.getElementById(eleId).href;
		var str1 =str.substring(0,(str.length-6));
		str1+=CNNIC_RndNum(6);
		document.getElementById(eleId).href=str1;
	}
	function CNNIC_RndNum(k){
		var rnd="";
		for (var i=0;i < k;i++)
			rnd+=Math.floor(Math.random()*10);
		return rnd;
	}

