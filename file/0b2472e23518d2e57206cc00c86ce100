	//关闭广告层
	function guanggao_none(){          
		divId.style.display="none";
		setZstCookie("guanggao_div_top", "divHidden");
		jQuery("#jq_pic_url").attr("src","");
		jQuery("#jq_pic_url").attr("width","");
		jQuery("#jq_pic_url").attr("height","");
		userClickTrack('AaicaizstClose001');
	}
	//添加cookies
	function setZstCookie(name,value){
	   var domain = document.domain;
	   var exp  = new Date();  
	   exp.setTime(exp.getTime() + 30*60*1000);//半小时
	   document.cookie =name+"="+encodeURIComponent(value) + ";expires=" + exp.toGMTString()+"; path=" + "/" + "; domain=" + domain ;  
	}
	//获取指定名称的cookie的值 
	function getZstCookie(objName){
	  	var arrStr = document.cookie.split("; ");  
	  	for(var i = 0;i < arrStr.length;i ++){  
	  		var temp = arrStr[i].split("=");  
	  		if(temp[0] == objName) 
	  		return unescape(temp[1]);  
	  	}	  
	}
	function openPicUrl(url){
	   _gaq.push(['_trackPageview','/goal/click/new_user_register.html']);	
	   window.open(url);
	}
</script>
