	function getDateInfo() {
		var d = new Date();
		var dateInfo;
		dateInfo = d.getFullYear() + "年" + (d.getMonth()+1) + "月" + d.getDate() + "日　星期" + '日一二三四五六'.charAt(d.getDay());
		return dateInfo;
	}
	
	document.all.dateInfo.innerHTML=getDateInfo();
	
		loginInfo.innerHTML = '您好，<a href="#" class="blueFont" onclick="popLoginDiv(document.all.login)">请登录！</a>';
	
	
	var intervalCount=1;
	function blinklink(){
		if (intervalCount==1){
			$('#messages').css('color','#FF3300');
			$('#messages').find('a').css('color','#FF3300');
			$('#notice').css('color','#FF3300');
			intervalCount--;
		}else{
			$('#messages').css('color','#000000');
			$('#messages').find('a').css('color','#000000');
			$('#notice').css('color','#000000');
			intervalCount++;
		}
	}
	setInterval('blinklink()',500);
	
	
	function popLoginSuccesDiv(divObj,isFormApp) {
		$('#fade').css('display','block');
		document.all.fade.style.height = Math.max(document.body.scrollHeight, document.body.clientHeight);
		divObj.style.position="absolute";
    	divObj.style.height = 465+"px";
    	divObj.style.width = 860+"px";
		divObj.style.top= document.body.scrollTop + (document.body.clientHeight - 480)/2+"px";
		divObj.style.left= (document.body.clientWidth - 860)/2+"px";
		divObj.style.display="";
	}
	
