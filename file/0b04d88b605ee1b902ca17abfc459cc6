<script type="text/javascript">
	var noSendMessage = true;
	$(function(){
		var mon = ":input[id='loginName']"
			+ ", :input[name='certNo']";
		$(mon).click(function() {
			var num = 0;
			$(mon).each(function(){
				if($.trim(this.value) == ""){
					num ++;
				}
				if(num > 1){
					// 获取验证码按钮暂时不能使用
					$("#send").attr("disabled", "disabled");
				}else if(noSendMessage){
					// 获取验证码按钮可以使用
					$("#send").removeAttr("disabled");
				}
			})
		});
	});
	
	var returnUrl = "";
	var userTypeValue = "";
	var isFirst="";
	function popLoginDiv(divObj,isFormApp,sysname) {
		$('#fade').css('display','block');
		document.all.fade.style.height = Math.max(document.body.scrollHeight, document.body.clientHeight);
		if (isFormApp != '1') {
			$('#login').find('table').css('display','');
			$('#login').find('li').css('display','');
			fnTabClick(0);
		}
		divObj.style.position="absolute";
    	divObj.style.height = 260+"px";
    	document.all.loginForm.style.height = "";
    	document.all.logWrap.style.height = "";
    	if(sysname == 'ht' ){
    		divObj.style.width = 500+"px";
    		divObj.style.height = 320+"px";
    		document.all.logWrap.style.height = 275+"px";
    		document.all.loginForm.style.height = 140+"px";
    		
    	}else if(sysname == 'gg') {
    		divObj.style.width = 395+"px";
    	}else if(sysname == 'mc') { //名称登记
    		divObj.style.width = 460+"px";
    		divObj.style.height = 310+"px";
    		document.all.logWrap.style.height = (310-40)+"px";
    		document.all.loginForm.style.height = 140+"px";
    	} else {
    		divObj.style.width = 380+"px";
    	}
		divObj.style.top= document.body.scrollTop + (document.body.clientHeight - 260)/2+"px";
		divObj.style.left= (document.body.clientWidth - 380)/2+"px";
		
		divObj.style.display="";
	}
	
	// 计算遮挡层高度
    var _height = function() {
        var scrollHeight, offsetHeight;
        if ($.browser.msie && $.browser.version < 7) { // IE6
            scrollHeight = Math.max(document.documentElement.scrollHeight, document.body.scrollHeight);
            offsetHeight = Math.max(document.documentElement.offsetHeight, document.body.offsetHeight);

            if (scrollHeight < offsetHeight) {
                return $(window).height() + "px";
            } else {
                return scrollHeight + "px";
            }
        } else {
            return $(document).height() + "px";
        }
    };
    
    // 计算遮挡层宽度
    var _width = function() {
        var scrollWidth, offsetWidth;
        if ($.browser.msie) { // IE
            scrollWidth = Math.max(document.documentElement.scrollWidth, document.body.scrollWidth);
            offsetWidth = Math.max(document.documentElement.offsetWidth, document.body.offsetWidth);

            if (scrollWidth < offsetWidth) {
                return $(window).width() + "px";
            } else {
                return scrollWidth + "px";
            }
        } else {
            return $(document).width() + "px";
        }
    };
	
	function closeLoginDiv(divObj) {
		divObj.style.display="none";
		$('#fade').css('display','none');
	}
	
	var mcVal = '0,1,3,6';//名称系统对应登录用户类型
	var ptmcVal = '3,5';//名称系统对应登录用户类型
	var nwzVal = '0,1,3';//内外资系统对应登录用户类型
	var gtVal = '0,2,3';//个体系统对应登录用户类型
	var njVal = '1,0';//年检系统对应登录用户类型
	var sghnVal = '0,1,2,3';
	var wlscVal = '0,1,2';
	var tmVal = '0,1,4';
	function toApp(url,sysName){
		returnUrl = url+"&sysName="+sysName;
		$("#sysUrl").val(returnUrl);
		
			$('#login').find('table').css('display','none');
			$('#login').find('li').css('display','none');
			if (sysName == 'mc') {
				//身份证登录(3)
				$('#tabs').find('li').eq(0).css('display','');				
				$('#tabs').find('li').eq(1).css('display','');
				$('#tabs').find('li').eq(3).css('display','');
				$('#tabs').find('li').eq(6).css('display','');								
				fnTabClick(3);
			}
			if (sysName == 'pt') {
				//身份证登录(3)
				$('#tabs').find('li').eq(3).css('display','');
				$('#tabs').find('li').eq(5).css('display','');
				fnTabClick(3);
			}
			if (sysName == 'nz' || sysName == 'wz') {
				//ca登录(1)和身份证登录(3)
				$('#tabs').find('li').eq(1).css('display','');
				$('#tabs').find('li').eq(3).css('display','');
				$('#tabs').find('li').eq(0).css('display','');
				fnTabClick(0);
			}
			if (sysName == 'gt') {
				//个体登录(2)和身份证登录(3)
				$('#tabs').find('li').eq(2).css('display','');
				$('#tabs').find('li').eq(3).css('display','');
				fnTabClick(2);
			}
			if (sysName == 'nj') {
				var feature = "height=540,width=950,top="+ (window.screen.availHeight-80-550)/2 + ",left="+ (window.screen.availWidth-10-850)/2 + ",toolbar=no,menubar=no,scrollbars=yes,resizable=yes,location=no,status=no";
				window.open("http://www.fjaic.gov.cn/bgs/gsdt/zwyw/201402/t20140220_118622.htm",'newwindow',feature);
			}
			
			if (sysName == 'sp' || sysName == 'gg' 
					|| sysName == 'ht' || sysName == 'nongz') {
				$('#tabs').find('li').eq(1).css('display','');
				$('#tabs').find('li').eq(2).css('display','');
				
				if(sysName != 'nongz'){
					$('#tabs').find('li').eq(3).css('display','');
				}
				if(sysName == 'ht'){
					$('#tabs').find('li').eq(6).css('display','');
				}
				$('#tabs').find('li').eq(0).css('display','');
				fnTabClick(0);
			}
			if (sysName == 'wlsc'){
				//ca登录(1)和个体户(2)
				$('#tabs').find('li').eq(1).css('display','');
				$('#tabs').find('li').eq(2).css('display','');
				$('#tabs').find('li').eq(0).css('display','');
				fnTabClick(0);
			}
			if (sysName == 'tm') {
				$('#tabs').find('li').eq(1).css('display','');
				$('#tabs').find('li').eq(4).css('display','');
				$('#tabs').find('li').eq(0).css('display','');
				fnTabClick(0);
			}
			popLoginDiv(document.all.login,'1',sysName);
			
		  
	}
	
	// 登录
	function toLogon(){
		
		disabledButton();
		var sysUrl = $("#sysUrl").val();
		if(userTypeValue=="0"){
			//证书登陆
 			if(login(sysUrl)==false){
				showButton();
			}
			else{
				//这句暂时不能执行，因为在打开的应用中，如果用户不存在，就将该用户登录信息删除了，导致报错，需要修改农资的应用，，不能删除登录用户信息
				//window.open(returnUrl);
			}
		}else if(userTypeValue == '6'){
			elLogin();
			showButton();
			return;
		}else if(userTypeValue == '7'){
			toContactLogin();
			showButton();
			return;
		} else if(logonCheck() == false){
			showButton();
			return;
		}else{
			$("#userTypeValue").val(userTypeValue);
			var params = "";
			var url = "";
			//注册号首次登陆
			if(userTypeValue == "1" && $('#ifFirstLogin').val() == "1"){
				url = "login.do?method=dologin4AjaxByFirstRegNum";
			}else{
				url = "login.do?method=dologin4Ajax";
			}
		}
		params=$("#loginForm").serialize();
		$.ajax({
			url:url,
			type:'POST',
			async: true,
			data: params, 
			dataType: "text",
			success:function(result) {
				if(result == "" || result == null){
					//登陆成功
					closeLoginDiv(document.all.login);
					window.location.reload();
				}else{
					isFirst="1";
					//登陆失败，显示验证码
					//$("#regCodeTr").show();
					$("#loginError").show();
					//fj.refreshCap($('#cap'));
					showButton();
				}
			},
		    error: function(XMLHttpRequest, textStatus, errorThrown) {       
				alert('系统异常，请联系技术支持！');        
			}
		});
	}
	
	// 注销
	function toLogoff(){
		var params ="loginId=";
		var url = "login.do?method=exit";
		$.ajax({
			url:url,
			type:'POST',
			data: params, 
			success:function(result) {
				window.location.reload();
			}
		});
	}
	
	function caLogin(){
		var ucert = $("#ucert").val();
		var usign = $("#usign").val();
		if(ucert=="" || usign==""){
			ucert = CAUtil.cert();
			usign = CAUtil.sign(random);
			$("#ucert").val(ucert);
			$("#usign").val(usign);
			$("#random").val(random);
		}
		var validUrl = "login.do?method=caLogin";
		var params=$("#loginForm").serialize();
			  $.ajax({
					type: "post",
					url: validUrl,
					data: params,
					async: false,
	                dataType : "json",
					success: function(data) {
					  if(data.status){
						 window.location.reload();
					  }else{
					  	var error = data.msg;
					  	if(/^\d+$/.test(error) && !isNaN(error)){
					  		window.location.href="login.do?method=caerror&retCode="+error;
					  	}else{
				    		alert(data.msg);
				    	  	return false;
				    	}
					  }
				    },
				    error: function(XMLHttpRequest, textStatus, errorThrown) {       
						alert('连接超时，请确认网络畅通后重试！');        
					}
				});
	}
	
	function elLogin(){
		// pin码
		var loginName = $.trim($("#loginName2").val());
		if(loginName == '') {
			alert("请输入PIN码！");
			return;
		}
		
		//获取电子营业执照信息
		getElMessage();
		
		var validUrl = "login.do?method=elLogin";
		var params=$("#loginForm").serialize();
		$.ajax({
				type: "post",
				url: validUrl,
				data: params,
				async: false,
	            dataType: "text",
				success: function(result) {
				  if(result == "success"){
					 window.location.reload();
				  } else if(/^\d+$/.test(result) && !isNaN(result)){
				  	 switch(result)
				  	 {
				  	 	case '10002':
							window.top.dialogMessage("电子执照已更新,<a herf='javascript:' style='color:blue' onclick=\"window.open('http://61.154.11.191:8080/businessQuery/index.jsp')\">请点击此链接更新电子执照</a>。",undefined,undefined,{title:'登录失败'});
				    	 	break;
				    	case '10003':
				    		window.top.dialogMessage("无电子执照数据，数据未同步。",undefined,undefined,{title:'登录失败'});
				    		break;
				    	case '10004':
				    		window.top.dialogMessage("参数错误。",undefined,undefined,{title:'登录失败'});
				    		break;
				    	case '10005':
				    		window.top.dialogMessage("电子执照失效。",undefined,undefined,{title:'登录失败'});
				    		break;
				    	case '10006':
				    		window.top.dialogMessage("系统代码错误。",undefined,undefined,{title:'登录失败'});
				    		break;
				    	case '10007':
				    		window.top.dialogMessage("系统代码失效。",undefined,undefined,{title:'登录失败'});
				    		break;
				    	case '10008':
				    		window.top.dialogMessage("非本系统签发电子执照。",undefined,undefined,{title:'登录失败'});
				    		break;
				    	default:
				    		window.top.dialogMessage("电子营业执照验证失败！",undefined,undefined,{title:'登录失败'});
			    	 }
				  } else {
			    	 window.top.dialogMessage(result,undefined,undefined,{title:'登录失败'});
				  }
			    },
			    error: function(XMLHttpRequest, textStatus, errorThrown) {       
					alert('连接超时，请确认网络畅通后重试！');        
				}
			});
	}
	
	function toContactLogin(){
		// 联络人备案登录
		var regNo = $.trim($("input[id='loginName']").val());
		var certNo = $.trim($("input[name='certNo']").val());
		var captcha = $.trim($("input[name='phonecaptcha']").val());
		
		if(regNo == ""){
			alert("请输入注册号！");
			return;
		}else if(certNo == ""){
			alert("请输入联络员证件号！");
			return;
		}else if(captcha == ""){
			alert("请输入备案手机验证码！");
			return;
		}
		
		var validUrl = "login.do?method=contactLogin";
		var params=$("#loginForm").serialize();
		$.ajax({
				type: "post",
				url: validUrl,
				data: params,
				async: false,
	            dataType: "text",
				success: function(result) {
				  if(result == "success"){
					 window.location.reload();
				  } else {
			    	 alert(result);
			    	 $("#loginError").show();
				  }
			    },
			    error: function(XMLHttpRequest, textStatus, errorThrown) {       
					alert('连接超时，请确认网络畅通后重试！');        
				}
			});
	}	


	function logonCheck() {
		if($('#ifFirstLogin').val() == "0"){
			if ($.trim($("#loginName").val()) == "") {
				window.alert("请填写用户名");
				$("#loginName")[0].focus();
				return false;
			}
			if (document.all.password.value == "") {
				window.alert("请填写密码");
				document.all.password.focus();
				return false;
			}
		}
	
	   	return true;
	}
	
	function disabledButton(){
		$('input[type="button"]').each(function(item, value){
			$(value).attr("disabled",true);
		});
		
	}
	
	function showButton(){
		$('input[type="button"]').each(function(item, value){
			$(value).attr("disabled",false);
		});
	}
	
	//获取随机数
	function getCAInfo(type) {
 		if(!CAUtil.init()) {
			return false;
		}
    	var url = "login.do?method=random";
	    $.ajax({
      	 		type:"post",
      	 		url:url,
      	 		async: true,
      	 		dataType: "text",
      	 		success: function(msg){
      	 			$("#random").val(msg);
      	 			if(type == '1'){
      	 				togetCAInfo(msg);
      	 			}else{
      	 				caLogin(msg);
      	 			}
      	 		}
      	 });
	}
	
