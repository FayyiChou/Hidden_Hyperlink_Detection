	var sysUrl = '';
	
	function agreeAction(){
		if (sysUrl != null && sysUrl != '') {
			//暂时写法
			sysUrl="http://wsgs.fjaic.gov.cn:80/name/login?1=1";
			
		}
	}
	
	function notAgreeAction(){
		closeLoginDiv(document.all.ptmcdj);
		toLogoff();
	}
	
	function toOpen(url,sysName){
