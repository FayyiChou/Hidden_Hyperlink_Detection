	var tpUSER=1;
	
//******************************
//
// 验证系统的登录页面调用的函数
//
//******************************
function getElMessage(){
	var pkObject = PkiCom_3E6E10ED_201311060001;
	var pinNo = $.trim($("#loginName2").val());
	ret = pkObject.BLicInit();
	
	if(ret !=0 ){
	   alert("智能卡初始化失败"+ret);
       return ret;
	}
	ret = pkObject.BLicLogin(tpUSER,pinNo);
    if (!ret){
        alert("登录智能卡失败，请确认输入的口令是否正确!");
        return -1;
    }
	if(ret){
		//获取证书
	  	var type=1;
		myType = pkObject.BLicReadLable("LAB_USERCERT_SIG",type)
		$("input[name=licenceEntity]").val(myType);
		
		//获取签名
		var signText = $("input[name=signText]").val();
		var keyType = pkObject.KeyType;
	    var hashAlgo = '33772';
	    if(keyType == 'RSA'){
	    	hashAlgo='32772';
	    }  
	    
		signdata = pkObject.BLicSign (signText,hashAlgo);
		if(signdata!=null){
			$("input[name=signValue]").val(signdata);
		}else{
			alert("签名失败");
		}
	}
  	//智能卡退出
	ret = pkObject.BLicLogout();
    if(!ret){
      alert("智能卡退出失败!");
      return -1;
    }
	ret = pkObject.BLicEnd();
	if(ret !=0){
		alert("智能卡释放失败"+ret);
        return ret;
	}
}
	
</script>


