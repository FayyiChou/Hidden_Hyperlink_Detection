<script>

	var userType = document.all.userType;
	var oldMsgCnt = document.all.messageDiv.innerHTML;
	function fnTabClick(nTab) {
		document.all.messageDiv.innerHTML = oldMsgCnt; //使用原值初始化；
		$('#ifFirstLogin').val("0");			
			for ( var i = 0; i < userType.length; i++) {
				userType[i].className = "";
			}
			userType[nTab].className = "current";
			document.all.loginBtn.disabled = "";
			document.all.phoneMessageDiv.style.display="none";
			if(nTab == 1){
				document.all.loginTb.style.display="none";
				document.all.regNoTable.style.display = "none";
				document.all.caTable.style.display="";
				userTypeValue = "0";
				document.all.messageDiv.style.display="";
			} else{
				document.all.loginTb.style.display="";
				document.all.link.style.display="none";
				document.all.passwordTr.style.display="";
				document.all.regNoTable.style.display = "none";
				document.all.caTable.style.display="none";
				document.all.messageDiv.style.display="none";
				document.all.phoneMessageTr.style.display = "none";
					document.all.certNoTr.style.display = "none";
				if(nTab == 2){
					$("#loginHref").show();
					document.all.registerTd.innerHTML = '<a id="loginHref" href="user.do?method=register&userType=1" class="red" target="_blank">注册新用户</a>';
					document.all.loginNameTh.innerText = "注册号：";
					userTypeValue = "1";
				}
				if(nTab == 3){
					$("#loginHref").show();
					document.all.registerTd.innerHTML = '<a id="loginHref" href="user.do?method=register&userType=2" class="red" target="_blank">注册新用户</a>';
					document.all.loginNameTh.innerText = "身份证号码：";
					document.all.resisterPwdTd.innerHTML = '<a href="user.do?method=getPassword" class="red" target="_blank">忘记密码';
					document.all.messageDiv.innerHTML = "<span style='color:red' >提示：自然人用户可以使用已注册的身份信息进行登陆</span>";
					document.all.messageDiv.style.display="";
					userTypeValue = "2";
				}
				if(nTab == 4){
					document.all.registerTd.innerHTML = '';
					document.all.loginNameTh.innerText = "机构代码：";
					userTypeValue = "3";
					document.all.registerTd.innerHTML = '<a id="loginHref" href="user.do?method=register&userType=3" class="red" target="_blank">注册新用户</a>';
				}
				if(nTab == 5){
					document.all.registerTd.innerHTML = '';
					document.all.loginNameTh.innerText = "用户名：";
					document.all.resisterPwdTd.innerText = "";
					userTypeValue = "8";
					$("#loginHref").hide();
				}
				if(nTab == 6){
					document.all.registerTd.innerHTML = '';
					document.all.loginNameTh.innerText = "注册号/统一社会信用号：";									
					document.all.passwordTr.style.display="none";
					document.all.phoneMessageTr.style.display = "";
					document.all.certNoTr.style.display = "";
					userTypeValue = "7";
					document.all.phoneMessageDiv.style.display="";
					$("#loginHref").hide();
				}
				$('#loginName2').hide();
				$('#loginName2').attr("disabled","disabled");
				$('#loginName').show();
				$('#loginName').attr("disabled",false);
				if(nTab == 0){
					document.all.link.style.display="";
					document.all.messageDiv.style.display="";
					document.all.passwordTr.style.display="none";
					document.all.registerTd.innerHTML = '';
					document.all.loginNameTh.innerText = "PIN码：";
					$("#loginName").hide();
					$('#loginName').attr("disabled","disabled");
					$('#loginName2').show();
					$('#loginName2').attr("disabled",false);
					
					userTypeValue = "6";
					$("#loginHref").hide();
				}
			}
	}
	
	userType[0].click();

	function regNoLogin(isFirst){
		if(isFirst == '1'){
			$('#ifFirstLogin').val("1");
			document.all.regNoTable.style.display = "";
			document.all.loginTb.style.display = "none";
			document.all.loginBtn.disabled = "disabled";
		}else{
			$('#ifFirstLogin').val("0");
			document.all.regNoTable.style.display = "none";
			document.all.loginTb.style.display = "";
			document.all.loginBtn.disabled = "";
		}
	}
	
	//检查注册号有效性
	function checkRegNum4Ajax(){
		var url = "login.do?method=checkRegNum4Ajax";
		$.post(url, 
			{
				regNum: $('#regNum').val()
			}, 
			function(data, status){
				if(data == "1"){
					document.all.loginBtn.disabled = "disabled";
					alert("该用户已经存在，请选择已注册用户登录!");
				}
				else{
					document.all.loginBtn.disabled = "";
				}
			});
	}
	
	function getMsg(){
		$("#send").attr("disabled", "disabled");
		var regNo = $.trim($("input[id='loginName']").val());
		var certNo = $.trim($("input[name='certNo']").val());
		if(certNo != "" && regNo != ""){
			$.ajax({
				url:"user.do?method=get_sms",
				type:'POST',
				async: true,
				data: {regNo: regNo,certNo: certNo}, 
				dataType: "text",
				success:function(result) {
					if(result == "1"){
						alert("短信验证码已发送，有效期为180秒。请在有效期内输入验证码！");
						noSendMessage = false;
						// 获取验证码按钮
						var send = $("#send");
						// 1分钟后可以重新获取
						var cd = 90;
						// 倒数计时器
						var count = function(ct, itv) {
							if(ct >= 0) {
								window.setTimeout(function() {
									// 动态设置按钮文字，并禁用按钮
									send.val("重新获取(" + ct + ")").attr("disabled", "disabled");
									// 倒数计时
									count(ct - 1, 1000);
								}, itv);
							} else {
								// 计时结束
								send.val("重新获取").removeAttr("disabled");
							}
						};
						// 开始倒数计时
						count(cd, 0);
			
					}else{
						alert(result);
						$("#send").removeAttr("disabled");
					}
				},
			    error: function(XMLHttpRequest, textStatus, errorThrown) {       
					alert('系统异常，请联系技术支持！');        
				}
			});
		}else{
			alert("请输入完整的企业联络员备案信息！");
			$("#send").removeAttr("disabled");
		}
	}
	

</script> 

