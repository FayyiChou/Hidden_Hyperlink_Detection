<script>
function swap_az(n){
	for(var i=1;i<=2;i++){
		var curC=document.getElementById("azview"+i);
		var curB=document.getElementById("az"+i);
		var curD=document.getElementById("azleft"+i);
		var curE=document.getElementById("azright"+i);
		var curF=document.getElementById("azmore"+i);

		if(n==i){
			curC.style.display="block";
			curB.className="az1";
			curD.className="azleft1";
			curE.className="azright1";
			curF.style.display="block";
	
		}
		else{
			curC.style.display="none";
			curB.className="az2";
			curD.className="azright2";
			curE.className="azright2";
			curF.style.display="none";

	    }
	}
} 
</script>
<style>
.az1{font-size:12px;color:#ffffff;font-weight:bold; background:url(/images/341/bm2_30.jpg) left top repeat-x}
.az2{font-size:12px;color:#333333;font-weight:bold;}
.azleft1{background:url(/images/341/bm2_29.jpg) left top no-repeat}
.azleft2{}
.azright1{background:url(/images/341/bm2_32.jpg) left top no-repeat}
.azright2{}
</style>
