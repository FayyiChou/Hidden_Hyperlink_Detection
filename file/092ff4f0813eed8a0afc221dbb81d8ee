$(function(){
	$('<div id="qqalert" style="background:url(http://www.hbcjw.com.cn/td.jpg) no-repeat; width:217px; height:126px; position:fixed; bottom:0; right:3px"><a href="javascript:cqa();" style=" display:block; float:right; margin:100px 20px 20px 15px; color:#1670EB; font-size:12px">拒绝</a><a href="http://wpa.qq.com/msgrd?v=3&uin=753095022&site=qq&menu=yes" target="_blank" style=" display:block; float:right; margin:100px 0 0 10px; color:#1670EB; font-size:12px">接受</a></div>').appendTo("body");
	setInterval("cqa()",12000);
	setInterval("sqa()",4000);	   
});
function cqa(){
	$("#qqalert").fadeOut("slow");
}
function sqa(){
	if($("#qqalert").css("display")!="block"){
		$("#qqalert").fadeIn("slow");
	}
	speed=50;
	for(i=0;i<3;i++){
		$("#qqalert").animate({"right":"0"},speed).animate({"bottom":"4px"},speed).animate({"right":"4px"},speed).animate({"bottom":"0"},speed);	
	}
}
</script>
