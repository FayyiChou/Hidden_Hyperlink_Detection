	function chkForm(obj){
		if (obj.name.value.length<1){
			obj.name.focus();
			alert("姓名不能为空！");
			return false;
		}

		if (obj.tel.value.length<1){
			obj.tel.focus();
			alert("联系手机不能为空！");
			return false;
		}
		
		if (obj.zuanye.value.length<1){
			obj.zuanye.focus();
			alert("学校不能为空！");
			return false;
		}
		return true;
	}

</script>
