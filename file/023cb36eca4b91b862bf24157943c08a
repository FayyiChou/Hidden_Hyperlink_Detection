<script>
    function changenews(obj,colsid,id){
        if(document.getElementById("c"+colsid)){document.getElementById("c"+colsid).style.backgroundPosition="0px -29px";document.getElementById("c"+colsid).id='';}
        obj.style.backgroundPosition="0px 0px";
        obj.id="c"+colsid;
        for(i=1;i<=4;i++){
            if(document.getElementById("n"+colsid+i)){document.getElementById("n"+colsid+i).style.display='none';}
        }
        document.getElementById("n"+colsid+id).style.display="block";
    }


    </script>
