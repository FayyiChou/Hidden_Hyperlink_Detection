<script type="text/javascript">
    //网易热分享   add by zhangjinbo 2015-05-15
    //获取当前页面的url
    var url         = encodeURIComponent(window.location.href);
    //获取当前页面的标题
    var title       = encodeURIComponent(document.title);
    //获取文章区域的所有图片
    var as        = document.getElementsByTagName("a");
    var imgsrc = '';
    for(var n=0;n<as.length;n++){
    	if(as[n].title == "幻灯模式查看图片"){
    		imgsrc = as[n].getElementsByTagName('img')[0].getAttribute('src');
    		break;
    	}
    }
    if(imgsrc){
        param   = "&image="+encodeURIComponent(imgsrc);
    }else{
        param   = '';
    }
    jQuery(".wyire").click(function(){
        window.open("http://hot.163.com/share.html?title="+title+param+"&url="+url,'_blank');
    });

</script>
