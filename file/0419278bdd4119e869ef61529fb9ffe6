<script type="text/javascript">
    $(function(){
        $(".bkbm_sxjt a").click(function(){
            if($(".bkbm_sxjt").hasClass("open")){
                scrollBer2();
                $(".ws_rowen").removeClass("intro_down").addClass("intro_open");
                $(".bkbm_sxjt").removeClass("open").addClass("down");
                $(".bmbk_jqayjs").show();
                $("#play_int").addClass("play_intro2");
            }else{

                $(".ws_rowen").removeClass("intro_open").addClass("intro_down");
                $(".bkbm_sxjt").removeClass("down").addClass("open");
                $(".bmbk_jqayjs").hide();
                $("#play_int").removeClass("play_intro2");
            } 
        });
    });

function bmbk_hide(){
	document.getElementById("bmbk_jqay_nobg").style.visibility="hidden";
}
    // 搜索图片放大
    $(function(){
        $(".thumb").click(function(){
            $(".zoomin-img").show();
            $(".zoomout-img").hide();
            $(".scroll-area").css("top","161px");
        });
        $("#seebuyzoomouticon").click(function(){
            $(".zoomin-img").hide();
            $(".zoomout-img").show();
            $(".scroll-area").css("top","53px");
        });
    });
    if($.cookie('ts_id') == 1){
    	$("#ts").hide();
    }else{
    	$.cookie('ts_id',1,{domain:advDomain},{expires:30});
    	$("#ts").click(function(){
    		$("#ts").hide();
    	}) 
    	setTimeout('$("#ts").hide()',5000); 
    }
</script>

