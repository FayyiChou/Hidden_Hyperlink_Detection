				var setPlayerEshop = function (obj){
					switch(obj.state){
						case "ready":
						var dressSwf = document.getElementById("wsplayer");
						/**
							在收到sdk发送的ready后，将剧集id--001，单集id--000111，div的id传给sdk
						*/
						dressSwf.getDressAdjust("7376761","7399507","dressContent");
						var btn = document.getElementById("startdressBtn");
						btn.onclick = onStartBtnClick;
						break;
					}
				}
			</script>
