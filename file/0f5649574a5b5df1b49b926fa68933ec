<script type="text/javascript">
    var dir = 1; //每步移动像素，大＝快
    var speed = 30; //循环周期（毫秒）大＝慢
    var MyMar = null;
    function Marquee() {//正常移动
        var demo = document.getElementById("demo5");
        var demo2 = document.getElementById("demo7");
        if (dir > 0 && (demo2.offsetWidth - demo.scrollLeft) <= 0) {
            demo.scrollLeft = 0;
        }
        if (dir < 0 && (demo.scrollLeft <= 0)) {
            demo.scrollLeft = demo2.offsetWidth;
        }
        demo.scrollLeft += dir;
    }
    document.getElementById("demo7").innerHTML = document.getElementById("demo6").innerHTML;
    MyMar = setInterval(Marquee, speed);
</script>













        
