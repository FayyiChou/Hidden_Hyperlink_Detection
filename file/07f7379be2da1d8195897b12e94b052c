function cgCount(message){
	var lenM=message.value.length;
	$id("cgNum").innerHTML=360-lenM;
	$id("cgNum").innerHTML<0?message.value=message.value.substring(0,360):"";
}
function cgSubmit(){
	if($id("cgNum").innerHTML<0){
		TIP("亲……超出字数限制了哦……");
		return false;
	}else if($id("cgNum").innerHTML==360){
		TIP("亲！请您输入内容哈！");
		return false;
	}
	if(getcookie('Sbyou_neT_comment_23564')){
	  TIP('抱歉，24小时之内只能评论一次！');
	  return false;
	}
}
