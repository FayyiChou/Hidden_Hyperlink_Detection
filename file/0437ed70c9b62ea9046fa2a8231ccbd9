<script>
$(function(){
	var url = window.location.href;
	var url_this = url.split('cn/');
	if(url_this[1]){
		var action = url_this[1].split('/');
		if(action[0]=='OtherPay'){
			if(action[1]=='paint_taocan'){
				$('.paint_foot').addClass('active');
			}
			if(action[1]=='keep_taocan'){
				$('.keep_foot').addClass('active');
			}
			if(action[1]=='refuel_taocan'){
				$('.refuel_foot').addClass('active');
			}
		}
		if(action[0]=='Index'){
			if(action[1]=='left' || action[1]=='product'){
				$('.my_foot').addClass('active');
			}
			
		}
	}
})
</script>
