	<script>

function test_weixin(){
	WeixinJSBridge.invoke("getBrandWCPayRequest",{
           "appId":"wxbc0d27acdf64647a",     //公众号名称，由商户传入     
           "timeStamp":" 1475032867",         //时间戳，自1970年以来的秒数     
           "nonceStr":"t30r20j7jrwlob3ytbe0e0zwv2v3c9qy", //随机串     
           "package":"prepay_id=wx20160928112107444ced07700089355013",     
           "signType":"MD5",         //微信签名方式：     
           "paySign":"329DFDAB50D1C98B092607ECFB2683E2" //微信签名 
       },function(res){
    WeixinJSBridge.log(res.err_msg);
    if(res.err_msg=="get_brand_wcpay_request:ok"){
      $('#load_span').text('正在获取支付结果...');
      $('#loading').show();
      window.location.href = "/Paint/user";
          
    }else{
      if(res.err_msg == "get_brand_wcpay_request:cancel"){
        var err_msg = "您取消了支付";
      }else if(res.err_msg == "get_brand_wcpay_request:fail"){
        var err_msg = "支付失败<br/>错误信息："+res.err_desc;
      }else{
        var err_msg = res.err_msg +"<br/>"+res.err_desc;
      }
      alert(err_msg);
    }
  });
}
	$(function(){
		$('.footer').bind('click',function(){
			var request_url=$(this).attr('request_url');
        	window.location.href=request_url;
		})

		$('.click_close1').bind('click',function(){
			$('.tips_wrap').hide();
			$('#sharemcover').hide();
		});
		
		$('.block').bind('click',function(){
			var key = $(this).attr('key');
			var check_k = $(this).attr('check_k');
			var request_url=$(this).attr('request_url');

				if(key =='check'){
					var car_number = $('#car_number').val();
					$.ajax({
		                type: 'post',
		                url: '/Car/check_car',
		                data:{},    
		                dataType: 'json',
		                success: function(info){
		                	//alert(info.status);
		                    if(info.status=='1'){
		                    	if(check_k == 'repair'){
			                    	if(car_number){
				                        $.post('/Repair/is_result', {number:car_number},function(data){
								            if(data.status){
								                window.location.href=request_url;
								            }else{
								            	$('.repairInput_add').show();
								            }
								        });
				                	}else{
						            	$('.repairInput_add').show();
						            }
						        }else{
						        	window.location.href=request_url;
						        }
		                    }
		                    if(info.status=='0'){
		                    	if(check_k == 'repair'){
		                    		$('.car_input').attr('key','repair');
		                    	}else{
		                    		$('.car_input').attr('key','');
		                    	}
		                    	$('.car_input').attr('success_url',request_url);
		                    	$('.car_input').show();
		                    }
		                }
		           });
				}else{
	        		window.location.href=request_url;
				}
			//}
	    })

		$('.do_cancel').bind('click',function(){
			$(this).parent().parent().hide();
		});

		$('#do_sure').bind('click',function(){
			var request_url=$(this).attr('request_url');
			window.location.href=request_url;
		});

		$('.jihuo_sure').bind('click',function(){
			var request_url=$(this).attr('request_url');
			var success_url = $('.jihuo').attr('success_url');
			var number = $('#number').val();
			var limit_day = $('#limit_day').val();
			var is_limit = $('#is_limit').val();
			$.post(request_url, {number:number,limit_day:limit_day,is_limit:is_limit,car_default:1},function(info){
					$('.jihuo').hide();
		            showTip(info.message);
		            if(info.status){
		            	if(success_url == '/Repair/lists'){
		            		$.post('/Repair/is_result', {number:number},function(data){
					            if(data.status){
					                window.location.href = success_url;
					            }else{
					            	$('.repairInput_add').show();
					            }
					        });
		            	}else{
		            		window.location.href = success_url;
		            	}
		            }
		        });
		});
		

		$('#car_add').bind('click',function(){
			var key = $('.car_input').attr('key');
			var success_url = $('.car_input').attr('success_url');
		    var number = $('#number').val();
		    var limit_day = 0;
		    var is_limit = 0;
		    var flag = true;
		    if ($.trim(number) == '') {
		            showTip('车牌号不为空');
		            flag = false;
		        }else{
		            var reg = /^[\u4e00-\u9fa5]{1}[a-zA-Z]{1}[a-zA-Z_0-9]{4}[\u4e00-\u9fa5_a-zA-Z_0-9]{1}$/;     
		            if(!reg.test(number)){    
		                showTip('车牌格式不正确，请参照浙A12345');
		                flag = false;
		            }else{
		                var sheng = number.substr(0,1);
		                if(sheng == '浙'){
		                    var letter = number.substr(1,1);
		                    var last = number.substr(6,1);
		                    if(isNaN(last)){
		                        var last = number.substr(5,1);
		                    }
		                    var last = parseInt(last);
		                    if(letter=='A' || letter=='a'){
		                        is_limit = 1;
		                        $('#is_limit').val(is_limit);
		                        if(last==1 || last==9){
		                            limit_day = 1;
		                        }
		                        if(last==2 || last==8){
		                            limit_day = 2;
		                        }
		                        if(last==3 || last==7){
		                            limit_day = 3;
		                        }
		                        if(last==4 || last==6){
		                            limit_day = 4;
		                        }
		                        if(last==5 || last==0){
		                            limit_day = 5;
		                        }
		                        $('#limit_day').val(limit_day);
		                    }
		                }
		            }    
		        }
		    if(flag){
		        $.post($('.car_input').attr('request_url'), {number:number,limit_day:limit_day,is_limit:is_limit},function(info){
		            //showTip(info.message);
		            if(info.status){
		            	$('.car_input').hide();
		            	if(key=='repair'){
			                $.post('/Repair/is_result', {number:number},function(data){
					            if(data.status){
					                window.location.href = success_url;
					            }else{
					            	$('.repairInput_add').show();
					            }
					        });
			            }else{
			            	window.location.href = success_url;
			            }
		            }else{
		            	$('.car_input').hide();
		            	if(info.message == 'app'){
		            		$('.jihuo').attr('success_url',success_url);
		            		$('.jihuo').show();
		            	}else{
		            		showTip(info.message);
		            	}
		            }
		        });
		    }
		})
	})
	</script>

