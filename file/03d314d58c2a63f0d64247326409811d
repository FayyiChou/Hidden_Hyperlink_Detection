  $('.xh_head_search').find('a').click(function(){
    var _w = $(this).prev().val(),
      _url = $(this).attr('url');
    window.location.href=_url+(_w!=''?'?w='+_w:'')
  })
  $('.xh_head_search').find('input').keyup(function(e){
    var _w = $(this).val(),
      _url = $(this).next().attr('url');
    if (e.keyCode==13)
      window.location.href=_url+(_w!=''?'?w='+_w:'')
  })
