<script language="JavaScript" type="text/JavaScript">
function login1(){
  var loginId = document.generalForm.loginId.value;
  if(loginId == null || loginId == ''){
	window.alert("请填写用户名");
	return;
  }
  document.generalForm.action="/cms/WebuserAction.do?method=login&entry=1";
  document.generalForm.submit();
  return true;
}
</script>
