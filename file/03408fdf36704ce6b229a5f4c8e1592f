    <script language="javascript">
        function mOvr(src,clrOver) {
            if (!src.contains(event.fromElement)) {
                src.bgColor = clrOver;
            }
        }
        function mOut(src,clrIn) {
            if (!src.contains(event.toElement)) {
                src.bgColor = clrIn;
            }
        }
    </script>
    <script>
        function xopen(Type,Url,Width,Height){

            if (Type == 'dialog')
            {
                window.showModalDialog(Url,'',"dialogWidth:" + Width + "px;dialogHeight:" + Height + "px;center:yes;status:no;scroll:yes;help:no;");
            }
            else
            {
                window.open(url,'',"width=" + Width + "px,height=" + Height + "px,resizable=1,scrollbars=1");
            }
        }
        var returnValue
        function showMidifyPwdForm(){
             var LoginId = document.getElementById('Login').value;
             var url = "/login/modifyPwd.jsp";
             var returnValue = window.showModalDialog(url,LoginId,
                     "dialogWidth:400px;dialogHeight:200px;center:yes;status:no;scroll:no;help:no;");
             if(returnValue=="success"){
                 var url2 = "/user/exit.do?fresh=" + new Date().getTime();
                 new Ajax.Request(url2, {
                     method: 'post',
                     onSuccess: function(transport) {
                         if(transport.responseText == 'succ'){
                             var url3 = "http://www.ccopyright.com.cn/cpcc/index.jsp";
                             window.location = url3;
                         }
                     },
                     onFailure: function(transport){
                         //	alert("数据传送失败，可能网络不稳定...");
                     }
                 });
            }
        }

        function findpwdForm(){
            var url = "/user/forgetPW.do";
            window.showModalDialog(url,"","dialogWidth:450px;dialogHeight:280px;resizable:no;status:yes;scroll:no;");
        }
        function openwin()
        {window.open("/cpcc/CommentSub.jsp",'newwindow', 'height=400, width=500, top=100,left=100, toolbar=no, menubar=no, scrollbars=no, resizable=no,location=no, status=no');
        }
        var returnValue
        function modify(){
              var LoginId = document.getElementById('Login').value;
              var url = "/goadatadic/userinfo.do";
              var returnValue = window.showModalDialog(url,LoginId,
                      "dialogWidth:400px;dialogHeight:600px;center:yes;status:no;scroll:no;help:no;");
              if(returnValue=="success"){
            //    location.reload();
                  var url2 = "/user/exit.do?fresh=" + new Date().getTime();
                  new Ajax.Request(url2, {
                      method: 'post',
                      onSuccess: function(transport) {
                          if(transport.responseText == 'succ'){
                              var url3 = "http://www.ccopyright.com.cn/cpcc/index.jsp";
                              window.location = url3;
                          }
                      },
                      onFailure: function(transport){
                          //	alert("数据传送失败，可能网络不稳定...");
                      }
                  });
              }
        }
    </script>
