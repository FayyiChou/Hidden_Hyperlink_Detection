			jQuery(".add-bookmark").click(function(){
			jQuery("#rcl-overlay").fadeIn(400);
			var id_post = parseInt(jQuery(this).attr("id").replace(/\D+/g,""));
			if(!id_post) id_post=0;
			var title = document.title;
			title=title.split('|');
