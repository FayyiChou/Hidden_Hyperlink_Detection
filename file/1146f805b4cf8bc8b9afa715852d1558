		var $logoCarousel_smartclass = $('.logo-carousel-smart');
		var $autoplay = $logoCarousel_smartclass.data('auto-play');
		var $timeout = $logoCarousel_smartclass.data('timeout');
		var $shownav = $logoCarousel_smartclass.data('show-nav') ? true : false;
		var $loopplay = $logoCarousel_smartclass.data('loop-play') ? true : false;
		$logoCarousel_smartclass.owlCarousel({
			loop: $loopplay,
			dots: false,
			nav: $shownav,
			autoplay: $autoplay,
			autoplayTimeout: $timeout,
			responsive:{
				0:{
					items: 2,
				},
				600:{
					items: 3,
				},
				1000:{
					items: 5,
				}
			}
		});
		$('.owl-controls .owl-nav .owl-prev').html('');
		$('.owl-controls .owl-nav .owl-next').html('');
		$('#qcf-btn-smart').click(function(){
			$(this).find('.fa').toggleClass('fa-minus');
			$(this).parent().find('.nav-r').toggleClass('nav-show');
		});
