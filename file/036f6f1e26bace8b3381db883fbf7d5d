	<script type="text/javascript">
	//微课简介
	window.onload=function(){
		var overHeight=$('.over-diva').height();
	    if(overHeight>60){
	    	$(".more-arrow").show();
	    	}
		}
		$(".more-arrow").click(function(){
			$(this).find('.down').hide();
			$(this).find('.up').show();
			$(this).parents(".lesson-info-wrap").find('.over-div').removeClass('h-60');
		});
		$(".w-lessoninfo").on('mouseleave','.lesson-info-wrap',function(){
			$('.more-arrow').find('.up').hide();
			$('.more-arrow').find('.down').show();
			$(this).find('.over-div').scrollTop(0);
			$(this).find('.over-div').addClass('h-60');
		});


	thickboxReady();
	//做题多选
		$('.zuoti-t').toggle(function(){
			$(this).find('.zuoti-kong').addClass('zuoti-black');
		},function(){
			$(this).find('.zuoti-kong').removeClass('zuoti-black');
		});
		$('.zuoti-tt').click(function(){
			$(this).parent().siblings().find('.zuoti-kong').removeClass('zuoti-black');
			$(this).find('.zuoti-kong').addClass('zuoti-black');
		})

		$('.tk-close,.btn-gray').click(function(){
			$('#tk_bg').hide();
			$('.edu-listen-tk').hide();
		});
		$('.pingjia').live('click',function(){
			$('.choiced').html('&#xe612;');
			$('.choiced').addClass('choice');
			$('.choiced').removeClass('choiced');


			$(this).children('.choice').addClass('choiced');
			$(this).children('.choice').html('&#xe637;');
			$(this).children('.choice').removeClass('choice');
		});
		$('.zz').toggle(function(){
			$(this).unbind("mouseenter").unbind("mouseleave");
			$(this).addClass('xz');
		},function(){
			$(this).unbind("mouseenter").unbind("mouseleave");
			$(this).removeClass('xz');
		});
		$('.change-tab').click(function(){
			 $(this).addClass("green-bod")
			 .siblings().removeClass("green-bod");
			 var index = $('.change-tab').index(this);
			 $(".tab-content").eq(index).show()
			 .siblings('.tab-content').hide();
			  if(parseInt(index)==2){
			 	$('.listen-Right-top').eq(1).show();
			 	$('.listen-Right-top').eq(0).hide();
			 }else{
			 	$('.listen-Right-top').eq(1).hide();
			 	$('.listen-Right-top').eq(0).show();
			 }
		})

		$(document).ready(function(){
			$('.listen-lashen-btn').click(function(){
				if($(".edu-listen-Left").hasClass("zhankai")){
					$(".edu-listen-Left").removeClass("zhankai");
					$('.edu-listen-Left').animate({'right':'390px'});
					$('.edu-listen-Right').animate({'width':'390px'});
					$('.top-content2').animate({'right':'390px'});
					$('.lashen').html('&#xe623;');
				}else{
					$(".edu-listen-Left").addClass("zhankai");
					$('.edu-listen-Left').animate({'right':'0'});
					$('.top-content2').animate({'right':'70px'});
					$('.edu-listen-Right').animate({'width':'0'});
					$('.lashen').html('&#xe624;');
				}
			});
			//baidu share config
			window._bd_share_config={
			    "common":{
			        "bdSnsKey":{},
			        "bdText":"砌体工程全过程技术控制（一）",
