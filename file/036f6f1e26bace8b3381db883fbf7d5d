			        "bdMiniList":false,
			        "bdPic":"",
			        "bdStyle":"0",
			        "bdSize":"32"
			    },
			    "share":{}
			};
			with(document)0[(getElementsByTagName('head')[0]||body).appendChild(createElement('script')).src='http://bdimg.share.baidu.com/static/api/js/share.js?v=89860593.js?cdnversion='+~(-new Date()/36e5)];

			var url_type='1';
			var flag ='0';
			var nowwidth = document.body.clientWidth || document.documentElement.clientWidth ||   window.innerWidth;
			if(nowwidth<500){
				nowwidth =500;
			}
			if(url_type>1 && flag >0)
			{
				var bheight = 650;
				var str=playlesson('','100%','100%');
				$('#videoplay1').html(str);
			}else{
				var playable = '1';//是否可以看视频
				if(parseInt(playable)){
					var nowheight = $('.play-window').height();
					playhtml = '{"uu":"e5fceee7d8","callbackJs":"call","vu":"f09f2b40d3","autoplay":1,"auto_play":1,"height":650,"payer_name":"188629a149104a347","check_code":"e3b21df65935b954cfeb143f8b17a711"}';
					playhtml = eval("("+playhtml+")");
					playhtml.height = nowheight;
					if(playhtml !=''){
						player = new CloudVodPlayer();
			    		player.init(playhtml,"videoplay1");
					}
				}else{
					$('.play-window').hide();
					$('.info-window').show();
				}

			}
			$('#weibo_msg').remove();
			//getcomments();
			$('#share').hover(function(){$('.zhul_fx_hvbg').show();},function(){$('.zhul_fx_hvbg').hide()});
