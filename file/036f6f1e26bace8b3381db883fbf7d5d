			$('.cron-list-Box').scrollTop( //目录章节位置控制
				$('.s-cur').offset().top - $('.cron-list-Box').offset().top +$('.cron-list-Box').scrollTop()-250
			);

			window.onresize=function(){
			     var h = document.documentElement.clientHeight;
			     h = h-140;
			    $('embed').height(h+"px");
			}

			if(parseInt(0)<=0 && parseInt(2) ==2 && parseInt(0)>0 && parseInt(1)==0&&parseInt()==0){
				//微信二维码支付回调
				var i = setInterval(function() {
					$.post("/pay/ispay",{sn:""},function(data){
						data=$.parseJSON(data);
						if(data.errNo==0){
							location.href="/pay/success?order_lesson_id="+data.order_lesson_id+"&type="+data.type+"&lesson_id="+data.lesson_id;
							return false;
						}
						//判断返回其它的错误或者超过200次则停止请求
						if(data.errNo!=-10001){
							clearInterval(i);
							if(data.errNo==-10000){
								location.href="/shopping/waitorder";
								return false;
							}
							alert('由于长时间未响应,请确实是否支付成功');
							location.href="/shopping/buylesson";
						}
					});
				},2000);
			}


			$('#comment_content .cron-list-Box').scroll(function(){
		    	var t = $(this).scrollTop();
		    	var t_h = $(this)[0].scrollHeight;
		    	var h = $(this).height();
		    	var isover = $('#isover').val();
		    	if((t+h>=t_h)&&isover<=0){
		    		$('#isover').val(1);
		    		var lesson_id = $("#lesson_id").val();
		    		var small_id = $('#small_id').val();
		    		var type=$('#from_type').val();
		    		$.post('/weiketang/getcommentlists',{lesson_id:lesson_id,small_id:small_id,type:type},function(data){
		    			var data=eval("("+data+")");
		    			if(data.errNo==0){
		    				$('#comment_content .cron-list-Box').append(data.result);
							$('#small_id').val(data.small_id);
							$('#isover').val(0);
		    			}else{
		    				$('#isover').val(1);
		    			}

					});
		    	}

		    })



		});
		$("#wx-share").click(function(event) {
			var _wxShareUrl=window.location.href;
			$("#qr-img").empty();
			var qrcode = new QRCode(document.getElementById("qr-img"), {
			    text: _wxShareUrl,
			    width: 205,
			    height: 205,
			    colorDark : "#000000",
			    colorLight : "#ffffff",
			    correctLevel : QRCode.CorrectLevel.H
			});

			$(".wx-qrcode").show();
		});

		//微信弹框
		$(document).mouseup(function(e){
			var _con = $(".wx-qrcode");
			if(!_con.is(e.target) && _con.has(e.target).length === 0){
				  _con.hide();
			}
		});
		$("#wx-pop-x").click(function(){
			$(".wx-qrcode").hide();
		});


