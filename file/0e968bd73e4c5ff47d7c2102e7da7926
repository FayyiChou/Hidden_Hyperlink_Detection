     <script type="text/javascript">
        function hideHint() {
            $(".login_ts").hide();
        }
        initHint();
        var interval = null;
        function checkqrcode() {
            $.get("/sso/qrcodecheck",
                    function(data) {
                        var code = data.code;
                        if (code == -1) {
                            $("#qr_img").hide();
                            $(".qr_hint").show();
                            if (interval != null) {
                                window.clearInterval(interval);
                            }
                        } else if (code == 0) {
                            location.href = "/sso/login?service=wmshop&continue=&location=687474703a2f2f73686f702e77616e6d65692e636f6d2f70726573616c652f636865636b6f7574";
                        }
                    }, "json");
        }
        function refreshqrcode() {
            $("#qr_img").show();
            $("#qr_img").attr("src", "/sso/qrcode?" + Math.random());
            $(".qr_hint").hide();
            interval = window.setInterval(checkqrcode, 3500);
        }
        $(function() {
            $(".login dl input").focus(function() {
                $(this).closest('dl').addClass('bd');
            });
            $(".login dl input").blur(function() {
                $(this).closest('dl').removeClass('bd');
            });
            $(".login_account .toggle").click(function() {
                $(".login_account").hide();
                $(".login_scan").show();
                refreshqrcode();
            });
            $(".login_scan .toggle").click(function() {
                $(".login_scan").hide();
                $(".login_account").show();
                if (interval != null) {
                    window.clearInterval(interval);
                }
            });
            $('.ico_cl').click(function(){
                $(this).parent().siblings('input').val('');
            });
        });
    </script>

