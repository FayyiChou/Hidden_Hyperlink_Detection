	var lheader = 0;
	headerall.each(function(k,v){
		if(v.className=='on') { fheader=0 };
		if(fheader) lheader+=$(v).width();
	})
	hdScroll.scrollTo(-1*(lheader-$(window).width()/2+header.width()/2),0);
}

