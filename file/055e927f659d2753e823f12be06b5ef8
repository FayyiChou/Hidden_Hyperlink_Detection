 var richStorage = richStorage || (function(Storage, JSON) {
 	"use strict";
 	if (!Storage) {
 		try {
 			var userData = document.documentElement,
 				name = location.hostname,
 				expires = new Date();
 			userData.addBehavior("#default#userData");
 			expires.setDate(365);
 			userData.expires = expires.toUTCString();
 			userData.save(name);
 			Storage = {
 				setItem: function(key, value) {
 					userData.load(name);
 					userData.setAttribute(key, value);
 					userData.save(name);
 				},
 				getItem: function(key) {
 					userData.load(name);
 					return userData.getAttribute(key);
 				},
 				removeItem: function(key) {
 					userData.load(name);
 					userData.removeAttribute(key);
 					userData.save(name);
 				},
 				clear: function() {
 					userData.load(name);
 					userData.expires = (new Date()).toUTCString();
 					userData.save(name);
 				}
 			};
 		} catch (e) {
 			Storage = {
 				setItem: function() {},
 				getItem: function() {
 					return null;
 				},
 				removeItem: function() {},
 				clear: function() {}
 			};
 		}
 	}
 	return {
 		set: function(k, v) {
 			return Storage.setItem(k, JSON.stringify(v));
 		},
 		get: function(k) {
 			var v = Storage.getItem(k);
 			if (typeof v != 'string' || v == '') {
 				v = 'null';
 			}
 			return JSON.parse(v);
 		},
 		remove: function(k) {
 			return Storage.removeItem(k);
 		},
 		clear: function() {
 			return Storage.clear();
 		}
 	};
 })(window.localStorage, window.JSON);
//获取cookie值
 function getCookie(name, value, expires) {
 	if (typeof value != 'undefined') {
 		var expires = !value ? -1 : expires;
 		if (typeof expires == 'number') {
 			var date = new Date();
 			date.setTime(date.getTime() + (expires * 60 * 1000));
 			expires = '; expires=' + date.toUTCString();
 		} else {
 			expires = '';
 		}
 		var _domain = location.host.indexOf('.500.com') != -1 ? '500.com' : '500boss.com';
 		document.cookie = [name, '=', encodeURIComponent(value), expires, '; path=/', '; domain='+_domain].join('');
 	} else {
 		if (m = new RegExp("(^|)" + name + "=([^;]*)(;|$)", "gi").exec(document.cookie)) {
 			return decodeURIComponent(m[2]);
 		}
 		return null;
 	}
 }	
;(function($,arturl){
	 var newsHtml = {//模版
			 newList:function(d){
					h = [];
					tpl = '<p class="news-list-t">{$title}{$imgurl}</p><p class="news-list-ft"><span class="{$kanti}"></span><span class="pul-time">{$addtime}</span><span class="zan">{$cmcount}</span></p>'; 
					for ( var i in d) {
						if($("#a_"+d[i]['id']).length){continue;}
						var href="",zt_tag='',hasimg;
						if(d[i]['imgurl']){
							d[i]['hasimg']='has-img';
							d[i]['imgurl']='<span class="img"><img src="http://img.500.com/upimages/mc_new/slt/20150831142521_3435.png" data-src="'+d[i]['imgurl']+'"></span>'; 
						}else{
							d[i]['hasimg']='';
						}
						if(d[i]['topicurl']){
							d[i]['kanti']='kanti';
							href = d[i]['topicurl'];
						}else if(d[i]['recommend']==1){
							d[i]['kanti']='kanti tuij';
						}else if(d[i]&&d[i]['isMatch']==1&&d[i]['statusid']=="1"){//赛前
							href = prefix+d[i]['url'];
						}else if(d[i]&&d[i]['isMatch']==1){
							href = prefix+d[i]['url'];
						}else{
							//href = detail_url+"/"+d[i]['id']+"-"+d[i]['sid']+".shtml";
							href = detail_url+"/detail-"+d[i]['id']+".shtml";
						}
						var li_srt = this.build(tpl, d[i]);
						li_srt = '<li class="'+d[i]['hasimg']+'" id="a_'+d[i]['id']+'" data-type=""><a href="'+href+'">'+zt_tag+''+li_srt+'</a></li>';
						h.push(li_srt);
					}
					return h.join("");
			},
			build:function build(tpl, data) {
					return tpl.replace(/\{\$(\w+)\}/g, function(a, b){
						return (b in data) ? data[b] : "";
					});
			}
	 };
	 var newsObj  = {
		init:function(){
			
			this.click.li_href();
			this.click.nav();
			this.click.pullUp();
			this.click.refresh();
			//添加浏览色
			var aids = JSON.parse(richStorage.get('news.aids'));
			for(i in aids){
				$("#"+aids[i]['aid']).find("p").eq(0).attr("style","color:#999");
				$("#"+aids[i]['aid']+"_t").find("p").attr("style","color:#999");
			}
		},
		ajax:{
			listadd:function(param){
				$.ajaxSettings.async = false;
				$.getJSON(arturl, param, function(data){
					if(data.length==0){
						if(param.neworold){
						}else{
							$("#pullUp").html('<span class="more-list"><em>无更多信息</em></span>').fadeOut(2000);
						}
						return;
					}
					var len = data.length,t = newsHtml.newList(data);
					if(param.neworold){
						$("#hot").attr("firstaid",data[0]['id']);
						$(t).insertBefore($('#hot').find('li').eq(0)) ;
					}else{
						$("#hot").attr("lastaid",data[data.length-1]['id']);
						$("#hot").append(t);
					}
				});
			}
		},
		click:{
			nav:function(){//导航栏广告切换
				if($(".erjicaidan") && $(".erjicaidan").length){
					$(".erjicaidan").css({"cursor":"pointer"}).click(function(){
						window.location.href = detail_url+'/'+$(this).attr("id")+'/';
					});
				}
			
			}, 
			li_href:function(){
				$(document).on("tap",".li_click",function(){
					var obj_t = $(this), 
					aid=obj_t.attr("id"),
					sava_arr= [],
					t=new Date().getTime(),
					t_l=1000*60*24*7,//一周数据
					isIn=false,
					loc_aids = JSON.parse(richStorage.get('news.aids'));
					
					obj_t.addClass("active");
					setTimeout(function(){
						obj_t.removeClass("active");
					},200)
					if(aid&&loc_aids){
						for(var i=0;i<loc_aids.length;i++){
							if(loc_aids[i]['aid']&&loc_aids[i]['aid']==aid){
								isIn = true;
								loc_aids[i]['t'] = t;
							}
							if(loc_aids[i]['t']&&(t-parseInt(loc_aids[i]['t']))<t_l){
								sava_arr.push({"aid":loc_aids[i]['aid'],"t":loc_aids[i]['t']}); 
							}
						}
					}
					if(!isIn){
						sava_arr.push({"aid":aid,"t":t}); 
					}
					richStorage.set('news.aids',JSON.stringify(sava_arr));
					if($(this).attr("hf"))window.location.href=$(this).attr("hf");
				}); 
			},
			pullUp:function(){
				$("#pullUp").click(function(){
					newsObj.moreScroll.pullUpAction(0);
					//添加浏览色
					var aids = JSON.parse(richStorage.get('news.aids'));
					for(i in aids){
						$("#"+aids[i]['aid']).find("h2").eq(0).attr("style","color:#999");
					}
				});
				
			},
			refresh:function(){
				$(".icon-refresh").tap(function(){
					 _o = $(this);
					 _o.addClass('on');
					 setTimeout(function(){
					 	_o.removeClass('on');
					 },1000);
					 newsObj.moreScroll.pullUpAction(1);
					 window.scrollTo(0,0);
				});
			},
		},
		moreScroll:{
			pullUpAction:function(neworold) {
				var nid = 1,
				id = "lastaid";
				if(neworold) id = "firstaid" ;
			 	lastaid=$('#hot').attr(id),
				param={id:nid,lastaid:lastaid,od:1,neworold:neworold};
			    newsObj.ajax.listadd(param);	
			    //加载压缩图片
			    $('body').optimizeImg();
			    $('.img img').lazyload({ effect : "fadeIn",threshold : 150 });
			}
			
		} 
	 };
	newsObj.init();
	$(".news-content-box").show();
})(Zepto,url);
</script>
