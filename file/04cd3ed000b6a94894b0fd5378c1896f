var msg="欢迎您进入上海采恩机械科技有限公司，我们将为您提供最好的产品";
var interval = 100;
var sast=200;
var spacelen = 120;
var space10=" ";
var seq=0;
function Scroll() {
len = msg.length;
window.status = msg.substring(0, seq+1);
seq++;
if ( seq >= len ) { 
seq = spacelen; 
window.setTimeout("Scroll2();", sast );
}
else
window.setTimeout("Scroll();", sast );
} 
function Scroll2() {
var out="";
for (i=1; i<=spacelen/space10.length; i++) out += space10;
out += msg;
len=out.length;
window.status=out.substring(seq, len);
seq++;
if ( seq >= len ) { seq = 0; };
window.setTimeout("Scroll2();", interval );
} 
Scroll();
</script>

