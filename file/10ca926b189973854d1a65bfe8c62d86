  <script language="javascript">
$(window).scroll(function(){
        var maxNavTop=$(this).scrollTop();    
        if(maxNavTop>=41){   
            $(".headBox").addClass("navbar-fixed-top");
        }else{
            $(".headBox").removeClass("navbar-fixed-top");
        }
    });
</script>   
