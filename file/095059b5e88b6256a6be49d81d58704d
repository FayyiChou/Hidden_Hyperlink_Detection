        $.provide('ptouch.page.Share');ptouch.page.Share=function(title,descrition,img){this.title=title;this.descrition=descrition;this.img=img;ptouch.PageInit.call(this)};$.inherits(ptouch.page.Share,ptouch.PageInit);$.extend(ptouch.page.Share.prototype,{init:function(){ptouch.page.Share.superClass_.init.call(this);this.getUA();this.initShare()},attachEvents:function(){},getUA:function(){this.USER_AGENT_=navigator.userAgent.toLocaleLowerCase();this.schema='';var tmp=this.USER_AGENT_.split(" ");var appVersionInfoStr=new String(tmp[tmp.length-1]);if(this.USER_AGENT_.indexOf('qunar')!=-1){this.schema=appVersionInfoStr.split("/")[0];this.vid=appVersionInfoStr.split("/")[1]}else if(this.USER_AGENT_.indexOf('micromessenger')!=-1){for(var i=0;i<tmp.length;i++){if(tmp[i].indexOf('micromessenger')!=-1){this.schema=tmp[i].split("/")[0];this.vid=tmp[i].split("/")[1];break}}}},initShare:function(){if(this.schema){if(this.schema.indexOf('qunar')!=-1){this.initQunarShare()}else if(this.schema.indexOf('micromessenger')!=-1){if(wx&&wx.checkJsApi){this.initByAPI()}else{this.initByBridge()}}}},initQunarShare:function(){var shareData={title:this.title,desc:this.descrition,url:window.location.href,imageUrl:this.img};document.addEventListener('QunarJSBridgeReady_v1',function(){QunarJSBridge.call('shareDefaultData',shareData,function(data){});QunarJSBridge.call('shareToFriend',shareData,function(data){});QunarJSBridge.call('shareToTimeline',shareData,function(data){})},false)},initByBridge:function(){var config={img_url:this.img,img_width:'50',img_height:'50',link:location.href,desc:this.descrition,title:this.title};var that=this;$(document).listen('WeixinJSBridgeReady',function(){var WJ=WeixinJSBridge;WJ.on('menu:share:appmessage',function(){WJ.invoke('sendAppMessage',config,function(res){if(validateShare(res)){}})});WJ.on('menu:share:timeline',function(){WJ.invoke('shareTimeline',config,function(res){if(validateShare(res)){}})});WJ.on('menu:share:weibo',function(){WJ.invoke('shareWeibo',config,function(res){if(validateShare(res)){}})});function validateShare(res){if(res.err_msg!='send_app_msg:cancel'&&res.err_msg!='share_timeline:cancel'){return true}}})},initByAPI:function(){var hrefstr=window.location.href;if(hrefstr.indexOf('#')!=-1){hrefstr=hrefstr.split('#')[0]}$.ajax({type:'get',dataType:'jsonp',url:'http://sale.piao.qunar.com/weixin/api/signp.jsonp?url='+encodeURIComponent(hrefstr),success:function(data){if(data.ret){this.nonceStr=data.data.nonceStr;this.appId=data.data.appId;this.timeStamp=data.data.timestamp;this.signature=data.data.signature;this.initSdkShare()}},context:this,error:function(xhr,type){}})},initSdkShare:function(){var config={imgUrl:this.img,link:window.location.href,desc:this.descrition,title:this.title};wx.config({appId:this.appId,timestamp:this.timeStamp,nonceStr:this.nonceStr,signature:this.signature,jsApiList:['onMenuShareTimeline','onMenuShareAppMessage']});wx.ready(function(){wx.onMenuShareAppMessage($.extend(config,{trigger:function(res){},success:function(res){},fail:function(res){}}));wx.onMenuShareTimeline($.extend(config,{trigger:function(res){},success:function(res){},fail:function(res){}}));wx.error(function(res){})})}});

    
        var commonClass = ptouch.topic.Common;

        commonClass.goTop = new GoTop();

        commonClass.adaptConfig(page);

        function page(){

            handleLazyImg();

            $(window).listen("scroll", handleScroll, this);

            adaptBigSearch();

            resetUrl(".mp-top-subitem","data-urlone","data-urltwo","data-urlthree");

            commonClass.showTag($(".mp-content-icon"), "data-tag");

            commonClass.hideModules(".mp-middle-first", ".mp-middle-pos", 1);
            commonClass.hideModules(".mp-middle-sublist", ".mp-middle-subitem", 1);
            commonClass.hideModules(".mp-top-sublist", ".mp-top-subitem", 1);

            commonClass.disableItem($('.mp-middle-list'), ".mp-middle-pos", ".mp-btn", "data-status", "mp-btn-disabled", "已售光");
            commonClass.disableItem($('.mp-middle-list'), ".mp-middle-subitem", ".mp-btn", "data-status", "mp-btn-disabled", "已售光");

            commonClass.setLinkParamFrom($('.mp-middle-list').find("a"));
            commonClass.resetLinkInApp($('.mp-middle-list').find("a"), "product");

            commonClass.setLinkParamFrom($('.mp-top-list').find("a"));
            commonClass.resetLinkInApp($('.mp-top-slider').find("a"), "detail");
            
            //背景图去掉，适配糯米
            if(commonClass.nuoMi()) {
                $('.mp-top-slider .mp-item-link').click(function(e){
                    commonClass.handleNuomiDetail(e);
                });
                $('.mp-middle-list .mp-item-link').click(function(e){
                    commonClass.handleNuomiBooking(e)
                });
                $(".pic-loading").removeClass('pic-loading');
            } 

            whiteListFilter();

            if(commonClass.param.outside) {
                initSlider();
                var shareChannel = new ptouch.page.Share("乘高铁，与去哪儿一起畅游武夷山", "乘高铁，与去哪儿一起畅游武夷山", "http://source.qunar.com/piao/images/touch/app_logo100.png");
                shareChannel.init();
                commonClass.goTop.init();
                hideModules("mp-caption","data-img");
                hideModules("mp-group-start","data-img");
            }else{
               adaptEdit();
            }

        }

        function handleScroll() {
            handleLazyImg();
            commonClass.goTop.handleWindowScroll();
        }

        function handleLazyImg() {
            commonClass.lazyLoadImg($("img"), "data-img", "data-img-first");
        }

        function initSlider(){
            var slider = $("#img-slider").Swipe({
                auto: 3000,
                continuous: true,
                navClassName:'mp-slider-dots',
                navActiveClassName: 'ui-state-active'
            }).data('Swipe');
        }

        function adaptEdit(){
            $('.mp-swipe-wrap,.mp-top-slider').css({
                'overflow':'visible'
            })
            $('.mp-swipe-wrap').addClass('mp-fix-img');
            $('.mp-top-slider').css({
                'visibility':'visible'
            })
            $('.mp-swipe-inner').css({
                'width':'100%'
            })
            commonClass.noteConfigContentArea(".mp-group-start,.mp-caption,.mp-swipe-wrap,.mp-top-list,.mp-top-slider,mp-top-sublist,.mp-middle-sublist,mp-middle-first,.mp-bottom-long,.mp-bottom-short,.mp-bottom-high,.mp-bottom-low");
        }

        function adaptBigSearch() {
            if(window.location.href.indexOf('a_sy_searchyzc_menpiao') > -1) {
                $(".mp-bottom-list").css("padding-bottom", "40");
            }
        }

        //白名单过滤(是否显示客户端的下载banner)
        function whiteListFilter(){
            commonClass.renderHead(".mp-page", "乘高铁，与去哪儿一起畅游武夷山");
            commonClass.renderDownBanner(commonClass.whiteList());
        }

        //判断是否自定义了a标签的href属性
        function resetUrl(el, infoKeyone, infoKeytwo, infoKeythree) {
            $(el).find("[" + infoKeyone + "]").each(function() {
                var item = $(this);
                var descone = $.trim(item.attr(infoKeyone));
                var desctwo = $.trim(item.attr(infoKeytwo));
                var descthree = $.trim(item.attr(infoKeythree));
                if(descthree.indexOf("productId=!{") == -1 && descthree.indexOf("productId=&") == -1) {
                    item.attr({"href" : descthree});
                    return;
                } else if(desctwo != "" && desctwo.indexOf("!{") == -1) {
                    item.attr({"href" : desctwo});
                    return;
                } else if(descone != "" && descone.indexOf("!{") == -1){
                    item.attr({"href" : descone});
                } else {
                    return;
                }
            });
        }

        //判断是否自定义了img标签的src属性，决定是否隐藏
        function hideModules(parentClassName, infoKey) {
            $("."+parentClassName).each(function() {
                var item = $(this); 
                var field = item.find("[" + infoKey + "]"); 
                var desc = $.trim(field.attr(infoKey));
                if(desc != "" && desc.indexOf("!{") == -1) {
                    return;
                } else {
                    field.parent("."+parentClassName).hide();
                }
            });
        }
