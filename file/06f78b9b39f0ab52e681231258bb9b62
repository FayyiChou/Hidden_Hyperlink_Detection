<script>
var bigimg = document.getElementById("bigpic");
var curprev = "http://icon.cheshi-img.com/picture/20110303/pre.cur";
var curnext = "http://icon.cheshi-img.com/picture/20110303/next.cur";
var lastpic = parseInt(0);
var firstpic = parseInt(0);
var prevurl = document.getElementById('prepic').href;
var nexturl = document.getElementById('nexpic').href;

function referURL(url) {
    //var isIe = (document.all) ? true : false;
    var isIe = (navigator.userAgent.indexOf('MSIE') >= 0) && (navigator.userAgent.indexOf('Opera') < 0);
    if (isIe) {
        var linka = document.createElement("a");
        linka.href = url;
        document.body.appendChild(linka);
        linka.click();
    } else {
        window.location = url;
    }
}
function bigImgClick(el) {
    showCursor(el);
    referURL(bigimg.getAttribute("onside") == 0 ? prevurl : nexturl);
}
if (window.addEventListener) {
    bigimg.addEventListener("mousemove", showCursor, false);
    bigimg.addEventListener("click", bigImgClick, false);
} else if (window.attachEvent) {
    bigimg.attachEvent("onmousemove", showCursor);
    bigimg.attachEvent("onclick", bigImgClick);
}
function showCursor(el) {
    e = window.event || el;
    var o = bigimg;
    if (e.clientX < document.body.clientWidth / 2) {
        if (firstpic != 1) {
            if (bigimg.getAttribute("onside") != "0") {
                o.style.cursor = "url(" + curprev + "),pointer";
                o.setAttribute("onside", 0);

            }
        }
    }
    else {
        if (bigimg.getAttribute("onside") != "1") {
            o.style.cursor = "url(" + curnext + "),pointer";
            o.setAttribute("onside", 1);
        }
    }
}
var CS = new Object();
(function(){
	//定义简便的document.getElementById方式
	CS.$ = function(x){
	    var e = document.getElementById(x);
	    return e;
	}
	//读cookie函数		
	CS.getCookie = function(name) {
		var start = document.cookie.indexOf(name + "=");
		var len = start + name.length + 1;
		if ((!start) && (name != document.cookie.substring(0, name.length))) {
			return null;
		}
		if (start == -1) return null;
		var end = document.cookie.indexOf(';', len);
		if (end == -1) end = document.cookie.length;
		return unescape(document.cookie.substring(len, end));
	};
	
	//写cookie函数
	CS.setCookie = function(name, value, expires) {
		var today = new Date();
		today.setTime(today.getTime());
		if (expires) {
			expires = expires * 1000 * 60 * 60 * 24;			
		}
		var expires_date = new Date(today.getTime() + (expires));
		document.cookie = name + '=' + escape(value) +
	        ((expires) ? ';expires=' + expires_date.toGMTString() : '');
	};
})();
//获取cookie值
var ptime = CS.getCookie('ptime_s');
var play = CS.getCookie('play_s');
if(ptime == null) {
	ptime = 5000;
	CS.setCookie('ptime_s',ptime,1);
}
if(play == null){
	play = 'playing';
	CS.setCookie('play_s','playing',1);
}
switch(ptime){  
	case '3000':
		CS.$('third').innerHTML = '<b>3秒</b>';
		CS.$('fifth').innerHTML = '5秒';
		CS.$('eight').innerHTML = '8秒';
		break;
	case '5000':
		CS.$('third').innerHTML = '3秒';
		CS.$('fifth').innerHTML = '<b>5秒</b>';
		CS.$('eight').innerHTML = '8秒';
		
		break;
	case '8000':
		CS.$('third').innerHTML = '3秒';
		CS.$('fifth').innerHTML = '5秒';
		CS.$('eight').innerHTML = '<b>8秒</b>';	
		break;
	default:
		CS.$('third').innerHTML = '3秒';
		CS.$('fifth').innerHTML = '<b>5秒</b>';        	
		CS.$('eight').innerHTML = '8秒';
		break;    	
}		
//按键盘 ← → 键可以翻页
document.onkeydown=chang_page;
function chang_page(e){
	var key = e ? e.which : event.keyCode;
	//向下
	if( key == 39){
		if(CS.$("nexpic").getAttribute('href'))
		location = CS.$("nexpic").getAttribute('href');
	}
	//向上
	if( key == 37){
		if(CS.$("prepic").getAttribute('href'))
		location = CS.$("prepic").getAttribute('href');
	}
}

//自动播放下一页
function go2nextpage(){
	if(CS.$("nexpic").getAttribute('href')){
		window.location.href=CS.$("nexpic").getAttribute('href');
	}else{
		CS.setCookie('play_s','stop',1);
		CS.$("play").setAttribute('class','play_2');
	}
	
}

//播放时间切换
CS.$("third").onclick = function(){
	this.innerHTML = '<b>3秒</b>';
	CS.$('fifth').innerHTML = '5秒';
	CS.$('eight').innerHTML = '8秒';
	CS.setCookie('ptime_s',3000,1);
}
CS.$("fifth").onclick = function(){
	this.innerHTML = '<b>5秒</b>';
	CS.$('third').innerHTML = '3秒';
	CS.$('eight').innerHTML = '8秒';
	CS.setCookie('ptime_s',5000,1);
}
CS.$("eight").onclick = function(){
	this.innerHTML = '<b>8秒</b>';
	CS.$('third').innerHTML = '3秒';
	CS.$('fifth').innerHTML = '5秒';
	CS.setCookie('ptime_s',8000,1);
}
//点击箭头区域到上一页
if(CS.$("up"))
CS.$("up").onclick = function(){
	window.location.href = CS.$("prepic").getAttribute('href');
};		
//点击箭头区域到下一页
if(CS.$("down"))
CS.$("down").onclick = function(){
	window.location.href = CS.$("nexpic").getAttribute('href');
};
	
//播放和暂停
var play_timer;
CS.$("play").onclick = function(){
	if(CS.$("play").className == 'play_1'){
		CS.$("play").className = 'play_2';
		CS.setCookie('play_s','stop',1);
		clearTimeout(play_timer);
	}else{
		CS.$("play").className = 'play_1';
		CS.setCookie('play_s','playing',1);
		play_timer = setTimeout("go2nextpage();",ptime);
	}
}
	
//判断是否播放
if(play=='playing'){
	CS.$("play").className = 'play_1';
	play_timer = setTimeout("go2nextpage();",ptime);
}else{
	CS.$("play").className = 'play_2';
	clearTimeout(play_timer);
}
	
//判断是否到终点
if(!CS.$("nexpic").getAttribute('href')){
	CS.$("down").style.display='none';
}
if(!CS.$("prepic").getAttribute('href')){
	CS.$("up").style.display='none';
}

//判断是否加载完成,调整左右框高度和图片高度自适应
function load(){
	var height = CS.$("bigpic").height;
	if(CS.$('up'))
		CS.$('up').style.height = height+'px';
	if(CS.$('down'))
		CS.$("down").style.height = height+'px';
}
</script>

