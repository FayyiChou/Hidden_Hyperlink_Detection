  <script type="text/javascript">
$(function(){
	$('#detail_more').click(function(){
		if($(this).hasClass('wating'))
			return false;
		$(this).addClass('wating');
		var page = $(this).attr('page'),
			cat = $(this).attr('cat'),
			id = 233632,
			game_id = 2412,
			_this = this;
		if(page <= 0){
			return false;
		}
		$.post('/m/wymobile/ajaxdetail',{'id':id,'game_id':game_id,'page':page,'cat':cat,'limit':5},function(data){
			$(_this).removeClass('wating');
			if(data.html)
				$('#similer_more').append(data.html);
			if(data.page!="end"){
				$(_this).attr('page',Number(page) + 1);
			}else{
				$(_this).attr('page',-1);
				$(_this).html("暂无更多信息");
			}
		},"json");
	});
});
</script> 
