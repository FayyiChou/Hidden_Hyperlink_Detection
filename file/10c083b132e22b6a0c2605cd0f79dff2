<script type="text/javascript">
    function getUserMessagesNumber() {
        var chkResult = "";
        var selfTradeOrderCount = 0;
        try {
            var arr = document.cookie.match(new RegExp("(^| )ASPNETSESSIONGUID=([^;]*)(;|$)"));
            if (arr == null || arr == "") {
                chkResult = "";
            }
            else {
                $.ajax({ url: "/chkLogin.ashx?ID=0", type: "get", async: false, data: null, success: function (msg) { chkResult = msg; } });
                $.ajax({ url: "/chkLogin.ashx?ID=1", type: "get", async: false, data: null, success: function (msg) { selfTradeOrderCount = parseInt(msg); } });
            }
            if (chkResult != "") {
                var name = chkResult.split("|")[0];
                var number = chkResult.split("|")[1];
                var cls = "msgslide";
                if (!parseInt(number) > 0) {
                    number = 0;
                    cls = "msgststic";
                }
                var html = "<p class='welogin'>您好，<a style=\" color:#0066CC;\" href=\"http://user.uu898.com\" title=\"快速进入我的UU898\" target=\"_blank\">" + name + "</a> ";
				html +=' <span class="Blue"><a class="loginout" href="http://user.uu898.com/LogOut.aspx?returnUrl=' + location.href + '" >[退出]</a></span>'
                html += "<a class =\"" + cls + "\" href ='http://user.uu898.com/MyMessage.aspx' >您3天内有" + number + "条未读消息</a>";
                html += '</p>'
                //if (selfTradeOrderCount > 0) {
                //    html += "<a style=\"margin-left:10px; color:red\" href ='http://user.uu898.com/myescortorder.aspx?g=2' >" + selfTradeOrderCount + "个待发货订单</a>";
                //}
                document.getElementById("newWelcome").innerHTML = html;
                document.getElementById("newWelcome").className = "welogin";
            }
            else {
                var html = '<p>您好，欢迎来到UU898！'
                    + '<span class="Blue"><a href="http://user.uu898.com/login.aspx?returnUrl=' + location.href + '">请登录</a></span>或'
                    + '<span class="Blue"><a href="http://user.uu898.com/reg.aspx" class="reg" target="_blank">免费注册</a>'
                    + '<a onclick="setBackUrl(true);QQLogin();return false;" target="_blank"><img src="http://s.img898.com/www/png/images/xzyx_01.jpg"/></a></span</p>';
                document.getElementById("newWelcome").innerHTML = html;
                document.getElementById("newWelcome").className = "we";
            }
        }
        catch (er) { }
    }
    function QQLogin() {
        var href = "https://graph.qq.com/oauth2.0/authorize?response_type=code&client_id=1102096350&scope=get_user_info&redirect_uri=http%3A%2F%2Fuser.uu898.com%2Flogin%2FpartnerLogin%3Ftype%3Dqq";
        var state = Math.round(Math.random() * 10000);
        window.open(href + "&state=" + state);
        return false;
    }
    function setBackUrl(tz) {
        document.cookie = "backUrl=" + location.href + ";domain=uu898.com;path=/";
        if (!tz) {
            location.href = "http://user.uu898.com/login.aspx";
        }
    }
    function setBackUrlParent(url) {
        document.cookie = "backUrl=" + url + ";domain=uu898.com;path=/";
    }
    getUserMessagesNumber();
</script>
