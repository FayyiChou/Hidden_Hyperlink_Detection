<script type="text/javascript">
    hasLoad = false;
    $(".mod_cate_hd_con").mouseover(function () {
        $("#SNmenuNav").show();
        if (hasLoad == false) {
            $.ajax({
                type: "GET",
                url: "/Include/CategoryMenu.ashx",
                data: "",
                beforeSend: function () {
                    //$("#SNmenuNav").html("正在加载中...");
                },
                success: function (msg) {
                    $("#SNmenuNav").append(msg);
                    var head = document.getElementsByTagName('head')[0];
                    var script = document.getElementById('scrHead');
                    if (script != null) {
                        script.parentNode.removeChild(script);
                    }
                    var d = new Date();
                    var times = d.getTime();
                    script = document.createElement('script');
                    script.type = 'text/javascript';
                    script.src = "http://img1.allinhui.com/WebResources/JLNLS/JS/Shopping/headBtm.js?" + times;
                    script.id = 'script_id';
                    head.appendChild(script);
                    hasLoad = true;
                },
                error: function (err) {
                    $("#SNmenuNav").html("商品分类加载失败..." + err);
                }
            });
        }
    });
</script>
