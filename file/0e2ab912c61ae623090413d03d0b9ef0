<script type="text/javascript" language="javascript">

    function removeHTMLTags() {
        if (document.getElementById && document.getElementById("PageHead1_TextBox1")) {
            var strInputCode = document.getElementById("PageHead1_TextBox1").value;
            /*
                This line is optional, it replaces escaped brackets with real ones,
                i.e. &lt; is replaced with < and &gt; is replaced with>
            */
            strInputCode = strInputCode.replace(/&(lt|gt);/g, function (strMatch, p1) {
                return (p1 == "lt") ? "<" : ">";
            });
            var strTagStrippedText = strInputCode.replace(/<\/?[^>]+(>|$)/g, "");
            //alert("Input code:\n" + strInputCode + "\n\nOutput text:\n" + strTagStrippedText);

            document.getElementById("PageHead1_TextBox1").value = strTagStrippedText;
            return true;
        }
    }
</script>
