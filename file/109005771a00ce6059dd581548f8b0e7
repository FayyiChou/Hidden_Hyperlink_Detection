<script type="text/javascript">
$(function() {
	var newsCon = "";
	var rightCon = "";
	$(".newsColumnCon").each(function() {
		newsCon += $(this).html();
		$(this).remove();
	});
	
	$(".rightColumnCon").each(function() {
		rightCon += $(this).html();
		$(this).remove();
	});
	
	if(newsCon != "") {
		$(".Wrapmian .Wrapmleft").prepend(newsCon);
	}
	
	if(rightCon != "") {
		$(".Wrapmian .Wrapmright").append(rightCon);
	}
});
</script>

