    <script type="text/javascript">
       function clickNav(url,sort){ 
            $.ajax({ url:"/navsort.htm?navsort="+sort, async:false ,success: function(date){
                }
            }); 
            //window.location.href="/"+url;
           if(url.indexOf("ttp://")!=-1){
               window.open(url);
           }else if(url.indexOf("ttps://")!=-1) {
               window.open(url);
           }else{
               window.open("/"+url);
           }
        }
        </script>
