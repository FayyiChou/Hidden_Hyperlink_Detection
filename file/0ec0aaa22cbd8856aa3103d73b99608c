function AddEmotion(editor, emotion)
{	
	var edt = document.getElementById(editor);
    if(edt)
    {
		if (edt.value == "字数上限为2000字")
		{
			edt.value = "";
		}
	    edt.value += emotion;
    }
    if(typeof(AfterAddEmotion)=="function")
    {
        AfterAddEmotion();
    }
}

DisplayEmotions("EmotionsDiv", "NoHtmlCommentContent");</body></html>