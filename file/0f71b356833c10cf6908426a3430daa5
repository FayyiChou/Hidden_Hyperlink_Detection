  var speed  = 0;//滚动速度
  var rows  = 100;//每行高度
  var stim  = 100; //停留时间倍数 * speed
  var stop  = 0; //初始化值，不管
  demo6.innerHTML = demo5.innerHTML
  function NewPostMarquee(){
    if(demo9.scrollTop%rows==0 && stop<=stim){
      stop++;
      return;
    }
    stop = 0;
    if(demo6.offsetTop-demo9.scrollTop<=0)
      demo9.scrollTop-=demo5.offsetHeight
    else{
      demo9.scrollTop++
    }
  }
  var NewPostMar    = setInterval(NewPostMarquee,speed)
  demo9.onmouseover = function() {clearInterval(NewPostMar)}
  demo9.onmouseout  = function() {NewPostMar=setInterval(NewPostMarquee,speed)}
-->
</SCRIPT>
      
