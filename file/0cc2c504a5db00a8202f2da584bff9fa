     	<script type="text/javascript">
        $(function(){
			var i=0;
			$("#main1R ul:eq("+0+")").css("display","block")
			$("#rightTop a").hover(function(){
				i=$(this).index("#rightTop a")
				if(i==0){
					return false
				}
				z=i-1
				$("#main1R ul").css("display","none")
				$("#main1R ul:eq("+z+")").css("display","block")
				$("#rightTop a").removeClass("selected")
				clearInterval(tim1);
			},function(){
				i=$(this).index("#rightTop a")
				if(i==0){
					return false
				}
				$(this).addClass("selected")
				tim1=setInterval(newsqh,5000);
			});
			
			function newsqh(){
				i++;
				if(i>=$("#main1R ul").length){i=0;}
				$("#rightTop a:not('.gmore')").removeClass("selected");
				$("#rightTop a:not('.gmore')").eq(i).addClass("selected");
				
				$("#main1R ul").hide();
				$("#main1R ul").eq(i).show();
				
			}
			var tim1=setInterval(newsqh,5000);
			
		})

        </script>
