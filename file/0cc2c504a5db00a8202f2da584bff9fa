<script type="text/javascript">
        var l=0;
		function newqh(){
			
			l++;
			if(l>4){l=0;}
			$(".subtit").each(function(index){
				$(".subtit").eq(index).find("li").eq(l).siblings("li").removeClass("on");
				$(".subtit").eq(index).find("li").eq(l).addClass("on");	
				var ind1=$(".subtit").eq(index).find("li").eq(l).index();
				$(".subtit").eq(index).find("li").eq(l).parent("ul").parent(".subtit").siblings(".indexnews").find("ul").hide();
				$(".subtit").eq(index).find("li").eq(l).parent("ul").parent(".subtit").siblings(".indexnews").find("ul").eq(ind1).show();
			});
		}
		

$(function(){
	setInterval(newqh,4000);
	$(".indexnews").each(function(){$(this).find("ul:first").show();});
	$(".subtit").each(function(){$(this).find("li:first").addClass("on");});
	$(".subtit ul li").mousemove(function(){
		$(this).siblings("li").removeClass("on");
		$(this).addClass("on");	
		var ind1=$(this).index();
		$(this).parent("ul").parent(".subtit").siblings(".indexnews").find("ul").hide();
		$(this).parent("ul").parent(".subtit").siblings(".indexnews").find("ul").eq(ind1).show();
	});
	
	$(".biaotit").each(function(){$(this).find("li:first").addClass("on");});
	$(".biaopic").each(function(){$(this).find("li:first").show();});
	$(".biaotit ul li").mouseover(function(){
		$(this).siblings("li").removeClass("on");
		$(this).addClass("on");
		var ind2=$(this).index();
		$(this).parent("ul").parent(".biaotit").siblings(".biaopic").find("li").hide();
		$(this).parent("ul").parent(".biaotit").siblings(".biaopic").find("li").eq(ind2).show();	
	});
	
	$(".gdbox1").jCarouselLite({  
     auto: 500, 
	 visible: 6, 
	 btnPrev: ".btnl1",   
     btnNext: ".btnr1",  
    speed: 1000 
    }); 
	$(".gdbox2").jCarouselLite({  
     auto: 500, 
	 visible: 6, 
	 btnPrev: ".btnl2",   
     btnNext: ".btnr2",  
    speed: 1000 
    }); 
	$(".nwcont1_box1").jCarouselLite({  
     auto: 4000, 
	 visible: 1,  
    speed: 500,
	vertical: true 
    }); 
});
</script>
<script type="text/javascript">
         function qhimg(obj,tim,gtim){
			$(obj).css({width:$(obj).find("img:first").css("width"),height:$(obj).find("img:first").css("height")});
			$(obj).children("dl").css("position","relative").find("dt").css("position","absolute");	
			$(obj).find("dt").hide();
			$(obj).find("dt:first").show();
			var lg=$(obj).find("dt").length;
			var li=0;
			setInterval(function(){li++;if(li>=lg){li=0;}$(obj).find("dt:visible").fadeOut();$(obj).find("dt").eq(li).fadeIn(gtim);},tim); 
		}
		$(function(){
			qhimg(".nwbanner_r",3000,500);
			qhimg(".biao1",3000,500);
			qhimg(".biao2",3000,500);
			qhimg(".biao3",3000,500);
			qhimg(".biao4",3000,500);
			qhimg(".biao5",3000,500);	
		});
        </script>
