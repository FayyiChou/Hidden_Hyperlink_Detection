        $(function(){
			$("#jtw51_menu .nav ul li").hover(function(){
				$("#jtw51_menu .nav ul li").removeClass("on");
				$(this).addClass("on");
				var ind="#jtw51_menu .subnav"+($(this).index()+1);
				$("#jtw51_menu .subnav ul").hide();
				$(ind).css("margin-left",120*$(this).index()+"px");
				if($(ind).find("li").length<3){
					$(ind).css("margin-left",135*$(this).index()+"px");	
				}
				$(ind).show();
			});	
		});
        </script>

