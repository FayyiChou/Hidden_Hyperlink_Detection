			 <script type="text/javascript">
	function changNav(num){
		//alert("come on!");
		for( var id=1; id<=9; id++){
			if( num == id){
				document.getElementById("nav"+id).style.display = "block";
			}else{
				document.getElementById("nav"+id).style.display = "none";	
			}
		}
	}
</script>
