function tt_relaart( bdata )
{
	var str = '';
	for(i=0; i<bdata.length; i++)
	{
		str += '<div class="news-item-img">';
		str += ' <a href="'+bdata[i].link+'">';
		str += ' <span class="img-wp"> <img src="'+bdata[i].pic+'"> </span>';
		str += ' <span class="title">'+bdata[i].title+'</span>';
		str += ' <span class="news-item-info"> <span>百度头条</span> </span>';
		str += ' </a>';
		str += ' </div>';
		
		if(i==19) break;
	}
	Pub.$('related_news').innerHTML = str;
}
