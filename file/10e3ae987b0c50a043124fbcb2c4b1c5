function tt_relatu( bdata )
{
	var str = '';
	for(i=0; i<bdata.length; i++)
	{		
		str += '<a href="'+bdata[i].link+'">';
		str += '<span class="img"> <img src="'+bdata[i].pic+'"> </span>';
		str += '<span class="title">'+bdata[i].title+'</span>';
		str += '</a>';
		
		if(i==2) break;
	}
	Pub.$('related_pic').innerHTML = str;
}

