<script>
		// JavaScript Document
//==========PC导航栏图标==========
$(".navimg").hover(
	function(){
		$(this).find(".hidden").fadeIn();
		$(".seachm").hide();
		$("#pcseach").hide();
		},
	function(){
		$(this).find(".hidden").hide();
})
	
$(".navseach").click(
	function(){
		$(this).find(".hidden").fadeToggle("fast");
		$("#pcseach").fadeToggle("fast");
})

$(".navwx").hover(
	function(){
		$(this).find(".hidden").fadeIn();
		$(".seachm").hide();
		$("#pcseach").hide();
		},
	function(){
		$(this).find(".hidden").hide();
})

//==========PC导航栏上划线事件==========
$(function(){
$(".navbar li").ready(function(){
	$(".navbar .nav_bottom").css("width",$(".navbar li").width())
	
	var index = $("#navindex").val(); 	//当前是第几个li的			
	var navbar = $(".navbar");
    var navli = $(".navbar li").eq(parseInt(index)+1);
    var navbottom = $(".navbar .nav_bottom");
										
    navbottom.css({					//上划线相对ul距离
        "width":$(".navbar li").width(),
		"left": $(".navbar li").position().left
    });
    $(".navbar li").hover(function() {//hover事件
            navbottom.stop().animate({
                "left": $(this).position().left
            },
            350);			
        },
    function() {
        navbottom.stop().animate({
            "left": $(".navbar li").position().left
        },
        200);
    });
})
});

$(window).resize(function(){
	$(".navbar .nav_bottom").css("width",$(".navbar li").width())
	});


//==========PC导航栏下拉==========

$(document).ready(function(e) {
    $(".pcheader ul li").hover(function(){
		var _height = $(this).find('.pcdl').height();
		$(this).parent().find(".pcdl").hide();
		$(this).find(".pcdl").stop().slideDown("slow");
		if( parseInt(_height) - 30 > 0 ){
			$('.pcheader').css('height','150px')
		}
	},function(){
		$(this).find(".pcdl").stop().slideUp();
		$('.pcheader').css('height','120px')
		})
});


$(".bb").click(function(){
	$(".wapnavbar").show();
	$(".wapnavbar").animate({right:'0px'},300);
	})
$(".wapclose,section").click(function(){
	$(".wapnavbar").animate({right:'-70%'},300);
	$(".wapnavbar").hide(1);
	})
//==========wap导航子菜单==========

	
$(document).ready(function(){
$(".wapline").click(function(){
		var _this = $('.wapdl',this);
		$(this).parent().find('dl').slideUp("fast");
		$(this).parent().find("span").removeClass("wap_down");
		if($(_this).is(':hidden')){
			$(this).find("span").addClass("wap_down");
			$(_this).slideDown("fast");
		}else{
			$(this).find("span").removeClass("wap_down");
			$(_this).slideUp();
		}
	}
)
})

