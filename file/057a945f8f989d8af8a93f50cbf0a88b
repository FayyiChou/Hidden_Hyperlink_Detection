 <script type="text/javascript">
$(function(){
	var _widthw = $(window).width();
	if( _widthw > 1500 ){
		var _width = 150;
	}else{
		var _width = 93;
	}
	var _length = $('.nav-bannerWrapper > ul > li').length;
	var _index = $('.nav-bannerWrapper > ul > li').find('.a_active').parent('li').index() + 1;
	
	if(_index > 6){
		var aaWidth = - (_index - 6) * _width + 'px' ;
	}else{
		var aaWidth = '0px';
	}
	//初始状态
	$('.nav-bannerWrapper').css({
		width: _length * _width + 'px',
		left: aaWidth
	});
	//左
	$('.bannerNext-left').on('click',function(){
		var _left = parseInt($('.nav-bannerWrapper').css('left'));
		if(_left < 0){
			$('.nav-bannerWrapper').animate({left: parseInt(_left) + _width + 'px'},100)
		}
	})
	//右
	$('.bannerNext-right').on('click',function(){
		var _right = parseInt($('.nav-bannerWrapper').css('left')) ;
		if( - _right + 6 * _width -_length * _width < 0){
			$('.nav-bannerWrapper').animate({left: parseInt(_right) - _width + 'px'},100)
		}
	})
	
})
function qqClose(){
	$('.qqService').remove();
}
</script>
