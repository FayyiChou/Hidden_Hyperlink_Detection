<script>

	function changego(value){

		 $.ajax({
		        type:'GET',
		        data:'',
		        url:"/school/cheangegoindex/nation/"+value,
		        success:function(data){
		            if(data!='fail'){
		            	//window.location.href="/school/cheangego/nation/"+value;
		            	if(value=='TOP'){
		            		window.location.reload();

		                }else{
		                	location.href = "/school/cheangego/nation/"+value;

			           }

