<script>
	function select(nation){
		$('#nation').val(nation);
	}

	function bsearch(num){
		if(num=='1'){
			var naname=$('#naname').val();
		}else{
			var naname=$('#naname1').val();
		}
		str='';
		if(naname!='' &&  naname!='海外院校搜索' && naname!='请输入大学名称，如：Harvard University'){
			var str =str+"/naname/"+naname;
		}

		location.href = "/school/index"+str;

	}


	function school_name_search(){

		var naname=$('#school_name_v').val();
		str='';
		if(naname!='' && naname!='海外院校搜索'){
			var str =str+"/naname/"+naname;
		}
		location.href = "/school/index"+str;
	}



	 function lookup(inputString,num) {

		  if(inputString.length == 0) {
		   // Hide the suggestion box.
		   $('#autoSuggestionsList').hide();
		  } else {

		   $.post("/school/searchboo", {queryString: inputString}, function(data){
				    if(data.length >0) {
//				     $('#autoSuggestionsList').show();
//				     $('#autoSuggestionsList').html(data);

					    if(num=='1'){
					    	$('#autoSuggestionsList').show();
						    $('#autoSuggestionsList').html(data);
						    $('#take').val('naname');
						}else{
							$('#autoSuggestionsList1').show();
						    $('#autoSuggestionsList1').html(data);
						    $('#take').val('naname1');
						}
				    }
			   });
		  }
	 } // lookup

//	 function fill(thisValue) {
//	  $('#naname').val(thisValue);
//	  setTimeout("$('#autoSuggestionsList').hide();", 200);
//	 }


	 function fill(thisValue) {

			var take= $('#take').val();
			if(take=='naname'){
				$('#naname').val(thisValue);
				setTimeout("$('#autoSuggestionsList').hide();", 200);
			}else{
				$('#naname1').val(thisValue);
				setTimeout("$('#autoSuggestionsList1').hide();", 200);
			}

		 }
	 function clicktext(){

			var str = $("#naname").val();
			if(str == "请输入大学名称，如：Harvard University"){
//				$("#naname").val("");
			}

	 }

	function blurinput(str){
		if(str=="index"){
		    setTimeout("$('#autoSuggestionsList').hide();", 200);

		}else if(str=="fav"){
		    setTimeout("$('#autoSuggestionsList_fav').hide();", 200);
		}
	}


	function entersearch(){
	    //alert(dd);
	   var event = window.event || arguments.callee.caller.arguments[0];
	   if (event.keyCode == 13)
	   {
		   bsearch();
	   }
	}

</script>



