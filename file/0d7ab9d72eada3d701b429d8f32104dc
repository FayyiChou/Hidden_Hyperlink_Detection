	function changeinputtype(p){
		p.value='';
		p.type='password';

	}
function login(){
	var uname=$("#uname").val();
	var password=$("#password").val();
	 $.ajax({
	        type:'GET',
	        data:'',
	        url:"/member/Login/uname/"+uname+"/password/"+password,
	        success:function(data){
	            if(data!='unlogin'){
	            	$("#loginpage").hide();
	            	$(".popmask").hide();
	            	$("#loguser").html(" ");

	            	$("#logusername").show();

	            	window.location.reload();

 	            	$("#global_form").val("1");
					$("#ajaxusername").html(data);

		        }else{
					alert("账号或密码错误");
			    }
	        },
	        dataType:'text',
	        error:function (){
	            alert("失败！");
	        }
	    })
}

function enterlogin(){
    //alert(dd);
   var event = window.event || arguments.callee.caller.arguments[0];
   if (event.keyCode == 13)
   {
	   login();
   }
}



function isub(){
	var name=$("#iname").val();
	var uemail=$("#iemail").val();
	var uphone=$("#iphone").val();
	var upassworda=$("#ipassworda").val();
	var password=$("#ipassword").val();

	if(name==""){
		alert("昵称不能为空");
		return false;
	}
	if(!checkiemail()){
		alert("邮箱/手机不正确，请重新输入");
		return false;
	}
// 	if(!checkiphone()){
// 		alert("手机号码格式不正确");
// 		return false;
// 	}

	if(!checkipassword()){
		alert("密码格式不正确");
		return false;
	}
	if(!checkipassworda()){
		alert("确认密码格式不正确");
		return false;
	}


	 $.ajax({
	        type:'GET',
	        data:'',
	        url:"/member/ajaxcreate/uname/"+uname+"/password/"+password+"/uemail/"+uemail+"/uphone/"+uphone+"/upassworda/"+upassworda+"/name/"+name,
	        success:function(data){
 	            if(data=='ok'){
		        	$("#loginpage").hide();
	            	$(".popmask").hide();

//$("#loguser").html("<dl class='usermenu relpos'><dt><span class='message'></span><span></span></dt><div class='usermenudrop hd'><p><a href='/centre/index'>个人中心</a></p><p><a href='javascript:viod(0)' onclick='loginout()'>注销用户</a></p></div></dl>");
	            	window.location.reload();
 		        }
 		        if(data=='no'){
 		        	alert("失败！");
 	 		     }
  		       if(data=='have'){
		        	alert("用户已经存在！");
	 		     }
	 		     if(data=='password'){
		 		    	alert("请正确填写密码！");

		 		}
	        },
	        dataType:'text',
	        error:function (){
	            alert("失败！");
	        }
	    })
}



function checkiemail(){
	 var reg = /^(\w-*\.*)+@(\w-?)+(\.\w{2,})+$/;

	 var iemail=$("#iemail").val();
	if(iemail=='' || !reg.test(iemail)){


		return false;
	}
	return true;
}

function checkiphone(){
	var reg= /^[1][358]\d{9}$/;
	var iphone=$("#iphone").val();
	if(iphone=='' || !reg.test(iphone)){


		return false;
	}
	return true;
}


function checkipassword(){
	var ipassword=$("#ipassword").val();
	if(ipassword=='' || ipassword.length<6 || ipassword.length>16){

		return false;
	}
	return true;
}


function checkipassworda(){

	var ipassword=$("#ipassword").val();
	var ipassworda=$("#ipassworda").val();

	if(ipassworda=='' || ipassword!=ipassworda){


		return false;
	}
	return true;

}










</SCRIPT>
