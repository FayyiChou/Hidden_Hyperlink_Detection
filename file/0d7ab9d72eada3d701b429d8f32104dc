<script>

function changeschools(){
	 $.ajax({
	        type:'POST',
	        url:"/site/changeschools",
	        data:{id:"2"},
	        async : false,
	        success:function(data){
			    if(data!='fail'){
			    	$("#index_schools").html(data);
		        }
	        },
	        dataType:'text',
	        error:function (){                
	            alert("失败！");
	        }
	    })

	
}


</script>


