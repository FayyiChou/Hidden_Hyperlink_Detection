$(function(){
	var TransObj=new transPart();
	$("a.yaunfen").bind("click",function(){
		TransObj.showMenu(5,0);
	});

	var phtm;
	$("a.pianhao").unbind().bind("mouseenter",function(){
		$(".pianhaoBox").stop().fadeIn("fast").unbind().bind("mouseleave",function(){
			$(".pianhaoBox").unbind().stop().fadeOut("fast");
		}).bind("mouseenter",function(){
			if(phtm){
				clearTimeout(phtm);
				phtm=null;
			}
		});
	}).bind("mouseleave",function(){
		phtm=setTimeout(function(){
			$(".pianhaoBox").stop().fadeOut("fast");
		},2000);
	})
	$(".reportstep").eq(0).show();
	$(".reportstep").each(function(id){
		$(this).find(".submitbox input").attr("rel",id);
	})
	$(".reportstep").find(".submitbox input").bind("click",changeStep);
})

function changeStep(){
	if($(this).hasClass("prebut")){
		var index=parseInt($(this).attr("rel"));
		$(".reportstep").hide().eq(index-1).show();
	}else if($(this).hasClass("nextbut")){
		var index=parseInt($(this).attr("rel"));
		$(".reportstep").hide().eq(index+1).show();
	}
	//alert(index)
}

function changeAct(selecter){
	var val=$(selecter).val();
	$(".act,.sat").hide();
	$("."+val).show();
}
function setupSearchBar(){
	$(".schoolkey >ul li").bind("mouseenter",function(){
		var index=$(this).index();
		$(this).parent().find(".nowid").removeClass("nowid");
		$(this).addClass("nowid");
		var keybox=$(".schoolkey .keybox");
		keybox.stop().hide()
		keybox.eq(index).stop().fadeIn("normal")

	})
}
var popwindow;
$(function(){
	$(".formtitle").bind("click",showForms);
	$("#submitebut").bind("click",function(){
		popwindow=new popoutWindow($("#dingweipop"));
		popwindow.popOut();
	})
	$("#closePopBut").bind("click",function(){
		if(popwindow){
			popwindow.destroy();
			popwindow=null;
		}
	})
})

function showForms(){
	var id=$(".formtitle").index($(this));
	$(".checklist").removeClass("open");
	if($(this).hasClass("open")){
		$(this).removeClass("open");
	}else{
		$(".formtitle").removeClass("open");
		$(this).addClass("open");
		$(".checklist[rel="+id+"]").addClass("open");//.hide().slideDown("fast");
		$.scrollTo($(this).offset().top-50,500);
	}
}



function externallinks() {

    if (!document.getElementsByTagName) return;

    var anchors = document.getElementsByTagName("a");

    for (var i = 0; i < anchors.length; i++) {

        var anchor = anchors[i];

        if (anchor.getAttribute("href") &&

			anchor.getAttribute("rel") == "external")

            anchor.target = "_blank";

    }

}

window.onload = externallinks;










