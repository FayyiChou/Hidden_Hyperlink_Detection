										 var _demo2 = document.getElementById("demo2");
										 demo2.innerHTML=demo1.innerHTML
										 function Marquee(){
										  if(_demo2.offsetTop-_demo.scrollTop<=0){
										   _demo.scrollTop-=_demo1.offsetHeight
										  }else{
										   _demo.scrollTop++
										  }
										 }
										 var MyMar=setInterval(Marquee,speed)
