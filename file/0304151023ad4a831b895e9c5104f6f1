							<script>
								var speed=90
								divRightToLeft2.innerHTML=divRightToLeft1.innerHTML
								function Marquee(){
								if(divRightToLeft2.offsetWidth-divRightToLeft.scrollLeft<=0)
									divRightToLeft.scrollLeft-=divRightToLeft1.offsetWidth
									else{
										divRightToLeft.scrollLeft += 2
									}
								}
								var MyMar=setInterval(Marquee,speed)
								divRightToLeft.onmouseover=function() {clearInterval(MyMar)}
								divRightToLeft.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
							</script>
