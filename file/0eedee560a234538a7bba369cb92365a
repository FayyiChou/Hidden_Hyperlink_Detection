        /*------判断radio是否有选中，获取选中的值--------*/
        $(function () {
            $("input[name=rdoka]").click(function () {
                showCont();
            });
      /*以下为新增代码*/
       showCont();
        $("input[class=subbtn]").click(function () {
                var kaval = $('input:radio[name="rdoka"]:checked').val();
                if (kaval == null) {
                    alert("请选择其中一种卡类型!");
                    return false;
                }
            });

        });
       

        function checkradio(categoryID) {
            var valka = $('input:radio[name="rdoka"]:checked').val();
            if (valka == null) {
                alert("请选择其中一种卡类型!");
                return false;
            } else {
                if (categoryID == 4) { //如果是淘宝太阳卡就只有普卡
                    $("#Type").val(1);
                } else {
                    $("#Type").val(valka);
                }
                var theform = document.getElementById('theform');
                theform.submit();

            }
        }
    </script> 



