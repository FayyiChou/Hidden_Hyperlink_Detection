        ////        var keyWord = encodeURI($("#keyWord").val());
        ////        url += "&keyType=" + keyType + "&keyword=" + data;
        //        //保存搜索的关键词
        ////        $.ajax({
        ////            type: "post",
        ////            url: "Ashx/saveKeyWord.ashx",
        ////            data: "q=" + data + "",
        ////            success: function (data) {
        ////                returnPage("Q/StationListBeta.aspx?" + url + "");
        ////            }
        ////        })


    });




    var cookieName = "searchHistory";
    function topSearch() {
        if (document.getElementById("txtSearch").value != "") {

            addKeyRecord(document.getElementById("txtSearch").value);

            setHistoryCookie(cookieName, { "txt": document.getElementById("txtSearch").value, "link": "keyType=0&keyword=" + document.getElementById("txtSearch").value });
            window.location.href = "/Q/StationList.aspx?keyType=0&keyword=" + document.getElementById("txtSearch").value;

        }
        else {
            window.location.href = "/Q/StationList.aspx";
        }
    }



    function setHistoryCookie(cookieName, value) {
        var oldValue = getCookie(cookieName);
        var newValue = "";
        var newValueArr = new Array();
        newValueArr.push(JSON.stringify(value));


        if (oldValue != "" && oldValue != null) {
            var historyList = JSON.parse("[" + oldValue + "]");
            for (var i = 0; i < historyList.length && i < 5; i++) {
                if (historyList[i].txt != value.txt) {
                    newValueArr.push(JSON.stringify(historyList[i]));
                }
            }
        }

        SetCookie(cookieName, newValueArr.join(","));
    }

    showHistoryList();

    function showHistoryList() {
        var historyList = JSON.parse("[" + getCookie(cookieName) + "]");  // [{ "txt": "会计", "link": "keyType=0&keyword=会计" }, { "txt": "", "link": "" }, { "txt": "", "link": "" }, { "txt": "", "link": "" }, { "txt": "", "link": ""}];
        if (historyList.length > 0 && historyList[0] != null) {

            for (var i = 0; i < historyList.length && i < 5 && historyList[i] != null; i++) {
                if (historyList[i].txt != "" && historyList[i].link != "") {
                    document.getElementById("historyList").innerHTML += "<p><a href='/Q/StationList.aspx?" + historyList[i].link + "'>" + historyList[i].txt + "</a></p>";
                    document.getElementById("nohistorydata").style.display = "none";
                    document.getElementById("clearhistorydata").style.display = "block";
                }
            }
        }
        else {
            document.getElementById("nohistorydata").style.display = "block";
            document.getElementById("clearhistorydata").style.display = "none";
        }

    }


    function clearHistory() {
        deleteCookie(cookieName);
        document.getElementById("nohistorydata").style.display = "block";
        document.getElementById("historyList").style.display = "none";
        document.getElementById("clearhistorydata").style.display = "none";
    }



    var searchRecord = "searchRecord";

    var yourItemS = getCookie(searchRecord);



    if (yourItemS == null || yourItemS == "") {
        yourItemS = { 
        "key0": [],
            "key1": [],
            "area": [],
            "main": [],
            "trade": [],
            "year": [],
            "edu": [],
            "edu": [],
            "pay": [],
            "type": [],
            "page": [1]
        }
    } else {
        yourItemS = JSON.parse(yourItemS);
    }


    //alert(JSON.stringify(yourItemS))


    function addItem(arr, val, len) {
        if (val != "") {
            var add = true;
            for (var i = 0; i < arr.length; i++) {
                if (arr[i] == val) {
                    add = false;
                }
            }
            if (add) {
                arr.unshift(val);
                if (arr.length > 3) {
                    arr.length = 3;
                }
            }
        }
    }

    function addKeyRecord(val) {

        addItem(yourItemS.key0, val, 3);
        SetCookie(searchRecord, JSON.stringify(yourItemS));
    }

</script>

    <a class="goto-top" id="gototop" href="javascript:go2Top();" style="display: none;" title="返回顶部"></a>
    <script src="Js/pageScroll.js" type="text/javascript"></script>
    <script type="text/javascript">

        var pageindex = 2;
        var end = false;


        $(window).scroll(function () {
            if (isScrollEnd()) {
                getList();
            }
        }
)


        var searchRecord = "searchRecord";

        var defaultItem = { "edu": ">=05", "pay": ">=07", "date": "3", "page": [1] }

        var yourItem = getCookie(searchRecord);

        //        if (yourItem == null || yourItem == "") {
        //            yourItem = defaultItem;

        //        } else {
        //            yourItem = JSON.parse(yourItem);
        //        }

        if (yourItem != null && yourItem != "") {
            yourItem = JSON.parse(yourItem);
            if (typeof (yourItem) != "object") {
                yourItem = defaultItem;
            }
        } else {
            yourItem = defaultItem;
        }


        function getList() {
            if (end) {
                return false;
            }
            if (yourItem.page > 5) {
                $("#loadMore").html("<a href='javascript:loadMore()'>查看更多职位</a>")
                return false;
            }
            for (var i in yourItem) {
                if (yourItem[i].length > 0 && typeof (yourItem[i]) == "object") {
                    yourItem[i] = yourItem[i].join(" ");
                }
            }

            // alert(JSON.stringify(yourItem))

            $.ajax({
                type: "post",
                url: "Q/ashx/getStationList.ashx",
                datatype: "json",
                data: yourItem, //location.search.replace("?", "") + "&page=" + pageindex + "&t=" + (new Date()).getTime(),
                beforeSend: function () {
                    $("#loadMore a").html("数据加载中");
                },
                success: function (data) {
                    //alert(data);
                    if (data == "NULL" || data == "-1" || data == "") {
                        $("#loadMore a").html("没有更多数据");

                        end = true;
                        if (yourItem.page == 1) {
                            yourItem = defaultItem;

                            getList();
                            deleteCookie(searchRecord);
                            end = false;
                        }
                    } else {
                        document.getElementById("kxList").innerHTML += data;
                        yourItem.page++;
                        $("#loadMore a").html("点击加载更多");
                    }
                },
                error: function () {
                    $("#loadMore a").html("数据加载错误");
                }
            });



        }

        var scrolled = false;
        var obj = document.getElementById("gototop");
        window.onscroll = function () {
            if (!scrolled) {
                getList();
                scrolled = true;
            }

            var x = (document.body.scrollTop || document.documentElement.scrollTop);
            if (x == 0) { obj.style.display = "none"; } else { obj.style.display = "block"; };
        };

        function go2Top() {
            document.documentElement.scrollTop = document.body.scrollTop = 0;
        }


        var screenHeight = getClientHeight();

        if (screenHeight > 700) {
            if (!scrolled) {
                getList();
                scrolled = true;
            }
        }


        function loadMore() {
            var urlArr = new Array();
            for (var i in yourItem) {
                if (yourItem[i] != "" && i != "page") {
                    urlArr.push(i + "=" + yourItem[i]);
                }
            }
            window.location.href = "Q/StationList.aspx?" + urlArr.join("&");
        }


    </script>
    <script src="Js/baidutongji.js" type="text/javascript"></script>


    </form>





</body></html>