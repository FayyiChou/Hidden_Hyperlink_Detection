var path="eng";

function Title_over(code1)
{
  if(code1=="1"){
    MM_swapImage('ImageT1','','images/header/'+path+'/index_title02_over.gif',1);
    MM_swapImage('ImageT2','','images/header/'+path+'/index_title03.gif',1);
    hideIt("Layer8");
    Layer7.style.display = "block";}
  else{
    MM_swapImage('ImageT1','','images/header/'+path+'/index_title02.gif',1);
    MM_swapImage('ImageT2','','images/header/'+path+'/index_title03_over.gif',1);
    hideIt("Layer7");
    Layer8.style.display = "block";
  }
}

$(function()
{
	var a = $("#slider").slideshow({
		width      : 1000,
		height     : 231,
		transition : 'fade',
		navigation : false,
		timer : false,
		control : false,
		delay: 5000
	});

	$(".jquery-slider-selector-span").on("mouseenter", function()
	{
		$(this).click();
		a.startTimer();
	});
});

