bgColor:$("#bgColor-img6").html(),
isCrisis:$("#isCrisis-img6").html()
}






    },							
    // set of images to be used
    // Initialis the carousel
    init_carousel:function(){
		isTouchSupported = 'ontouchstart' in window;
		startEvent = isTouchSupported ? 'touchstart' : 'click';		
		// Load all the images
		cts.manuLife.carousel.load_images(cts.manuLife.carousel.images);
		
$('.imageBGImagesList .imageBGimg').imgpreload({
			all: function()	{
				$('.preloader').hide();
				$('.buttonSection').show();
				cts.manuLife.carousel.load_thumbnails(cts.manuLife.carousel.images);
				//cts.manuLife.carousel.carousel_setOn('img'+ (Math.floor(Math.random() * 5) + 1));
cts.manuLife.carousel.carousel_setOn(cts.manuLife.carousel.initImg);
				cts.manuLife.carousel.assign_click_on_thumb();
				if(!isTouchSupported) {
					cts.manuLife.carousel.assign_hover_on_thumb();
				}
			}
		});
    },
    // Load the thumbnails
    load_images:function(img){


		jQuery.each(img,function(index,data){
			
			 if(data.isCrisis!='' && randomImgOn) {
			 	cts.manuLife.carousel.initImg=index;
			 	randomImgOn = false;
             }
$('.imageBGImagesList').append('<a href="'+data.bgButtonLink+'" id="bgImageLink'+index+'" style="display:none;"><img class="imageBGimg" id="bgImage'+index+'" src="'+data.mainImg+'" alt="'+data.thumbAltText+'"/></a>');

		});
      

    },
    load_thumbnails:function(img){
		jQuery.each(img,function(index,data){
			// Creating the li item from the List
			
$('.carousel_slider ul').append('<li><div class="image_item" id="'+index+'"><div class="thumbs"><img src="'+data.thumbImg+'" /></div><div class="progress"><div class="progress_bar"></div></div><div class="msgBox">'+data.thumbAltText+'</div></div></li>');
		});
    },
    // Deactivate any slide 
    carousel_setOff:function(id){
		// Push down the thumbnail image
		
$('#bgImage'+id).animate({
			opacity: '0',
			avoidTransforms:true, 
			leaveTransforms:true
		},cts.manuLife.carousel.animationDelay);

$('#bgImageLink'+id).hide();
$('.carousel_slider #'+id).parent().animate({
			top: '0px',
			avoidTransforms:true, 
			leaveTransforms:true
		},cts.manuLife.carousel.animationDelay);
		
$('#'+id+' .progress_bar').stop();
		
$('#'+id+' .progress_bar').css('width','0%');
		
$('.carousel_holder .imageBGMessageBox').css('opacity','0');
    },
    // Activate any slide 
    carousel_setOn:function(id){
		// At the first time of the loading
		if(cts.manuLife.carousel.currentItem != ''){
			cts.manuLife.carousel.carousel_setOff(cts.manuLife.carousel.currentItem);
		}
		// Set the current Item
		cts.manuLife.carousel.currentItem = id;
		// Pop up the thumbnail image
		
$('.carousel_slider #'+id).parent().animate({
			top: '-22px',
			avoidTransforms:true, 
			leaveTransforms:true
		},cts.manuLife.carousel.animationDelay, function() {
			// Fade out the current Background Image		
			$('.carousel_holder .imageBGMessageBox .heading').html(cts.manuLife.carousel.images[id].bgHeading);
			$('.carousel_holder .imageBGMessageBox .leftMsg').html(cts.manuLife.carousel.images[id].bgText);
			if(cts.manuLife.carousel.images[id].bgSmallImgPath ==''){
				$('.carousel_holder .imageBGMessageBox .rightMsgImgID').hide(0);
			}
			else {
				$('.carousel_holder .imageBGMessageBox .rightMsgImgID').show(0).attr('src',cts.manuLife.carousel.images[id].bgSmallImgPath);
			}

	
              if (cts.manuLife.carousel.images[id].bgButtonText=="")
              {
				$('.buttonSection').hide();
              }else{
				$('.buttonSection').show();
			$('.carousel_holder .imageBGMessageBox .buttonSection').html(cts.manuLife.carousel.images[id].bgButtonText);
			
			$('.carousel_holder .imageBGMessageBox .buttonSection').attr('href', cts.manuLife.carousel.images[id].bgButtonLink);
			$('.carousel_holder .imageBGMessageBox .buttonSection').attr('target', cts.manuLife.carousel.images[id].bgButtonLinkTarget);
			$('.carousel_holder .imageBGMessageBox ').attr('alt', cts.manuLife.carousel.images[id].thumbAltText);

};
			
			$('.carousel_holder .imageBGMessageBox .link').attr('href', cts.manuLife.carousel.images[id].bgButtonLink);
			$('.carousel_holder .imageBGMessageBox .link').attr('target', cts.manuLife.carousel.images[id].bgButtonLinkTarget);

			var nextID = (
			$('#'+id).parent().next().length == 0)?'img1':
			$('#'+id).parent().next().find('.image_item').attr('id');
			// Change the current Background Image
			
			$('.carousel_holder .imageBGMessageBox').animate({
				opacity:'1',
				avoidTransforms:true, 
				leaveTransforms:true
			},cts.manuLife.carousel.animationDelay );
			// Fade in the current Background Image
			if(cts.manuLife.carousel.images[id].bgColor != 'none'){
				$('.carousel_holder .imageBGMessageBox').addClass('opaqueBG');
			} else {
				$('.carousel_holder .imageBGMessageBox').removeClass('opaqueBG');
			}
			$('#bgImage'+id).animate({
				opacity: '1',
				avoidTransforms:true, leaveTransforms:true
			},cts.manuLife.carousel.animationDelay,function(){
				// Animate the progress bar
				$('#bgImageLink'+id).show();			
				$('#'+id+' .progress_bar').animate({
					width: '100%',
					avoidTransforms:true, leaveTransforms:true
				},cts.manuLife.carousel.slideDelay,"linear",function(){
					cts.manuLife.carousel.carousel_setOn(nextID);
				});	
			});	
		});
    },
    // Assign the functionality on hover on any slide 
    assign_hover_on_thumb:function(){
		$('.carousel_slider .image_item').hover(function(){
			// On hover in pop up the image
			var id = $(this).attr('id');
			if(id != cts.manuLife.carousel.currentItem){
				$(this).parent().animate({
					top: '-22px',
					avoidTransforms:true, leaveTransforms:true
				},cts.manuLife.carousel.animationDelay);	
			}
			$(this).find('.msgBox').fadeIn(100);
		},function(){
			// On hover out push down the image
			var id = $(this).attr('id');
			if(id != cts.manuLife.carousel.currentItem){
				$(this).parent().animate({
					top: '0px',
					avoidTransforms:true, leaveTransforms:true
				},cts.manuLife.carousel.animationDelay);
			}
			$(this).find('.msgBox').fadeOut(100);
		});
    },
    // Assign the functionality on Click on any slide 
    assign_click_on_thumb:function(){
		$('.carousel_slider .image_item').on(startEvent, function(){
			// Load the cuurent item being clicked
			if($(this).attr('id') != cts.manuLife.carousel.currentItem){
				cts.manuLife.carousel.carousel_setOn($(this).attr('id'));
			}
		});
    }	
};
</SCRIPT> 

</div>  

    <!--幻灯结束-->
  
