                        <SCRIPT type="text/javascript">
/* Done by Shubhanjan 363453 */
var cts = {};
var randomImg='img'+ (Math.floor(Math.random() * 1) + 1);
var randomImgOn= true;

cts.manuLife = {};
cts.manuLife.carousel = {
    currentItem		: '', 		// to track the current slide
    slideDelay		: 5000,	// delay between imahe change
    animationDelay	: 200,		// delay for the hover and fade in fade out animation
    initImg         : randomImg,
images			: {




'img1' : {
mainImg :$("#mainImg-img1").html(), 
thumbImg :$("#thumbImg-img1").html(), 
bgHeading:$("#bgHeading-img1").html(),
bgText:$("#bgText-img1").html(),
bgButtonText:$("#bgButtonText-img1").html(),
bgButtonLink:$("#bgButtonLink-img1").html(),
bgButtonLinkTarget:$("#bgButtonLinkTarget-img1").html(),
bgSmallImgPath:'',
thumbAltText:$("#thumbAltText-img1").html(),
bgColor:$("#bgColor-img1").html(),
