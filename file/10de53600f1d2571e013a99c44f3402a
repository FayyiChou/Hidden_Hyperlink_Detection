<script>
    function Search()
    {
        var key=document.getElementById("txtKey");
        if(key.value==""  || key.value=="搜索")
        {
            alert("请填写您要搜索的内容 ！");
            key.focus();
            return;
        }
        window.open('http://manulife-sinochem.1tiao.net/manusearch/index.html?search='+ escape(key.value),'_block');
    }
</script>

