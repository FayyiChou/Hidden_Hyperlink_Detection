        var thePlayer;  //保存当前播放器以便操作
        $(function() {
            thePlayer = jwplayer('videoplay').setup({
                flashplayer: '/jwplayer/jwplayer.flash.swf',
                'file': 'http://www.minfangshikong.cn/uploadfile/video/ftmfw.mp4',
