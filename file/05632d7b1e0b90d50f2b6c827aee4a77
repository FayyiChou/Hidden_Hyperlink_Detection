                width: 405,
                height: 255,
                dock: false
            });
            
            $('.video-small a').click(function(){
                $('.video-small a').removeClass('active');
                $(this).addClass('active');
                var vurl=$(this).attr('vurl');
                var iurl=$(this).attr('iurl');
                thePlayer.load([{file:vurl,image:iurl}]);
                $('#vtitle').html($(this).attr('title'));
            });
            //播放 暂停
            $('.player-play').click(function() {
                if (thePlayer.getState() != 'PLAYING') {
                    thePlayer.play(true);
                    this.value = '暂停';
                } else {
                    thePlayer.play(false);
                    this.value = '播放';
                }
            });
            
            //停止
            $('.player-stop').click(function() { thePlayer.stop(); });

        });
    
