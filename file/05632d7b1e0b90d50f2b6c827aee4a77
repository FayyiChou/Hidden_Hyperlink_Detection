			<script type="text/javascript">
		$(document).ready(function(){

			var	my_jPlayer = $("#jquery_jplayer");
			var	opt_play_first = false, 
				opt_auto_play = true, 
				opt_text_playing = "Now playing",
				opt_text_selected = "Track selected"; 
			my_jPlayer.jPlayer({
				ready: function () {
					$("#jp_container .track-default").click();
				},
				
				play: function(event) {
					
				},
				pause: function(event) {
					
				},
				ended: function(event) {
					
				},
				loop: true,
				swfPath: "http://www.bjftmf.gov.cn/statics/js/",
				supplied: "mp3",
				wmode: "window"
			});
			$(".mmusic span").on('click',function(e) {
				if($(this).hasClass('active')){
					if($(this).hasClass('play')){
						my_jPlayer.jPlayer("pause");
						$(this).removeClass('play');
						$(this).addClass('pause');
					}else{
						my_jPlayer.jPlayer('play');
						$(this).removeClass('pause');
						$(this).addClass('play');
					}
				}else{
					$('.mmusic span').removeClass('active');
					$('.mmusic span').removeClass('play');
					my_jPlayer.jPlayer("setMedia", {
						mp3: $(this).attr("href")
					});
					my_jPlayer.jPlayer("play");
					$(this).addClass('active');
					$(this).addClass('play');
				}
				
				return false;
			});

			
		});
		</script>
