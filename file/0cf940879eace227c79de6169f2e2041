<script>
$( document ).ready(function() {
setInterval(function(){var gok = $(".reveal-modal-bg").css("display"); $(".vsf").val(gok); }, 100);
});
function formaValidate(url){
		$.ajax({
			url: url, 
			type: "POST",
			data: {vsf:$('#forma_feedback input[name=vsf]').val(),f_send:$('#forma_feedback input[name=f_send]').val(),f_name:$('#forma_feedback input[name=f_name]').val(),f_fame:$('#forma_feedback input[name=f_fame]').val(),f_org:$('#forma_feedback input[name=f_org]').val(),f_occ:$('#forma_feedback input[name=f_occ]').val(),f_dev:$('#forma_feedback select[name=f_dev]').val(),f_city:$('#forma_feedback select[name=f_city]').val(),f_city2:$('#forma_feedback input[name=f_city2]').val(),f_phone:$('#forma_feedback input[name=f_phone]').val(),f_email:$('#forma_feedback input[name=f_email]').val(),f_text:$('#forma_feedback textarea[name=f_text]').val(),form_submit:$('#forma_feedback input[name=form_submit]').val()},
			success: function(data){
				
				var error = $(data).find('.errors').html();
				//var ok = $(data).find('#forma_feedback .ok-mess').html();
				console.log(error);
				if(error !== null)
					$('.errors').html(error);
				else{
					$('#feedback').hide();
					$('.errors').html("");
					$('img.gfeed').hide();
					$('.ok-mess').html('Ваше сообщение было успешно отправлено!');
				}
					
					
			}
		
		});
	}
</script>

