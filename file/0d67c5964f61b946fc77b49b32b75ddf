        <div class="fjiia-middle-box-top"><span class="spanTitle">CIA/CCSA考试成绩查询</span></div>
        <div class="fjiia-middle-correlation-list">
        <!--CIA/CCSA考试成绩查询 开始-->
			<script type="text/javascript">
            function  tosubmit(){
                var loginName = document.getElementById("loginName").value;
                var cardType = document.getElementById("cardType").value;
                var userCard = document.getElementById("userCard").value;
                var zkzh = document.getElementById("zkzh").value;
                var zjlxtr = document.getElementById('zjlx_tr');
                var zjhmtr = document.getElementById('zjhm_tr');
                var zkzhtr = document.getElementById('zkzh_tr');
                if(loginName==""){
                    alert("请填写考生姓名!");return false;
                }
            
                if(zjlxtr.style.display==''&&cardType==""){
                    alert("请填写证件类型!");return false;
                }
                if(zjhmtr.style.display==''&&userCard==""){
                    alert("请填写证件号码!");return false;
                }
                if(zkzhtr.style.display==''&&zkzh==""){
                    alert("请填写准考证号!");return false;
                }
            }
            
                String.prototype.TrimAll   =   function()  
                {  
                    return   this.replace(/\s+/g ,   "");  
                }  
                function StringTrimAll(obj)
                {
                    obj.value=obj.value.TrimAll();
                }
                
                function changeValue(){
                    var numtype1 = document.getElementById('zjtype');
                    var numtype2 = document.getElementById('kztype');
                    var zjlxtr = document.getElementById('zjlx_tr');
                    var zjhmtr = document.getElementById('zjhm_tr');
                    var zkzhtr = document.getElementById('zkzh_tr');
                    if(numtype1.checked==true){
                         
                        document.getElementById("zkzh").value='';
                        zjlxtr.style.display="";
                        zjhmtr.style.display="";
                        zkzhtr.style.display="none";
                    }
                    if(numtype2.checked==true){
                        document.getElementById("cardType").value='';
                        document.getElementById("userCard").value='';
                        zjlxtr.style.display="none";
                        zjhmtr.style.display="none";
                        zkzhtr.style.display="";
                    }
                    
                }
            
            </script>
              <form name="search" method="post" action="http://59.151.61.116:8081/nsxh_kaoshi/search.do?method=searchKsCjInfo" onsubmit="return tosubmit();">
            
            <table width="100%" border="0" cellpadding="0" cellspacing="0">
              <tr>
                <td width="80">&nbsp;</td>
                <td><input name="numtype" id="zjtype" value="0" type="radio" checked="checked" onClick=" changeValue();"/>证件号码
                    <input name="numtype" id="kztype" value="1" type="radio" onClick=" changeValue();"/>准考证 </td>
              </tr>

              <tr>
                <td align="right">考生姓名：</td>
                <td align="left"><input name="loginName" type="text" id="loginName" onBlur="StringTrimAll(this);" size="15"/></td>
              </tr>
              <tr id="zjlx_tr">
                <td align="right">证件类型：</td>
                <td align="left"><select name="cardType" id="cardType">
                    <option value="">---请选择---</option>
                    <option value="1">身份证</option>
                    <option value="2">护照</option>
                    <option value="3">军(警)官证</option>
                    <option value="4">台胞证</option>
                    <option value="36">其它</option>
                  </select></td>
              </tr>
              <tr id="zjhm_tr">
                <td align="right">证件号：</td>
                <td align="left"><input name="userCard" type="text" id="userCard"  onblur="StringTrimAll(this);" size="18"/></td>
              </tr>
              <tr id="zkzh_tr" style="display: none">
                 <td align="right">准考证号：</td>
                 <td align="left"><input name="zkzh" type="text" id="zkzh"  onblur="StringTrimAll(this);" size="18"/></td>
              </tr>
            
            
              <tr>
                <td >&nbsp;</td>
                
