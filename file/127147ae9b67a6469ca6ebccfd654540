 function get_xszq_Credits(id)
 {
     var theEvent = window.event || arguments.callee.caller.arguments[0];
     var left = theEvent.clientX;
     var y = theEvent.clientY;
     var sctop = $(this).scrollTop();
     var top = y + sctop;
     var uid ='0';
     var creditsUrl = '/freshman/getcredit';
     if (uid < 1) {
         return show_login();
     } else
     {

         $.ajax({
             type: "POST",
             url: creditsUrl,
             data: 'id=' + id,
             success: function (result) {
                 if (result > 0)
                 {
                     var id = "divPopp" + left + "_" + top;
                     var $tip = $('<div id="' + id + '" style="display: none; position: absolute;z-index: 9999;"><img src="/images/ub+.png" style="vertical-align: top;"/><span style="font-size: 18px;font-weight: bolder;line-height: 30px;margin-left: 5px;">' + result + '</span></div>');
                     $('body').append($tip);
                     $('#' + id).attr("style", "top:" + top + "px" + ";left:" + left + "px" + ";position: absolute;color:#f60;");
                     $('#' + id).show();
                     $('#' + id).animate({
                         "opacity": "hide",
                         "top": 0
                     }, 2000, function () {
                         $(this).remove();
                     });

                     $("#sure"+id).removeClass("sure_done");
                     $("#sure"+id).addClass("have_taked");
                     $("#sure"+id).html("已完成");
                     $("#sure"+id).removeAttr("onclick");
                 }
                 else
                 {
                     window.location.reload();
                 }
             }
         });
     }
 }
    $('#backtop').click(function(){
        $('html,body').animate({scrollTop:$('#top').offset().top},500);
    });

    //滑动导航
      jQuery(window).bind("scroll",function(){
  	
        var offb=$('#backtop').offset().top;  	 
        var j_scroll = 0;
        
        if(offb>$('#m1').offset().top&&offb<$('#m2').offset().top)
        {
            j_scroll=1; 

        }
        if(offb>$('#m2').offset().top&&offb<$('#m3').offset().top)
        {
            j_scroll=2; 
        }
        if(offb>$('#m3').offset().top&&offb<$('#m4').offset().top)
        {  			
            j_scroll=3;
        }
        if(offb>$('#m4').offset().top)
        {  			
            j_scroll=4; 
        }
          $('.fnav a').removeClass('fnav_hov').eq(j_scroll).addClass('fnav_hov');
    })  

//教程左右切换
var index =0;
var tab_a =$('.jc_m ul li');

function show_jc()
{
	show_center();show_c('jc');
	tab_a.eq(0).show().siblings().hide();
	index = 0;
}

 $(".next").click(function(){
	index+=1; 
	if(index>7)	index =7; 
	if(index<8)
	{
		tab_a.eq(index).show().siblings().hide();
	}
 });
  $(".pre").click(function(){ 
	index-=1; 
	if(index<1) index=0; 
	tab_a.eq(index).show().siblings().hide(); 
 });

    var slideHeight =410;
    var defHeight = $('.list_wrap').height();
    if(defHeight >= slideHeight){
        $('.list_wrap').css('height' , slideHeight + 'px');
        $('.read_more').click(function(){
            $('.read_more').toggleClass("read_more_active");
            var curHeight = $('.list_wrap').height();
            if(curHeight == slideHeight){
                $('.list_wrap').animate({
                    height: defHeight
                }, "normal");
                $('.read_more').text('点击隐藏任务');
            }else{
                $('.list_wrap').animate({
                    height: slideHeight
                }, "normal");
                $('.read_more').text('点击展开更多任务');
            }
            return false;
        });
    };


</script>
    

