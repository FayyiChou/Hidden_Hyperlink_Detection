function GetQuickAsk(id){
	if($("#li_"+id).attr("class")=="show_1"){
		$(".list li").attr("class","show_1");		
		$("#li_"+id).attr("class",'show_2');		
		document.getElementById("answer_"+id).focus();	
		$("#quickaskbt_"+id).attr("class","quickaskbt_1");
		}
		else if($("#li_"+id).attr("class")=="show_2"){
		$("#li_"+id).attr('class','show_1');
		$("#quickaskbt_"+id).attr("class","quickaskbt");		
			}

	}

	
 function SendAnswer(id){
	 contentstr=$("#answer_"+id).val();
	 if(contentstr==""){
		 $.dialog.alert("回复内容不能为空.");
		 return false;
		 }
	if($("#mid").val()==0){
		dologin();
		return false;
		}
		$("#li_"+id).attr("class","show_1");
		$("#answer_"+id).val('');
	$.post("post.php",{'action':'answer','content':contentstr,'id':id,'isajax':'1'},function(data){
		if(data.indexOf("成功提交")>-1){
				$(".tipShadow").eq(0).show();
				$(".tipShadow").eq(0).css({"top":$("#li_"+id).offset().top,"left":$("#li_"+id).offset().left+300});
				$(".tipShadow").eq(0).fadeOut(3000);
				}
		})
	
	 }
   function dologin(){
			   $.dialog.load('/data/js/login.php',{'id':'art1','title':'登 录','lock':true},false)
			   }
	$(document).ready(
	  function(){
		   $(".pname").load("/data/js/cityname.php");
		   $.get("/data/js/cityname.php",{"tag":"getlink"},function(data){
			   $("#psite").attr("href",data);
			   });
		  }
	);			   
</script> 
