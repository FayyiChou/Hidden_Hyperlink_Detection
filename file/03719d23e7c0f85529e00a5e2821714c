        function checkLanguage() {
            var lan = navigator.language;
            if (!lan) {//ie
                lan = navigator.browserLanguage;
            }
            if (lan.indexOf('zh') == -1) {
                location.href = 'index.html';
                return false;
            }
            return true;
        }
        if (checkLanguage()) {
            if (location.href.indexOf("from") == -1) {
                var ua = navigator.userAgent.toLowerCase();
                if (ua.indexOf("android") > -1
                    || ua.indexOf("iphone") > -1
                    || ua.indexOf("ipod") > -1
                    || ua.indexOf("ipad") > -1) {
                    window.location.href = ("http://m.700bike.com.cn/news/index");
                }
            }
        }
