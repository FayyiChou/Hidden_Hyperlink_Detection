			$('#float_block').addClass('abs');
		}
		if(vTopfooter-$(window).scrollTop()>=$("#float_block").height()){
			$('#float_block').removeClass('abs');
			$('#float_block').addClass('float_fix');
		}
	  } 
	  else
	  {
		$('#float_block').removeClass('float_fix');
		
	  }	
})
})
</script> -->
