        <script type="text/javascript" src="http://code.jquery.com/jquery-1.8.3.min.js" ></script>
        <script type="text/javascript" src="http://code.jquery.com/ui/1.10.3/jquery-ui.min.js" ></script>
        <script type="text/javascript">
            function Privacy() {
                $("#Privacy").show();
                $("#Default").hide();
            }
            function Register(btn) {
                $(".Register").animate({ height: 'show', opacity: 'show' }, 'slow');
                $(".Login").hide();
                $(".Register_title").hide();
                $(".Login_title").show();
            }
            function Login() {
                $(".Register").hide();
                $(".Login").animate({ height: 'show', opacity: 'show' }, 'slow');
                $(".Register_title").animate({ height: 'show', opacity: 'show' }, 'slow');
                $(".Login_title").hide();
            }

            //註冊登入框，輸入Email帳號後出現下面的框
            $(document).ready(function() {
                var MailData = ["gmail.com", "hotmail.com", "msn.com", "yahoo.com", "yahoo.com.tw", "pchome.com.tw"];
                $("#ctl00_txtRegEmail").autocomplete({
                    autoFocus: true,
                    source: function(request, response) {
                        var KeyValue = request.term,
                        index = KeyValue.indexOf("@"),
                        Address = KeyValue,
                        host = "",
                        result = [];

                        //result.push(KeyValue);
                        if (index > -1) {
                            Address = KeyValue.slice(0, index);
                            host = KeyValue.slice(index + 1);
                        }
                        if (Address) {
                            var findedHosts = (host ? $.grep(MailData, function(value) {
                                return value.indexOf(host) > -1;
                            }) : MailData),
                        findedResults = $.map(findedHosts, function(value) {
                            return Address + "@" + value;
                        });
                            result = result.concat($.makeArray(findedResults));
                        }
                        response(result);
                    }
                });

                $("#ctl00_txtLoginEmail").autocomplete({
                    autoFocus: true,
                    source: function(request, response) {
                        var KeyValue = request.term,
                        index = KeyValue.indexOf("@"),
                        Address = KeyValue,
                        host = "",
                        result = [];

                        //result.push(KeyValue);
                        if (index > -1) {
                            Address = KeyValue.slice(0, index);
                            host = KeyValue.slice(index + 1);
                        }
                        if (Address) {
                            var findedHosts = (host ? $.grep(MailData, function(value) {
                                return value.indexOf(host) > -1;
                            }) : MailData),
                        findedResults = $.map(findedHosts, function(value) {
                            return Address + "@" + value;
                        });
                            result = result.concat($.makeArray(findedResults));
                        }
                        response(result);
                    }
                });
            });

        </script>
