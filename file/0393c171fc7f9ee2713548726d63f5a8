            function fbRegistration() {
              if (document.getElementById("ctl00_chkPrivacyisRead").checked == true)
              {
                FB.login(function(response) {
                    if (response.authResponse) {
                        //accessToken = response.authResponse.accessToken;
                        FB.api('/me', function(response) {
			                          uid = response.id;
			                          email = response.email;
			                          var form = document.createElement("form");
			                          form.setAttribute("method", 'post');
			                          form.setAttribute("action", 'StaffRegistration.aspx?UserID=' + email + '&UID=FB_' + uid);

			                          document.body.appendChild(form);
			                          form.submit();
			            });
                    } else {  alert('您需要同意使用DoSurvey才能使用FaceBook登入');  }
                }, { scope: 'email' });
              }
              else
              {
                  document.getElementById("ctl00_labelRegErr").textContent = "請閱讀隱私權保護政策";
              }
            };

            function ChkPrivacy() {
                if (document.getElementById("ctl00_chkPrivacyisRead").checked == true) {
                    return true;
                }
                else {
                    document.getElementById("ctl00_labelRegErr").textContent = "請閱讀隱私權保護政策";
                    return false;
                }
            }

            function fbLogin() {
                FB.login(function(response) {
                    if (response.authResponse) {
                        
                        FB.api('/me', function(response) {
			                          uid = response.id;
			                          email = response.email;
			                          //alert(uid);
			                          var form = document.createElement("form");
			                          form.setAttribute("method", 'post');
			                          form.setAttribute("action", 'StaffSignIn.aspx?UserID=' + email + '&UID=FB_' + uid);

			                          document.body.appendChild(form);
			                          form.submit();
			                          //return false;
			            });
                        
                        /*
