//选中当前导航
(function ($) {
var urlArr = location.href.split ('/');
var lastpath = urlArr.pop ();
var lastbutonepath = urlArr.pop ();
var navArr = ['index','?m=photo','category-0.html|article','wenda|question','?m=category|category-','?m=article&a=rss','app'];
for(var i=0;i<navArr.length ;i++ ){
var temp = navArr[i].split ('|');
for(var n=0;n < temp.length ;n++ ){
if(lastpath.indexOf(temp[n]) == 0){
$('#nav li').removeClass('current').eq(i).addClass('current');
return;
}
if(i==4){
var reg1 = /category-([1-9][0-9]+).html/;
var reg2 = /category-([0-9]+)-([0-9]+).html/;
if(reg1.test (lastpath)){
$('#nav li').removeClass('current').eq(4).addClass('current');
return;
}
if(reg2.test (lastpath)){
$('#nav li').removeClass('current').eq(4).addClass('current');
return;
}
}
}
}
if(lastbutonepath.indexOf('wendatag') == 0){
$('#nav li').removeClass('current').eq(3).addClass('current');
return;
}
$('#nav li').removeClass('current').eq(0).addClass('current');

setTimeout(function(){
if($("#banner_ad").size() && $("#banner_ad img").height() ){
$("#banner_ad").height( $("#banner_ad img").height() );
}
}, 1000);
})(jQuery);

