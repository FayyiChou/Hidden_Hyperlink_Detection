  <script type="text/javascript">
var curparm = '';

function pagegoo(url) {
	$.ajax({
		url: url,
                 jsonpCallback: 'ajaxCallback1',
                error:function(XMLHttpRequest, textStatus, errorThrown){alert(textStatus)},
		success: function(r) {
			if (r.ajaxtxt == '') {
                               
				$("#contents").html('<li class="kong">没有搜索到您想要的结果，请尝试简化您的搜索关键词</li>')
			} else {
				$("#contents").html(r.ajaxtxt)
			}
			$("#pages").html(r.pages);
			$("#pagesx").html(r.pagesx);
			$("#counts").html(r.c);
			if (r.tiaojian != undefined && r.tiaojian != null) {
				curparm = '';
				for (var i in r.tiaojian) {
					if (r.tiaojian[i] != '') {
						curparm += i + "-" + r.tiaojian[i] + "-"
					}
				}
			}
			if (r.cates != undefined && r.cates != null) {
				var arr = new Array();
				arr[0] = '<a href="javascript:void(0);" class="current" data="0"><span>全部</span></a>';
				var x = 1;
				for (var i in r.cates) {
					if (r.cates[i] != null) {
						arr[x] = "<a href='javascript:void(0)' data='" + r.cates[i]['list_id'] + "'><span>" + r.cates[i]['list_name'] + "</span></a>";
						x++
					}
				}
				str = arr.join('<em>|</em>');
				$("#catecont").html(str);
				$("#catecont a").click(function(e) {
					curparm= $(this).attr('data');
					$("#catecont a").removeClass('current');
					$(this).addClass('current');
                                       
					pagegoo("/e/ffcode/4usky_searchclass.php?kw="+escape('杰森・雷特')+"&cid=" + curparm)
				})
			}
			$('.list-page a,.short-page a').click(function(e) {
				pagegoo($(this).attr('href'));
				return false
			})
		},
		dataType: 'jsonp',
		timeout: 20000
		//error: function() {
			//$("#contents").html('<br />服务器繁忙，请再试试...')
		//}
	})
}
$("#catecont a").click(function(e) {
	curparm = $(this).attr('data');
	$("#catecont a").removeClass('current');
	$(this).addClass('current');
	pagegoo("/e/ffcode/4usky_searchclass.php?kw="+escape('杰森・雷特')+"&cid=" + curparm)
})
//window.onerror = function() {
	//return true
//}
</script>  
