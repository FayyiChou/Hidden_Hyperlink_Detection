<script language="javascript">
//栏目切换
function changeColumn(thisid){
	var spanTitle=document.getElementById(thisid).childNodes;
	 for (var i=0;i<spanTitle.length;i++){
		spanTitle[i].onmousemove=function(){
			for (var k=0;k<spanTitle.length;k++){
				spanTitle[k].style.backgroundColor='#AAD0F0';
			}
			this.style.backgroundColor='#FFFFFF';
			var spanContentid="spanContent"+right(thisid,1);
			var spanContent=document.getElementById(spanContentid).childNodes;
			for (var j=0; j<spanContent.length;j++){spanContent[j].style.display='none';}
			spanContent[this.id].style.display='block';
		}
	 }

}
function right(mainStr,lngLen) { 
	if (mainStr.length-lngLen>=0 && mainStr.length>=0 && mainStr.length-lngLen<=mainStr.length) { 
		return mainStr.substring(mainStr.length-lngLen,mainStr.length);
	} 
	else{return null;} 
} 
//栏目名链接
function goto(id){
	window.location.href ='goto.asp?topicid='+id;
}
</script>