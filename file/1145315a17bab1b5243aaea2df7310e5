<script language="javascript">

function optionClass(){
	this.tname=null;
	this.create=function(){
	for(var i=0; i<6; i++){
		this.tname[i].onmouseover=function(){
		this.style.backgroundImage="url(img/menu_bg.jpg)";
		this.style.backgroundRepeat="repeat";
		this.firstChild.style.lineHeight="21px";
		this.firstChild.style.color="#4f95ed";
		}
		this.tname[i].onmouseout=function(){
		this.style.backgroundImage='';
		this.firstChild.style.color="#ffffff";
		}
	}
	}
}
function onloads(){
  var opt=new optionClass();
  opt.tname=document.getElementsByName("topic_root");
  opt.create();
}
onloads();
</script></th>
