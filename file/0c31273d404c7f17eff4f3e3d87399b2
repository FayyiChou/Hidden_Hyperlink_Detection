<script language='javascript'>
$(function(){
$('.ewm_small img').click(function(){
$('.ewm_small').hide();
$('.ewm').show();
});

$('.ewm').click(function(){
$('.ewm_small').show();
$('.ewm').hide();
});
$('.ewm_small').css('background-color', $('.ewm div').css('background-color'));
$('.ewm_small').css('color', $('.ewm div').css('color'));
var ewmtop = $('.layout-nav').offset().top+50;
floatResize();
$('.index-left-nav').click(function(){
  window.scroll(0, 0);
})
floatScroll();

window.onscroll = function(){
floatScroll();
}

window.onresize = function(){
floatResize();
}

$('.frame_bottom_center').click(function(){
window.scroll(0, 0);
})

function floatScroll(){
var ts = $(document).scrollTop();
if(ts>ewmtop){
$('.index-left-nav').css('display','');
//$('.ewm').css('top','40px');
//$('.ewm_small').css('top','40px');
}else{
var tt = ewmtop-ts;
$('.index-left-nav').css('display','none');
//$('.ewm').css('top',tt+'px');
//$('.ewm_small').css('top',tt+'px');
}
}
function floatResize(){
var window_width = $(window).width();
var ewmleft=$(".slide").offset().left;
if(window_width>(1200+20)){
var right = (window_width-1200)/2-24;
$('.index-left-nav').css('right',right);
}else{
$('.index-left-nav').css('display','none');
}
var left = (window_width-1200)/2-30;
//$('.ewm_small').css('left',left);
if (ewmleft>$(".ewm_jq").width()) {
//$(".ewm").css("left",parseInt(ewmleft-$(".ewm_jq").width())+"px");
} else {
//$(".ewm").css("left","0px");
}
}
})
