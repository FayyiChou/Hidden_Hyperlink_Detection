$('#btnCrop').click(function() {
var allowImgageType = ['jpg', 'jpeg', 'png', 'gif'];
var file = $("#file1").val();
if (file.length > 0) {
var pos = file.lastIndexOf(".");
var ext = file.substring(pos + 1).toLowerCase();
if($.inArray(ext, allowImgageType) != -1) {
ajaxFileUpload();
}else {alert("请选择jpg,jpeg,png,gif类型的图片");
}}else {alert("请选择jpg,jpeg,png,gif类型的图片");
}return false;
});
function ajaxFileUpload() {
$.ajaxFileUpload({
url: '/ajax.php?action=upload-slide',
secureuri: false, 
fileElementId: 'file1',
t:Math.random(),
dataType: 'json', 
success: function (data, status){
if(data.result==1){
$("#oImg").attr({src: data.src});
$('#imgsrc').val(data.path);
$('#iw').val(data.width);
$('#ih').val(data.height);
SnapSlide();
}else{
alert(data.msg);
}
},
error: function (data, status, e){
alert(e);
}
})
return false;
}
function SnapSlide() {
var y = document.getElementById('oImg').style.top;
var x = document.getElementById('oImg').style.left;
x = x.replace('-', '');
x = x.replace('px', '');
y = y.replace('-', '');
y = y.replace('px', '');
var sn = $('#slideNo').val();
$.getJSON('/ajax.php?action=snap-slide', {sn: sn, x: x, y: y, w: 1200, h: 360, iw: $('#iw').val(), ih: $('#ih').val(), src: $('#imgsrc').val()}, function(data) {alert(data.msg);
$('#s_'+ sn + ' img').attr({src: data.src});
$("#slide-upload").css('display', 'none');
$(".slide").css('display', 'block');
$(".slide-edit").css('display', 'block');
var owl = $("#owl-carousel").data('owlCarousel');
owl.goTo(sn);
});
return false;
}});
</script>
<script>
function silde_change() {
var pic = document.getElementById("oImg"), file = document.getElementById("file1");
var ext=file.value.substring(file.value.lastIndexOf(".")+1).toLowerCase();
    if(ext!='png'&&ext!='jpg'&&ext!='jpeg'){         alert("图片的格式必须为png或者jpg或者jpeg格式！");
          return;
     }     var isIE = navigator.userAgent.match(/MSIE/)!= null,         isIE6 = navigator.userAgent.match(/MSIE 6.0/)!= null;
      if(isIE) {        file.select();
        var reallocalpath = document.selection.createRange().text;
         if (isIE6) {   
         pic.src = reallocalpath;
         } else {
         pic.style.filter = "progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod='image',src=\"" + reallocalpath + "\")";
         pic.src = 'data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==';
         }     }else {
         html5Reader(file);
     }} 
function html5Reader(file){  
var file = file.files[0];
var reader = new FileReader();
reader.readAsDataURL(file);
reader.onload = function(e){  
var pic = document.getElementById("oImg");
pic.src=this.result;
}
}
</script>
