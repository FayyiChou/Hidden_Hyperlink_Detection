$(document).ready(function() {
	var s = $.cookie("zoom");
	if (s === undefined) {s=1;}

	 function setScale(scale) {
		 $("body").css("transform", "scale(" + scale + ")");
		 $("body").css("transformOrigin", "50% 0");
		$.cookie("zoom", scale, {path: "/"});
	 }

	setScale(s);

	 $("#bigger").click(function() {
		s=Number(s)+0.1;
		setScale(s);
	});

	 $("#smaller").click(function() {
		s=Number(s)-0.1;
		setScale(s);
	});

	 $("#normal").click(function() {
		s=1;
		setScale(s);
	});
});

</script>
