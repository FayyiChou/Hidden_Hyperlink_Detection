<script>
setInterval(function(){
  $('#recent-book ol>li>a,aside>div.aside-cont>nav>ul>li>a').each(function(i, e){
      e.href = e.href.replace(/^(https?\:\/\/)v\.huya\.com(\/)/, '$1video.duowan.com$2');
