<script>

$(document).ready(function() { 
		getHotKeys();	 
});
//ajax获取easydig里面的热词
function getHotKeys(){
   var url="/dig/searchhots.action";
       $.ajax({
			type : "post",
			url : url,
			data:"",
			dataType:"json",
			success : function(data, textStatus) {
				if(data){
					if(data["searchHots"]){
						$("#rz").html("");
						var _datas = data["searchHots"];
						var zj_htmls ="<a  class='zt01'>搜索热词：</a>";
var str = "";
						for(var i=0;i<_datas.length-1;i++){
str+=_datas[i].query;
if(str.length<=9){
						  if(_datas[i].query!=""){
zj_htmls +="<a  href='#' onclick='searchq(\""+_datas[i].query+"\")' >"+_datas[i].query+"&nbsp;</a>";
							}
           
}else{break;}
					    } 
						$("#rz").html(zj_htmls);
						return false;
					} 
				}
			},
	 		error:function(){
				
				return false;
			}
		});
}
function searchq(q){
document.getElementById('q').value = q;
document.getElementById('searchForm').submit();
}
</script>
 
