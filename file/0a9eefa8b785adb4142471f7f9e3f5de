<script>
        

		
        function mamaLogin(url) {
                if (url) {
                    window.location.href = url;
                } else {
                    window.location.href = 'http://passport.mama.cn/wapindex/?key=try.mama.cn&service=http%253A%252F%252Ftry.mama.cn%252FWap%252FTryList%252Fdetail%252Fid%252F544&sign=ec47ebd2878fd03b6af9234c4c274ff8';
                }
            }

        
        
		
        
          
        function openLogin(url) {
            
            mamaLogin(url);
                    }
    </script> 



