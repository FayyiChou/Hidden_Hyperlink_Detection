    <script>
        var iBodyWidth = $(window).width()-20;
        $("iframe").each(function(){
            var iW = $(this).width();
            var iH = $(this).height();
            $(this).attr("width",iBodyWidth);
            $(this).attr("height",iBodyWidth*iH/iW);
            $(this).css({
                "display":"block",
                "margin":"0 auto"
            });
        });

        $(function(){
             $("#show_fail .x_pink").on("touchend",function(){
                 $("#show_fail").hide();
                 location.href="//try.mama.cn/Wap/TryList";
             });

            $("#show_login .x_pink").on("touchend",function(){
                $("#show_login").hide();
                location.href='http://passport.mama.cn/wapindex/?key=try.mama.cn&service=http%253A%252F%252Ftry.mama.cn%252FWap%252FTryList%252Fdetail%252Fid%252F544&sign=ec47ebd2878fd03b6af9234c4c274ff8';
            });

            $("#show_myinfo .x_pink").on("touchend",function(){
                $("#show_myinfo").hide();
                //location.href='//try.mama.cn/Wap/My';
                location.href="//try.mama.cn/Wap/My/myInfo";
            })

            $("#show_personal .x_pink").on("touchend",function(){
                $("#show_personal").hide();
                $.post("//try.mama.cn/Wap/My/check_user/",{from:'popup'},function(data){
                    if(data.status=='success'){
                        window.location.href="//try.mama.cn/Wap/My/myInfo";
                    }
                })
            })

            $("#show_personal .x_grey").on("touchend",function(){
                 $("#show_personal").hide();
            })

            $("#show_fail_report .icon").on("touchend",function(){
                $("#show_fail_report").hide();
                return false;
            })

            $("#show_fail_report .x_pink").on("touchend",function(){
                $("#show_fail_report").hide();
                window.location.href= "//try.mama.cn/Wap/My/myTry/status/1";
                return false;
            })
        })
        var id='544';
        var periods = '1';
        var host = location.protocol+'//'+location.host;
        var submit_count = 1;
        $("#apply_submit").on("touchend",function(){
             //var url = host+"/Wap/TryList/check_apply/";
             var url = "//try.mama.cn/Wap/TryList/check_apply/";
             $.post(url,{id:id,periods:periods,submitcount:submit_count},
                    function(data){
                        if(data.status=='identity'){
                            $("#show_fail").show();
                            $("#show_fail_msg").text(data.msg);
                        }else if(data.status=='personal'){
                            $("#show_personal").show();
                            $("#show_personal_msg").text(data.msg);
                        }else if(data.status=='city'){
                            $("#show_fail").show();
                            $("#show_fail_msg").text(data.msg);
                        }else if(data.status=='has_apply'){
                            $("#show_fail").show();
                            $("#show_fail_msg").text(data.msg);
                        }else if(data.status=='black'){
                            $("#show_fail").show();
                            $("#show_fail_msg").text(data.msg);
                        }else if(data.status=='no_pregnant'){
                            $("#show_myinfo").show();
                            $("#show_myinfo_msg").text(data.msg);
                        }else if(data.status=='pass_apply'){
                            //location.href= host+"/Wap/TryList/apply?id=544";
                            location.href= "//try.mama.cn/Wap/TryList/apply?id=544";
                        }else if(data.status=='no_login'){
                            $("#show_login").show();
                            $("#show_login_msg").text(data.msg);
                            setTimeout(openLogin,3000);
                        }else if(data.status == 'is_open'){
                            $("#show_fail").show();
                            $("#show_fail_msg").text(data.msg);
                        }else if(data.status == 'is_report'){
                            $("#show_fail_report").show();
                            $("#show_fail_msg_report").text(data.msg);
                        }
                        submit_count++;
                    });
          });

    </script>

