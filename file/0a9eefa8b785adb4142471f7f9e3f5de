    //mshareTitle = escape(mshareTitle);
    // app分享到微信的信息读取函数
    var Mama = new Object();
    Mama.getShare = function (funcname) {
        if(typeof(funcname) == 'undefined' || funcname == null){ //ios使用
            return '{"mshareTitle":"'+mshareTitle+'","mshareDesc":"'+mshareDesc+'","mshareImage":"'+mshareImages+'","mshareUrl":"'+mshareUrl+'"}';
        }else{ // android使用
            eval(funcname + '(\'{"mshareTitle":"'+mshareTitle+'","mshareDesc":"'+mshareDesc+'","mshareImage":"'+mshareImages+'","mshareUrl":"'+mshareUrl+'"}\')');

        }
    }

</script>
