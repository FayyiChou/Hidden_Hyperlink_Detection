  <script type="text/javascript">



function auto_height(){
     var h= document.documentElement.clientHeight-0;
 var high = document.getElementById('wp');
 high.style.height=h+"px";
 }	

 $('#klp_asideon').bind('click', function() {
    if($('body').css('overflow-x') != 'hidden') {
        $('body').css('overflow-x', 'hidden'); 
        $('#klp_main').addClass('klp_kai');
$('#klp_asidecon').addClass('klp_side');
$('#klp_aside_open').addClass('klp_open');
$('#klp_asideon').addClass('klp_kg');
    } else {
        $('body').css('overflow-x', '');
        $('#klp_main').removeClass('klp_kai');
$('#klp_asidecon').removeClass('klp_side');
$('#klp_aside_open').removeClass('klp_open');
$('#klp_asideon').removeClass('klp_kg');
    }
});


if(window.onload!=null){   
    window.onload=function(){auto_height();};  
}
</script>  
