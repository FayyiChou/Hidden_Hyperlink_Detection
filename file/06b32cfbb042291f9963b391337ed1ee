    //设置页面文字大小
    function SetFontSize(areaid, type) {
        if (type == "down") {
            if (parseInt(document.getElementById("FontSize").value) > 14) {
                document.getElementById(areaid).style.fontSize = (parseInt(document.getElementById("FontSize").value) - 2) + "px";
                document.getElementById("FontSize").value = parseInt(document.getElementById("FontSize").value) - 2;
                if (parseInt(document.getElementById("FontSize").value) == 16) {
                    document.getElementById("FontSizeDown").className = "icon font_down";
                }
                else {
                    document.getElementById("FontSizeDown").className = "icon font_down font_down_disable";
                }
                if (parseInt(document.getElementById("FontSize").value) < 20) {
                    document.getElementById("FontSizeUp").className = "icon font_up";
                }
            }
            else {
                document.getElementById("FontSizeDown").className = "icon font_down font_down_disable";
            }
        }
        else {
            if (parseInt(document.getElementById("FontSize").value) < 18) {
                document.getElementById(areaid).style.fontSize = (parseInt(document.getElementById("FontSize").value) + 2) + "px";
                document.getElementById("FontSize").value = parseInt(document.getElementById("FontSize").value) + 2;
                if (parseInt(document.getElementById("FontSize").value) == 18) {
                    document.getElementById("FontSizeUp").className = "icon font_up font_up_disable";
                }
                else {
                    document.getElementById("FontSizeUp").className = "icon font_up";
                }
                if (parseInt(document.getElementById("FontSize").value) > 14) {
                    document.getElementById("FontSizeDown").className = "icon font_down";
                }
            }
            else {
                document.getElementById("FontSizeUp").className = "icon font_up font_up_disable";
            }
        }
    }

    //加入收藏
    function AddFavorite(sURL, sTitle) {
        try {
            window.external.addFavorite(sURL, sTitle);
        }
        catch (e) {
            try {
                window.sidebar.addPanel(sTitle, sURL, "");
            }
            catch (e) {
                alert("此操作被浏览器拒绝，请使用Ctrl+D进行添加");
            }
        }
    }
</script> 
