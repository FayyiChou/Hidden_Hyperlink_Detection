       <script type="text/javascript">
                        //判断含空格回车为空
                        function javaTrim(str) {
                            for (var i = 0; (str.charAt(i) == ' ') && i < str.length; i++);
                            if (i == str.length) return ''; //whole string is space
                            var newstr = str.substr(i);
                            for (var i = newstr.length - 1; newstr.charAt(i) == ' ' && i >= 0; i--);
                            newstr = newstr.substr(0, i + 1);
                            return newstr;
                        }

                        function comment_checkSubmit() {
                            if ($('#comment_UserName').val() == "") {
                                layer.msg("用户名不能为空！", 2, -1);
                                return false;
                            } else if ($('#comment_Password').val() == "") {
                                layer.msg("密码不能为空！", 2, -1);
                                return false;
                            } else {
                                $("#UserLoginForm").submit();
                            }
                        }

                        function checkSubmit2() {
                            var strValue = $('#content').val();
                            strValue = strValue.replace(/\n/g, '');

                            if ($('#content').val() == "文明上网，登录发帖" || javaTrim(strValue) == '') {
                                layer.msg("评论内容不能为空！", 2, -1);
                                return false;
                            } else if ($('#content').val().length > 500) {
                                layer.msg("评论内容不能大于500个字！", 2, -1);
                                return false;
                            } else if ($('#content').val().length < 1) {
                                layer.msg("评论内容不能小于1个字！", 2, -1);
                                return false;
                            } else {
                                $("#AddCommentForm").submit();
                            }

                        }

                        //按回车键提交表单
                        $(document).keydown(function (event) {
                            if (event.keyCode == 13) {
                                checkSubmit();
                            }
                        });
                    </script> 
       <script language="javascript" type="text/javascript">

    function click_a(divDisplay) {
        if (document.getElementById(divDisplay).style.display != "block") {
            $(".replay-dd").hide();
            document.getElementById(divDisplay).style.display = "block";
        }
        else {
            document.getElementById(divDisplay).style.display = "none";
        }
    }

    function click_b(divDisplay) {
        if ($(divDisplay).parent().parent().next().css('display') == "none") {
            $(".replay-dd").hide();
            $(divDisplay).parent().parent().next().toggle();
        }
        else{
            $(divDisplay).parent().parent().next().toggle();
        }
    }


</script> 
