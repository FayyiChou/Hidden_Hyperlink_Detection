  <script src="http://style.rexian.cn/SuperAdmin/Validform/Validform_v5.3.2.js" type="text/javascript"></script> 
  <script src="http://style.rexian.cn/SuperAdmin/Validform/Validform_Datatype.js" type="text/javascript"></script> 
  <script type="text/javascript">
        function toLogin() {
            var url = location.href;
            location.href = "https://graph.qq.com/oauth2.0/authorize?client_id=101267546&response_type=token&scope=get_user_info&redirect_uri=http://LoginApi.rexian.cn/QQ/Login/QQLogin?url=" + url;
        }
        function ToWeiBoLogin() {
            var url = location.href;
            location.href = "https://api.weibo.com/oauth2/authorize?client_id=796644251&response_type=code&redirect_uri=http://LoginApi.rexian.cn/WeiBo/Login/Login?url=" + url;
        }

        function ToWeiXinLogin() {
            var url = "http://LoginApi.rexian.cn/WeiXin/Login/Login?url=" + location.href;
            var UrlEnCode = encodeURIComponent(url);
            var State=3145255004168460;
            location.href = "https://open.weixin.qq.com/connect/qrconnect?appid=wx65436fcedb5c9078&redirect_uri=" + UrlEnCode + "&response_type=code&scope=snsapi_login&state="+ State +"#wechat_redirect";
        }
        function ToZFBLogin() {
            location.href = "http://LoginApi.rexian.cn/ZhiFuBao/Login/Login?url=" + location.href;
        }

        //找到Form
        $(function () {

            //为form添加验证事件
            $("#TopUserLoginForm").Validform({
                tiptype: function (msg, o, cssctl) {
                    if (!o.obj.is("form")) {
                        if (o.type == 2) {
                            //$.Hidemsg();//公用方法关闭信息提示框;
                        } else {
                            //layer.msg(msg, 2, -1);
                        }
                    }
                },
                tipSweep: true,
                ajaxPost: true,
                ignoreHidden: true,
                callback: function (data) {
                    showMsg(data);
                }
            });
            $("#TopStoreLoginForm").Validform({
                tiptype: function (msg, o, cssctl) {
                    if (!o.obj.is("form")) {
                        if (o.type == 2) {
                            //$.Hidemsg();//公用方法关闭信息提示框;
                        } else {
                            //layer.msg(msg, 2, -1);
                        }
                    }
                },
                tipSweep: true,
                ajaxPost: true,
                ignoreHidden: true,
                callback: function (data) {
                    showMsg(data);
                }
            });

            //为form添加验证事件
            $("#UserLoginForm").Validform({
                tiptype: 3,
                ajaxPost: true,
                ignoreHidden: true,
                callback: function (data) {
                    showMsg(data);
                }
            });

            $("#AddCommentForm").Validform({
                tiptype: 3,
                ajaxPost: true,
                ignoreHidden: true,
                callback: function (data) {
                    showMsg(data);
                }
            }); 

            //$("#newsReplyForm").Validform({
            //    tiptype: 3,
            //    ajaxPost: true,
            //    ignoreHidden: true,
            //    callback: function (data) {
            //        showMsg(data);
            //    }
            //});

            //删除按钮点击事件
            $(".delbtn").click(function () {
                var url = $(this).attr("url");
                layer.confirm('您确定要删除吗？', function (index) {
                    layer.close(index);
                    execute(url);
                });
            });

            //删除按钮点击事件
            $(".Cancellation").click(function () {
                var url = $(this).attr("url");
                layer.confirm('您确定要注销吗？', function (index) {
                    layer.close(index);
                    execute(url);
                });
            });
            //其他操作按钮点击事件
            $(".operbtn").click(function () {
                var url = $(this).attr("url");
                execute(url);
            });

            $(".guanzhu").click(function () {
                var url = $(this).attr("url");
                execute(url);
            });

            $(".shoucang").click(function () {
                var url = $(this).attr("url");
                execute(url);
            });
            $(".xihuan").click(function () {
                var url = $(this).attr("url");
                execute(url);
            });

        });

        //执行方法
        function execute(url) {
            $.ajax({
                type: "get",
                url: url,
                success: function (data, textStatus) {
                    showMsg(data);
                }
            });
        }

        //显示消息
        function showMsg(data) {
            if (data.status == "0") {
                layer.msg(data.info, 2, -1);
                if (data.url.length > 0) {
                    setTimeout(function () {
                        window.location.href = data.url;
                    }, 500);
                } else {
                    setTimeout(function () {
                        window.location.href = window.location.href;
                    }, 500);
                }
            }
            if (data.status == "1") {
                layer.msg(data.info, 2, -1);
                setTimeout(function () {
                    window.location.href = data.url;
                }, 500);
            }
            if (data.status == "2") {
                layer.msg(data.info, 2, -1);
                setTimeout(function () {
                    javascript: history.go(-1);
                }, 500);
            }
            if (data.status == "3") {
                document.write(data.url);
            }
            if (data.status == "4") {
                layer.msg(data.info, 2, -1);
            }
        }

        //登录提交判断
        function checkSubmit(formName) {
            if ($("form[id=" + formName + "] input[id='UserName']").val() == "请输入用户名") {
                //alert("用户名不能为空！");
                layer.msg("用户名不能为空！", 2, -1);
                return false;
            } else if ($("form[id=" + formName + "] input[id='Password']").val() == "") {
                //alert("密码不能为空！");
                layer.msg("密码不能为空！", 2, -1);
                return false;
            } else {
                $("#" + formName + "").submit();
            }
        }

        function bindtextbox(formname, username, password) {
            //对用户名框进行操作
            var $username = $("form[id=" + formname + "] input[id=" + username + "] ");
            $username.focus(function () {   //用户名框获得鼠标焦点
                if ($username.val() == this.defaultValue) {
                    $(this).val("");              //如果符合条件，则清空文本框内容
                }
                $(this).addClass("focus");
            });
            $username.blur(function () { //用户名框失去鼠标焦点
                if ($username.val() == "") {
                    $(this).val(this.defaultValue); //如果符合条件，则设置内容
                }
                $(this).removeClass("focus");
            });

            //对密码框进行操作
            var $pwd = $("form[id=" + formname + "] input[id=pwd] ");
            var $password = $("form[id=" + formname + "] input[id=" + password + "] ");
            $password.hide();
            $pwd.focus(function () {
                if ($pwd.val() == this.defaultValue)
                    $pwd.hide();
                $password.show().focus();
                $password.show();
                $($password).addClass("focus");
            });

            $password.blur(function () {
                if ($password.val() == "") {
                    $password.hide();
                    $pwd.show();
                }
                $(this).removeClass("focus");
            });

        }


    </script> 
