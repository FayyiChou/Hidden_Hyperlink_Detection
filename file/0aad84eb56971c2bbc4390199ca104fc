demo2_top.innerHTML=demo1_top.innerHTML;
function Marqueetest(){
if(demo_top.scrollLeft>=demo1_top.scrollWidth){
demo_top.scrollLeft=0;
}
else{
demo_top.scrollLeft++;
}
}
var MyMartest=setInterval(Marqueetest,speedtest);
