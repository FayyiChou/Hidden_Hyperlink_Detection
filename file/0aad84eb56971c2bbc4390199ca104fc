$(document).ready(function(){
$('#welcome').fadeTo(2000, 1).delay(2000).animate({
opacity: 0,
marginTop: '-=200'
},
1000,
function() {
$('#welcome').hide();
});
$(window).scroll(function() {
if ($(window).scrollTop() > 50) {
$('#jump li:eq(0)').fadeIn(800);
} else {
$('#jump li:eq(0)').fadeOut(800);
}
});
$("#top").click(function() {
$('body,html').animate({
scrollTop: 0
},
1000);
return false;
});
});
</script>
