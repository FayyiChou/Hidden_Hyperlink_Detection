    <script type="text/javascript">
    var speed_2=200;
    var i=0;
     $(".find_"+2).find('td').each(function(){
        i++;
     });
     if(i>8){
         marquePic2_2.innerHTML=marquePic1_2.innerHTML; 
     }
//    marquePic2_2.innerHTML=marquePic1_2.innerHTML; 
    function Marquee_2(){ 
        if(demo_2.scrollLeft>=marquePic1_2.scrollWidth){ 
              demo_2.scrollLeft=0;
        }else{ 
             demo_2.scrollLeft++; 
        } 
    } 
    var MyMar_2=setInterval(Marquee_2,speed_2); 
    demo_2.onmouseover=function() {
        clearInterval(MyMar_2);
    } 
    demo_2.onmouseout=function() {
        MyMar_2=setInterval(Marquee_2,speed_2);
    } 
    </script>
