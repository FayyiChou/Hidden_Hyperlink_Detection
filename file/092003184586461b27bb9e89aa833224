    <script type="text/javascript">
    var speed_3=200;
    var i=0;
     $(".find_"+3).find('td').each(function(){
        i++;
     });
     if(i>8){
         marquePic2_3.innerHTML=marquePic1_3.innerHTML; 
     }
//    marquePic2_3.innerHTML=marquePic1_3.innerHTML; 
    function Marquee_3(){ 
        if(demo_3.scrollLeft>=marquePic1_3.scrollWidth){ 
              demo_3.scrollLeft=0;
        }else{ 
             demo_3.scrollLeft++; 
        } 
    } 
    var MyMar_3=setInterval(Marquee_3,speed_3); 
    demo_3.onmouseover=function() {
        clearInterval(MyMar_3);
    } 
    demo_3.onmouseout=function() {
        MyMar_3=setInterval(Marquee_3,speed_3);
    } 
    </script>
