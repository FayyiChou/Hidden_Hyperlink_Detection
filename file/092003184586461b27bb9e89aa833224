    function change_text(e) {
        $("#serach_tab").find("li").each(function(){
            if(e==($(this).attr('data-num'))){
                 $(this).addClass('serch_click');
            }else{
                $(this).removeClass('serch_click');
            }
         });
        if(e==1){
             document.getElementById('keyword').setAttribute("placeholder","搜索 商品");
             $("#chang_kuang").val(e);
        }else{
             document.getElementById('keyword').setAttribute("placeholder","搜索 店铺");
             $("#chang_kuang").val(e);
        }
    }
    $(function () {
        checkCart();
    });
