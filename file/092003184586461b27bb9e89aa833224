    <script type="text/javascript">
    var speed_5=200;
    var i=0;
     $(".find_"+5).find('td').each(function(){
        i++;
     });
     if(i>8){
         marquePic2_5.innerHTML=marquePic1_5.innerHTML; 
     }
//    marquePic2_5.innerHTML=marquePic1_5.innerHTML; 
    function Marquee_5(){ 
        if(demo_5.scrollLeft>=marquePic1_5.scrollWidth){ 
              demo_5.scrollLeft=0;
        }else{ 
             demo_5.scrollLeft++; 
        } 
    } 
    var MyMar_5=setInterval(Marquee_5,speed_5); 
    demo_5.onmouseover=function() {
        clearInterval(MyMar_5);
    } 
    demo_5.onmouseout=function() {
        MyMar_5=setInterval(Marquee_5,speed_5);
    } 
    </script>
