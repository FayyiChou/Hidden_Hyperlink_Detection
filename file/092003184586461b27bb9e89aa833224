    <script type="text/javascript">
    var speed_4=200;
    var i=0;
     $(".find_"+4).find('td').each(function(){
        i++;
     });
     if(i>8){
         marquePic2_4.innerHTML=marquePic1_4.innerHTML; 
     }
//    marquePic2_4.innerHTML=marquePic1_4.innerHTML; 
    function Marquee_4(){ 
        if(demo_4.scrollLeft>=marquePic1_4.scrollWidth){ 
              demo_4.scrollLeft=0;
        }else{ 
             demo_4.scrollLeft++; 
        } 
    } 
    var MyMar_4=setInterval(Marquee_4,speed_4); 
    demo_4.onmouseover=function() {
        clearInterval(MyMar_4);
    } 
    demo_4.onmouseout=function() {
        MyMar_4=setInterval(Marquee_4,speed_4);
    } 
    </script>
