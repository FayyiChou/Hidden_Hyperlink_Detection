    <script type="text/javascript">
    var speed_6=200;
    var i=0;
     $(".find_"+6).find('td').each(function(){
        i++;
     });
     if(i>8){
         marquePic2_6.innerHTML=marquePic1_6.innerHTML; 
     }
//    marquePic2_6.innerHTML=marquePic1_6.innerHTML; 
    function Marquee_6(){ 
        if(demo_6.scrollLeft>=marquePic1_6.scrollWidth){ 
              demo_6.scrollLeft=0;
        }else{ 
             demo_6.scrollLeft++; 
        } 
    } 
    var MyMar_6=setInterval(Marquee_6,speed_6); 
    demo_6.onmouseover=function() {
        clearInterval(MyMar_6);
    } 
    demo_6.onmouseout=function() {
        MyMar_6=setInterval(Marquee_6,speed_6);
    } 
    </script>
