    <script type="text/javascript">
    var speed_7=200;
    var i=0;
     $(".find_"+7).find('td').each(function(){
        i++;
     });
     if(i>8){
         marquePic2_7.innerHTML=marquePic1_7.innerHTML; 
     }
//    marquePic2_14.innerHTML=marquePic1_14.innerHTML; 
    function Marquee_7(){ 
        if(demo_7.scrollLeft>=marquePic1_7.scrollWidth){ 
              demo_7.scrollLeft=0;
        }else{ 
             demo_7.scrollLeft++; 
        } 
    } 
    var MyMar_7=setInterval(Marquee_7,speed_7); 
    demo_7.onmouseover=function() {
        clearInterval(MyMar_7);
    } 
    demo_7.onmouseout=function() {
        MyMar_7=setInterval(Marquee_7,speed_7);
    } 
    </script>

