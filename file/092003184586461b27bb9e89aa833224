            $("#main .mainright .mt .mc .m-c").hover(function () {
            $(this).parent(".mc").find("div").removeClass('corrent');
            $(this).addClass("corrent");
            var data_id = $(this).attr('id');
            $(this).parent('.mc').nextAll('.mc-inner').css('display', 'none');
            $('#main .mainright .mt .' + data_id).css('display', 'block');
            }, function () {
