					  <script>
					   function singleQuery(){
					     var selectSingle = document.getElementById('singleTeam');
					     
					     document.location='/resultschedule.do?lid=4&sid=2016&singleTeam='+selectSingle.value+'&lang='+langs;
					   }
					   document.getElementById('singleTeam').value='';
					  
					   function doubleTeamQuery(){
					     var selectHost = document.getElementById('hostTeam');
					     var selectVisit = document.getElementById('visitTeam');
					     
					     if(selectHost.value != '' && selectVisit.value != ''){
					       if(selectHost.value == selectVisit.value){
					         alert('不能選擇同一球隊，請重新選擇！');
					         return;
					       }
					     }else{
					       alert('請選擇球隊!');
					       return;
					     }
					     
					     document.location='/resultschedule.do?lid=4&sid=2016&hostTeam='+selectHost.value+'&visitTeam='+selectVisit.value+'&lang='+langs;
					   }
					   
					   document.getElementById('hostTeam').value='';
					   document.getElementById('visitTeam').value='';
					  </script>
