				    var maxRoundNum = parseInt('38');
				    if(maxRoundNum == 0 ){
				       maxRoundNum = 38;
				    }
				    var rowL='';
				    if(maxRoundNum%2!=0){
				    	rowL=(maxRoundNum+1)/2;
				    }
				    if(maxRoundNum%2==0){
				    	rowL = (maxRoundNum)/2;
				    }
				     
