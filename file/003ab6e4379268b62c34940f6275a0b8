				<script>
				  function showRound(number){
				    document.location='/resultschedule.do?lid=4&sid=2016&roundNum='+number+'&lang='+langs;
				  }
				  
				  var ths = document.getElementsByName('rounds');
				  for(var i=0;i<ths.length;i++){
				    ths[i].className = "";
				  }
				  var roundNum = '29';
				  ths[(parseInt(roundNum)-1)].className = 'selected';
				  
