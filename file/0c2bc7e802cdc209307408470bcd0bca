<script type="text/javascript">
jQuery(document).ready(function($) {
    hs.graphicsDir = "http://www.klinm.com/content/templates/moxiaonai/js/highslide/graphics/";
	hs.align = "center";
	hs.transitions = ["expand", "crossfade"];
	hs.outlineType = "rounded-white";
	hs.wrapperClassName = "dark borderless floating-caption";
	hs.fadeInOut = !0;
	hs.dimmingOpacity = .75;
    hs.addSlideshow({
        interval: 5000,
        repeat: true,
        useControls: true,
        fixedControls: "fit",
        overlayOptions: {
            opacity: 0.75,
            position: "bottom center",
            hideOnMouseOut: true
        }
    });
	jQuery(function($){$("a[href$=jpg],a[href$=gif],a[href$=png],a[href$=jpeg],a[href$=bmp]").addClass("highslide").each(function(){this.onclick=function(){return hs.expand(this)}});})
});
</script>
