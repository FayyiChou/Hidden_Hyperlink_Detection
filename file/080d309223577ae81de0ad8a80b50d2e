    function getImgNaturalDimensions(img, callback) {
        if (img.naturalWidth) {
            callback(img.naturalWidth, img.naturalHeight);
        } else {
            var image = new Image();

            image.onload = function() {
                callback(image.width, image.height);
            };
            image.src = img.src;
        }
    }

    function q(selector, ele, isAll) {
        return (ele || document)[isAll ? 'querySelectorAll': 'querySelector'](selector);
    }

    function createMark(data, wr, hr) {
        var ret = '';
        var v;
        var fv;

        var curId = bbb[0].id;

        for(var i = 0, l = data.length; i < l; i++) {
            v = data[i];
            fv = filterData(bbb, 'id', v.id);
            if(v.x && v.y && fv) {
                ret += '<span class="mark-label' + (v.id == curId ? ' clicked': '')+ '" data-id="' + v.id + '" style="left:' + (v.x*wr-12) + 'px;top:' + (v.y*hr-30) + 'px;"><span class="l-tle">' + (fv.building_code||'') + '</span><span class="l-des">' + (fv.sale_status_name ||'') + '</span><span class="l-arrow"></span></span>';
            }
        }

        $markWrap.innerHTML += ret;
    }

    function filterData(ori, id, val) {
        for(var i = 0, l = ori.length; i <l; i++) {
            if(ori[i][id] == val) {
                return ori[i];
            }
        }
    }


    

    if(document.querySelector) {
        var $mod = q("#build-info");
        var $imgWrap = q(".img-wrap", $mod);
