                preEle = tar;
            }
        });

        getImgNaturalDimensions($img, function(oriImgWidth, oriImgHeight) {
            var containerWidth = parseInt($imgWrap.style.width),
                containerHeight = parseInt($imgWrap.style.height),

                containerRatio = containerWidth / containerHeight,
                oriImgRatio = oriImgWidth / oriImgHeight;

            var newImgWidth = 0,
                newImgHeight = 0,

                resizeKey = '',
                resizeVal = '';

            if (oriImgRatio > containerRatio) {
            // wide image
                if (oriImgWidth < containerWidth) {
                    newImgWidth = containerWidth;
                } else {
                    newImgWidth = oriImgWidth;
                }
                resizeKey = 'width';
                resizeVal = newImgWidth + 'px';
                newImgHeight = newImgWidth / oriImgRatio;

            } else {
            // tall image
                if (oriImgHeight < containerHeight) {
                    newImgHeight = containerHeight;
                } else {
                    newImgHeight = oriImgHeight;
                }
                resizeKey = 'height';
                resizeVal = newImgHeight + 'px';
                newImgWidth = newImgHeight * oriImgRatio;
            }

            $img.style[resizeKey] = resizeVal;

            var wRatio = newImgWidth / 800, // 坐标系统参考图片宽度
                hRatio = newImgHeight / (800 / oriImgRatio);

            createMark(aaa, wRatio, hRatio);
            preEle = q(".clicked", $markWrap);
        });
    }
    
}());
