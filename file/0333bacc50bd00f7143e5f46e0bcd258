//左右滚动JS/////////////////////////////////////////////////////

var speed=10//速度数值越大速度越慢
LrDemo643202.innerHTML=LrDemo643201.innerHTML
function MarqueeLeft64320(){
if(LrDemo643202.offsetWidth-LrDemo64320.scrollLeft<=0)
LrDemo64320.scrollLeft-=LrDemo643201.offsetWidth
else{
LrDemo64320.scrollLeft++
}
}
var MyMar64320=setInterval(MarqueeLeft64320,speed)
LrDemo64320.onmouseover=function() {clearInterval(MyMar64320)}
LrDemo64320.onmouseout=function() {MyMar64320=setInterval(MarqueeLeft64320,speed)}
 
////////////////////////////////////////////////////////////////////////
</SCRIPT>
