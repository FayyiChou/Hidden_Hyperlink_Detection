<script type="text/javascript">
$(function() {
	$.slidenav(300);

	var sliderElement = $('.slidesjs');
	sliderElement.slidesjs({
		width: 1100,
		height: 504,
		play: {
			auto: true,
			pauseOnHover: true,
		},
		navigation: {
			effect: 'fade'
		},
		pagination: {
			effect: 'fade'
		},
		effect: {
			fade: {
			speed: 400
		}
		},
		callback: {
			start: function(number) {
				number = $('.slidesjs-pagination a.active').html();
				$('.banner_nav .icon').each(function(i) {
					var index = i + 1;
					if(index == number) {
						$(this).removeClass('s-banner_nav-s'+ index).addClass('s-banner_nav-s'+ index +'_current');
					} else {
						$(this).removeClass('s-banner_nav-s'+ index +'_current').addClass('s-banner_nav-s'+ index);
					}
				});
			}
		}
	});
	var slidernav = sliderElement.find('.slidesjs-navigation');
	$('.banner').hover(function() {
		slidernav.stop().animate({
			opacity: 1
		})
	}, function() {
		slidernav.stop().animate({
			opacity: 0
		});
	});
});
</script>
