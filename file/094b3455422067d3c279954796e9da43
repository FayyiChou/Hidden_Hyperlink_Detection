        $('.js_refreshCaptcha , .js_commonCaptchaImg').click(function () {
            var imgsrc = 'http://account.gyyx.cn/captcha/create?bid=ertvfgre&r='+Math.random()+'.png';
            $('.js_commonCaptchaImg').attr('src',imgsrc);
        })

        /*获取验证码类型*/
        $.ajax({
            url: 'http://account.gyyx.cn/captcha/CorsNeedCaptcha',
            data: {
                bid: 'ertvfgre',
                r: Math.random()
            },
            dataType: 'jsonp',
            jsonp: 'jsoncallback',
            type: 'get',
            success: function (data) {
                if (data.type == '0') {
                    $('.reg_form_web .js_china_captcha_box').remove();

                    var imgsrc = 'http://account.gyyx.cn/captcha/create?bid=ertvfgre&r=' + Math.random() + '.png';
                    $('.js_commonCaptchaImg').attr('src', imgsrc);
                } else if (data.type == '1') {
                    $('.reg_form_web .js_Common_captcha_box').remove();
                } else {
                    $('.reg_form_web .js_china_captcha_box').remove();
                    $('.reg_form_web .js_Common_captcha_box').remove();
                }
            }
        })
    })
</script>
        
    </div>
