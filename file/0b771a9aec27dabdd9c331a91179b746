<script>
    var $ = jQuery;
    var qrcode;
    $(function () {
        if (typeof(ablumDataJson) != "undefined") {  //有专辑栏
            if (ablumDataJson[currentId]) { //视频在专辑里
                var index = $("#" + currentId).index(); //获取索引值,以便计算滚动高度
                var scrollNum = (index / 6) * 406 - 100;
                $("#" + currentId).addClass("currentAblum");
                $(".ablum").scrollTop(scrollNum);
            }
        }
        window.addEventListener('message', function (e) {
            var nextObj;
            var nextId = "";
            var ablumId = "";
            if (ablumDataJson[currentId]) {   //视频在专辑里
                currentId = e.data;
                if (currentId != $(".ablum div:last").attr("id")) {
                    nextObj = $("#" + currentId).next();
                    nextId = nextObj.attr("id");
                    ablumId = ablumDataJson[currentId].album_id;
                } else {
                    return null;
                }
            } else {
                nextId = $(".ablum div:first").attr("id");
                ablumId = ablumDataJson[nextId].album_id;
            }
            if (nextId != "") {
                if (ablumId != undefined) {
                    window.location.href = "/video/" + nextId + ".html?a=" + ablumId;
                } else {
                    window.location.href = "/video/" + nextId + ".html";
                }

            }
        }, false);


        //获取视频列表状态
        var videoId = $(".mod_player video").attr("id");
        $("#video_" + videoId).addClass("current");

        //排行榜切换
        $('#tab1').mouseover(function () {
            $("#tab2").removeClass("current");
            if (!$('#tab1').hasClass('current')) {
                $("#tab1").addClass("current");
            }
            if ($('.tab1').hasClass('hide')) {
                $(".tab1").removeClass("hide");
            }
            if (!$('.tab2').hasClass('hide')) {
                $(".tab2").addClass("hide");
            }
        });


        $('#tab2').mouseover(function () {
            $("#tab1").removeClass("current");
            if (!$('#tab2').hasClass('current')) {
                $("#tab2").addClass("current");
            }
            if ($('.tab2').hasClass('hide')) {
                $(".tab2").removeClass("hide");
            }
            if (!$('.tab1').hasClass('hide')) {
                $(".tab1").addClass("hide");
            }
        });

        var playBoxs = document.getElementsByClassName("playbox");
        for (var i in playBoxs) {
            $(playBoxs[i]).mouseover(function () {
                $(this).css('opacity', 1);
            })

            $(playBoxs[i]).mouseout(function () {
                $(this).css('opacity', 0);
            })

        }

        $(".copyUrl").click(function () {
            copyToClipboard($(this).prev().attr('value'));
            return false;
        });

        //折叠视频简介
        var desc = $('.mod_video_desc_info').data('intro');
        if (desc.length > 80) {
            $(".short-intro").html(desc.substring(0, 80) + "...  " + "<a href='javascript:void(0);' class='open-intro'>展开&gt;</a>");
        } else {
            $(".short-intro").html(desc);
        }

        $('.open-intro').click(function () {
            $('.short-intro').hide();
            $('.more-intro').show();
        });

        $('.close-intro').click(function () {
            $('.short-intro').show();
            $('.more-intro').hide();
        });

        var videoBind = $(".index_cont video").attr("id");
        $('#' + videoBind).bind('contextmenu', function () {
            return false;
        }); //屏蔽视频右键点击
    });


    function changeVideo(id) {
        window.location.href = "/video/" + id + ".html";
    }

    function nextPlay(id) {
        var nextId = $("#video_" + id).next('li').attr('id').split('_')[1];
        window.location.href = "/video/" + nextId + ".html";
    }

    function shareUrl(obj) {
        $('.sharePanel').toggle();
    }

    function showWXBox() {
        $("#wxBox").toggle();
        if (!qrcode) {
            qrcode = new QRCode(document.getElementById("wxBoxCode"), {
                width: 170,
                height: 170
            });

            var url = window.location.href;

            if (url) {
                qrcode.makeCode(url);
            }
        } else {
            return false;
        }
    }

    function closeWXBox() {
        $("#wxBox").hide();
    }


</script>


</body></html>