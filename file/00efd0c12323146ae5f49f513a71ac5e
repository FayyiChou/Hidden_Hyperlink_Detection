	/**/
	$(function(){
		//总共的页数
		function pageNum(gameLength){
			var num = Math.ceil(gameLength/24);
			var html = '';
			var clazz = "";
			html += '<a onclick="showPageNumGame(1);">\u9996\u9875</a>';
			for(var i=1;i<=num;i++){
				clazz = i == 1 ? 'On' : '' ;
				html += '<a class="'+clazz+'" href="javascript:;" onclick="showPageNumGame('+i+');">'+i+'</a>';
			}
			html += '<a onclick="showPageNumGame('+num+');">\u672b\u9875</a>';
			$(".page_nav").html(html);
		}
		pageNum(gameLength);
		showPageNumGame(1);
		$(".page_nav a").click(function(){
			$(this).addClass("On").siblings().removeClass("On");
		});
	})

	//显示游戏
	function showPageNumGame(page){
		var i = 0;
		var html = '';
		var num = 24 ; // 显示条数
		var pagenum = page*num;
		i = (page - 1)*num ;
		//gameLength = 94
		//if(pagenum >= gameLength){pagenum = gameLength}
		pagenum = pagenum >= gameLength ? gameLength : pagenum ;
		i >= gameLength ? gameLength : (page - 1)*num ;
		for( i;i<pagenum;i++){
			var gamelink = "plugin.php?id=yxwg&a=game&gid="+gameJsons[i]['id'];
			html += '<li><a href="/'+gameJsons[i]['host']+'" target="_blank"><span>'+gameJsons[i]['name']+'</span><i class="icoundefined"></i></a></li>';
		}
		$(".all_games_list").html(html);
	}
	</script> 
