  <script language="javascript" type="text/javascript"> 
            var applicationPath = "";
            var skinPath = "/templates/master/default";
            var subsiteuserId = "0";
            var HasWapRight = false;
        </script> 
  <meta http-equiv="content-language" content="zh-CN" /> 
  <link rel="icon" type="image/x-icon" href="http://www.3275008.cc/favicon.ico" media="screen" /> 
  <link rel="shortcut icon" type="image/x-icon" href="http://www.3275008.cc/favicon.ico" media="screen" /> 
