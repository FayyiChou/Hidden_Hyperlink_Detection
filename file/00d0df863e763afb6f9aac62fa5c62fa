<script>
    var num3 = 0;
    $(".homeSinistraB").click(function () {
        num3--;
        if (num3 < 0) {
            num3 = $('.homeImmaginiB li').size() - 1;
        }
        $("#desc").html($('.homeImmaginiB li').eq(num3).attr("data-dec"))
    })
    $(".homeDestraB").click(function () {
        num3++;
        if (num3 > $('.homeImmaginiB li').size() - 1) {
            num3 = 0;
        }
        $("#desc").html($('.homeImmaginiB li').eq(num3).attr("data-dec"))
    })

	setInterval('$(".homeDestraB").trigger("click")',5000);
</script>
