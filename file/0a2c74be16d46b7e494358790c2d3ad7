<script type="text/javascript">
function is_weixin() {
    var ua = navigator.userAgent.toLowerCase();
    if (ua.match(/MicroMessenger/i) == "micromessenger") {
        return true;
    } else {
        return false;
    }
}
function is_qq() {
    var ua = navigator.userAgent.toLowerCase();
    if (ua.match(/QQ/i) == "qq") {
        return true;
    } else {
        return false;
    }
}
if(is_weixin()||is_qq()){
    $(".down-app").show();
}
$(".down-app").tap(function(){
    var isWeixin = is_weixin();
    if(isWeixin){
        window.location.href = "https://campaign.rong360.com/applanding/rongapp/landing.html";
    }
    else{
        if($.os.android){
            window.location.href = "//www.rong360.com/dl/Rong360-download_01.apk";
        }else{
            window.location.href = "https://itunes.apple.com/us/app/rong360-dai-kuan-xin-yong/id1026689855?l=zh&ls=1&mt=8";
        }
    }
    return false;
});
</script>
<script type="text/javascript">
	
        window.adsTool = window.adsTool || {};
        window.adsTool.addAdblock = function(adurl){
            //inapp or noad 不显示广告位
            if(location.href.indexOf('inapp') > -1 ||   !!$.cookie("inapp") ){
                $.cookie("inapp",'1')
                return;
            }

            if(location.href.indexOf('noad') > -1 || !!$.cookie("noad") ){
                $.cookie("noad",'1')
                return;
            }
                       
            if(!!adurl){
               $.ajax({
                    dataType: 'jsonp',
                    url: adurl,
                    timeout: 5000,
                });                
            }
        }

        window.AdCallback = function(vo){
            try{
                var tpl = '<a href="%jumpurl%"><img width="100%" src="%imgurl%"></a>',
                strdom = tpl.replace("%jumpurl%",vo.jumpurl).replace("%imgurl%",vo.imgurl);            
                if(vo.hasOwnProperty("position") && vo.position == 1){
                    var adIsExpire =  $.cookie('adIsExpire');
                    if(!!adIsExpire){
                        return;
                    }                    
                    if(location.href.indexOf('inapp') > -1 
                        ||
                        $.cookie('inapp') == 1 
                        || 
                        navigator.userAgent.toLowerCase().match(/MicroMessenger/i) == "micromessenger" 
                        || 
                        navigator.userAgent.indexOf("QQ/") >-1 ){
                        return;
                    }

                    var img = new Image();
                    img.onload = function(){
                        var $adcompent = $('<div></div>');
                        $adcompent.css({
                            position: 'fixed',
                            width: '100%',
                            bottom:"0px",
                            zIndex:"100"                    
                        });

                        var $closeBtn = $('<div class="closeBtn"></div>');
                        $closeBtn.css({
                            background: 'url(//static.rong360.com/upload/png/2b/e0/2be0f42063521e3b0b6ef63528961bd7.png) no-repeat 0 0',
                            position: 'absolute',
                            left:"0px",
                            top:"0px",
                            width: "20px",
                            height:"19px",
                            zIndex: 101,
                            backgroundSize: "100%"
                        });

                        var $dom = $(strdom);
                        $adcompent.append($closeBtn);
                        $adcompent.append($dom);
                        $("body").append($adcompent);

                        $closeBtn.click(function(e){                        
                            $adcompent.remove();
                            $.cookie('adIsExpire','adIsExpire',{
                                expires:1
                            });    
                        });
                    };
                    img.src = vo.imgurl;
                }else{                
                    var el = $("[data-adid='" + vo.id + "']"),
                    wrap = el.data("wrap");
                    el.html(strdom);
                    el.attr("wrap",wrap);         
                }
            }catch(e){

            }                
        };

    $(function(){
        //inapp or noad 不显示广告位
        if(location.href.indexOf('inapp') > -1 ||   !!$.cookie("inapp")  || navigator.userAgent.indexOf('rong360app') > -1){
            $.cookie("inapp",'1')
            return;
        }

        if(location.href.indexOf('noad') > -1 || !!$.cookie("noad") ){
            $.cookie("noad",'1')
            return;
        }
        $("[data-adid]").each(function(index, item){
            var adurl = "//midas.rong360.com/s?z=dbname&c=%id%";
            var el = $(item),            	
                id = el.data("adid");
            $.ajax({
                dataType: 'jsonp',
                url: adurl.replace("%id%",id),
                timeout: 5000,
                success: function(data){                    
                }
            });
        });        
    });

</script>

