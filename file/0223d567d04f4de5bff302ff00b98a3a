     $('#projects-search-form').submit();
  });
  $('#projects-search-form').submit(function(){
    query_name = $('.code-search-input').val()
    if(query_name != undefined && $.trim(query_name).length < 5){
      $('#search_top-search-input').focus();
      alert("代码搜索关键字不能少于5个");
      return false;
    }
  })
  
  // End
</script>

