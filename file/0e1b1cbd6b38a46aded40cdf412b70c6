                _chart.myChartbar.el.on('click', function(params) {
                    // 控制台打印数据的名称
                    console.log(params.dataIndex);
                    var num = params.dataIndex;
                    $(".owner-active").removeClass("owner-active");
                    _chart.myChartbar.select = num;
                    $("#hidMin").val(_chart.myChartbar.data.list[num].min * 10000);
                    $("#hidMax").val(_chart.myChartbar.data.list[num].max * 10000);
                    if (facprice > 0) {
                        if (_chart.myChartbar.data.list[num].min > facprice) {
                            _chart.columntips.text(_chart.myChartbar.data.list[num].min.toFixed(2) + "~" + _chart.myChartbar.data.list[num].max.toFixed(2) + "万区间共有" + _chart.myChartbar.data.list[num].total + "个裸车价，这个区间的价格相比厂商指导价" +  "高" + (_chart.myChartbar.data.list[num].min - facprice).toFixed(2) + "~"  + (_chart.myChartbar.data.list[num].max - facprice).toFixed(2) + "万");
                        } else if (_chart.myChartbar.data.list[num].max < facprice) {
                            _chart.columntips.text(_chart.myChartbar.data.list[num].min.toFixed(2) + "~" + _chart.myChartbar.data.list[num].max.toFixed(2) + "万区间共有" + _chart.myChartbar.data.list[num].total + "个裸车价，这个区间的价格相比厂商指导价低"  + (facprice - _chart.myChartbar.data.list[num].min).toFixed(2) + "~"  + (facprice - _chart.myChartbar.data.list[num].max).toFixed(2)   + "万");
                        } else {
                            _chart.columntips.text(_chart.myChartbar.data.list[num].min.toFixed(2) + "~" + _chart.myChartbar.data.list[num].max.toFixed(2) + "万区间共有" + _chart.myChartbar.data.list[num].total + "个裸车价，这个区间的价格相比厂商指导价" + (facprice > _chart.myChartbar.data.list[num].min ? ("低" + (facprice - _chart.myChartbar.data.list[num].min).toFixed(2)) : ("高" + (_chart.myChartbar.data.list[num].min - facprice).toFixed(2))) + "万~" + (facprice > _chart.myChartbar.data.list[num].max ? ("低" + (facprice - _chart.myChartbar.data.list[num].max).toFixed(2)) : ("高" + (_chart.myChartbar.data.list[num].max - facprice).toFixed(2))) + "万");
                        }
                    } else {
                        _chart.columntips.text(_chart.myChartbar.data.list[num].min.toFixed(2) + "~" + _chart.myChartbar.data.list[num].max.toFixed(2) + "万区间共有" + _chart.myChartbar.data.list[num].total + "个裸车价");
                    }
                    _chart.selectspantip.text("3个月内 " + _chart.myChartbar.data.list[num].min.toFixed(2) + "~" + _chart.myChartbar.data.list[num].max.toFixed(2) + "万 带发票").parent().parent().removeClass("fn-hide");

                    _chart.downplay(_chart.myChartbar);
                    _chart.highlight(_chart.myChartbar);


                    _page.pageIndex = 1;
                    _page.min = _chart.myChartbar.data.list[num].min * 10000;
                    _page.max = _chart.myChartbar.data.list[num].max * 10000;
                    _page.Invoice = 1;
                    _page.get();
                    //owner-active
                    setTimeout('window.scrollTo(0, $(".distributed-title").offset().top);', 200);
                    _trackEvent.push({ 'eid': '2|1211013|132|4168|203248|300701', 'val': 1 });

                });
                _chart.myChartline.el.on('click', function(params) {
                    var index = params.dataIndex;
                    _chart.myChartline.select = index;
                    _chart.chartresize();
                    if (facprice > 0) {
                        var lineendtime = _chart.myChartline.data[index].endTime;
                        if (facprice > _chart.myChartline.data[index].avgPrice)
                            _chart.linetips.text("截止到" + lineendtime.substr(0, 4) + "年" + lineendtime.substr(4, 2) + "月" + lineendtime.substr(6, 2) + "日，车主购车均价为" + (_chart.myChartline.data[index].avgPrice.toFixed(2)) + "万，低于厂商指导价" + (facprice - _chart.myChartline.data[index].avgPrice).toFixed(2) + "万");
                        else {
                            _chart.linetips.text("截止到" + lineendtime.substr(0, 4) + "年" + lineendtime.substr(4, 2) + "月" + lineendtime.substr(6, 2) + "日，车主购车均价为" + (_chart.myChartline.data[index].avgPrice.toFixed(2)) + "万，高于厂商指导价" + (_chart.myChartline.data[index].avgPrice - facprice).toFixed(2) + "万");
                        }
                    } else {
                        _chart.linetips.text("三个月内带发票车主购车均价为" + _chart.myChartline.data[index].avgPrice.toFixed(2) + "万");
                    }
                    window.scrollTo(0, $(".distributed-title").offset().top);
                    _trackEvent.push({ 'eid': '2|1211013|132|4168|203249|300704', 'val': 1 });
                });
            },
            clearcheck: function(parameters) {
                _page.pageIndex = 1;
                _page.min = 0;
                _page.max = 0;
                _page.Invoice = _chart.invoice();
                _page.get();
                $("#ccgcjj").addClass("owner-active");
                _chart.selectspantip.text("").parent().parent().addClass("fn-hide");
                //取消选中
                _chart.myChartbar.select = -1;
                _chart.downplay(_chart.myChartbar);
                _chart.columntips.text(_chart.myChartbar.tipstext);
            },
            invoice: function() {
                if ($("#any2").attr("checked")) {
                    return 1;
                } else {
                    return 0;
                }
            },
        };
        // 使用刚指定的配置项和数据显示图表。
        $(".distributed-title span").on("click", function() {
            _chart.chartresize();
        });
        window.onresize = function() { _chart.chartresize(); };


