<script type="text/javascript">
    $(function() {
        /**
         *根据面包屑调整导航栏背景指向
         */
        //console.log($('#breadcrumb').find('li:eq(1) a').html());

        if( $('#breadcrumb').length === 0) {
            $('.menu li:eq(0)').attr('id', 'current');
        }

        $('.menu li:not(li li)').each(function(index){
            if($(this).find('a').html() === $('#breadcrumb').find('li:eq(1) a').html()) {
                $(this).parent().find('#current').removeAttr('id');
                $(this).attr('id', 'current');
            }
        })
    })

 </script>
<script>
	var slider;
	var ii = 1;
	var totalRange;
	var moveRange = 102;
	function sliderF(){
	
	slider = setInterval(function(){
	
		if(ii==$('.ls-thumbnail-slide img').length){
			totalRange = 795;
			//$('.ls-thumbnail-slide').css('left',(totalRange+'px'));
			$('.ls-thumbnail-slide').animate({left:totalRange + 'px'},1500);
			ii = 1;
			
		} else {
			if(ii>7){
				
			}else {
				totalRange = 795-moveRange*(ii-1);
				//$('.imageDiv').css('left',(totalRange+'px'));
				$('.ls-thumbnail-slide').animate({left:totalRange + 'px'},1500);
				//console.log(totalRange);
				
			}
			ii++;
			
		}
	},5800);
	}
	
	//setTimeout(sliderF,1500);
	sliderF();
	
	$('.ls-layer').mouseover(function(){
			//clearInterval(active);
			clearInterval(slider);
		});
		
		$('.ls-layer').mouseout(function(){
			//clearInterval(active);
			sliderF();
		});
	
	
	function clickOnThumbNailImage(){
		$('.ls-thumbnail-slide img').each(function(i,o){
		$(this).click(function(){
		
		clearInterval(slider);
		ii = i+1;
		sliderF();
		});
	});
	}
	
	function which_is_active_image(){
		//判断哪个图片是激活状态
	$('.ls-thumbnail-slide img').each(function(i,o){
		if($(this).hasClass('ls-thumb-active')){
			//console.log('ddddddd');
			clearInterval(slider);
			ii = i+1;		
			
			if(ii==8){
				totalRange = 795;
				$('.ls-thumbnail-slide').animate({left:totalRange + 'px'},1500);
				ii = 1;
			}else {
				totalRange = 795-moveRange*(ii-1);
				$('.ls-thumbnail-slide').animate({left:totalRange + 'px'},1500);
			}
			
			ii++;
			sliderF();
		}
	});
	////
	}
	
	function which_is_active_image2(){
		//判断哪个图片是激活状态
	$('.ls-thumbnail-slide img').each(function(i,o){
		if($(this).hasClass('ls-thumb-active')){
			//console.log('ddddddd');
			clearInterval(slider);
			ii = i;		
			
			if(ii==0){
				ii = 8;
				totalRange = 795-moveRange*(7-1);
				$('.ls-thumbnail-slide').animate({left:totalRange + 'px'},1500);
			}else {
				totalRange = 795-moveRange*(ii-1);
				$('.ls-thumbnail-slide').animate({left:totalRange + 'px'},1500);
			}
						
			ii--;
			sliderF();
			ii++;
		}
	});
	////
	}
	
	function nav_prev_next(){
		$('.ls-nav-next').click(function(){
			which_is_active_image();
			//console.log('testtttt');
		});
		
		$('.ls-nav-prev').click(function(){
			which_is_active_image2();
			//console.log('testtttt');
		});
	}
	
	
	
	
	
	setTimeout(clickOnThumbNailImage,1000);
	setTimeout(nav_prev_next,1000);

</script>

  <!-- WP Super Cache is installed but broken. The constant WPCACHEHOME must be set in the file wp-config.php and point at the WP Super Cache plugin directory. -->