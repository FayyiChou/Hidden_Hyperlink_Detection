<script language="javascript" type="text/javascript">
	<!--
        var zbRoomID=0;
		//显示用户登陆或注册窗口

		function showLoginFrom(id)
		{
		    zbRoomID = id;
			ShowMask();//显示遮照层

			ShowFrom();
		}
		
		//创建并显示遮照层
		function ShowMask()
		{
			var newMask = document.createElement("div");
			newMask.id = 'MaskLayer';
			newMask.style.position = "absolute";
			newMask.style.zIndex = "1";
			_scrollWidth = Math.max(document.body.scrollWidth, document.documentElement.scrollWidth);
			_scrollHeight = Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);
			newMask.style.width = _scrollWidth + "px";
			newMask.style.height = _scrollHeight + "px";
			newMask.style.top = "0px";
			newMask.style.left = "0px";
			newMask.style.background = "#33393C";
			newMask.style.filter = "alpha(opacity=10)";
			newMask.style.opacity = "0.20";
			document.body.appendChild(newMask);
		}

		//关闭遮照层代码
		function CloseMask()
		{

			var docEle = function() {
			return document.getElementById(arguments[0]) || false;
            }
            try {
                document.body.removeChild(docEle('MaskLayer'));
            } catch (e) { }
		}
		
		//显示窗口
		function ShowFrom()
		{
			$("#LoginFrom").css("left",(Math.max(document.body.scrollWidth, document.documentElement.scrollWidth)-$("#LoginFrom").width())/2+"px");
			$("#LoginFrom").css("top", (($(window).height())-$("#LoginFrom").height())/2+$(document).scrollTop()+"px")
			$("#LoginFrom").fadeIn("slow");
			FreeReg_Click();
		}
		
		//显示注册用户信息
		function FreeReg_Click()
		{
			$("#User_LoginFrom").hide();
			$("#User_RegFrom").fadeIn("slow"); 
		}
		
        //显示用户登陆窗口
		function Login_Click()
		{
			$("#User_RegFrom").hide();
			$("#User_LoginFrom").fadeIn("slow"); 
		}

		//获取手机验证码

		function GetCheckCode() 
        {
            if (!IsNull($("#Reg_Mobile").val())) {
              
                    $(this).attr("disabled", true);
                    var me = this;
                    var parameter = {};
                    parameter["moble.m_Mobile"] = $("#Reg_Mobile").val();
                    $.ajax({
                        cache: false,
                        dataType: "json",
                        type: "POST",
                        data: parameter,
                        url: "/Ajax/GetAuthCode",
                        timeout: 5000,
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            alert("获手机认证码失败，请重新获取！");
                            $(me).attr("disabled", false);
                        },
                        success: function (data) {
                            if (parseInt(data.Data.m_RetCode) != 0) {
                                $(me).attr("disabled", false);
                            }
                            else {
                                setTimeout(function () {
                                    $(me).attr("disabled", false);
                                }, 30);
                            }
                            alert(data.Data.m_Message);
                        }
                    });  
            }
            else {
                alert("请填写手机号！");
            }
        }

        //用户注册按钮事件
        function UserRegistory() {
            var mobile, pwd, aucode, repwd, account;
            account = $("#Reg_Account").val();
            nicheng = $("#nicheng").val();
            mobile = $("#Reg_Mobile").val();
            aucode = $("#CheckCode").val();
            pwd = $("#Reg_Pwd").val();
            repwd = $("#Reg_RePwd").val();

            if (IsNull(account)) {
                alert("请填写用户注册账号!");
                $("#Reg_Account").focus();
                return false;
            }
			
			if (IsNull(nicheng)) {
                alert("请填写用户昵称!");
                $("#Reg_Account").focus();
                return false;
            }

            if (IsNull(mobile)) {
                alert("请填写用户注册手机号！");
                $("#Reg_Mobile").focus();
                return false;
            }

            if (!IsMobel(mobile)) 
            {
                alert("请填写正确的注册手机号！");
                $("#Reg_Mobile").focus();
                return false;
            }

            if (IsNull(pwd)) {
                alert("请填写用户注册密码！");
                $("#Reg_Pwd").focus();
                return false;
            }

            if (IsNull(repwd)) {
                alert("请填写重复密码！");
                $("#Reg_RePwd").focus();
                return false;
            }

            /*if (IsNull(aucode)) {
                alert("请填写手机验证码！");
                $("#CheckCode").focus();
                return false;
            }*/

            if (pwd != repwd) {
                alert("原始密码与重复密码不一致，请重新输入重复密码！");
                $("#Reg_RePwd").focus();
                $("#Reg_RePwd").select();
                return false;
            }
            return true;
        }

        //用户自动登陆
        function autoUserLogin() {
            var userName, userPwdj, autoLog;
            userName = $("#Mobile").val();
            userPwd = $("#Pwd").val();
            autoLog = $("#AutoLog").attr("checked");
             if (IsNull(userName)) {
                return;
            }

            if (IsNull(userPwd)) {
                return;
            }

            var me = this;
            var parameter = {};
            parameter["e.m_mobile"] = userName;
            parameter["e.m_pwd"] = userPwd;
            parameter["e.m_autolog"] = autoLog;

            $.ajax({
                cache: false,
                dataType: "json",
                type: "POST",
                data: parameter,
                url: "/Ajax/Login",
                timeout: 5000,
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    alert("自动登陆脚本运行出现异常，请刷新页面重试！");
                },
                success: function (data) {
                    if (data.Data.m_RetCode == 0) {
                        location.reload(true);
                    }
                    else {
                        alert(data.Data.m_Message);
                    }
                }
            });
        }

        //用户登陆
        function OnLogin() {
            var account, userPwd,autoLog;
            account = $("#Account").val();
            userPwd = $("#Pwd").val();

            if (IsNull(account)) {
                alert("请填写登陆账号！");
                $("#Account").focus();
                return false;
            }

            if (IsNull(userPwd)) {
                alert("请填写用户登陆密码！");
                $("#Pwd").focus();
                return false;
            }

            return true;
        }
		
		//用户登陆
        function OnLogins() {
            var account, userPwd,autoLog;
            account = $("#Accounts").val();
            userPwd = $("#Pwds").val();

            if (IsNull(account)) {
                alert("请填写登陆账号！");
                $("#Accounts").focus();
                return false;
            }

            if (IsNull(userPwd)) {
                alert("请填写用户登陆密码！");
                $("#Pwds").focus();
                return false;
            }

            return true;
        }
	-->
	</script>

