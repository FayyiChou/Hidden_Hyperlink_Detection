    //账号验证
    function CheckAccount(me) {
        if (IsNull($(me).val())) {
            ShowErrMessage("请填写登陆账号信息！", me);
            return;
        }
        $.ajax({
            cache: false,
            dataType: "json",
            type: "POST",
            data: "account=" + $(me).val(),
            url: "/Ajax/ExistsAccount",
            timeout: 5000,
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert("脚本运行出现异常，请刷新页面重试！");
            },
            success: function (data) {
                if (data.Data.m_RetCode == 0) {
                    alert("此账号可用！");
                }
                else {
                    ShowErrMessage(data.Data.m_Message, me);
                }
            }
        });
    }



    //账号验证
    function Check_Account(me) {
        if (IsNull($(me).val())) {
            alert("请填写登陆账号信息！");
            $(me).focus();
            return;
        }
        $.ajax({
            cache: false,
            dataType: "json",
            type: "POST",
            data: "account=" + $(me).val(),
            url: "/Ajax/ExistsAccount",
            timeout: 5000,
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert("脚本运行出现异常，请刷新页面重试！");
            },
            success: function (data) {
                if (data.Data.m_RetCode != 0) {
                    alert(data.Data.m_Message);
                    $(me).focus();
                }
            }
        });
    }

    //电话号码检测
    function CheckMobile(me) 
    {
        if (IsNull($(me).val())) {
            ShowErrMessage("请填写电话号码信息！", me);
            return;
        }
        if (!IsMobel($(me).val())) {
            ShowErrMessage("请填写正确的手机号码！", me);
            return;
        }
        HideErrMessage(me);
    }


    //验证码检测 
    function CheckCode(me) {
        if (IsNull($(me).val())) {
            ShowErrMessage("请填写验证码信息！", me);
            return;
        }
        HideErrMessage(me);
    }

    //密码检测 
    function CheckRegPwd(me) {
        if (IsNull($(me).val())) {
            ShowErrMessage("请填写账户登陆密码！", me);
            return;
        }

        if ($(me).val().length < 6) {
            ShowErrMessage("请填写字符长度大于6个字符的有密码！", me);
            return;
        }
        HideErrMessage(me);
    }

    //重复密码检测 
    function CheckRePwd(me) {
        CheckRegPwd(me);
        if ($(me).val() != $("#Reg_Pwd").val()) {
            ShowErrMessage("重复密码与密码不一至！", me);
            return;
        }
        HideErrMessage(me);
    }

    //隐藏错误提示信息
    function HideErrMessage(me) {
        try {
            var err = $(me).parent().parent().find(".ValurError");
            if (err != undefined) {
                $(err).hide();
            }
        } catch (e) { }
    }

    //错误信息显示
    function ShowErrMessage(errmsg, me) {
        try {
            alert(errmsg);
            $(me).focus();
        } catch (e) { }
    }
    /*function ShowErrMessage(errmsg, me) {
        try 
        {
            var err = $(me).parent().parent().find(".ValurError");
            var err_timerspacer = null;
            var err_IsShow = false;
            var err_counts = 3;
            if (err != undefined) {
                $(err).attr("title", errmsg);
                $(err).show();
                err_IsShow = true;
                err_timerspacer = setInterval(function () {
                    if (--err_counts >= 0) {
