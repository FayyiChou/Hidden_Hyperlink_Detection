    <script>
        $( function(){
            $(window).resize(function() {
                if($(this).width() <= 1000){
                    $("#header-outer-wrapper").addClass("mw1000");
                }
                else{
                    $("#header-outer-wrapper").removeClass("mw1000");
                }
            });
            $( "#contact_us" ).click( function(){
                var address = $( "div.ld-address" );
                if( $( address ).is( ":visible" ) ){
                    $( address ).hide();
                }
                else{
                    $( address ).show();
                }
            } );
        } );
