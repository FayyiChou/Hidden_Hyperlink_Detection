  <script>
  function sendContact() {
	   $.post("http://www.sceneii.net/assets/php/send_contact.php",
	   {
		   name: $("#contactname").val() + $("#name2").val(),
		   email: $("#contactemail").val() + $("#email2").val(),
		   mobile: $("#contactmobile").val() + $("#mobile2").val(),
		   dob: $("#contactmsgdob").val(),
       how: $("#contacthow").val(),
       subject: $("#subject").val(),
		   msg: $("#contactmsg").val() + $("#msg2").val()},
	   function(result){
       $(".contact_done").html("<h4>Thanks very much for getting in touch</h4>");
       $(".contact_footer").html("");
	   setTimeout("$('#contact').closeModal()",5000);
    });
  }


  function setSubject(subj) {
    $("#subject").val(subj);
  }
  </script>









  
  
  <script>
  function sendsaw() {
     $.post("http://www.sceneii.net/assets/php/send_saw.php",
     {
       sawname: $("#sawname").val(),
       sawemail: $("#sawemail").val(),
       sawdob: $("#sawdob").val(),
       sawwhy: $("#sawwhy").val()},
     function(result){
       $(".saw_done").html("<h4>Thanks very much</h4>");
       $(".contact_footer").html("");
     setTimeout("$('#saw2').closeModal()",5000);
    });
  }
</script>


