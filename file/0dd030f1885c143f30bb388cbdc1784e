<script>
 
var LZUtil=LZUtil||{
 $:function(id){return document.getElementById(id);},
 $N:function(name){return document.getElementsByName(name);},
 $T:function(tagName){return document.getElementsByTagName(tagName);},
 activeTabById : function(activeCssObj,elems,divs){
  var elemsObj = [];
  var divsObj = [];
  var elemlen = elems.length;
 
  for(var i = 0; i < elemlen; ++i)
  {
   elemsObj.push(typeof elems[i]=='object'?elems[i]:this.$(elems[i]));
   divsObj.push(typeof divs[i]=='object'?divs[i]: this.$(divs[i]));
  }
  for(var i = 0; i < elemlen; ++i)
  { 
   elemsObj["tabIndex"]=0;
   elemsObj[i]["tabIndex"] = i;
   if(i>0&&divsObj[i].style.display!='none')
   {
    divsObj[i].style.display='none'
   }
   elemsObj[i][activeCssObj.event] = function(){
       if(this.tabIndex==elemsObj["tabIndex"])
           return;
    divsObj[elemsObj["tabIndex"]].style.display='none';
    divsObj[this["tabIndex"]].style.display='block';
    elemsObj[elemsObj["tabIndex"]].className=activeCssObj.inactiveCss;
    elemsObj[this["tabIndex"]].className=activeCssObj.activeCss;
    elemsObj["tabIndex"]=this["tabIndex"];
   }
  }
 },
 switchElem:function(activeCssObj,elems,init_active_index){
  var elemlen = elems.length;  
  for(var i = 0; i < elemlen; ++i)
  { 
   elemsObj["tabIndex"]=0;
   elemsObj[i]["tabIndex"] = i;
   elemsObj[i][activeCssObj.event] = function(){
       if(this.tabIndex==elemsObj["tabIndex"])
           return;
    elemsObj[elemsObj["tabIndex"]].className=activeCssObj.inactiveCss;
    elemsObj[this["tabIndex"]].className=activeCssObj.activeCss;
    elemsObj["tabIndex"]=this["tabIndex"];
   }
  }
  elemsObj[0].className=activeCssObj.activeCss;
 }
};
 LZUtil.activeTabById({event:"onmouseover",
         activeCss:"sel1",
         inactiveCss:""},
                     ["tab1_t1","tab1_t2","tab1_t3"],
                 LZUtil.$("tab1_c").getElementsByTagName("div")
                     );
LZUtil.activeTabById({event:"onmouseover",
         activeCss:"sel2",
         inactiveCss:""},
                     ["tab2_t1","tab2_t2"],
                 LZUtil.$("tab2_c").getElementsByTagName("ul")
                     );
					 LZUtil.activeTabById({event:"onmouseover",
         activeCss:"sel3",
         inactiveCss:""},
                     ["tab3_t1","tab3_t2","tab3_t3","tab3_t4","tab3_t5"],
                 LZUtil.$("tab3_c").getElementsByTagName("li")
                     );
LZUtil.activeTabById({event:"onmouseover",
         activeCss:"green12b sel4",
         inactiveCss:"green12b"},
                     ["tab4_t1","tab4_t2"],
                ["tab4_c1","tab4_c2"]
                     );

</script>


<SCRIPT language=JavaScript type=text/javascript>

lastScrollY=0;
function heartBeat(){ 
var diffY;
if (document.documentElement && document.documentElement.scrollTop)
diffY = document.documentElement.scrollTop;
else if (document.body)
diffY = document.body.scrollTop

percent=.1*(diffY-lastScrollY); 
if(percent>0)percent=Math.ceil(percent); 
else percent=Math.floor(percent); 
document.getElementById("lovexin12").style.top=parseInt(document.getElementById
("lovexin12").style.top)+percent+"px";
document.getElementById("lovexin14").style.top=parseInt(document.getElementById
("lovexin12").style.top)+percent+"px";
lastScrollY=lastScrollY+percent; 
}

suspendcode12="<DIV id=\"lovexin12\" style='left:2px;POSITION:absolute;TOP:40px;'><a href='http://www.gzhnews.com/' target=_blank><img src='/img/duilian左.gif' ></a><br><img src='/img/close.gif' style='cursor:hand;' onClick=\"document.getElementById('lovexin12').style.display='none';\"></div>"
suspendcode14="<DIV id=\"lovexin14\" style='right:2px;POSITION:absolute;TOP:40px;'><a href='http://www.gzhnews.com/' target=_blank><img src='/img/duilian右.gif' ></a><br><img src='/img/close.gif' style='cursor:hand;' onClick=\"document.getElementById('lovexin14').style.display='none';\"></div>"
document.write(suspendcode12); 
document.write(suspendcode14); 
window.setInterval("heartBeat()",1);//

</script>
