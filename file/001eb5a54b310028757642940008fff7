		<!--底部删除信息框-->
		
	<script type="text/javascript" src="/wap/html5_resources/system_js/mui.min.js?v=20170328"></script>
	<script type="text/javascript" src="/wap/html5_resources/system_js/app.js?v=20170328"></script>
	<script type="text/javascript" src="/wap/html5_resources/system_js/jquery.js?v=20170328"></script>
		<script>
			mui.init();
			mui('#contentscroll').scroll();
			mui('.mui-bar-tab').on('tap','a',function(){document.location.href=this.href;});
			function deleteMail() {
				if(""=="star") {
					$.post("starMark.wo",{_label:0,_mid:34378,_fid:1},function(data){
						if(data=="success"){			
						}else{ 
							mui.toast("删除邮件时，取消星标标记失败");
							return false;
						}
				    });
				}
				document.location.href="/wap/mailbox/mailHandle.wo?sessionid=096306956252575515651610820:0&delete=delete&starMail=&fid=1&name=&messageIDs=34378&pageNum=1";
			}
			$(function(){
				$(".mwap-content-headerbg").outerHeight();
				$("#contentscroll").css("margin-top",$(".mwap-content-headerbg").height()+45);
			})
			window.addEventListener('tap', function(event) {
				if(event.target.classList.contains("mui-navigate-right")) {
					$("#contentscroll").css("margin-top",$(".mwap-content-headerbg").height()+45);
				}
			})
			function showMoreAtt() {
				document.getElementById('moreAtt').style.display='block';
				document.getElementById('moreAttBtn').style.display='none';
			}
			function bodyResize(){
				var mailBodyWidth = $("#mailBody").width();
				var scrWidth = $(document.body).width();
				if(mailBodyWidth > scrWidth){
					var ratio = (scrWidth-20)/mailBodyWidth;
					if(ratio<0.3) return;
					$("#mailBody").css("-webkit-transform","scale3d("+ratio+","+ratio+",1)").css("-webkit-transform-origin","0px 0px 0px");
					var fid="1";
					if(fid!=2){
						$("#mailBodyBox").css("height",$("#mailBody").height()*ratio+95).css("overflow-y","srcoll").css("overflow-x","hidden");
					}else{
					    $("#mailBodyBox").css("height",$("#mailBody").height()*ratio+20).css("overflow-y","srcoll").css("overflow-x","hidden");
					}
				}
			}
			function imgLoaded(){
				var imgdefereds = [];
				$("#mailBody img").each(function() {
					var dfd = $.Deferred();
					$(this).bind('load', function() {dfd.resolve();}).bind('error', function() {dfd.resolve();});
					if (this.complete) dfd.resolve();
					imgdefereds.push(dfd);
				});
				$.when.apply(null, imgdefereds).done(function() {
					bodyResize();
				});
			}
			$().ready(function() {
				imgLoaded();
			});
		</script>
	
</div></body></html>