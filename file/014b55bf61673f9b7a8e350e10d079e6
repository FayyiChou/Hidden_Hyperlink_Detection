                <script>
                    var speed = 30//速度数值越大速度越慢
                    var colee_right5 = document.getElementById("colee_right5");
                    var colee_right4 = document.getElementById("colee_right4");
                    var colee_right3 = document.getElementById("colee_right3");
                    colee_right5.innerHTML = colee_right4.innerHTML
                    function Marquee5() {
                        if (colee_right3.scrollLeft <= 0)
                            colee_right3.scrollLeft += colee_right5.offsetWidth
                        else {
                            colee_right3.scrollLeft--
                        }
                    }
                    var MyMar5 = setInterval(Marquee5, speed)
                    colee_right3.onmouseover = function() {
                        clearInterval(MyMar5)
                    }
                    colee_right3.onmouseout = function() {
                        MyMar5 = setInterval(Marquee5, speed)
                    }
                </script>    
