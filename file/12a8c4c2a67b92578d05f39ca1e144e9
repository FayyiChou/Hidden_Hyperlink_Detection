<script>
    var mySwiper = new Swiper('.swiper-container',{
        grabCursor: true,
        autoplay: 5000
    })
    $('.left-arrow').on('click', function(e){
        e.preventDefault()
        mySwiper.swipePrev()
    })
    $('.right-arrow').on('click', function(e){
        e.preventDefault()
        mySwiper.swipeNext()
    })
    $(function(){
        $(".swiper-slide").on("click",function(){
            var href_url = $(this).find('a').attr('href');
            location.href = href_url;
        });
    })
</script>

