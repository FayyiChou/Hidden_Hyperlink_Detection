<script>



	var width_temp = "";  //临时宽度
	var periods_width=$(".periods").width();
	var periods_more_width=$(".periods_more").width();
	if(periods_width>periods_more_width)
		width_temp=periods_width;
	else
		width_temp=periods_more_width;
	$(".periods").width(width_temp);
	$(".periods_more").width(width_temp);
	$(".periods0").hover(
			function(){
				$(".periods_more").css("visibility","visible");
				$(".periods").addClass("p_cur");
			},
			function(){
				$(".periods_more").css("visibility","hidden");
				$(".periods").removeClass("p_cur");
			}
	)
	$(".p_m_items").mouseover(function(){
		$(this).addClass("cur");
	});
	$(".p_m_items").mouseout(function(){
		$(this).removeClass("cur");
	});


var starMap=1;
</script>

