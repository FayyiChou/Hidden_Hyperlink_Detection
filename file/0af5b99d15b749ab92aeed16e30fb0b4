<!-- 价格走势图 -->
<script type="text/javascript">
require.config({
paths:{
echarts: '/js/echarts'
}
});
<!-- 二手房房价走势图 -->
function graph(type) {
require(
[
'echarts',
'echarts/chart/line'
],
function (ec) {
var url="";
var loc="";
var name="";
var danwei="";
if(type==1){
url='/xiaoqu/charDataLineNew.do';
loc="ie_echart"+type;
name="单位:元/平方米";
danwei="元/平方米";
}if(type==2){
url='/xiaoqu/charDataLine_leaseNew.do';
loc="ie_echart"+type;
name="单位:元/月";
danwei="元/月";
}
var myChart = ec.init(document.getElementById(loc));
myChart.clear();
var option={
tooltip : {
trigger: 'axis',
formatter:function(params){
if(params.length>0){
var str=params[0].name+'</br>';
for(var i in params){
str+=(params[i].value=="-"?"":params[i].seriesName+':'+(params[i].value==""?"无数据":params[i].value+danwei)+'</br>');
}
return str;
}
},
textStyle:{
align:'left'
}
},
legend: {
padding:0,
x : 300,
y : 0,
data:[]
},
grid:{
x:60,y:20,x2:10,y2:23
},
xAxis : [
{
type : 'category',
boundaryGap : false,
data : [],
splitLine:false,
axisLabel: {
interval:1
},axisTick:{
show:true,
interval:function(value){
return true;
}
}
}
],
yAxis : [
{
name : name,
min:0,
max:20000,
type : 'value',
axisLabel : {
formatter: '{value}'
}
}
],
series : []
};
$.ajax({
url:url,
type:'post',
dataType: 'json',
data:{"floorid":'3116709'},
success:function(data){
option.yAxis[0].min=data[3].min;
option.yAxis[0].max=data[4].max;
option.legend.data=data[1].legend;
option.xAxis[0].data = data[2].xAxis;
option.series = data[0].series;
myChart.setOption(option,true);
}
});
}
);
}
graph(1);
</script>
<script type="text/javascript" defer="defer">
var flag=false;
$("#fjzs li").mouseover(
function(){
if($(this).text()=="二手房房价走势"&&flag){
flag=false;
graph(1);
}
if($(this).text()=="租金走势"&&!flag){
flag=true;
graph(2);
}
});
$("#fjzs li").mouseleave(
function(){
});
</script>
