<script language="JavaScript" type="text/JavaScript">
function searchDeal(){
	var keyword = $("#searchText").val();
	if(keyword=="") keyword=null;
	window.open("/websearch_1_"+keyword+".jhtml");
}
function doCkeck(type){
	$('#searchMode').val(type);
	var keystr=document.getElementById("keyword");
	if(!keystr.value){
		alert("请输入检索关键词");
		document.getElementById("keyword").focus();	  
		//return false;
	}

	if(type=='adv'){
		var choosefield = $('input:radio[name=searchfile]:checked').val();
		if(!choosefield){
			alert('请选择搜索的项！');
			return false;
		}
		$('#keyword').val(choosefield+":"+keystr.value);

		var action1 = $("#searchForm").attr("action");
		var action2 = action1 + "?newsTypeID=29&searchMode="+type+"&keyword="+$('#keyword').val();
		$("#searchForm").attr("action",action2);
		document.getElementById('searchForm').submit();
		//return true;

	}
	if(type=='single'){
		  document.getElementById('searchForm').submit();
		  //return true;
	}
} 
function doCkeck1(type){
	$('#searchMode').val(type);
	var keystr=document.getElementById("keyword");
	if(!keystr.value){
		alert("请输入检索关键词");
		//document.getElementById("keyword").focus();	  
		return false;
	}

	if(type=='adv'){
		
		$('#keyword').val("title"+":"+keystr.value);

		var action1 = $("#searchForm").attr("action");
		var action2 = action1 + "?newsTypeID=29&searchMode="+type+"&keyword="+$('#keyword').val();
		$("#searchForm").attr("action",action2);
		//document.getElementById('searchForm').submit();
		return true;

	}
	if(type=='single'){
		  //document.getElementById('searchForm').submit();
		  return true;
	}
} 

 　document.onkeydown = function(evt){
   　 var evt = window.event?window.event:evt;
    　if (evt.keyCode==13) {
          var flag = doCkeck1('adv');
		  if(flag){
			document.getElementById('searchForm').submit();
		  }
    　}
   }
$(function() {
	$('#keyword').val('');

});	
