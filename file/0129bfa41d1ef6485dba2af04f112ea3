var maxlimit=10000;
$('#message_content').keyup(function() {
	data = document.getElementById("message_content").value.length;
	if (data > maxlimit)
	{
		document.getElementById("message_content").value = document.getElementById("message_content").value.substring(0, maxlimit);
	}
	else
	{
		var len_rem = maxlimit - data;
		$('#remLen').val(len_rem);
