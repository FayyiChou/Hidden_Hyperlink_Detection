                  <td width="32"><div align="center"><img src="images/moveright.jpg" width="15" height="60" onMouseOver="goright();" style="cursor:hand;" alt="向右"></div></td>
                </tr>
<input type="hidden" name="goon" id="goon" value="" />
<script>
function goleft(){
	document.all('goon').value="";
}
function goright(){
	document.all('goon').value="1";
}

  var speed=30
  demo2.innerHTML=demo1.innerHTML
 demo.scrollLeft=demo.scrollWidth
  function Marquee(){
  
  if(document.all('goon').value==""){
  if(demo2.offsetWidth-demo.scrollLeft<=0)
  demo.scrollLeft-=demo1.offsetWidth
  else{
  demo.scrollLeft++
  }
  }
  
  if(document.all('goon').value=="1"){
  if(demo.scrollLeft<=0)
  demo.scrollLeft+=demo2.offsetWidth
  else{
  demo.scrollLeft--
  }
  }
  
  }
  var MyMar=setInterval(Marquee,speed)
  demo.onmouseover=function() {clearInterval(MyMar)}
  demo.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
  </script>
