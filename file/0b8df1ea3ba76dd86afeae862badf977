<script>
$(function(){
    var fenmian = $('#fenmian').val();
    if(fenmian != ''){
    	$("body").css("background-image","url("+fenmian+")");
    }
});

function page(obj){
    var page_html = $(obj).html().replace('...','');
    var type = $(obj).parent().attr("class");
    if(type == 'pages'){
        var page_html = $(obj).html().replace('...','');
        if(page_html=='上一页'){
            page = parseInt($(".pages a.current").html())-1;
        }else if(page_html=='下一页'){
            page = parseInt($(".pages a.current").html())+1;
        }else{
            page = parseInt(page_html);
        }
    }else{
        var page = 1;
    }
    var url = $("#host").val();
    var pic_host = $("#pic_host").val();
    var ajax = 1;
    $.ajax({
        type:'get',
        url:url,
        data:{ajax:ajax,page:page},
        success:function(res){
            var res = eval('('+res+')');
            if(res.code==1){
                var html='';
                for(var i=0;i<res.count;i++){
                	html += '<dd>';
                    html += '<h5><a href="'+res['list'][i]['link']+'" target="_blank">'+res['list'][i]['title']+'</a></h5>';
                    if(res['list'][i]['small_pic']!=''){
                    html += '<div class="left"><img src="'+pic_host+res['list'][i]['small_pic']+'" /></div>';
                    }else{
                    html += '<div class="left"><img src="" /></div>';
                    }
                    html += '<div class="right">';
                    html += '<p>'+res['list'][i]['simplecon']+'...</p>';
                    html += '<span>更新时间：';
                    html += '<b>'+res['list'][i]['updatetime_str']+'</b>';
                    html += '<a href="'+res['list'][i]['link']+'" target="_blank">查看详情</a>';
                    html += '</span>';
                    html += '</div>';
                    html += '</dd>';
                }
                $('.content_list').html(html);
                $('.pages').html(res.page_html);
                $("html,body").animate({scrollTop: $(".act").offset().top}, 1000);
            }else{
                $('.content_list').html('<dd><h5><a>'+res.msg+'</a></h5></dd>');
            }
        }
    });
}
</script>
