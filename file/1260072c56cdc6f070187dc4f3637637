    msg += "原因: \n";

    old_get('feedback_folder_title').value = title;
    old_get('feedback_folder_title').name  = 'title';
    old_get('feedback_folder_contents').value = msg;
    old_get('feedback_folder_contents').name  = 'contents';
    old_get('feedback_folder_form').submit();
    old_get('feedback_folder_title').name  = '';
    old_get('feedback_folder_contents').name  = '';
}
