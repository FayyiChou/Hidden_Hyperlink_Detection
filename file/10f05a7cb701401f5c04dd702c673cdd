	<script type="text/javascript" src="http://shouyou.3dmgame.com/home/tpl/pc/js/game_lib/ku.js"></script>
	<script type="text/javascript">
		$(function() {

			$("#searchSelected").click(function() {
				$("#searchTab").show();
				$(this).addClass("searchOpen");
			});

			$("#searchTab li").hover(function() {
				$(this).addClass("selected");
			}, function() {
				$(this).removeClass("selected");
			});

			$("#searchTab li").click(function() {
				$("#searchSelected").html($(this).html());
				$("#searchTab").hide();
				$("#searchSelected").removeClass("searchOpen");
			});

		});
	</script>
	<script type="text/javascript">
		$(document).ready(function () { 
    //TotalNum 总数　这里写死了 
    var TotalNum = 200; 
    //首次加载 
    //分页事件 
	
    $("#Pagination").pagination("96", { 
        prev_text: "上一页", 
        next_text: "下一页", 
		current_page:0,
        num_edge_entries: 2, 
        num_display_entries: 3, 
        //回调 
        callback: pageselectCallback 
    }); 
 
    function pageselectCallback(page) { 
		var num = page+1;
		window.location.href = "http://shouyou.3dmgame.com/g/1_0_0_0_0_0_0_0_" + num + "_t/";
    } 
}); 
		 
	</script>
	<script>
		$('.bodyer-right-phb li').mousemove(function(){
			$('u').css('margin-top','')
			$('.dowl-game').css('margin-top','')
			$('.game-pic').hide();
			$(this).find('u').css('margin-top','30px')
			$(this).find('.dowl-game').css('margin-top','36px')
			$(this).find('p').css({'margin':'-6px 0 0 0','color':'#000','font-weight':'bold','padding-top':'6px'})
			$(this).find('.game-pic').show()
			$(this).find('span').addClass("phbmove");
			$(this).find('.dowl-game').css('background','#26AFDC')
			$(this).find('.dowl-game a').css('color','#ffffff')

			})
		$('.bodyer-right-phb li').mouseenter(function(){
			$('u').css('margin-top','')
			$('.dowl-game').css('margin-top','')
			$('.game-pic').hide();
			$('span').removeClass("phbmove");
			$('p').css({'margin':'','color':'','font-weight':'','padding-top':''})
			$('.dowl-game').css('background','')
			$('.dowl-game a').css('color','')
			})
	</script>
<script>

var leibie = '';
var leixing = '';
var ticai = '';
var lang = '';
var system = '';
var t_sort = '';
var p_sort = '';
var h_sort = '';

function search_keywords(num,type){ //查询链接
	var str = '';
	switch(type){
		case 'lb':
			str += num+'_';
			system = $('#xitong a[class=n]').attr('value');
			ticai = $('#lables a[class=n]').attr('value');
			lang = $('#yuyan a[class=n]').attr('value');
			leixing = $('#leixing a[class=n]').attr('value');
			//t_sort = $('#t_sort').attr('value');
			//p_sort = $('#p_sort').attr('value');
			//h_sort = $('#h_sort').attr('value');
			t_sort = 0;//全部都按desc
			p_sort = 0;//全部都按desc
			h_sort = 0;//全部都按desc
			str = str + system + '_' + ticai+ '_' + lang + '_' + leixing + '_' + t_sort + '_' + p_sort + '_' + h_sort + '_1_' + 't';
			break;
		case 'sy':
			str += num+'_';
			leixing = $('#leixing a[class=n]').attr('value');
			leibie = $('#leibie a[class=n]').attr('value');
			ticai = $('#lables a[class=n]').attr('value');
			lang = $('#yuyan a[class=n]').attr('value');
			//t_sort = $('#t_sort').attr('value');
			//p_sort = $('#p_sort').attr('value');
			//h_sort = $('#h_sort').attr('value');
			t_sort = 0;//全部都按desc
			p_sort = 0;//全部都按desc
			h_sort = 0;//全部都按desc
			str = leibie + '_' + str + ticai+ '_' + lang + '_' + leixing + '_' + t_sort + '_' + p_sort + '_' + h_sort + '_1_' + 't';
			break;
		
		case 'tc':
			str += num+'_';
			leixing = $('#leixing a[class=n]').attr('value');
			leibie = $('#leibie a[class=n]').attr('value');
			system = $('#xitong a[class=n]').attr('value');
			lang = $('#yuyan a[class=n]').attr('value');
			//t_sort = $('#t_sort').attr('value');
			//p_sort = $('#p_sort').attr('value');
			//h_sort = $('#h_sort').attr('value');
			t_sort = 0;//全部都按desc
			p_sort = 0;//全部都按desc
			h_sort = 0;//全部都按desc
			str = leibie + '_' + system + '_' + str + lang + '_' + leixing + '_' + t_sort + '_' + p_sort + '_' + h_sort + '_1_' + 't';
			break;
		
		case 'la':
			str += num+'_';
			leixing = $('#leixing a[class=n]').attr('value');
			leibie = $('#leibie a[class=n]').attr('value');
			system = $('#xitong a[class=n]').attr('value');
			ticai = $('#lables a[class=n]').attr('value');
			//t_sort = $('#t_sort').attr('value');
			//p_sort = $('#p_sort').attr('value');
			//h_sort = $('#h_sort').attr('value');
			t_sort = 0;//全部都按desc
			p_sort = 0;//全部都按desc
			h_sort = 0;//全部都按desc
			str = leibie + '_' + system + '_' + ticai+ '_' + str + leixing + '_' + t_sort + '_' + p_sort + '_' + h_sort + '_1_' + 't';
			break;
		case 'nav':
			str += num+'_';
			leixing = $('#leixing a[class=n]').attr('value');
			leibie = $('#leibie a[class=n]').attr('value');
			system = $('#xitong a[class=n]').attr('value');
			ticai = $('#lables a[class=n]').attr('value');
			lang = $('#yuyan a[class=n]').attr('value');
			//t_sort = $('#t_sort').attr('value');
			//p_sort = $('#p_sort').attr('value');
			//h_sort = $('#h_sort').attr('value');
			t_sort = 0;//全部都按desc
			p_sort = 0;//全部都按desc
			h_sort = 0;//全部都按desc
			str = leibie + '_' + system + '_' + ticai+ '_' + lang + '_' + str + t_sort + '_' + p_sort + '_' + h_sort + '_1_' + 't';
			break;
		case 't_sort':
			if(num == 0){
				num = 1;
			}else{
				num = 0;
			}
			//str += num+'_';
			leixing = $('#leixing a[class=n]').attr('value');
			leibie = $('#leibie a[class=n]').attr('value');
			system = $('#xitong a[class=n]').attr('value');
			ticai = $('#lables a[class=n]').attr('value');
			lang = $('#yuyan a[class=n]').attr('value');
			//p_sort = $('#p_sort').attr('value');
			//h_sort = $('#h_sort').attr('value');
			//t_sort = num;
			t_sort = 0;
			p_sort = 0;//全部都按desc
			h_sort = 0;//全部都按desc
			str = leibie + '_' + system + '_' + ticai+ '_' + lang + '_' + leixing + '_' + t_sort + '_' + p_sort + '_' + h_sort + '_' + '1' + '_' + 't';
			break;
		case 'p_sort':
			if(num == 0){
				num = 1;
			}else{
				num = 0;
			}
			//str += num+'_';
			leixing = $('#leixing a[class=n]').attr('value');
			leibie = $('#leibie a[class=n]').attr('value');
			system = $('#xitong a[class=n]').attr('value');
			ticai = $('#lables a[class=n]').attr('value');
			lang = $('#yuyan a[class=n]').attr('value');
			//t_sort = $('#t_sort').attr('value');
			//h_sort = $('#h_sort').attr('value');
			//p_sort = num;
			t_sort = 0;//全部都按desc
			h_sort = 0;//全部都按desc
			p_sort = 0;
			str = leibie + '_' + system + '_' + ticai+ '_' + lang + '_' + leixing + '_' + t_sort + '_' + p_sort + '_'  + h_sort + '_' + '1' + '_' + 'p';
			break;
		case 'h_sort':
			if(num == 0){
				num = 1;
			}else{
				num = 0;
			}
			//str += num;
			leixing = $('#leixing a[class=n]').attr('value');
			leibie = $('#leibie a[class=n]').attr('value');
			system = $('#xitong a[class=n]').attr('value');
			ticai = $('#lables a[class=n]').attr('value');
			lang = $('#yuyan a[class=n]').attr('value');
			//t_sort = $('#t_sort').attr('value');
			//h_sort = num;
			//p_sort = $('#p_sort').attr('value');
			t_sort = 0;//全部都按desc
			h_sort = 0;
			p_sort = 0;//全部都按desc
			str = leibie + '_' + system + '_' + ticai+ '_' + lang + '_' + leixing + '_' + t_sort + '_' + p_sort + '_' +  h_sort + '_' + '1' + '_' + 'h';
			break;
		default:
			break;
	}
	window.location.href = "http://" + window.location.host + '/g/' +str + '/' ;
}

</script>
<script>
	 // pages 居中
        $(window).ready(function(e) {
			var width = $('.pages').width()/2;
           //alert(width)
		   $('.pages').css('margin-left',400- width)
        })
