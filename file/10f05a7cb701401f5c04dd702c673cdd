								<script>
									var arr;
									var len
									function search_nav(num,type){
										search_keywords(num,type);
										return 0;
										var id = num;
										var type = type;
										$.post('index.php?act=search&op=nav',{id:id,type:type},function(data){
											if(data.code == 1){
												arr = data.data;
												len = arr.length;
												var html = '';
												html += '<a class="n" id="lables_0" onclick="lables(0,\'tc\')" value="0">全部</a>';
												for(var i=0;i<len;i++){
													html += '<a class="m" id="lables_'+arr[i].id+'"  onclick="lables('+arr[i].id+',\'tc\')" value="'+arr[i].id+'">'+arr[i].name+'</a>';
												}
												if(html != ''){
													$('#lables').html(html);
												}
											}
											
										},'json');
									}
									
									function lables(num,type){
										var html = '';
										if(num == 0){
											html += '<a class="n" id="lables_0" onclick="lables(0,\'tc\')" value="0" >全部</a>';
											for(var i=0;i<len;i++){
												html += '<a class="m" id="lables_'+arr[i].id+'"  onclick="lables('+arr[i].id+',\'tc\')">'+arr[i].name+'</a>';
											}
										}
										if(num != 0){
											html += '<a class="m" id="lables_0" onclick="lables(0,\'tc\')" value="0" >全部</a>';
											for(var i=0;i<len;i++){
												if(num == arr[i].id){
													html += '<a class="n" id="lables_'+arr[i].id+'"  onclick="lables('+arr[i].id+',\'tc\')" value="'+arr[i].id+'" >'+arr[i].name+'</a>';
												}else{
													html += '<a class="m" id="lables_'+arr[i].id+'"  onclick="lables('+arr[i].id+',\'tc\')" value="'+arr[i].id+'" >'+arr[i].name+'</a>';
												}
												
											}
										}
										if(html != ''){
											$('#lables').html(html);
										}
										search_keywords(num,type);
									}
									
								</script>
