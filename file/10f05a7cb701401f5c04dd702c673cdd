							<script>
							$(window).ready(function(e) {
								var px_tm =$("#t_sort").attr("value")
								var px_pf =$("#p_sort").attr("value")
								var px_rd =$("#h_sort").attr("value")
							   if( px_tm == 0){
								$('#t_sort').html("时间排序↓")
								}else{
									$('#t_sort').html("时间排序↑")
									}
								if( px_pf == 0){
								$('#p_sort').html("评分排序↓")
								}else{
									$('#p_sort').html("评分排序↑")
									}
								if( px_rd == 0){
								$('#h_sort').html("热度排序↓")
								}else{
									$('#h_sort').html("热度排序↑")
								}
