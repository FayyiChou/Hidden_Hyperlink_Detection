                        function secHoverLi(DivName, objectName, objectTable, n) {
                            titles = $("#" + objectName + ">li");
                            tables = $("#" + DivName + ">[name=" + objectTable + "]");
                            titles.each(function (i) {
                                if (i == n) {
                                    titles.eq(i).removeClass().addClass("nowtag");
                                    tables.eq(i).show();
                                } else {
                                    titles.eq(i).removeClass();
                                    tables.eq(i).hide();
                                }
                            });
                        }
