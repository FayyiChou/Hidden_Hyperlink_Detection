    <script type="text/javascript">
     var wait;
	$(document).ready(function(){
       $(".yz_z_xln").click(function(){
           $(".pl_yz").slideToggle();
       });
  
   $(".yz_srsj_hqyzm").click(function(){
       $(".yz_srsj_hqyzm").hide();
       $(".yz_srsj_zzhq").show();
       wait=setInterval(getnums, 1000);  
    });
});

 function getnums(){
    if(($("#times").text()-0)>0)
      {   
         $("#times").text($("#times").text()-1);
      } 
    else
      {
         $(".yz_srsj_hqyzm").show();
         $(".yz_srsj_zzhq").hide();
         $("#times").text("60");
         clearInterval(wait);
      } 
};
function validUser()
{
  var cookies=getCookievalue('21rednet');
  var userid="";
  if(cookies!=null)
  {
    userid= cookies.split('&')[0].split('=')[1];
  }
$.ajax({
        type : "get",
        //async : false,
        url : "http://pinglun.hongxiu.com/wap/isuserbind.ashx?uid="+userid,
       
       // data : "",
       // cache : false, //默认值true
        dataType : "jsonp",
        //jsonp: "callback",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
        jsonpCallback:"sendOrSubmit",
            //自定义的jsonp回调函数名称，默认为jQuery自动生成的随机函数名
            //如果这里自定了jsonp的回调函数，则success函数则不起作用;否则success将起作用
        success : function(data){
                console.log(data);

        },
        error:function(){
               console.log("erroe");
        }
    });
}

function sendOrSubmit(result)
{
    if(result==true)
    {
         $("#cmtbackurl").val("http://"+document.domain+"/aspxnovel/framev4/commentok.htm");
	    commentform.submit();
    }
    else
    {
        popUpValid();
    }
}

function sendVaild()
{   
    $.ajax({
        type : "get",
        async : false,
        url : "http://pinglun.hongxiu.com/wap/sendmess.ashx", //实际上访问时产生的地址为: ajax.ashx?callbackfun=jsonpCallback&id=10
        data : {mobile : $("#mobile").val()},
        cache : false, //默认值true
        dataType : "jsonp",
        jsonp: "callbackfun",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
        jsonpCallback:"jsonpCallback",
            //自定义的jsonp回调函数名称，默认为jQuery自动生成的随机函数名
            //如果这里自定了jsonp的回调函数，则success函数则不起作用;否则success将起作用
        success : function(json){
        console.log(json);
//            alert(json.message);
        },
        error:function(){
            alert("erroe");
        }
    });
    
}
function submitCode()
{
 var cookies=getCookievalue('21rednet');
  var userid="";
  if(cookies!=null)
  {
    userid= cookies.split('&')[0].split('=')[1];
  }
   $.ajax({
        type : "get",
        async : false,
        url : "http://pinglun.hongxiu.com/wap/checkcode.ashx", //实际上访问时产生的地址为: ajax.ashx?callbackfun=jsonpCallback&id=10
        data : {mobile : $("#mobile").val(),
                vcode:$("#vcode").val(),
                uid:userid
        },
        cache : false, //默认值true
        dataType : "jsonp",
        //jsonp: "callbackfun",//传递给请求处理程序或页面的，用以获得jsonp回调函数名的参数名(默认为:callback)
        jsonpCallback:"pl_close",
            //自定义的jsonp回调函数名称，默认为jQuery自动生成的随机函数名
            //如果这里自定了jsonp的回调函数，则success函数则不起作用;否则success将起作用
        success : function(result){
      
        },
        error:function(){
            alert("erroe");
        }
    });

}
function pl_close(result)
{
 if(result==true)
   {
       jQuery('.pl_yz').hide();
       pl_submitv3();
   }
 else
   {
             
   } 
}
function popUpValid()
{
jQuery('.pl_yz').show();
}

function getCookievalue(name)
{
var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
if(arr=document.cookie.match(reg))
return unescape(arr[2]);
else
return null;
}
