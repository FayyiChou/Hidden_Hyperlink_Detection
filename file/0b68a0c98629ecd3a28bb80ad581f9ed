<script type="text/javascript">
    function tab(obj){
        var index=$(obj).attr("index");
        $(obj).siblings().removeClass("tab_on");
        $(obj).addClass("tab_on");
        $(".tab_box ul").hide();
        $(".tab_box ul").eq(index).show();
        
    }
    $(function(){
        $(".tab_menu li").on("click",function(){
            var _this=this;
            tab(_this);
        })
    })
</script>
 
 
