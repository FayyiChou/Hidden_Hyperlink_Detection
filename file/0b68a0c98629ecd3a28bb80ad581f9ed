<script>
function setoby(k){
	var url = window.location.href;
	if(url.indexOf('?')>0){
		url=url.substring(0,url.lastIndexOf('?'));
	}
	//alert(url);
	if(url.indexOf('/oby_')>-1){
		url = url.replace(/\/oby_(\w+)/, '/oby_'+k);
	}else{
		if(url.indexOf('/jiaxiao')>-1){
			
		}else{
			url = '/jiaxiao/';
		}
		if(url.indexOf('/k_')>-1){
			url = url.replace(/\/k_/, '/oby_'+k+'/k_');
		}else{
			//判断URL中是否含/
			if(url.indexOf('/',url.length-1)>-1){
				url = url+'oby_'+k+'/';
			}else{
				url = url+'/'+'oby_'+k+'/';
			}
			
		}
	}
	window.location = url;
}

function getjxlist(param){
	var city=$('#citylistname').attr('cityname');
	var area=$(param).attr('areaname');
	window.location ='/jiaxiao/'+city+'/'+area+'/';
}

function getalljxlist(param){
	var city=$('#citylistname').attr('cityname');
	window.location ='/jiaxiao/'+city+'/';
}

</script>
