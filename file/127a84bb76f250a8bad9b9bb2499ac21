<script language="JavaScript">
	var w_i='1';			//窗口id
	var w_top=25;		//距顶部距离
	var w_left=30;		//距左边距离

	function tanchuang(url,w,h)
	{
         window.open(url,w_i,'width='+w+',height='+h+',top='+w_top+',left='+w_left+',toolbar=no, menubar=no, scrollbars=yes, resizable=no, location=no, status=no');
	 w_i=w_i+'1';
	 w_top=w_top+25;
	 w_left=w_left+30
 	}	
	function tanchuang_auto()
	{
	//var tc_url="/hbgsww_new/hbgssy/sytc.htm";
         var tc_url="";
	if (tc_url != ""){
	     window.open(tc_url,w_i,'width=500,height=370,top='+w_top+',left='+w_left+',toolbar=no, menubar=no, scrollbars=yes, resizable=no, location=no, status=no');
	 w_i=w_i+'1';
	 w_top=w_top+45;
	 w_left=w_left+40;
 	}
	}	
		function tanchuang_auto1()
	{
	var tc_url="";
	if (tc_url != ""){
	     window.open(tc_url,w_i,'width=501,height=390,top='+w_top+',left='+w_left+',toolbar=no, menubar=no, scrollbars=yes, resizable=no, location=no, status=no');
	 w_i=w_i+'1';
	 w_top=w_top+25;
	 w_left=w_left+30;
 	}
	}
	
