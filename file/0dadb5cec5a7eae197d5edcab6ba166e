<script>
    $(document).ready(function(){
        //控制隐藏
        var p = $(".art_cont").html();
        var stringMaxLength = 2000;
        if(p.length > stringMaxLength){
            $(".art_cont").after('<div class="more"><span>查看更多</span><i class="fx"></i></div>');
            $(".art_cont").html(p.substring(0,stringMaxLength)+ " ..........");
            $(document).on('click','.more',function(){
                if($(this).find("i").hasClass("fx")){
                    $(".art_cont").html(p);
                    $(this).find("span").text("收起全部");
                    $(this).find("i").removeClass("fx").addClass("c");
                }else{
                    $(this).find("i").removeClass("c").addClass("fx");
                    $(this).find("span").text("查看更多");
                    $(".art_cont").html(p.substring(0,stringMaxLength)+ " ..........");
                }
            });
        }

    });
	    $(document).ready(function(){
        if($(".pager li").length){
            $('.hao_page').css('display', 'none');
		}else{
			$('.pager').css('display', 'none');
			}
  });
  
  
  $(".mor").click(function(){
    if($(this).find("i").hasClass("mor_i")){
      $(this).find("i").removeClass("mor_i").addClass("ii");
      $(".thebottomull").removeClass("thebottomull").addClass("ul1");
	   $(this).find("span").removeClass("fl_span").addClass("span");
     $(this).find("span").text("点击收起");
    }else{
      $(this).find("i").removeClass("ii").addClass("mor_i");
      $(".ul1").removeClass("ul1").addClass("thebottomull");
	  $(this).find("span").removeClass("span").addClass("fl_span");
     $(this).find("span").text("查看全部");
    }
	     //展开
  });	
          $(document).ready(function(){
            var top = 80;
            $(window).scroll(function(){
                if($(this).scrollTop()>top){
                    $(".backtop").fadeIn(600);
                }
                else{
                    $(".backtop").fadeOut(600);
                }
            });
            $(".backtop").click(function(){
                $("body,html").animate({
                    "scrollTop":"0"
                },400)
            })
            //返回顶部

        });

  </script>

