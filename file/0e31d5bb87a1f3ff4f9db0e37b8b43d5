function addNew(ss){
var nowDate = new Date();
var ccDate=new Date(ss.replace(/-/g,"/"));
//if(nowDate-ccDate<1269001000){   //15
if(nowDate-ccDate<559201000){   //3日
   document.write("<img src='images/job/new.gif' width='30' height='12' border='0' class=imgnew>");
}
}
function addNewPic(ss,t){
var nowDate = new Date();
var ccDate=new Date(ss.replace(/-/g,"/"));
if(nowDate-ccDate<(t*86400000+1000)){   //t日
   document.write("<img src='images/job/new.gif' width='30' height='12' border='0' class=imgnew>");
}
}
