<script>
islogin();
function Cookie(){
	this.SetValue=function(name,value,hours,path,domain){
		var str=new String();
		var nextTime=new Date();
		nextTime.setHours(nextTime.getHours()+hours);
		str=name+"="+escape(value);
		if(hours)
			str+=";expires="+nextTime.toGMTString();
		if(path)
			str+=";path="+path;
		if(domain)
			str+=";domain="+domain;
		document.cookie=str;
	}
	this.GetValue=function(name){
		var rs=new RegExp("(^|)"+name+"=([^;]*)(;|$)","gi").exec(document.cookie),tmp;
		if(tmp=rs)
			return unescape(tmp[2]);
		return null;
	}
}
function islogin(){
	var ck=new Cookie(); 
	var groupId = ck.GetValue("groupId");
	if(groupId==386){
		
		window.location.href="http://lib.flytv.com.cn/login/login.action?backurl=http%3A%2F%2Fbook.zh.chaoxing.com%2Fgofirstdrs.jsp%3Fd";
	}
	if(groupId==22){
		window.location.href="http://www.ucdrs.superlib.net/areas/ucdrs/error/404.html";
	}
	if(groupId==502){
		window.location.href="http://lib.cczu.superlib.net/login/login.action";
	}
	if(groupId==431){
		window.location.href="http://www.ucdrs.superlib.net/areas/ucdrs/error/404.html";
	}
	if(groupId==505){
		window.location.href="http://passport.yunya.chaoxing.com/userauth.shtml?refer=http%3A//yunya.chaoxing.com/";
	}
	if(groupId==512){
		window.location.href="http://passport.dllib.chaoxing.com/";
	}
	if(groupId==513){
		window.location.href="http://passport.wqlib.chaoxing.com/";
	}
	if(groupId==601){
		window.location.href="https://sslvpn.bcsa.edu.cn";
	}
	if(groupId==270){
		window.location.href="http://www.hnlm.superlib.net/areas/hnlm/tishi/show.html";
	}
	
}
function ready(){
	var s = document.getElementById('s');
	var k = 4;
	setInterval(function(){
		if( k> 0)s.innerHTML = k--;
		else window.location = 'http://so.zh.chaoxing.com/login/login.action?backurl=http://book.zh.chaoxing.com/gofirstdrs.jsp?d';
	}, 1000);
}
