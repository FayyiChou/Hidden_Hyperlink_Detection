<!--400电话初始化-->
<script type="text/javascript">
    var deal400 = new Tel400('100046127', '22', $(".400Tel"));

</script>
<script type="text/javascript" language="javascript">
    $(function () {
        var handleid = setInterval(function () {
            var tels = $(".400Tel");
            var tel = "";
            if (tels.length > 0) {
                tel = $(tels[0]).html() || $(tels[0]).text();
            }
            var addr = "西安市西三环与石化大道十字向北200米路西";
            var brandGId = 448;
            if ($.trim(tel) != "") {
                var saleType = $.trim($(".header .tel").html());
                if (brandGId == 463) {
                    $(".header .tel").empty().html("认证购车咨询热线：<strong>" + $.trim(tel) + "</strong>" + saleType);
                } else {
                    $(".header .tel").empty().html("电话：<strong>" + $.trim(tel) + "</strong>" + saleType);
                }
                if (addr.length >= 28) {
                    $(".header .adress").empty().append("地址：" + addr.substring(0, 30) + "...");
                } else {
                    $(".header .adress").empty().append("地址：" + addr);
                }
                clearInterval(handleid);
            } else {
                if (addr.length >= 42) {
                    $(".header .adress").empty().append("地址：" + addr.substring(0, 41) + "...");
                } else {
                    $(".header .adress").empty().append("地址：" + addr);
                }

                $(".tel400none").parent().hide();
                var card400 = $(".card400");
                if (card400.length > 0) {
                    var tel = $.trim(card400.attr("data-tel"));

                    if (tel != "") {
                        card400.text(tel);
                        card400.next().remove();
                        card400.removeClass("imp");
                    } else {
                        tel.parent().hide();
                    }


                }
            }

        }, 500);
    });
</script>
<!--END OF 400电话初始化-->
