	//刷新酷讯酒店价格
	function refreKuxunprice(id){
		var tm1 = $2("txtComeDate").value;
			var tm2 = $2("txtOutDate").value;
		loadPrice(id, tm1, tm2);
	}

    //预定酒店
  	function dobook(a, b, c) {
  		var doBook = document.doBook;
  		doBook.action = "http://hotel.mapbar.com/woyaoyuding/" + b + "/"
  		doBook.hid.value = a;
  		doBook.rid.value = b;
  		doBook.pid.value = c;
  		doBook.submit();
  	}
  	function refreshprice(id, Order_api) {
  		try {
  			var tm1 = $2("txtComeDate").value;
