<script src="http://js.3conline.com/min2/temp/v2/plugin-locate,plugin-locate_auto.js" charset="utf-8"></script>
<script>
/*城市联动*/
var pro01='';
var pro02='';
for(var i=0;i<areaData.length;i++){
if(i==0){
for(var j=0;j<areaData[0].citys.length;j++){
pro01+='<dd><a href="javascript:void(0)" proid='+areaData[0].citys[j].cityId+'>'+areaData[0].citys[j].name+'</a></dd>';
}	
}
else{
pro02+='<dd><a href="javascript:void(0)" proid='+areaData[i].proId+'>'+areaData[i].pro+'</a></dd>';
}
$('.pro').find('dl').html('');	
$('.pro').find('dl').html('<dd><a href="javascript:void(0)" proid="-1">请选择省份</a></dd>'+pro01+pro02);
}
$('.pro strong').click(function(){
$('.pro dl').show();
})
$('.pro dd a').click(function(){
if($(this).parent('dd').index()<=5){//直辖市等
$('.citys').find('dl').html('');	
$('.citys').find('dl').html('<dd><a href="javascript:void(0)" cityid="'+$(this).attr('proid')+'">'+$(this).html()+'</a></dd>');
$(this).parents('.mnsel').children('i').html($(this).html());
$(this).parents('.mnsel').siblings('.citys ').find('i').html($(this).html());
setContact($(this).html());
$('.mnsel dl').hide();
$('.citys,.citys strong').css('cursor','default');
return;
}
var lscity='';
var proname=$(this).html();
for(var i in areaData){
if(areaData[i].pro==proname){
for(var j=0;j<areaData[i].citys.length;j++){
lscity+='<dd><a href="javascript:void(0)" cityid='+areaData[i].citys[j].cityId+'>'+areaData[i].citys[j].name+'</a></dd>';
}
}
}
$('.citys').find('dl').html('');	
$('.citys').find('dl').html(lscity);
$(this).parents('.mnsel').children('i').html(proname);
$(this).parents('.mnsel').siblings('.citys ').find('i').html('请选择城市');
$(this).parents('.mnsel').siblings('.citys ').find('dl').show();
$('.citys dd a').click(function(){
var city=$(this).html();
$(this).parents('.mnsel').children('i').html(city);
setContact(city);
$('.mnsel dl').hide();
})
$('.citys strong').show();
$('.citys,.citys strong').css('cursor','pointer');
$('.citys dl').scrollTop(0);
})
$('.citys strong').click(function(event){
if($(this).siblings('i').html()=='请选择省份' || $(this).siblings('i').html()=='北京' || $(this).siblings('i').html()=='重庆' || $(this).siblings('i').html()=='上海' || $(this).siblings('i').html()=='天津' ||$(this).siblings('i').html()=='天津滨海'){
return;
}
$(this).siblings('dl').show();
})
//设置联系信息 cityContactData
function setContact(city){
if(city in cityContactData){
$("#contactName").html(cityContactData[city][0]);
$("#contactPhone").html(cityContactData[city][1]);
$("#contactEmail").html(cityContactData[city][2]);
$("#contactAddress").html(cityContactData[city][3]);
for(var i=0;i<areaData.length;i++){
var citys = areaData[i].citys;
for(var j=0;j<citys.length;j++){
if(citys[j].name == city){
LocateAuto.setByCityId(//设置定位信息到Cookie中
citys[j].cityId,
function(data) { //成功请求接口后执行方法
//todo
},
function(data) { //该方法可选，请求接口异常执行方法，返回的数据记录了错误信息
//todo
})
}
}
}
}else{//默认
$("#contactName").html("周娟");
$("#contactPhone").html("020-38178288-8147");
$("#contactEmail").html("zhoujuan@pcauto.com.cn");
$("#contactAddress").html("广州市天河区小新塘高普路115号");
}
}
</script>
<!--js结束-->
<script type="text/javascript">
function xuanz(){
$('.popdp,.covers').show();	
}
function suggest(){
$('.popkrcat,.covers').show();	
}
//xuanz();
var dName = "";
var dgName = "";
var cookieArray=document.cookie.split("; ");
for(var i=0;i<cookieArray.length;i++){ 
var str = cookieArray[i].split('='); 
var cName = $.trim(str[0]);
if(cName=='_dname_'){
try{dName = decodeURI(str[1]);}catch(e){}
}
if(cName=='_dgname_'){
try{dgName = decodeURI(str[1]);}catch(e){}
}
}
if(dName!=""){
$("#username").val(dName);
$("#username").css("color","#333");
}
//refresh captcha code
function changeCaptcha() {
document.getElementById("imgv").src = '//captcha.pcauto.com.cn/captcha/v.jpg?' + new Date().getTime();
}
//login submit
function loginSubmit(){
if ($.trim($("#username").val()) == '' || $.trim($("#username").val()) == '输入您的账号/手机号') {
$("#errorTips").html("<i></i>用户名不能为空").show();
return;
}
if ($.trim($("#password").val()) == '' || $.trim($("#password").val()) == '请输入密码') {
$("#errorTips").html("<i></i>密码不能为空").show();
return;
}
if ($.trim($("#captcha").val()) == '' || $.trim($("#captcha").val()) == '验证码') {
$("#errorTips").html("<i></i>验证码不能为空").show();
return;
}
$("#loginForm").ajaxSubmit({
beforeSubmit:function(){
$("#errorTips").html("<i></i>正在登录……").show();
},
success:function(code){
if (code == '1') {
window.location.href = "http://price.pcauto.com.cn/shangjia/admin/";
} else if (code == '2') {
} else if (code == '3') {
window.location.href = "http://price.pcauto.com.cn/shangjia/admin/";
} else if (code == '0') {
$("#errorTips").html("<i></i>用户名或者密码不正确").show();
} else if (code == '-1') {
$("#errorTips").html("<i></i>用户名或者密码不正确").show();
} else if (code == '-2') {
$("#errorTips").html("<i></i>用户名/密码/验证码不能为空").show();
} else if (code == '-3') {
$("#errorTips").html("<i></i>未签署合同或者合同已到期").show();
} else if (code == '-4') {
$("#errorTips").html("<i></i>商家帐号已禁用").show();
} else if (code == '-5') {
$("#errorTips").html("<i></i>验证码不正确").show();
} else if (code == '-6') {
suggest();
//弹窗
//$("#errorTips").html("<i></i>暂未支持手机号登录，酷养车用户请前往旧页面登录!").show();
} else {//属于多家经销商
$.post("/shangjia/admin/login/getMultiDeales?id="+code,function(data){
data=eval("("+data+")");
$("#dealerList").empty();
for(var i=0;i<data.length;i++){
$("#dealerList").append("<li><a href=\"/shangjia/admin/login/mobile/multi2?sid="+data[i].topsalesId+"\">"+data[i].name+"</a></li>");
}
xuanz();
});
}
$("#captcha").val('');
changeCaptcha();
}
});
}
document.onkeydown=function(event){
var e = event || window.event || arguments.callee.caller.arguments[0];
if(e && e.keyCode==13){ // enter 键
loginSubmit();
}
}; 
</script>
<!--放大镜-->
<script type="text/javascript" src="//js.3conline.com/pcauto/2016/cheshangbang/js/jquery.zoomy0.5.min.js"></script>
<script type="text/javascript">
//This is it
$(function(){ 
$('.zoom').zoomy();
LocateAuto.init({
device: 'pc',
cityCode: '',
callback:function(data){
areaId = data.cityId;
areaName = data.city.replace('市','');
if(areaId==""){
areaId=1;
areaName="广州";
}
setContact(areaName);
pro = data.pro.replace('省','');
$("#pro").html(pro);
$("#city").html(areaName);
}
});
});
</script>
