<script type="text/javascript">
	$(document).ready(function(){
		var defaultHeight=250;
		function scrollfloat(){
			var height=$(document).scrollTop()+defaultHeight;
			$('#window').animate({
				top:height+'px'
			},{
				duration:1000,
				queue:false
			});
			$('#window1').animate({
				top:height+'px'
			},{
				duration:1000,
				queue:false
			});
		}
		$(window).scroll(scrollfloat);
	});
	function closefloatingwindow(){
		$('#window').hide();
		$('#window1').hide();
	}
</script>













