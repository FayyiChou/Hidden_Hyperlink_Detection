    var timer = null;
    $(function () {
        var oCityTemp = document.getElementById("citydiv");
        var curindex = 1;
        timer = setInterval(function () {
            $("#map>ul>li").eq(curindex++).mouseenter();
            if (curindex >= $("#map>ul>li").length) {
                curindex = 1;
            }
        }, 2000);
        $("#city2").html(citys[0].city);
        $("#houses2").html(citys[0].houses);
        $("#room2").html(citys[0].room);
        $("#memberName2").html(citys[0].memberName);
        $("#joinTime2").html(citys[0].joinTime);
        $("#foundTime2").html(citys[0].foundTime);
        $("#valuerNumber2").html(citys[0].valuerNumber);
        $("#updateTime2").html(citys[0].updateTime);
        var ii = 0;
        $("#map>ul>li").mouseenter(function (e) {
            $("#map>ul>li").stop();
            oCityTemp.style.left = -240 + "px";
            $("#cityBox1").hide();
            $("#cityBox2").show();
            $(this).addClass("hover").siblings().removeClass("hover");
            var i = $("#map>ul>li").index(this);
            $("#city1").html(citys[i].city);
            $("#houses1").html(citys[i].houses);
            $("#room1").html(citys[i].room);
            $("#memberName1").html(citys[i].memberName);
            $("#joinTime1").html(citys[i].joinTime);
            $("#foundTime1").html(citys[i].foundTime);
            $("#valuerNumber1").html(citys[i].valuerNumber);
            $("#updateTime1").html(citys[i].updateTime);
            $("#city2").html(citys[ii].city);
            $("#houses2").html(citys[ii].houses);
            $("#room2").html(citys[ii].room);
            $("#memberName2").html(citys[ii].memberName);
            $("#joinTime2").html(citys[ii].joinTime);
            $("#foundTime2").html(citys[ii].foundTime);
            $("#valuerNumber2").html(citys[ii].valuerNumber);
            $("#updateTime2").html(citys[ii].updateTime);
            ii = $("#map>ul>li").index(this);
            $("#cityBox1").fadeIn();
            $("#cityBox2").fadeOut();
            startMove(oCityTemp, { left: -30 });

            if (e.hasOwnProperty('originalEvent')) {
                clearInterval(timer);
                curindex = $(this).index() + 1;
            }


        });
        $("#cityTemp").mouseenter(function () {
            clearInterval(timer);
        })
        $("#cityTemp").mouseleave(function (e) {
            if (e.hasOwnProperty('originalEvent')) {
                timer = setInterval(function () {
                    $("#map>ul>li").eq(curindex++).mouseover();
                    if (curindex >= $("#map>ul>li").length) {
                        curindex = 1;
                    }
                }, 2000);
            }
        })
        $("#map>ul>li").mouseleave(function (e) {
            if (e.hasOwnProperty('originalEvent')) {
                timer = setInterval(function () {
                    $("#map>ul>li").eq(curindex++).mouseover();
                    if (curindex >= $("#map>ul>li").length) {
                        curindex = 1;
                    }
                }, 2000);
            }
        });

    })
</script>
</html>
