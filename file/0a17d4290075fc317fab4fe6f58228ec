<script type="text/javascript">
                    $(function(){     //判断浏览器是否支持placeholder属性  
                        var text = "请输入您要搜索的报告名称";
                        $("#HeaderInputSearch").focus(function () {
                            if ($("#HeaderInputSearch").val() == text || $("#HeaderInputSearch").val() == "") {
                                $(this).css("color", "#333");
                                $(this).val("");
                                }    
                            });       
                        $("#HeaderInputSearch").blur(function () {
                            if ($("#HeaderInputSearch").val() == text || $("#HeaderInputSearch").val() == "") {
                                $(this).val("请输入您要搜索的报告名称");
                                $(this).css("color", "#cdcdcd");
                            }
                            });     
                            
                    });
                </script>
