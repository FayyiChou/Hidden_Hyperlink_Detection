			layer.msg('请输入账户和密码！',{time:2000});
			return;	
		}
		$.post("/login.htm",{ac:"login",phone:u,pwd:p},function(res){
			var data=eval("("+res+")");
			layer.msg(data.msg,{time:2000});
			if(data.status){
// 				var loginid = dlGetCookie("mb_id");
