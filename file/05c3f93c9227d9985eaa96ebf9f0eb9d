        content:$('.pop-login')
	});
	//点击登录按钮      
	$("#popLoginBtn").click(function(){
		var u=$("#username").val();
		var p=$("#passwd").val();
		if($("#autologin").attr("checked")){
			var a=$("#autologin").val();
		}else{
			var a="";	
		}
