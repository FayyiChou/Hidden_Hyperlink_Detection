<script type="text/javascript" charset="utf-8">
jQuery(function($){
	$("div.mega").after().hide();
	$('span.mega_link_title').click(function() {
		$(this).toggleClass("active");
		if ($(this).hasClass('active')) {
			$(this).next("div.mega").slideDown(0,function() {
				$(this).parents().siblings().children("div.mega").slideUp(0);
				$(this).parents().siblings().children("span.mega_link_title").removeClass("active");
			});
		} else {
			$(this).removeClass("active");
			$(this).next("div.mega").slideUp(0);
		}
	});
	
	$('span.mega_link_close').click(function(){
		$(this).parent("div.mega").slideUp(0);
		$(this).parent().prev("span.mega_link_title").removeClass("active");
	});
	
	$(document).click(function(event) { // (1)
	    if (!$.contains($("#gnavi")[0], event.target)) { // (2)
	        $("div.mega").hide();
	        $("span.mega_link_title").removeClass("active");
	    }
	});
    
});


</script>        
