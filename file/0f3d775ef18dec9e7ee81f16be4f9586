<!--Execute flickerplate-->
<script>
    $(function () {
        $('.slider').flickerplate(
                {
                    auto_flick: true,
                    auto_flick_delay: 8,
                    flick_animation: 'transform-slide'
                });
    });
</script>

<script>
    $(document).ready(function () {
        var url = window.location.pathname;
        var index = url.lastIndexOf("/") + 1;
        var filenameWithExtension = url.substr(index);

        if ((filenameWithExtension.toUpperCase() == "INDEX.PHP") || (filenameWithExtension.toUpperCase() == ""))
        {
            $('#lnkhome').attr("class", "select");
            $('#lnkhome2').attr("class", "select");
        } else if ((filenameWithExtension.toUpperCase() == "ALBUMS.PHP") || (filenameWithExtension.toUpperCase() == "GALLERY.PHP"))
        {
            $('#lnkgallery').attr("class", "select");
        } else if (filenameWithExtension.toUpperCase() == "EVENTS.PHP")
        {
            $('#lnkevents').attr("class", "select");
        } else if (filenameWithExtension.toUpperCase() == "CONTACT.PHP")
        {
            $('#lnkcontact').attr("class", "select");
        }
    });

</script>
