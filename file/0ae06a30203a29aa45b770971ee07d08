<script>
var speed = 5;
var currentpos;
var timer;
function setSpeed(){
	speed = parseInt(Scroll.value);
	if (speed < 1 || speed > 10){
	   speed=5;
	   Scroll.value=5;
	}
}
function initialize(){
    timer = setInterval ("scrollwindow()",300/speed);
}
function sc(){
    clearInterval(timer);
}
function scrollwindow(){
    currentpos = document.body.scrollTop;
    window.scroll(0,++currentpos);
    if (currentpos !=document.body.scrollTop) sc();
}
document.onmousedown = sc
document.ondblclick = initialize
</script>
