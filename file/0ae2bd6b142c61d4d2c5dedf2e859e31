<script>

$(function(){
	$("#sub_bar").hover(function(){
		$(this).children("ul").fadeIn();
	},function(){
		$(this).children("ul").fadeOut();
	});
	$("#sub_bar li").click(function(){
		$("#sub_bar ul li").each(function(){
			$(this).find("a").removeClass("nonce");
		});
		$("#sub_bar span").html("<span>"+$(this).text()+"</span>");
		$(this).find("a").addClass("nonce");
		var dataHref = $(this).find("a").attr("data-href");
		$("#search_item").val(dataHref);
	});
	$("#search_txt").focus(function(){
		$(this).attr("style", "background:none");
	});
	$("#search_txt").blur(function(){
		if ($(this).val() == ''){
			$(this).removeAttr("style");
		}
	});
});

</script>
