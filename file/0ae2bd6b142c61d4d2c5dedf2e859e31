<script language="javascript" type="text/javascript">
var id		= '3394';
var gameid	= '2292';

$(document).ready(function(){
	var cookieName='plateComCookie_'+id;
	$.ajax({
		type:'GET',
		url:'/user.php?m=ajax_plateLeve&id='+id,
		success:function(txt){
			var txts=txt.split('|');
			var txtss=txts[1].split('#');;
			$('#plateLeve').removeClass();
			$('#plateLeve').addClass('right_'+txtss[0]);
			$('#comgood').html('');
			$('#comgood').html(txtss[1]);
			$('#comordinary').html('');
			$('#comordinary').html(txtss[2]);			
			$('#combad').html('');
			$('#combad').html(txtss[3]);			
			$('#comaveri').html('');
			$('#comaveri').html(txtss[4]+'.<span>'+txtss[5]+'</span>');
			var plateComCookie=document.cookie.indexOf(cookieName)<=-1?false:true;
			if(plateComCookie){$('input[id^="combutton_"]').attr('disabled', 'disabled');}else{$('input[id^="combutton_"]').removeAttr('disabled');}			
		}
	});
	$('input[id^="combutton_"]').click(function(){
		var type=this.id.substring(10);
		$.ajax({
			type:'GET',
			url:'/user.php?m=ajax_plateScore&id='+id+'&type='+type,
			success:function(txt){
				var txts=txt.split('|');
				if(txts[0]=='success'){
					var exp=new Date();
					exp.setTime(exp.getTime()+10000*86400000);
					document.cookie=cookieName+"="+cookieName+";expires="+exp.toGMTString();
					window.location.reload();
				}else{
					showError('评分失败，请稍候重试');
				}	
			}
		});		
	});
	if(loginTag){$('#userlogindiv').hide();$('#passlogindiv').hide();}else{$('#userlogindiv').show();$('#passlogindiv').show();}
	$.ajax({
		type:'GET',
		url:'/user.php?m=ajax_plateGameServer&plateid='+id+"&gameid="+gameid,
		success:function(txt){
			var txts = txt.split('|');
			$('#weekserverspan').empty();
			$('#allserverspan').empty();
			$('#weekserverspan').html(txts[1]+'组');
			$('#allserverspan').html(txts[0]+'组');			
		}
	});		
});	

</script>
