<script type="text/javascript">
function selform(val){
	var search_txt		= jQuery('#search_txt').val();
	var search_item		= jQuery('#search_item').val();
	var url				= "http://www.9k9k.com/search.php?m=searchlist_list";
	url+="&param="+search_item+"&soinput="+encodeURIComponent(search_txt);
	window.location.href = url;
}
</script>



