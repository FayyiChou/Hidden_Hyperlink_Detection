<script>
  var u = navigator.userAgent;
  var t = {
    ios: !! u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/),
    android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1,
    weixin: u.toLowerCase().match(/MicroMessenger/i) == "micromessenger"
  };
  // 微信里面提示框
  if(t.weixin){
    var wrapper = document.getElementById('wrapper');
    document.getElementById('ios-tips').style.display = 'block';
    wrapper.style.display = 'block';
    wrapper.style.height = document.body.scrollHeight;
    window.scrollTo(0,0);
  }
  else{
   
	    if(t.ios){
	      // IOS下载地址放这里
	      location.href = 'https://itunes.apple.com/cn/app/zhang-shang-tong-ji/id750829891?l=en&amp;mt=8';
	    }
    
	    if(t.android){
	      // Android下载地址放这里
	      location.href = 'http://mobileservice.guahao.cn/common/version/downurl?appid=p_android_wuhan_tjyy';
	    }
  }
</script>

