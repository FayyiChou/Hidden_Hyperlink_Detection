				</iframe>
			</td>
		</tr>
    </table>
</div>
<script type="text/javascript">
function queryBsrl(sjlx){
	$("#bsrljsp").attr("src","http://www.jjjtax.gov.cn/bjsat/office/jsp/bsrl_qt/bsrl.jsp?sjlx="+sjlx);	
	$("#bsrljsp").load(function(){
		if(sjlx==1){
		$("#gsrl").css("font-weight","bold");
		$("#dsrl").css("font-weight","normal");
		}
		if(sjlx==2){
			$("#dsrl").css("font-weight","bold");
			$("#gsrl").css("font-weight","normal");
		}
	});
}
</script>
  </div>
</div>	
