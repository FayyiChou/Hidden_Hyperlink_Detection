      <script language=javascript>
	var focus_width=670     /*幻灯片新闻图片宽度*/
	var focus_height=290   /*幻灯片新闻图片高度*/
	var text_height=20    /*幻灯片新闻文字标题高度*/
var swf_height = 310	
//var swf_height = focus_height+text_height

	var pics = '';
	var links = '';
	var texts = '';

	function ati(url, img, title)
	{
		if(pics != '')
		{
			pics = "|" + pics;
			links = "|" + links;
			texts = "|" + texts;
		}

		pics = escape(img) + pics;
		links = escape(url) + links;
		texts = title + texts;
	}
 
</script>
       
