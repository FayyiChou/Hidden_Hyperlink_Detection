		$("#tax").submit(function(){
			var result=$("#tax select[name='region']").val();
			
			
			if(result=='bjgs'){
				window.open('http://www.bjtax.gov.cn/ptfp/fpindex.jsp');
			}
			if(result=='tjgs'){
				window.open('http://fpxxbd.tjsat.gov.cn/fpxxbd/');
			}
			if(result=='hbgs'){
				window.open('http://dzfp.he-n-tax.gov.cn/dzfpFpywQuery.do');
			}
			if(result=='bjds'){
				window.open('http://zwcx.tax861.gov.cn/fpcx.jsp');
			}
			if(result=='tjds'){
				window.open('http://fpcx.tjcs.gov.cn/index_Query.jsp');
			}
			if(result=='hbds'){
				window.open('http://www.hebds.gov.cn/fapiao/');
			}
		return false;
		})
	</script> 







 
