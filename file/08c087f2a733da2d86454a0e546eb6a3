					  <script type="text/javascript" language="javascript">
	function chkaction(keyword){
	 if(keyword=="")
	 {
			alert("对不起，请输入关键字");
			document.getElementById("keyword").focus();	
			return false;	
		}
	 else
		{
		document.getElementById('searchask').action = "http://wb.99.com/search.php?keyword="+keyword+"&game=12";
		document.getElementById('searchask').submit();
		}
	}
	function AddAsk(keyword)
	{	
		if(keyword=="")
		{
			  location.href="http://wb.99.com/member/login.php?backUrl=http://wb.99.com/question.php";
		}
		else
		{
			  location.href="http://wb.99.com/member/login.php?backUrl=http://wb.99.com/member/question.php?keyword="+keyword;
		}
	}
	</script>
