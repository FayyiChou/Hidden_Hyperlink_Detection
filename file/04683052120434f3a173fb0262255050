        <script type="text/javascript"> 
							var countdown=60; 
							function settime(obj) { 
								if (countdown == 0) { 
									obj.removeAttr("disabled");    
									obj.val("获取验证码"); 
									countdown = 60; 
									return;
								} else { 
									
									obj.attr("disabled", true); 
									obj.val("重新发送(" + countdown + ")"); 
									countdown--; 
								} 
							setTimeout(function() { 
								settime(obj) }
								,1000) 	
							}
						</script> 
        <script>
							$(document).ready(
							function() {													
								$("#Dj_M1").click(function() {
									var mobile = document.getElementById("shouji").value;
									var payid = document.getElementById("payid").value;
									if(mobile == "")
									{
										layer.alert('请输入手机号码！', {
											extend: 'Mvcg/style.css', //加载您的扩展样式
											skin: 'layer-ext-moon',
											title: '<i class="fa fa-volume-up"></i> 系统提示',//不显示标题
											icon: 2
										});
									} 
									else if(!/^13[0-9]{9}$|14[0-9]{9}|15[0-9]{9}$|18[0-9]{9}$/.test(mobile))
									{
										layer.alert('手机号码不正码！', {
											extend: 'Mvcg/style.css', //加载您的扩展样式
											skin: 'layer-ext-moon',
											title: '<i class="fa fa-volume-up"></i> 系统提示',//不显示标题
											icon: 2
										});
									} 
									else 
									{
										$.ajax({
											type: "POST",
											url: "/Home/Member/loginsms.html",
											data: {
												mobile: mobile,
												payid : payid,
											},
											datatype: "txt",
											success: function(result) {
												var a = String($.trim(result));
												if(a == "发送成功"){
													//layer.alert(a, {
													//	extend: 'Mvcg/style.css', //加载您的扩展样式
													//	skin: 'layer-ext-moon',
													//	title: '<i class="fa fa-volume-up"></i> 系统提示',//不显示标题
													//	icon: 1
													//});

													settime($("#Dj_M1"));
												}else{
													layer.alert(a, {
														extend: 'Mvcg/style.css', //加载您的扩展样式
														skin: 'layer-ext-moon',
														title: '<i class="fa fa-volume-up"></i> 系统提示',//不显示标题
														icon: 2
													});
												}
											}
										});
									}
								});
						
							})
						</script> 
