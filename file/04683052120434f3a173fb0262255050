  <script>
// AJAX判断登录状态
$.ajax({
	type : "GET",
	url : "/Home/Member/ajaxChkLogin.html",
	dataType : "json",
	success : function(data)
	{
		if(data.login == 1)
		{
			if(data.shouji != null)
				var html = '您好,<a href="/index.php/Home/Buyer/index/id/Buyer">'+data.shouji+'</a> &nbsp;<a onclick="logout()" href="javascript:;">退出</a>';
			if(data.username != null)
				var html = '您好,<a href="/index.php/Home/Buyer/index/id/Buyer">'+data.username+'</a> &nbsp;<a onclick="logout()" href="javascript:;">退出</a>';
		}			
		else
 			var html = '<a href="/login.html">登录</a>';
		$("#logInfo").html(html);
	}
});
</script> 
  <script>
function logout()
{
	$.ajax({
		url:'/Home/Member/logout.html',
		dataType:'json',
		type:'post',
		success:function(msg){
			if(msg.logout == 1)
			{
				layer.confirm('您已成功退出网站', {
					extend: 'Mvcg/style.css', //加载您的扩展样式
					skin: 'layer-ext-moon',
					title: '<i class="fa fa-volume-up"></i> 精时精模网提示',//不显示标题
					icon: 6,
					btn: ['确定'] //按钮
				}, function() {
					location.href  = '/';
				});
			}
			else
				layer.alert('您退出精时精网失败', {
				  icon: 5,
				  skin: 'layer-ext-moon'
				});
		}
	});
}
</script> 
