        function clickProPic(thisPicID) {
            var srcstr = jQuery('#' + thisPicID)[0].src;
            srcstr = srcstr.replace("/Thumb/", "/");
            jQuery('#MainPicImg')[0].src = srcstr;
            jQuery('#MainPicShow')[0].href = srcstr;
            jQuery('.smallProductImg ul li').removeClass("showChoose");
            jQuery('#' + thisPicID).parent().parent().parent().parent().parent().addClass("showChoose");
        }
        jQuery(document).ready(function () {
            if ($(".productParameter > table tr").size() <= 5) {
                $(".parameterMore").hide();
            }
            else {
                $(".productParameter").addClass("hideParameter");
            }
            $(".parameterMore").find("a").click(function () {
                $(this).toggleClass("more");
                $(".productParameter").toggleClass("hideParameter", 300);
            });
        });
    </script>


