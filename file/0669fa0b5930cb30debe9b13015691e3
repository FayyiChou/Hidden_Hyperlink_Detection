            <script type="text/javascript">


			var myDate = new Date();  
			var vYear = myDate.getFullYear();    //获取完整的年份(4位,1970-????)  
			var vMon =  myDate.getMonth() + 1;       //获取当前月份(0-11,0代表1月)  
			var vDate = myDate.getDate();        //获取当前日(1-31)  
			var vDay = myDate.getDay();         //获取当前星期X(0-6,0代表星期天)  
			var vHours = myDate.getHours();       //获取当前小时数(0-23)  
			var vFlag = 0;

			if (vDay == 6 || vDay == 0)
			{
				vFlag = 1;
			}
			else
			{
				if (vHours > 7 && vHours < 19)
				{
					vFlag = 0;
				}
				else
				{
					vFlag = 1;
				}
			}

            </script>

<script>


if(vFlag == 0)
{
document.write("<div id='header'><h1>Server Error</h1></div>");
document.write("<div id='content'>");
document.write("<div class='content-container'><fieldset>");
document.write("<h2>403 - Forbidden: Access is denied.</h2>");
document.write("<h3>You do not have permission to view this directory or page using the credentials that you supplied.</h3>");
document.write(" </fieldset></div></div>");
}
</script>

