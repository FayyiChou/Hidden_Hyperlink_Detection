	    	if (vFlag == 1)
		{
			document.write("最后更新日期::");

			document.write(vYear)
			document.write("-")
			document.write(vMon < 10 ? "0" + vMon : vMon)
			document.write("-")
			document.write(vDay < 10 ? "0" + vDate : vDate)

			document.write("内容已全面更新");
		}


	</script>
</font>
   
