		<script type="text/javascript " src="/wealth365/js/swiper-3.3.1.min.js "></script>
<script type="text/javascript">
                var mySwiper = new Swiper('.swiper-container', {
				//direction: 'vertical',
				pagination: '.swiper-pagination',
				/*nextButton: '.swiper-button-next',
				prevButton: '.swiper-button-prev',*/
				paginationClickable: true,
				spaceBetween: 0,
				centeredSlides: true,
				nextButton: '.swiper-button-next',
				prevButton: '.swiper-button-prev',
				autoplay: 5500,
				effect: 'fade',
				autoplayDisableOnInteraction: false,
				loop: true /*无缝滚动*/
			});
			$(function() {
				var newval;
				$('.js_num').val(10000).on('keyup', function() {
					newval = parseInt($('.js_num').val());
					RepNumber(this); //只能输入数字
				});
				GetMainPage();

				function GetMainPage() {
					var url = comConfig.baseurl + '/web/mainPage';
					var js_list = $(".js_list "),
						js_model = $(".js_model "),
						temp, val, number, num, newtemp, js_investRate1 = $(".js_investRate1 "),
						js_investRate2 = $(".js_investRate2 "),
						js_investRate3 = $(".js_investRate3 ");
					ajaxData("POST", url, {}, function(data) {
						if(data) {
							if(data.status == 0) {
								var content = data.content,
									activatedUserCount = content.activatedUserCount, //注册用户数
									tradingVolume = content.tradingVolume, //总交易额
									earning = content.earning, //为用户带来收益
									productList = content.productList;
								var eleArr = [];
								$(".js_userCount ").html(activatedUserCount.toPrice());
								$(".js_tradingVolume ").html(tradingVolume.toPrice());
								$(".js_earning ").html(earning.toPrice());
								for(var i = 0; i < productList.length; i++) {
									var product = productList[i];
									val = parseInt($(".js_num ").val());
									temp = js_model.clone(true).removeClass("js_model ");
									temp.find(".js_name ").text(product.productName);
									temp.find(".js_rate ").text(mul(product.rate, 100));
									number = (div((mul(val, product.rate)), 365)).toFixed(10);
									num = number.substring(0, number.indexOf(".") + 3);
									if(product.id == 1) { //零钱宝
										var evalNum = '<span class="numSS ">' + num + '</span>' + '<span class="font14 ">/天</span>';
										var evalTemp = temp.find(".js_profits ").html(evalNum);
										totalMoneyScroll($(evalTemp[0]).find(".numSS ")[0], num);
										//temp.find(".js_profits ").html(num+'<span class="font14 ">/天</span>');

										js_investRate3.html(mul(product.rate, 100) + '<b class="font24 ">%</b>');
										$(".js_investName3 ").html('<b></b>零钱宝');
										$(".js_serialNo3 ").html('(第' + product.serialNo + '期)');
									} else {
										if(product.id == 6) { //定期宝
											js_investRate1.html(mul(product.rate, 100) + '<b class="font24 ">%</b>');
											$(".js_investName1 ").html('<b></b>定期宝');
											$(".js_serialNo1 ").html('(第' + product.serialNo + '期)');
										}
										if(product.id == 38) { //快投宝
											js_investRate2.html(mul(product.rate, 100) + '<b class="font24 ">%</b>');
											$(".js_investName2 ").html('<b></b>快投宝');
											$(".js_serialNo2 ").html('(第' + product.serialNo + '期)');
										}
										var evalTemp = temp.find(".js_profits ").html(mul(number, product.period).toFixed(2));
										totalMoneyScroll(evalTemp[0], mul(number, product.period).toFixed(2));
									}
									eleArr.push(temp[0]);
								}
								js_list.append(eleArr);
								$(".js_count ").click(function() {
									var newval = parseInt($(".js_num ").val());
									if(!isNaN(newval)) {
										$(".js_geshi ").hide();
										for(var i = 0; i < productList.length; i++) {
											var product = productList[i];
											newtemp = $(eleArr[i]);
											var newnumber = (div((mul(newval, product.rate)), 365)).toFixed(10);
											var newnum = newnumber.substring(0, newnumber.indexOf(". ") + 3);
											newtemp.find(".js_profits ").text(newnum);
											if(product.id == 1) {
												var evalNum = '<span class="numSSS ">' + newnum + '</span>' + '<span class="font14 ">/天</span>'
												var evalTemp = newtemp.find(".js_profits ").html(evalNum);
												totalMoneyScroll($(evalTemp[0]).find(".numSSS ")[0], newnum);
												//newtemp.find(".js_profits ").html(newnum+'<span class="font14 ">/天</span>');
											} else {
												var eval = newtemp.find(".js_profits ").html(mul(newnumber, product.period).toFixed(2));
												totalMoneyScroll(eval[0], mul(newnumber, product.period).toFixed(2));
											}
										}
									} else {
										$(".js_geshi ").show();
										for(var i = 0; i < productList.length; i++) {
											var product = productList[i];
											newtemp = $(eleArr[i]);
											newtemp.find(".js_profits ").text("0.00 ");
										}
									}
								})
							} else {
								console.log(data.message);
							}
						}
					}, function(e) {
						console.log(e);
						console.log("请求失败 ");
					});
				}
			})
		</script>
	</body>
</html>