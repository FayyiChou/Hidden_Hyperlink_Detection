<script>

if( /Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
	gotoMobile1();
}

function gotoMobile1() {
	var href_str = window.location.href;
	var desktop = /(.*)\?desktop$/;
	var desktop2 = /(.*)\&desktop*/;
	
	var isDesktop = "";
	try {
		isDesktop =	sessionStorage.getItem("isDesktop");
	} catch (e) {
		throw "get isDesktop error";
	}
	
	if (href_str.match(desktop) || href_str.match(desktop2)) {
		try {
			sessionStorage.setItem("isDesktop", "true");
		} catch (e) {
			throw "save isDesktop error";
		}
		return 0;
	}else if(isDesktop == "true"){
		return 0;
	}
	var re = /((http|https):\/\/.*)(zh_TW|en_US|ja_JP)(.*)/i;
	var found = href_str.match( re );
	
	if (found) {
		var testStr = found[4];
		if (! testStr.match(/^\/(m)\//)) {
			window.location.href = found[1] + found[3] + "/m" + found[4];
		}
	} else {
		window.location.href ="/en_US/m/index.html";
	}
	
	return 0;
}

</script>


