function doIcon(url,funcID){
	$("#iconFrm input[name=mFuncID]").val(funcID);
	$("#iconFrm").attr("action",url);
	$("#iconFrm").attr("target","_self");
	$("#iconFrm").submit();
}

function doExternal(url, funcID, SID){
	$("#iconFrm input[name=mFuncID]").val('D0636733C6861689');
	$("#iconFrm input[name=sFuncID]").val(funcID);
	$("#iconFrm input[name=SID]").val(SID);
	$("#iconFrm input[name=fid]").val(funcID);
	$("#iconFrm").attr("action",url);
	$("#iconFrm").attr("target","_blank");
	$("#iconFrm").submit();	  
}

function checkImage(){
  var maxWidth = 0;
  temp = $("div.detailContent img"); 
  for(var i=0;i<temp.length;i++){
    if(temp[i].width > maxWidth){
      maxWidth = temp[i].width;
    }
  }
  
  if(maxWidth > 910){
    document.getElementById("content").className = "detailContent scrollbar";
  }
}

function slideLine(box,stf,delay,scroll,speed,h){ 
 //取得id  
 var slideupBox = document.getElementById(box); 
 //預設值 delay:幾毫秒滾動一次(1000毫秒=1秒) 
 //       speed:數字越小越快，h:高度 
 var delay = delay||3000,speed = speed||50,h = h||10; 
 var tid = null,pause = false; 
 //setInterval跟setTimeout的用法可以咕狗研究一下~ 
 var su = function(){tid=setInterval(slideup, speed);} 
 //主要動作的地方 
 var slideup = function(){  	      
     //當滑鼠移到上面的時候就會暫停 
     if(pause) return; 
     //滾動條往下滾動 數字越大會越快但是看起來越不連貫，所以這邊用1      
     slideupBox.scrollTop += scroll;      
     //滾動到一個高度(h)的時候就停止      
     if(slideupBox.scrollTop%h == 0){ 
        //跟setInterval搭配使用的 
        clearInterval(tid); 
        //將剛剛滾動上去的前一項加回到整列的最後一項 
        slideupBox.appendChild(slideupBox.getElementsByTagName(stf)[0]); 
        //再重設滾動條到最上面 
        slideupBox.scrollTop = 0; 
        //延遲多久再執行一次 
        setTimeout(su, delay); 
        
    } 
 } 
 //滑鼠移上去會暫停 移走會繼續動 
 slideupBox.onmouseover=function(){pause=true;} 
 slideupBox.onmouseout=function(){pause=false;} 
 //起始的地方，沒有這個就不會動囉 
 setTimeout(su, delay); 
} 
function doFuncList(funcID){
	var funcForm=document.createElement("form");
	funcForm.setAttribute("name","funcForm");
	funcForm.setAttribute("id","funcForm");
	funcForm.setAttribute("action" ,"/en_US/exhibitor/subFuncList.html" );
	funcForm.setAttribute("target","_self");
	funcForm.setAttribute("method","post");

	var mFuncID=document.createElement("input");
	mFuncID.setAttribute("name","mFuncID");
	mFuncID.setAttribute("type","hidden");
	mFuncID.setAttribute("value",funcID);
	funcForm.appendChild(mFuncID);

	var body = document.getElementsByTagName("body");
	body[0].appendChild(funcForm);
	funcForm.submit();
}
//-->
