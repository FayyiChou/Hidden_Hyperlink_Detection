<script language="JavaScript" type="text/JavaScript">
   var currentActBanner = 1;
   function activeBannerFade(self,totalCount){
     if(currentActBanner!=self){
        $('#actPhoto'+currentActBanner).fadeOut('2000', function() {
        $('#actPhoto'+self).fadeIn(1000);                           
        //$('#activityBannerCount'+currentActBanner).attr("src","https://download.taiwantradeshows.com.tw/files/css/tpl06/en_US/c01/button/"+currentActBanner+".gif");
        //$('#activityBannerCount'+self).attr("src","https://download.taiwantradeshows.com.tw/files/css/tpl06/en_US/c01/button/"+self+"c.gif");
         currentActBanner = self;
      });               
     }
   }
</script>



