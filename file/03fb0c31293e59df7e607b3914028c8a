var productPhotoSize = 112;
function doLeft(){
  var value = document.getElementById("pictb").style.left.replace("-", "").replace("px", "");
  if(value != "0"){
    value = Math.abs(parseInt(value) - productPhotoSize);
    value = "-"+value+"px";
    document.getElementById("pictb").style.left = value;
  }
  //關閉Random
  clearTimeout(random1);
  clearTimeout(random2);
}

function doRight(photoNum){
  var value = document.getElementById("pictb").style.left.replace("-", "").replace("px", "");
  if(parseInt(value) != (photoNum - 6)*productPhotoSize && photoNum >= 6){
    value = parseInt(value) + productPhotoSize;
    value = "-"+value+"px";
    document.getElementById("pictb").style.left = value;
  }
  //關閉Random
  clearTimeout(random1);
  clearTimeout(random2);
}


