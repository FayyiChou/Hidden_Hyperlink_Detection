function doSearch(val, isSubmit) {
	if(val) {
		  //hot keyword click
		  window.location.href = encodeURI(val);
		  return;
	}
  if($("input#condition").val()=="" ||  $("input#condition").val()=="Please enter keyword"){  	
      return false;
  }else if($("input#condition").val().length<=1){
  	  alert("Please enter more than 2 characters");
  	  return false;  
  }else if($("input#condition").val().match(/!|@|#|\$|%|\^|&|\*|\(|\)|\?|<|>|\\"|\{|\}|\[|\]|\\|\||\~/)){  	                                      
  	  alert("The following characters are not allowed,!@#$%^*() ?><{}[]|~");
  	  return false;
  }else if(isSubmit){
  	  //input key down
  	  if($("input[name=search]:checked").val()){
        $("#searchForm").attr('action', $("input[name=search]:checked").val());
        $("input[name=search]:checked").attr("disabled", true);
      }else{
      	$("#searchForm").attr("action", $("#search").val());
      	$("#search").attr("disabled", true);
      }
      $("#year").attr("disabled", true);
  	  return true;
  }else{
  	  //click search button
  	  var act = "";
  	  if($("input[name=search]:checked").val()){
        act = $("input[name=search]:checked").val();
        $("input[name=search]:checked").attr("disabled", true);
      }else{
      	act = $("#search").val();
      	$("#search").attr("disabled", true);
      }
      $("#year").attr("disabled", true);
      $("#searchForm").attr("action", act);
      $("#searchForm").submit();
  }
}


function doSearchAdv(formType) {	
	var erroCode = 0;
	if(formType=="exh"){
		  if($("input#subjectExh").val().length<=1 && $("input#exhBrandName").val().length<=1){
      	 erroCode=2;
      }else if($("input#subjectExh").val().match(/!|@|#|\$|%|\^|&|\*|\(|\)|\?|<|>|\\"|\{|\}|\[|\]|\\|\||\~/)
      	|| $("input#exhBrandName").val().match(/!|@|#|\$|%|\^|&|\*|\(|\)|\?|<|>|\\"|\{|\}|\[|\]|\\|\||\~/)){
      	 erroCode=3;
      }
	}else if(formType=="exhProd"){		 
		  if($("input#exhMainProduct").val().length<=1){
      	 erroCode=2;
      }else if($("input#exhMainProduct").val().match(/!|@|#|\$|%|\^|&|\*|\(|\)|\?|<|>|\\"|\{|\}|\[|\]|\\|\||\~/)){
      	 erroCode=3;
      }       
	}else if(formType=="prod"){
		  if($("input#subjectProd").val().length<=1 && $("input#productDesc").val().length<=1){
      	 erroCode=2;
      }else if($("input#subjectProd").val().match(/!|@|#|\$|%|\^|&|\*|\(|\)|\?|<|>|\\"|\{|\}|\[|\]|\\|\||\~/)
      	|| $("input#productDesc").val().match(/!|@|#|\$|%|\^|&|\*|\(|\)|\?|<|>|\\"|\{|\}|\[|\]|\\|\||\~/)){
      	 erroCode=3;
      }		
  }
  if(erroCode==2){
  	 alert("Please enter more than 2 characters");
  	  return false;  
  }else if(erroCode==3){
  	   alert("The following characters are not allowed,!@#$%^*() ?><{}[]|~");
  	  return false;
  }else{
  	  return true;
  }
}

function advSearchLink(){
		if($("#pageForm").attr("action")==undefined){
			 window.location="/en_US/search/advSearch.html";
		}else{
			$("#pageForm").attr("action","/en_US/search/advSearch.html");
			$("#pageForm").submit();
		}
	  
	}

