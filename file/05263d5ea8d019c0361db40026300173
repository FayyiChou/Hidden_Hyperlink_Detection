    var up_url = "";
    var next_url = "";
    up_url = "";
    next_url = "http://www.jiankang.com/detail/183485_2.shtml";

    $(function(){
        $(".main_left_con img").bind("click",function(){
            if(next_url == ''){
                return false;
            }
            window.location.href=next_url;
        });
    })
    //左右翻页
    function keyUp(e) {
        if(navigator.appName == "Microsoft Internet Explorer"){
            var keycode = event.keyCode;var realkey = String.fromCharCode(event.keyCode);
        }else{
            var keycode = e.which;var realkey = String.fromCharCode(e.which);
        }
        if(keycode==39 && next_url != ''){
            window.location.href=next_url;
        }
        if(keycode==37 && up_url != ''){
            window.location.href=up_url;
        }
    }
    document.onkeyup = keyUp;
    //图片滚动
