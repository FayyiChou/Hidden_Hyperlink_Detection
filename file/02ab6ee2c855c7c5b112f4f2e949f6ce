<script>
var oTop=fixd.offsetTop;
window.onscroll=function(){	
	var fixd=document.getElementById('fixd');
	var st=document.documentElement.scrollTop||document.body.scrollTop;		
	if(st>=oTop){
		if(window.XMLHttpRequest){
			fixd.style.position='fixed';
			fixd.style.top=0;				
		}else{
			fixd.style.top=oTop+'px';
		}
}else{
	fixd.style.position='absolute';
	fixd.style.top=oTop+'px';
}

var aside=document.getElementById('aside');
var asideH=document.getElementById('asideH');
aside.style.height=asideH.offsetHeight+340+'px';
}
</script>

