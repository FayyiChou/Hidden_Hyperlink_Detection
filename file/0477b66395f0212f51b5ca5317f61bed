var speed31=25//速度数值越大速度越慢
demo1232.innerHTML=demo1231.innerHTML
function Marquee1(){
if(demo1232.offsetWidth-demo123.scrollLeft<=0)
demo123.scrollLeft-=demo1231.offsetWidth
else{
demo123.scrollLeft++
}
}
var MyMar1=setInterval(Marquee1,speed31)
demo123.onmouseover=function() {clearInterval(MyMar1)}
