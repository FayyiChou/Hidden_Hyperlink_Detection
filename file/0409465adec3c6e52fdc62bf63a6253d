//        else {
            jQuery.ajax({
                url: "/default.aspx?controller=Home&action=load_more_newfiles&rnd=" + Math.random(),
                type: 'POST',
                data: { page: more_newfile_count },
                beforeSend: function () { },
                error: function () {
                    alert("发生错误数据未能载入");
                    jQuery('.readmore > p').html("更多")
                },
                success: function (html) {
                    var result = jQuery(html);
                    jQuery('.share_hide').append(result);
                    jQuery(".share_hide").show();
                    jQuery('.readmore > p').html("更多")
                }
            });
        //}
        //jQuery(".share_hide").show();
