<script type="text/javascript">
    var more_newfile_count = 1;
    jQuery(".readmore").click(function () {
        jQuery('.readmore > p').html("载入中请稍候...")
        more_newfile_count++;
//        if (more_newfile_count > 5) {
//            location.href = "/default.aspx?controller=Home&action=NewList&page=1"
