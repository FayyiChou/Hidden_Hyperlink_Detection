<script language="javascript">
$(function(){
	//$("#appraisalType").attr("value",0);
	//选择地区J-address
	$("#J-address").click(function(){
		$(".filter-section .select-hd .text").addClass("cur");
		$(".filter-section .select-bd").show();
	});
	$(".area-hd a.close").click(function(){
		$(".filter-item .select-bd").hide();
	});
	//选择地区标签切换
	$(".area-hd a.select").click(function(){
	    $(this).addClass("current").siblings().removeClass("current");
	    var index = $(this).index();
	    $(".area-bd .area-list").eq(index).show().siblings().hide();
	});
});


var regionArr;
var atgregion;
setDefaultRegion();
function setDefaultRegion(){
	atgregion = $.cookie("atgregion");
	if(null != atgregion){
		regionArr = atgregion.split("|");
	}else{
		atgregion = "11010200|北京北京市朝阳区(五环里)全境|11010000|11000000|110102001";
		$.cookie("atgregion", atgregion, {
	        expires: 30,
	        path: '/',
	        domain: ".gome.com.cn"
	    });	
		regionArr = atgregion.split("|");
	}
}


function initDistrict(){
	if(null != atgregion){
		queryCityTemp();
	}	
}


function queryCityTemp(){
	queryCity(regionArr[3]);		
	setTimeout ("queryCountryTemp()", 200);
}

/**/
function queryCountryTemp(){
	queryCountry(regionArr[2]);
	setTimeout ("queryTownTemp()", 200);
}

function queryTownTemp(){
	queryTown(regionArr[0]);
	setTimeout ("queryDeliveryTemp()", 200);
}

function queryDeliveryTemp(){
	if(regionArr.length>4){
		queryDelivery(regionArr[4]);	
	}else{
		queryDelivery(regionArr[0]+"1");
	}	
	//getHeight(350);
}
initDistrict();
setTimeout ("getHeight()", 1000);
includeheader('shfw');
</script>
