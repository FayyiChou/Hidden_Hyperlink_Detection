<script type="text/javascript">
//右侧头像位置
//var dynSite = '//www.gome.com.cn';
//var contextPath = "/ec/homeus";

var isLogin = 0;
var infovalue = "";

//初始化
$(document).ready(function() {
	if($(".usercon").attr("done")!="true"){
		$.ajax({
			url: "//member"+cookieDomain+"/myaccount/profitAccountDetail/getProfitAccountDetail",
			type:"get", 
			cache: false,
			dataType:"jsonp",
			jsonpName:"ckdata",			//json响应标识
			data:{type:2},					//请求标识
			//timeout:10000,
			error:function(XMLHttpRequest, textStatus, errorThrown){
				if(textStatus=="timeout"){
				}else{
				}
			},
			success: function(data){
			}					
		});
	}		 
});

/**
 * 回调Function
 */
function ckdata(data){
	isLogin = data.isLogin;
	 //不在线
	if(isLogin == 0){
		//$(".loginlist").show();
		//$(".userBox").hide();
		$('#hc_shfw_userLogin').show();	
		//disableLink();
	}else if(isLogin == 1){		 
		$('#hc_shfw_userName').show();		 
		 //在线
		 var nickname = data.nikename,profile = data.profileImageUrl, grade = data.profileGrade;			 					 
		 infovalue = "userId="+data.id+"&loginname="+data.login+"&grade="+data.profileGrade
		 +"&name="+data.nikename+"&memo=&hashCode=";
		 //设置用户昵称
		 if(nickname!=undefined && nickname!=""){
			 var nick = cut_str(nickname,14);
			 $(".h_index_userName").html(nick);
		 }
		 //用户头像
		 if(profile!=undefined && profile!=""){//<em class="head_bg"></em>
			 $(".h_index_userHead").html('<a target="_blank" href="'+dynSite+contextPath+'/myaccount/gome/profileHome.jsp?flag=a"><img width="100px" height="100px" onerror="nofind(this);" src="'+profile+'"/><em class="head_bg_sh"></em></a>');
		 }
		 //用户等级昵称
		 if(grade!=undefined && grade!=""){
			 $(".userName").html('<img src="//img.gomein.net.cn/images/myGome_grade'+grade+'.jpg" />');
		 }
	 }	
}
function disableLink() {
	$(".userbtn a").each(function() {
		$(this).attr("style", "color:#A19B9B");
		$(this).attr("onclick", "");
		$(this).attr("href", "javascript:void();");
		$(this).attr("target", "");
	});
}
function cut_str(str, len){
    var char_length = 0;
    for (var i = 0; i < str.length; i++){
        var son_str = str.charAt(i);
        encodeURI(son_str).length > 2 ? char_length += 1 : char_length += 0.5;
        if (char_length >= len){
            var sub_len = char_length == len ? i+1 : i;
            return str.substr(0, sub_len); 
            break;
        }
    }
    if(char_length <= len){
    	return str;
    }
}
