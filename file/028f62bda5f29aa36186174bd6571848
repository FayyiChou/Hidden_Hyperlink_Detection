<script type="text/javascript">
getsendServer.getServerIndex("changjianwenti1","","psfw");

/*物流与配送-配送费用点击事件*/
$("#sendPrice .hd").delegate("li","click",function(){
	var index=$(this).index();
	$("#sendPrice .hd li").each(function(){
		 $(this).removeClass('cur');
	 })
	 $("#sendPrice .bd .bd_item").each(function(){
		 $(this).removeClass('cur');
	 })
	 $(this).addClass("cur");
	  $("#sendPrice .bd .bd_item").eq(index).addClass("cur");
	  getHeight();
})



/**物流与配送-配送范围查询点击事件**/
$("#sendarea .hd").delegate("li","click",function(){
	var index=$(this).index();
	$("#sendarea .hd li").each(function(){
		 $(this).removeClass('cur');
	 })
	 $("#sendarea .bd .bd_item").each(function(){
		 $(this).removeClass('cur');
	 })
	 $(this).addClass("cur");
	  $("#sendarea .bd .bd_item").eq(index).addClass("cur");
	  getHeight();
})



