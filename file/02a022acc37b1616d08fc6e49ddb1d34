<script language="javascript" type="text/javascript">
    function search_init(){
        obj = document.getElementById('word');
        if(obj.value=='攻略/路书/游记'){
            obj.value = '';
        }else{
            obj.select();
        }
    }

    function addFavorites(){
        if (document.all){
            window.external.addFavorite('http://z.mawengwo.cn','蚂蜂窝旅游指南');
        }else if (window.sidebar){
            window.sidebar.addPanel('蚂蜂窝旅游指南', 'http://z.mawengwo.cn', "");
        }
    }

    function setHomepage(){
        if (document.all){
            document.body.style.behavior='url(#default#homepage)';
            document.body.setHomePage('http://z.mawengwo.cn');
        }else if (window.sidebar){
            if(window.netscape){
                try{ 
                    netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect"); 
                } catch (e) { 
                    alert( "该操作被浏览器拒绝，如果想启用该功能，请在地址栏内输入 about:config,然后将项 signed.applets.codebase_principal_support 值改为true" ); 
                }
            }
            var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components. interfaces.nsIPrefBranch);
            prefs.setCharPref('browser.startup.homepage','http://z.mawengwo.cn');
        }
    }

    function show_login(){
        $.prompt($("#pnl_login").clone().find(".ipt_email").attr("id","login_ipt_email").end().find(".ipt_pwd_mirror").attr("id","ipt_pwd_mirror").end().find(".ipt_pwd").attr("id","login_ipt_pwd").end().find(".errinfo").attr("id","login_errinfo").end().find(".ckb_isremember").attr("id","ckb_isremember").end().find('.login_from').attr("id", "login_from").end().find(".login_code_box").attr("id","login_code_box").end().html(),{buttons:{}});
        $("#login_ipt_email").inputDefault({text:"邮箱/手机号"});
        $("#ipt_pwd_mirror").inputDefault({text:"请输入密码"});
        $("#ipt_pwd_mirror").focus(function(){
            $(this).hide().next().show().trigger("focus");
        });
        $("#login_ipt_pwd").blur(function(){
            if($(this).val().length==0)
                $(this).hide().prev().show().trigger("blur");
        });
        $("#ckb_isremember").change(function(){
            if($(this).is(":checked")){
                $(this).val("1");
            }else{
                $(this).val("0");
            }
        });
    }
</script>

