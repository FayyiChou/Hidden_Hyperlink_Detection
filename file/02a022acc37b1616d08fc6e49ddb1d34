	$(function(){
		//三种检索，类型切换
		$(".s-nav li").each(function(i){
			$(this).click(function(){
				$(this).addClass("on").siblings('li').removeClass('on');
				$("#s-nav-"+i).removeClass("hide").siblings('dl').addClass('hide');
			});
		});

		//交换左右搜索框内容
		$('.s-change').click(function(){
			var left_input_obj = $('#s-input-left');
			var right_input_obj = $('#s-input-right');
			var tmp = left_input_obj.val();
			left_input_obj.val(right_input_obj.val());
			right_input_obj.val(tmp);
		});
		//搜索提交跳转
		$(".s-btn").click(function(){
			$(this).parent().submit();
		});
	})
</script>

