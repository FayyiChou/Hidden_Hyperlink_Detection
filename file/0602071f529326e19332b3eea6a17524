<script language="javascript">
function comment(rt) {
    try {
	    if(rt == 1) var action = 'reply';
		else var action = 'newthread';

        var f = document.pform;
        var article_class_id = '{$CatId}';
        var news_id = '{$ContId}';
        var news_title = '{$Title}';
        if (!news_title) news_title = document.title;
		var news_title = news_title.replaceAll('"','`');
		var news_title = news_title.replaceAll("'",'`');
        news_title = encodeURI(news_title);
        var news_url = location.href;
		var baseurl = 'http://bbs.cnfol.com/api/newspost.php?news_id=' + news_id +'&news_url=' + news_url + '&action='+action+'&news_title=' + news_title;

        if (rt==1) {
            if (f.post_content.value=="") {
                alert('请输入评论内容');
                f.post_content.focus();
                return false;
            }
            f.action = baseurl+'&commit='+encodeURI(document.getElementById('post_content').value);
            return true;
        } else
		referURL(baseurl);
    } catch(e) {}
}

String.prototype.replaceAll = function(reallyDo, replaceWith, ignoreCase) {   
    if (!RegExp.prototype.isPrototypeOf(reallyDo)) {   
        return this.replace(new RegExp(reallyDo, (ignoreCase ? "gi": "g")), replaceWith);   
    } else {   
        return this.replace(reallyDo, replaceWith);   
    }   
}   

function referURL(url){
var isIe=(document.all)?true:false;
if(isIe) {
var linka = document.createElement('a');
linka.href=url;
linka.target = "_blank";
document.body.appendChild(linka);
linka.click();
}else {
window.open(url);
}
}
</script>
