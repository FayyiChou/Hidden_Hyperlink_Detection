<SCRIPT>
/*
* 置顶广告，在页面完全下载 1  秒后，显示左侧弹出广告
，
* 大约2秒后，自动关闭，并弹出新窗口
*/

function zdclose()
{   
	document.getElementById("guanggao4").style.display='none';
	window.clearTimeout(zdggtimec);
	ckopen();
}

function zdopen()
{   
	document.getElementById("guanggao4").style.display="block";
	window.clearTimeout(zdggtime);
	zdggtimec=window.setTimeout("zdclose()",3000);
	
}
var zdggtime=window.setTimeout("zdopen()",1);

</SCRIPT>
