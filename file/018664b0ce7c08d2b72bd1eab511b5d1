                var config = {
                    //总页数
                    totalNum:10,
                    //当前页数
                    currentNum:1
                };
                //分页
                var pager = P({
                    containerId : 'DDS_payPager',
                    showPage : 6,
                    totalNum : parseInt(config.totalNum),
                    onChange : function(num){
                        var url = window.location.href;
                        if(url.indexOf('pageNo')==-1) {
                            url += (url.indexOf('?')==-1)?'?':'&';
                            url+='pageNo='+num;
                        }
                        else {
                            url = url.replace(/pageNo=\d+/g,'pageNo='+num);
                        }
                        window.location.href = url;
                    }
                });
                pager.goTo(parseInt(config.currentNum));
            });
