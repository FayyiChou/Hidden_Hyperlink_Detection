	<script type="text/javascript">﻿// copyright moowup 2016 | toute reproduction même partielle est formellement interdite //

// copyright moowup 2016 | toute reproduction même partielle est formellement interdite //

//var url_common_chercherVille = "/mon-moowup/favoris/groops.html";
var url_common = "/commun.html";
var url_common_activite_liste = "/mon-moowup/favoris/favoris.html";
var url_creamoow = "/creamoow/creamoow-gestion.html";

var desactivePopInContentPrincipalBackground = true;
var interval_verifModifData = null;
var closePage_callBackFunction = null;
var nom_page_active = null;

$.ajaxSetup ({
	cache: false,
	async: false,
	beforeSend: function() { $('#loader, .background-loader').show(); },
	complete: function() { $('#loader, .background-loader').hide(); },
	success: function() { $('#loader, .background-loader').hide(); }	
});


$(function ()
{
	/* datepicker http://jqueryui.com/datepicker/ */
	$.datepicker.setDefaults( $.datepicker.regional['fr'] );
	
	$("#dashboardChercherDate").click(function () {
		$(this).val('');
		$(this).blur();
	});
	
    $("#dashboardChercherDate").datepicker({
    	showButtonPanel: true,
    	dateFormat: 'dd/mm/yy',
    	minDate:0,
    	maxDate: "+2M"
    });
    
	
    // http://do-web.com/jcontent/settings
	$("div#footerSliderText").jContent({
		orientation: 'vertical', 
        easing: 'easeOutCirc', 
        duration: 1000,
        speed:1300,
        width:624,
        height:20,
        auto: true,
        direction: 'next', //or 'prev'
        pause: 1500,
        pause_on_hover: true
    });	

	
    $.ucfirst = function(str)
    {
        var text = str;
        var parts = text.split(' '),
            len = parts.length,
            i, words = [];
        for (i = 0; i < len; i++) {
            var part = parts[i];
            var first = part[0].toUpperCase();
            var rest = part.substring(1, part.length);
            var word = first + rest;
            words.push(word);
        }
        return words.join(' ');
    }	
	
	
	$('.copyright, .copyright a').click(function ()
	{
		var verifClass = $(this).hasClass('show');
		
		if (verifClass === false)
		{
			$(this).addClass('show');
			$(this).html('<div><a href="http://www.moowup.com" target="_blank"><b>moowup</b> - Leuville / Orge - © 2016</a></div>');
		}
		else
		{
			var copy = $(this).attr('data-copy');
			
			$(this).removeClass('show');
			$(this).html(copy);
		}
	});	
	
	
	$('#btFooterFlux').click(function ()
	{
		var verifClass = $(this).hasClass('show');
		
		if (verifClass === false) 
		{
			$('.buttonClose, .social').hide();
			$('#footerSliderText').removeClass('cache');
			
			$('#btFooterFlux span.icon').css('background-image','url(/images/google+-ui-sprite-colour.png)');
			
			$(this).addClass('show');
			
			var turn = $(this).attr('data-turnOn');
			$('span', this).html(turn);
			
		} 
		else 
		{
			$('.buttonClose, .social').show();
			$('#footerSliderText').addClass('cache');
			
			Core.iniBtFooter();
			
			$(this).removeClass('show');
			
			var turn = $(this).attr('data-turnOff');
			$('span', this).html(turn);			
		}
	});		
});


//récupérer variables d'url
$.extend({
  getUrlVars: function(){
    var vars = [], hash;
    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
    for(var i = 0; i < hashes.length; i++)
    {
      hash = hashes[i].split('=');
      vars.push(hash[0]);
      vars[hash[0]] = hash[1];
    }
    return vars;
  },
  getUrlVar: function(name){
    return $.getUrlVars()[name];
  }
});

//FIX input placeholder IE
function ieInputFix() {
	//FIX input placeholder IE
	var input = document.createElement("input");
    if(('placeholder' in input)==false) { 
		$('[placeholder]').focus(function() {
			var i = $(this);
			if(i.val() == i.attr('placeholder')) {
				i.val('').removeClass('placeholder');
				if(i.hasClass('password')) {
					i.removeClass('password');
					this.type='password';
				}			
			}
		}).blur(function() {
			var i = $(this);	
			if(i.val() == '' || i.val() == i.attr('placeholder')) {
				if(this.type=='password') {
					i.addClass('password');
					this.type='text';
				}
				i.addClass('placeholder').val(i.attr('placeholder'));
			}
		}).blur().parents('form').submit(function() {
			$(this).find('[placeholder]').each(function() {
				var i = $(this);
				if(i.val() == i.attr('placeholder'))
					i.val('');
			})
		});
	}  	
}


var accessVar = $.getUrlVar('access');

if (accessVar)
{
	if (accessVar == "denied" || accessVar == "denied#") 
	{
		$('.tn-box').addClass('tn-box-color-2');
		$('.tn-box').addClass('tn-box-active');
		$('#messageTxt').html('<div class="symbol" style="float: left; width: 30px; color: #990000;">×</div><div style="float: left; width: 300px; margin-top: 10px; margin-left: 10px;font-size:14px">Vous avez été déconnécté de votre compte !</div>');	
		setTimeout(function() {
			$('.tn-box').remove(); 
			location.href = '/'; 
		}, 2000);
	}
}	


var Core =
{
	tooltip: function() { if (!Modernizr.touch) { $('.tooltip, div[rel="tooltip"]').tooltipster(); } },
	tooltipContent: function() { if (!Modernizr.touch) { $('.tooltipContent, div[rel="tooltipContent"]').tooltipster(); } },
	
	iniBtFooter: function() { $('#btFooterFlux span.icon, #btFooterQuiSommesNous span.icon, #btFooterContact span.icon, #btFooterCgu span.icon, #btPubFooter span.icon').removeAttr('style'); },
	
	showContent: function(colorArray, breadCrumbArray, closeColorArray)
	{
		
		$('#footerBarContent').slideDown(1000,'easeOutQuad',function(){});//callback 
		$('.menu').css({'display':'none'});
		
		$('#dashboardAccordion').addClass('dashboardAjustement');
		$('#dashboardAccordion .btn').css('width','183px');
		
		$('.leftColorBlock').css({'background-color' : colorArray}); 
		$('.welcome, .inner').css({'background-color' : colorArray});

	    if (Modernizr.touch) {
	      $('body').css({'background-color' : colorArray+'  !important'});
	      $('.ui-accordion').css({'width' : '209px'});
	    }

	    //$('.welcome, .inner, .dashboardContent').css({'background-color' : colorArray});
		$('.welcome, .inner').addClass('color');
		$('.welcome').css({'border' : '5px solid #F8F8F8'});
		//$('.pseudoWelcome').css({'width' : '190px'});
		
		$('.dashboardContent').css({'height' : '416px'});
		$('.dashboardContent').css({'-webkit-border-radius' : '0 0 0 10px'});
		$('.dashboardContent').css({'border-radius' : '0 0 0 10px'});
		
		$('.dashboardContent').css({'border-top' : '0px solid #F8F8F8'});
		$('.dashboardContent').css({'border-bottom' : '5px solid #F8F8F8'});						
		$('.dashboardContent').css({'border-left' : '5px solid #F8F8F8'});
		$('.dashboardContent').css({'border-right' : '5px solid #F8F8F8'});
		
		$('.dashboardSubContent').css({'border-left' : '5px solid #F8F8F8'});
		$('.dashboardSubContent').css({'border-right' : '5px solid #F8F8F8'});						
		
		$('.dashboardSubContent p').css({'width' : '194px'});
		$('.dashboardSubContent p').css({'border' : '10px solid '+colorArray+''});
				
		if (!Functions.empty(breadCrumbArray)) {
			$('.contentHeader').addClass(breadCrumbArray);
		}
		
		$('.close').remove();
		$('#navBigButtons #footerBarContent').append('<span class="close tooltipContent" title="Quitter"><i class="fa fa-times"></i></span>');
		$('.close').addClass(closeColorArray);
		
		$('.close').click(function(e)
		{
			if(typeof closePage_callBackFunction === "function") {
				closePage_callBackFunction();
			} else {
				Core.hideContent();
			}
			
			popInContentPrincipal_callBackFunction = null;
			$('#footerBar .buttons a').removeClass("selected");
		});	
		
		$(document).keyup(function(e)
		{
			if (e.keyCode == 27)
			{
				if(typeof closePage_callBackFunction === "function") {
					closePage_callBackFunction();
				} else {
					Core.hideContent();
				}				
			}
			
			popInContentPrincipal_callBackFunction = null;
			$('#footerBar .buttons a').removeClass("selected");
		});	
		
		Core.tooltipContent();
	},
	
	hideContent: function ()
	{
		if (arguments.length > 0) {
			var param_page = arguments[0];
		}
		
		deleteVarPageSite();
		
		
		$('.tooltipContent, div[rel="tooltipContent"]').tooltipster('destroy');
		
		$('#dashboardContentMask').addClass('cache');
		$('#dashboardContentMask').unbind('click');
		
		$(document).unbind('keydown');
		
		if (typeof closeContentPrincipal === "function") {
			closeContentPrincipal();
		}
		
		if (typeof closePopInContent === "function") {
			closePopInContent();
		}		
		
		$('#calendar').unbind();
		
		if (true === cnn) {
			$('#calendar').fullCalendar('destroy');
		}
		
		$('#dashboardAccordion').removeClass('dashboardAjustement');
		$('#dashboardAccordion .btn').removeAttr('style');
		
		$('.menu .content').removeClass('expanded');
		$('.menu li').removeClass('cover');	
		$('#footerBarContent').slideUp(1000,'easeOutExpo',function(){});
		$('.welcome').css({'background-color' : '#34393D'});
		$('.inner').css({'background-color' : '#f8f8f8'});
		$('.welcome').css({'border' : '0px solid #F8F8F8'});

	    if (Modernizr.touch) {
	      $('html, body').css({'background-color' : 'white'});
	      $('.ui-accordion').css({'width' : '220px'});
	    }

		$('#btFooterQuiSommesNous span.icon, #btFooterContact span.icon, #btFooterCgu span.icon, #btPubFooter span.icon').removeAttr('style');
		
		$('.dashboardContent').css({'height' : '426px'});
		$('.dashboardContent').css({'-webkit-border-radius' : '0'});
		$('.dashboardContent').css({'border-radius' : '0'});		
		
		$('.dashboardContent').css({'border-top' : '5px solid #F8F8F8'});
		$('.dashboardContent').css({'border-bottom' : '5px solid #F8F8F8'});
		$('.dashboardContent').css({'border-left' : '0px solid #F8F8F8'});
		$('.dashboardContent').css({'border-right' : '0px solid #F8F8F8'});	

		$('.dashboardSubContent').css({'border-left' : '0px solid #F8F8F8'});
		$('.dashboardSubContent').css({'border-right' : '0px solid #F8F8F8'});
		
		$('.dashboardSubContent p').css({'width' : '204px'});
		$('.dashboardSubContent p').css({'border' : '10px solid #34393D'});
		
		$('.close').remove();
		$('.menu').css({'display':'block'});
		$('#footerBar .buttons a').removeClass("selected");
		
		$('#dashboardAccordion').removeClass('dashboardAjustement');
	}
};


//fonction de chargement dynamique fichier js/css
var filesadded = "" //list of files already added

var Functions =
{
	closePopinAide: function() {
		$('.pop-in-content-principal.pop-in-normal .content div.videoHTML').remove();
	},
	
	insertVideoPopinAide: function(url_video)
	{
		return '<iframe class="-display-none" width="460" height="259" src="//www.youtube.com/embed/'+url_video+'" frameborder="0" allowfullscreen></iframe>';
	},
		
	verifMailStr: function (mail)
	{
		if (true == Functions.empty(mail)) {
			return false;
		}
		
		var reEmail = /^([a-zA-Z0-9]+(([\.\-\_]?[a-zA-Z0-9]+)+)?)\@(([a-zA-Z0-9]+[\.\-\_])+[a-zA-Z]{2,4})$/gi;
	    var result = mail.match(reEmail);

	    if (result == null) {
	        return false;
	    } else {
	        return true;
	    }
	},		
		
	verifEmail: function (email)
	{
		var jqXHR = $.ajax({
		type: "POST",
		async: true,
		url: url_common,
		data: "param=verif-email&id="+email,
		dataType: "json",
		async: false,
		success: function(json) {}
		});

		var json = $.parseJSON( jqXHR.responseText );
		
		return json.data;
	},
		
	encodeBr: function (str)
	{
		return str.replace(/\n/gm,'{br}');	    
	},		
	
	decodeBr: function (str)
	{
		if (true == Functions.empty(str)) {
			return '';
		}

		str = str.replace(/\{br\}/gm, "\n");
		
		return str;		
	},	
	
	setOptionInit: function (id)
	{
		var test = $('#'+id).length;
		if (test > 0) { document.getElementById(id).options.length = 0; }
	},	
	
	setOption: function (id, key, val)
	{
		var bDefaultSelected = bSelected = false;
		
		var post_params = {};
		if (arguments.length == 5) 
		{
			bDefaultSelected = arguments[3]; // Boolean that indicates whether the option is the default selection.
			bSelected = arguments[4]; // Boolean that indicates whether this option is selected when it is added to the collection.
		}
		
		var test = $('#'+id).length;
		
		if (test > 0) { document.getElementById(id).options[document.getElementById(id).options.length] = new Option(val, key,bDefaultSelected, bSelected); }
	},
	
	ucfirst: function (string)
	{
		if (true == Functions.empty(string)) { return ''; }
	    return string.charAt(0).toUpperCase() + string.slice(1);
	},	
	
	lowercase: function (string)
	{
		if (true == Functions.empty(string)) { return ''; }
	    return string.toLowerCase();
	},	
	
	uppercase: function (string)
	{
		if (true == Functions.empty(string)) { return ''; }
	    return string.toUpperCase();
	},	
		
	substr: function (data, limit, point)
	{
		if (true == Functions.empty(data)) {
			return data;
		}
		
		var data_length = data.length;
		
		if (data_length > limit) {
			return data.substr(0, limit)+(point != '' ? point : '');
		} else {
			return data;
		}
	},
		
	isEmpty: function (obj) 
	{
		if (typeof obj == 'undefined' || obj === null || obj === 'null' || obj === '') return true;
		if (typeof obj == 'number' && isNaN(obj)) return true;
		if (obj instanceof Date && isNaN(Number(obj))) return true;
		
		return false;
	},

	empty: function (data)
	{
		return (data == '' || data == 'null' || data == null || data == 'undefined' || data == undefined || data == '0' ? true : false);
	},
	
	checkLoadJsCssFile: function(filename, filetype)
	{
	 if (filesadded.indexOf("["+filename+"]") == -1)
	 {
	  this.loadJsCssFile(filename, filetype)
	  filesadded+="["+filename+"]" //List of files added in the form "[filename1],[filename2],etc"
	 }
	 else
	  alert("file already added!")
	},
	
	loadJsCssFile: function(filename, filetype)
	{
	 if (filetype=="js"){ //if filename is a external JavaScript file
	  var fileref=document.createElement('script')
	  fileref.setAttribute("type","text/javascript")
	  fileref.setAttribute("src", filename)
	 }
	 else if (filetype=="css"){ //if filename is an external CSS file
	  var fileref=document.createElement("link")
	  fileref.setAttribute("rel", "stylesheet")
	  fileref.setAttribute("type", "text/css")
	  fileref.setAttribute("href", filename)
	 }
	 if (typeof fileref!="undefined")
	  document.getElementsByTagName("head")[0].appendChild(fileref)
	}	
};		


function var_dump (tab)
{
    if(arguments.length == 2) {var indent = arguments[1] + "\t";}
    else{ var indent = "\t";}
    var i = 0;
    var elements="";
    for (var elt in tab) {
        elements += (i ? ",\n " : " ") + indent + "[" + elt + "]:";
        switch (typeof tab[elt])
        {
            case "string" : elements += "\"" + tab[elt] + "\""; break;
            case "number" : elements += tab[elt]; break;
            case "object" :
            if (tab[elt] == null) elements += "*null*";
            else if (tab[elt]) elements += var_dump(tab[elt], indent); break;
            case "undefined" : elements += "*undefined*"; break;
            default : elements += tab[elt];
        }
        i++;
    }
    return "Array(" + i + ") {\n" + elements + "\n" + (arguments[1] ? arguments[1] : "") + "}";
}


function buildLieuRDV(idVille, idFamille, idType, idActivite, id_select)
{	
	$.ajax({
	   type: "POST",
	   async: true,
	   cache: false,
	   url: url_creamoow,
	   data: "param=get-lieu-byid&idVille="+idVille+"&idFamille="+idFamille+"&idType="+idType+"&idActivite="+idActivite,
	   dataType: "json",
	   success: function(json)
	   {
		   if (json.etat == 'ok')
		   {
			   var cible = document.getElementById('form_Moow_LieuRdv');
			   while (cible.hasChildNodes()) {
			   cible.removeChild(cible.lastChild);
			   }			   
			   
			   var value_proximite = json.data.proximite;
			   var value_pratique = json.data.praticable;
			   var value_partenaire_1 = json.data.partenaire_1;
			   var value_partenaire_2 = json.data.partenaire_2;
			   var value_partenaire_3 = json.data.partenaire_3;
			   
			   var oSelect = document.getElementById('form_Moow_LieuRdv');
			   
			   oSelect.options[oSelect.options.length] = new Option("-", 'xxx');
			   
			   // init
			   $('#form_Moow_LieuRdv').removeAttr('disabled');
			   
			   // ligne par default
			   var oGroup = document.createElement('optgroup'); 

			   oSelect.appendChild(oGroup);
			   oGroup.label = "Je propose";				// Je propose	   
			   
			   oSelect.options[oSelect.options.length] = new Option("- De créer un lieu pour le moow...", 'new');			   
		//	   oSelect.options[oSelect.options.length] = new Option("D'utiliser mon adresse", 'adr');			   
			   			   
			   if (!Functions.empty(value_proximite))
			   {
				   var oGroup = document.createElement('optgroup'); 
				   oSelect.appendChild(oGroup);
				   oGroup.label = "De choisir un lieu de proximité"; 	// De Proximité
	
				   for (var key in value_proximite)
				   {
					   oSelect.options[oSelect.options.length]
					   = new Option('- '+value_proximite[key]['lieu_nom'], value_proximite[key]['lieu_ID']);	// modifié
				   }
	
				   if (false == Functions.empty(id_select) && typeof(id_select) !== "function") {
					   $('#form_Moow_LieuRdv').val(id_select);
				   } else if (typeof(id_select) === "function") {
					   id_select();
				   }
			   }
			   
			   if (!Functions.empty(value_pratique))
			   {
				   var oGroup = document.createElement('optgroup'); 
				   oSelect.appendChild(oGroup);
				   oGroup.label = "De choisir un lieu de pratique"; 		// de pratique
				   
				   for (var key in value_pratique)
				   {
					   oSelect.options[oSelect.options.length]
					   = new Option('- '+value_pratique[key]['lieu_nom'], value_pratique[key]['lieu_ID']);	//modifié
				   }
	
				   if (false == Functions.empty(id_select) && typeof(id_select) !== "function") {
					   $('#form_Moow_LieuRdv').val(id_select);
				   } else if (typeof(id_select) === "function") {
					   id_select();
				   }	
		   		}
			   			   
			   if (!Functions.empty(value_partenaire_1))
			   {
				   var oGroup = document.createElement('optgroup'); 
				   oSelect.appendChild(oGroup);
				   oGroup.label = "De choisir un lieu partenaire"; 		// Partenaire
				   
				   for (var key in value_partenaire_1)
				   {
					   oSelect.options[oSelect.options.length]
					   = new Option('- '+value_partenaire_1[key]['lieu_nom']+' ('+value_partenaire_1[key]['ville_nom']+')', value_partenaire_1[key]['lieu_ID']);
				   }
	
				   if (false == Functions.empty(id_select) && typeof(id_select) !== "function") {
					   $('#form_Moow_LieuRdv').val(id_select);
				   } else if (typeof(id_select) === "function") {
					   id_select();
				   }	
		   		}			   
			   
				if (!Functions.empty(value_partenaire_2))
			   	{
				   for (var key in value_partenaire_2)
				   {
					   oSelect.options[oSelect.options.length]
					   = new Option('- '+value_partenaire_2[key]['lieu_nom']+' ('+value_partenaire_2[key]['ville_nom']+')', value_partenaire_2[key]['lieu_ID']);
				   }
	
				   if (false == Functions.empty(id_select) && typeof(id_select) !== "function") {
					   $('#form_Moow_LieuRdv').val(id_select);
				   } else if (typeof(id_select) === "function") {
					   id_select();
				   }	
		   		}
			   
			   if (!Functions.empty(value_partenaire_3))
			   {
				   for (var key in value_partenaire_3)
				   {
					   oSelect.options[oSelect.options.length]
					   = new Option('- '+value_partenaire_3[key]['lieu_nom']+' ('+value_partenaire_3[key]['ville_nom']+')', value_partenaire_3[key]['lieu_ID']);
				   }
	
				   if (false == Functions.empty(id_select) && typeof(id_select) !== "function") {
					   $('#form_Moow_LieuRdv').val(id_select);
				   } else if (typeof(id_select) === "function") {
					   id_select();
				   }	
		   		}
		   } else {
		   }
	   },
	   error: function(e){
		}
	});
}


function setTermVille(terme)
{
	var jqXHR = $.ajax({
        type: "POST",
        async: false,
        //contentType: "application/json; charset=utf-8",
        url: url_common,
        data: "param=get-villes-by-terme&terme="+terme,
        dataType: "json",
        success: function(json) {}
	});

	var json = $.parseJSON( jqXHR.responseText );
	return json.data;
}


function setTermActivity(terme)
{
	var jqXHR = $.ajax({
        type: "POST",
        async: false,
        url: url_common,
        data: "param=get-activity-by-terme&terme="+terme,
        dataType: "json",
        success: function(json) {}
	});

	var json = $.parseJSON( jqXHR.responseText );
	return json.data;
}


function getJoursNaissance()
{
	var html = '';
	
	for(var i=1; i<32; i++)
	{
		html += '<option value="'+(i<10 ? '0'+i : i)+'">'+i+'</option>';
	}
	
	return html;
}


function getMoisNaissance()
{
	var html = '';
	
	for(var i=1; i<13; i++)
	{
		html += '<option value="'+(i<10 ? '0'+i : i)+'">'+i+'</option>';
	}
	
	return html;
}


$(function($)
{

/*	$.timepicker.regional['GB'] = {
			currentText: 'Now',
			closeText: 'Done',
