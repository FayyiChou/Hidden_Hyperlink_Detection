var dashboardChercherFamille = null;
var dashboardChercherVille = null;
var dashboardChercherPratique = null;	
var dashboardChercherGenre = null;	
var dashmoowChercherDate = null;


$(function ()
{

	$(document).on('change', "#dashboardChercherFamille, #formChercherFamille", function( event )  // devrait être bon
	{
		var idAttr = $(this).attr('id');
		var idFamille = $(this).val();
		$('#dashboardChercherFamille, #formChercherFamille').val(idFamille);	

//		document.getElementById('dashboardChercherPratique').options.length = 0;
//		document.getElementById('formChercherPratique').options.length = 0;
		
		if (idFamille == 'xxx') 
		{
			Functions.setOptionInit('dashboardChercherPratique');
			Functions.setOptionInit('formChercherPratique');
			
			$('#dashboardChercherPratique, #formChercherPratique').attr('disabled','disabled');				
		} 
		else
		{
			$('#dashboardChercherFamille, #formChercherFamille').removeAttr('style');
			
			$.ajax({
			   type: "POST",
			   async: true,
			   url: url_common,
			   data: "param=get-fonction-by-famille&id="+idFamille,
			   dataType: "json",
			   success: function(json)
			   {
				   if (json.etat == 'ok')
				   {
					   var value = json.data;
					   Functions.setOptionInit('dashboardChercherPratique');
					   Functions.setOptionInit('formChercherPratique');
						
					   $('#dashboardChercherPratique, #formChercherPratique').removeAttr('disabled');

					   Functions.setOption('dashboardChercherPratique', 'xxx', 'Choisir une pratique...');
					   Functions.setOption('formChercherPratique', 'xxx', 'Choisir une pratique...');

					   for (var key in value)
					   {
						   Functions.setOption('dashboardChercherPratique', value[key]['fonction_ID'], value[key]['fonction_phrase_demo']);
						   Functions.setOption('formChercherPratique', value[key]['fonction_ID'], value[key]['fonction_phrase_demo']);
					   }

				   } else {
				   }
			   }
			 });
		}
	});
	

	$("#dashboardChercherVille").click(function () 	// semble bon
	{ 
		$(this).val(''); 

		var p = $("#dashboardChercherVille").position();
		$('.ui-autocomplete').css('display','none').css('left',p.left).css('top', (p.top+=2) );
		
		$('#dashboardChercherVille_hidden').val('');

	});
	
	$("#dashboardChercherVille").autocomplete(		// semble bon	
	{
        source: function (request, response)
        {
        	var term = request['term'];
        	
        	if (true == Functions.empty(term)) {
        		return;
        	}
        	
        	var data = setTermVille(term);
        	
        	response($.map( data, function(item)
        	{
                return {
                    id: item.ville_ID,
                    ville: item.ville_nom,
                    cp: item.ville_code_postal
                 };
            }));        	
        },
        open: function()
        { 
            $('.ui-menu').width(210);
        } ,
        minLength: 2,
        select: function(event, ui)
        {
        	var ville = ui.item.ville;
        	var idVille = ui.item.id;
        	var cp = ui.item.cp;
        	
            $('#dashboardChercherVille_hidden').val(idVille);
            
            event.preventDefault();
            
            this.value = 
            (ville.length > 15 ? ville.substr(0,15)+'...' : ville) + ' - ' + cp;
        }
        
    }).data("autocomplete")._renderItem = function (ul, item)
    {
        return $("<li></li>")
        .data("item.autocomplete", item)
        .append("<a>" + (item.ville.length > 17 ? item.ville.substr(0,17)+'...' : item.ville) + "<span style='float:right'>" + item.cp + "</span></a>")
        .appendTo(ul);
	};

});



function getChercherUnMoow(type)
{
	$('#dashboardChercherFamille, #formChercherFamille').removeAttr('style');
	$('#dashboardChercherVille, #formChercherVille').removeAttr('style');


	if (type == 'dashboard')
	{
		var var_famille = $('#dashboardChercherFamille').val();
		var var_ville = $('#dashboardChercherVille_hidden').val();
		var var_date = $('#dashmoowChercherDate').val();
		var var_pratique = $('#dashboardChercherPratique option:selected').val();
//		var var_pratique = $('#dashboardChercherPratique').val();
		var var_genre = $('#dashboardChercherGenre').val();
	}
	else if (type == 'page')
	{
		var var_famille = $('#formChercherFamille').val();
		var var_ville = $('#formChercherVille_hidden').val();
		var var_date = $('#formoowChercherDate').val();
		var var_pratique = $('#formChercherPratique option:selected').val();
//		var var_pratique = $('#formChercherPratique').val();
		var var_genre = $('#formChercherGenre').val();
	}

	if (true == Functions.empty(var_ville))
	{
		$('#dashboardChercherVille, #formChercherVille').css('border', '1px solid blue');
		return;
	}	

/*	if (true == Functions.empty(var_pratique)) {
	{
		$('#dashboardChercherPratique, #formChercherPratique').css('border', '1px solid blue');
		return;
	}
*/
	if (var_famille == 'xxx') {
		$('#dashboardChercherFamille, #formChercherFamille').css('border', '1px solid blue');
		return;
	}
	
	$("#loadedFooterBarContent").load('demoow-result.html',
	{
		famille 	: var_famille,
		ville		: var_ville,
		date		: var_date,
		pratique	: var_pratique,
		genre		: var_genre
	}
//	,function (responseText, textStatus, XMLHttpRequest)
	,function (responseText, textStatus, XMLHttpRequest)
	{
//			alert("Erreur au chargement du ( "+var_pratique+" ) !!!");

		if (textStatus == "error") 
		{
			alert("Erreur au chargement du ( "+XMLHttpRequest+" ) !!!");
		} 
		else 
		{
			Core.iniBtFooter();
			
			if (!Modernizr.touch) {
				//Core.tooltip();
				ieInputFix();
			}			
		}
	});
}


function initFormDemoow()
{

	$('#dashboardChercherFamille, #formChercherFamille, #dashboardChercherVille, #dashboardChercherVille_hidden, #formChercherVille_hidden, #dashboardformChercherVille_hidden, #dashboardChercherGenre, #formChercherGenre, #dashboardChercherPratique, #formChercherPratique, #dashmoowChercherDate, #formoowChercherDate').val('');
	$('#formChercherVille, #formChercherFamille').css('border-color', '#eab89f');
	$('#dashboardChercherFamille, #dashboardChercherVille, #dashboardChercherGenre, #dashboardChercherPratique, #dashmoowChercherDate').removeAttr('style');	

}

