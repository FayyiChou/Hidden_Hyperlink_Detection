			isRTL: false
		};

	$.timepicker.regional['fr'] = {
			currentText: 'Maintenant',
			closeText: 'Fermer',
			amNames: ['AM', 'A'],
			pmNames: ['PM', 'P'],
			timeFormat: 'HH:mm',
			timeSuffix: '',
			timeOnlyTitle: 'Choisir Heure',
			timeText: 'Heure',
			hourText: 'Heures',
			minuteText: 'Minutes',
			secondText: 'Secondes',
			millisecText: 'Millisecond',
			microsecText: 'Microsecond',
			timezoneText: 'Time Zone',
			isRTL: false
		};
*/
	$.datepicker.regional['en-GB'] = {
