$('.menu .menuLi:nth-child(5)'),
$('.menu .menuLi:nth-child(6)'),
$('.menu .menuLi:nth-child(7)'),
$('.menu .menuLi:nth-child(8)'),
$('.menu .menuLi:nth-child(9)')
];	

$.each(pageArray, function(index, value) 
{ 
	$(btArray[index]).click(function()
	{
		if(value == 'bonplans') {
			$("#dashboardAccordion").accordion("activate", 4);
		}
		
		loadPage(pageArray[index]);
	});
});


function loadModule(page, params, id)
{
	if (!Functions.empty(params)) {
		params = "?"+params;
	}
	
	$(id).load(page+".html"+params, function (responseText, textStatus, XMLHttpRequest)
	{
		if (textStatus == "error") {
			 alert("Erreur au chargement du module ( "+page+" ) !!!");
		} else {
			nom_page_active = page;
		}		
	});
}


function loadPage(page)
{
	deleteVarPageSite();
	
	var post_params = {};
	if (arguments.length > 1) {
		post_params = arguments[1];
	}
	
	$("#loadedFooterBarContent").load(page+".html", post_params, function (responseText, textStatus, XMLHttpRequest)
	{
		Core.iniBtFooter();
		
		if (textStatus == "success") 
		{
			nom_page_active = page;

	//  =============================== Footer / Travail ==============================
			
			if(page == 'contact')
			{
				$('#btFooterContact span.icon').css('background-image','url(/images/google+-ui-sprite-colour.png)');
				Core.showContent('#34393D', 'breadcrumbContact', '');	
				$("#dashboardAccordion").accordion("activate", 2);
				$("#contentContact").mCustomScrollbar({ scrollButtons:{ enable:false } });
			}				
			
			if(page == 'cgu')
			{
				$('#btFooterCgu span.icon').css('background-image','url(/images/google+-ui-sprite-colour.png)');
				Core.showContent('#34393D', 'breadcrumbCgu', '');
				$("#dashboardAccordion").accordion("activate", 5);
				$("#contentCGU").mCustomScrollbar({ scrollButtons:{ enable:false } });
			}				
						
			if(page == 'annonceurs')
			{
				$('#btPubFooter span.icon').css('background-image','url(/images/google+-ui-sprite-colour.png)');
				Core.showContent('#69003F', 'breadcrumbPub', 'closeColor8');
			}
			
			if(page == 'apropos' || page == 'quisommesnous')
			{
				$('#btFooterQuiSommesNous span.icon').css('background-image','url(/images/google+-ui-sprite-colour.png)');
				Core.showContent('#714A28', 'breadcrumbQuisommesnous', 'closeColor3');
				$("#contentQuisommesnous").mCustomScrollbar({ scrollButtons:{ enable:true } });
			}				
			
	//  =============================== Travail ==============================

			if(page == 'inscription') 
			{
				Core.showContent('#789AB7', 'breadcrumbInscription', 'closeColor1');
				//loadInscriptionForm();
			}
			
			if(page == 'connexion') 
			{
				Core.showContent('#91AB31', 'breadcrumbConnexion', 'closeColor2');
				//loadConnexionForm();
			}
	
			if(page == 'buzz')
			{
				Core.showContent('#E58600', 'breadcrumbPlan', 'color-Color6');
				$("#contentBuzz").mCustomScrollbar({ scrollButtons:{ enable:false } });
			}
			
			if(page == 'demoow')
			{
				$("#dashboardAccordion").accordion("activate", 0);
				Core.showContent('#C64500', 'breadcrumbDemo', 'closeColor5');
			}				
			
			if(page == 'aide')
			{
				$("#dashboardAccordion").accordion("activate", 2);
				Core.showContent('#7F5DAC', 'breadcrumbAide', 'color-Color4');
				$("#contentAide").mCustomScrollbar({ scrollButtons:{ enable:true } });
			}	
			
			if(page == 'bonplans')
			{
				Core.showContent('#5477B7', 'breadcrumbPlan', 'closeColor7');
				$("#dashboardAccordion").accordion("activate", 6);
			}

			if(page == 'moower')
			{
				Core.showContent('#91AB31', 'breadcrumbConnexion', 'closeColor2');
				$("#dashboardAccordion").accordion("activate", 1);
			}
			
			if(page == 'boutique')
			{
				$("#dashboardAccordion").accordion("activate", 4);
				Core.showContent('#393043', 'breadcrumbBoutique', 'color-Color9');
				$("#contentBoutique").mCustomScrollbar({ scrollButtons:{ enable:true } });
			}

			if(page == 'unsubscribe') 
			{
				$("#dashboardAccordion").accordion("activate", 4);
				Core.showContent('#E58600', 'breadcrumbPlan', 'color-Color6');
			}

		}

		if (!Modernizr.touch) {
			//Core.tooltip();
			ieInputFix();
		}
		
		if (textStatus == "error") 
		{
			 alert("Erreur au chargement de la page !!!");
		}
	});	
}


$(function ()
{
	var get_page = $.getUrlVar('p');
	var get_page_type = $.getUrlVar('ptype');	
	
	if (false === Functions.empty(get_page) 
		&& get_page == 'connexion' 
		&& get_page_type == 'activation')
	{
		var email = $.getUrlVar('m');
		var key = $.getUrlVar('ukey');
		
		loadPage('connexion', {p:'connexion', ptype:'activation', ukey:key, m:email, });
	}
	
	
	if (false === Functions.empty(get_page) 
		&& get_page == 'connexion' 
		&& get_page_type == 'newpassword')
	{	
		var email = $.getUrlVar('m');
		
		loadPage('connexion', {p:'connexion', ptype:'newpassword', m:email, });
	}


	if (false === Functions.empty(get_page) 
		&& get_page == 'unsubscribe' 
		&& get_page_type == 'unsubscribe')
	{
		var email = $.getUrlVar('m');
		var key = $.getUrlVar('ukey');
		
		loadPage('unsubscribe', {p:'unsubscribe', ptype:'unsubscribe', ukey:key, m:email, });
	}

});

function getPubForm()
{
	$.ajax({
		type: "POST",     
		url: url_common,
		async: true,		// false
		data:'param=pub-visited',
		dataType: 'json',
		success: function(d)
		{
			if (d.etat == 'ok') 
			{
				if (true !== Functions.empty(d.data) && d.data != "error") 
				{
					var pub = d.data;
					var pub_ID = pub['publicis_ID'];
					var pub_send = pub['publicis_send'];
					var pub_count = pub['publicis_count'];
					var pub_click = pub['publicis_click'];
				//	var type = ;
				//	var pays = ;

					$('#PubForm').html('<a href="http://'+pub['publicis_url']+'" target="_blank"><img src="/medias/publicis/'+pub['publicis_image']+'.gif" alt="'+pub['partenaire_enseigne']+'" title="'+pub['publicis_texte']+'" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
					$('#PubForm').attr('data-id', pub['publicis_ID']).attr('data-count', pub['publicis_click']);

					setTimeout(function () {
					loadPubForm(pub_ID, pub_send, pub_count);
					}, 45000);	// 60000 = 1mn
				}			
				else if (d.data == "error")
				{
					$('#PubForm').html('<a href="http://www.moowup.com" target="_blank"><img src="/medias/publicis/moowup.gif" alt="moowup" title="Contactez nous !" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
					$('#PubForm').attr('data-id', '1').attr('data-count', '1');

					setTimeout(function () {
					getPubForm();
					}, 45000);	// 60000 = 1mn
				}			
			}
			else if (d.etat == 'ko' )
			{
				$('#PubForm').html('<a href="http://www.moowup.com" target="_blank"><img src="/medias/publicis/moowup.gif" alt="moowup" title="Contactez nous !" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
				$('#PubForm').attr('data-id', '1').attr('data-count', '1');
			}			
		}
	});
}


function loadPubForm(PubID, Pubsend, Pubcount)
{
	$.ajax({     
		type: "POST",     
		url: url_common,
		async: true,		// false
		data:"param=reload-pub-visited&IdPub=" + PubID + "&SendPub=" + Pubsend + "&CountPub=" + Pubcount,
		dataType: 'json',
		success: function(d)
		{
			if (d.etat == 'ok') 
			{
				if (true !== Functions.empty(d.data) && d.data != "error") 
				{
					var pub = d.data;
					var pub_ID = pub['publicis_ID'];
					var pub_send = pub['publicis_send'];
					var pub_count = pub['publicis_count'];
					var pub_click = pub['publicis_click'];
				//	var type = ;
				//	var pays = ;

					$('#PubForm').html('<a href="http://'+pub['publicis_url']+'" target="_blank"><img src="/medias/publicis/'+pub['publicis_image']+'.gif" alt="'+pub['partenaire_enseigne']+'" title="'+pub['publicis_texte']+'" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
					$('#PubForm').attr('data-id', pub['publicis_ID']).attr('data-count', pub['publicis_click']);

					setTimeout(function () {
					ReloadPubForm(pub_ID, pub_send, pub_count);
					}, 45000);	// 60000 = 1mn
				}			
				else if (d.data == "error")
				{
					$('#PubForm').html('<a href="http://www.moowup.com" target="_blank"><img src="/medias/publicis/moowup.gif" alt="moowup" title="Contactez nous !" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
					$('#PubForm').attr('data-id', '1').attr('data-count', '1');

					setTimeout(function () {
					getPubForm();
					}, 45000);	// 60000 = 1mn
				}			
			}
			else if (d.etat == 'ko' )
			{
				$('#PubForm').html('<a href="http://www.moowup.com" target="_blank"><img src="/medias/publicis/moowup.gif" alt="moowup" title="Contactez nous !" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
				$('#PubForm').attr('data-id', '1').attr('data-count', '1');
			}			
		}
	});
}

function ReloadPubForm(PubID, Pubsend, Pubcount)
{
	$.ajax({     
		type: "POST",     
		url: url_common,
		async: true,		// false
		data:"param=reload-pub-visited&IdPub=" + PubID + "&SendPub=" + Pubsend + "&CountPub=" + Pubcount,
		dataType: 'json',
		success: function(d)
		{
			if (d.etat == 'ok') 
			{
				if (true !== Functions.empty(d.data) && d.data != "error") 
				{
					var pub = d.data;
					var pub_ID = pub['publicis_ID'];
					var pub_send = pub['publicis_send'];
					var pub_count = pub['publicis_count'];
					var pub_click = pub['publicis_click'];
				//	var type = ;
				//	var pays = ;

					$('#PubForm').html('<a href="http://'+pub['publicis_url']+'" target="_blank"><img src="/medias/publicis/'+pub['publicis_image']+'.gif" alt="'+pub['partenaire_enseigne']+'" title="'+pub['publicis_texte']+'" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
					$('#PubForm').attr('data-id', pub['publicis_ID']).attr('data-count', pub['publicis_click']);

					setTimeout(function () {
					loadPubForm(pub_ID, pub_send, pub_count);
					}, 45000);	// 60000 = 1mn
				}			
				else if (d.data == "error")
				{
					$('#PubForm').html('<a href="http://www.moowup.com" target="_blank"><img src="/medias/publicis/moowup.gif" alt="moowup" title="Contactez nous !" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
					$('#PubForm').attr('data-id', '1').attr('data-count', '1');

					setTimeout(function () {
					getPubForm();
					}, 45000);	// 60000 = 1mn
				}			
			}
			else if (d.etat == 'ko' )
			{
				$('#PubForm').html('<a href="http://www.moowup.com" target="_blank"><img src="/medias/publicis/moowup.gif" alt="moowup" title="Contactez nous !" border="0" align="right" width="728" hspace="3" vspace="3" height="90"/></a>');
				$('#PubForm').attr('data-id', '1').attr('data-count', '1');
			}			
		}
	});
}


function deleteVarPageSite()
{
	if (true === Functions.empty(nom_page_active)) {
		return false;
	}
	
	switch(nom_page_active)
	{
		case 'connexion':
			break;
	}
	
	showPopInContentPrincipalMiniInit();
	showPopInContentPrincipalXBigInit();
	setColRightPopInContentPrincipalMiniInit();
	
	$('#form_Email_Pseudo').unbind('click');
	$('#form_Email').unbind('click');
//	$('.rightButtons.btQuestion').unbind('click');	// modifié
	
	delete desactivePopInContentPrincipalBackground;
	delete var_Iteration_mCustomScrollbar_aideModule;
	delete btSave_confirm;

//	delete helperContentRightButton;	

	$('.pop-in-content-principal .pop-in-close, .pop-in-content-principal-background').unbind('click');

	$(document).off('click', '.pop-in-content-principal.pop-in-big .pop-in-close');
	$(document).off('click', '.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .pop-in-close');
	$(document).off('click', '.pop-in-content-principal.pop-in-xbig .pop-in-close');
	
	// page inscription
	$(document).off('click', '#id_save_inscription_etape1');
	$(document).off('click', '#id_save_inscription_etape2');
	$(document).off('click', '#id_refresh_inscription_etape2');
	$(document).off('click', '#id_save_inscription_etape3');
	$(document).off('click', '#id_refresh_inscription_etape3');
	$(document).off('click', '#id_save_inscription_etape4');
	$(document).off('change', '#form_listeFamille, #form_listeTypes, #form_listeActivites, #form_DetailNiveau');
	$(document).off('change', '#form_DateNaissanceJour, #form_DateNaissanceMois, #form_DateNaissanceAnnees, #form_Genre');
	$(document).off('change', '#form_listeFamille');
	$(document).off('change', '#form_listeTypes');
	$(document).off('click', '#id_senddatauser_inscription');
	$(document).off('click', '#form_listeActivites');
	$(document).off('click', '#form_VilleResidence');
	$(document).off('click', '#btSave_confirm');

	$(document).off('click', 'form_Moow_TxtTitleDescriptionLarge');
	$(document).off('click', '#bt_form_Moow_Email');
	$(document).off('click', '#bt_form_Moow_Prenom');
	$(document).off('click', '#bt_form_Moow_Mobil');
	$(document).off('click', '#bt_form_Moow_Photo');

	$('#form_Name').unbind('click');
	$('#form_SurName').unbind('click');
	$("#form_VilleResidence").unbind();
	
	// page annonceurs
	$('#form_Prenom').unbind('click');
	$("#form_Ville").unbind();

	$(document).off('keyup', '#form_Password');
	$(document).off('hover', '#contentAgendaListEvents');
	$(document).off('hover', '#contentAide');	
	
	// general 
	$('.rightButtons.btQuestion').unbind('click');
	$('.rightButtons.btLeft').unbind('click');
	$('.rightButtons.btTrash').unbind('click');
	$('.rightButtons.btSave').unbind('click');
	$('.rightButtons.btRight').unbind('click');	
	$('.rightButtons.btUp').unbind('click');
	$('.rightButtons.btDown').unbind('click');
	$('.rightButtons.btCrea').unbind('click');	
	$('.rightButtons.btUserGroup').unbind('click');
	$('.rightButtons.btMoowup').unbind('click');
	$('.rightButtons.btActualise').unbind('click');
	
	$(document).off('click', '.rightButtons.btQuestion');
	$(document).off('click', '.rightButtons.btLeft');
	$(document).off('click', '.rightButtons.btTrash');
	$(document).off('click', '.rightButtons.btSave');
	$(document).off('click', '.rightButtons.btRight');
	$(document).off('click', '.rightButtons.btUp');
	$(document).off('click', '.rightButtons.btDown');	
	$(document).off('click', '.rightButtons.btCrea');
	$(document).off('click', '.rightButtons.btUserGroup');
//	$(document).off('click', '.rightButtons.btMoowup');
	$(document).off('click', '.rightButtons.btActualise');

	return true;
}



