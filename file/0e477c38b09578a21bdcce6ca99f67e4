function closePopInContentPrincipalBig()
{
	$('.pop-in-content-principal-background').addClass('display-none');
	$('.pop-in-content-principal.pop-in-big').addClass('display-none');
	$('.pop-in-content-principal.pop-in-big .content').removeAttr('style');

	closePopInContent();
}

function showPopInContentMiniUserInfo()
{
	$('.pop-in-content-principal-background-infouser').removeClass('display-none');
	$('.pop-in-content-principal.pop-in-infouser').removeClass('display-none');
}

function setPastillePopInContentPrincipalMini(titre)
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal div.pastille').removeClass('cache');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal div.pastille h4').html( titre );
}

function setTextPopInContentPrincipalMini(titre, description)
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .description span').html( titre );
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-normal').html( description );
}

function setTextPopInContentPrincipalMiniOptional(html)
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-optional').html( html );
}

function setColRightPopInContentPrincipalMiniNormalZindex(zIndex)
{
	$('.pop-in-content-principal-background').css('z-index', (zIndex-1) );
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal').css('z-index', zIndex);
}

function setColRightPopInContentPrincipalMiniNormalClass(cls)
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-normal').addClass(cls).removeClass('cache');
}

function setColRightPopInContentPrincipalMiniOptionalClass(cls)
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-optional').addClass(cls).removeClass('cache');
}

function setColRightPopInContentPrincipalMiniMarginTop(margin)
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content .content-right').css('margin-top', margin);
}

function setColRightPopInContentPrincipalMini(icon_name, id)
{
	var html = '<span class="pop-in-bt-right" id="' + id + '"><i class="fa ' + icon_name + '"></i></span>';
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-right').append( html );
}

function setColRightPopInContentPrincipalMiniInit()
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-right').html('');
}

function showPopInContentPrincipalMiniInit()
{
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal div.pastille').addClass('cache');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal div.pastille h4').html('');

	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal').removeAttr('data-name');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal').removeAttr('style');

	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal span.pop-in-close').removeAttr('style');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .description.pop-in-mini').removeAttr('style');

	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .pastille span').html( '' ).addClass('cache');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .description span').html( '' );

	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content .content-right').html('');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content .content-right').css('margin-top', '0');

	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-normal').html( '' );
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-optional').html('');

	var _this = $('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-normal');
	$(_this).removeAttr('class').addClass('content-html').addClass('mini-normal');

	var _this = $('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-html.mini-optional');
	$(_this).removeAttr('class').addClass('content-html').addClass('mini-optional').addClass('cache');
}


function showPopInContentPrincipalMini()
{
	var verif = $('div.pop-in-content-principal-background').hasClass('display-none');

	$('.pop-in-content-principal-background').removeClass('display-none');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal').removeClass('display-none');
}

function closePopInContentPrincipalMini()
{
	$('.pop-in-content-principal-background').addClass('display-none');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal').addClass('display-none');

	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-right').html('');
	setTextPopInContentPrincipalMini('Moowup', '');

	closePopInContent();
}

function showPopInContentPrincipalMiniUltra()
{
	$('.pop-in-content-principal-background').removeClass('display-none');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-ultra').removeClass('display-none');
}

function closePopInContentPrincipalMiniUltra()
{
	$('.pop-in-content-principal-background').addClass('display-none');
	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-ultra').addClass('display-none');

	closePopInContent();
}

var popInContentPrincipal_callBackFunction = null;

function setClosePopInContentPrincipal_callBackFunction(func)
{
	popInContentPrincipal_callBackFunction = func;
}

function closePopInContent()
{

	var arglength = arguments.length;
	var arg = (arglength > 0 ? arguments[0] : null);

	$('.pop-in-content-principal .content .content-right').removeAttr('style');
	$('.pop-in-content-principal .content .content-right').empty();

	$('.pop-in-content-principal, .pop-in-content-principal-background').addClass('display-none');
	$('.pop-in-content-principal.pop-in-normal').addClass('display-none');

	$('.pop-in-content-principal .description span').html('');
	$('.pop-in-content-principal.pop-in-normal .content .content-html').html('');
	$('.pop-in-content-principal.pop-in-mini-ultra .content .content-html').html('');

	$('.pop-in-content-principal.pop-in-big .content').html('');
	$('.pop-in-content-principal.pop-in-xbig .content .content-xhtml').empty();

	$('.pop-in-content-principal.pop-in-mini.pop-in-mini-normal .content-right').empty();
	$('.pop-in-content-principal.pop-in-xbig .content .content-right').empty();

	showPopInContentPrincipalMiniInit();
	showPopInContentPrincipalBigInit();		// ajouté
	showPopInContentPrincipalXBigInit();
	setColRightPopInContentPrincipalMiniInit();

	if (arglength > 0 && true === arg || true === desactivePopInContentPrincipalBackground)
	{
		$(document).unbind('keyup');
		$(document).keyup(function(e) {
			if (e.keyCode == 27)
			{
				if (true === arg) {
			  		Core.hideContent();
			  	} else {
			  		closePopInContent();
			  	}
			}
		});
	}
	if(typeof popInContentPrincipal_callBackFunction === "function")
	{
		popInContentPrincipal_callBackFunction();
		popInContentPrincipal_callBackFunction = null;
	}
}


function closePopIn()
{
	$('.pop-in-content-principal .pop-in-close, .pop-in-content-principal-background').unbind('click');

	var arglength = arguments.length;
	var arg = (arglength > 0 ? arguments[0] : null);

	if (true === arg || true === desactivePopInContentPrincipalBackground)
	{
		$('.pop-in-content-principal .pop-in-close, .pop-in-content-principal-background').click(function () {
			closePopInContent();
		});
	}
	else if (false === arg || false === desactivePopInContentPrincipalBackground)
	{
		$('.pop-in-content-principal .pop-in-close').click(function () {
			closePopInContent(false);
		});
	}
	else {
		$('.pop-in-content-principal .pop-in-close, .pop-in-content-principal-background').unbind('click');
	}
}


$(function () {
	closePopIn();
});
</script>
</body>
</html>
