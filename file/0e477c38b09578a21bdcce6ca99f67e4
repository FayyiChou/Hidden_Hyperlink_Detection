var dashboardTrouverFamille = null;
var dashboardTrouverVille = null;
var dashboardTrouverGenre = null;	
var dashboardTrouverMoows = null;	
var dashboardTrouverMoowys = null;


$(function ()
{

	initFormDemoow();

	$(document).on('change', "#dashboardTrouverFamille, #formTrouverFamille", function( event )
	{
		var idAttr = $(this).attr('id');
		var idFamille = $(this).val();
		$('#dashboardTrouverFamille, #formTrouverFamille').val(idFamille);	

	});


	$("#dashboardTrouverVille").click(function () 	// semble bon
	{ 
		$(this).val(''); 

		var p = $("#dashboardTrouverVille").position();
		$('.ui-autocomplete').css('display','none').css('left',p.left).css('top', (p.top+=2) );
		
		$('#dashboardTrouverVille_hidden').val('');

	});
	
	$("#dashboardTrouverVille").autocomplete(		// semble bon	
	{
        source: function (request, response)
        {
        	var term = request['term'];
        	
        	if (true == Functions.empty(term)) {
        		return;
        	}
        	
        	var data = setTermVille(term);
        	
        	response($.map( data, function(item)
        	{
                return {
                    	id: item.ville_ID,
                 		ville: item.ville_nom,
                 		cp: item.ville_code_postal
                 };
            }));        	
        },
        open: function()
        { 
            $('.ui-menu').width(210);
        } ,
        minLength: 2,
        select: function(event, ui)
        {
        	var ville = ui.item.ville;
        	var idVille = ui.item.id;
        	var cp = ui.item.cp;
        	
            $('#dashboardTrouverVille_hidden').val(idVille);
            
            event.preventDefault();
            
            this.value = 
            (ville.length > 15 ? ville.substr(0,15)+'...' : ville) + ' - ' + cp;
        }
        
    }).data("autocomplete")._renderItem = function (ul, item)
    {
        return $("<li></li>")
        .data("item.autocomplete", item)
        .append("<a>" + (item.ville.length > 17 ? item.ville.substr(0,17)+'...' : item.ville) + "<span style='float:right'>" + item.cp + "</span></a>")
        .appendTo(ul);
	};

});


function getTrouverUnUser(type)
{

	$('#dashboardTrouverFamille').removeAttr('style');
	$('#dashboardTrouverVille, #formTrouverVille').removeAttr('style');
//	$('#dashboardTrouverFamille, #formTrouverFamille').removeAttr('style');


	if(type == 'dashboard')
	{
		var var_famille = $('#dashboardTrouverFamille').val();
		var var_ville = $('#dashboardTrouverVille_hidden').val();
		var var_genre = $('#dashboardTrouverGenre').val();
		var var_moows = $('#dashboardTrouverMoows').val();		
		var var_moowys = $('#dashboardTrouverMoowys').val();		

	}
	else if (type == 'page')
	{	
		var var_famille = $('#formTrouverFamille').val();
		var var_ville = $('#formTrouverVille_hidden').val();
		var var_genre = $('#dashboardTrouverGenre').val();
		var var_moows = $('#dashboardTrouverMoows').val();
		var var_moowys = $('#dashboardTrouverMoowys').val();
	}


	if (true == Functions.empty(var_ville))
	{
		$('#dashboardTrouverVille, #formTrouverVille').css('border', '1px solid red');
		return;
	}	

	if (var_famille == 'xxx')
	{
		$('#dashboardTrouverFamille, #formTrouverFamille').css('border', '1px solid red');
		return;
	}	

		$("#loadedFooterBarContent").load('moower-result.html',
	{
		famille 	: var_famille,
		ville		: var_ville,
		genre 		: var_genre,
		moowys 		: var_moowys,
		moows		: var_moows
	}
	,function (responseText, textStatus, XMLHttpRequest)
	{
		if (textStatus == "error") 
		{
			alert("Erreur au chargement du ( "+XMLHttpRequest+" ) !!!");
		}
		else
		{
			Core.iniBtFooter();
					
			if (!Modernizr.touch) {
				// Core.tooltip();
				ieInputFix();
			}			
		}
	});
}

function initFormMoower()
{
	$('#dashboardTrouverFamille, #formTrouverFamille, #dashboardTrouverVille, #dashboardTrouverVille_hidden, #formTrouverVille_hidden, #formTrouverVille, #dashboardTrouverGenre, #dashboardTrouverMoows, #dashboardTrouverMoowys').val('');
//	$('#formTrouverVille, #formTrouverFamille').css('border-color', '#91AB31');
	$('#dashboardTrouverFamille, #dashboardTrouverVille, #dashboardTrouverGenre, #dashboardTrouverMoows, #dashboardTrouverMoowys').removeAttr('style');	
	$('#formChercherFamille, #formChercherVille, #formoowChercherDate, #formChercherPratique, #formChercherGenre').css('border-color', '#EAB89F');
}


// garde les donnees en memoire
var dashboardBonplanRegion = null;
var dashboardBonplanVille = null;
var dashboardBonplanActivite = null;
var dashboardBonplanDate = null;

var modBonplanActivite = false;

$(function ()
{
	$(document).on('change', "#dashboardBonplanRegion, #formBonplanRegion", function( event )
	{ 
		var idAttr = $(this).attr('id');
		var idRegion = $(this).val();
		$('#dashboardBonplanRegion, #formBonplanRegion').val(idRegion);	
		
		// Region
		/*if (idAttr == 'dashboardBonplanRegion') {
		} else if (idAttr == 'formBonplanRegion') {
		}*/

		if (idRegion == 'xxx') 
		{
			Functions.setOptionInit('dashboardBonplanVille');
			Functions.setOptionInit('formBonplanVille');
			
			$('#dashboardBonplanVille, #formBonplanVille').attr('disabled','disabled');	
			$('#dashboardBonplanActivite, #formBonplanActivite').attr('disabled','disabled');
			
			getFamillesBonPlan();
		} 
		else
		{
			$('#dashboardBonplanRegion, #formBonplanRegion').removeAttr('style');
			$('#dashboardBonplanActivite, #formBonplanActivite').removeAttr('disabled');
			
			$.ajax({
			   type: "POST",
			   async: true,
			   url: url_common,
			   data: "param=get-villes-by-region-bonplan&id="+idRegion,
			   dataType: "json",
			   success: function(json)
			   {
				   if (json.etat == 'ok')
				   {
					   var value = json.data;
					   Functions.setOptionInit('dashboardBonplanVille');
					   Functions.setOptionInit('formBonplanVille');
						
					   $('#dashboardBonplanVille, #formBonplanVille').removeAttr('disabled');

					   // ligne par default
					   Functions.setOption('dashboardBonplanVille', 'xxx', 'Choisir une ville...');
					   Functions.setOption('formBonplanVille', 'xxx', 'Choisir une ville...');

					   for (var key in value)
					   {
						   Functions.setOption('dashboardBonplanVille', value[key]['ville_ID'], value[key]['ville_nom']);
						   Functions.setOption('formBonplanVille', value[key]['ville_ID'], value[key]['ville_nom']);
					   }

				   } else {
				   }
			   }
			 });
		}
	});
	
	
	$(document).on('change', "#dashboardBonplanVille, #formBonplanVille", function( event )
	{ 
		var idAttr = $(this).attr('id');
		var idVille = $(this).val();
		$('#dashboardBonplanVille, #formBonplanVille').val(idVille);
		
		if (idAttr == 'dashboardBonplanVille')
		{
			var idActivite = $('#dashboardBonplanActivite').val();
		} 
		else if (idAttr == 'formBonplanVille') 
		{
			var idActivite = $('#formBonplanActivite').val();
		}	
		
		if (idVille == 'xxx') 
		{
			getFamillesBonPlan();
		}
		else
		{
			if (idActivite == 'xxx' || false === modBonplanActivite)
			{
				$.ajax({
				   type: "POST",
				   async: true,
				   url: url_common,
				   data: "param=get-familles-by-ville-bonplan&id="+idVille,
				   dataType: "json",
				   success: function(json)
				   {
					   if (json.etat == 'ok')
					   {
						   var value = json.data;
						   Functions.setOptionInit('dashboardBonplanActivite');
						   Functions.setOptionInit('formBonplanActivite');
						   
						   $('#dashboardBonplanActivite, #formBonplanActivite').removeAttr('disabled');
	
						   Functions.setOption('dashboardBonplanActivite', 'xxx', 'Choisir une activité...');
						   Functions.setOption('formBonplanActivite', 'xxx', 'Choisir une activité...');
	
						   for (var key in value)
						   {
							   Functions.setOption('dashboardBonplanActivite', value[key]['famille_ID'], value[key]['famille_nom']);
							   Functions.setOption('formBonplanActivite', value[key]['famille_ID'], value[key]['famille_nom']);
							   
						   }
	
					   } else {
					   }
				   }
				});
			}
		}		
	});
	
	
	$(document).on('change', "#dashboardBonplanActivite, #formBonplanActivite", function( event )
	{ 
		var idAttr = $(this).attr('id');
		var idActivite = $(this).val();
		$('#dashboardBonplanActivite, #formBonplanActivite').val(idActivite);
		
		
		if (idAttr == 'dashboardBonplanActivite') 
		{
			var idRegion = $('#dashboardBonplanRegion').val();
			var idVille = $('#dashboardBonplanVille').val();
		} 
		else if (idAttr == 'formBonplanActivite') 
		{
			var idRegion = $('#formBonplanRegion').val();
			var idVille = $('#formBonplanVille').val();
		}
					
		if (idRegion != 'xxx' && (idVille == 'xxx' || idVille == null) && idActivite != 'xxx') {
			modBonplanActivite = true;
		}
		
		if (idActivite == 'xxx') 
		{
			Functions.setOptionInit('dashboardBonplanVille');
			Functions.setOptionInit('formBonplanVille');
			
			$('#dashboardBonplanVille, #formBonplanVille').attr('disabled','disabled');	
		}
		else if (idRegion != 'xxx' && (idVille == 'xxx' || true === modBonplanActivite))
		{
			$.ajax({
			   type: "POST",
			   async: true,
			   url: url_common,
			   data: "param=get-villes-by-famille-bonplan&id="+idActivite+"&id2="+idRegion,
			   dataType: "json",
			   success: function(json)
			   {
				   if (json.etat == 'ok')
				   {
					   var value = json.data;
					   Functions.setOptionInit('dashboardBonplanVille');
					   Functions.setOptionInit('formBonplanVille');
						
					   $('#dashboardBonplanVille, #formBonplanVille').removeAttr('disabled');

					   Functions.setOption('dashboardBonplanVille', 'xxx', 'Choisir une ville...');
					   Functions.setOption('formBonplanVille', 'xxx', 'Choisir une ville...');

					   for (var key in value)
					   {
						   Functions.setOption('dashboardBonplanVille', value[key]['ville_ID'], value[key]['ville_nom']);
						   Functions.setOption('formBonplanVille', value[key]['ville_ID'], value[key]['ville_nom']);
					   }

				   } else {
				   }
			   }
			});
		}		
	});	
});


function getFamillesBonPlan()
{
	$.ajax({
	   type: "POST",
	   async: true,
	   url: url_common,
	   data: "param=get-familles",
	   dataType: "json",
	   success: function(json)
	   {
		   if (json.etat == 'ok')
		   {
			   var value = json.data;
			   Functions.setOptionInit('dashboardBonplanActivite');
			   Functions.setOptionInit('formBonplanActivite');
			   
			   Functions.setOption('dashboardBonplanActivite', 'xxx', 'Choisir une activité...');
			   Functions.setOption('formBonplanActivite', 'xxx', 'Choisir une activité...');

			   for (var key in value)
			   {
				   Functions.setOption('dashboardBonplanActivite', value[key]['famille_ID'], value[key]['famille_nom']);
				   Functions.setOption('formBonplanActivite', value[key]['famille_ID'], value[key]['famille_nom']);				   
			   }

		   } else {
		   }
	   }
	});
}


function getChercherUnBonPlans(type)
{
	$('#dashboardBonplanRegion, #formBonplanRegion').removeAttr('style');
	
	if (type == 'dashboard')
	{
		var var_region = $('#dashboardBonplanRegion').val();
		var var_ville = $('#dashboardBonplanVille').val();
		var var_activite = $('#dashboardBonplanActivite').val();
		var var_date = $('#dashboardBonplanDate').val();
	}
	else if (type == 'page')
	{
		var var_region = $('#formBonplanRegion').val();
		var var_ville = $('#formBonplanVille').val();
		var var_activite = $('#formBonplanActivite').val();
		var var_date = $('#formBonplanDate').val();
	}
	
	
	if (var_region == 'xxx') {
		$('#dashboardBonplanRegion, #formBonplanRegion').css('border', '2px solid red');
		return;
	}	
	
	dashboardBonplanRegion = var_region;
	dashboardBonplanVille = var_ville;
	dashboardBonplanActivite = var_activite;
	dashboardBonplanDate = var_date;	
	
	$("#loadedFooterBarContent").load("bonplans-result.html",
	{
		region 		: var_region,
		ville		: var_ville,
		activite 	: var_activite,
		date		: var_date
	}
	,function (responseText, textStatus, XMLHttpRequest)
	{
		if (textStatus == "error") 
		{
			alert("Erreur au chargement du ( "+responseText+" ) !!!");
		}
		else
		{
			Core.iniBtFooter();
			
			//$("#contentBonsPlan").mCustomScrollbar({ scrollButtons:{ enable:true } });
			Core.showContent('#5477B7', 'breadcrumbPlan', 'closeColor7');
			
			if (!Modernizr.touch) {
				//Core.tooltip();
				ieInputFix();
			}			
		}
	});
}


function initFormsBonPlan()
{
	$('#dashboardBonplanRegion, #dashboardBonplanDate, #formBonplanRegion, #formBonplanDate').val('xxx');
	$('#dashboardBonplanRegion, #formBonplanRegion').removeAttr('style');
	
	Functions.setOptionInit('dashboardBonplanVille');
	Functions.setOptionInit('formBonplanVille');	
	
	$('#dashboardBonplanVille, #formBonplanVille').attr('disabled','disabled');	
	
	getFamillesBonPlan();
}

$(function ()
{
	$('#dashboardFaireConnaitreEmail').click(function() {
		$(this).val( Functions.lowercase($(this).val()) );
	}).change(function() {
		$(this).val( Functions.lowercase($(this).val()) );
	});
	
	var btSave_confirm = false;

	$('#dashboardFaireConnaitreButton').click(function ()
	{
		if (true === btSave_confirm) {
			return;
		}
		
		initBorderColorRedFaireConnaitre();
		
		var form_Message = $('#dashboardFaireConnaitreMessage').val();
		var form_Choix = $('#dashboardFaireConnaitreMessage option:selected').text();
		var form_Email = Functions.lowercase( $('#dashboardFaireConnaitreEmail').val() );

		var visiteur = "" || false;

		if (false === visiteur)
		{
			var nom_visiteur = "Visiteur";
		}
		else if (false !== visiteur)
		{
			var nom_visiteur = "";
		}

		var error = false;
		
		if (form_Message === 'xxx')
		{
			$('#dashboardFaireConnaitreMessage').css({'border-color':'red','border-width':'1px','border-style':'solid'});
			error = true;
		}	
	
		if (false === Functions.verifMailStr(form_Email))
		{
			$('#dashboardFaireConnaitreEmail').css({'border-color':'red','border-width':'1px','border-style':'solid'});
			error = true;
		}	
		
		if (true === error)
		{
			return;
		}		
	
		if (false === error)
		{
			btSave_confirm = true;
			
			var form_data = "param=save-connaitre" + "&form_Choix=" + form_Choix + "&form_Email=" + form_Email + "&form_Message=" + form_Message;

	        $.ajax({
	            type: "POST",
	            async: true,
	            url: url_common,
	            data: form_data,
	            dataType: "json",
	            success: function (d)
	            {
		        	var data = d.data;
//	            	if (d.etat == 'ok')
					if (d.data == 'ok')

	            	{

	            		btSave_confirm = false; 

	            		initFormFaireConnaitre();
	  	          		initBorderColorRedFaireConnaitre();

		    			var html = "<p><p><strong>{visiteur}</strong>,<br><br><br>Votre invitation à découvrir <strong>moowup</strong>, au destinataire de l'adresse <strong>{mail}</strong> est envoyée...<br><br>Et nous tenons à vous en remercier.<br><br><br>L'équipe <strong>moowup</strong>.</p><p>";
		    			html = html.replace('{visiteur}',nom_visiteur);
		    			html = html.replace('{mail}',form_Email);
	            		
	            		showPopInContentPrincipalMiniInit();
	            		setTextPopInContentPrincipalMini("Invitation envoyée", html);
	            		
	            		showPopInContentPrincipalMini(this);
	            		buildKeyupPopInContentPrincipal();

	            	}

	            },
	            error: function (result) {}
		       });
			}
		});
 
 
 
 	});


function initFormFaireConnaitre()
{
	$('#dashboardFaireConnaitreMessage').val('xxx');
	$('#dashboardFaireConnaitreEmail').val('');
}


function initBorderColorRedFaireConnaitre()
{
	$('#dashboardFaireConnaitreMessage, #dashboardFaireConnaitreEmail').css('border', '0');
}


</script>
	<script>
function buildKeyupPopInContentPrincipal()
{
	var arglength = arguments.length;
	var arg = (arglength > 0 ? arguments[0] : null);

	if (arglength > 0 && true === arg || typeof desactivePopInContentPrincipalBackground !== 'undefined' && true === desactivePopInContentPrincipalBackground)
	{

		$(document).unbind('keyup');
		$(document).keyup(function(e) {
			if (e.keyCode == 27) {
				closePopInContent();
			}
		});
	}

	if (true === arg) {
		closePopIn();
	}
}

function showPopInContentPrincipal()
{
	$('.pop-in-content-principal-background').removeClass('display-none');
	$('.pop-in-content-principal.pop-in-normal').removeClass('display-none');
}

function closePopInContentPrincipal()
{
	$('.pop-in-content-principal-background').addClass('display-none');
	$('.pop-in-content-principal.pop-in-normal').addClass('display-none');

	closePopInContent();
}

function showPopInContentPrincipalXBigInit()
{
	$('.pop-in-content-principal.pop-in-xbig .description h4').prepend('');
	$('.pop-in-content-principal.pop-in-xbig .pastille.button.pop-in-xbig').attr('data-id', '');
	$('.pop-in-content-principal.pop-in-xbig .pastille.button.pop-in-xbig').attr('data-id2', '');
	$('.pop-in-content-principal.pop-in-xbig .pastille.button.pop-in-xbig').removeAttr('data-total');


	$('.pop-in-content-principal.pop-in-xbig span.pop-in-close').removeAttr('style');
	$('.pop-in-content-principal.pop-in-xbig .content').removeAttr('style');

	$('.pop-in-content-principal.pop-in-xbig .button.pop-in-xbig h4').html( '' );
	$('.pop-in-content-principal.pop-in-xbig .button.pop-in-xbig').addClass('cache');
	$('.pop-in-content-principal.pop-in-xbig .description span').html( '' ).removeAttr('style');

	$('.pop-in-content-principal.pop-in-xbig .content .content-xhtml').html('').removeAttr('style');
	$('.pop-in-content-principal.pop-in-xbig .content .content-right').html('').removeAttr('style');

	var _this = $('.pop-in-content-principal.pop-in-xbig .content');
	$(_this).removeAttr('class').addClass('content').removeAttr('style');
}

function setColRightPopInContentPrincipalXBigMarginTop(margin)
{
	$('.pop-in-content-principal.pop-in-xbig .content .content-right').css('margin-top', margin);
}

function setColRightPopInContentPrincipalXBig(icon_name, id)
{
	var html = '<span class="pop-in-bt-right" id="' + id + '"><i class="fa ' + icon_name + '"></i></span>';
	$('.pop-in-content-principal.pop-in-xbig .content-right').append( html );
}

function showPopInContentPrincipalXBig()
{
	$('.pop-in-content-principal-background').removeClass('display-none');
	$('.pop-in-content-principal.pop-in-xbig').removeClass('display-none');
}

function setTextPopInContentPrincipalXBig(titre, description, r, r_html)
{
	$('.pop-in-content-principal .description span').html( titre );

	if (true === r) {
		var right = '<div class="content-right">'+r_html+'</div>';
	} else {
		var right = '';
	}


	var html = '<div class="content-html xbig">'+description+'</div>'+right;

	$('.pop-in-content-principal.pop-in-xbig .content').html( html );
}

function closePopInContentPrincipalXBig()
{
	$('.pop-in-content-principal-background').addClass('display-none');
	$('.pop-in-content-principal.pop-in-big').addClass('display-none');

	$('.pop-in-content-principal.pop-in-xbig .content').removeAttr('style');

	closePopInContent();
}

function setColRightPopInContentPrincipalBigMarginTop(margin)
{
//	$('.pop-in-content-principal.pop-in-big .content .content-right').css('margin-top', margin);
	$('.pop-in-content-principal.pop-in-big .content').css('margin-top', margin);
}

function showPopInContentPrincipalBigInit()
{
	$('.pop-in-content-principal.pop-in-big div.pastille').addClass('cache');
	$('.pop-in-content-principal.pop-in-big div.pastille h4').html('');

	$('.pop-in-content-principal.pop-in-big').removeAttr('data-name');
	$('.pop-in-content-principal.pop-in-big').removeAttr('style');

	$('.pop-in-content-principal.pop-in-big span.pop-in-close').removeAttr('style');
	$('.pop-in-content-principal.pop-in-big .description.pop-in-mini').removeAttr('style');

	$('.pop-in-content-principal.pop-in-big .pastille span').html( '' ).addClass('cache');
	$('.pop-in-content-principal.pop-in-big .description span').html( '' );

	$('.pop-in-content-principal.pop-in-big .content .content-right').html('');
	$('.pop-in-content-principal.pop-in-big .content .content-right').css('margin-top', '0');	// ok

	$('.pop-in-content-principal.pop-in-big .content-html').html('');

//	$('.pop-in-content-principal.pop-in-big .content-html.mini-normal').html('');
//	$('.pop-in-content-principal.pop-in-big .content-html.mini-optional').html('');

//	var _this = $('.pop-in-content-principal.pop-in-big .content-html.mini-normal');
//	$(_this).removeAttr('class').addClass('content-html').addClass('mini-normal');

//	var _this = $('.pop-in-content-principal.pop-in-big .content-html.mini-optional');
//	$(_this).removeAttr('class').addClass('content-html').addClass('mini-optional').addClass('cache');
}

function showPopInContentPrincipalBig()
{
	$('.pop-in-content-principal-background').removeClass('display-none');
	$('.pop-in-content-principal.pop-in-big').removeClass('display-none');
}

function setTextPopInContentPrincipalBig(titre, texte, r, r_html)
{
	$('.pop-in-content-principal .description span').html( titre );

	if (true === r) {
		var right = '<div class="content-right">'+r_html+'</div>';
	} else {
		var right = '';
	}

	var html = '<div class="content-html big">'+texte+'</div>'+right;
	$('.pop-in-content-principal.pop-in-big .content').html( html );
}

function setAnswerPopInContentPrincipalBig(titre, texte, answer, bouton)
{
	$('.pop-in-content-principal .description span').html( titre );
	var html = '<div class="content-html big">'+texte+answer+'</div><div class="content-right">'+bouton+'</div>';

	$('.pop-in-content-principal.pop-in-big .content').html(html);
}

function setModifyPopInContentPrincipalBig(titre, texte, bouton)
{
	$('.pop-in-content-principal .description span').html( titre );
	var html = '<div class="content-html big">'+texte+'</div><div class="content-right">'+bouton+'</div>';

	$('.pop-in-content-principal.pop-in-big .content').html(html);
}

