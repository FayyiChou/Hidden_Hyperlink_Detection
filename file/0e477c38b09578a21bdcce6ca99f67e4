'quisommesnous',
'contact',
'cgu',
'annonceurs',
'inscription', 
'connexion', 
'quisommesnous', 
'buzz', 
'demoow', 
'aide', 
'bonplans', 
'annonceurs', 
'boutique',
'moower',
'unsubscribe'
];

var btArray = [
$('#btFooterQuiSommesNous'),
$('#btFooterContact'),
$('#btFooterCgu'),
$('#btPubFooter'),
//$('#btFooterLangue'),
//$('#btFooterGoogle'),
//$('#btFooterFacebook'),
//$('#btFooterTwitter'),
//$('#btFooterLinkedin'),

$('.menu .menuLi:nth-child(1)'),
