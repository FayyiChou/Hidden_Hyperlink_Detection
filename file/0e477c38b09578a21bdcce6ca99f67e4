$(function ()
{
	getPubForm();

	if (!Modernizr.touch) {
		Core.tooltip();
	}
	
	//  =============================== footer ==============================
	
	$(document).on('click', ".btPageContact", function( event ) {
		loadPage('contact');
	});
	
	$(document).on('click', ".btPageAnnonceur", function( event ) {
		loadPage('annonceurs');
	});

	//  =============================== Buttons ==============================
	
	$(document).on('click', ".rightButtons.btMoowup", function( event ) {
		loadPage('inscription');
	});	
	
	//  =============================== Bienvenue ==============================
	
	if ($.cookie('moowup') != 'show-bienvenue')
	{
		$("#modPlandetravailleBienvenue").show();
		
		$(document).click(function()
		{
			$("#modPlandetravailleBienvenue").hide();
			$.cookie('moowup', 'show-bienvenue', { expires: 365, path: "/" });
		});
	} 
/*	else if ($.cookie('messageDashboardBienvenue') != 'show-dashboard')
	{
		$("#modDashboardBienvenue").show();
		
		$(document).click(function()
		{
			$("#modDashboardBienvenue").hide();
			$.cookie('messageDashboardBienvenue', 'show-dashboard', { expires: 1, path: "/" });
		});
	} */

	//  =============================== dashboard ==============================

	$(".dashboardChercherBt.btDemoow").click(function() {
		getChercherUnMoow('dashboard');
	});
	
	$("#dashboardAccordion_chercher-un-moow").click(function() {
		loadPage('demoow');
	});
	
	$(".dashboardChercherBt.btMoower").click(function() {
		getTrouverUnUser('dashboard');
	});

	$("#dashboardAccordion_chercher-un-moower").click(function() {
		loadPage('moower');
	});
	
	$(".dashboardChercherBt.btBonPlans").click(function() {
		getChercherUnBonPlans('dashboard');
	});
	
	$("#dashboardAccordion_chercher-un-bon-plan").click(function() {
	//$("#dashboardAccordion").accordion("activate", 6);
	initFormsBonPlan();
	loadPage('bonplans');
	});	
	
	//  =============================== Ouverture de page ==============================
	
	$("#dashboardAccordion_mon-activite, #dashboardAccordion_mes-statistiques, #dashboardAccordion_mon-moowup").click(function() {
		Core.hideContent();
	});	
	
	$("#dashboardMonupFavoris, #dashboardMonupGroops, #dashboardMonupProfil, #nombreMoowSMS, #nombreMoowAccepte, #nombreMoowPropose, #nombreMoowRecu, #nombreMoowAgenda, #nombreMoowCreer, #nombreMoowBonPlan, #nombreStatsProposes, #nombreStatsVoter, #nombreStatsRecu").click(function() {
		loadPage('connexion');
	});	
	
	$("#dashboardAccordion_faire-connaitre").click(function() {
		loadPage('aide');
	});	

	$("#PubForm").click(function ()
	{
		var PubID = $(this).attr('data-id');
		var PubClick = $(this).attr('data-count');
		
		if (!Functions.empty(PubID))
		{
			$.post( url_common, {
				param: "pub-clicked", 
				IdPub: PubID,
				ClickPub: PubClick
			} );
		}
	});
	
});

