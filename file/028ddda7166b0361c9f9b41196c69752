	<script type="text/javascript">
	jQuery(".hotelAd").slide( 
	{ mainCell:".bd ul",autoPlay:true} 
	);
	jQuery(function(){
		hotel_banner();
	});
	function hotel_banner(){
		var $obj=jQuery('#hotelAd').parent().parent();
		var obj=$obj.get(0);
		var oheight=obj.style.height;
		var owidth=obj.style.width;
		var imgs=jQuery("#hotelAd img").each(
			function(){
				jQuery(this).attr('height',oheight);
				jQuery(this).attr('width',owidth);
			}
		);
	};	
	</script>
