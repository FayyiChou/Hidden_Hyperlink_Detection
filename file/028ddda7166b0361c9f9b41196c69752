$(function(){
	//图片路径为空换成暂无图片,图片onerror也换成默认图片
    $("img").each(function(){
		if($(this).attr("src").indexOf("blank") > -1 || $(this).attr("src") == ""){
		  $(this).attr('src', '/images/nopicture.png');
		  return;
		}
		$(this).error(function(){
			$(this).attr('src', '/images/nopicture.png');
		});
	});
});
