<script type="text/javascript"> 
function tabs(name,m,n){ 
	for( var i=1;i<=n;i++){ 
		var menu = document.getElementById(name+i); 
		var showDiv = document.getElementById(name+"_"+i); 
		menu.className = i==m ?"y":""; 

		showDiv.style.display = i==m?"block":"none"; 
	} 
} 
</script> 

