          <script type="text/javascript">
		  		function resetkccx(){
					$('.kccx select').val(0)
					$('.kccx .nj .cur').removeClass('cur');
					$(".kccx .nj_0").addClass('cur');
				}
				function gokccx(){
					var catid=0;
						if($('.kccx .nj .cur').val()!=0)
						{
							catid = $('.kccx .nj .cur').val();
						}
						else
						{
							catid = $('.kccx #classtype').val();
						}
					
					if(catid != 0)
					{
						window.open('list-'+catid+'-1.html','_blank');

					}
					else
					{
						alert("请选择查询条件");
					}
				}
				function gozycx(){
					var catid=0;
					if($('.zycx .school .cur').val()!=0)
					{
						catid = $('.zycx .school .cur').val();
					}
					else
					{
						if($('.zycx .nj .cur').val()!=0)
						{
							catid = $('.zycx .nj .cur').val();
						}
						else
						{
							catid = $('.zycx #classtype').val();
						}
					}
					if(catid != 0)
					{
						window.open('list-'+catid+'-1.html','_blank');
					}
					else
					{
						alert("请选择查询条件");
					}

				}
				function resetzycx(){
					$('.zycx select').val(0)
					$('.zycx .nj .cur').removeClass('cur');
					$(".zycx .nj_0").addClass('cur');
					$('.zycx .school .cur').removeClass('cur');
					$(".zycx .school_0").addClass('cur');
				}
				$(document).ready(function(e) {
                    $(".kccx #classtype").change(function(e) {
						$('.kccx .nj .cur').removeClass('cur');
						$(".kccx .nj_"+$(this).val()).addClass('cur');
					});
					$(".kccx #nj").change(function(e) {
                        $('.kccx .school .cur').removeClass('cur');
						$('.kccx .school_'+$(this).val()).addClass('cur');
                    });
					$(".zycx #classtype").change(function(e) {
						$('.zycx .nj .cur').removeClass('cur');
						$(".zycx .nj_"+$(this).val()).addClass('cur');
					});
					$(".zycx #nj").change(function(e) {
                        $('.zycx .school .cur').removeClass('cur');
						$('.zycx .school_'+$(this).val()).addClass('cur');
                    });
                });
          </script>
