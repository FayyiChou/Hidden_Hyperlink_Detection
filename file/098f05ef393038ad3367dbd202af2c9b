            function getSelectValue(name) {
                var obj = document.getElementById(name);
                return obj.value;
            }
            function GetFocus(obj) {
                if (!isVarDefined(obj)) {
                    return false;
                }
                else {
                    obj.focus();
                }
            }
            function isVarDefined(obj) {
                if (typeof (obj) == "undefined") {
                    return false;
                } else {
                    return true;
                }
            }
            function loginAccessLoginFun() {
                var checkbool = false;
                if ($("#checkRem").attr("checked") == true) {
                    checkbool = true;
                }
                else {
                    checkbool = false;
                }
                var loginTypeValue = getSelectValue("selectPopLoginType");
                var loginName = $("#loginAccessName").val();
                var loginPwd = $("#loginAccessPwd").val();
                var loginAccessPwd1 = $("#loginAccessPwd1").val();
                if (loginName == "" || loginPwd == "") {
                    //$("#showMssage").empty();
                    //$("#showMssage").html("用户名和密码不能为空");
                    alert("用户名和密码不能为空");
                    GetFocus($("#loginAccessName"));
                    return false;
                }
                if (loginAccessPwd1 == "") {
                    alert("验证码不能为空");
                    GetFocus($("#loginAccessPwd1"));
                    return false;
                }
                var backuser = "";
                var backSeesion = "";
                $.ajax({
                    url: "../ashx/loginJson.ashx",
                    type: "POST",
                    data: { UserName: loginName, Password: loginPwd, loginType: loginTypeValue, ScreenX: screen.width, ScreenY: screen.height, code: loginAccessPwd1 },
