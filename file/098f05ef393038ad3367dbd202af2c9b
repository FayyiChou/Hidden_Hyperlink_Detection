            function getSubText(str, length) {
                return str.length > length ? str.substr(0, length) + '...' : str;
            }
            function getuserfind(sid, startnumber, type) {
                var reg = /^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$/;
                var reg1 = /^[0-9]*[1-9][0-9]*$/;
                if (!reg.test(sid)) {
                    alert("请输入有效的参数");
                    return false;
                }
                if (!reg1.test(startnumber)) {
                    alert("请输入有效的参数");
                    return false;
                }
                $.ajax({
                    type: "GET",
                    url: "https://jifen.91huayi.com/home/Getuserintegrallevel",
                    data: { sid: sid, startnumber: startnumber, zip: Math.random() },
                    cache: false,
                    error: function () {
                        alert("程序出错,请联系管理员.");
                        return false;
                    },
                    dataType: "jsonp",
                    jsonp: 'callback',
                    beforeSend: function () {
                        $("#page_net img").attr("src", "https://www.91huayi.com/huayi_yidou/images/page_loading.gif");
                    },
                    success: function (msg) {
                        if (msg != null) {
                            if (msg.success == true) {
                                //头像
                                if (msg.img != null && msg.img != "") {
                                    $(".g-sd1-1 img").attr("src", msg.img);
                                }

                                //加载用户信息
                                $("#comUserName_jf").html('我的医豆：' + msg.integral);
                                $("#comUserName_dj").html('我的等级：' + msg.level);
                                //$("#my_yd").attr("href", "http://10.0.6.204:8333/testindex.html?id="+sid);
                            }
                            else {
                                alert(msg.message);
                                return false;
                            }
                        }
                    }

                });
            }
            function userInfo() {
                $.ajax({
                    type: "POST",
                    url: "../ashx/getUserInfo.ashx",
                    success: function (data) {
                        if (data.indexOf("<\/script>") > -1) {
                            eval(data.replace("<script>", "").replace("<\/script>", ""));
                        }
                        else {
                            var json = eval(data);
                            $.each(json, function (id, item) {

                                var userArea = item.CityName + ";" + item.CountyName;


                                userinfohtml = "<div class='lg_tit'>个人信息</div><div class='l' style=' height:105px;background:none; width:98px;'><img style='height:100px; width:78px;;' src='" + item.ImgUrl + "' /></div><div class='r' style=' width:280px; margin-left:0px;'><ul><li style='margin-bottom:0px;'><span style='width:50px;'>姓&#12288;名</span>" + item.UserName + "</li><li style='margin-bottom:0px;'><span style='width:50px;'>医通卡</span>" + item.UserYtNo + "</li><li style='margin-bottom:0px;'><span style='width:50px;'>专&#12288;业</span><lable title='" + item.UserDept + "'>" + getSubText(item.UserDept, 6) + "</lable></li><li style='margin-bottom:0px;'><span style='width:50px;'>职&#12288;称</span><lable title='" + item.UserTitle + "'>" + getSubText(item.UserTitle, 6) + "</lable></li> </ul></div><div class='clear'></div><div class='b'><ul><li style='margin-bottom:0px;'><span style='width:76px;'>地&#12288;&#12288;区</span><lable title='" + userArea + "'>" + getSubText(userArea, 10) + "</lable></li><li style='margin-bottom:0px;'><span style='width:76px;'>单&#12288;&#12288;位</span><lable title='" + item.UserUnit + "'>" + getSubText(item.UserUnit, 10) + "</lable></li><li><span  style=' width:250px;'><a id='comUserName_jf' href='" + "http://www.91huayi.com/huayi_yidou.html?id=" + item.UserID + "' target='_blank' style='display:block;'></a><lable id='comUserName_dj'></lable></span></li></ul></div><div class='lg_box2'><a href='http://kjpt8.91huayi.com/personel/validate.aspx' target='_blank'>学分查询</a><a href='" + "https://hyuser.91huayi.com/AccountCenter/modifyAccount.aspx?tag=cme&uid=" + item.UserID + "' >修改信息</a><a href='../pages/card_mes.aspx' target='_blank'>在线绑卡</a></div><div class='lg_box1'><a class='login_btn'href='../secure/login_out.aspx'></a><a class='change' href='" + "https://hyuser.91huayi.com/AccountCenter/setpwd.aspx?tag=cme&uid=" + item.UserID + "&user_name=" + item.AccountName + "'" + " target='_blank' >修改密码</a><div class='clear'></div></div>";
                                $("#logindiv").hide();
                                $("#spName").addClass("login2");
                                $("#spName").html(userinfohtml);

                                getuserfind(item.UserID, 1, 2);

                            });
                        }
                    }
                });
            }


            function loading() {
                $("#logindiv").html("正在登录...");
            }

            function verification() {

                $("#logindiv").html("正在验证...");
            }
            $(document).ready(function () {
             

                $.ajax({
                    type: "POST",
                    url: "../ashx/getSession.ashx",
                    beforeSend: verification,
                    success: function (msg) {
                        if (msg == "nullvalue") {
                            checkClient();
                        }
                        else {
                            userInfo();
                        }
                    }
                });
            });

            function checkClient() {
                if (getCookie("userName") != null && getCookie("userPwd") != null) {
                    var clientName = getCookie("userName");
                    var clientPwd = getCookie("userPwd");
                    var loginTypeValue = getCookie("loginType");
                    $.ajax({
                        url: "../ashx/loginJson.ashx",
                        type: "POST",
                        data: { UserName: clientName, Password: clientPwd, loginType: loginTypeValue },
                        beforeSend: verification,
                        success: function (data) {
                            var json = eval(data);
                            var backname = "";
                            var serverSession = "";
                            $.each(json, function (id, item) {
                                backname = item.userName;
                                serverSession = item.sessionType;
                                if (backname != "01" && backname != "0" && backname != "1") {
                                    userInfo();
                                }
                                else {
                                    $("#logindiv").empty();
                                    $("#logindiv").html(loginhtml);
                                    GetFocus($("#loginAccessName"));
                                }
                            })
                        }
                    });
                }
                else {
                    $("#logindiv").empty();
                    $("#logindiv").html(loginhtml);
                    GetFocus($("#loginAccessName"));
                }
            }
            function getCookie(sKey) {
                var sCookie = document.cookie;
                if (!sCookie) return null;
                var sTag = sKey + "=";
                var iBegin = sCookie.indexOf(sTag);
                if (iBegin < 0) return null;
                iBegin += sTag.length;
                var iEnd = sCookie.indexOf(";", iBegin);
                if (iEnd < 0) iEnd = sCookie.length;
                return sCookie.substring(iBegin, iEnd);
            }

            function setCookie(sKey, sValue, sDomain, sPath, sExpires, blSecure) {
                var sCookieStr = sKey + "=" + sValue + ";";
                if (sDomain) sCookieStr += " DOMAIN=" + sDomain + ";";
                if (sPath) sCookieStr += " PATH=" + sPath + ";";
                if (sExpires) sCookieStr += " EXPIRES=" + sExpires + ";";
                if (blSecure) sCookieStr += " SECURE";
                document.cookie = sCookieStr;
            }

            function delCookie(name) {
                var exp = new Date();
                exp.setTime(exp.getTime() - 1);
                var cval = getCookie(name);
                if (cval != null) document.cookie = name + "=" + cval + ";path=/;expires=" + exp.toGMTString();
            }
            
        </script>


