            function moreSlider(box) {
                var _this = $(box);
                var head = $(".tabbox li", _this);
                var cont = $(".cpanel", _this);
                head.click(function () {
                    var index = head.index(this);
                    var more = $(".tabMore");
                    for (var i = 0; i < more.length; i++) {
                        if (i == index)
                            $(more[i]).show();
                        else
                            $(more[i]).hide();
                    }
                });
            }
