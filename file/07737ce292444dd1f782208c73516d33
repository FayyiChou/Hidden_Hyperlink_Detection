                        <script>
	function viewPage_d2_1479_2a258_Marquee_(index, count, head,jsonArray,uid)
	{       
       if(uid!='')
       {           
             for(var i=0;i<jsonArray[0].length;i++)
             {              
               var temptitle= document.getElementById( uid + '_lab_'+i);
               var tempimg=document.getElementById( uid + '_img_'+i);
                
                var arrayLength=jsonArray[index].length;
                         
                if(arrayLength<=i)
               {
                  if(temptitle !=null)
                  {
                     temptitle.style.display='none';
                  }                  
                   if(tempimg !=null)
                  {  
                    tempimg.style.display='none';  
                  }              
               }
               else
               {            
                   if(tempimg !=null)
                     {                   
                       tempimg.style.display='';                
                       tempimg.src=jsonArray[index][i].bigPhotoSrc;
                       tempimg.onclick= new Function( jsonArray[index][i].onclick);
                     }
                   if(temptitle !=null)
                    {
                      temptitle.style.display='block'; 
                      temptitle.innerHTML=jsonArray[index][i].title;
                      temptitle.setAttribute('title',jsonArray[index][i].title);
                      //temptitle.onclick=jsonArray[index][i].onclick;
                    }

                }
    
              } 
        }

	}
</script>
