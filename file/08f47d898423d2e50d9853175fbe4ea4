$(function(){
	var oLi = $(".tab_menu li");
	if(oLi.length == 1){
		oLi.attr("class","tab1_on");
	}
	if(oLi.length > 1){
		$(oLi).each(function (i, n) {
			$(oLi[i]).attr("index",i);
			$(oLi[i]).bind("click",function(){
				$(this).addClass('tab_on').siblings().removeClass('tab_on');
				var index = parseInt($(this).attr("index"));
				$($(".tab_box ul").get(index)).css('display','block').siblings().css('display','none');
			});

		});
		
	}
});
