   <script type="text/javascript">
function openVideo(path){
	$.layer({
		type: 2,
		title: false,
		shade: [0.1,'#fff', true],
		border : [5, 0.3, '#666', true],
		offset: ['',''],
		area: ['640px','480px'],
		iframe: {src: '/pages/highlights.jsp?path=' + path}
	});
}
$(document).ready(function(){
	$(".video_rate").jRating({
		showRateInfo : false,
		step : true,
		length : 5,
		canRateAgain : true,
		nbRates : 999,
		rateMax : 5,
		onClick : function(element, rate) {
			$("#evaluate_star").val(rate);
		}
	});
});
function doevaluate(){
	var id = $("#evaluate_id").val();//"84213d6c-8ada-485b-97b0-2be0368980b9";
	var this_user_evaluated_id = $.cookie('this_user_evaluated_id');
	if(this_user_evaluated_id != null && this_user_evaluated_id == id){
		alert("您已经投参与过评价啦！");
	}else{
		var star = $("#evaluate_star").val();
		$.ajax({
			type : "POST",
			url : "/home!evaluate",
			data : { id:id, star:star },
			dataType : "json",
			success : function(data) {
				if (data.resCode == 200) {
					$.cookie('this_user_evaluated_id', id, { expires: 366,  path: '/' });
					window.location.reload();
				}
			},
			error : function() {alert("error");}
		});
	}
}
</script> 
