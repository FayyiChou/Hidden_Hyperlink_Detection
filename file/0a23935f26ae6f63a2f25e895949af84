function showsubmenu(sid)

{

	var aaa = document.getElementsByTagName('ul');

	var j =0;

	for(i=0;i<aaa.length;i++)

	{

		if(aaa[i].id.indexOf('submenu_')>-1)			

			aaa[i].style.display = 'none';

	}	

	var bbb = document.getElementById("submenu_" + sid );

	bbb.style.display= '';

}

</SCRIPT>
	

