<script language="javascript">
var flyArray = new Array();

function hang(id, fixedTop)
{
	var scrollTop = window.document.documentElement.scrollTop;

	var obj = document.getElementById("hang_" + id);
	var objTop = parseInt(obj.style.top, 10);

	if (objTop + 5 < scrollTop + fixedTop)
	{
		obj.style.top = objTop + 5 + "px";
	}
	else if (objTop - 5 > scrollTop + fixedTop)
	{
		obj.style.top = objTop - 5 + "px";
	}
}


function fly(id)
{
	var T = window.document.documentElement.scrollTop;
	var L = window.document.documentElement.scrollLeft;
	var B = T + window.document.documentElement.clientHeight;
	var R = L + window.document.documentElement.clientWidth;

	var obj = document.getElementById("fly_" + id);

	var objTop = parseInt(obj.style.top, 10);
	var objLeft = parseInt(obj.style.left, 10);
	var objWidth = parseInt(obj.style.width, 10);
	var objHeight = parseInt(obj.style.height, 10);

	obj.style.left = objLeft + flyArray[id][1] + "px";
	obj.style.top = objTop + flyArray[id][2] + "px";

	if (objLeft + objWidth >= R - 3)
	{
		flyArray[id][1] = - 1;
	}
	else if (objLeft <= L + 3)
	{
		flyArray[id][1] = 1;
	}

	if (objTop + objHeight >= B - 3)
	{
		flyArray[id][2] = - 1;
	}
	else if (objTop <= T + 3)
	{
		flyArray[id][2] = 1;
	}
}
</script>


