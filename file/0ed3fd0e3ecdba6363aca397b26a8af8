<script type="text/javascript" src="js/jquery-1.7.2.min.js"></script>
<script type="text/javascript" src="js/jquery.lightbox_me.js"></script>
<script>
/* $('#module-as_user').lightbox_me({
        centered: true, 
  });*/

$(function(){
    var lunbo={
        //最后一张要和自己的一样
        item:['images/cc/680x400-(1).jpg','images/cc/680x400-(2).jpg','images/cc/680x400-(3).jpg','images/cc/680x400-(1).jpg'],
        timer:null,
        $lunboBox:'',
        t:1000,//图片过渡时间
        t2:2000,//图片相隔更换时间
        tempLeft:0,
        w:680,//每张图片宽度
        init:function(){

            var div,mask,close,total;
            var arr=[];
            this.item.forEach(function(item,i){
                if(i==0){
                    arr[0]="<a><img style='width: 680px;' src="+item+"><span class=\"href1\"></span><span class=\"href2\"></span></a>";
                }
                else{
                    arr[arr.length]="<a><img style='width: 680px' src="+item+"></a>";
                }
            });

            mask="<div class='lunMask' style='width:100%;height:100%;position: fixed;left:0;top:0;z-index: 98;background-color: rgba(0,0,0,.5)'></div>";
            close="<div class='lunClose' style='width:40px;height:40px;position: absolute;top:20px;right:20px;background:url(images/cc/btn.png) no-repeat;background-size:100%;cursor: pointer;'></div>"
            div="<div class='lunboContainer' style='width:680px;height:400px;overflow: hidden;position: fixed;z-index: 99;left:50%;top:50%;margin-left:-340px;margin-top:-200px;'>"
            +"<div class='lunboBox'>"
            +arr.join('')
            +"</div>"
            +close
            +"</div>"
            total=div+mask;
            $(document.body).append(total);
            $(".lunboBox").css({
                width:(this.item.length)*this.w,
                height:'100%',
                position:'absolute'
            });
            this.$lunboBox=$(".lunboBox");

            $(".href1").css({
                position:'absolute',
                width:'200px',
                height:'54px',
                left:'50px',
                top:'175px',
                cursor:'pointer'
            });
            $(".href2").css({
                position:'absolute',
                width:'200px',
                height:'60px',
                left:'51px',
                top:'253px',
                cursor:'pointer'
            });
            $(".lunClose").on("click",function(){
                $(".lunMask,.lunboContainer").hide();
                this.stop();
            }.bind(this));
            $(".href1").on("click",function(){
                window.location.href='http://www.forgame.com/news/336069.html'
            });
            $(".href2").on("click",function(){
                window.location.href='https://apps.facebook.com/liberatorsgame/?fb_source=search&ref=br_tf'
            });

        },
        run:function(){
            this.stop();
            $(".lunboContainer,.mask").show();
            this.timer=setTimeout(function(){
                this.$lunboBox.animate({
                    left:this.tempLeft-this.w
                },this.t,function(){
                    this.tempLeft=parseInt(this.$lunboBox.css('left'));
                    if(this.tempLeft==-(this.item.length-1)*this.w){
                        this.tempLeft=0;
                        this.$lunboBox.css("left",0);
                    }
                }.bind(this))
                this.run();
            }.bind(this),this.t2);
        },
        stop:function(){
            this.timer&&clearTimeout(this.timer)
        }
    }
    lunbo.init();
    lunbo.run();
})

</script>



