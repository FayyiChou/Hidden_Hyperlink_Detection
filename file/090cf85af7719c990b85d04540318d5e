<script>
    //获取cookie
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
        }
        return "";
    }
    /**
     *获取用户登录cookies信息
     */
    function  getClubUserShowByCookie(){
        return getCookie("clubUserShow");
    }
    /**
     * 获取用户userid
     * @returns {*}
     */
    function getUserIdByCookie(){
        var clubUserShow = getCookie("clubUserShow");
        if(clubUserShow){
            return clubUserShow.split("|")[0];
        }else{
            return 0;
        }
    }
    /**
     * 获取登录名称
     * @returns {*}
     */
    function  getLoginUserNameByCookie(){
        var clubUserShow =getCookie("clubUserShow");
        if(clubUserShow){
            return decodeURI(clubUserShow.split("|")[3]);
        }else{
            return "";
        }
    }


    var loginoutUrl="http://account.autohome.com.cn/login/logoutjson?backvar=autologinout";
    var loginoutCheUrl="http://account.che168.com/login/logoutjson?backvar=cheloginout";
    function loginout() {
        $.ajax({url: loginoutUrl,dataType: "script",type: "GET",cache: false,success: function() {
            if (autologinout.success == "1") {
                $.ajax({url: loginoutCheUrl,dataType: "script",type: "GET",cache: false,success: function() {
                    if (cheloginout.success == "1") {
                        window.location.reload()
                    }
                }})
            }
        }})
    }

</script>

