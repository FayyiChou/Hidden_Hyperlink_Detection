        /**
         * 房型
         */
        $("#room-ppl-div ul li a").click(function(e){
            var target = $(e.target);
            var a = target.parent().parent().parent().find("a[type='button']");
            a.attr('value',target.attr('value'));
            a.text(target.text());
        });

        /**
         * 地區
         */
        $("#subzone-div a[type='button']").each(function(index,item){
            $(item).prop('disabled',true);
        });
        $("#zone-div ul li a").click(function(e){
            var target = $(e.target);
            var subzoneDiv = target.parent().parent().parent().parent().find('#subzone-div');
            var a = target.parent().parent().parent().find("a[type='button']");
            a.attr('value',target.attr('value'));
            a.text(target.text());

            if(target.attr('value') in subZoneChart){
                subzoneDiv.find("a[type='button']").prop('disabled',false);
                var list = subZoneChart[a.attr('value')];
                var ul = subzoneDiv.find("ul");
                ul.empty();
                var i = 0;
                if(list != null){
                    ul.append('<li><a value="0">全區</a></li>');
                    $.each(list, function(index, value) {
                        ul.append('<li><a value="' + index +'">' + value + '</a></li>');
                        if(i ==0){
                            var a = ul.parent().find("a[type='button']");
                            a.attr('value',0);
                            a.text('全區');
                        }
                        i++;
                    });
                }

                ul.find('li a').click(function(e){
                    var target = $(e.target);
                    var a = target.parent().parent().parent().find("a[type='button']");
                    a.attr('value',target.attr('value'));
                    a.text(target.text());
                });
            }else{
                var a = subzoneDiv.find("a[type='button']");
                a.prop('disabled',true);
                a.attr('value','');
                a.html('&nbsp;');
                subzoneDiv.find("ul").empty();
            }
        });

