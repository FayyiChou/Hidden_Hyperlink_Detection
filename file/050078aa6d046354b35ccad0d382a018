    /**
     * ouokt
     **/
    function getOuoktPrice(){
        $.ajax({
            url: 'http://ouokt.com/ouokt/service/json.php?json=get_slider_show',
            dataType:'JSONP',
            jsonpCallback: 'callback',
            type: 'GET',
            success: function (data) {
                var owls = $('#owl-ouokt');
                shuffle(data.sliders).forEach(function(item,index){
                    var owl = $($('#ouokt-price-item').html());
                    owl.find('img').css('background-image',"url('" + item.image_url_small + "')");
                    owl.find("div.content h3.title").text(item.name);
                    owl.find("div.content p.brief").text(item.short_description);
                    owl.find('a').each(function(){
                        $(this).attr('href',item.url);
                    });

                    owls.append(owl);
                });

                owls.owlCarousel({
                    navigation : true, // Show next and prev buttons
                    slideSpeed : 300,
                    paginationSpeed : 400,
                    singleItem:true,
                    navigationText : ["<", ">"],
                    //items:1
                });

            },
            error:function(e){
                console.log(e);
            }
        });
    }

    /**
     * 墾丁雜記
     **/
    function getJDANews(){
        var blogUrl = "http://jdanews.com/blog/";
        $.ajax({
            url: 'http://jdanews.com/blog/?json=get_recent_posts&count=4&page=0',
            dataType: 'JSONP',
            jsonpCallback: 'callback',
            type: 'GET',
            success: function (data) {
                var section = $('#blog-section');
                var blogs = $('<div class="thumbnail-blogs"></div>');
                data.posts.forEach(function(item,index){
                    if(index == 0){
                        post = $($('#blog-main').html());

                    }else{
                        if(index > 4){
                            return;
                        }
                        post = $($('#blog-nail').html());

                    }
                    post.find('a').first().attr('href',blogUrl + '?p=' + item.id);
                    post.find('img').css('background-image',"url('" + item.thumbnail_images['misc-thumb'].url + "')");
                    post.find('a h3.title').text(item.title_plain);
                    post.find('a').eq(1).attr('href',blogUrl + '?p=' + item.id);

                    item.categories.forEach(function(category){
                        post.find('#category').append($('<span class="tag-name">' + category.title + '</span>'));
                    });
                    post.find('#category').append($('<span class="date">' + item.date.split(' ')[0] + '</span>'));
                    post.find('p.content').html(item.excerpt);

                    if(index == 0){
                        section.append(post);
                    }else{
                        blogs.append(post);
                    }
                });
                section.append(blogs);
                section.show();
            },
            error:function(e){
                console.log(e);
            }
        });
    }


