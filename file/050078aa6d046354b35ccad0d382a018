                target.addClass('active');
            }
        });


        var areaTable = {"2":"kenting","2,6":"kenting-bay","2,7":"kenting-north","2,8":"kenting-south","2,9":"kenting-farm","2,10":"kenting-around","1":"souch-beach","1,2":"south-beach-face-sea","1,3":"souch-beach-north","11":"chuanfanshih","14":"eluanbi","12":"hengchun","12,36":"hengchun-north","12,11":"hengchun-center","12,35":"hengchun-sourth","12,12":"hengchun-west","13":"western-kenting","13,15":"longluantan","13,13":"houbihu","13,14":"kenting-seashore","15":"manjhou","16":"checheng","17":"sihjhongsi","18":"mudan","19":"fenggang"};
        var themeNameTable = {"21":"home-stay-inn","24":"spa-and-pool-villa","40":"couple-theme-bnb","54":"elder-friendly-bnb","33":"family-bnb","32":"pet-friendly-bnb","35":"BBQ-bnb","19":"sea-view-bnb"};

        /**
         * 開始搜尋
         **/
        $("#inn-search button.bottom-btn,#inn-search button.search-btn").click(function (e) {
            var target = $(e.target);
            var div = target.parent().parent();
            var dateInput = div.find("#datetimepicker,#datetimepicker-m");

            var innListUrl = "/inn-list";


            //區域
            var temp = '';
            var zonea = div.find("#zone-div a[type='button']");
            if(zonea.attr('value') != undefined && zonea.attr('value') != 0 && zonea.attr('value') != ''){
                var temp = zonea.attr('value');
                //innListUrl = innListUrl + "&area=" + zonea.attr('value');
                //子區域
                zonea = div.find("#subzone-div a[type='button']");
                if(zonea.attr('value') != undefined && zonea.attr('value') != 0 && zonea.attr('value') != ''){
                    //innListUrl = innListUrl + "&sarea=" + zonea.attr('value');
                    temp += ',' + zonea.attr('value');
                }else{

                }
            }
            if(temp in areaTable){
                innListUrl +=  '/' + areaTable[temp];
            }


            //設施條件
            var cates = [];
            var theme = "";
            div.find("#device-check-ul li.active").each(function(index,item){
                item = $(item);
                if(item.attr('type') == 'cate'){
                    cates.push(item.attr('value'));
                }else if(item.attr('type') == 'theme'){
                    var value = item.attr('value');
                    if(value in themeNameTable){
                        theme = '/' + themeNameTable[value];
                    }

                }
            });
            var cate = "";
            cates.sort(function(a, b){return a-b});
            cates.forEach(function(item,index){
                if(index == 0){
                    cate += "/cate/";
                }else{
                    cate += '-';
                }
                cate += item;
            });
            innListUrl += theme + cate;


            //人數
            var ppla = div.find("#room-ppl-div a[type='button']");
            if(ppla.attr('value') != undefined && ppla.attr('value') != 0 && ppla.attr('value') != ''){
                innListUrl = innListUrl + "/ppl/" + ppla.attr('value');
            }


            //日期
            var dateI = null;
            if(dateInput.data("DateTimePicker").date() != null) {
                var date = new Date(dateInput.data("DateTimePicker").date().format());
                dateI = date.getFullYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate();
                innListUrl = innListUrl + "/dateI/" + dateI;
            }


            location.href = innListUrl;

        });

        $('#btn-search').click(function () {
            var input = $('#searchInput');
            location.href = "/search/?q=" + input.val();
        });
        $("#searchInput").on('keyup', function (e) {
            if (e.keyCode == 13) {
                location.href = "/search/?q=" + $(this).val();
            }
        });

        /**
         * 天氣
         */
        var data = {};
        data.Device = 'WEB';
        data.CCode = 'TW';
        $.ajax({
            url: "/uukt_3/services/misc/weather.php",
            type: 'POST',
            data:data,
            dataType:'json',
            crossDomain: true,
            error: function (response) {
                console.log(response);
            },
            success: function (response) {
                if(response.Result == 0){
                    weatherInit(response.Value);
                }

            },
            beforeSend: function () {
            },
            complete: function () {
            }
        });

    });


    function weatherInit(source){
        var otherdays = source.OtherDay;
        var div = $('#weather-div');
        div.find(".today p.day-today").text("今天" + source.Week);
        div.find(".today .icon-data p.temperature").text(source.Tempe);
        div.find(".today .icon-data p.description").text(source.WeatN);

        div.find(".today .icon-data img").css('background-image','url(' + weatherImageUrl(source.WeatV) + ')');
//        div.find(".today .icon-data img").css('background-color','white');

        div.find(".future-6 .list-root .one-day").each(function(index,item){
            item = $(item);
            var otherday = otherdays[index];
            if(index == 0){
                item.find("p.date .day").text("明天");
            }else if(index == 1){
                item.find("p.date .day").text("後天");
            }else{
                item.find("p.date .day").text(otherday.Date);
            }
            item.find("p.date .week").text(otherday.Week);

            item.find("p.temperature").text(otherday.Tempe);
            item.find("p.description").text(otherday.WeatN);

            item.find("img").css('background-image','url(' + weatherImageUrl(otherday.WeatV) + ')');
//            div.find("img").css('background-color','white');

        });
    }

    function weatherImageUrl(weather){
        return "/uukt_3/images/main/weather/w-" + weather + '.png';
    }

    /**
     * 將陣列隨機排序
     * @param array
     * @returns {*}
     */
    function shuffle(array) {
        var m = array.length, t, i;

        // While there remain elements to shuffle…
        while (m) {

            // Pick a remaining element…
            i = Math.floor(Math.random() * m--);

            // And swap it with the current element.
            t = array[m];
            array[m] = array[i];
            array[i] = t;
        }

        return array;
    }
</script>
</body>
</html>

