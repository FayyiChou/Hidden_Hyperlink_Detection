<script type="text/javascript">

jq10("#topsel_s").click(function(){
 		var username=jq10("#words1").val();
jq10('#type1').val('des');
    jq10("#scbar_form").attr("action",'plugin.php?id=zsj_search&type=design&words1='+username);
});
