 function errorhandle_followmod(msg) {
 		showDialog(msg, 'notice',  '<img src="' + IMGDIR + '/loading.gif"> ', null, true, null, '', '', '', 5);
 }

     function succeedhandle_followmod(url, msg, values) {
           var numObj = $('followernum_'+values['fuid']);
          if(numObj) {followernum = parseInt(numObj.innerHTML);}
            if(values['type'] == 'add') {
               if(values['from'] == 'head') {
                    if($('followflag')) $('followflag').style.display = '';
                    if($('unfollowflag')) $('unfollowflag').style.display = 'none';
                    if($('fbkname_'+values['fuid'])) $('fbkname_'+values['fuid']).style.display = '';
               } else if($('a_followmod_'+values['fuid'])) {

                    $('a_followmod_'+values['fuid']).innerHTML = '已关注';
                    $('a_followmod2_'+values['fuid']).innerHTML = '已关注';
                    $('a_followmod3_'+values['fuid']).innerHTML = '已关注';

                    if(values['from'] != 'block') {
                        // $('a_followmod_'+values['fuid']).className = 'ny_good_a03';
//		 $('a_followmod_'+values['fuid']).style.backgroundColor = '#3885bb'; 
                    }
                    $('a_followmod_'+values['fuid']).href = 'home.php?mod=spacecp&ac=follow&op=del&fuid='+values['fuid']+(values['from'] == 'block' ? '&from=block' : '');
$('a_followmod2_'+values['fuid']).href = 'home.php?mod=spacecp&ac=follow&op=del&fuid='+values['fuid']+(values['from'] == 'block' ? '&from=block' : '');
$('a_followmod3_'+values['fuid']).href = 'home.php?mod=spacecp&ac=follow&op=del&fuid='+values['fuid']+(values['from'] == 'block' ? '&from=block' : '');


               }
               if(numObj) {
                    numObj.innerHTML = followernum + 1;
               }

          } else if(values['type'] == 'del') {
                if(values['from'] == 'head') {
                    if($('followflag')) $('followflag').style.display = 'none';
                    if($('unfollowflag')) $('unfollowflag').style.display = '';
                    if($('followbkame_'+values['fuid'])) $('followbkame_'+values['fuid']).innerHTML = '';
                    if($('fbkname_'+values['fuid'])) {
                         $('fbkname_'+values['fuid']).innerHTML = '[!follow_add_bkname!]';
                         $('fbkname_'+values['fuid']).style.display = 'none';
                    }
               } else if($('a_followmod_'+values['fuid']))  {
                    $('a_followmod_'+values['fuid']).innerHTML = '加关注';
                    $('a_followmod2_'+values['fuid']).innerHTML = '加关注';
                    $('a_followmod3_'+values['fuid']).innerHTML = '加关注';

                    if(values['from'] != 'block') {
                     //    $('a_followmod_'+values['fuid']).className = 'ny_good_a03';
//			 $('a_followmod_'+values['fuid']).style.backgroundColor = '#ff6600';
                    }
                    $('a_followmod_'+values['fuid']).href = 'home.php?mod=spacecp&ac=follow&op=add&hash=a937620b&fuid='+values['fuid']+(values['from'] == 'block' ? '&from=block' : '');
$('a_followmod2_'+values['fuid']).href = 'home.php?mod=spacecp&ac=follow&op=add&hash=a937620b&fuid='+values['fuid']+(values['from'] == 'block' ? '&from=block' : '');
$('a_followmod3_'+values['fuid']).href = 'home.php?mod=spacecp&ac=follow&op=add&hash=a937620b&fuid='+values['fuid']+(values['from'] == 'block' ? '&from=block' : '');
               }
               if(numObj) {
                    numObj.innerHTML = followernum - 1;
               }
          } else if(values['type'] == 'special') {
               if(values['from'] == 'head') {
                    var specialObj = $('specialflag_'+values['fuid']);
                    if(values['special'] == 1) {
                         specialObj.className = 'flw_specialfo';
                         specialObj.innerHTML = '!follow_add_special_following!';
                    } else {
                         specialObj.className = 'flw_specialunfo';
                         specialObj.innerHTML = '!follow_del_special_following!';
                    }
                    specialObj.title = specialObj.innerHTML;
                    specialObj.href = 'home.php?mod=spacecp&ac=follow&op=add&hash=a937620b&special='+values['special']+'&fuid='+values['fuid']+'&from=head';
               } else {
                    $('a_specialfollow_'+values['fuid']).innerHTML = values['special'] == 1 ? '!follow_add_special_following!' : '!follow_del_special_following!';
                    $('a_specialfollow_'+values['fuid']).href = 'home.php?mod=spacecp&ac=follow&op=add&hash=a937620b&special='+values['special']+'&fuid='+values['fuid'];
               }
          }
     }
