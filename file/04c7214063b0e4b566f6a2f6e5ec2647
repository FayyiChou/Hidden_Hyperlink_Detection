$('a_fav2_158632').href='home.php?mod=spacecp&ac=favorite&type=thread&id=731010&formhash=a937620b';
 		 	$('a_fav2_158632').innerHTML='<i class="icon_fix icon_fixb"></i>';
 }else{
    msg = '已收藏';
$('a_fav_158632').href='plugin.php?id=zsj_one:ajax_delForum&formhash=a937620b&mod=fav&tid=731010';
$('a_fav2_158632').href='plugin.php?id=zsj_one:ajax_delForum&formhash=a937620b&mod=fav&tid=731010';
             $('a_fav2_158632').innerHTML='<i class="icon_fix icon_fixd"></i>';

 }
 $('a_fav_158632').innerHTML=msg;
 $('a_fav_158632').title=msg;
 $('a_fav2_158632').title=msg;
 showDialog(msg, 'notice',  '<img src="' + IMGDIR + '/loading.gif"> ', null, true, null, '', '', '', 5);

