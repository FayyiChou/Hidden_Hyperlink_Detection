//这个是图片加载宽度超出截取
var nowimg=jQuery(".previews").length;
function xianshii()
{
var isIE=!!window.ActiveXObject; 
    var isIE6=isIE&&!window.XMLHttpRequest; 
    var isIE8=isIE&&!!document.documentMode; 
    var isIE7=isIE&&!isIE6&&!isIE8; 

    var win = (jQuery(window).scrollTop() + jQuery(window).height());
    jQuery(".previews").each(function () {
        var top = jQuery(this).offset().top;	
        if (top < win) {
            if (jQuery(this).attr("mysrc") != "") {
 var mysrc=jQuery(this).attr("mysrc")
 var _this=jQuery(this);
              jQuery(this).attr("src","");
  jQuery(this).attr("mysrc","");
  
  if(isIE||isIE6||isIE8||isIE7){
                if( _this.attr("src")==""){
   _this.attr("src",mysrc);
    }
             }else{
  jQuery.getJSON(mysrc+"&imageInfo", function(data){
     var mywidth=data['width'];
 if(mywidth>1142){
                   _this.attr("src",mysrc+"/thumbnail/1142x");
}
else{
                   _this.attr("src",mysrc);
}
             });
 }
            }
        }
    });
 }
    jQuery(window).scroll(function () {
    if(nowimg>0){
            xianshii();
}
  });
 if(nowimg>0){
    xianshii();
      }
//截取图片完毕


