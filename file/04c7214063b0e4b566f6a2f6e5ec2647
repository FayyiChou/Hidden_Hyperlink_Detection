jq10("a[id^='sea1_']").click(function(){
 		var strstr=jq10(this).attr("id");
var type=strstr.replace("sea1_",""); 
        jq10('#type1').val(type);		
jq10('#scbar_mod').val('search');
jq10("#scbar_form").attr("action",'http://www.zhisheji.com/plugin.php?id=zsj_search');
        jq10('#type1').val(type);	
jq10('#searchtype1').val(type);
 
});
</script>



