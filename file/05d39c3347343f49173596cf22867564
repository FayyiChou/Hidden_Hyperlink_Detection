            (function(){
                var imgList = document.querySelectorAll("img.preload"),
                        imgSrcList = [];
                for(var i = 0;i < imgList.length;i++){
                    var _m = imgList[i],
                            _img = new Image();
                    _img.src = _m.src;
                    imgSrcList.push({img:_m, r_img: _img});
