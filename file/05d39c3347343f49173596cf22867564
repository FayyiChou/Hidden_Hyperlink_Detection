                function getDownLayer(){
                    var html = "<div style='width:90%;left:5%;padding:15px 0;background:#fbfceb;color:#fff;top:33%;position:relative;border-radius:5px;'>";
                            html += '<img src="https://m.jzb.com/y/m_recall/no_jzb.png" style="width:60%;margin-top:-28%;" >';
