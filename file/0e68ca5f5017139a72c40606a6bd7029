     <script>
         function pageInitialize(){
             gototargetmodel();
             $("html, body").scrollTop(0);
             fangsuo();
             $(".about_ss").hide();

             $(document).scroll(function() {
                 if($(window).scrollTop()>0){
                    $(".about_ss").show();
                 }else{
                     $(".about_ss").hide();
                 }
             });
             $("body").click(function(){
                 $(".enum").hide();

             });
             $(".pic_wenzi").css({"color":"#000"})
            $(".pic_title").mouseover(function(){
                $(this).children().children().hide();
                $(this).children().children().next().show();
                TweenMax.to($(this).children().children().next(),0.5,{scale:1.1});
                TweenMax.to($(this).children().children(),0.5,{scale:1.1});
                $(this).css({"background-color":"black"})
                $(this).children().next().css({"color":"#907b53"})
                $(".pic_wenzi2").css({"color":"gray"});
            })
             $(".pic_title").mouseout(function(){
                 $(this).children().children().show();
                 $(this).children().children().next().hide();
                 $(".pic_wenzi").show();
                 $(".pic_wenzi2").show();
                 TweenMax.to($(this).children().children().next(),0.5,{scale:1});
                 TweenMax.to($(this).children().children(),0.5,{scale:1});
                 $(this).css({"background-color":"white"})
                 $(".pic_wenzi").css({"color":"#000"})

             })
             $(".menu_btn").click(function(){
                 event.stopPropagation();
                 $(".enum").animate({height: 'toggle', opacity: 'toggle'}, 300,function(){
                 })
             })
             $(".about_us").click(function(){
                 $(".about_ss").show();
                 var y=$(".about_ss").offset().top;
                 $("html").animate({scrollTop:y},300);
                 $("body").animate({scrollTop:y},300);
             });
             $(".service").click(function(){
                 $(".about_ss").show();
                 var y=$(".about_ss").offset().top;
                 $("html").animate({scrollTop:y},300);
                 $("body").animate({scrollTop:y},300);
             });
             $(".case").click(function(){
                 $(".about_ss").show();
                 var y=$(".about_content").offset().top;
                 $("html").animate({scrollTop:y-250},300);
                 $("body").animate({scrollTop:y},300);
             });
             $(".key").click(function(){
                 $(".about_ss").show();
                 var y=$(".family").offset().top;
                 $("html").animate({scrollTop:y},300);
                 $("body").animate({scrollTop:y},300);
             });
             $(".contact").click(function(){
                 $(".about_ss").show();
                 var y=$(".map").offset().top;
                 $("html").animate({scrollTop:y+200},300);
                 $("body").animate({scrollTop:y},300);
             });


         }
         function fangsuo(){
             //TweenLite.set($.role("app-content-portrait"),{width:$(window).width()})
             TweenLite.to($.role("app-content-portrait"),0,{scale:$(window).width()/1920,transformOrigin:"top left"});
            // $.role("app-content-portrait").css({"height":6700*$(window).width()/1920});
             $(window).resize(function(){TweenLite.set($.role("app-content-portrait"),{scale:$(window).width()/1920,transformOrigin:"0 0"});$(".about_ss").show();})
         }
         function gototargetmodel(){
             var modelname=window.location.search.substr(7,1);
             switch (modelname){
                 case '1':
                     $(".about_ss").show();
                     var y=$(".about_ss").offset().top;
                     $("body").animate({scrollTop:y},300);
                     break;
                 case '2':
                     $(".about_ss").show();
                     var y=$(".about_content").offset().top;
                     $("body").animate({scrollTop:y-250},300);
                     break;
                 case '3':
                     $(".about_ss").show();
                     var y=$(".family").offset().top;
                     $("body").animate({scrollTop:y},300);
                     break;
                 case '4':
                     $(".about_ss").show();
                     var y=$(".map").offset().top;
                     $("body").animate({scrollTop:y+200},300);
                     break;
                 default :
                     break;
             }

         }
     </script>
     <script data-main="Public/lib/app.document" src="Public/lib/loader/require.js"></script>

