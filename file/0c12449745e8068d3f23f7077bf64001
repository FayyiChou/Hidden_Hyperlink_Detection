function ChangeTab_xw(Index,Id){		
		var blk	=	Id	+	'_blk';
		for(i=1;i < 3;i++){
			var idx	=	Id	+	i;
			if(Index==i){
				$("#"+blk+i).css("display","block");
				document.getElementById(idx).className="tpchose";
			}else{
				
				$("#"+blk+i).css("display","none");
				document.getElementById(idx).className="";
			}
		}
}

//图片新闻切换
function ChangeTpxw(No,Id){
		var tit = Id+'_tit';
		var blk = Id+'_blk';
		
			if(No==1){
				document.getElementById(tit+'1').className="tp tpchose";
				document.getElementById(tit+'2').className="sp";
				document.getElementById(blk+'1').style.display="block";
				document.getElementById(blk+'2').style.display="none";
			}else if(No==2){
				document.getElementById(tit+'1').className="tp";
				document.getElementById(tit+'2').className="sp tpchose";
				document.getElementById(blk+'1').style.display="none";
				document.getElementById(blk+'2').style.display="block";
			}
		
	
}
</script>

