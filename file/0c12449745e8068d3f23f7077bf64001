<script language="JavaScript">
	//背景图随机变换
	$(document).ready(function() {  
		
	　　 image = new Array(); //定义image为图片数量的数组
	　　 image [0] = '/pub/jscz/images/b_bg1.jpg' //背景图象的路径
	　　 image [1] = '/pub/jscz/images/b_bg2.jpg'
	　　 number = Math.floor(Math.random() * image.length);		
	　	document.getElementById('body').style.background="url("+ image[number] +")  top no-repeat fixed";	
		
	});
	

	//导航下拉--网上办事图片切换
	function changebg_wsbs(no){
		for(var i=1; i<=2; i++){
			var id = "dh_wsbs" + i;
			var id_no = "dh_wsbs" + "_no" + i;
			if(no == i){
				document.getElementById(id).style.display="block";
				document.getElementById(id_no).className="chose";
			}else{
				document.getElementById(id).style.display="none";
				document.getElementById(id_no).className="";
			}
		}
		
	}
</script>
<script type="text/javascript">
//网站友情链接
function showPad(no){
	var menu = ['','fmenu1','fmenu2','fmenu3','fmenu4'];
	var pad = ['','fpad1','fpad2','fpad3','fpad4'];
	
	var padsitu	=	document.getElementById(menu[no]).className;
	if('wzdh_nor' == padsitu){
		for(var i = 1; i < menu.length; i++){
			if(no == i){
				document.getElementById(pad[i]).style.display = 'block';
				document.getElementById(menu[i]).className = 'wzdh_chose';	
				document.getElementById(menu[i]).getElementsByTagName('img').item(0).src='images/wzdh_li2.png';	
				
			}else{
				document.getElementById(pad[i]).style.display = 'none';
				document.getElementById(menu[i]).className = 'wzdh_nor';
				document.getElementById(menu[i]).getElementsByTagName('img').item(0).src='images/wzdh_li1.png';				
			}
		}		
	}else if('wzdh_chose' == padsitu){		
		document.getElementById(pad[no]).style.display = 'none';
		document.getElementById(menu[no]).className = 'wzdh_nor';
		document.getElementById(menu[no]).getElementsByTagName('img').item(0).src='images/wzdh_li1.png';	
	}
}
