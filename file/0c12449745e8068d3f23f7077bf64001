			document.getElementById('nav_t'+id).className=" n_gz";	
		}else if(6 == id){
			document.getElementById('nav_t'+id).className=" n_zx";	
		}else{
			document.getElementById('nav_t'+id).className=" n_czwh";
		}
	closetimer = window.setTimeout(mclose, timeout);
}
function mcancelclosetime(id)
{
	if(closetimer)
	{
		window.clearTimeout(closetimer);
		closetimer = null;
		
		document.getElementById('nav-current').style.background="url(./images/navbg_707.png) no-repeat";
		
