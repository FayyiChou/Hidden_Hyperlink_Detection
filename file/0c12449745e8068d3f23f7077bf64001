function mclosetime(id)
{
	if(1 == id){
			document.getElementById('nav_t'+id).className="n_sy";
			
		}else if(2 == id){
			document.getElementById('nav_t'+id).className="n_czt";
		}else if(3 == id){
			document.getElementById('nav_t'+id).className="n_xw";	
