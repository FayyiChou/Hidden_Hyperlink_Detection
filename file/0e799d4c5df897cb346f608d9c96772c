    <script language="javascript">
        function light(str) {
            if (str == 1) {
                img1.src = "/images/pic06-1-1.gif";
            } else if (str == 2) {
                img2.src = "/images/pic06-2-1.gif";
            } else if (str == 3) {
                img3.src = "/images/pic06-3-1.gif";
            } else if (str == 4) {
                img4.src = "/images/pic06-4-1.gif";
            } else if (str == 5) {
                img5.src = "/images/pic06-5-1.gif";
            } else if (str == 6) {
                img6.src = "/images/pic06-6-1.gif";
            } else if (str == 7) {
                img7.src = "/images/pic06-7-1.gif";
            }
        }
        function dark(str) {
            if (str == 1) {
                img1.src = "/images/pic06-1.gif";
            } else if (str == 2) {
                img2.src = "/images/pic06-2.gif";
            } else if (str == 3) {
                img3.src = "/images/pic06-3.gif";
            } else if (str == 4) {
                img4.src = "/images/pic06-4.gif";
            } else if (str == 5) {
                img5.src = "/images/pic06-5.gif";
            } else if (str == 6) {
                img6.src = "/images/pic06-6.gif";
            } else if (str == 7) {
                img7.src = "/images/pic06-7.gif";
            }
        }
    </script>

