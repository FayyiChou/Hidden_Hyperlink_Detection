<script language="javascript">
    function searchtitle()
    {
        var searchInput = document.getElementById("searchInput").value;
        if (searchInput == "" || searchInput == "请输入文章标题") {
            alert('请输入搜索条件。');
            document.getElementById("searchInput").focus();
        } else {
            window.location.href = "/cgi-bin/research/Research?function=Search&searchInput=" + searchInput;
        }
    }
    function reportsearchtitle()
    {

        var searchInput = document.getElementById("searchInput").value;
        if (searchInput == ""|| searchInput =="请输入搜索内容") {
            alert('请输入搜索条件。');
            document.getElementById("searchInput").focus();
        } else {
            openSearchResult(searchInput);
           // window.location.href = "/cgi-bin/research/Research?function=Search&searchInput=" + searchInput + "&type=" + "spqh";
        }
    }
</script>
