	function CheckLogin(formlog)
	{
		if (document.formlog.UserName.value=="")
		{
			alert("会员帐号必填");
			formlog.UserName.focus();
			return (false);
		}
		if (document.formlog.Password.value=="")
		{
			alert("密码必填");
			formlog.Password.focus();
			return (false);
		}
	}
</script>
