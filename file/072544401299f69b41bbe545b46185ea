var flag=false;
function DrawImage(ImgD,FitWidth,FitHeight){
	var image=new Image();
	var iwidth = FitWidth; //定义允许图片宽度
	var iheight = FitHeight; //定义允许图片高度
	image.src=ImgD.src;
	if(image.width>0 && image.height>0)
	{
		flag=true;
