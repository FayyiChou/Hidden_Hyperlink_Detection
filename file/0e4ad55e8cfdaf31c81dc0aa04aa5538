  <script type="text/javascript">
function topLoginForm(obj){
	if(obj.user_name.value == ''){
		alert('请输入用户名');
		obj.user_name.focus();
		return false;
	}
	else if(obj.password.value == ''){
		alert('请输入密码');
		obj.password.focus();
		return false;
	}
	else if(obj.vsn.value == ''){
		alert('请输入验证码');
		obj.vsn.focus();
		return false;
	}
}
</script>   
