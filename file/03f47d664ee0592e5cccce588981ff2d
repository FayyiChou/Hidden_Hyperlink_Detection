    //检查110028、110027两只基金，当日购买累计金额不能超过100万元
    //add by yougang 2014-10-30
    function checkAmount(itemCode) {
        if (itemCode == '110028' || itemCode == '110027') {
            if ($("#errorMessage").html() == '') {
                $("#errorMessage").html("当日购买累计金额不能超过10万元");
                $("#errorInfo").fadeIn();
                $("#feeTip").hide();
            }
        }
    }
   
</script>
