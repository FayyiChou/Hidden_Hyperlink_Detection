    AM.ready("md5",function() {
        $(".add_zan .zanBox ,.ticket_more").live("click",function() {
            var str = "http://huzhou.19lou.com/forum-1298-thread-2026401314680892199-1-1.html";
            var m5 = $.md5(str);
            var p1 = m5.substr(0,2);
            var p2 = m5.substr(2,2);
