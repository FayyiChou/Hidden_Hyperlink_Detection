      <script language=JavaScript>
function ch_value(n,obj){
	if(n == ""){
		cError = cError + obj + " !\n";
    	bErrorFlag = true;
	}
}

function iCheckFieldsEnews(theForm){

	cError = "请检查以下栏位:\n\n";
	bErrorFlag=false;
    
	ch_value(theForm.mail.value,'电子邮箱不能为空');

	mail = theForm.mail.value;
	if(mail != ""){
		if ((mail.length<5 ) || (mail.indexOf("@")==-1) ||(mail.indexOf(".")==-1)){
			cError=cError+"电子邮件格式有误!\n";
			bErrorFlag=true;
		}
	}

	if (bErrorFlag){
   		alert(cError);
		bErrorFlag=false;
   		return false;
  	}else{
   		return true;
  	}

}

</script>
