<script type="text/javascript">
	var List = $('input[name="image"]'); 
     var pics="";
     var texts="";
	// var swfUrl="";
	for(i=0;i<List.length;i++){
		if(i==0){
			pics=List[i].value;
			texts = ""+i+""+i+"";
			
		}else{
			pics=pics+"|"+List[i].value;
			texts = texts+"|"+i+""+i+"";
		}
	}

	//var urlList=document.getElementsByName("imageUrl");
	var urlList = $('input[name="imageUrl"]'); 
	var links = "";
	for(j=0;j<urlList.length;j++){
		if(j==0){
			links=urlList[j].value;
			//swfUrl = links.substring(links.indexOf("//")+2,links.indexOf(":8083")+5);
		}else{
			links=links+"|"+urlList[j].value;
		}
		
	}
	
     var focus_width=303
     var focus_height=183
     var text_height=0
     var swf_height = focus_height+text_height
     
    // var pics=imgUrl1+"|"+imgUrl2+"|"+imgUrl3+"|"+imgUrl4
	//var links=imgLink1+"|"+imgLink2+"|"+imgLink3+"|"+imgLink4
  //   var texts=imgtext1+"|"+imgtext2+"|"+imgtext3+"|"+imgtext4
     var flashCode = '<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/hotdeploy/flash/swflash.cab#version=6,0,0,0" width="'+ focus_width +'" height="'+ swf_height +'">';
     flashCode = flashCode + '<param name="allowScriptAccess" value="sameDomain"><param name="movie" value="http://www.minnov.com/wp-content/themes/minnov/focus2.swf"><param name="quality" value="high"><param name="bgcolor" value="#F0F0F0">';
     flashCode = flashCode + '<param name="menu" value="false"><param name=wmode value="opaque">';
     flashCode = flashCode + '<param name="FlashVars" value="pics='+pics+'&links='+links+'&alt=msy&texts='+texts+'&borderwidth='+focus_width+'&borderheight='+focus_height+'&textheight='+text_height+'">';
     flashCode = flashCode + '<embed src="http://www.minnov.com/wp-content/themes/minnov/focus2.swf" quality="high" pluginspage="http://www.macromedia.com/go/getflashplayer" type="application/x-shockwave-flash" width="'+ focus_width +'" height="'+ swf_height +'" FlashVars="pics='+pics+'&links='+links+'&texts='+texts+'&borderwidth='+focus_width+'&borderheight='+focus_height+'&textheight='+text_height+'"></embed>';
     flashCode = flashCode + '</object>';
