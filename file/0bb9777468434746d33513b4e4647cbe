               if(readCookie('userId')!=''){
                  uid=readCookie('userId');
               }else{
                  alert("请先登录！");
                  return false;
               }
                var state=t.getAttribute("hasnotice");
                console.log(state);
                if(state=="false"){
                    t.innerText="关注";
                    t.setAttribute("hasnotice","true");
