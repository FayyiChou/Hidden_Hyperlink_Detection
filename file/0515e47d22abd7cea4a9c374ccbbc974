function Pasize(){
	var pinnerHTML = document.getElementById('photoList').innerHTML;
	var psize = pinnerHTML.split('&lt;li&gt;');
	var t1 = document.getElementById('psize');
	var t2 = document.getElementById('psize1');
	t1.innerHTML=psize.length-1;
	t2.innerHTML=psize.length-1;
}
Pasize();

