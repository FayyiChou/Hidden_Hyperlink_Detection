        <script>
            (function () {
                var bg1 = $('.header-bg.bg-1'),
                    bg2 = $('.header-bg.bg-2'),
					bg3 = $('.header-bg.bg-3'),
					bg4 = $('.header-bg.bg-4'),
					bg5 = $('.header-bg.bg-5'),
                    bullet = $('.bullets li'),
                    flag = true,
                    time = '',
                    loop = function () {
                         if (flag) {
                            $('.button-group').stop().fadeOut(2000);
                            bg1.stop().fadeOut(2000);
                            bg2.stop().fadeIn(2000);
                            bullet.removeClass('active').eq(1).addClass('active');
                        } else {
                            bg2.stop().fadeOut(2000);
                            bg1.stop().fadeIn(2000);
                            $('.button-group').stop().fadeIn(2000);
                            bullet.removeClass('active').eq(0).addClass('active');
                        }
						
						
                        flag = !flag;
                    };
                bullet.click(function () {
                    var index = $(this).attr('data-index');
                    clearInterval(time);
					
					if (index == 1) {
                        bg1.stop().fadeOut();
						bg3.stop().fadeOut();
						bg4.stop().fadeOut();
						bg5.stop().fadeOut();
                        bg2.stop().fadeIn();
                        $('.button-group').stop().fadeIn();
                        bullet.removeClass('active').eq(1).addClass('active');
                    }
					if (index == 2) {
                        bg1.stop().fadeOut();
						bg2.stop().fadeOut();
						bg4.stop().fadeOut();
						bg5.stop().fadeOut();
                        bg3.stop().fadeIn();
                        $('.button-group').stop().fadeIn();
                        bullet.removeClass('active').eq(2).addClass('active');
                    }
					if (index == 3) {
                        bg1.stop().fadeOut();
						bg2.stop().fadeOut();
						bg3.stop().fadeOut();
						bg5.stop().fadeOut();
                        bg4.stop().fadeIn();
                        $('.button-group').stop().fadeIn();
                        bullet.removeClass('active').eq(3).addClass('active');
                    }
					if (index == 4) {
                        bg1.stop().fadeOut();
						bg2.stop().fadeOut();
						bg3.stop().fadeOut();
						bg4.stop().fadeOut();
                        bg5.stop().fadeIn();
                        $('.button-group').stop().fadeIn();
                        bullet.removeClass('active').eq(4).addClass('active');
                    }
					
				
					
					
					
					
					
                    flag = !flag;
                    time = setInterval(loop,8000);
                });
                time = setInterval(loop,8000);
            })();
        </script>
   
