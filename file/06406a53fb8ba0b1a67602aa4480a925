    function setErrorContent(){
    	var startTime = Date.parse("Oct 14 2016 18:30:00");
    	var endTime = Date.parse("Oct 16 2016 08:00:00");
			var timestamp = Date.parse(new Date());
			if(timestamp == startTime || (timestamp > startTime && timestamp < endTime) || timestamp == endTime){
				$("#dealError").html('尊敬的21CN邮箱用户，为提升服务质量，我们将在10月15日22点-16日凌晨4点对系统进行升级，期间可能出现服务短暂中断的情况，给您带来的不便，请谅解。21CN邮箱运营团队');
