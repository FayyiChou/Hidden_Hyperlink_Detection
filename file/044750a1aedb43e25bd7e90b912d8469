   <script type="text/javascript" src="http://husadw.com/themes/dreamGinge/images/site.js"></script> 
   <img src="index.php?cron=page%3Aindex&amp;p=" width="1" height="1" border="none" /> 
   <div id="template-modal" style="display:none;"> 
    <div class="dialog"> 
     <div class="dialog-title"> 
      <div class="title span-auto">
       {title}
      </div> 
      <div class="dialog-close-btn">
       X
      </div> 
      <div style="clear:both"></div> 
     </div> 
     <div class="dialog-content">
       {content} 
     </div> 
    </div> 
