	$(window).scroll(function () {
		if ($(this).scrollTop() > 300) {
			$('div.up').fadeIn();
		} else {
			$('div.up').fadeOut();
		}
	});
	$('div.up').click(function () {
		$('body,html').animate({
		scrollTop: 0
		}, 800);
		return false;
		});
	});
	$(function(){
		$('div.news_1, div.news_2,div.news_3,div.photo-block').mouseenter(function(){
			$(this).find('div.show-hide').fadeIn();
			$(this).find('div.gray').fadeIn();
		});
		$('div.news_1, div.news_2,div.news_3,div.photo-block').mouseleave(function(){
			$(this).find('div.show-hide').fadeOut();
			$(this).find('div.gray').fadeOut();
		});
		$('.call-us').click(function(){
			callvision = $('.call-us-form').css('display');
			if(callvision == 'none'){
			$('.call-us-form').css('display','block');
			}else{
			$('.call-us-form').css('display','none');
			}
		});
		$('.zvon').click(function(){
			var name = $('.u-name').val();
			var phone = $('.u-phone').val();
			var text = $('.u-text').val();
			var send = 'Send';
			$.ajax({
			type:'post',
			url:'/bitrix/AJAX/zakaz-zvonka.php',
			data:{
			'NAME':name,
			'PHONE':phone,
			'TEXT':text,
			'SEND':send
			},
			success:function(data){
			$('.res-info').html(data);
			}
			});
			return false;
		});
		var carousel = $("#carousel").waterwheelCarousel({
		autoPlay:4000,
		separationMultiplier:0.95,
		opacityMultiplier:1 ,
		sizeMultiplier:0.9,
		separation:160,
		flankingItems: 2,
		movingToCenter: function ($item) {
			$('#callback-output').prepend('movingToCenter: ' + $item.attr('id') + '<br/>');
		},
		movedToCenter: function ($item) {
			$('#callback-output').prepend('movedToCenter: ' + $item.attr('id') + '<br/>');
		},
		movingFromCenter: function ($item) {
			$('#callback-output').prepend('movingFromCenter: ' + $item.attr('id') + '<br/>');
