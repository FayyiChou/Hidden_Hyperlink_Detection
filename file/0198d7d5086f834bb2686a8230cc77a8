<script language="javascript">
var scheduleObject = $("scheduleSpan");
var loadingObject = $("scheduleLoading");
var theYear = 2017;
var theMonth = 2;

function getLastMonthSchedule()
{
	if (theMonth > 1)
	{
		theMonth = theMonth - 1;
	}
	else
	{
		theYear = theYear - 1;
		theMonth = 12;
	}
	if (theYear > 2100 || theYear < 2000)
	{
		theYear = 2017;
	}
	getSchedule(theYear,theMonth);
}

function getNextMonthSchedule()
{
	if (theMonth < 12)
	{
		theMonth = theMonth + 1;
	}
	else
	{
		theYear = theYear + 1;
		theMonth = 1;
	}
	if (theYear > 2100 || theYear < 2000)
	{
		theYear = 2017;
	}
	getSchedule(theYear,theMonth);
}

function getSchedule(getYear,getMonth)
{
	loadingObject.style.display = "block";
	var url = buildUrl("schedule_get.php","y",getYear,"m",getMonth);
	ajaxGetEval(url,function(xmlhttp)
	{
		scheduleObject.innerHTML = xmlhttp.responseText;
		loadingObject.style.display = "none";
	});
}
</script>


