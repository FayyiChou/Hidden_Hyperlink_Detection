<script language="javascript">
function ctlentYYDB(evt)
{
	if(evt.keyCode == 13)
	{
		serachYYDB();
	}
}

function serachYYDB()
{
	ajaxGetTag($('yydbSearchInput'));
}

function doTab(tabTag,titleKey,tabKey,maxNum)
{
	/*
	titleKey = "tabTitle";
	tabKey = "tab";
	maxNum = 2;
	*/
	maxNum = parseInt(maxNum);
	if (maxNum < 2)
	{
		maxNum = 2;
	}

	for (i=1; i<=maxNum; i++)
	{
		eval("titleVar = titleKey+i");
		eval("tabVar = tabKey+i");

		if (tabTag == i)
		{
			$(titleVar).style.borderBottom = '1px none #000000';
			$(titleVar).style.background = '#FFFFFF';
			$(titleVar).style.color = '#000000';
			$(tabVar).style.display = '';
		}
		else
		{
			$(titleVar).style.borderBottom = '1px solid #000000';
			$(titleVar).style.background = '#EEEEEE';
			$(titleVar).style.color = '#666666';
			$(tabVar).style.display = 'none';
		}
	}
}
</script>

