function replyOne(someone){	
oldContent = $("message").value;
prefix = "@" + someone + " ";
newContent = ''
if(oldContent.length > 0){
if (oldContent != prefix) {
newContent = prefix + oldContent;
}
} else {
newContent = prefix
}
$("message").focus();
$("message").value = newContent;
}
</script>

