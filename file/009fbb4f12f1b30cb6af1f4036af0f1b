<script>
var sign_today = new Date();
var tmpyear  = sign_year  = 
2017;//sign_today.getFullYear();
var tmpmonth = sign_month = 
4;//sign_today.getMonth()+1;
var tmpday   = sign_day   = 
1;//sign_today.getDate();
function showSign(data) {
if(data.error) alert('数据错误，请重试');
var sign_container = $('sign_tip_container');
sign_container.style.display = 'block';
$('calendar_span').innerHTML = sign_year+"年"+sign_month+"月"
var html = '';
//当前月第一天是周几
var startDay = new Date(sign_year,sign_month-1,1).getDay();
//本月有多少天 用下个月的最的第0天表示月最后一天
var nDays = new Date(sign_year,sign_month,0).getDate();
//画日历
var numRow = 0;
var h = Math.ceil((startDay+nDays)/7);
var calendarCantioner = $('sign_succ_canlerdar_days');
var l=1;
for (i=1;i<=h;i++) {
numRow++;
tr = calendarCantioner.insertRow(calendarCantioner.rows.length);
if(i == 1) {
for(j=0;j<7;j++) {
td = tr.insertCell(j);
for (jd in data.data) {
if(jd == l && j>=startDay) {
if(sign_year == tmpyear &&
sign_month == tmpmonth && l == tmpday){
td.className = 'j_'+i+'_'+k+ " signed_anime_day";
$('sign_index_num').innerHTML=data.data[jd].rank;

for (sign in data) {
if(sign == 'contineSign') {
$('contineSign').innerHTML=data.contineSign;
}
if(sign == 'allSign') {
$('allSign').innerHTML=data.allSign;
}
if(sign == 'signaddStatus') {
if(data.signaddStatus == 1) {
$('sign_succ_title').innerHTML = '签到成功！';
} else {
$('sign_succ_title').innerHTML = '今日已签到！,请明日继续努力';
}
}
}
} else {
td.className = 'j_'+i+'_'+k+ " signed_day";

}
td.title = "签到增加财富值";
break;
} else {
td.className = 'j_'+i+'_'+k;
}
}
//td.className = 'j_'+i+'_'+j;
if(j<startDay) {
td.innerHTML = '&nbsp;';
} else {
td.innerHTML = l;
l++;
}

}
} else {
for(k=0;k<7;k++) {
td = tr.insertCell(k);
for (jd in data.data) {
if(jd == l) {
if(sign_year == tmpyear &&
sign_month == tmpmonth && l == tmpday){
td.className = 'j_'+i+'_'+k+ " signed_anime_day";
$('sign_index_num').innerHTML=data.data[jd].rank;
for (sign in data) {
if(sign == 'contineSign') {
$('contineSign').innerHTML=data.contineSign;
}
if(sign == 'allSign') {
$('allSign').innerHTML=data.allSign;
}
if(sign == 'signaddStatus') {
if(data.signaddStatus == 1) {
$('sign_succ_title').innerHTML = '签到成功！';
} else {
$('sign_succ_title').innerHTML = '今日已签到！请明日继续努力';
}
}
}
} else {
td.className = 'j_'+i+'_'+k+ " signed_day";
}
td.title = "签到增加财富值";
break;
} else {
td.className = 'j_'+i+'_'+k;
}
}

if(l<=nDays)
td.innerHTML = l;
else
td.innerHTML = '&nbsp;';
l++;
}
}
}

$('sign_contain_info').onmouseover = function(e) {
sign_container.style.display='block';
}
$('sign_contain_info').onmouseout = function(e) {
sign_container.style.display='none';
}
}
function resetRows(){
var calendarCantioner = $('sign_succ_canlerdar_days');
var trs = findtags(calendarCantioner,'tr');
var trlength = trs.length;
for(i=trs.length-1;i>=0;i--){
trs[i].parentNode.removeChild(trs[i]);
}
}
$('calendar_pre').onclick = function(event) {
sign_month = sign_month-1;
if(sign_month == 0) {
sign_month = 12;
sign_year = sign_year-1;
}
getSignLog(sign_year,sign_month,'signlog')
}
$('calendar_next').onclick = function(event) {

sign_month = sign_month+1;
if(sign_month == 13) {
sign_month = 1;
sign_year = sign_year+1;
}
getSignLog(sign_year,sign_month,'signlog')
}
function getSignLog(year,month,type,event) {
resetRows();
if(type == 'signadd') {
sign_year = tmpyear;
sign_month = tmpmonth;
if (event.pageX) {
mousePosX = event.pageX;
} else {
dom=document.documentElement;
mousePosX = event.clientX+dom.scrollLeft-dom.clientLeft;
}
$('sign_tip_container').style.left = (mousePosX - 166)+'px';
}
var url='./getview.php?type='+type;
sajax = Ajax('JSON');
sajax.post(url+'&inajax=1&rod='+Math.random(),'year='+year+"&month="+month,showSign);
$('signClick').innerHTML = ' 今日已签到';
}
</script>
