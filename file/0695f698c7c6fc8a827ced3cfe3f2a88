var speed3=30
var MyMar3=setInterval(Marquee3,speed3)
demo22.innerHTML=demo21.innerHTML
demo20.onmouseover=function() {clearInterval(MyMar3)}
demo20.onmouseout=function() {MyMar3=setInterval(Marquee3,speed3)}
function Marquee3(){
if(demo22.offsetWidth-demo20.scrollLeft<=0)
   demo20.scrollLeft-=demo21.offsetWidth
else{
   demo20.scrollLeft++
}
}
