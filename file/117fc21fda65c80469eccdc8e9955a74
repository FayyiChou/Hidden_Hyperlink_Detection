    <script>
        var SchoolId = -1;
        var comefromcss = "http://file.safetree.com.cn/special/SchoolVote2015/Styles/css/push.css?r=183";
        var comefrom = 0;
        $(function () {
            WebServes.checkUserLoginNoLayer(function (json) {
                if (json != null && json.userid > -1) {//已登录

                    var RegisterUrl = getCookie("ServerSide");
                    if (RegisterUrl == "-1" || RegisterUrl == null) {//非实验区
                        location.href = "2015ModelSchool_f.html";
                    }
                    else {
                        comefrom = json.comefrom;
                        weixin();
                        if (json.cityid == 330600 || json.cityid == 320500) {//苏州、绍兴显示学校信息
                            $("#PCtp").show();
                        }
                        if (json.region == 0) {
                            SchoolId = json.schoolid;
                            //GetQrCode(SchoolId, json.coutryid);
                            StudentVoteStatus(SchoolId);
                            LoadStudentIndexData(SchoolId);
                            var UserId = getCookie("UserID");
                            $("#y_login").show();
                            if (getCookie(UserId + "_2015sjtp") != null) {
                                $("#y_vote").show();
                            }
                        } else {
                            location.href = "2015ModelSchool_f.html";
                        }
                    }
                } else {//未登录
                    weixin();
                    $("#y_login").hide();
                }
            });

            inputTipText();
            GetTopSchool();
        });

        function GetTopSchool() {
            $.getJSON(Asmx + "/GetTopSchool?jsoncallback=?", { r: Math.random() }, function (json) {
                var Html = '<tr><th>学校</th><th width="20%">累计票数</th><th width="20%">本校学生投票</th><th width="25%">校外投票<span>(邀请好友投票)</span></th></tr>';
                if (json) {
                    if (json.length > 0) {
                        for (var i = 0; i < json.length; i++) {
                            var AllVoteTotal = json[i].SchoolVoteTotal + json[i].OutSchoolVoteTotal;
                            var title = json[i].PrvName + "-" + json[i].CityName + " " + json[i].SchoolName;
                            Html += "<tr><td width='35%''><div class='nm-div'><a title='" + title + "'>" + title + "</a></div></td><td>" + AllVoteTotal + "</td><td>" + json[i].SchoolVoteTotal + "</td><td>" + json[i].OutSchoolVoteTotal + "</td></tr>";
                        }
                    } else {
                        Html += "<tr><td colspan=4>暂无数据!</td></tr>";
                    }
                } else {
                    Html += "<tr><td colspan=4>暂无数据!</td></tr>";
                }
                $("#Top10").html(Html);
            });
        }

        function LoadStudentIndexData(schoolid) {
            $.getJSON(Asmx + "/LoadStudentIndexData?jsoncallback=?", { r: Math.random(), SchoolID: schoolid }, function (json) {
                if (json) {
                    var SchoolName = "未知";
                    var SchoolVoteTotal = "0";
                    var SchoolVoteRate = "0%";
                    var OutSchoolVoteTotal = "0";
                    var SchoolRank = "0";
                    if (json.SchoolName != null && json.SchoolName.length > 0) {
                        SchoolName = json.SchoolName;
                    }
                    if (json.SchoolVoteTotal != null) {
                        SchoolVoteTotal = json.SchoolVoteTotal;
                    }
                    if (json.SchoolVoteRate != null) {
                        SchoolVoteRate = json.SchoolVoteRate;
                    }
                    if (json.OutSchoolVoteTotal != null) {
                        OutSchoolVoteTotal = json.OutSchoolVoteTotal;
                    }
                    if (json.SchoolRank != null) {
                        SchoolRank = json.SchoolRank;
                    }
                    $("#SchoolName").text(SchoolName);
                    $("#SchoolVoteTotal").text(SchoolVoteTotal);
                    $("#SchoolVoteRate").text(SchoolVoteRate);
                    $("#OutSchoolVoteTotal").text(OutSchoolVoteTotal);
                    var SRHtml = "全国排名:<span>";
                    SRHtml += SchoolRank > 1000 ? "1000</span>以外" : (SchoolRank == 0 ? "-" : SchoolRank) + "</span>";
                    $("#SchoolRank").html(SRHtml);
                }
            });
        }
        function weixin() {
            if (comefrom > 0) {
                $(".wxbox-img-pingtai03").attr("src", 'http://file.safetree.com.cn/angel/push/push.html?location=100003&siteid=' + comefrom + '&test=1&css=' + comefromcss);
                $(".wxbox-img-zonghao03").hide();
                $(".wxbox-img-pingtai03").show();

                $(".wxbox-img-pingtai07").attr("src", 'http://file.safetree.com.cn/angel/push/push.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss);
                $(".wxbox-img-zonghao07").hide();
                $(".wxbox-img-pingtai07").show();
                $(".wxbox-img-pingtai08").attr("src", 'http://file.safetree.com.cn/angel/push/push.html?location=100008&siteid=' + comefrom + '&test=1&css=' + comefromcss);
                
            } else {
                $.getJSON(Asmx + "/getEplatformByIP?jsoncallback=?", { r: Math.random() }, function (res) {
                    if (res != null) {
                        comefrom = res.EptID;
                        if (comefrom > 0) {
                            $(".wxbox-img-pingtai03").attr("src", 'http://file.safetree.com.cn/angel/push/push.html?location=100003&siteid=' + comefrom + '&test=1&css=' + comefromcss);
                            $(".wxbox-img-zonghao03").hide();
                            $(".wxbox-img-pingtai03").show();

                            $(".wxbox-img-pingtai07").attr("src", 'http://file.safetree.com.cn/angel/push/push.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss);
                            $(".wxbox-img-zonghao07").hide();
                            $(".wxbox-img-pingtai07").show();
                            $(".wxbox-img-pingtai08").attr("src", 'http://file.safetree.com.cn/angel/push/push.html?location=100008&siteid=' + comefrom + '&test=1&css=' + comefromcss);
                        }

                    }
                });
            }
        }

        function Vote() {
            if (qgIsEnd == 0) {
                $("#w_vote").hide();
                $("#y_vote_ing").show();
                var UserId = getCookie("UserID");
                if (getCookie(UserId + "_2015sjtp") == null) {
                    var strGUID = getCookie("PhoneGUID");
                    if (strGUID == null || strGUID == "") {
                        strGUID = new GUID().newGUID();
                        setCookie("PhoneGUID", strGUID, 365);
                    }
                    var Referrer = document.referrer != null && document.referrer.length > 0 ? document.referrer : "";
                    var BrowserInfo = special.BrowserInfo();
                    var thisShareUserID = getCookie("ShareUserID") != null ? getCookie("ShareUserID") : "";
                    var thisShareUserGUID = getCookie("ShareUserGUID") != null ? getCookie("ShareUserGUID") : "";
                    $.getJSON(Asmx + "/SubmitVote?jsoncallback=?", { r: Math.random(), VoteType: 1, ObjSchoolID: SchoolId, ShareUserID: "", ShareUserGUID: "", UserClient: special.IsPC() ? 1 : 2, Browser: BrowserInfo.appname + "|" + BrowserInfo.version, Url: location.href, Referrer: Referrer }, function (json) {
                        if (json) {
                            if (json.Status == "True") {
                                $("#y_vote_ing").hide();
                                $("#y_vote").show();
                                show('tph-box');
                                setCookie(UserId + "_2015sjtp", 1, 365);
                                var voteTotal = parseInt($("#SchoolVoteTotal").text());
                                $("#SchoolVoteTotal").text(voteTotal + 1);
                            } else {
                                $("#y_vote_ing").hide();
                                if (json.Msg == "非实验区学生用户") {
                                    $("#w_vote").show();
                                    alert("您不是学生用户，无法进行投票!");
                                } else {
                                    $("#y_vote").show();
                                    alert(json.Msg);
                                }
                            }
                        }
                    });
                } else {
                    alert("您之前已经投过票了");
                    $("#y_vote_ing").hide();
                    $("#y_vote").show();
                    setCookie(UserId + "_2015sjtp", 1, 365);
                }
            } else {
                alert("抱歉！活动已结束，谢谢参与！");
            }

        }

        function StudentVoteStatus(SchoolID) {
            $.getJSON(Asmx + "/StudentVoteStatus?jsoncallback=?", { SchoolID:SchoolID,r: Math.random() }, function (json) {
                if (json) {
                    if (json.Status == "True") {
                        var UserId = getCookie("UserID");
                        setCookie(UserId + "_2015sjtp", 1, 365);
                        $("#w_vote").hide();
                        $("#y_vote").show();
                    }
                }
            })
        }

