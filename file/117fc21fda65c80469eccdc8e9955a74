            if (e.keyCode == 13) {
                special.userLogin();
            }
        });
        $(".tph-box .wx-tp, .cont-boxar .wx-tp").click(function () {
            $(".wx-wxfxbox").show();
            popCoverDiv(); //锁屏
        });
        $(".logintc .input_bga, .logintc .input_bgb").mouseover(function () {
            $(this).css({ "border-color": "#0075c2" });
        }).mouseout(function () {
            $(this).css({ "border-color": "#ccc" });
        }); 
        
