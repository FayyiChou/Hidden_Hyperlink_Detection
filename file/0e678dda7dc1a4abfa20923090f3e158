	var getQuizMoney = function(data) {
		if (data != null && typeof data == 'object') {
			alert(dwr.util.toDescriptiveString(data, 2));
		} else {
			alert(dwr.util.toDescriptiveString(data, 1));
		}
	}
	var alertGMSG = function(data) {
		if (data != null && typeof data == 'object') {
			alert(dwr.util.toDescriptiveString(data, 2));
		} else {
			var d=dwr.util.toDescriptiveString(data, 1);
			if(d.length!=0){
			alert(d);
			}
		}
	}
	//UNameCheck.syncMemberInfo(alertGMSG);
	function resume(){return true;}
	//onerror=resume;
	
