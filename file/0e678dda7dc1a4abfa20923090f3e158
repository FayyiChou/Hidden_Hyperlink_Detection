<script type="text/javascript">
/*
 *网吧地图窗口
 */
function networkStoreAddress(id){
	$.layer({
							type : 2,
							title : ['网吧地图',true],
							iframe : {src : '/contest/getAddress?id='+id},
							area : ['620px' , '500px'],
							offset : ['50px','']
						});	
	}
/*
*预约窗口
*/
function showWindow(url,title){
	$.layer({
							type : 2,
							title : [title,true],
							iframe : {src : url},
							area : ['620px' , '500px'],
							offset : ['50px','']
						});
	}
function showWindowLoginSync(url,title){
	$.layer({
							type : 2,
							title : [title,true],
							iframe : {src : url},
							area : ['620px' , '500px'],
							offset : ['50px',''],
							end: function(){
								//$("#top").load("/member/memberTop.jsp?date="+date.getTime());
								}
						});
	}
/*弹出窗口，判断是否需要登录*/
	function isLogin() {
		var r=false;
		$.ajax({ url: "/action/api/getLoginName",async:false, context: document.body, success: function(e){
			console.log(e);
		if(e.indexOf("吧友")!=-1||e.indexOf("游客")!=-1){
				$(".openlogin").click();
				//showWindowLoginSync('/loginPop.jsp','请先登录');
				r=false;
			}else{
				r=true;
				}
      }});
	 // alert(r);
		return r;
	}
	
function showWindowLogin(url,title){
	if(!isLogin()){
		return;
		}
	$.layer({
							type : 2,
							title : [title,true],
							iframe : {src : url},
							area : ['620px' , '500px'],
							offset : ['50px','']
						});
}
//close
/* if(""==1){
	var i = parent.layer.getFrameIndex(window.name);
	parent.layer.close(i);
} */
</script>
    <script type="text/javascript">
    //签到
    function qiandao(){
    	var qiandaoCount=$("#qiandaoCount").text();
     if(true){
	    $(".openlogin").click();
       }else{
    	        $.ajax({
			type: "post",
            url: "/payRed",
            dataType: "text",
			success: function(msg){
    	        	if(msg=="two"){
    	        		alert("今天已经签到过了~~")
    	        	}else{
    	        		alert(msg)
    	        		$("#qiandaoCount").text(parseInt(qiandaoCount)+1)
    	        	}
	      	}
	      });
				 }
    }
    ;(function($) {
        $.fn.extend({
            Tabs: function(options) {
                // 处理参数
                options = $.extend({
                    event: 'mouseover',
                    timeout: 0,
                    auto: 0,
                    callback: null
                }, options);
                var self = $(this),
                    tabBox = self.children('div.tab_box').children('div'),
                    menu = self.children('ul.tab_menu'),
                    items = menu.find('li'),
                    timer;
                var tabHandle = function(elem) {
                        elem.siblings('li')
                            .removeClass('current')
                            .end()
                            .addClass('current');
                        tabBox.siblings('div')
                            .addClass('hide')
                            .end()
                            .eq(elem.index())
                            .removeClass('hide');
                    },
                    delay = function(elem, time) {
                        time ? setTimeout(function() {
                            tabHandle(elem);
                        }, time) : tabHandle(elem);
                    },
                    start = function() {
                        if (!options.auto) return;
                        timer = setInterval(autoRun, options.auto);
                    },
                    autoRun = function() {
                        var current = menu.find('li.current'),
                            firstItem = items.eq(0),
                            len = items.length,
                            index = current.index() + 1,
                            item = index === len ? firstItem : current.next('li'),
                            i = index === len ? 0 : index;
                        current.removeClass('current');
                        item.addClass('current');
                        tabBox.siblings('div')
                            .addClass('hide')
                            .end()
                            .eq(i)
                            .removeClass('hide');
                    };
                items.bind(options.event, function() {
                    delay($(this), options.timeout);
                    if (options.callback) {
                        options.callback(self);
                    }
                });
                if (options.auto) {
                    start();
                    self.hover(function() {
                        clearInterval(timer);
                        timer = undefined;
                    }, function() {
                        start();
                    });
                }
                return this;
            }
        });
    })(jQuery);
    function tabInit(a,b,act){
        a.on(act,function(e){
            e.stopPropagation();
            var inde = a.index(this);
            a.removeClass("active").eq(inde).addClass("active");
            b.hide().eq(inde).show();
        })
    }
    $(document).ready(function() {
        tabInit($(".f_bonus_tab li"),$(".f_top_bonus .cont"),"mouseover");
        tabInit($(".f_video .videotab"),$(".f_video .f_videolist"),"mouseover");
        $("#fbanner").Tabs({
          auto: 3000
      });
        /*投资*/
        $("#m-boxlist-tzxm .bd li").each(function(i){ $("#m-boxlist-tzxm .bd li").slice(i*4,i*4+4).wrapAll("<ul></ul>");});
        /*赛程*/
        $("#m-boxlist-sczb .bd li").each(function(i){ $("#m-boxlist-sczb .bd li").slice(i*5,i*5+5).wrapAll("<ul></ul>");});
        $(".bd>ul").each(function(index, el) {
        	$(this).find('li').first().addClass('first');
        	$(this).find('li').last().addClass('last');
        });
        $(".m-boxlist").slide({titCell:".hd ul",mainCell:".bd",autoPage:true,effect:"leftLoop",autoPlay:false});
        $(".m-boxlist").hover(function(){
        	$(this).addClass("active");
        },function(){
        	$(this).removeClass("active")
        })
        $(".matchsearch").on('click','.btn' ,function(event) {
            layer.open({
    			type: 2,
    			title: '赛点查询',
    			shadeClose: true,
    			shade: 0.8,
    			area: ['610px', '510px'],
    			content: '/iframe/actpk.html' //iframe的url
            });
        });
    });
        /*弹出窗口，判断是否需要登录*/
	function showRoom(id) {
		var r=false;
		$.ajax({ url: "/action/api/roomCan?qid="+id,async:false, context: document.body, success: function(e){
			console.log(e);
		if(e==0){
			r = true;
		}else if(e!=0){
			if(e=="请先登录"){
				$(".openlogin").click();
				//showWindowLoginSync('/loginPop.jsp','请先登录');
			}else if(e=="该房间比赛已经结束"){
				alert(e);
				loadRooms(0);
			}else{
			 alert(e);
			}
			r = false;
		}else{
       		alert("请求失败，请稍后重试");
			r = false;
		}
      }});
		return r;
	}
    </script>
</body>
</html>
