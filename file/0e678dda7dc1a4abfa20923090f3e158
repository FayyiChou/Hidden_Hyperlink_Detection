<script type="text/javascript">
$(document).ready(function() {
        $(".uc_nav").on('click', '.title', function(event) {
            event.preventDefault();
            var $this = $(this);
            $this.next(".titlegrop").toggle(200);
        });
        
        $("#mark").on("keyup",function(){
		var code=$("#mark").val();	
		var phone = $("#phone").val();
         if (code.length >=6 ) {
        	 $.ajax({
			type: "post",
            url: "/action/sms/back/checkCode",
            data: {code:code,phone:phone,newUser:1},
            dataType: "text",
			success: function(msg){
            	if(msg=="ok"){
            	$("#showInfo").text("验证成功")
            	$("#showInfo").css("color","green")
            	clearInterval(setVTime);
                $("#Send").css("display","none");
            	 	setVtimes = 0;
             	}else{
             	$("#showInfo").text("验证失败")
             	$("#showInfo").css("color","red")
             	}
	      	}
	      });
    };
})
    });
function daohang(val){
		  $.ajax({
			type: "post",
            url: "/dh",
            data: {dh:val},
            dataType: "text",
			success: function(msg){
	      	}
	      });
}
  function checkUserName(){
	  var flag=false;
	  var val=$("#username").val();
	  if(val==""||val==null){
		  $("#userName").text("用户名不能为空！")
           $("#userName").css("color","red");
	  }else{
		  	  $.ajax({
			type: "post",
            url: "/checkUserName",
            async:false,
            data: {username:val},
            dataType: "text",
			success: function(msg){
            		if(msg=="ok"){
            		$("#userName").text("用户名已存在！")
            		$("#userName").css("color","red");
            	}else{
            		if(val!=""){
            		$("#userName").text("")
            		}
            		flag=true;
            	}
	      	}
	      });
	  }
	  return flag;
  }
   function checkEmail(){
	    var flag=false;
	   var val=$("#email").val();
	   var reg = /^([a-zA-Z0-9_-])+@([a-zA-Z0-9_-])+((\.[a-zA-Z0-9_-]{2,3}){1,2})$/;
	   if(val==""||val==null){
		   $("#Email").text("邮箱不能为空！")
           $("#Email").css("color","red");
	   }else if(!reg.test(val)){
		    $("#Email").text("邮箱格式不对！")
           $("#Email").css("color","red");
	   }else{
		     $.ajax({
			type: "post",
            url: "/checkEmail",
             async:false,
            data: {email:val},
            dataType: "text",
			success: function(msg){
            	if(msg=="ok"){
            		$("#Email").text("邮箱已存在！")
            		$("#Email").css("color","red");
            	}else{
            			$("#Email").text("")
            		flag=true;
            	}
	      	}
	      });
	   }
	   return flag;
  }
   function checkPass(){
	   var flag=false;
	    var val=$("#password").val();
	    if(val==""||val==null){
	    	$("#pass").text("密码不能为空！")
            $("#pass").css("color","red");
	    }else{
	    	$("#pass").text("")
	    	flag=true;
	    }
	    return flag;
   }
   function checkPassre(){
	   var flag=false;
	    var val=$("#password").val();
	    var valre=$("#repassword").val();
	    if(valre==""||valre==null){
	    	$("#repass").text("确认密码不能为空！")
            $("#repass").css("color","red");
	    }else if(val!=valre){
	    	$("#repass").text("两次密码不一致")
            $("#repass").css("color","red");
	    }else{
	    	$("#repass").text("")
	    	flag=true;
	    }
	    return flag;
   }
   function checkPhone(){
	   var flag=false;
	    var val=$("#phone").val();
	    var p=/^13[0-9]{9}$|14[0-9]{9}|15[0-9]{9}$|18[0-9]{9}$|17[0-9]{9}$/;
	    if(val==""||val==null){
	    	 $("#cPhone").text("手机号不能为空")
	    	  $("#cPhone").css("color","red");
	    }else
	       if(!(p.test(val))){ 
				   $("#cPhone").text("手机格式有误！")
				    $("#cPhone").css("color","red");
			   }else{
				    $("#cPhone").text("")
				   flag=true;
			   }
	    return flag;
   }
   
     var setVTime = null;
	  var setVtimes = 0;
	function setVal(id,val,txt){
			if(val>0){
				$(id).text("剩"+val+"秒");
			}else{
				$(id).text(txt);
				clearInterval(setVTime);
				$(id).removeClass("disabled");
			}
		}
      function checkP(){
	   var flag=false;
	    var val=$("#phone").val();
	    var p=/^13[0-9]{9}$|14[0-9]{9}|15[0-9]{9}$|18[0-9]{9}$|17[0-9]{9}$/;
	    if(val==""||val==null){
	    	 $("#cPhone").text("手机号不能为空")
	    	  $("#cPhone").css("color","red");
	    }else
	       if(!(p.test(val))){ 
				   $("#cPhone").text("手机格式有误！")
				    $("#cPhone").css("color","red");
			   }else{
				    $("#cPhone").text("")
				   flag=true;
			if(val==""||val==null){
			$("#cphone").text("请填写手机号！")
			$("#cphone").css("color","red");
			}else{
			if (setVtimes == 0) {
			$.ajax({
			type: "post",
            url: "/action/sms/back/checkPhone",
            data: {phone:val},
            dataType: "text",
			success: function(msg){
            if(msg=="ok"){
           if(setVtimes > 0){
				return false;
			}
            $("#Send").addClass("disabled");
				setVtimes = 120;
						setVTime = setInterval(function(){
							setVal("#Send",setVtimes,"重发");
							if(setVtimes >0){
								setVtimes -=1;
							}
						},1000);
						$("#showCode").css("display","block");
             	}else{
             		$("#cPhone").text("该手机号已被使用！")
				    $("#cPhone").css("color","red");
             	}
	      	}
	      });
			}
			}
			   }
   }
    function checkQq(){
	   var flag=false;
		var userQQ=$("#qqinfo").val();
		if(userQQ==""||userQQ==null){
			   $("#cQq").text("QQ不能为空！")
				$("#cQq").css("color","red");
		}else if(isNaN(userQQ)){
				$("#cQq").text("QQ输入有误！")
				$("#cQq").css("color","red");
			}else if(userQQ.length<4||userQQ.length>13){
				$("#cQq").text("QQ格式有误！")
				$("#cQq").css("color","red");
			}else{
				$("#cQq").text("")
				flag=true;
			}
		return flag;
   }
   function checkForm(){
	   var flag=false;
  if(checkUserName()&checkEmail()&checkPass()&checkPassre()&checkPhone()&checkQq()){
	  if($("#showInfo").text()=="验证成功"){
		   flag=true;
	  }else{
		  $("#cPhone").text("请验证手机号！")
		  $("#cPhone").css("color","red");
	  }
	   }
	   return flag;
   }
</script>
