<script language="javascript" type="text/javascript">
		function SubmitSearch(url)
		 {
		var siteid='请输入搜索关键词!';
			if(document.getElementById('txtKeyWord'))
			{
			  var keyword=Trim(document.getElementById('txtKeyWord').value);
			  if(keyword=="")
			  {
				 alert(siteid);
				 document.getElementById('txtKeyWord').onfocus;
				 return false;
			  }
			  else
			  {
				 window.open(url+"&keyword="+encodeURIComponent(keyword));
			  }
			
			} 
		
		 }
		 function LTrim(str) {
			if (str == null) {
				return "";
			}	
			var len = 0;
			while(str.charAt(len) == " ")
				len++;
			return str.substring(len);
		}
		
		
		function RTrim(str) {
			if (str == null) {
				return "";
			}
			var len = str.length;
			while(str.charAt(len-1) == " ")
				len--;
			return str.substring(0,len);
		}	
		
		
		function Trim(str) {		
			return LTrim(RTrim(str));
		}
		function searchEnter(keyword,e)
			{
				var keycode = 0;
				if(window.event) {
					keycode = event.keyCode;
				}
				else {
					keycode = e.which;
				}
				if (keycode == 13) {
					SubmitSearch('/templates/T_Second/index.aspx?nodeid=38');
					return false;
				}
				else {
					return true;
				}
			}	
        </script>
