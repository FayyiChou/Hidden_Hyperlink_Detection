  <script type="text/javascript">
$(function(){
	var offsetTop=0;
	var offsetTopStr=0;
    $(window).scroll(function (){  
        var st=$(document).scrollTop(); 
        if(st>100){  
		offsetTop="-85px";
		offsetTopStr="5px";
        }else{   
		offsetTop="0px";
		offsetTopStr="90px";
        }  
        $(".header").animate({top : offsetTop },{ duration:300 , queue:false });  
		 $(".lb_dh").animate({top : offsetTopStr },{ duration:300 , queue:false });  
    });  
	
	})
</script> 
