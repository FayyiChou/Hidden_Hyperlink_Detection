<script>
	document.domain = "tiancity.com";
	function TcLoginWin_Init(){
		TcLoginWin.SiteId = 51936;
		TcLoginWin.ReLoadPage = true;
	}
	$(function(){
		$(".shovel li").click(function(){
			$(this).stop(true,true).addClass('active').siblings().stop(true,true).removeClass('active');
			$(".tudi").fadeIn().css("z-index",3);
			$(".tudi a").eq($(this).index()).stop(true,true).fadeIn().siblings().stop(true,true).fadeOut();
		});
		$(".close").click(function(){
			$(".tudi").css("z-index",1);
		});
	});

	function lottery(type){
		if (TcLoginWin.LoginSpecial()) {
			$.ajax({
				url:'index.php?c=request&m=lottery&type='+type,
				type:'get',
				dataType:'json',
				success:function(data)
				{
					if(data.irv==200)
					{
						if(type=='gold_shovel')
							$('#layer1 p').html('恭喜您用金铲子挖到了'+data.prize_name);
						else if(type=='silver_shovel')
							$('#layer1 p').html('恭喜您用银铲子挖到了'+data.prize_name);
						else
							$('#layer1 p').html('恭喜您用铜铲子挖到了'+data.prize_name);
						$('#layer1 font').html('Coupon:'+data.coupon);
						setTimes(type);
						showDialog.show(layer1);
					}
					else if(data.irv==301)
					{
						showDialog.show(layer2);
					}
					else
					{
						alert(data.msg);
					}
					return;
				}
			});
		}
	}

	function setTimes(type)
	{
		var times = $('#'+type).html();
		times = times>0?(parseInt(times)-1):0;
		$('#'+type).html(times);
	}
</script>
