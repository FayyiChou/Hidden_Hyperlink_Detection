		//群组空间字排序
		function wdbreak(contain,textCount){
		    var html="",len=0;
		    var $a = contain.find('a');
			$a.each(function(index){
			
				 var $this=$(this),text = $.trim($this.text());
				  var strArr=text.match(/[^\u4E00-\u9FA0]/g);
				  var sLen=0;
				   if(!strArr){
				      sLen+=text.length
				   }
				   else{
					   sLen+=text.length-strArr.length/2;
				   }
				     len+=sLen;
					 if(len<=textCount&& $a.length!=(index+1)){
						 if(($a.eq(index+1).text().length+len)<=textCount){
							 $this.after(" \/")
						 }
						 else{
							 len=0;
							 $this.after("<br>");
						 }
					 }
					 else{
						 len=sLen;
					 }
			})
		}
		wdbreak($('.wid-xd .wid-cls-nav'),10)
		wdbreak($('.wid-xk .wid-cls-nav'),12)
		wdbreak($('.wid-qy .wid-cls-nav'),12)
		wdbreak($('.wid-fl .wid-cls-nav'),15)
	    /*弹出层*/
 })
