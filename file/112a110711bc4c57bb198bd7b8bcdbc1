<script type="text/javascript">
   $(function(){ 
	 //返回顶部
	   var bt=$(".backtoTop");
	   bt.click(function(){
	   	$(window).scrollTop(0);
	   }); 
	   $(window).scroll(function(){
		   if( $(window).scrollTop()<parseInt($('.wid-header').css('height'))){
			   bt.hide(100);
		   }
		   else{
			   bt.show(100);
		   }
	   })
	   
   })
</script>
