		var intId,intTotalCount = 0;
		setTimeout(qinDrive,1000);
		function qinDrive(){
			intId=setInterval(function(){
				var i=Math.random() *514;
				var img = $(rainHtml);
				img.css('left',i+"px");
				$('.wid-rain').append(img);
				intTotalCount++;
				if(intTotalCount==40){
				  $('.qindao-ad').addClass("qanimate2 fadeOut")
				}
				else if(intTotalCount==70){
				   $('.qindao-ad').removeClass('wid-bg10 fadeOut qanimate2').addClass("wid-bg7 qanimate2-fadeIn fadeIn")
				}
				else if(intTotalCount==150){
				   $('.qindao-ad').removeClass("qanimate2-fadeIn fadeIn").addClass('fadeOut qanimate2');
				}
				else if(intTotalCount==180){
				   $('.qin-sun').fadeIn(1500);
				   $('.qindao-ad').removeClass('wid-bg7 fadeOut qanimate2').addClass("wid-bg8 qanimate2-fadeIn fadeIn");
				  
				}
				else if(intTotalCount==250){
				     clearInterval(intId);
					  $('.wid-rain').empty();
					  $('.qindao-ad-step3').find("[data-animate]").each(function(){
								  $(this).addClass($(this).attr('data-animate'));
					  })
					  $('.qindao-ad-step3').show(0);
					  intTotalCount = 0;
				}
				//
			},25)
	    }
