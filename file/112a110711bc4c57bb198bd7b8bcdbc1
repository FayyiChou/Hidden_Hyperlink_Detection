<script type="text/javascript">
$(function(){
	var $header=$('.wid-header'),
	    sTop = parseInt(0+$header.css('margin-bottom'))+$header.height()/2,
	    imgLogo=$header.find('.wid-header-logo img'),
	    isNanJing = (imgLogo.attr('src').indexOf('nanjing')!=-1);
	if($(window).scrollTop()>=sTop){
		   $header.addClass('wid-hd-scroll');
		   if(!isNanJing){
		      imgLogo.attr('src',imgLogo.attr('src').replace('wid-header-logo','wid-header-logo2'));
		   }
		   $('.csp-change-btn').addClass("wid-scroll-csp");
		   //$header.find('.mod-nav').hide(0);
	}
	$(window).resize(function(){
		$header.find('.fixed-top').width($(document).width())
        if( $("body").hasClass( "o_h_p" ) ){
            $header.find('.fixed-top').width($(document).width()-17)
        }
		/*
			if($(window).width()<1100){
				$header.find('.layout').width($(window).width());
			}
			else{
				$header.find('.layout').width(1100);
			}
		*/
	}).trigger('resize')
	

	$(window).scroll(function(){
		
		if( $( ".container" ).children( "div" ).hasClass( "left_bar" ) && $( ".container" ).children( "div" ).hasClass( "right_bar" ) ){
			menuMainPos();
		}

		var winTop=$(window).scrollTop();
		/*滚动加线*/
		if(winTop>0){
			$(".fixed-top").css('borderBottom','1px solid #D9D9D9');
		}else{
			$(".fixed-top").css('borderBottom','none');
		}
		$header.find('.fixed-top').css("left",0-$(window).scrollLeft()+"px");
	   if($(window).scrollTop()>=sTop&&(!$header.hasClass('wid-hd-scroll')) && '' !='isGroupFlag'){
		   $header.addClass('wid-hd-scroll');
		   //$header.find('.mod-nav').hide(0);
		   if(!isNanJing){
		      imgLogo.attr('src',imgLogo.attr('src').replace('wid-header-logo','wid-header-logo2'));
		   }
		     $('.csp-change-btn').addClass("wid-scroll-csp");
		   $(window).scrollTop($(window).scrollTop()+49);
		   
		    //弹出层高度及定位 miaohu
			if( $( ".modal_cont" ).length > 0 ){
				var mc_height = window.innerHeight - 131; //弹出窗口的最大最小高度
				var t_height = mc_height - 106;//弹出层内"tm_cont"的最大最小高度
				if( window.innerHeight - $( ".fixed-top" ).height() <  680 ){
					$( ".modal_cont" ).css({ "max-height":mc_height , "top":"91px" }); //设置弹出窗口的高度和位置
					$( ".modal_cont .tm_cont" ).css({ "min-height":t_height , "max-height":t_height }); //设置弹出窗口的高度和位置
				} else {
					$( ".modal_cont" ).css({ "max-height":"640px" , "top":"91px" }); //设置弹出窗口的高度和位置
					$( ".modal_cont .tm_cont" ).css({ "min-height":"530px" , "max-height":"530px" }); //设置弹出窗口的高度和位置
				}
			}
			//教师和学生成长档案导航和tab的位置
			$( "#wrapper" ).addClass( "wrapper-fixed-top" );
		    //论文、评优、课题等模块的导航和菜单位置
		    $( ".page-main-content" ).addClass( "page-fixed-top" );

	   }
	   else if($(window).scrollTop()<sTop&&$header.hasClass('wid-hd-scroll') && '' !='isGroupFlag'){
		   $header.removeClass('wid-hd-scroll');
		   //$header.find('.mod-nav').show(0);
		   if(!isNanJing){
		      imgLogo.attr('src',imgLogo.attr('src').replace('wid-header-logo2','wid-header-logo'));
		   }
		   $('.csp-change-btn').removeClass("wid-scroll-csp");
		   $(window).scrollTop($(window).scrollTop()-49);
		   
		    //弹出层高度及定位 miaohu
		    if( $( ".modal_cont" ).length > 0 ){
				var mc_height = window.innerHeight - 180; //弹出窗口的最大最小高度
				var t_height = mc_height - 106;//弹出层内"tm_cont"的最大最小高度
				if( window.innerHeight - $( ".fixed-top" ).height() <  680 ){
					$( ".modal_cont" ).css({ "max-height":mc_height , "top":"140px" }); //设置弹出窗口的高度和位置
					$( ".modal_cont .tm_cont" ).css({ "min-height":t_height , "max-height":t_height }); //设置弹出窗口的高度和位置
				} else {
					$( ".modal_cont" ).css({ "max-height":"640px" , "top":"140px" }); //设置弹出窗口的高度和位置
					$( ".modal_cont .tm_cont" ).css({ "min-height":"530px" , "max-height":"530px" }); //设置弹出窗口的高度和位置
				}
		    }
		    //教师和学生成长档案导航和tab的位置
			$( "#wrapper" ).removeClass( "wrapper-fixed-top" );
		    //论文、评优、课题等模块的导航和菜单位置
		    $( ".page-main-content" ).removeClass( "page-fixed-top" );

	   }
	   
	}).trigger('scroll');
    if(isLogin == "true" && (userIdentity == "ID002" || userIdentity == "ID003") && isTrain == "false" ){
        using(['fancyBox'], function(){
            $("#addUserTeachAttrPage").fancybox({
                'type' : 'ajax',
                'showCloseButton': false,
                'hideOnOverlayClick' : false,
                'titlePosition' : 'inside',
                scrolling:'visible',
                'onCleanup':function(){
                    return false;
                },
                'onStart': function(){
                    //showCloseButton设置为false不起作用
                    //手动移除关闭按钮
                    $('#fancybox-wrap').addClass("fancybox-visible");
                    $("#fancybox-close").remove();
                },
                'onClosed': function(){
                    $('#fancybox-wrap').removeClass("fancybox-visible");
                }
            });
            
            //判断是否添加了教学属性
            if(userTeachAttrFlag == "false"){
                $("#addUserTeachAttrPage").click();
            }
        });
    }
	//判断用户机构是否为空
    if(isLogin == "true" && isTrain =="false"){
        using(['fancyBox'], function(){
            $("#addUserOrgRpPage").fancybox({
                'type' : 'ajax',
                'hideOnOverlayClick' : false,
                'titlePosition' : 'inside',
                scrolling:'visible',
                'onComplete': function(){
   				 //关闭弹出框
   					$('.fancy-cancel-btn').click(function(){
   						$.fancybox.close();
   					})
   				  
   				},
   				'onClosed':function(){
   					$('#fancybox-outer').removeClass('style02');
   					$('#fancybox-wrap').removeClass("fancybox-visible");
   				}
            });
            //所属机构是否为空判断
            if(isHasOrgFlag == "false" && canclApplyOrg == 'false'){
                $("#addUserOrgRpPage").click();
            }
        });
    }
    
	// 华中培训首次登录个人信息确认
	if(isLogin == "true" && isTrain =="true"){
        using(['fancyBox'], function(){
            $("#trainAddUserTeachAttrPage").fancybox({
                'type' : 'ajax',
                'showCloseButton': false,
                'hideOnOverlayClick' : false,
                'titlePosition' : 'inside',
                scrolling:'visible',
                'onCleanup':function(){
                    return false;
                },
                'onStart': function(){
                    //showCloseButton设置为false不起作用
//                    //手动移除关闭按钮
                    $('#fancybox-wrap').addClass("fancybox-visible");
                    $("#fancybox-close").remove();
                },
                'onClosed': function(){
                    $('#fancybox-wrap').removeClass("fancybox-visible");
                }
            });

            //判断是否添加了教学属性
            if(userTeachAttrFlag == "false"){
                $("#trainAddUserTeachAttrPage").click();
            }
        });
    }
})
function checkWorkRoom(){
		/*
		if($('.for-work-group').val() == 'workRoom'){//绑定工作室
			$('.wid-header-nav').find("li").removeClass('current');	
			$('.header-work-room-btn').addClass('current');	
		}else{
			*/
			var typeName=$('.for-work-group').val();
			$.ajax({
				   url:"/agroup/cworkRoom/mcheckWorkRoom.do",
				   type:"post",
				   data:{"typeName":typeName},
				   dataType:"json",
				   success:function(jsonData){
				   		if(jsonData.checkFlag){
					   		$('.wid-header-nav').find("li").removeClass('current');	
							$('.header-work-room-btn').addClass('current');	
						}
					}
				});
		//}
}
</script>
<script type="text/javascript"> 
	    //初始化数据  
	    var seconds = 0;
	    var quartz = "";
	    var loginTime = '';
	   $(document).ready(function() {
	  		//华东师范
	   		var isHDProject = '';
	        //表示已经登录(表示用户登录) 
			if('1' == isHDProject)
			{	
		        if(loginTime  != ""){
		            var now = new Date();  
		            var nowTime = now.getTime();    //同样的当前时间的毫秒数  
		            var onlineTime = nowTime - loginTime;   //得到在线时长的毫秒数  
		              
		            //调用下面的三个函数进行时、分、秒的转换  
		            seconds = MillisecondToSecond(onlineTime);  
		            quartz = setInterval(timing,1000);
		        }
	        } 
	    });  
	      
    //定时任务（动态显示登录时长）当到达10小时我们会弹出alert，然后清除Interval  
    function timing(){
        seconds++;  
        if(seconds%300 == 0){
        	$.ajax({
				type:'post',
				url:'atraining.cappTrainUserGradeController.msaveOrUpdateOnlineTime.do',
				dataType : 'json',
				success: function(result){
		         }
		    });
        }  
    }  
      
     //毫秒转秒  
    function MillisecondToSecond(msd) {  
        var timeSecond = parseFloat(msd) / 1000;  
        if (null != timeSecond && "" != timeSecond) {  
            timeSecond = parseInt((parseFloat((parseFloat(timeSecond / 3600.0) - parseInt(timeSecond / 3600.0)) * 60) -  
                parseInt((parseFloat(timeSecond / 3600.0) - parseInt(timeSecond / 3600.0)) * 60)) * 60);  
        }  
        return timeSecond;  
    }    
	
	
</script> 
