<script type="text/javascript">
$(function(){
	
	$('.post-new-teachactivity').click(function(){
		var _this = $(this);
		var _url = _this.attr('url');
		if(_url == null || _url == '') {
			$('.wid-login-btn').click();
			return;
		}
		window.location.href = _url;
	});
	$.chkrdo=function(){
	       $("input:checkbox,input:radio").not(".ui-transformed").each(function(){
		        var $thi = $(this);
		        var $chcrad = $('<a  href="javascript:void(0)"></a>'),
				    defaultClass = $thi.is(':radio')?"ui-input-r":"ui-input-c",
				    disableClass=$thi.is(':enabled')?"":"ui-disabled",
					selectedClass=$thi.is(':checked')?"ui-select":"",
					name=$thi.attr('name')?$thi.attr('name'):"";
				
					defaultClass+=" "+($thi.attr('class')?$thi.attr('class'):"");
					$chcrad.addClass(defaultClass+" "+disableClass+" "+" "+selectedClass);
					$chcrad.attr('name',name);
					$thi.before($chcrad)
					$thi.addClass('ui-transformed');		
		   }).click(function(e){
					     var $thi=$(this);
						 e.stopPropagation();
						 if($thi.is(':radio')){
						   $('.ui-input-r[name='+$thi.attr('name')+']').removeClass('ui-select');
						 }
	                    if($thi.is(':checked')){
		                  $thi.prev('a').addClass('ui-select');
		                }
		                else{
		                  $thi.prev('a').removeClass('ui-select');
		                }
		 })
	       
    }
    $.chkrdo();
});

function getCommentList(activId){
	$.ajax({
		url:"/acommonapp/chome/mgetbestHotCommentList.do",
		type:"post",
		dataType:"json",
		data: {"activId":activId},
		success:function(data){
			alert(data);
			//$(".selectPeople").html("");
		}
	});	
}

