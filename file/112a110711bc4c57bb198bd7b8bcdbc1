		document.location.href = '/agroup/cgroupHome/msearchGroupIndex.do?periodId=' + periodId;
  });

  $('.goSearchSubject').unbind("click").bind("click", function(){
		var subjectId = $(this).attr("subjectId");
		document.location.href = '/agroup/cgroupHome/msearchGroupIndex.do?subjectId=' + subjectId;
  });

  $('.goSearchRegion').unbind("click").bind("click", function(){
		var regionId = $(this).attr("regionId");
		document.location.href = '/agroup/cgroupHome/msearchGroupIndex.do?regionId=' + regionId;
  });

  $('.goSearchType').unbind("click").bind("click", function(){
		var typeId = $(this).attr("typeId");
		document.location.href = '/agroup/cgroupHome/msearchGroupIndex.do?typeId=' + typeId;
  });
})

