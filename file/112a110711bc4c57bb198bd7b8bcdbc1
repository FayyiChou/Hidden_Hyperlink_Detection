    var userOrgRpFlag = "";
    var userIdentity = "";
    var isHasOrgFlag = "";
    var canclApplyOrg = "";
	var isTrain = "false";

	if(isLogin == "true"){
		var currUrl = window.location.href;//"/WEB-INF/apps/commonapp/jsp/home/index.jsp";
		var jobTitleFlag = "";
		
		if(jobTitleFlag == "true" && currUrl.indexOf("auserapp/csettings/mindex.do") < 0){//职务职称必填
			//判断职务职称是否已经填写
			var jobTitle = "";
