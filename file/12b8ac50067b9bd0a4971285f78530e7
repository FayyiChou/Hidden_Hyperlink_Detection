		var url = "t20170331_3199037"+"_"+(_num-1)+"."+"shtml";
		if(_num<=1||_num==null)
			location.href = "t20170331_3199037"+"."+"shtml";
		else if(_num>countPage)
			alert("本栏目最多"+countPage+"页");
		else
			location.href = url;
	}
