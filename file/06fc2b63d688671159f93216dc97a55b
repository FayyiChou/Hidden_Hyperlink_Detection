    <script type="text/javascript">
    $(document).ready(function() {
        $('.menubtn img').on('click', function(event) {
            if ($('nav').hasClass('active')) {
                $('nav').removeClass('active');
            } else {
                $('nav').addClass('active');
            }


        });
		
		$('.dropdown-toggle').dropdown();
		
        function clk(type, pos, near) {
            $(type).on('click', function(event) {
                if ($(pos).hasClass('on')) {
                    $(pos).removeClass('on');

                } else {
                    $(pos).addClass('on');
                    $(near).removeClass('on');
                }
            });
        }
        clk('.contactbox .online', '.qrcode', '.telinfo');
        clk('.contactbox .tel', '.telinfo', '.qrcode');

        $(window).resize(function(event) {
            if ($(window).width() > 1200) {
                $('._2p3a').width(382);
            } else {
                $('._2p3a').width(300);
            }
        });



    });
    </script>
