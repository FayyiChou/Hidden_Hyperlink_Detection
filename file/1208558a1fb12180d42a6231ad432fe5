    <script>
    $(function () {
        setRenderFullScreen($('#view_banner_35_277067150'));
        var bannerPostion_view_banner_35_277067150 = $('#view_banner_35_277067150').css('position');
        var $banner_view_banner_35_277067150 = $('#view_banner_35_277067150');
        var bannerTop_view_banner_35_277067150 = $banner_view_banner_35_277067150.position().top;
        var bannderBottom_view_banner_35_277067150=$banner_view_banner_35_277067150.css('bottom');
        if (bannerPostion_view_banner_35_277067150 === 'fixed' &&
            bannerTop_view_banner_35_277067150 == 0 &&
            bannderBottom_view_banner_35_277067150 === 'auto') {
            $('#fullScreen_view_banner_35_277067150').css('position', 'fixed');
        } else {
            if (bannerPostion_view_banner_35_277067150 === 'fixed' &&bannerTop_view_banner_35_277067150 !== 0 && bannderBottom_view_banner_35_277067150 === '0px') {
                $('#fullScreen_view_banner_35_277067150').css({'position':'fixed','top':'auto','bottom':'0px'});
            }
        }
    });
</script>


