<script typeof="text/javascript">
    var htmlzindex = $("#view_search_32_277067150").css("z-index");
    var bannerindex = $("#view_search_32_277067150").parents("[id^=view_banner]").css("z-index");
    $('#view_search_32_277067150 .w_search_select').click(function () {
        $(this).children("ul.w_search_select_list").toggle(); if ($(this).children("ul.w_search_select_list").css("display") == "none") { $("#view_search_32_277067150").css("z-index", htmlzindex).parents("[id^=view_banner]").css("z-index", bannerindex); } else { $("#view_search_32_277067150").css("z-index", "9999999").parents("[id^=view_banner]").css("z-index", "9999999"); };
    });
    $('#view_search_32_277067150 .w_search_select').mouseenter(function () {
        $(this).children("ul.w_search_select_list").show(); $("#view_search_32_277067150").css("z-index", "9999999").parents("[id^=view_banner]").css("z-index", "9999999");
    });
    $('#view_search_32_277067150 .w_search_select').mouseleave(function () {
        $(this).children("ul.w_search_select_list").hide(); $("#view_search_32_277067150").css("z-index", htmlzindex).parents("[id^=view_banner]").css("z-index", bannerindex);
    });
    $("#view_search_32_277067150 .w_search_select .w_search_select_list li").click(function () {

        var textvalue = $(this).text();
        var type = $(this).attr("id");
        $(this).parent().siblings(".w_search_select_txt").text(textvalue);
        $("#view_search_32_277067150 #w_search_select_add").val(type);

    });

    $(function () {

        var hoverIcon = "";
        if (hoverIcon != "") {
            var obj = $("#view_search_32_277067150").find(".w_search_btn i");
            obj.unbind("hover");
            obj.hover(function () {
                $(this).html(hoverIcon);
            },
            function () {
                $(this).html("");
            });
        }

        $("#view_search_32_277067150 .w_search_btn").click(function () {

            var keyword = $("#view_search_32_277067150").find(".w_search_input").val();
            var cate = $("#view_search_32_277067150").find("#w_search_select_add").val();
            var catename = $("#view_search_32_277067150").find("span.w_search_select_txt").text();
            if (keyword.replace(/(^\s*)|(\s*$)/g, "") == "") {
                alert("搜索关键字不能为空")
                return;
            }
            if (cate == "")
                cate = 0;
            if ("False".toLocaleLowerCase() != "true")

                if ("False".toLocaleLowerCase() == "false") {
                    window.location.href = "/searchresultlist?keyword=" + keyword + "&searchCategory=" + cate + "|" + catename + "&htmlSmartId=0&isshowsearch=False";
                } else {
                    TurnPageSmartView("277067150", "", "False", "1", "", "", cate + "|" + catename, keyword, "false");
                }

        });
        $("#view_search_32_277067150 .w_search_input").keydown(function (e) {
            if (e.keyCode == 13) {
                $("#view_search_32_277067150 .w_search_btn").click();
            }
        })
        $("#view_search_32_277067150 .yibuFrameContent").removeClass("overflow_hidden");
    });

</script></div>
