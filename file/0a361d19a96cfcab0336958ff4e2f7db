function submitJS(){
         var searchWords = document.getElementById("searchWords").value;
         var pattern = new RegExp("[`~!@#$%^&*()+=|{}':;',\\[\\].<>/?~！@#￥%……&*（）——+|{}【】‘；：”“’。，、？]");
         if(searchWords==""){
            alert("请输入检索词");
            return false;
         }
         else if(pattern.test(searchWords)){  
            alert("请不要在检索词中包含特殊字符");
           return false;     
         }
	var url ="/LDJAPP/search/search_new/rskszx/search_rskszx.jsp?searchWords="+encodeURIComponent(encodeURIComponent(searchWords));
	window.open(url);
}
</script>

