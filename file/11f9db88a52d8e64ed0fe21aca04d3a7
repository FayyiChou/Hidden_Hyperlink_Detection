<script type="text/javascript">

$(function(){
	// 正数为正确的提示   负数 为错误
	var err = {
		 '1':'活动提交成功！','-1':'活动提交出错！','-2':'您的3YX账号不能为空！'};
	var errCode = $.trim("");
  if(errCode != null && errCode.length > 0){
        layer.msg(err[errCode]);
      }
});

function submitForm() {
	
	if(!CheckUtil.isNotNull($('#userAccount').val())) {
		layer.msg("您的3YX账号不能为空！");
		return false;
	}
	
	$('#dlUserActivity').submit();
	$('#sug_fbacksubmit').attr("disabled","disabled");
}

</script>



