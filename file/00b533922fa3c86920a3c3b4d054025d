	$(".side ul li").hover(function(){
		$(this).find(".sidebox").stop().animate({"width":"150px"},100).css({"opacity":"1","filter":"Alpha(opacity=100)","background":"#1fb5c7"})	
	},function(){
		$(this).find(".sidebox").stop().animate({"width":"50px"},100).css({"opacity":"0.8","filter":"Alpha(opacity=80)","background":"#000"})	
	});
	
});

