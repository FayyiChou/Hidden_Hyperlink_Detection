<!--//--><![CDATA[//><!--
jQuery(document).ready(function(){
					jQuery("#slider").nivoSlider({
						effect: "random", // Specify sets like: "fold,fade,sliceDown"
						slices: 15, // For slice animations
						boxCols: 8, // For box animations
						boxRows: 4, // For box animations
						animSpeed: 500, // Slide transition speed
						pauseTime: 5000, // How long each slide will show
						startSlide: 0, // Set starting Slide (0 index)
						directionNav: true, // Next & Prev navigation
						controlNav: false, // 1,2,3... navigation
						controlNavThumbs: false, // Use thumbnails for Control Nav
						pauseOnHover: true, // Stop animation while hovering
						manualAdvance: false, // Force manual transitions
						prevText: "<i class=\"icon-angle-left\"></i>", // Prev directionNav text
						nextText: "<i class=\"icon-angle-right\"></i>", // Next directionNav text
						randomStart: false, // Start on a random slide
						beforeChange: function(){ jQuery(".nivo-caption").fadeOut("slow"); }, // Triggers before a slide transition
						afterChange: function(){ maxxNivoTweak(); }, // Triggers after a slide transition
						slideshowEnd: function(){}, // Triggers after all slides have been shown
						lastSlide: function(){}, // Triggers when last slide is shown
						afterLoad: function(){ jQuery(".nivo-caption").attr("id","slide-1"); maxxNivoCaptionPosition(); } // Triggers when slider has loaded
					});

                    function maxxNivoTweak(){
                        jQuery(".nivo-caption").fadeIn("slow");
                        var captionQty = jQuery(".nivo-html-caption");
                        var captionId = jQuery(".nivo-caption").attr("id");

                        // Add ID to caption so we can colour the text per slide
                        if(captionId != "undefined"){
                            var currentId = parseInt(jQuery(".nivo-caption").attr("id").replace("slide-",""));
                            if(currentId < captionQty.length){
                                jQuery(".nivo-caption").attr("id","slide-"+(currentId+1));
                            } else {
                                jQuery(".nivo-caption").attr("id","slide-1");
                            }
                        }

                        // Change margin of nivo-caption depending on space on the left of the screen
                        maxxNivoCaptionPosition();

                    } // end function

                    function maxxNivoCaptionPosition(){
                        var pageWidth = document.body.clientWidth;
                        if(pageWidth > 550){
                            var headerCon = jQuery("header .container").width();
                            var nivoLeft = (pageWidth-headerCon)/2;
                            jQuery(".container-caption").css("marginLeft",nivoLeft+"px");
                        }
                    } // end function

                    jQuery(window).resize(function(){
                        maxxNivoCaptionPosition();
                    });

				});
//--><!]]>
</script>
