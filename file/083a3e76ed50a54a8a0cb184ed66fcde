	<script language="JavaScript" type="text/javascript">
function std_playUrl(obj)
{
if(obj==null)
{
  var ok=window.clipboardData.setData("Text",document.title+"\n"+document.location.toString());
  if(ok==true)alert("當前網址已被復制！"); 
  }
else
{
  var url=window.clipboardData.getData("Text");
  if(url==null){alert("剪切板文本內容為空！");return false;}
  var re1 = /^((http|https|ftp|rtsp|mms|pmn):(\/\/|\\\\)[A-Za-z0-9\./=\?%\-&_~`@\]\':+!]+)/ig;
  var re2 = /([a-zA-Z0-9_][.](\w)+[.]{1,}(net|com|cn|org|cc)(((\/[\~]*|\\[\~]*)(\w)+)|[.](\w)+)*(((([?](\w)+){1}[=]*))*((\w)+){1}([\&](\w)+[\=](\w)+)*)*)/ig;
  var t1=re1.test(url),t2=re2.test(url)
  if(!t1&&!t2){alert("剪切板內容不是網址，請檢查!");return false;}
  if(confirm("確認要打開以下網址？\n\n"+url)){obj.href=(t1?"":"http://")+url;obj.target="_blank";return true}
  return false;
  }
}
function geturl(){
var dizhi=document.location.toString();
var lastxwbtzhi=document.title+"-南海網-www.hinews.cn";
window.external.AddFavorite(dizhi, lastxwbtzhi);
}
