    function resetUrlByLink() {
        var links = document.getElementsByTagName("a");
        for (var i = 0; i < links.length; i++) {
            var link = links[i];
            if (link.href.indexOf("http://radiol.dxy.cn") != 0 && link.href.indexOf("https://auth.dxy.cn") == -1) {
                link.target = "_blank";
            }
        }
    }




</script>

