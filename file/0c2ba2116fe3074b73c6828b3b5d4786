<script type="text/javascript"> 
$(function () {
   var form = $('form.J_ajaxForms');
        if ($.support.msie) {
            form.find('[placeholder]').each(function () {
                var input = $(this);
                if (input.val() == input.attr('placeholder')) {
                    input.val('');
                }
            });
        }
        form.validate({
			errorElement:"span",
			onclick: false,
			errorPlacement: function(error, element) {
				 //error.appendTo(element.parent());  
			},
            rules: {'name':{required:1},'shouji':{required:1},'email':{required:1,email: true},'introduce':{required:1},'validate':{required:1}},      
            messages: {'name':{required:'Please enter your name!'},
					  'shouji':{required:'Please enter telephone number!'},
					  'email':{required:'Please enter email address!',email: "Please enter a valid email address!"},					 
					  'introduce':{required:'Please enter your message!'},
					  'validate':{required:'Please enter verification code!'}
					  },
			highlight:function (element, errorClass) {
				$(element).parents("li").addClass('error');
            },
            unhighlight: function (element, errorClass) {
                $(element).parents("li").removeClass('error');
            },
            onfocusout: function(element){
			   this.element(element);
			},
            submitHandler: function (forms) {
				 $(forms).ajaxSubmit({
                    url: form.attr('action'), //按钮上是否自定义提交地址(多按钮情况)
                    dataType: 'json',
                    beforeSubmit: function (arr, $form, options) {
					},
                    success: function (data, statusText, xhr, $form) {	
						var mesg = new Array();
                        if(data.status){
							mesg.msg   = 'Thank you for your message, we will contact you as soon as possible!';							
							mesg.time = 3000;						
							mesg.callback = "http://www.ounuohk.net/index.php";	
						    resultTip(mesg);  
						}else{
							mesg.error = 'warning';
							mesg.msg   = data.info;
							resultTip(mesg);	
						}
                    }
                });
			}
        });
});
</script> 
