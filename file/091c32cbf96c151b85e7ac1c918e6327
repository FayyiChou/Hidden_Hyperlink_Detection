  <script>
$(document).ready(function(){
	// 菜单导航
	$("#menubox li").hover(
	  	function(){
			$(this).addClass("shownav");
	  	},
	  	function(){
			$(this).removeClass("shownav");
	  	}
	);
	
	// 关闭背景
