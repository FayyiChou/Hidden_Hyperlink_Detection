                            if (getCookie(getCookie("UserID") + "_zk2016SafeCar") == null || getCookie(getCookie("UserID") + "_zk2016SafeCar") == "") {
                                //调用《非实验区完成状态接口》
                                $.getJSON(Asmx + "/GetFinishStatus?jsoncallback=?", { r: Math.random() }, function (fres) {
                                    if (fres.Status == "True") {
                                        if (fres.StepOne == "True") {
                                            //已完成  设置cookies 更改完成状态相关样式
                                            var zkcookName = getCookie("UserID") + "_zk2016SafeCar";
                                            var cookName = getCookie("UserID") + "_2016SafeCar";
                                            setCookie(zkcookName, 1, 365);
                                            setCookie(cookName, 1, 365);
                                            finishStatus(1);
                                        } else {
                                            finishStatus(0);
                                            if (getCookie("clickone") == 2) {
                                                delCookie("clickone");
                                                tijiao(0);
                                            }
                                        }
                                    }
                                    else {
                                        finishStatus(0);
                                        if (getCookie("clickone") == 2) {
                                            delCookie("clickone");
                                            tijiao(0);
                                        }
                                    }
                                });
                            } else {//设置cookies
                                var zkcookName = getCookie("UserID") + "_zk2016SafeCar";
                                setCookie(zkcookName, 1, 365);
                                finishStatus(1);
                            }
                        }
                        //非实验区 非教师、校级权限 样式 
                        else {
                            if (getCookie(getCookie("UserID") + "_zk2016SafeCar") != null && getCookie(getCookie("UserID") + "_zk2016SafeCar") != "") {
                                $("#work2").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png'>");
                            } else {
                                finishStatus(0);
                            }
                        }
                    }
                    else //实验区
                    {
                        isshiyanqu = 1;
                        if (region == 1 || region == 7) {
                            if (getCookie(getCookie("UserID") + "_2016SafeCar") == null || getCookie(getCookie("UserID") + "_2016SafeCar") == "") {
                                GetFinishStatus(function (res) {
                                    if (res.Status == "True") {
                                        if (res.StepOne == "True") {
                                            //已完成  设置cookies 更改完成状态相关样式
                                            var cookName = getCookie("UserID") + "_2016SafeCar";
                                            setCookie(cookName, 1, 365);
                                            finishStatus(1);
                                        } else {
                                            finishStatus(0);
                                            if (getCookie("clickone") == 2) {
                                                delCookie("clickone");
                                                tijiao(0);
                                            }
                                        }
                                    }
                                    else {
                                        finishStatus(0);
                                        if (getCookie("clickone") == 2) {
                                            delCookie("clickone");
                                            tijiao(0);
                                        }
                                    }
                                });
                            } else {
                                finishStatus(1)
                            }
                        }
                        //实验区 非教师、校级权限 样式 
                        else {
                            //获取完成情况  取cookies
                            GetFinishStatus(function (res) {
                                if (res != null) {
                                    if (res.StepOne == "True") {
                                        //已完成  设置cookies 更改完成状态相关样式
                                        var cookName = getCookie("UserID") + "_2016SafeCar";
                                        setCookie(cookName, 1, 365);
                                        $("#work2").html("<img src='http://file.safetree.com.cn/special/2016FirstClass/Styles/images/2016FirstClass/nav_wc.png' />");
                                    }
                                }
                            });
                        }
                    }
                } else {
                    finishStatus(0);
                }
                weixin(comefrom, function (a, b, c) {
                    comefrom = a, cityname = b, ipArea = c;
                    $("#weixin_cityname").append("教育百师通" + cityname);
                    if (comefrom >= 0) {
                        $("#wxbox-img1").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                        $("#wxbox-img2").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();");
                        $("#wxbox-img4").attr("src", 'http://file.safetree.com.cn/angel/push/pushweixin.html?location=100007&siteid=' + comefrom + '&test=1&css=' + comefromcss).attr("onload", "javascript:getTitle();")
                    }
                });
            });

            //点击完成情况按钮按钮
            $("#FinishSituation").click(function () {
                if (region > 0) {
                    window.location.href = "/2016SchoolBusSafety_Execution_School.html";
                } else {
                    window.location.href = "/2016SchoolBusSafety_Execution_family.html";
                }
            });

        });

        //签名
        //isFinish  =>0,未完成    1,已完成
        function tijiao(isFinish) {
            //判断用户是否登录 isLogin=>0:未登录;1:已登入
            if (isLogin == 0) {
                //弹登录框 提示登录
                special.showLogin(3)
            }
            else {
                // 校级、教师
                if (region == 1 || region == 7) {
                    //获取完成状态 
                    //已完成
                    if (isFinish == 1) {
                        if (getCookie(getCookie("UserID") + "_zk2016SafeCar") == null) {//主库未读入								  
                            $.getJSON(Asmx + "/SpecialSign?jsoncallback=?", { r: Math.random(), Prv: prvCode, City: cityId, County: countryId, School: schoolid, Grade: grade, Class: classRoom, UserType: region, Comefrom: comefrom }, function (res) {
                                if (res != null && res.Status == "True") {
                                    //设置Cookie  
                                    var zkcookName = getCookie("UserID") + "_zk2016SafeCar";
                                    setCookie(zkcookName, 1, 365);
                                    finishStatus(1);
                                }
                                else if (res.Msg.length > 0) {
                                    alert(res.Msg);
                                    return false;
                                }

                            });
                        } //已读入
                        else {
                            finishStatus(1);
                        }
                    }
                    //未完成
                    else {
                        //专题没结束
                        if (qgIsEnd == 0) {
                            $("#w_finish").hide();
                            $("#w_finishing").show();
                            //实验区
                            if (isshiyanqu == 1) {
                                //平台写入数据
                                var time = new Date();
                                var str = time.getHours();
                                SpecialSign(function (res2) {
                                    //写入成功  设置cookies
                                    if (res2 != null) {
                                        if (res2.Status == "True") {
                                            var cookName = getCookie("UserID") + "_2016SafeCar";
                                            setCookie(cookName, 1, 365);
                                            //调用主库
                                            $.getJSON(Asmx + "/SpecialSign?jsoncallback=?", { r: Math.random(), Prv: prvCode, City: cityId, County: countryId, School: schoolid, Grade: grade, Class: classRoom, UserType: region, Comefrom: comefrom }, function (res3) {
                                                if (res3 != null) {
                                                    if (res3.Status == "True") {
                                                        //设cookies 
                                                        var zkcookName = getCookie("UserID") + "_zk2016SafeCar";
                                                        setCookie(zkcookName, 1, 365);
                                                        finishStatus(1);

                                                    } else if (res3.Msg.length > 0) {
                                                        alert(res3.Msg);
                                                        finishStatus(0);
                                                    }
                                                }
                                            });
                                        }
                                        else if (res2.Msg.length > 0) {
                                            alert(res2.Msg);
                                            finishStatus(0);
                                        }
                                    }
                                    else {
                                        finishStatus(0);
                                    }
                                });
                            }
                            //非实验区
                            else {
                                $("#w_finish").hide();
                                $("#w_finishing").show();
                                $.getJSON(Asmx + "/SpecialSign?jsoncallback=?", { r: Math.random(), Prv: prvCode, City: cityId, County: countryId, School: schoolid, Grade: grade, Class: classRoom, UserType: region, Comefrom: comefrom }, function (res4) {
                                    if (res4 != null && res4.Status == "True") {
                                        //设置Cookie 
                                        var zkcookName = getCookie("UserID") + "_zk2016SafeCar";
                                        var cookName = getCookie("UserID") + "_2016SafeCar";
                                        setCookie(zkcookName, 1, 365);
                                        setCookie(cookName, 1, 365);
                                        finishStatus(1);
                                    }
                                    else if (res4.Msg.length > 0) {
                                        alert(res4.Msg);
                                        finishStatus(0);
                                    }
                                });
                            }
                        }
                        //专题已结束
                        else {
                            alert("专题已结束，谢谢参与！");
                        }
                    }
                }
                //非教师、校级
                else {
                    //显示无权限
                    alert("无法进行操作，您没有此权限");
                }
            }
        }

        function finishStatus(qmStatus) {//0:未完成, -1：完成中, 1:已完成
            if (qmStatus == 0) {
                $("#w_finish").show();
                $("#w_finishing").hide();
                $("#finished").hide();
                $("#step1").show();
            }
            else if (qmStatus == -1) {
                $("#w_finish").hide();
                $("#w_finishing").show();
                $("#finished").hide();
                $("#step1").hide();
            } else {
                $("#w_finishing").hide();
                $("#w_finish").hide();
                $("#finished").show();
                $("#step1").show();
                $("#step1").attr("class", "y-wc");
                $("#work1").html("<img src='http://file.safetree.com.cn/special/2016Drowning/Styles/images/2016Drowning/nav_wc.png' />");
            }
        }

