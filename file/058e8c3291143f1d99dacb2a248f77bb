                    ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
                    android: u.indexOf('Android') > -1 //android终端或者uc浏览器                   
                };
            } ()
        }

        if (browser.versions.ios == true || browser.versions.android == true) {
            location.href="http://www.wed0570.com/mobile/";
            return;
