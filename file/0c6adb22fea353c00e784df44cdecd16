<script type="text/javascript">
$(function(){

//回车绑定
document.onkeydown = function(evt){
   //回车的时候先判断当前是否有jbox处于打开状态，如果有，则关闭此jbox，如果没有，则提交表单数据
   var evt= window.event?window.event:evt;
   if(evt.keyCode==13) {
   var act = document.activeElement.id;
   if(typeof(act)=='undefined' || act == '' ){
		return;
   }
      var pop2 =  $(".pop2");
      if ($(".pop2").is(":hidden") && $(".pop").is(":hidden")) {
         var $loginCont1=$("#loginCont1");
         var $loginCont2=$("#loginCont2");
         if($loginCont1.length){
            var zhi1=$loginCont1.css("display");
            if(zhi1=="block"){
	           $("#nameTipLi").hide();
               mysubmit();
            }
         }
         if($loginCont2.length){
            var zhi2=$loginCont2.css("display");
            if(zhi2=="block"){
               smysubmit();
            }
         }
      } else {
         if($(".pop2").is(":visible")){
            $("#pop2CloseButton").click();
         }
         if($(".pop").is(":visible")){
            $("a[class='zc_affirm']").click();
         }
      }
   }
}

   var myurl=$.fn.getUrl();
   if(myurl!=null&&myurl!=''){
      var obj = $.fn.getObj(myurl);
      if(obj!=null){
         var msg= eval("obj.msg");
         var msgInfo;
         var studyUrl= eval("obj.studyUrl");
         var loginType = eval("obj.secondLogin");
         if (loginType != null && loginType != "") {
            $("#mylogin").find("h2").last().click();
         }
         if(msg=='orderIdEmpty'){
               msgInfo = '报名序号为空!';
               DABox.info(msgInfo, '温馨提示');
            }
        if(msg=='cannotFindPerson'){
               msgInfo = '未找到从业人员!';
               DABox.info(msgInfo, '温馨提示');
            }
            else if(msg=='notSign'){
               msgInfo = '未进行报名!';
               DABox.info(msgInfo, '温馨提示');
            }
            else if(msg=='havePassAndRegister'){
               DABox.info('已完成了此次培训，并已登记!', '温馨提示');
            }
            else if(msg=='accountAndPwdError'){
               DABox.info('网校账户或密码错误!', '温馨提示');
            }
            else if (msg=='signAndTrainNotMatch'){
               DABox.info('报名网校与培训网校不一致!', '温馨提示');
            }
	    else if (msg=='noPay'){
               DABox.info('报名人员未进行缴费!', '温馨提示');
            }
	    else if (msg=='trainTimeOverdue'){
               DABox.info('培训截止时间已过，此次培训失效!', '温馨提示');
            }
	    else if (msg=='otherError'){
               DABox.info('其他错误!', '温馨提示');
            }
            else if (msg=='resultTypeError'){
               DABox.info('接口返回类型错误!', '温馨提示');
            }   
	    else if (msg=='callInterfaceError'){
               DABox.info('接口调用失败!', '温馨提示');
            }  
         if(msg!=null&&msg!=''){
            if(msg=='loginByLoginNameAndPwdError'){
               msgInfo = '登录名或者密码错误!';
               DABox.info(msgInfo, '温馨提示');
            }
            if(msg=='loginByIdCardAndRealNameError'){
               msgInfo = '身份证号或者姓名错误!';
               DABox.info(msgInfo, '温馨提示');
            }
            if(msg=='loginByAccountNoAndRealNameError'){
               msgInfo = '会计证号或者姓名错误!';
               DABox.info(msgInfo, '温馨提示');
            }
            else if(msg=='loginByCardAndPassWordError'){
               msgInfo = '学习卡无效或者卡号密码错误!';
               DABox.info(msgInfo, '温馨提示');
            }
            else if(msg=='loginAreaError'){
               DABox.info('地区选择错误,请重新选择地区!', '温馨提示');
            }
            else if(msg=='loginByCardAndPassWordErrorNoMatch'){
               DABox.info('地区选择错误,请核对卡所属地区!', '温馨提示');
            }
            else if (msg=='loginByIdCardError'){
               DABox.info('请核实身份证号是否正确!', '温馨提示');
            }
            else if (msg=='loginByIdCardOrRegisterRealNameError'){
               DABox.info('姓名输入错误!', '温馨提示');
            }
            else if (msg=='loginByIdCardErrorJL'){
               var temp="请核实身份证号是否输入正确以及是否已购课，如未购课则无法登录，请点击下方购买。<br><a href='http://www.jlkj.gov.cn/web/showIndexWebContents.action'>购买年度课程</a>";
               rbcAlert(temp);
            }
            else if (msg=='loginByIdCardOldPlatError'&&studyUrl!=null&&studyUrl!=''){
               var temp="您已在另一平台学习，请点击进入继续学习！<br><a href='"+studyUrl+"'>进入学习中心</a>";
               rbcAlert(temp);
            }
            else if (msg=='loginByIdCard2014BeforeError'&&studyUrl!=null&&studyUrl!=''){
               var temp="您尚未学习完成2014年之前的课程，请点击进入继续学习！<br><a href='"+studyUrl+"'>进入学习中心</a>";
               rbcAlert(temp);
            }
            else if (msg=='loginByIdCardErrorNo2014'&&studyUrl!=null&&studyUrl!=''){
               var temp="您已在另一平台缴费学习2014年度之前的课程，请点击进入继续学习!<br>如需学习2014年度课程，请点击下方购买.<br><a href='"+studyUrl+"'>进入学习中心</a>&nbsp;&nbsp;<a href='http://www.jlkj.gov.cn/web/showIndexWebContents.action'>购买2014年度课程</a>"
               rbcAlert(temp);
            }
            else if (msg=='loginByIdCardError2014Studied'){
               var temp="您已在另一平台学习，请点击进入继续学习！<br><a href='"+studyUrl+"'>进入学习中心</a>";
               rbcAlert(temp);
            }
            else{
               //alert(msg);
               //$("#formerror").show();
               if($("#formerror").length){
                  $("#formerror").html('<span></span>'+msgInfo);
                  $("#formerror").show();
               }else{
                  DABox.info(msgInfo, '温馨提示');
               }
            }
         }
      }
   }
   var rndzz=Math.uuid();//UUID
   //主登录方式
   var $captchaImage = $("#captchaImage");
   // 更换验证码
   if ($captchaImage.length) {
      $captchaImage.click(function() {
         $captchaImage.attr("src", "http://study.dongao.com/study/register/userRegister/verifyCode/captchaId="+rndzz+"&timestamp=" + (new Date()).valueOf());
      });
      $captchaImage.attr("src","http://study.dongao.com/study/register/userRegister/verifyCode/captchaId="+rndzz);
   }
   //次登录方式
   var $scaptchaImage = $("#scaptchaImage");
   if ($scaptchaImage.length) {
      $scaptchaImage.click(function() {
         $scaptchaImage.attr("src", "http://study.dongao.com/study/register/userRegister/verifyCode/captchaId="+rndzz+"&timestamp=" + (new Date()).valueOf());
      });
      $scaptchaImage.attr("src","http://study.dongao.com/study/register/userRegister/verifyCode/captchaId="+rndzz);
   }
   if ($("#captchaId").length) { 
      $("#captchaId").val(rndzz);
   }
   if ($("#scaptchaId").length) {
      $("#scaptchaId").val(rndzz);
   }
   var cookValue=jQuery.jCookie({"name":'partnerId',"domain":'dongao.com',"path":'/'});
   if(cookValue!=false){
      jQuery.jCookie({"name":'partnerId',"domain":'dongao.com',"path":"/","value":null});
   }
   jQuery.jCookie({"name":'partnerId',"domain":'dongao.com',"path":'/',"value":'139'});
   
   //合作周期id
   var cookValue2=jQuery.jCookie({"name":'periodId',"domain":'dongao.com',"path":'/'});
   if(cookValue2!=false){
      jQuery.jCookie({"name":'periodId',"domain":'dongao.com',"path":"/","value":null});
   }
   jQuery.jCookie({"name":'periodId',"domain":'dongao.com',"path":'/',"value":'140'});
   
   var homeUrl=jQuery.jCookie({"name":'homeUrl',"domain":'dongao.com',"path":'/'});
   if(homeUrl!=false){
      jQuery.jCookie({"name":'homeUrl',"domain":'dongao.com',"path":"/","value":null});
   }
   jQuery.jCookie({"name":'homeUrl',"domain":'dongao.com',"path":'/',"value":'http://shiyan.dongao.com'});

   $.ajax({
      type: "GET",
      cache: false,
      url: "http://study.dongao.com/study/index/validateVisitEffective?jsoncallback=abc",
      dataType: "jsonp",
      async : false,
      success : function(data) {         
         if(data == false){
            window.location.href = "http://study.dongao.com/study/index/notOpenGuide"; 
         }    
      }
   });
   

   window.mysubmit=function(){
      var captchaId= $("#captchaId").val();
      var captcha=$("#captcha").val();
      if(captcha==null||captcha==""||captcha=="验证码"){
         DABox.info('验证码不能为空！', '温馨提示', {closed:function(){
            var captchaDom = $("#captcha");
            captchaDom.focus();
         }});
         return false;
      }
      if($("#username").length){
         var username= $.trim($("#username").val());
         var usernameDom = $("#username");
         if(username==null||username==""||username=="帐号"){
            DABox.info('用户名不能为空！', '温馨提示', {closed:function(){
               usernameDom.focus();
            }});
            return false;
         } else {
            usernameDom.val(username);;
         }
      }
      if($("#card").length){
         var card = $.trim($("#card").val());
         var cardDom = $("#card");
         if(card==null||card==""||card=="卡号"){
            DABox.info('卡号不能为空！', '温馨提示', {closed:function(){
               cardDom.focus();
            }});
            return false;
         } else {
            cardDom.val(card);
         }
      }
      if($("#password").length){
         var password=$("#password").val();
         if(password==null||password==""){
            DABox.info('密码不能为空！', '温馨提示', {closed:function(){
               var passwordDom = $("#password");
               passwordDom.focus();
            }});
            return false;
         }
      }
      if($("#idcard").length){
         var idcard = $.trim($("#idcard").val());
         var idcardDom = $("#idcard");
         if(idcard==null||idcard==""||idcard=="身份证"){
            DABox.info('身份证号不能为空！', '温馨提示', {closed:function(){
               idcardDom.focus();
            }});
            return false;
         } else {
            idcardDom.val(idcard);
         }
      }
      if($("#realname").length){
         var realname = $.trim($("#realname").val());
         var realnameDom = $("#realname");
         if(realname==null||realname==""||realname=="姓名"){
            DABox.info('姓名不能为空！', '温馨提示', {closed:function(){
               realnameDom.focus();
            }});
            return false;
         } else {
            realnameDom.val(realname);
         }
      }
      
      $.ajax({
         type:"get",
         async: false,
         url:"http://study.dongao.com/study/login/testcode?callback=?",
         data:{"captchaId":captchaId,"captcha":captcha},
         dataType:"jsonp",
         success:function(data){
            if(data == false){
               DABox.info('验证码错误，请重新输入！', '温馨提示', {closed:function(){
                  var captchaDom = $("#captcha");
                  captchaDom.focus();
               }});
               var $captchaImage = $("#captchaImage");
               if ($captchaImage.length) {
                  $captchaImage.trigger("click");
               }
               return false;
            }else{
               $.ajax({
                     type: "GET",
                     cache: false,
                     url: "http://study.dongao.com/study/login/validateTimeEffective?jsoncallback=abc",
                     dataType: "jsonp",
                     async : false,
                   success : function(data) {
                     if (data == 1) {
                        $.ajax({
	                        type: "GET",
		                    cache: false,
		                    url: "http://study.dongao.com/study/login/loginByIdCardAndRealNameOrRegisterCheck?idcard="+$("#idcard").val(),
		                    dataType: "jsonp",
		                    async : false,
		                    success : function(data) {
		                        if(data == 1){
			                       DABox.info('身份证号不能为空！', '温馨提示', {closed:function(){
						               idcardDom.focus();
						           }});
						           return false;
		                        }else if(data == 2){
		                            $("#idCardTipSpan").html($("#idcard").val());
		                            $("a[class='zc_affirm']").click(function(){
		                                $('.pop').fadeOut();
									    $('.mask').animate({opacity:0},function(){
									        $('.mask').remove();
									    });
									    if('undefined' == typeof(document.body.style.maxHeight)){
									        $('.ie6_iframe').remove();
									    }
										$("#longinform").submit();
		                            });
		                            dialogMove('.pop h3','.pop');
		                        
		                        }else if(data == 3){
		                        	$("#longinform").submit();
		                        }
		                    }
                        });
                        
                     } else if (data == 2) {
                        DABox.info('当前合作周期尚未开始，请耐心等待！', '温馨提示');
                        return false;
                     } else if (data == 3) {
                        DABox.info('当前合作周期已经结束!', '温馨提示');
                        return false;
                     }            
                   }
                 });
            }
         }
      });

   }
   window.smysubmit=function(){
      var captchaId= $("#scaptchaId").val();
      var captcha=$("#scaptcha").val();
      if(captcha==null||captcha==""||captcha=="验证码"){
         DABox.info('验证码不能为空！', '温馨提示', {closed:function(){
            var scaptchaDom = $("#scaptcha");
            scaptchaDom.focus();
         }});
         return false;
      }
      if($("#susername").length){
         var susername = $.trim($("#susername").val());
         var susernameDom = $("#susername");
         if(susername==null||susername==""){
            DABox.info('用户名不能为空！', '温馨提示', {closed:function(){
               susernameDom.focus();
            }});
            return false;
         } else {
            susernameDom.val(susername);
         }
      }
      if($("#scard").length){
         var scard = $.trim($("#scard").val());
         var scardDom = $("#scard");
         if(scard==null||scard==""||scard=="卡号"){
            DABox.info('卡号不能为空！', '温馨提示', {closed:function(){
               scardDom.focus();
            }});
            return false;
         } else {
            scardDom.val(scard);
         }
      }
      if($("#spassword").length){
         var spassword=$("#spassword").val();
         if(spassword==null||spassword==""){
            DABox.info('密码不能为空！', '温馨提示', {closed:function(){
               var spasswordDom = $("#spassword");
               spasswordDom.focus();
            }});
            return false;
         }
      }
      if($("#sidcard").length){
         var sidcard = $.trim($("#sidcard").val());
         var sidcardDom = $("#sidcard");
         if(sidcard==null||sidcard==""||sidcard=="身份证"){
            DABox.info('身份证号不能为空！', '温馨提示', {closed:function(){
               sidcardDom.focus();
            }});
            return false;
         } else {
            sidcardDom.val(sidcard);
         }
      }
      if($("#srealname").length){
         var srealname = $.trim($("#srealname").val());
         var srealnameDom = $("#srealname");
         if(srealname==null||srealname==""||srealname=="姓名"){
            DABox.info('姓名不能为空！', '温馨提示', {closed:function(){
               srealnameDom.focus();
            }});
            return false;
         } else {
            srealnameDom.val(srealname);
         }
      }
      
      $.ajax({
         type:"get",
         async: false,
         url:"http://study.dongao.com/study/login/testcode?callback=?",
         data:{"captchaId":captchaId,"captcha":captcha},
         dataType:"json",
         success:function(data){
            if(data == false){
               DABox.info('验证码错误，请重新输入！', '温馨提示', {closed:function(){
                  var captchaDom = $("#scaptcha");
                  captchaDom.focus();
               }});
               var $scaptchaImage = $("#scaptchaImage");
               if ($scaptchaImage.length) {
                  $scaptchaImage.trigger("click");
               }
               return false;
            }else{
               $.ajax({
                     type: "GET",
                     cache: false,
                     url: "http://study.dongao.com/study/login/validateTimeEffective?jsoncallback=abc",
                     dataType: "jsonp",
                     async : false,
                   success : function(data) {
                     if (data == 1) {
                            $("#slonginform").submit();
                        
                     } else if (data == 2) {
                        DABox.info('当前合作周期尚未开始，请耐心等待！', '温馨提示');
                        return false;
                     } else if (data == 3) {
                        DABox.info('当前合作周期已经结束!', '温馨提示');
                        return false;
                     }            
                   }
                 });
            }
         }
      });

   }

   //判断是否登录
   $.ajax({
         type:"post",
         async: false,
         url:"http://study.dongao.com/study/u/validateLogin?callback=?",
         data:{},
         dataType:"jsonp",
         success:function(data){
            if(data!=false){//已登录
               if($("#mylogin").length){
                  $("#mylogin").hide();
               }
               var years=data.years;
               var username=data.username;
               var areaname=decodeURIComponent(data.areaname);
			   var userId = data.userId;
			   var custmoerH=$("#customer_kf").html();
			   var customerTp=custmoerH.replace(/\[0\]/g, username).replace(/\[1\]/g, userId);
			   $("#customer_kf").html(customerTp);
               if(years!=""){
                  var hl=$("#loginAndYear").html();
                  var tp=hl.replace(/\[0\]/g, username).replace(/\[2\]/g, years).replace(/\[1\]/g, areaname);
                  $("#loginAndYear").html(tp);                  
                  $("#loginAndYear").show();
                  $(".yindao span").click(function(){
                     $(".yindao").remove();
                  })
                  if($("#loginAndYear dd:eq(0)").height()>25){
                     $(".yindao").css("bottom","-23px")
                  }
               }else{
                  var hl=$("#loginNotYear").html();
                  var tp=hl.replace(/\[0\]/g, username).replace(/\[1\]/g, areaname);
                  $("#loginNotYear").html(tp);
                  $("#loginNotYear").show();
                  $(".yindao span").click(function(){
                     $(".yindao").remove();
                  })
                  if($("#loginNotYear dd:eq(0)").height()>25){
                     $(".yindao").css("bottom","-23px")
                  }
               }
            }else{//未登录 default
               
            }
         }
      });
});


function validateIdCard(type) {
   if (type == "first") {
      //第一种登录方式 
      var idcard=$("#idcard").val();
      if(idcard==null||idcard==""||idcard=="身份证"){
         DABox.info('身份证号不能为空！', '温馨提示', {closed:function(){
            var idcardDom = $("#idcard");
            idcardDom.focus();
         }});
         return false;
      } else {
         if(isCardID(idcard) == false) {  
            DABox.info('身份证号不合法！', '温馨提示', {closed:function(){
            var idcardDom = $("#idcard");
            idcardDom.focus();
            }});
            return false;  
         }
         $("#nameTipLi").hide();
         window.mysubmit(); 
      }
   } else if (type == "second") {
      //第二种登录方式
      var sidcard=$("#sidcard").val();
      if(sidcard==null||sidcard==""||sidcard=="身份证"){
         DABox.info('身份证号不能为空！', '温馨提示', {closed:function(){
            var sidcardDom = $("#sidcard");
            sidcardDom.focus();
         }});
         return false;
      } else { 
         if(isCardID(sidcard) == false) {   
            DABox.info('身份证号不合法！', '温馨提示', {closed:function(){
            var sidcardDom = $("#sidcard");
            sidcardDom.focus();
         }});
            return false;  
         }  
         window.smysubmit();  
      }
   }
}

function displaySchedule(type, obj) {
   var categoryNumber = $("#categoryNumber").val();

   if (type == "year") {
      var yearLiDoms = $("li[name='yearLi']");
      var currentYearIndex = $(obj).attr("indexValue");
      $.each(yearLiDoms, function(index, data){
         if (index == currentYearIndex) {
            $(data).attr("class", "tab_active");
         } else {
            $(data).attr("class", "tab_normal");
         }
      })
   } else if (type == "category") {
      var categoryLiDoms = $("li[name='categoryLi']");
      var currentCategoryIndex = $(obj).attr("indexValue");
      $.each(categoryLiDoms, function(index, data){
         if (index == currentCategoryIndex) {
            $(data).attr("class", "selected");
         } else {
            $(data).removeClass("selected");
         }
      })
   }

   //先获取到年份索引
   var yearIndex = $("li[name='yearLi'][class='tab_active']").attr("indexValue");
   //再获取到类别的索引
   var categoryIndex = $("li[name='categoryLi'][class='selected']").attr("indexValue");
   var scheduleIndex;
   if (yearIndex == 0) {
      scheduleIndex = parseInt(yearIndex) + parseInt(categoryIndex);
   } else {
      scheduleIndex = parseInt(categoryNumber) * parseInt(yearIndex) + parseInt(categoryIndex);
   }
   var divDoms = $("div .tab_block3");
   $.each(divDoms, function(index, data){
      if (index == scheduleIndex) {
         $(data).css("display", "block");
      } else {
         $(data).css("display", "none");
      }
   })

   displayOrHide("close");
}

function logout(){
   $.ajax({
      type : "post",
      url : "http://study.dongao.com/study/u/logoutCw",
      dataType:"jsonp",
      async: false,
      success : function(data) {
         $.ajax({
            type : "post",
            url : data.url,
            dataType:"jsonp",
            success : function(data) {
               window.location='http://study.dongao.com/study/u/logout';
            },
            error: function(e){
               window.location='http://study.dongao.com/study/u/logout';
            }
         });
      },
      error: function(e){
         window.location='http://study.dongao.com/study/u/logout';
      }
   });   
}

function tabActive(obj) {
   var ulDom = $(obj).parent();
   var liDoms = ulDom.find("li");
   $.each(liDoms, function(index, data){
      $(data).attr("class", "tab_normal");
   })
   $(obj).attr("class", "tab_active");

   //设置课表的显示与否
   var indexValue = $(obj).attr("indexValue");
   var divDoms = $("div .tab_block3");
   $.each(divDoms, function(index, data){
      if (index == indexValue) {
         $(data).css("display", "block");
      } else {
         $(data).css("display", "none");
      }
   })
   displayOrHide("close");
}

//显示公告还是培训说明
function displayDiv(id, obj) {
   if (id == "trainDescription") {
      $(obj).attr("class", "li1 li_active1");
      $(obj).parent().find("li").eq(1).attr("class", "li2 li_normal2");
      $("#trainDescription").css("display", "block");
      $("#notice").css("display", "none");
   } else if (id == "notice") {
      $(obj).attr("class", "li2 li_active2");
      $(obj).parent().find("li").eq(0).attr("class", "li1 li_normal1");
      $("#notice").css("display", "block");
      $("#trainDescription").css("display", "none");
   }
}

//点击展开或者收起按钮时
function displayOrHide(type) {
   if (type == "open") {
      $("#open").css("display", "none");
      $("#close").css("display", "block");
      $(".tab_block3").addClass("showTable");
   } else if (type == "close") {
      $("#close").css("display", "none");
      $("#open").css("display", "block");
      $(".tab_block3").removeClass("showTable");
   }
}
</script>

