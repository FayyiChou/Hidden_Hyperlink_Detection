<script>
//弹出窗口js
$(function(){

  $(".toMyCenter").on("click",function(){
    $.ajax({
      url: "http://study.dongao.com/study/u/validateStudyCenter?callback=?",
      dataType: "jsonp",
      timeout: 5000,
      success: function(data, status){
          if(data.status=="true"){
            window.location.href=data.url;   
          }else{
            $(".tc").show();
            $(".tcbg").show();
          }
      },
      error: function(XHR, textStatus, errorThrown){
        $(".tc").show();
        $(".tcbg").show();
      }
    });
  });
   $(".tcTit img").click(function(){
      $(".tc").hide();
      $(".tcbg").hide();
   })

   //默认显示第一个年度的课表
   var divDoms = $("div .tab_block3");
   $.each(divDoms, function(index, data){
      if (index == 0) {
         $(data).css("display", "block");
      } else {
         $(data).css("display", "none");
      }
   })

});
</script>
<!--弹出框end-->
