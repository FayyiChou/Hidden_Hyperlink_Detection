    scrollPicPartner.arrRightId = "Right_Partner"; //右箭头ID

    scrollPicPartner.frameWidth = 235; //显示框宽度

    scrollPicPartner.pageWidth = 235; //翻页宽度

    scrollPicPartner.speed = 10; //移动速度(单位毫秒，越小越快)
    scrollPicPartner.space = 10; //每次移动像素(单位px，越大越快)
    scrollPicPartner.autoPlay = true; //自动播放
    scrollPicPartner.autoPlayTime = 3; //自动播放间隔时间(秒)

    scrollPicPartner.initialize(); //初始化						
		
