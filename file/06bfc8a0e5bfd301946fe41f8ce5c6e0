			var x=$(this).offset().left;

			var y=$(this).offset().top+30;

			$('#load_pop').css({left:x,top:y,display:"block"});

			var i=$(this).index();

			$('#load_pop').find('.load_box').eq(i).show().siblings().hide();

		})

