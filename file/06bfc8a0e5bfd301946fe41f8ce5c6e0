<script type="text/javascript">
function changeLike(){
	$.ajax({
		type:'get',
		url:'/?c=widget&w=flash&f=detail_like&ajax=1',
		success:function(data){
			var data = eval('('+data+')');
			if(data.code==1){
				var html='';
				var url='';
				var img='';
				var count=data['count'];
				for(var i=0;i<count;i++){
					url='/show/'+data['list'][i]['data']['id']+'.html';
					img='http://img.kuai8.com'+data['list'][i]['data']['icon'];
					html += '<li>';
					html += '<a class="mimg" href="'+url+'"><img src="'+img+'" alt="'+data['list'][i]['data']['name']+'"><span class="fillet"></span></a>';
					html += '<p><a href="'+url+'">'+data['list'][i]['data']['name']+'</a></p>';
					html += '</li>';
				}
				$('#detail_like').html(html);
			}
		}
	});
}
</script>
