$(document).ready(function(e) {			
	t = $('.fixed').offset().top;
	mh = $('article').height();
	mt = $('article').offset().top;
	fh = $('.fixed').height();
	$(window).scroll(function(e){
		s = $(document).scrollTop();	
		if(s > t-1){
			$('.fixed').css('position','fixed');
			if(s + fh > mh+mt){
				$('.fixed').css('top',mh+mt-s-fh+'px');	
			}else{
				$('.fixed').css('top',mh-s+'px');
				$('.fixed').offset({top:s});
			}				
		}else{
			$('.fixed').css('position','');
		}
	})
});
