                // 对应图片的文字描述
var array = new Array();array[0] = "省领导到我院视察指导工作";
array[1] = "松原市委书记高福平到我院调研";
array[2] = "我院举行党的群众路线教育实践活动动员大会";
array[3] = "松原市市长李相国到我院调研";
array[4] = "市委书记王常松及各位领导一行来我院指导工作";
