<script type="text/javascript">
// 导航点击效果
  $(function () {
    var report=$("#content_report").offset().top-50; 
    var item=$("#content_item").offset().top-50; 
    var mobile=$("#content_mobile").offset().top-50; 
    var contact=$("#content_contact").offset().top-180;
    
    $(".nav_report").css('background-color', '#007e7a');
    $(".nav li").find("a").click(function (e) {
    	$('.nav').find('a').css('background-color', 'inherit');
    	$(this).css('background-color', '#007e7a');
    	aAnimate($(this).attr("class"));
    });
    var aAnimate=function(e){
    	if(e=="nav_report"){
    		$("html,body").animate({scrollTop:report},1000); 
    	}
		if(e=="nav_item"){
			$("html,body").animate({scrollTop:item},1000);	
		}
		if(e=="nav_mobile"){
			$("html,body").animate({scrollTop:mobile},1000);
		}
		if(e=="nav_contact"){
			$("html,body").animate({scrollTop:contact},1000);
		}
    };
	$(window).scroll(function(event){
		var val=$(document).scrollTop();// 获取当期当前页面被卷曲的头部
		if(val>=report)   // 我们判断肯定是从大往小判断
		{
			$(".nav_report").css('background-color', '#007e7a').parent().siblings('li').find("a").css('background-color', 'inherit');  //当前的加载current类
		}
		if(val>=item)
		{
			$(".nav_item").css('background-color', '#007e7a').parent().siblings('li').find("a").css('background-color', 'inherit');  //当前的加载current类
		}
		if(val>=mobile-1)
		{
			$(".nav_mobile").css('background-color', '#007e7a').parent().siblings('li').find("a").css('background-color', 'inherit');  //当前的加载current类
		}
		if(val>=contact-83)
		{
			$(".nav_contact").css('background-color', '#007e7a').parent().siblings('li').find("a").css('background-color', 'inherit');  //当前的加载current类
		}
	});  
  });

  </script>
 <script>
 var dataJson=[]; 
 $(function() {
	 $.ajax({
			type:"post",
			contentType: "application/json",
			dataType:"json",
			data:{},
			url:"/web/deptManagement/getAllLessee.do",
			async: false, 
			success:function(data){
				for (var i = 0; i < data.length; i++) {
		            var item = data[i];
		            var dept_name = item.dept_name;
		            var login_url = item.login_url;
		            var a ={};
		            a.label=dept_name;
		            a.value=dept_name;
		            a.url=login_url;
		            dataJson[i]=a;
			   	}
			}
	});
 	// 显示学校list
  	$("#tags2").autocomplete({
   		source: dataJson,
   		select: function(event, ui){
   			$(this).value = ui.item.value;
   			$("#url").val( ui.item.url );
   			$('#button').removeClass().addClass("success");
   		}
  	});
 	
  	$('#tags2').bind('input propertychange', function() {
  		var schoolname = $.trim($("#tags2").val());
 		 var flag = false;
 		 for (var i = 0; i < dataJson.length; i++) {
 			 var item = dataJson[i];
 			 if(schoolname==item.value){
 				$("#url").val(item.url);
 				 flag = true;
 			 }
 		 }
 		 if(flag == true){
 			 $('#button').removeClass().addClass("success");
 		 }else if(flag == false){
 			$("#url").val("");
 			 $('#button').removeClass().addClass("failed");
 		 }
  	});
  	

  	// 显示area list
  	$('#area').selectlist({
		zIndex: 10,
		width: 250,
		height: 50
	});

	// checkbox切换
	// $(".search .checkbox").click(function(event) {
 //     	$(this).children('i').addClass('picked');
 //     	$(this).siblings('.checkbox').children('i').removeClass('picked')
 // 	});
 });
 
 function webLogin(){
	 if ($('#button').hasClass('success')){
		 window.location.href = ""+"/"+$("#url").val();
	 }
	 
 }
 
</script>



