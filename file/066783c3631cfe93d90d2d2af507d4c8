    <script type="text/javascript" src="http://oi9ar5y1q.bkt.clouddn.com/static/js/jquery.lazyload.min.js"></script>
    <script type="text/javascript" src="http://oi9ar5y1q.bkt.clouddn.com/static/js/dropload.min.js"></script>
    <script>
        $(function() {
            $("img.lazy").lazyload();
			$(".gz1").live("click",function(){

                var $this = $(this);

                var _trade_id = $(this).attr('trade_id');

                $.ajax({    
                    type: "POST",
                    url: "/attention/add",
                    data: {"trade_id":_trade_id},
                    dataType: "json",
                    success: function(re) {
                        if (re == 0) {
                            $this.html('<img src="/static/images/lists/guanzhus.png" style="width:15px; display:inline-block;" />&nbsp;已关注');
                        } else if (re == 1) {
                            location.href = '/user/login';
                        } else if (re == 2) {
                            alert('关注异常!')
                        }
                    }
                });

			})
        });
		
		$(function(){
			var banner_top = $(".banner_fixed").offset().top;
			$(".banner_fixed_img").css("top",banner_top);
		})
    </script>
    <script>
      $(function(){
        var _width=$(window).width()-40;
        $('.categroyScroll .title').css({'width':_width});
      })
      $(function(){
            var exchange_list=$('.exchange_list');
            var _width=$(window).width()
            var lis=exchange_list.find('li');
            var li_width_2=(_width-14)/2;
             var li_height_2=li_width_2*1.5;
            var li_width_3=(_width-18)/3;
             var li_height_3=li_width_3*1.5;
            if($(window).width()<350){
                lis.width(li_width_2);
                lis.find('.pic .pic_pro').css({height:li_height_2,width:li_width_2});
            }else{
                lis.width(li_width_3);
                lis.find('.pic .pic_pro').css({height:li_height_3,width:li_width_3});
            }
        })
       $(function(){
            var $slide_toggle=$('.slide_toggle')
            var $arrow_down_btn=$('.arrow_down_btn');
            $slide_toggle.each(function(){
                var _this=$(this);
                $arrow_down_btn.click(function(){
                    $slide_toggle.toggle();
                    $arrow_down_btn.text($arrow_down_btn.text() == "" ? " " : "");
                    if ($arrow_down_btn.text() == "") {
                        $arrow_down_btn.removeClass("upSpan2");
                        
                    } else {
                        $arrow_down_btn.addClass("upSpan2");
                    }
               })
            })
           
        })
       $(function () {
            showScroll();
            function showScroll() {
                $(window).scroll(function () {
                  var scrollValue = $(window).scrollTop();
                  scrollValue > 80 ? $('.toTop').fadeIn() : $('.toTop').fadeOut();
                });
                $('.toTop').click(function () {
                  $("html,body").animate({ scrollTop: 0 }, 200);
                });
            }
        });
    </script>
    <script>
       // 上拉加载更多
        $(function(){
            var _cid = '';
            var _odb = '1';
            var _type = '1';

            var _width = $(window).width();

            var _w;

            if($(window).width()<350){
                _w = (_width-14)/2;
            }else{
                _w = (_width-18)/3;
            }

            $.ajax({
                type: 'POST',
                url: '/lists/apply_more',
                data: {t:_type,c:_cid,o:_odb,page:1,width:_w},
                dataType: 'json',
                success: function(data){

                    if (data['next_page'] == 0) {
                        me.lock();
                        me.noData();
                    } else {
                        $('.lists').attr('next_page', data['next_page']);
                        $('.lists').append(data['data']);
                        me.resetload();
                    }
                },
                error: function(xhr, type){
                    //alert('Ajax error!');
                    // 即使加载出错，也得重置
                    me.resetload();
                }
            });

            // dropload
            $('.content').dropload({
                scrollArea : window,
                loadDownFn : function(me){
                    $.ajax({
                        type: 'POST',
                        url: '/lists/apply_more',
                        data: {t:_type,c:_cid,o:_odb,page:$('.lists').attr('next_page'),width:_w},
                        dataType: 'json',
                        success: function(data){

                            if (data['next_page'] == 0) {
                                me.lock();
                                me.noData();
                            } else {
                                $('.lists').attr('next_page', data['next_page']);
                                $('.lists').append(data['data']);
                                me.resetload();
                            }
                        },
                        error: function(xhr, type){
                            //alert('Ajax error!');
                            // 即使加载出错，也得重置
                            me.resetload();
                        }
                    });
                }
            });
        });
    </script>

</body></html>