                                '<td>更新日期</td>'+
                            '</tr>';
                    for(var i=start;i<end;i++){
                        htmls += '<tr>';
                        htmls += '  <td><a href="/down/146/'+resp[i].file_id+'/'+resp[i].file_name+'">'+resp[i].info+'</a></td>';
                        htmls += '  <td>'+resp[i].version+'</td>';
                        htmls += '  <td>'+resp[i].main_game_version+'</td>';
                        htmls += '  <td>'+resp[i].update_time+'</td>';
                        htmls += '</tr>';
                    }

                htmls += '</table>';
             $('.block11').html(htmls);        
        }
        function show_page_html(total_page,page,resp){
            var page_htmls = '<div class="pageCut">'; 
            if(page > 1 && page <= total_page){
                page_htmls += '<a href="javascript:show_list_html('+(page-1)+');" class="pre"><上一页</a>';
            }     
            for(var i=1;i<=total_page;i++){
                if(page == i){
                    page_htmls += '<a href="javascript:show_list_html('+i+');" class="cur">'+i+'</a>';
                }else{
                    page_htmls += '<a href="javascript:show_list_html('+i+');">'+i+'</a>';
                }
            }
            if(page < total_page){
                 page_htmls += '<a href="javascript:show_list_html('+(page+1)+');" class="nex">下一页></a>';
            }

            page_htmls += '</div>';

            $(".block10").html(page_htmls);

        }

        $('#good').click(function(){
            var good_html = parseInt($("#good").html());
            $.getJSON('/vote/146/good',function(json){
                if(!json.success){
                    alert(json.msg);
                }else{
                    $('#good').html(good_html+1);
                    $("#point").text(json.msg);
                }
            });
        });

        $('#bad').click(function(){
             var bad_html = parseInt($("#bad").html());
            $.getJSON('/vote/146/bad',function(json){
                if(!json.success){
                    alert(json.msg);
                }else{
                    $('#bad').html(bad_html+1);
                    $('#point').text(json.msg);
                }
            });
        });
        </script>
