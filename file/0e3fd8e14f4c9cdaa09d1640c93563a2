       var staticTranslations = new Object();
                function translate(input){
                    var translateElement = $('<div></div>');
                    if(staticTranslations[input]!==undefined){
                        translateElement.html(staticTranslations[input]);
                        return translateElement.html();
                    }
                    return input;
                }
                staticTranslations['Выбрано'] = 'Elegido';
                staticTranslations['Search'] = 'Búsqueda';
                staticTranslations['None selected'] = 'Ninguna seleccionada';
                staticTranslations['All selected'] = 'Todo seleccionado';
                staticTranslations['Select all'] = 'Seleccionar todo';
                staticTranslations['Selected'] = 'Seleccionado';
                staticTranslations['Choose Hour'] = 'Elija horas';
                staticTranslations['Hour'] = 'Hora';
                staticTranslations['Choose Minutes'] = 'Elija Minutos';
                staticTranslations['Minutes'] = 'Minutos';
                staticTranslations['Choose'] = 'Elegir';
