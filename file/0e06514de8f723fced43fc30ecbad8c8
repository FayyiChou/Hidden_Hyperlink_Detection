<script language="javascript">

template.helper("uescape",
function(v) {
    return unescape(v);
});

template.helper("imgfilter",
function(v) {

    v = unescape(v);
    v = v.replace(/<img [\s\S]*?(\/)?>/gi, "");
    return v.replace(/<([a-z]+?)(?:\s+?[^>]*?)?>\s*?<\/\1>/gi, "").replace("<p align=\"center\">　　&nbsp;</p>","");
});

$(function() {
	 
	if(docdata.hasComment=="1"){
		$("a.picsCmt").attr("href","http://3g.sun0769.com/pinglun.html?moduleid=1188&title="+escape(docdata.title)+"&id="+docdata.docId);
	}else{
		$("a.picsCmt").hide();
		}
	$(".picsSina").attr("href", "http://service.weibo.com/share/share.php?url="+encodeURI(location.href)+"?ch=share_weibo&language=zh_cn&title="+encodeURI(docdata.title)+"&pic="+docdata.pics+"&searchPic=true" );
	$(".picsQQ").attr("href","http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url="+encodeURI(location.href)+"%3Fch%3Dshare_qqZone&amp;title="+encodeURI(docdata.title)+"&amp;showcount=0&amp;summary=&amp;pics="+docdata.pics);
    $.getJSON(docdata.docId+"/map.js",
    function(data) {
        var html = template('slideTpl', data);
        $('#swiper-wrapper').append(html);
        html = template('txtTpl', data);
        //console.log(html);
        $('#picTxt ul').append(html);
		
		doSlide();
		
    })
//$('.swiper-slide img').lazyload({effect: "fadeIn"});

});


</script>
