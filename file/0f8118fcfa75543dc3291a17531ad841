		evt.init();
		
		require(['service/common/analysis'], function(module) {
			var analysisData =  {"sDisplayName":"itemDetail","sCategoryId":1100000002,"sBrandId":15234,"sBelongStoreId":1000011692,"sLayoutId":1003311250,"sItemId":1003141201,"sCurrentUrl":"http://shoppingw.naver.com/itemdetail/1003311250"} ;
	
			module.sendAnalysis(analysisData);
		});

	});
});

require(['variable'], function(variable){
	variable.set({
		"photoInfraDomains" : ["shop1.phinf.naver.net","shop2.phinf.naver.net","bshop.phinf.naver.net","cshop.phinf.naver.net"]
	});
});

