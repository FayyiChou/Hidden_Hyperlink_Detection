  <script>
    /*
     * 请修改下面js对象的内容以完成定制
     * 内容：请定制为需要提醒用户的内容,
     * 时间：请定制为事件发生的时间，格式为：年-月-日 时:分
     * advance：请定制为需要提前提醒用户的时间，0为准时提醒
     * url： 请定制为提醒用户访问的url链接
     * icon： 生成的icon的大小，可选值为1，2，3，尺寸分别为90*24，63*24，50*16
     * 注意：如果不使用标准接口，而是按需要自己生成链接地址
     * 请用encodeURIComponent对内容和时间进行编码。
     */

    function gotoRemind(gamename, serverid, mtime) {

      var mTimeData = new Date(parseInt(mtime) * 1000);

      var mins = mTimeData.getMinutes();

      if (mins < 10) mins = "0" + mins;
      var mShowTime = (1900 + mTimeData.getYear()) + "-" + (mTimeData.getMonth() + 1) + "-" + mTimeData.getDate() + " " + mTimeData.getHours() + ":" + mins;

      var mContent = "温馨提示：萌乐《" +  gamename + "》正式 " + serverid +  "服于今日" + mTimeData.getHours() + ":" + mins   + "开启，祝您游戏愉快！";

      window.open("http://qzs.qq.com/snsapp/app/bee/widget/open.htm#content=" + encodeURIComponent(mContent) +'&time='  +  encodeURIComponent(mShowTime)) +'&advance=0' + '&url=' + encodeURIComponent("www.baidu.com");
    }
    </script>
</div>
		</div>

