<script type="text/javascript">
$("#rpassword0").keydown(function(e){
	var curkey = e.which;
	if(curkey == 13){
		userlogin(0);
	}
});

function GetRequest() {
	var url = location.search; //获取url中"?"符后的字串
	var theRequest = new Object();
	if (url.indexOf("?") != -1) {
		var str = url.substr(1);
		strs = str.split("&");
		for(var i = 0; i < strs.length; i ++) {
			theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);
		}
	}
	return theRequest;
}


function userlogin(id) {

	var url = "/index.php?m=index&a=ajax_dologin";

	if (login(id)) {
		var mUserName = $("#username" + id).val();
		var mPassword = $("#rpassword" + id).val();
		var mCookietime = $("#cookietime" + id).val();
		var mSeccode = $("#seccode").val();

		$.post(url, {username:mUserName, password: mPassword, cookietime: mCookietime, type : id, seccode: mSeccode}, function(data) {
			if (data.status == 1) {
				//登陆成功
				var ischeck = document.getElementById("cookietime0").checked;
				if (ischeck) {
					setcookie("REMEMBER_ACCOUNT", mUserName, 30);
				} else {
					delcookie("REMEMBER_ACCOUNT");
				}
				var Request = new Object();
				Request = GetRequest();
				//document.write(data.html);
				if(Request['uri']!='undefined' && Request['uri']){
					window.location.href = Request['uri'];
				}else{
					window.location.href = '/';
				}
			} else {
				alert(data.msg);
				if(data.submitCount > 2){
					$(".login-box").addClass('login-code');
					$(".wrap-code").css('display','block');
				}
				$("#vcode").attr("src",$("#vcode").attr("src")+'?rand=' + Math.random());
			}
		});
	};
}

//首页登录框判断
function login(id) {

  var mUserName = $("#username" + id);
  var mPassword = $("#rpassword" + id);

  if (mUserName.val() === "")
  {
	  alert("请输入用户名！");
	  return false;
  }
  if (mPassword.val() === "")
  {
	alert("请输入密码！");
	return false;
  } else if (mPassword.val().length < 6 || mPassword.val().length > 20) {
	alert("您输入的密码长度不符合，请重新输入！");
	return false;
  }
  return true;
}

//退出
function loginout() {

	var url = "/index.php?m=Index&a=ajax_logout";
	$.get(url, function(data) {
		window.location.reload();
	});
}



$(function(){
  setRememberAccount();
});

//记住账号
function setRememberAccount() {

  var mcookie = getcookie("REMEMBER_ACCOUNT");

  if (mcookie != null) {
	  $("#username0").val(mcookie);
	  $("#username1").val(mcookie);
  };
}

//进入游戏
function lastplay_joingame(game_id, serverid) {

  var url = "/index.php?m=JoinGame&game_id=" + game_id +"&server_id=" + serverid;

  window.open(url);
}



