	$(".tab-handler a").each(function(){
		var d;
		$(this).hover(function(){
			var _this = $(this);
			var tab_content = $.trim(_this.attr("rel"));
			_this.parent().addClass("current").siblings().removeClass("current");
			_this.addClass("current").siblings().removeClass("current"); // 首页科室子栏目(- -#)
			$(tab_content).show().siblings(".tab-content").hide();
			_this.parent().parent().siblings("a").attr("href", _this.attr("href"));
		},function(){
			clearTimeout(d);
		});
	});
	
});
