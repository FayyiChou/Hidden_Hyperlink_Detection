<script type="text/javascript">
    var yjipage={pageindex:1,pagesize:8,regionId:203515};
    var yjipcount=4,yjistar=1,yjiend=4;
    setauthorinfo();
    function setauthorinfo(){
        $(".liTop").each(function (i) {
            var _this = $(this), author = _this.attr("deffer"), footid = _this.attr("footid");            
            $.getJSON("http://my.lotour.com/i/Common/GetMemberMessage?memberId=" + author + "&callback=?", function (data) {
                if (data.Success) {
                    var result = data.Result.Data;
                    _this.find("img").attr("src", "http://img.lotour.net/UserPhoto/" + result.Photo_180);
                    _this.find("a:eq(1)").attr("title", result.NickName).html(result.NickName);                    
                }
            });            
        });        
    }
    function yjfillipage(i){
        i=i<1?1:i;
        i=i>4 ? 4 :i;
        if(i!=yjipage.pageindex){
            yjipage.pageindex=i;
            yjistar=yjipage.pageindex-4;
            yjistar=istar<1?1:yjistar;
            yjiend=yjistar+9;
            yjiend=yjiend>yjipcount?yjipcount:yjiend;
            yjistar=yjiend-9<1?1:yjiend-9;
            $.ajax({
                cache: false,
                type: "POST",
                dataType: "jsonp",
                url: "http://api.lotour.net/brandhome/region/GetDMSYouJi",
                data:yjipage,
                success: function (data) {                    
                    var temp="",zhe="";
                    $.each(data.result,function(i,val){
                        if(i%4==0){zhe = "id=\"ml0\"";}else{zhe="";}
                        temp+='<li '+zhe+'>'+
                        '<div class="liTop" deffer="'+val.Author +'" footid="'+val.Id +'">'+
                        '<a href="http://my.lotour.com/i/'+val.Author+'" target="_blank" class="tx">'+
                        '<img src="http://img.lotour.net/zip/20110411/index_mrtu.jpg"></a>'+
                        '<a href="http://my.lotour.com/i/'+val.Author +'" target="_blank" class="name"></a>';
                        var playwith="";
                        if (val.PlayWith > 0)
                        {                       
                            playwith = "独自";
                            switch (val.PlayWith)
                            {
                                case 2:
                                    playwith = "和朋友";
                                    break;
                                case 3:
                                    playwith = "和爱人";
                                    break;
                                case 4:
                                    playwith = "和父母";
                                    break;
                                case 5:
                                    playwith = "和家人";
                                    break;
                            }
                        }
                        var d =eval(val.PublishTime.replace(/\/Date\((\d+)\)\//gi, "new Date($1)"));
                        var month =parseInt(d.getMonth())+1;
                        temp+='<p class="liTopTxt">我'+playwith+'于'+month+'月来到了</p>'+
                                '<p class="liTopTxt">这里，留下最美好回忆</p>'+
                                '</div>'+
                                '<div class="liBottom">'+
                                '<a href="'+val.Url+'" target="_blank">'+
                                '<div class="liPic">'+
                                '<img src="'+val.PicList[0] +'">';
                        var tt = val.Title;
                        if(val.Title.length>12)
                        {
                            tt=val.Title.substring(0,12)+'...' ;
                        }
                        temp+='</div><p class="liBottomTxt">'+tt+'</p></a></div></li>';
                    });
                    $(".yjUl").html(temp);
                    $("#yjpage .page_num").empty();
                    $("body,html").animate({
                        scrollTop: $(".youjiWrap").offset().top-100
                    }, 500);
                    setauthorinfo();
                    for (var j = yjistar; j <= yjiend; j++) {
                        $("#yjpage .page_num").append("<span class=\""+(j==i?"on":"")+"\" deffer=\""+j+"\">"+j+"</span>");
                    }
                }
            });
        }
    }
    
    $("#yjpage .page_num").click(function(event){
        var $elem = $(event.target).closest("span");
        if($elem.length==1){
            yjfillipage(parseInt($elem.attr("deffer")));
        }
    });
    $("#yjpage .the_first").click(function(){
        yjfillipage(1);
    });
    $("#yjpage .prev_page").click(function(){
        yjfillipage(yjipage.pageindex-1);
    });
    $("#yjpage .next_page").click(function(){
        yjfillipage(yjipage.pageindex+1);
    });
    $("#yjpage .the_last").click(function(){
        yjfillipage(4);
    });
    
</script>
