<script id="itemTemplate2" type="text/x-jquery-tmpl">
    <li class="clearfix">
        <div class="lt">
            <a target="_blank" href="${Url}" class="tt" title="${Title}">${Title}</a>
        </div>
        <span class="sn" deffer="${Id}" ttype="${Url.indexOf('lg')>-0?1:0}"></span>
    </li> 
</script>
    <script type="text/javascript" src="http://js.lotour.net/jquery.tmpl.min.js"></script>
<script type="text/javascript">
    var npage={page:1,type:0,pagesize:9,id:203515,depth:4};
    var npcount=38,nstar=1,nend=6;
    function fillnpv(){
        var arr1=[],arr2=[];
        $("span.sn").each(function(){
            if($(this).attr("ttype")=="0")
                arr1.push($(this).attr("deffer"));
            else
                arr2.push($(this).attr("deffer"));
        });
        $.getJSON("http://api.lotour.net/pv/SelectInformationPvs?ids="+arr1.toString()+"&callback=?", function (data) {
            $.each(data,function(i,val){
                var pv=val.pageview;
                if(pv>=10000){
                    pv= Math.round(pv / 10000 * 10) / 10 +"万";
                }
                $("span.sn[deffer="+val.id+"][ttype=0]").html(pv);
            });
        });
        $.getJSON("http://api.lotour.net/pv/SelectPvs?ids="+arr2.toString()+"&callback=?", function (data) {
            $.each(data,function(i,val){
                var pv=val.pageview;
                if(pv>=10000){
                    pv= Math.round(pv / 10000 * 10) / 10 +"万";
                }
                $("span.sn[deffer="+val.id+"][ttype=1]").html(pv);
            });
        });
    }
    fillnpv();
    function fillNews(i){
        i=i<1?1:i;
        i=i>38 ? 38 :i;
        if(i!=npage.page){
            npage.page=i;
            nstar=npage.page-2;
            nstar=nstar<1?1:nstar;
            nend=nstar+5;
            nend=nend>npcount?npcount:nend;
            nstar=nend-5<1?1:nend-5;
            $.ajax({
                cache: false,
                type: "POST",
                dataType: "jsonp",
                url: "http://api.lotour.com/brandhome/API/SelectNews",
                data:npage,
                success: function (data) {
                    var temp = $("#itemTemplate2").tmpl(data);
                    $("#unews").html(temp);
                    $(".page_num-1").empty();
                    $("body,html").animate({
                        scrollTop: $(".zhoubian").offset().top-$(".tit_bottom ").height()
                    }, 500);
                    fillnpv();
                    for (var j = nstar; j <= nend; j++) {
                        $(".page_num-1").append("<span class=\""+(j==i?"on":"")+"\" deffer=\""+j+"\">"+j+"</span>");
                    }
                }
            });
        }
    }
    $(".page_num-1").click(function(event){
        var $elem = $(event.target).closest("span");
        if($elem.length==1){
            fillNews(parseInt($elem.attr("deffer")));
        }
    });
    $(".the_first-1").click(function(){
        fillNews(1);
    });
    $(".prev_page-1").click(function(){
        fillNews(npage.page-1);
    });
    $(".next_page-1").click(function(){
        fillNews(npage.page+1);
    });
    $(".the_last-1").click(function(){
        fillNews(38);
    });
</script>
    
