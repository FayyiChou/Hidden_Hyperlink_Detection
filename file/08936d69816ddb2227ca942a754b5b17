    function fillipage(i){
        i=i<1?1:i;
        i=i>5 ? 5 :i;
        if(i!=ipage.page){
            ipage.page=i;
            istar=ipage.page-4;
            istar=istar<1?1:istar;
            iend=istar+9;
            iend=iend>ipcount?ipcount:iend;
            istar=iend-9<1?1:iend-9;
