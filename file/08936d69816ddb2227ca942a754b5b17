    <script type="text/javascript">
        var mapInfo = {
            lat: parseFloat('25.430873'),
        lng: parseFloat('107.880034')
    };
        var miniMapBox = $('#J_miniMap'), miniMap, MiniMarker;
        function initMiniMap() {
           
            var latLng = new google.maps.LatLng(mapInfo.lat, mapInfo.lng);
            var mapOptions = {
                center: latLng,
                zoom: 8,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                disableDefaultUI: true
            };
            if($('#J_miniMap').length>0){
                miniMap = new google.maps.Map(miniMapBox.get(0), mapOptions);
            }
            var markerHtml = '';
            new MiniMarker(miniMap, markerHtml, latLng);


            var marker = new google.maps.Marker({
                position: latLng
            });
        }
        function MarkerOverlay() {
            var Overlay = function (map, text, latLng, ready) {
                this.map = map;
                this.text = text;
                this.latLng = latLng;
                this.ready = ready;
                this.div = null;
                this.setMap(this.map);
            }
            Overlay.prototype = new google.maps.OverlayView();
            $.extend(Overlay.prototype, {
                onAdd: function () {
                    var div = $('<div/>').html(this.text).css('position', 'absolute');
                    $(this.getPanes().overlayImage).append(div);
                    this.div = div;
                    if (typeof this.ready === 'function') {
                        this.ready.call(this);
                    }
                },
                draw: function () {
                    var latLng = this.getProjection().fromLatLngToDivPixel(this.latLng);
                    this.div.css({
                        'left': latLng.x - this.div.width() / 2,
                        'bottom': -latLng.y
                    });
                },
                onRemove: function () {
                    this.div.remove();
                    this.div = null;
                }
            });
            return Overlay;
        }
        $(function () {
            var mapload = new MfwMapLoad(function () {
                if (mapInfo == undefined || !$.isPlainObject(mapInfo) || $.isEmptyObject(mapInfo)) {
                    return;
                }
                MiniMarker = MarkerOverlay();
                initMiniMap();
            }, 5000, false);
            mapload.autoLoadMap();
        });
    </script>

