                '<a href="'+val.AuthorUrl+'" target="_blank"><img src="'+val.AuthorHeadPic+'" class="reasonTx"></a>'+
                '<p class="name"><a href="'+val.AuthorUrl+'" target="_blank">'+val.NickName+'</a></p>'+
                '<p class="lliang" deffer="'+val.AuthorId+'_'+val.AuthorType+'"></p><p class="llTxt">浏览量</p>'+
                '<a href="'+val.AuthorUrl+'" target="_blank" class="txt">'+val.Introduction+'</a>'+
                '<p class="dyBtn" deffer="'+val.AuthorId+'"><img src="http://img1.lotour.net/inspiration/dingyue/dy+.png"><span>订阅</span></p>'+
            '</div>'+
            '<div class="modR">'+
                //'<p class="title">'+val.Title+'</p>'+
                '<a  href="'+val.RegionUrl+'" target="_black" class="title">'+val.RegionName+'</a>'+
                '<ul class="bqCont">';
                        $.each(val.Tags,function(i1,val1){                            
                            temp1+='<li data-id="" deffer="'+val1.Id+'">'+
                                '<a href="'+val.Url+'" target="_blank">'+
                                    '<img src="'+val1.Pic+'">';
                            var reason = val1.Id == 0 ? val.Title : val1.Reason;
                            temp1+='<p>'+reason+'</p></a></li>';
                        });
                        temp+=temp1+'</ul>'+
                        '<ul class="xqBq">';
                        $.each(val.Tags,function(i2,val2){
                            var fm="";
                            if(i2==0){
                                fm="fm";
                            }
                            temp2+='<li><span class="yuan '+fm+'">'+val2.Name+'</span></li>';
                        });
                        temp+=temp2+'</ul>'+
                    '</div>'+
                '</div>';
                    });
                    $("#divlg").html(temp);
                    $("#page .page_num").empty();
                    $("body,html").animate({
                        scrollTop: $(".reasonWrap").offset().top-100
                    }, 500);
                    fillpv();
                    fillClassName();
                    //fnModule();
                    for (var j = istar; j <= iend; j++) {
                        $("#page .page_num").append("<span class=\""+(j==i?"on":"")+"\" deffer=\""+j+"\">"+j+"</span>");
                    }
                }
            });
        }
    }
    $("#page .page_num").click(function(event){
        var $elem = $(event.target).closest("span");
        if($elem.length==1){
            fillipage(parseInt($elem.attr("deffer")));
        }
    });
    $("#page .the_first").click(function(){
        fillipage(1);
    });
    $("#page .prev_page").click(function(){
        fillipage(ipage.page-1);
    });
    $("#page .next_page").click(function(){
        fillipage(ipage.page+1);
    });
    $("#page .the_last").click(function(){
        fillipage(5);
    });
</script>

