        if($(".company_1").height()>115){
            $(".company_1").css({"height":"114px","overflow":"hidden"});
            $(".more_xiala").css({"display":"block"});
        }else{
            $(".company_1").css({"height":"auto"});
            $(".more_xiala").css({"display":"none"});
        }


