    var refreshtime = 120;
    var initialload = false;
    
    $(document).ready(function() {
        setTimeout(populateFeedOutput, initialload ? refreshtime * 1000 : 10);
    });
    
    function populateFeedOutput() {
        var obj = $("#FeedContainer");
        obj.load("http://www.macearchive.org:80/Services/AggregatedFeedService.aspx?SiteId=1&NumItems=30", "");
        
        if (refreshtime != 0)    
            setTimeout(populateFeedOutput, refreshtime * 1000);        
    }
