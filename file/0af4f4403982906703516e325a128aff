    <script type="text/javascript">
       jQuery(function($) {
           $("#owl-banner").owlCarousel({
                singleItem: true,
                paginationNumbers: false,
                autoPlay: 4000,
                slideSpeed : 300,
                paginationSpeed : 400,
           });
        });

		var _hmt = _hmt || [];
		(function() {
		  var hm = document.createElement("script");
		  hm.src = "//hm.baidu.com/hm.js?822241f3b3996998ec72c06f45206e40";
		  var s = document.getElementsByTagName("script")[0]; 
		  s.parentNode.insertBefore(hm, s);
		})();
    </script>
    <script>
    	$.fn.serializeObject = function()  
		{  
		   var o = {};  
		   var a = this.serializeArray();  
		   $.each(a, function() {  
		       if (o[this.name]) {  
		           if (!o[this.name].push) {  
		               o[this.name] = [o[this.name]];  
		           }  
		           o[this.name].push(this.value || '');  
		       } else {  
		           o[this.name] = this.value || '';  
		       }  
		   });  
		   return o;  
		};
		    
    
    
        $(function(){
        
        	
        
        	jQuery('#tech').bind("submit", function(){
        	
		         var options = {
		                url: '/resourceinfo',
		                type: 'post',
		                cache: false,
		                contentType: "application/json",
		                data: JSON.stringify($("#tech").serializeObject()),
		                success: function (data) {
		                    swal("", "您的信息已收到！\n\n经详细评估后，我们会第一时间联系您！", "success");
		                    $('#myTechModal').modal('hide');
		                },
		                error: function (){
		                	swal("", "很抱歉，服务器开小差了...\n\n请稍后重试或咨询客服！", "error");
		                }
		         };
		         $.ajax(options);
		         return false;
	   		});
	   		
	   		
	   		
            $("#tech-smt").click(function(){
            
           	    var EmailReg = /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/; //邮件正则
                var PhoneReg = /^0{0,1}(13[0-9]|15[0-9]|153|156|18[7-9])[0-9]{8}$/ ; //手机正则
                
                var $nickname = $('.technickname');
                var $phone = $('.techphone');
                var $qq = $('.techqq');
                var $desc = $('.techdesc');
                if($nickname.val() == ''){
                    swal("", "请填写联系人姓名！", "error");
                    return false;
                }
                else if($phone.val() == ''){
                    swal("", "请填写联系电话！", "error");
                    return false;
                }
                else if(!PhoneReg.test($phone.val())){
                    swal("", "手机格式错咯... 请输入正确手机格式！", "error");
                }
                else if($qq.val() == ''){
                    swal("", "请填写QQ！", "error");
                    return false;
                }
                else if($desc.val() == ''){
                    swal("", "请填写您的技术描述！", "error");
                    return false;
                }
            });
            
            
            jQuery('#data').bind("submit", function(){
        		
		         var options = {
		                url: '/resourceinfo',
		                type: 'post',
		                cache: false,
		                contentType: "application/json",
		                data: JSON.stringify($("#data").serializeObject()),
		                success: function (data) {
		                    swal("", "您的信息已收到！\n\n经详细评估后，我们会第一时间联系您！", "success");
		                    $('#myDataModal').modal('hide');
		                },
		                error: function (){
		                	swal("", "很抱歉，服务器开小差了...\n\n请稍后重试或咨询客服！", "error");
		                }
		         };
		         $.ajax(options);
		         return false;
	   		});
	   		
	   		
	   		
            $("#data-smt").click(function(){
            
            	var EmailReg = /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/; //邮件正则
                var PhoneReg = /^0{0,1}(13[0-9]|15[0-9]|153|156|18[7-9])[0-9]{8}$/ ; //手机正则
                
                var $nickname = $('.datanickname');
                var $phone = $('.dataphone');
                var $qq = $('.dataqq');
                var $desc = $('.datadesc');
                var $propertylist = $('.datapropertylist');
                if($nickname.val() == ''){
                    swal("", "请填写联系人姓名！", "error");
                    return false;
                }
                else if($phone.val() == ''){
                    swal("", "请填写联系电话！", "error");
                    return false;
                }
                else if(!PhoneReg.test($phone.val())){
                    swal("", "手机格式错咯... 请输入正确手机格式！", "error");
                }
                else if($qq.val() == ''){
                    swal("", "请填写QQ！", "error");
                    return false;
                }
                else if($desc.val() == ''){
                    swal("", "请填写您的技术描述！", "error");
                    return false;
                }
                else if($propertylist.val() == ''){
                    swal("", "请填写字段清单！", "error");
                    return false;
                }
            });
        });
    </script>
</body>
</html>