  <script type="text/javascript">
$(function(){
    $.post("/forum/ajax_bbs.asp",{action:"online"});
	function online(){
	   $.post("/forum/ajax_bbs.asp",{action:"online"});
	   clearInterval(myonline);
	}
	var myonline = setInterval(online,60000);
})
</script> 
