        //加载
        if( $(obj).attr('cid') > 0 ){
            
			var cid = $(obj).attr('cid');

			$("#goodslist"+cid).html('<img src="/Public/Home/images/loading.jpg" style="height: 30px;padding-left: 50%;margin-top: 50px;">');
            //showLoading();
            $.post("/Goods/getGoodsList.html", {'cid':cid}, function(data){
 				if(data.status){
 					$("#goodslist"+cid).html(data.info);
 		            $(obj).attr('cid',0);
 	    		} else {
 	    		}
 			}, "json");
        }
    }
    
    function showLoading(){
        
                $('.loading_div').shCircleLoader({
                        color: 'red',
                        dots: 24,
                        dotsRadius: 13,
                        keyframes:
                                '0%   {background: #00A538;    {prefix}transform: scale(1)}\
                                20%  {background: #95C716; {prefix}transform: scale(.4)}\
                                40%  {background: #00A538;    {prefix}transform: scale(0)}\
                                50%  {background: #00A538;    {prefix}transform: scale(1)}\
                                70%  {background: #95C716; {prefix}transform: scale(.4)}\
                                90%  {background: #00A538;    {prefix}transform: scale(0)}\
                                100% {background: #00A538;    {prefix}transform: scale(1)}'
                });
    }
 
 
  setTimeout("countSecond()", 1000);
   
   function countSecond(){
       
       $(".container").width();
        var windowWidth = $(window).width();
        var box_left=(windowWidth-$(".container").width())/2-40;
        $("#box").css("left",box_left);
        var f_banner_hei=$(".f_banner").height()+696;
        var fertilizer=$(".fertilizer").height()+65;
        $('#box ul li').click(function(){
                var this_index=$(this).index();
                $('body,html').animate({ scrollTop: f_banner_hei+ (fertilizer*this_index)}, 300);
                $(this).find('.num').hide().siblings('.word').css('display','block');
                $(this).siblings('li').find('.num').css('display','block').siblings('.word').hide();

        });
        // @ 给窗口加滚动条事件
        $(window).scroll(function(){
                // 获得窗口滚动上去的距离
                var ling = $(document).scrollTop();
                // 在标题栏显示滚动的距离
                //document.title = ling;
                // 如果滚动距离大于1534的时候让滚动框出来
                if(ling>f_banner_hei){
                        $('#box').show();
                }
                if(/*f_banner_hei<ling && */ling<f_banner_hei+fertilizer){
                        // 让第一层的数字隐藏，文字显示，让其他兄弟元素的li数字显示，文字隐藏
                        $('#box ul li').eq(0).find('.num').hide().siblings('.word').css('display','block');
                        $('#box ul li').eq(0).siblings('li').find('.num').css('display','block').siblings('.word').hide();
                }else if(ling<f_banner_hei+(fertilizer*2)){
                        $('#box ul li').eq(1).find('.num').hide().siblings('.word').css('display','block');
                        $('#box ul li').eq(1).siblings('li').find('.num').css('display','block').siblings('.word').hide();
                }else if(ling<f_banner_hei+(fertilizer*3)){
                        $('#box ul li').eq(2).find('.num').hide().siblings('.word').css('display','block');
                        $('#box ul li').eq(2).siblings('li').find('.num').css('display','block').siblings('.word').hide();
                }else if(ling<f_banner_hei+(fertilizer*4)){
                        $('#box ul li').eq(3).find('.num').hide().siblings('.word').css('display','block');
                        $('#box ul li').eq(3).siblings('li').find('.num').css('display','block').siblings('.word').hide();
                }
                if(ling>f_banner_hei+(fertilizer*4)+400 || ling<f_banner_hei){
                        // $('#box').css('display','none');  // @ 这一句和下一句效果一样。
                        $('#box').hide();
                }

        })
       
   }
