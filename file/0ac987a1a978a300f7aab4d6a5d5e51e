	seajs.use(['jquery'], function(jquery) {
		$(function() {
			$('input[name=kw]').bind({
				focus: function() {
					if (this.value == this.defaultValue) {
						this.value = "";
					}
				},
				blur: function() {
					if (this.value == "") {
						this.value = this.defaultValue;
					}
				}
			});
		})
