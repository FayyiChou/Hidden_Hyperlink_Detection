<script type="text/javascript">
$(document).ready(function() {
  $("#fbc_close_link").click(function(){
	$("#fam_btm_wrap").animate({left:'-100%'},"500");
	$("#fam_btm_show").animate({left:'0'},"500");
  });
  $("#fam_btm_show").click(function(){
	$("#fam_btm_show").animate({left:'-100%'},"500");
	$("#fam_btm_wrap").animate({left:'0px'},"500");
  });
  
  
  $("#fbc_btn").click(function(){
	var sReg = /^[\u4e00-\u9fa5]{2,5}$/;
	
    if(!sReg.test($.trim($("#p_name").val()))){
      $(".fbc_error").html("请输入您的称呼...");
	  $("#p_name").focus();
	  $("#p_name").css("border-color","#f00000");
	  return false;
    }else{
	  $("#p_name").css("border-color","#fff");
      $(".fbc_error").html("");
    }

    if($.trim($("#p_phone").val())==''){
      $(".fbc_error").html("请输入您的电话...");
	  $("#p_phone").focus();
	  $("#p_phone").css("border-color","#f00000");
	  return false;
    }else{
	  $("#p_phone").css("border-color","#fff");
      $(".fbc_error").html("");
    }
	
	if($(".fbc_sel").val()=='性别'){
	  $(".fbc_error").html("请选择性别...");
	  return false;
	}
	
	var fbc_text;
	fbc_text = $("#p_name").val() + " 是位 " + $(".fbc_sel").val();
	


    $.ajax({
      type:"post",
	  dateType:"text",
	  async:false,
	  url:"/ajax/save_guest.php",
	  data:"p_name="+$.trim($("#p_name").val())+"&p_address="+$("#p_address").val()+"&p_phone="+$("#p_phone").val()+"&p_mail="+$("#p_mail").val()+"&p_title="+$("#p_title").val()+"&p_content="+fbc_text,
	  error:function(){
	    $(".fbc_error").html("请拨打服务热线0371-55509578。");
	  },
	  success:function(o){
	    var f	= o.substring(0,1);
	    if(f=="0"){
	      $(".fbc_error").html(o.substring(1));
	    }
	    if(f=="1"){
	      $(".fbc_error").html(o.substring(1));
	  	  $("input[type=reset]").trigger("click");
	    }
	  }
    });
  });
});
</script>
