<script type="text/javascript">
var jql=jQuery.noConflict();
    </script>
<script type="text/javascript">
//选项卡与延迟插件结合
function lazyloadForPart(container){
    container.find('img').each(function(){
        var original = jql(this).attr("original");
        if (original) {
            jql(this).attr('src', original).removeAttr('original');
        }
    });
}

function TabHeads(c,a,b){

jql(c).hover(function(){

jql(c).each(function(){
jql(this).removeClass(a)
});

jql(b).each(function(){
jql(this).hide()
});

jql(this).addClass(a);

var d=jql(c).index(this);

jql(b).eq(d).each(function(){
jql(this).show();
lazyloadForPart(jql(b));

});

})

}

jql(document).ready(function(){

TabHeads("#topic01 dt","on","#topic01 dd");
})
</script>
