	// 点击预订
  	$(document).ready(function(){
  		$('.f_book_btn').live('click',function(){
  			var fromCityName = encodeURIComponent($(this).attr('from_city_name'));
  			var fromStationName = encodeURIComponent($(this).attr('from_station_name'));
  			var toCityName = encodeURIComponent($(this).attr('to_city_name'));
  			var busNum = $(this).attr('bus_number');
  			var hashkey = $(this).attr('hashkey');
  			var date = $(this).attr('date');
  			var utmSource = $G("utm_source");
  			if (utmSource == 'undefined' || utmSource == '' || utmSource == 'null' || utmSource == null) {
  				utmSource = '';
  				utmSource = getCookie('order_channel');
  				if (utmSource == 'undefined' || utmSource == '' || utmSource == 'null' || utmSource == null) {
  					utmSource = '';
  				}
  			}
  			
  			var self = $(this);
  			$("#f_tip_yanzheng").show();
  			$.post("/index.php?param=ajax/getYupiao.html",{from:fromCityName,to:toCityName,hashkey:hashkey,date:date},
  			function(data){
  				$("#f_tip_yanzheng").hide();
  				if(data == null || data.yupiaoCnt >= 1){
  					var url ='/buy.html?from='+fromCityName+'&fromStation='+fromStationName+'&to='+toCityName+'&busNo='+busNum+'&hashkey='+hashkey+'&date='+date+'&utm_source='+utmSource;
  					document.location = url;
  				}else{
  					self.removeClass('btn_orage').addClass('btn_grey').removeClass('f_book_btn');
  					$("#f_tip_zt_wupiao").show();
  				}
  			},"json");
  		});
  	});
  		
  		
