<script type="text/javascript">
	var sendSms = 0;
	
	jQuery('.btn_look').live('click', function(){
		jQuery('#tipWin_ctrip').show();
	});
	
	// 发送短信
	jQuery('#sendSms').live('mousedown', function(){
		var txtMobileNo = jQuery('#txtMobileNo');
		var self = jQuery(this);

		if (sendSms == 1) {
			return;
		}
		
		if (!checkMobile(txtMobileNo)) {
			return;
		}
		
		sendSms = 1;
		ajaxSendAppSMS(txtMobileNo.val());
		
		var second = 59;
        var t = setInterval(function () {
        	if (second > 0) {
        		self.addClass('btn_grey').html("(" + second + "秒后)重新获取短信");
            } else {
            	self.removeClass("btn_grey").html("免费发送");
            }
            second--;
            if (second < 0) {
            	sendSms = 0;
            	clearInterval(t);
            }
                
        }, 1000);
	});
	
	function checkMobile(txtMobileNo) {
		if (txtMobileNo.val() == '' || txtMobileNo.val() == '请输入手机号码') {
			txtMobileNo.focus();
			jQuery('.input_wa_txt').html('请输入手机号码').show();
			return false;
		}
		
		if (!/^1[34578]\d{9}$/.test(txtMobileNo.val())) {
			txtMobileNo.focus();
			jQuery('.input_wa_txt').html('手机号码格式错误').show();
            return false;
		}
		
		jQuery('.input_wa_txt').html('').hide();
		return true;
	}
	
	jQuery('#txtMobileNo').live('focus', function(){
		jQuery(this).css("color", "#000");
    	if (jQuery(this).val() == jQuery(this).attr("tooltip")) {
			jQuery(this).val('');
		}
	});
	
	jQuery('#txtMobileNo').live('blur', function(){
		checkMobile(jQuery(this));
	});
	
	// 发送获取下载链接短信
	function ajaxSendAppSMS(mobile) {
		var url = 'http://qiche.tieyou.com/index.php?param=/ajax/sendBusActivityPushAppSms&mobile='+ mobile +'&ref=ctrip&jsoncallback=?';
		jQuery.getJSON(
			url,
		    function(data) {
				
		    }
		);
	}
</script>
