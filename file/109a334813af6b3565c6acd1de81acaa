$(document).ready(function() {
  var image = new Image();
	$(".content img").each(function(img) {
    image.src   = $(this).attr('src');
    if(image.height > 35) {
  		$(this).after('<a class="show_img_box" index="t'+img+'" href="'+this.src+'" title="'+this.title+'"></a>');
  		$(this).clone().appendTo("a.show_img_box[index=t"+img+"]");
  		$(this).remove();
    }
	});
	$("a.show_img_box").fancybox({
		'autoScale'			: false
	});
});
/**
 * 限制最大宽度
 */
var games = {};
games.DrawImg = function(obj, maxWidth) {
	var imgWidth = $(obj).width();
	var imgHeight = $(obj).height();
	
	if(imgWidth > maxWidth) {
		$(obj).width(maxWidth);
		$(obj).height((maxWidth/imgWidth)*imgHeight);
	}
}


//页面加载时执行
$(function(){
	$(".content img").load(function(){
		games.DrawImg(this, 460);
	});
});
</script> 
