<!--FAST悬浮！-->
<script type="text/javascript" charset="utf-8">
                        (function(){
                            var postAct = document.getElementById("fastzb");
                            var postActTop = postAct.offsetTop;
                            document.onscroll = function() {
                                var docTop = document.body.scrollTop | document.documentElement.scrollTop;
                                if (postActTop < (docTop + 10)) {
                                    postAct.setAttribute("style", "position:fixed; width:250px; top: 10px; z-index:300; box-shadow: 0 0 3px 1px rgba(0,0,0,0)");
                                } else {
                                    postAct.setAttribute("style", "");
                                }
                            }
                        })()
                    </script>
