        <script>
            function cunshi_cun(yijiid, obj, num) {
                var str = "";
                str = str + "<iframe src='cunshi_cun.asp?yijiid=" + yijiid + "' width='978' height='240' scrolling='no' frameborder='0'></iframe>";
                window.cunshicun.innerHTML = str;
                for (var i = 1; i <= num; i++) {
                    $('#liid' + i).removeClass();
                }
                $(obj).attr('class', 'lion');
            }
            function cunshi_xianshow() {
                $('#cunshixian').css('display', '');
            }
            function cunshi_xianhide() {
                $('#cunshixian').css('display', 'none');
            }
        </script>

