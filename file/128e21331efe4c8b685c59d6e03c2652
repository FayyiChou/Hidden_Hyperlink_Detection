                    <script>
                        var speedb = 10
                        demob2.innerHTML = demob1.innerHTML//克隆demo1为demo2  
                        function Marqueeb() {
                            if (demob2.offsetWidth - demob.scrollLeft <= 0)//当滚动至demo1与demo2交界时  
                                demob.scrollLeft -= demob1.offsetWidth//demo跳到最顶端  
                            else {
                                demob.scrollLeft++  //如果是横向的 将 所有的 height top 改成 width left  
                            }
                        }
                        var MyMarb = setInterval(Marqueeb, speedb)//设置定时器  
                        demob.onmouseover = function() { clearInterval(MyMarb) } //鼠标移上时清除定时器达到滚动停止的目的  
                        demob.onmouseout = function() { MyMarb = setInterval(Marqueeb, speedb) } //鼠标移开时重设定时器  
                    </script>

                </ul>

