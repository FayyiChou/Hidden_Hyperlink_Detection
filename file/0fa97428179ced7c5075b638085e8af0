      <SCRIPT language=javascript type=text/javascript>
var speed1=30
var lue=document.getElementById("lue")
var lue1=document.getElementById("lue1")
var lue2=document.getElementById("lue2")

lue2.innerHTML=lue1.innerHTML

function Marquee1()
{
	if(lue2.offsetWidth-lue.scrollLeft<=0)
	{
		lue.scrollLeft-=lue1.offsetWidth
	}
	else
	{
	    lue.scrollLeft++
	}
}
var MyMar1=setInterval(Marquee1,speed1)
lue.onmouseover=function()
{
    clearInterval(MyMar1)
}
lue.onmouseout=function()
{
    MyMar1=setInterval(Marquee1,speed1)
}
</SCRIPT>
