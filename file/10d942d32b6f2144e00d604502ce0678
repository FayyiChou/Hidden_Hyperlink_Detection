		function corres(ln, f){
	      if(ln=="cn"){
	        for(var i=0;i<cn.length;i++){
		      if(cn[i]==f){
		        return en[i];
		      }
		    }
	      }else{
	        for(var j=0;j<en.length;j++){
		      if(en[j]==f){
		        return cn[j];
		      }
		    }
	      }
