<script language="Javascript">
function switchlogintype(v)
{
	var lun = new Array("用户名:","姓名:","姓名:","学习卡:","验证码:","档案号:","姓名：","姓名：");
	var lup = new Array("密　码:","身份证:","证书号:","密　码:","密　码:","身份证:","手机号：","会计证：");
	document.getElementById("LoginUserName").innerHTML = lun[v];
	document.getElementById("LoginUserPassword").innerHTML = lup[v];
	
	
}
function ce_login()
{
	BOX_show("layer_ce_login");
}
function cancel_login()
{
	BOX_remove("layer_ce_login");
}
function do_ajax_login()
{

	$("#loadinginfo").html("<img src='/images/loading1.gif'>");
	var logintype = $("#ce_LoginType").val();
	var username = $("#ce_username").val();
	var userpwd = $("#ce_userpwd").val();
	if ((username != "") && (userpwd != ""))
	{
		$.ajax({
			url:"/user/celogin.jsp",
			data:{"LoginType":logintype,"username":username,"userpwd":userpwd},
			cache:false,
			type:"POST",
			success:function(d)
			{
				if (d == "ok")
				{
					//登录成功。
					$("#loadinginfo").html("登录成功！");
					location.href = "http://ce.esnai.net";
				}
				else
				{
					$("#loadinginfo").html("登录失败！");
				}
			}
		});
	}
	else
	{
		$("#loadinginfo").html("请输入登录信息！");
	}
}
</script>
