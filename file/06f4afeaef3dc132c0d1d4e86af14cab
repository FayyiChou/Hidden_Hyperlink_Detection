        }

    });

//commonred init
    var loginByMobileObj = touchRedCommon.loginByMobileInit($('.qn_main')),
        $loginByMobile = $('.get_packet');

    $('.qn_main').on('input','[data-type="phone"],[data-type="vcode"]',function() {
        var phone = $('[data-type="phone"]').val(),
            vcode = $('[data-type="vcode"]').val();

        if(/^1[0-9]{10}$/.test(phone) && !$('.code-line').hasClass('code-show')) {
            submitAjax({action: "submit", mobile: phone, bd_source: "", couponId: $.hash() == '#' ? '' : $.hash(), vcode: vcode});
            return ;
        }
        if(/^1[0-9]{10}$/.test(phone) && ( !$('.code-line').hasClass('code-show') ? true : /^\d{6}$/.test(vcode) && $loginByMobile.hasClass('regetVcode') )) {
                $loginByMobile.addClass('login_active');
        }else {
                $loginByMobile.removeClass('login_active');
        }
    });

//    $('.m_login').on('click','.get_packet',function(e) {
