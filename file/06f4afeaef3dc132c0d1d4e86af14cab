<script type="text/javascript">
		var orders = {
			keys : [],
			values : []
		};
		var pattern = /^horder|zorder/g;
		for(var i = 0, len = localStorage.length; i < len; i++){
			var key = localStorage.key(i);
			if(key.match(pattern)){
				var orderNum = key.substring(7);
				var order = JSON.parse(localStorage.getItem(key));
				if(localStorage.getItem("localOrder_hotel_" + orderNum) == null){
					orders.values.push(order);
					orders.keys.push(key);
				}
			}
		}

		//设置新结构
		if(orders.keys.length > 0 && orders.values.length > 0){
			$.ajax({
				type: 'post',
				url: '/api/hotel/touchordertoken',
				data: {
					orders: JSON.stringify(orders.values)
				},
				dataType: 'json',
				success: function(data) {
					if (data && data.ret) {

						var result = JSON.parse(data.data),//异常处理
							keys = Object.keys(result);

						for(var i = 0; i < keys.length; i++){
							var key = keys[i];
							var value = result[key];
							console.log(key);
							localStorage.setItem("localOrder_hotel_" + key, JSON.stringify(value));
							//localStorage.removeItem("horder_" + key);
						}

					}else{
						//监控
					}
				}
			});
		}



</script>

