    loginByMobileObj.afterGetVcodeSuccess = function() {
        $loginByMobile.addClass('regetVcode');
        var vcode = $('[data-type="vcode"]').val();
    };

    $('.qn_main').on('click','.login_active',function() {
        var couponId = $.hash(),
            tel = $('[data-type="phone"]').val(),
            vcode = $('[data-type="vcode"]').val();
        couponId = couponId == '#' ? '' : couponId;
        $(this).removeClass('login_active');

        submitAjax({action: "submit", mobile: tel, bd_source: "", couponId: couponId, vcode: vcode});
    });

    function submitAjax(param) {
        try{
            localStorage.setItem("gift_mobile",param.mobile);
        }catch(e){}
        $.ajax({
            type: "POST",
            url: "/h5/red/secondVersionVipRed",
            dataType: "json",
            data: param,
            success: function (data) {
                if (data.isCorrectVcode) {
                  //qunar代金券数据
                    var tplData={
                        status: data.status,
                        hotel: data.hotel,
                        car: data.car,
                        dujia: data.dujia,
                        guojiche: data.guojiche,
                        huiyuan: data.huiyuan,
                        daijinquan: data.daijinquan,
                        huichang: data.huichang,
                        menpiao: data.menpiao,
                        zhongdianfang: data.zhongdianfang,
                        huochepiao:data.huochepiao,
                        tel: data.tel,
                        couponId: data.couponId,
                        totalMoney: data.totalMoney + ''
                    };
                    //请求百度外卖券数据
                    $.ajax({
                        type:"GET",
                        url:"/api/activity/baiduCoupon?phone="+param.mobile,
                        dataType:"json",
                        success:function(res){
                          if(res.ret){
                            $.extend(tplData,res.data);
                          }
                        },
                        error:function(res){
                          console.log(res.msg);
                        },
                        complete:function(){
                          saveInfo(tplData);
                          $(".qn_main").qRender($('#resultTpl'),tplData);
                        }
                    });
                    if(data.couponId == "") {
                        data.couponId = " ";
                    }
                    $.hash(data.couponId);
                    if(channel == "push") {
                        push();
                    }
                    if(data.status == 1 || data.status ==2){
                        try{
                            localStorage.setItem('touch_red_geted',new Date().getTime());
                        }catch(e){}
                    }

                } else {
                    $.Captcha.refreshCaptcha();
                    if(data.msg != '新用户需验证码领取' ) {
                        alert(data.msg);
                    }
                    $('.code-line').addClass('code-show');
                    $('[data-type="vcode"]').val('');
                }

            },
            error: function (data) {
                $('.login_active').addClass('login_active');
                alert("服务器繁忙！稍后再试...");
            }
        });
    }

    function saveInfo(data) {
        storage = window.localStorage;
        storage.setItem("packetInfo",JSON.stringify(data));
    }

    function push() {

        setTimeout(function(){
            location.href = document.location.protocol + "//touch.qunar.com/h5/client?bd_source=&sScheme=0&scheme=home&touchUrl=download?bd_source=";
        },12000);
        $('.qn_main').on('click','.m_result', function (e) {
            if($(event.target).closest(".js_rule_toggle").length > 0) {
                return;
            }
            location.href = document.location.protocol + "//touch.qunar.com/h5/client?bd_source=&sScheme=0&scheme=home&touchUrl=download?bd_source=";
        });
    }

</script>


