<script>
    document.title = "去哪儿礼包限时放送，呼唤小伙伴，还有200元现金拿不停！";
    QunarAPI.showOptionMenu({});
    QunarAPI.config({wxUrl: document.location.protocol + '//touch.qunar.com/api/activity/weixin/signature'});
    QunarAPI.ready(function () {
        QunarAPI.onMenuShare({
            title: '送您600元旅游大礼包，想去哪儿就去哪儿！', // 标题
            link: location.href, // 链接URL
            desc: '分享礼包给给小伙伴，再得最高200元现金!', // 描述
            imgUrl: document.location.protocol + '//simg1.qunarzz.com/site/images/wap/touch/images/v2/zhuanti/red/vipred_shareTitle_3.png', // 分享图标
            success: function () {
            },// 用户确认分享后执行的回调函数
            cancel: function () {
            } // 用户取消分享后执行的回调函数
        });
    });
    var channel = "",
        isLogin = "false",
        jsApi = false,
        checkJsApi = (function() {
            QunarAPI.ready(function () {
                // 调用QunarAPI提供的各种接口
                QunarAPI.checkJsApi({
                    jsApiList: ['showShareItems'], // 需要检测的JS接口列表
                    success: function (res) {
                        if (res.showShareItems) {
                            jsApi = true;
                        } else {
                            jsApi = false;
                        }
                    }
                });
            })
        })();
// 非首次打开此页面
    var storage = window.localStorage,
        data,
        gift_mobile='',
        oldHtml = $('.qn_main').html();
   data = storage.getItem('packetInfo');
    try{
        gift_mobile=JSON.parse(localStorage.getItem("gift_mobile"));
    }catch(e){}
    if(gift_mobile || isLogin == "true"){
        var couponId = $.hash() == '#' ? '' : $.hash();
        $.ajax({
            type: "POST",
            url: "/h5/red/secondVersionVipRed",
            dataType: "json",
            data: {
                action: "submit",
                mobile: gift_mobile,
                bd_source: "",
                couponId: couponId
            },
            success: function (data) {
                if (data.isCorrectVcode) {
                  //qunar代金券数据
                  var tplData={
                      status: data.status,
                      hotel: data.hotel,
                      car: data.car,
                      dujia: data.dujia,
                      guojiche: data.guojiche,
                      huiyuan: data.huiyuan,
                      daijinquan: data.daijinquan,
                      huichang: data.huichang,
                      menpiao: data.menpiao,
                      zhongdianfang: data.zhongdianfang,
                      huochepiao: data.huochepiao,
                      tel: data.tel,
                      couponId: data.couponId,
                      totalMoney: data.totalMoney + ''
                  };
                  //请求百度外卖券数据
                  $.ajax({
                      type:"GET",
                      url:"/api/activity/baiduCoupon?phone="+gift_mobile,
                      dataType:"json",
                      success:function(res){
                        if(res.ret){
                          $.extend(tplData,res.data);
                        }
                      },
                      error:function(res){
                        console.log(res.msg);
                      },
                      complete:function(){
                        saveInfo(tplData);
                        $(".qn_main").qRender($('#resultTpl'),tplData);
                      }
                  });
                    if(data.couponId == "") {
                        data.couponId = " ";
                    }
                    $.hash(data.couponId);
                    if(channel == "push") {
                        push();
                    }
                    if(data.status == 1 || data.status ==2){
                        try{
                            localStorage.setItem('touch_red_geted',new Date().getTime());
                        }catch(e){}
                    }
                } else {
                    $.Captcha.refreshCaptcha();
                    pageRouting();
                    if(data.msg != '新用户需验证码领取' ) {
                        alert(data.msg);
                    }
                    $('.code-line').addClass('code-show');
                    $('[data-type="vcode"]').val('');
                }

            },
            error: function (data) {
                alert("服务器繁忙！稍后再试...");
            }
        });
    }else{
        pageRouting();
    }
    function pageRouting(data){
        if(data) {
//            data = JSON.parse(data);
            data.status = data.status == 1 ? 2 : data.status;
            $(".qn_main").qRender($('#resultTpl'),{
              status:data.status,
              hotel: data.hotel,
              car: data.car,
              dujia: data.dujia,
              guojiche: data.guojiche,
              huiyuan: data.huiyuan,
              daijinquan: data.daijinquan,
              huichang: data.huichang,
              huochepiao: data.huochepiao,
              tel: data.tel,
              couponId: data.couponId,
              totalMoney: data.totalMoney + ''
            });
            $.hash(data.couponId);
            if(channel == "push") {
                push();
            }
        }
    }
