      <script>
　     //判斷是否為 IE	
	  var explorer = window.navigator.userAgent ;
	  if (explorer.indexOf("MSIE") >= 0) {//ie10及以下
	  var b_name=navigator.appName;
	  var b_version=navigator.appVersion;
	  var version=b_version.split(";");
	  version=version[1].replace(/[ ]/g,"");
	  version=version.split('MSIE')[1];
	  browser='IE:'+version;
	  alert("本系統不建議使用IE瀏覽器，建議您使用Chorme或Firefox瀏覽器進行操作。");
	  }else if (explorer.indexOf("Firefox") >= 0){//Firefox
	  browser='Firefox';
	  }else if (explorer.indexOf("Chrome") >= 0){//Chrome
	  browser='Chrome';
	  }else if (explorer.indexOf("Opera") >= 0){//Opera
	  browser='Opera';
	  }else if (explorer.indexOf("Safari") >= 0){//Safari
	  browser='Safari';
	  }else if (explorer.indexOf("Trident/7.0") >= 0){//IE11
	  browser='IE:10.0以上';
	  alert("本系統不建議使用IE瀏覽器，建議您使用Chorme或Firefox瀏覽器進行操作。");
	  }
	  </script>   
       
    <script type="text/javascript">
		function Featured_change(n){
			for(a=1;a<=5;a++){
				document.getElementById("Featured_"+a).style.display = "none";
			}

			document.getElementById("Featured_"+n).style.display = "inline-block";
		}
		function menu_change(n){
			document.getElementById("menu_"+n).src = "images/icons/menu_"+n+"_b.png";
		}
		function menu_back(n){
			document.getElementById("menu_"+n).src = "images/icons/menu_"+n+".png";
		}
	</script>    
