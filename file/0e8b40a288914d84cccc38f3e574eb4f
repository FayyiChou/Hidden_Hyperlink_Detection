<script src="http://bbs.gxsky.com/wx/2016/gohome/js/layer/layer.js" type="text/javascript"></script>
<script>
var jq = jQuery.noConflict();
var flag = true;
function tgAjax(tid,uid,uname) {
if(flag==false){
return false;
}
flag = false;
jq.ajax({
  type:'POST',
  url:"JCK/tgPost.php",
  data:{tid:tid,uid:uid,uname:uname},
  success: function(data) {
flag = true;
if(data=='error1'){
layer.alert('数据错误，请稍后重试！',{title:'温馨提示',icon: 2});  
}else if(data=='error2'){
layer.alert('楼主已申请领取金币，您不能再申请。请推荐其他帖子试试吧！',{title:'温馨提示',icon: 2});  
}else if(data=='error3'){
layer.alert('每个账号每天只能参与5次哦~请明天再来！',{title:'温馨提示',icon: 2});  
}else if(data=='error4'){
layer.alert('本帖子发布时间超过72小时，不能申请金币！请点击其他帖子试试吧！',{title:'温馨提示',icon: 2});  
}else if(data=='success'){
layer.alert('申请领取金币成功！请关注结果公布。',{title:'温馨提示',icon: 1});  
}else{
layer.alert('服务器错误，请稍后重试！',{title:'温馨提示',icon: 2});
}

}

});

}
</script>
