    <script>
	$("#kuaijiezhif").click(function()
	{
		
		
		var subject=$("#subject").val();
		var money=$("#money").val();
		if(subject=='')
		{
		$("#subject").css('border-width','1px');
		$("#subject").css('border-color','#26a867');
		$("#subject").focus();
		return false;
		}
		
		if(money=='' || parseInt(money)<=0)
		{
		$("#money").css('border-width','1px');
		$("#money").css('border-color','#26a867');
		$("#money").focus();
		return false;
		}
		$("#fastalipay").submit();
	});
	
	function control(e,o){
 
            var v=o.value|0;
 
            if(v<=0){
 
                o.value='';
 
                o.focus();
 
            }
 
        };
	</script>
