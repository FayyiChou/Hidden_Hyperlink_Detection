function changelocation(locationid)
    {
    document.add.smallsort.length = 0; 

    var locationid=locationid;
    var i;
    document.add.smallsort.options[0] = new Option('请选择二级栏目','');
    for (i=0;i < 43; i++)
        {
            if (subcat[i][1] == locationid)
            { 
            document.add.smallsort.options[document.add.smallsort.length] = new Option(subcat[i][0], subcat[i][2]);
            }        
        }
        
    }   
