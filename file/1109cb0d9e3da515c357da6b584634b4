		var otherShowQuestionTag=0;
			if(showDegree == '1' || showQuestionTag == '1' || showMessageCount == '1' ){
					FrontNews_list01.getAjaxValueByNewsId("206","false",showDegree,showMessageCount,showQuestionTag,"FrontNews_list01-b2_VisitedCount_1","FrontNews_list01-b2_CommentCount_1","FrontNews_list01-b2_showQuestionTag_1");
