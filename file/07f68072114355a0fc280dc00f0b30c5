<script>
	//回到顶部
  $(window).scroll(function(){
        var sTop = document.body.scrollTop;
        if(sTop > 150){
            $("#totop").addClass("active");
        }else{
            $("#totop").removeClass("active");
        };
    });
	  function goTop(acceleration, time) {
        acceleration = acceleration || 0.1;
        time = time || 16;
        var x1 = 0 , y1 = 0 , x2 = 0 , y2 = 0 , x3 = 0 , y3 = 0;
        if (document.documentElement) {
            x1 = document.documentElement.scrollLeft || 0;
            y1 = document.documentElement.scrollTop || 0;
        }
        if (document.body) {
            x2 = document.body.scrollLeft || 0;
            y2 = document.body.scrollTop || 0;
        }
        var x3 = window.scrollX || 0;
        var y3 = window.scrollY || 0;
        var x = Math.max(x1, Math.max(x2, x3));
        var y = Math.max(y1, Math.max(y2, y3));
        var speed = 1 + acceleration;
        window.scrollTo(Math.floor(x / speed), Math.floor(y / speed));
        if (x > 0 || y > 0) {
            var invokeFunction = "goTop(" + acceleration + ", " + time + ")";
            window.setTimeout(invokeFunction, time);
        }
    }
        $(".totop").on("click", function() {
            goTop();
        });   
</script>
<script type="text/javascript">
//相关阅读滑动
$(function(){
    $('.j_slide_box').on('touchstart', globalTouchHandler);
    $('.j_slide_box').on('touchmove', globalTouchHandler);
    $('.j_slide_box').on('touchend', globalTouchHandler);
})
var globalTouch = {};
function globalTouchHandler (e) {
    var e = e || window.event,
            el = $(this);
    //e = e.originalEvent;
    // console.log(e.target.parentNode);
    //console.log(e.type);
    if (e.touches.length <= 1) {
        switch(e.type){
            case 'touchstart':

                globalTouch.startDOM=el;
                globalTouch.x=e.touches[0].clientX;
                globalTouch.y=e.touches[0].clientY;
                globalTouch.dx=0;
                globalTouch.dy=0;
                globalTouch.touches=e.touches;
                break;

            case 'touchmove':
                globalTouch.dx=e.touches[0].clientX-globalTouch.x;
                globalTouch.dy=e.touches[0].clientY-globalTouch.y;
                if (Math.abs(globalTouch.dx) > Math.abs(globalTouch.dy)) {
                    e.stopPropagation();
                    e.preventDefault();
                }

                break;

            case 'touchend':

                if (globalTouch.dy < -100 || globalTouch.dy > 100) {
                    return;
                }
                if (el.hasClass('ul')) {
                    if (globalTouch.dx < -40) {
                        checkMovePhoto(globalTouch.dx, globalTouch.dy, el);
                        return;
                    }

                    if (globalTouch.dx > 40) {
                        checkMovePhoto(globalTouch.dx, globalTouch.dy, el);
                        return;
                    }
                    return;
                }
                if (globalTouch.dx < -40) {
                    checkMove(globalTouch.dx, globalTouch.dy, el);
                    return;
                }

                if (globalTouch.dx > 40) {
                    checkMove(globalTouch.dx, globalTouch.dy, el);
                    return;
                }

                globalTouch.dx = null;
                globalTouch.dy = null;
                // removeClass(el,'hover');
                break;
        }
    }
    if (e.touches.length > 1) {
        // window.top.location.reload();
    }
}
var checkMove = function (x, y, el) {
    var curr = parseInt(el.attr('data-index')),
        parentWidth = el.width(),
        liWidth,
        lis = el.find('.about-item'),
        marginRight = 10,
        totalWidth = 0,
        moveLeft,
        nextIndex,
        li;


    if (globalTouch.dx < -40) {

        nextIndex = curr+1;
        li = lis.eq(nextIndex);
    }

    if (globalTouch.dx > 40) {

        nextIndex = curr-1;
        li = lis.eq(nextIndex);
    }

    for (var i = 0, len = nextIndex; i < len; i ++) {
        totalWidth += lis.eq(i).width();
        totalWidth += 10;
    }

    if (li.width() && nextIndex >= 0) {
        liWidth = li.width();
    } else {
        return;
    }

    if (nextIndex == lis.length-1) {
        moveLeft = totalWidth - (parentWidth-liWidth);
    } else {
        moveLeft = totalWidth - (parentWidth-liWidth)/2;
    }

    el.attr('style', ' -webkit-transform: translate3d(-'+moveLeft+'px, 0px, 0px); transform: translate3d(-'+moveLeft+'px, 0px, 0px);')
    el.attr('data-index', nextIndex);
};
</script>
