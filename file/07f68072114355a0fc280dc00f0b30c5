<!--滚动新闻-->
	 

<script>
    var newsScroll = function (ele,time) {
        this.el = ele;
        this.element = this.el.children;
        this.length = this.element.length;
        this.height = this.element[0].offsetHeight;
        this.timer = 0;
        this.index = 0;
        this.speed = time || 3e3;
        this.init();
    };
    newsScroll.prototype = {
        init : function () {
            var len = this.length;
            if(1 != len){
                for(;len--;){
                    this.element[len];
                    len ? this.translate(len,this.height,0) : this.translate(len,0,0);
                }
                this.setTimer();
            }
        },
        translate : function (index, hei, dur) {
            var a = this.element[index];
            a.style.webkitTransitionDuration = dur + "ms";
            a.style.webkitTransform = "translate3d(0," + hei + "px,0)";
            a.style.transitionDuration = dur + "ms";
            a.style.transform = "translate3d(0," + hei + "px,0)";
        },
        getNext : function (index) {
            return index == this.length - 1 ? index = 0 : ++index;
        },
        handle: function () {
            this.translate(this.index, -this.height,500);
            this.index = this.getNext(this.index);
            this.translate(this.index,0,500);
            var that = this;
            setTimeout(function () {
                that.translate(that.getNext(that.index), that.height,0);
            },500)
        },
        setTimer: function () {
            var that = this;
            this.timer = setInterval(function () {
                that.handle();
            }, this.speed)
        }
    }
    var newsobj = document.getElementById("j_news_scroll");
    var newsScrollObj = new newsScroll(newsobj);
</script>
