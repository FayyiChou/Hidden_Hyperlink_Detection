<script>
	$("#zt_more").click(function(){
		var nextpage=parseInt($(this).attr('rel'));
		var pagenum = 4;
		var type = 1;
		var allpage = parseInt($(this).attr("allpage"));//总页数
		//如果当前页大于总页数 不发送请求。
		if(nextpage > allpage+1){
			return false;
		}
		getztdata(nextpage, pagenum, type);
		$(this).attr('rel',nextpage+1);
	});
	
	function getztdata(nextpage,pagenum,type){
		 $.ajax({
            type:'GET',
            dataType: "jsonp",
            async:false,
            url: "http://interface.sina.cn/games/gpapi/zt/comos_collect_index.d.shtml",
            data:{
              nextpage:nextpage,
			  type : type,
			  pagenum:pagenum
            },
             success: function(d){
                var datastr=d.result.data.newsstr; 
	
				 if(d.result.status.code==1){
					 var allpage = d.result.data.allPage;//全部的页数
					
					$("#zt_news .news-list-more").before(datastr); 
					$(".more").attr("allpage", allpage);//将总条数放入到页面中
					  if(allpage == nextpage){
						$(".more").parent().remove();
					}	
					 //相关阅读滑动参数
					 /* $(function(){
						$('.j_slide_box').on('touchstart', globalTouchHandler);
						$('.j_slide_box').on('touchmove', globalTouchHandler);
						$('.j_slide_box').on('touchend', globalTouchHandler);
					})*/
				}
                    
				
            }
        });

	}
	getztdata(1,4,1);
</script>
    </div>
