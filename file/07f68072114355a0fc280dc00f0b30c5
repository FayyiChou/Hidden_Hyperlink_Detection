<script>
    var home = {};  //命名空间
    home.openNav = !0;  //默认展开
    home.$hide = $(".topnav").find(".hide");
    home.$more = $(".topnav").find(".more");
    home.$more.addClass("open");
    home.$hide.removeClass("hide");
    home.$more.find(".down-arrow").addClass("open");
    home.$more.on("click",function(e){
        e.preventDefault();
        if(home.openNav){  //如果为展开状态
            $(this).find(".down-arrow").removeClass("open");
            home.$hide.addClass("hide");
            home.openNav = !1;
        }else{
            $(this).find(".down-arrow").addClass("open");
            home.$hide.removeClass("hide");
            home.openNav = !0;
        }
    })
</script>


