            <script>
                    //读取网游资讯
                        function getweblist(e){
                            //console.log($(e).attr("rel"));
                            var npage=parseInt($(e).attr("rel"));
                            getdata(1,npage,"weblist",5);
                            //取完数据页码加一
                            $(e).attr("rel",npage+1);
                        }
                    </script>
                    <script>
                    //读取接口数据
                        function getdata(fid,page,divId){
						var pageSize=arguments[3]?arguments[3]:3;   
                            //console.log(divId);
                                 $.ajax({
                                    type:'GET',
                                    dataType: "jsonp",
                                    async:false,
                                    url: "http://games.sina.cn/interface/2016_game_wap_home_mid_list_in.shtml?callback=?",
                                    data:{
                                      fid:fid,
                                      page:page,
									  pageSize:pageSize
                                    },
                                     success: function(d){
                                     
                                        if(d.result.status.code==200){
                                            $("#"+divId).append(d.result.data);
                                        }else{
                                            return false;
                                        }
                                    }
                                });
                              }
                    </script>
        </div>
    </div>
</section>
