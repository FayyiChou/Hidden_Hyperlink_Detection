<!--a标签加统计标签pos=22-->
	<script>
		var qs = 'pos=22';
	$('a').each(function(){
		var href = $(this).attr('href');
		if(href!="javascript:void(0)") {
			href += (href.match(/\?/) ? '&' : '?') + qs;
			$(this).attr('href', href);
		}
	});	
		function delOverpos(pid){
			//$("#weblist a,#sylist a,#dwlist a,#mnlist a").slice(0, 5).each(function(){
			$('#'+pid+' a').slice(0, 5).each(function(){
			   var href = $(this).attr('href');
						
				if(href)  {
					//console.log(href.replace(/&pos=22/,'') );
					href=href.replace(/&pos=22/,'') ;
					$(this).attr('href', href);
				}
			});	
		}
		delOverpos('weblist');delOverpos('sylist');delOverpos('dwlist');delOverpos('mnlist');
		
		
	</script>
