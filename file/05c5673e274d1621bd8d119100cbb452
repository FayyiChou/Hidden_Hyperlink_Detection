<SCRIPT type=text/javascript>

	$(function(){

		//top

		$('.btn-gywm').click(function(){

			var $btn = $('.btn-gywm');

			var $wrap1 = $('.wrap1');

			var h1 = $('.wrap1').height();

			var h2 = $('.wrap1-i').height();

			if( h1==3 ) {

				$btn.css('backgroundPosition','-138px -93px');

				$wrap1.animate( {height:h2}, 700, 'easeOutCubic', function(){

					$(this).css('height', 'auto');		

				});

			}

			else {

				$btn.css('backgroundPosition','-138px -61px');

				$wrap1.animate( {height:3}, 700, 'easeOutCubic');

			}

		});

		

		

		

		//弹出动画

		function popupA(f) {

			$.modal('<div class="popup2"><div id="flash"></div><a href="#" class="popup-close" title="关闭">关闭</a>',{

				overlayClose:true,

				opacity:80,

				overlayCss: {backgroundColor:"#000"}

			})

			var params = { scale:"noscale" }

			swfobject.embedSWF("swf/"+f+".swf", "flash", "300", "250", "9.0.0", "expressInstall.swf",params);

		}

			

		

	})

</SCRIPT>

