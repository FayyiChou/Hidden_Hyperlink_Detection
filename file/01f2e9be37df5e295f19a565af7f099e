<script type="text/javascript">
//nav
$(document).ready(function () {
    jQuery.navlevel2 = function (level1, dytime) {

        $(level1).mouseenter(function () {
            varthis = $(this);
            delytime = setTimeout(function () {
                varthis.find('div.fu').slideDown();
            }, dytime);

        });
        $(level1).mouseleave(function () {
            clearTimeout(delytime);
            $(this).find('div.fu').slideUp();
        });

    };
    $.navlevel2("li.mainlevel", 200);
});

//
$('#who_con_id').cxScroll();

//who 切换
$(document).ready(function () {

	for( var i=0; i<$('.who-li-btn').length; i++) {
		$('.who-li-btn').eq(i).on('mouseover', (function(i){
			return function() {
				if($('.who-li-btn').length == $('.who-qiehuan').length) {
					for( var j=0; j<$('.who-li-btn').length; j++) {
						$('.who-li-btn .who-btn-img').eq(j).css({'background-position': '0 0'});
						$('.who-btn-txt').eq(j).css({'color': '#fff'});
						$('.who-qiehuan').eq(j).hide();
					}
					$('.who-li-btn .who-btn-img').eq(i).css({'background-position': '0 -76px'});
					$('.who-btn-txt').eq(i).css({'color': '#edaa1a'});
					$('.who-qiehuan').eq(i).show();
				}
			}
		})(i));
	}
});
</script>


