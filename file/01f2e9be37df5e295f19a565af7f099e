<script type="text/javascript">

$(function(){
	$('#WebsiteDefault__ctl0_btnPost').click(function(){
	
		if($('#WebsiteDefault__ctl0_name').val()==""){
			alert('姓名不能为空');
			return false;
		}
		else if($('#('#WebsiteDefault__ctl0_phone').val()=="" || $('#WebsiteDefault__ctl0_tel').val()=="")){
			alert('电话或者手机不能为空');
			return false;
		}
		else if($('#WebsiteDefault__ctl0_content').val()==""){
		alert('留言内容部能为空');
			return false;
		
		}
	});
}

</script>
