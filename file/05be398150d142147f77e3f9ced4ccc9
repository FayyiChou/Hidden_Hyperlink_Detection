		function getUserList(){
			var map = {};
			$("img[user]").each(function(){
			    var obj = $(this);
			    map[obj.attr("user")]=obj.attr("uname");
			});
			var html = "<ul>";
		    for(var k in map){
		    	html+="<li><a href='javascript:reply("+k+",\""+map[k]+"\")'>"+map[k]+"</a></li>";
		    }
		    k+="</ul>";
		    return html;
		}
