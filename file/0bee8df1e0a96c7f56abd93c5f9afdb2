<script>
	$(document).ready(function(){
	  $(".pb_open").click(function(){
		  $(this).hide();
		  $(".pb_closed").show();
		  $(".sns").fadeIn();
		  $(".menu").fadeIn();
		  $(".phone_light").show();
	  });
	  $(".pb_closed").click(function(){
		  $(this).hide();
		  $(".pb_open").show();
		  $(".sns").hide();
		  $(".menu").hide();
		  $(".phone_light").fadeOut();
	  });
	  $("#wxbt").click(function(){
		  $(".wx_QRcord").show();
		  //$(".pb_open").show();
		  //$(".pb_closed").hide();
		  //$(".menu").hide();
		  //$(".sns").hide();
		  //$(".phone_light").fadeOut();
	  });
	  $(".wx_QRcord").click(function(){
		  $(this).hide();
		  $(".pb_closed").show();
		  $(".sns").show();
		  $(".menu").show();
		  $(".pb_open").hide();
	  });
	});
</script>


