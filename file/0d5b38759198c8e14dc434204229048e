<script type="text/javascript">
	var mallurl = "http://mall.sdrcu.com";
	var passporturl = "http://user.sdrcu.com";
	var membertype=5;
	var membertype_=5;
	var islogin="";
	$(document).ready(function(){
		loadNavigation();//获取导航栏地址
		//$("#topbar_left").html('<span>您好！欢迎来到山东农商银行智e购</span><a style="color:#444444;" href="/MallWeb/teseguan/Feature_List.html">【切换地区】</a>');
		// 为了避免首次加载时头部空白页，先初始化一句话
		var url = "/MallWeb/pages/base/header.jsp";
		$.post(url,function(result){
			eval("var obj = " + result);
			islogin = obj.flag;//是否登录
			if(islogin == 0){//用户未登录
				showTopText(0);//获取未登录时顶部内容
				$("#myorder").attr("href","javascript:showLoginPenl('"+passporturl+"',mallurl+'/MallWeb/member/index_buy_public.html?url=buy_myorder')");
			}else{//用户已登录
				// 用户类型
				var obj_membertype = obj.membertype;
				//如果用户为登录状态且存在用户类型时设置用户类型
				if(obj_membertype!=null||obj_membertype!=undefined){
					//设置全局变量用户类型值
					membertype=obj_membertype;
					membertype_ = membertype;
				}
				$.post("/MallWeb/pages/base/getnewmembertype.jsp",function(result){
					eval("var res = " + result);
					document.getElementById("topbar_left").innerHTML =res.type+decodeURI(obj.content) +"<span>,欢迎来到山东农商银行智e购</span><span><a href='javascript:void(0);' onclick='mallLogout()'>[退出]</a></span>&nbsp;<a style='color:#444444;' href='/MallWeb/teseguan/Feature_List.html'>【切换地区】</a>";
				});
				var str='';
				$.post("/MallWeb/pages/member/isSaler.jsp",function(result){
					var res = $.trim(result)
					if(res=="true"){
						showTopText(1);//获取买家登录时顶部内容
						$("#myorder").attr("href",mallurl+"/MallWeb/member/index_buy_public.html?url=buy_myorder");
						//买家
					}else if(res=="ischild"){
						showTopText(3);//获取子账户登录时顶部内容
					}else{
						showTopText(2);//获取卖家登录时顶部内容
						$("#myorder").attr("href",mallurl+"/MallWeb/member/index_buy_public.html?url=buy_myorder");
					}
				});
				readTotalNewLetter()//获取未读消息总数
				window.setTimeout("showHeadCart()",1000);//设置1秒刷新购物车信息	
				}
	   		});
		
		setKey();//设置关键字
		resSubMenu();//渲染商品分类面板悬浮效果
	});
	// 首页右侧  悬浮购物车
	function cartNotLogin(){
		//登录
		if(islogin==1){
			window.location.href = mallurl+"/MallWeb/cart/index.html";
		}else{
			var showUrl=passporturl+"/PassportWeb/zh_cn/member/loginDiv.html?url="+mallurl+"/MallWeb/cart/index.html";
			PopupWnd.show("您尚未登录",360,150,showUrl);
		}
	}
	// 首页右侧  悬浮我的订单
	function orderNotLogin(){
		//登录
		if(islogin==1){  
			window.location.href = mallurl+"/MallWeb/member/index_buy_public.html?url=buy_myorder";
		}else{
			var showUrl=passporturl+"/PassportWeb/zh_cn/member/loginDiv.html?url="+mallurl+"/MallWeb/member/index_buy_public.html?url=buy_myorder";
			PopupWnd.show("您尚未登录",360,150,showUrl);
		}
	}
	function change(){
		$("#searchList").hide();
	}

 </script>



