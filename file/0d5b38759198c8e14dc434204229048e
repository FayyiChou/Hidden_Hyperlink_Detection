	 $("#nav_san_span").css("display", "block");
		 if($("#firstcategory0").html()==""||$("#firstcategory0").html()==null||$("#showcategory").val()==0){
				//alert($("#showcategory").val())
			$("#showcategory").val(1);
		 	//showcategory();

		 }
	 });
	 $("#categorys-hd").mouseout(function() {
		 $("#nav_san_span").removeClass("nav_san2");
		 $("#nav_san_span").addClass("nav_san");
		 $("#nav_san_span").css("display", "block");
		 $("#J_categorys_category").css("display", "block");
	 });

	 $("#J_categorys_category").mouseover(function() {
		 $("#nav_san_span").removeClass("nav_san");
		 $("#nav_san_span").addClass("nav_san2");
		  $("#nav_san_span").css("display", "block");
		 $("#J_categorys_category").css("display", "block");
	 });
	 $("#J_categorys_category").mouseout(function() {
		 $("#nav_san_span").removeClass("nav_san2");
		 $("#nav_san_span").addClass("nav_san");
		  $("#nav_san_span").css("display", "block");
		 $("#J_categorys_category").css("display", "block");
	 });
	 
	 $("#childcategory").mouseover(function() {
		 $("#nav_san_span").removeClass("nav_san");
		 $("#nav_san_span").addClass("nav_san2");
		  $("#nav_san_span").css("display", "block");
		 $("#J_categorys_category").css("display", "block");
	 });
	 
	 $("#childcategory").mouseout(function() {
		 $("#nav_san_span").removeClass("nav_san2");
		 $("#nav_san_span").addClass("nav_san");
		  $("#nav_san_span").css("display", "block");
		 $("#J_categorys_category").css("display", "block");
	 });
 
	 $.post("/MallWeb/pages/member/isSaler.jsp",function(result){
		 var res = $.trim(result)
			if(res!="true"){
					$(".ticnav").hide();
					$(".saljia").hide();
		 			//document.getElementById("ticketnav").style.display = "none";
		 		}else{
		 			$(".ticnav").show();
		 			$(".saljia").show();
		 			//document.getElementById("ticketnav").style.display = "";
		 		}
		    });

});
    function clock(){
	     var t = $("#scrollDiv ul li");
		 if(t.size()>1){	//只有一条记录时不滚动
	    	$("#scrollDiv ul").animate({marginTop:"-25px"},1000,function(){
	           $(this).css({marginTop:"0px"}).find("li:first").appendTo(this);      
		    })
        }
    }
    $(function(){
	   var myvar=setInterval("clock()",8000);
	      $("#scrollDiv").mouseover(function(){
	          clearInterval(myvar);                          
	      }).mouseout(function(){
	      	  clearInterval(myvar);
	          myvar = setInterval("clock()",8000);
	   });
    })
    
   //10.20首页改版JS.....
$(function(){
	$('.x_leftBtn').hide();
	var w = $('.x_slidBox').width();
	var l = $('.x_sliderInfo').length;
	var d = $('.x_sliderInfo').width();
		$('.x_slidBox').width(l*d);
		w = l*d;
	var z = w-d;
	
	$('.x_rightBtn').click(function(){
		var newL =$('.x_slidBox').css('left');
		var t = parseInt(newL);
		if(-t == z-d){			
			$('.x_rightBtn').hide();
			
		}
		if(-t == z){			
			$('.x_rightBtn').hide();
			return;
		}
		$('.x_slidBox').animate({'left':t-d});
		$('.x_leftBtn').show();
	});
	$('.x_leftBtn').click(function(){
		var newL =$('.x_slidBox').css('left');
		var t = parseInt(newL);
		if(t == -d){
			$('.x_leftBtn').hide();			
		}
		if(t == 0){				
			return;
		}
		$('.x_slidBox').animate({'left':(t+d)});
		$('.x_rightBtn').show();		
	});
});
</script>
