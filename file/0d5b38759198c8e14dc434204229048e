<script type="text/javascript">
	$(document).ready(function(){
		// 获取当前地址，参数，
		var href = window.location.href;
		var referrer = document.referrer;
		var param ='href:'+ href +' | '+'referrer:'+ referrer;
		$.post("/MallWeb/pages/index/saveIndexRecords.jsp",{"param":param});
		
		$("#J_categorys_category").css("display", "block");
		$("#nav_san_span").css("display", "block");
	
		$("#categorys-hd").mouseover(function() {
			$("#showcategory").val(1);
			 $("#nav_san_span").removeClass("nav_san");
			 $("#nav_san_span").addClass("nav_san2");
			 $("#J_categorys_category").css("display", "block");
			 $("#nav_san_span").css("display", "block");
			 if($("#firstcategory0").html()==""||$("#firstcategory0").html()==null||$("#showcategory").val()==0){
					//alert($("#showcategory").val())
					$("#showcategory").val(1);
				 	//showcategory();
	
			 }
		 });
		 $("#categorys-hd").mouseout(function() {
			 $("#nav_san_span").removeClass("nav_san2");
			 $("#nav_san_span").addClass("nav_san");
			 $("#nav_san_span").css("display", "block");
			 $("#J_categorys_category").css("display", "block");
		 });
	
		 $("#J_categorys_category").mouseover(function() {
			 $("#nav_san_span").removeClass("nav_san");
			 $("#nav_san_span").addClass("nav_san2");
			  $("#nav_san_span").css("display", "block");
			 $("#J_categorys_category").css("display", "block");
		 });
		 $("#J_categorys_category").mouseout(function() {
			 $("#nav_san_span").removeClass("nav_san2");
			 $("#nav_san_span").addClass("nav_san");
			  $("#nav_san_span").css("display", "block");
			 $("#J_categorys_category").css("display", "block");
		 });
		 
		 $("#childcategory").mouseover(function() {
			 $("#nav_san_span").removeClass("nav_san");
			 $("#nav_san_span").addClass("nav_san2");
			  $("#nav_san_span").css("display", "block");
			 $("#J_categorys_category").css("display", "block");
		 });
		 
		 $("#childcategory").mouseout(function() {
			 $("#nav_san_span").removeClass("nav_san2");
			 $("#nav_san_span").addClass("nav_san");
			  $("#nav_san_span").css("display", "block");
			 $("#J_categorys_category").css("display", "block");
		 });

});

</script>

