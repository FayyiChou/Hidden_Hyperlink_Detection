<script type="text/javascript">
$(function() {
    function e(e, n) {
        "undefined" != typeof s_gi && (s.country = "be", s.campaignid = "cookie", s.linkTrackVars = "eVar33,events", s.linkTrackEvents = "event45", s.eVar33 = s.country + ":" + s.campaignid + ":" + e, s.events = "event45", s.tl(this, n, s.campaignid + ":" + e))
    }

	if ($.cookies.get("cookiesaccepted") == null) {
		$("#cookie_bar_new").css("display", "block");

		if(sessionStorage.getItem('previous_url') == null) {
			sessionStorage.previous_url = window.location.href;
				console.log('place session');
		} else {
			if(window.location.pathname == '/be/info/cookies.html') {
				console.log('cookie policy');
			} else if(sessionStorage.getItem('previous_url') == 'https://www.samsung.com/be/info/cookies.html') {
				console.log('cookie policy');
			} else if(sessionStorage.getItem('previous_url') == 'http://www.samsung.com/be/info/cookies.html') {
				console.log('cookie policy');
			} else if(sessionStorage.getItem('previous_url') == window.location.href) {
				console.log('refresh');
			} else {
				console.log('place cookie');
				var n = new Date;
				n.setTime(n.getTime() + 7776e6);
				sessionStorage.removeItem('previous_url');

				$(".cookie_bar").hide();

				if ($.cookies.get("interestischecked") == false) {
					$.cookies.set("cookiesaccepted", "true", {expiresAt: n, domain:".samsung.com"}), e("instellingen:interesses|uit", "o");
					console.log("interestischecked is not checked"); 
				} else {
					$.cookies.set("cookiesaccepted", "true", {expiresAt: n, domain:".samsung.com"});
					$.cookies.set("interestischecked", "true", {expiresAt: n, domain:".samsung.com"}), e("instellingen:interesses|aan", "o");
					console.log("interestischecked is checked"); 
				}
			}
		}
	}

	$( "#settings_btn" ).click(function() {
	    $(".cookie_bar_settings").slideToggle("slow");
	     e("instellingen:open", "o");
	});

	$( "#opslaan_btn" ).click(function() {
	    $(".cookie_bar_settings").slideToggle("slow");
	});
	
	$( "#opslaan_btn" ).click(function() {
		if($('#interesses').prop('checked')) {
			$.cookies.set("interestischecked", "true", {expiresAt: n, domain:".samsung.com"}), e("instellingen:opslaan:interesses|aan", "o");
		} else {
			$.cookies.set("interestischecked", "false", {expiresAt: n, domain:".samsung.com"}), e("instellingen:opslaan:interesses|uit", "o");
			console.log("interestischecked is not checked"); 
		}
	});

	$("#akkoord_btn").click(function() {
		var n = new Date;
		n.setTime(n.getTime() + 7776e6);
		$(".cookie_bar").slideToggle("fast");

		if($('#interesses').prop('checked')) {
			$.cookies.set("cookiesaccepted", "true", {expiresAt: n, domain:".samsung.com"});
			$.cookies.set("interestischecked", "true", {expiresAt: n, domain:".samsung.com"}), e("instellingen:ikbegrijphet:interesses|aan", "o");
			console.log("interestischecked is checked"); 
		} else {
			$.cookies.set("cookiesaccepted", "true", {expiresAt: n, domain:".samsung.com"});
			$.cookies.set("interestischecked", "false", {expiresAt: n, domain:".samsung.com"}), e("instellingen:ikbegrijphet:interesses|uit", "o");
			console.log("interestischecked is not checked"); 
		}
	});

	if ($(window).width() < 468) {
   		$( "<p class=\"read-more\">Lees meer..</p>" ).insertAfter( ".text_wrapper" );
   	}

	$(window).resize(function() {
		if ($(window).width() < 468) {
			if ( $(".read-more").length ) {
	   		} else {
	   			$( "<p class=\"read-more\">Lees meer..</p>" ).insertAfter( ".text_wrapper" );
	   		}
	   	} else {
	   		$( ".read-more" ).remove();
	   	}
	});

	$(".read-more, .text_wrapper").click(function() {
		$('.cookie_bar_content .text_wrapper').css({'height':'auto'});
		$(".read-more").hide();
	});


});
