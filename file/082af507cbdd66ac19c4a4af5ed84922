	function chk_update9(obj)
	{	
		if(obj.xcaty.value == "0" && (obj.xkeyword.value == "" || obj.xkeyword.value == "-- Please Input Keyword --")){
			alert("Must select Category or enter the Keywords!!");
			return false;
		}
	
		obj.submit();
		return true;
	}
