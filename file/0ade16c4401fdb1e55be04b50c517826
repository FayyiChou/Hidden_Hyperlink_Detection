		<script>
			var fss;
			$(function() {
				fss = new ddfullscreenslider({
					sliderid: 'dowebok',
					onslide: function($slide, index) {
						$("#mengban").fadeOut();
						var m = index - 1;
						var n = index + 1;
						if (m == -1) {
							$("#div" + index).css("display", "block");
							$("#div" + n).css("display", "none");
						}
						//当前最大屏数是5
						else if (n == 5) {
							$("#div" + m).css("display", "none");
							$("#div" + index).css("display", "block");
						} else if (m != -1 && n != 5) {
							$("#div" + m).css("display", "none");
							$("#div" + index).css("display", "block");
							$("#div" + n).css("display", "none");
						}
					}
				});
			});
		</script>
		<script>
			$(document).ready(function() {
				var screenWidth = document.body.clientWidth;
				if (screenWidth > 768) {
					$("#downlodLink").click(function() {
						$("#mengban").fadeIn();
						return false; //阻止FF,IE跳转
					})
					$("#Android_2weiClose").click(function() {
						$("#mengban").fadeOut();
					})
				} else if (screenWidth <= 768) {
					document.getElementById('download').onclick = function(e){
						if(navigator.userAgent.match(/(iPhone|iPod|iPad);?/i)){
							//var ifr = document.createElement('iframe');  
							//ifr.src = 'com.12sporting.ForAlipay://';
							//ifr.style.display = 'none';  
							//document.body.appendChild(ifr);
							window.location.href = "com.12sporting.ForAlipay://";
							setTimeout(function() {
								//document.body.removeChild(ifr);
								window.location.href = "https://itunes.apple.com/cn/app/duo-rui-yun-dong/id958061289";
							 }, 2000)
							return;
						}else if(navigator.userAgent.match(/android/i)){
							if(isWeiXin()){
								$("#downlodLink").attr("href", "http://a.app.qq.com/o/simple.jsp?pkgname=com.hnjc.dl");	
							}else{
								var ifr = document.createElement('iframe');  
								ifr.src = 'com.hnjc.dl://';  
								ifr.style.display = 'none';  
								document.body.appendChild(ifr);
								window.setTimeout(function(){
									document.body.removeChild(ifr);
									window.location.href = "http://d1.12sporting.com/apk/hnjc_dl-JianCheng-release.apk";
								},2000)
							}
						}
					};	
				}
			})
			
			function isWeiXin() {
				var ua = window.navigator.userAgent.toLowerCase();
				if (ua.match(/MicroMessenger/i) == 'micromessenger') {
					return true;
				} else {
					return false;
				}
			}
		</script>
