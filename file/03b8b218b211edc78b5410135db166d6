			$.fn.clearAuthFields = function(){
				oV={l:this.find('[name="login"]').val(),p:this.find('[name="password"]').val()};
				this.find(':text,:password').focus(function(){
					o=$(this); n=o.attr('name');
					if (n=='login' && o.val()==oV.l || n=='password' && o.val()==oV.p){
