<script type="text/javascript">
    	$(document).ready(function(e) {
            $(".sa").hover(function(){
				$(this).addClass("sa_hover");
				$(".lanmu").stop(true,false).slideDown(100);
				}, function(){
					$(this).removeClass("sa_hover");
					$(".lanmu").stop(true,false).slideUp(100);
					});
			$(".lanmu>a").click(function(){
					$(".lanmu").css("display","none");
					var tex=$(this).html();
					var lamindex=$(this).index();
					$("#s0").html(tex);
					$("#SearchType").val(lamindex)
					})
        });
    </script>
