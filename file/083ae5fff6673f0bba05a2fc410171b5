$(document).ready(function(e) {
	$(".menu>ul>li").hover(function(){
	$(this).children("a").addClass("hover").parent("li").siblings().children("a").removeClass("hover");
	$(this).children("ul").stop(true,true).slideDown("fast");
	}, function(){
	$(this).children("ul").stop(true,true).slideUp("fast");
	})
	$(".servicelistmain ul li:last").css("border","none");
});
</script>
<div class="banner">
		
		<SCRIPT type=text/javascript>
		var _c = _h = 0;
		$(document).ready(function () {
			$('#play  li').click(function(){
				var i = $(this).attr('alt') - 1;
				clearInterval(_h);
				_c = i;
				//play();
				change(i);       
			})
			$("#pic img").hover(function(){clearInterval(_h)}, function(){play()});
			play();
		})
		function play()
		{
			_h = setInterval("auto()", 8000);    //图片切换间隔时间，6000为毫秒=6秒
		}
		function change(i)
		{
			$('#play li').css({background:"#fff", color:"#000"}).eq(i).css({background:"#e80100", color:"#fff"}).blur();
			$("#pic img").fadeOut('slow').eq(i).fadeIn('slow');
		}
		function auto()
		{   
			_c = _c > 1 ? 0 : _c + 1;    //_c的值+2为显示图片数 最少3张
			change(_c);
		}
		</SCRIPT>
