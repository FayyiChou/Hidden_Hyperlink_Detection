    <script>

    $(function() {
        
        var currentUser = {user_id:'', name:'', platform: '[]'};
        

        listener.on('com.neihanshequ.user', 'change', function(type, user) {
            currentUser = user;
        });

        window.isUserLogin = function() {
            return currentUser && !!currentUser.user_id;
        };

        window.getCurrentUser = function() {
            return currentUser;
        };
    });

    function getPlatform() {
        var user = getCurrentUser();
        var platform = user.platform || '[]';
        if(typeof platform == 'string') {
            platform = JSON.parse(platform);
            user.platform = platform;
        }
        return platform;
    }
    </script>



