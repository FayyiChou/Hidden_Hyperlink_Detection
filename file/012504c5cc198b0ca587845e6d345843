    <script type="text/javascript">

        UserActionModule.init({
            ctn: '.content-wrapper .content',
            isDetailPage: 'detail' == 'detail'
        });

        listener.on('com.neihanshequ.user', 'change', function(type, user) {
            var $loginBtn = $('#loginBtn');
            if(user) {  //login
                $loginBtn.find('a')
                    .attr('href', '/user/'+user.user_id+'/')
                    .text('我的');
                $loginBtn.addClass('show-logout');
            }else {  //logout
                $loginBtn.find('a')
                    .attr('href', "javascript:listener.trigger('com.neihanshequ', 'showLogin');")
                    .text('登录');
                $loginBtn.removeClass('show-logout');
            }
        });
    
        $(function() {


            var hash = location.hash;
            if(hash && hash.indexOf('show-post-window') != -1) {
                hash = hash.substr(1).replace('show-post-window', '');
                var params = hash.split('~');
                listener.trigger('com.neihanshequ', 'showPost', params);
            }

            setTimeout(loadLazyImage, 400);
            $(document.body).find('.timeago').timeago();

            var onClosePopup = function(event) {
                $(event.currentTarget).parents('.popup-window-wrapper').hide();
            };
            $('.popup-window-wrapper .close').click(onClosePopup);
            $('.popup-window-wrapper .mask').click(onClosePopup);
            formatDurationItems();

            var $backTopBtn = $('#backTopBtn');
            $backTopBtn.click(function() {
                window.scrollTo(0, 0);
            });

            $(window).bind('scrollEnd', function(event, data) {
                var t = (data || event._args || {scrollTop: 0}).scrollTop;
                if(t > 600) {
                    $backTopBtn.show();
                }else if(t < 100) {
                    $backTopBtn.hide();
                }
            });

            $('.logout').click(function(event) {
                $.ajax({
                    url: '/auth/ajax_logout/'
                }).done(function() {
                    listener.trigger('com.neihanshequ.user', 'change', null);
                })
                event.stopPropagation();
                event.preventDefault();
                return false;
            });

        });

        initScrollEvents();
    </script>






