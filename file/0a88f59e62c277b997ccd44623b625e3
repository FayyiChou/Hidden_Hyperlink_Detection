                                    var speed=30;
                                    var demo_a = $("#demo_a");
                                    var demo1_a = $("#demo1_a");
                                    var demo2_a = $("#demo2_a");
                                    demo2_a.html(demo1_a.html());
                                    function Marquee_a(){ 
                                        if(demo_a.scrollLeft()>=demo1_a.width())
                                            demo_a.scrollLeft(0); 
                                        else{
                                            demo_a.scrollLeft(demo_a.scrollLeft()+1);
                                        }
                                    } 
                                    var MyMar_a=setInterval(Marquee_a,speed) 
                                    demo_a.mouseover(function() {
                                        clearInterval(MyMar_a);
                                    } )
                                    demo_a.mouseout(function() {
                                        MyMar_a=setInterval(Marquee_a,speed);
                                    } )		
