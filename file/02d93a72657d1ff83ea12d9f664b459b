function change_ent_type (ttype,tlang){   // åùå ïåðåäàåì çíà÷åíèå ÿçûêà ââîäà
var vproc="http://kpfu.ru/portal/portal_supersite.enter_eu";var vparam={p_type:ttype,p_lang:tlang};
$.post(vproc, vparam , function(data) {
        $("#login").empty();
        $("#login").html(data);
        });    
  }
</script>
