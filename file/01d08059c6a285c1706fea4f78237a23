function show_lft_banner(){
var lft_banner=$(".lft-banner");

	var getscrollTop = $(document).scrollTop();
	if (getscrollTop < 70) {
		lft_banner.removeClass("fixed");
	}else{
		lft_banner.addClass("fixed");
	}
}
