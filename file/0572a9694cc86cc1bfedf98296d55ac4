    <script language="JavaScript">

        $(function () {
            $('#ctl00_MainC_tbuserName').focus();
        })

        function save_username() {
            if ($('#cookietime').attr('checked') == true) {
                var username = $('#username').val();
                setcookie('username', username, 3);
            } else {
                delcookie('username');
            }
        }
        var username = getcookie('username');
        if (username != '' && username != null) {
            $('#ctl00_MainC_tbuserName').val(username);
            $('#cookietime').attr('checked', true);
        }

        function checkMe() {
            var userName = document.getElementById("ctl00_MainC_tbuserName").value;
            var pwd = document.getElementById("ctl00_MainC_Tbpwd").value;
            if (userName == "") {
                alert("请输入用户名"); return false;
            }
            if (pwd == "") {
                alert("请输入密码"); return false;
            }
            else {
                save_username();
                return true;
            }
        }
    </script>
