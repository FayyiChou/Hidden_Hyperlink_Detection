   function getArticleKeywords(){
	   var articleId = "800323";
	   var  url='http://mp.api.3g.cnfol.com/'+'api/getArticleKeywords?articleId='+articleId;
	   $.ajax({
                url:url,
                type:"get",
                dataType:"jsonp",
                jsonp: 'jsoncallback',
