										var oTimeOut = document.getElementById("TimeOut");
										if (msg != '') {
											var url='http://mp.3g.cnfol.com/Comment/addComent';
											$.ajax({
					                url:url,
					                type:'post',
					                dataType:'json',
					                data:{'ArticleId':articleId,'UserID':UserID,'weMediaId':weMediaId,'Username':uname,'Content':msg,'Source':'1'},
					                success:function(data){
					                    if (data.sta=='10000') {
																oTimeOut.innerHTML = "<span class=\"CzTimeOutCnt\">" + data.result + '稍后可查看' + "</span>";
																oTimeOut.style.display = 'block';
																setTimeout(function() {
																		oTimeOut.style.display = 'none'
																},
																800);
																$('#commit').val('');
					                    }else{
					                        alert(data.result);
					                        //window.location.reload()
					                    };
					                }
					            })
										} else {
												alert('请输入评论内容！');
												return;
										}
								} else {
										alert('请您登陆后参与评论！');
										window.location.reload(true);
								}
						}
				</script>

		</article>
