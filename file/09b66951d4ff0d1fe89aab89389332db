   <!--首页城市搜索功能实现 start--> 
   <script>
    //html展现
    var areaData;
    $.ajax({
        type: "GET",
        url: "/tools/submit_ajax.ashx?action=cityArray",
        dataType: "json",
        success: function (data) {
            areaData = data.msg;//已经分好组的城市数据

            var provinceFirstLetterArrs = data.provinceFirstLetterArrs;//所有省份首字母
            for (var i = 0; i < provinceFirstLetterArrs.length; i++) {
                $("#letArea1").append($('<a>', { href: 'javascript:void(0)', text: provinceFirstLetterArrs[i], data: 'anchor' }));
            }

            var is_city = $('#hiddIsSite').val();//判断是否是城市站页
            //城市点击事件
            $("#letArea1 a").click(function () {
                var self = this;
                $(".cityLet").each(function (i, item) {
                    if ($(item).find('.letter').first().text() == $(self).text()) {
                        $("#cityArea1")[0].scrollTop = $(".cityLet")[i].offsetTop;
                    }
                })
            })

            var s = ''
            for (var i = 0; i < provinceFirstLetterArrs.length; i++) {
                s += '<dl class="cityLet">';
                var isFirs = true;
                for (var j = 0; j < areaData.length; j++) {
                    if (areaData[j].proFirstLetter == provinceFirstLetterArrs[i]) {
                        s += '<dt class="pCity">';
                        s += '<span class="province">' + areaData[j].pro + '：</span>'
                        if (isFirs) {
                            s += '<span class="letter">' + provinceFirstLetterArrs[i] + '</span>';
                            isFirs = false;
                        }
                        s += '</dt><dd class="citys">';
                        for (var k = 0; k < areaData[j].citys.length; k++) {
                            if (is_city == "1") {
                                //如果是城市站，则切换效果为直接跳转至选中的城市
                                s += '<a href="/' + areaData[j].citys[k].ab + '">' + areaData[j].citys[k].name + '</a>';
                            }
                            else {
                                //如果是官网首页，则只是切换页面显示的城市内容
                                s += '<a onclick="ChangeCity(\'' + areaData[j].citys[k].ab + '\')" href="javascript:void(0);" cno="' + areaData[j].citys[k].cno + '">' + areaData[j].citys[k].name + '</a>'
                            }
                        }
                        s += '</dd>';
                    }
                }
                s += '</dl>'
            }
            $('#cityArea1').append(s);
            $(".cityLet").each(function (i, item) {
                if ($(item).html() == "") {
                    $(item).remove();
                }
            })
        }
    });
    
    //搜索事件
    $(function () {
        $('#sCity1').focus(function (e) {
            if (this.value == "请输入城市名") {
                this.className = "on";
                this.value = "";
            } else {
                search(e);
            }
        });
        $('#sCity1').blur(function () {
            if (this.value == "") {
                this.className = "";
                this.value = "请输入城市名";
            }
        });
        $('#sCity1').keyup(function (e) {
            search(e);
        })
    })
    //搜索事件的执行
    function search(e) {
        e = e || window.event;
        if (e.keyCode == 40 || e.keyCode == 38 || e.keyCode == 13)
            return;
        var count = 0, val = $('#sCity1').val().replace(/\s/g, ""),
                resultHtml = "";
        if (val) {
            for (var i = 0; i < areaData.length; i++) {
                var record = areaData[i].citys;
                for (var j = 0; j < record.length; j++) {
                    var curCity = record[j]
                            ,
                            cityName = record[j].name
                            ,
                            pinyin = record[j].pinyin,
                            ab = record[j].ab;
                    if (cityName.indexOf(val) > -1 || pinyin.indexOf(val) > -1 || ab.indexOf(val) > -1) {
                        if (count >= 10) {
                            break;
                        }
                        resultHtml += '<a href="javascript:void(0)" onclick="ChangeCity(\'' + ab + '\')" cno="' + curCity.cno + '">' + cityName + '   ' + pinyin + '</a><br/>';
                        count++;
                    }
                }
            }
            if (count > 0) {
                $("#resultList1").html(resultHtml);
                $("#resultList1").show();
                $("#tipNo1").hide();
            } else {
                $("#tipNo1").show();
                $("#resultList1").hide();
            }
        } else {
            $("#tipNo1").hide();
            $("#resultList1").hide();
        }
    }
</script> 
