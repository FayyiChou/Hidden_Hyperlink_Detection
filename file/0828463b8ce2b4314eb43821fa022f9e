    <script type="text/javascript">
     var wait;
	jQuery(document).ready(function(){
       jQuery(".yz_z_xln").click(function(){
           jQuery(".pl_yz").slideToggle();
       });
  
   jQuery(".yz_srsj_hqyzm").click(function(){
       jQuery(".yz_srsj_hqyzm").hide();
       jQuery(".yz_srsj_zzhq").show();
       wait=setInterval(getnums, 1000);  
    });
});

 function getnums(){
    if((jQuery("#times").text()-0)>0)
      {   
         jQuery("#times").text(jQuery("#times").text()-1);
      } 
    else
      {
         jQuery(".yz_srsj_hqyzm").show();
         jQuery(".yz_srsj_zzhq").hide();
         jQuery("#times").text("60");
         clearInterval(wait);
      } 
};

    </script>
	
