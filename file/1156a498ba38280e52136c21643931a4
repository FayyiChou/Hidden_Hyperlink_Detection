   baidu.dom.ready(function(){
    //adjust height
    setTimeout(function(){
     var m= baidu.dom.query('.l-main-col')[0];mht=m.clientHeight,lht = baidu.dom.query('.l-right-col')[0].clientHeight;
         parseInt(mht) < parseInt(lht) && baidu.dom.setStyle(m, 'height', lht);
     },500);
    var __el = baidu.g('load_more'), __parent = baidu.g('feedList'),disable=false ,last_id = 0 ;
    function loadComment(){
        if(disable == false){
            disable = true;
            __el.innerHTML='<a class="WB_loadingLine" href="#" onclick="return false;" action-type="load_more"><span class="WB_loadingMini"></span><span class="WB_icon_txt">正在加载</span></a>';
            baidu.ajax.get("/ajax/commentlist?url="+encodeURIComponent(window.location.href)+"&commentid="+last_id,function(h,r){
                var req = baidu.json.parse(r);
                if(req.errno == 0 && req.data.list.length > 0 ){
                    last_id = req.data.last_id ; 
                    var tpl=baidu.template('new-comment-list', req.data);
                    baidu.dom.insertHTML(__parent,'beforeEnd',tpl);
                    __el.innerHTML='<a class="WB_loadingLine WB_loadingNow" href="#" onclick="return false;" action-type="load_more" ><em>更多</em></a>';
                    disable = false;
                }else if(req.errno != 0 ){
                    __el.innerHTML='<a class="WB_loadingLine" href="#" onclick="return false;" ><span class="WB_tipS_err"></span><span class="WB_icon_txt">加载失败，请重试</span></a>';
                }else if(req.data.list.length == 0 && last_id == 0 ){
                    __el.innerHTML='<div class="wc_list no_comments"><p><span class="WB_tipS_warn"></span><span class="WB_icon_txt">还没有人评论过，赶快抢沙发吧！</span></p></div>';
                }else{
                    __el.innerHTML='<a class="WB_noContent" href="#" onclick="return false;">没有更早的评论了</a>';
                }
            });
        }	
    }
    __el && baidu.event.on(__el, 'click', function(e){
        loadComment();
    });
    loadComment();

    var __up = baidu.g('up_article') , __down = baidu.g('down_article'), __up_num = baidu.g('up_btn_num') , __down_num = baidu.g('down_btn_num'),up_down_flag = false;

    function num2str(str){
        var num = (str*1) + 1 ; 
        return num ; 
    }
    function upvsdown(action){
        var url = "nid=2659169379944023585&type="+action;
        baidu.ajax.post("/ajax/newssupport",url,function(h,r){
            var req = baidu.json.parse(r);
            if(req.errno == 0 ){
                if(action == "up"){
                    __up_num.innerHTML = num2str(__up_num.innerHTML); 
                }else{
                    __down_num.innerHTML = num2str(__down_num.innerHTML); 
                }
                up_down_flag = true ; 
                if(action == "up"){
                    baidu.dom.removeClass('up_icon','up_gray');
                    baidu.dom.addClass('up_icon','up');
                }else{
                    baidu.dom.removeClass('down_icon','down_gray');
                    baidu.dom.addClass('down_icon','down');
                }
                /*baidu.dom.addClass("up_article","up_down_btn_gray");
                baidu.dom.addClass("down_article","up_down_btn_gray");
                baidu.dom.addClass("up_btn_num","up_down_btn_num_gray");
                baidu.dom.addClass("down_btn_num","up_down_btn_num_gray");
                baidu.dom.removeClass("up_txt","up_down_txt_hover");
                baidu.dom.removeClass("down_txt","up_down_txt_hover");*/
            }
        });
    }
    __up&&baidu.event.on(__up,'click',function(e){
        if(up_down_flag){return false;}
        upvsdown("up");
    });
    __down&&baidu.event.on(__down,'click',function(e){
        if(up_down_flag){return false;}
        upvsdown("down");
    });

    $('.authorRssBtn').click(function(e){
        if(!$("#login").is(":hidden")){
            baidu.g("login").click();
            return false;
        }
        var  = $(this),
            txt = .find('.b').text(),
            changeTxt,changeClass,status;
        if(txt == '已关注'){
            status = 2;
            changeTxt = '关注';
            changeClass = 'i iadd';
        }else if(txt == '关注'){
            status = 1;
            changeTxt = '已关注';
            changeClass = 'i iminus';
        }
        // baidu.ajax.get("/ajax/subscribe?uid="+uid+"&mid="+mid+"&type=1&status="+status+"&t="+(+new Date),function(h,r){
        //         var res = baidu.json.parse(r);
        //         if(res.errno == '0'){
        //             .find('.b').text(changeTxt);
        //             .find('.i').removeClass().addClass(changeClass);

        //         }
            
        // });
        $.ajaxSetup({ cache: false });
        $.ajax({
            url: "/ajax/subscribe?uid="+uid+"&mid="+mid+"&type=1&status="+status,
            type: 'GET',
            dataType: 'json',
            success: function(res){
                if(res.errno == '0'){
                    .find('.b').text(changeTxt);
                    .find('.i').removeClass().addClass(changeClass);
                }
            }
        });

    });

    $.each($('.videoArea'),function(i,e){
