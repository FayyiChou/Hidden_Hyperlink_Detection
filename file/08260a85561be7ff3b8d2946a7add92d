		<script type="text/javascript">
		var username = unescape(decodeURI(getCookie("username2")));
		if (username == "")
			var str = '<span class="gray" style="color:#031948">欢迎来到中国二手游艇网！</span>';
		else 
			var str = '<span class="gray">您好，'+username+'</span>&nbsp;<a href="register/logout.asp" target="_self" >退出</a>';
		$("#login").html(str);
		
		function getCookie(name)
		{
			var strCookie = document.cookie;
			var arrCookie = strCookie.split("; ");//notice the space after the ';'
			for (var i=0;i<arrCookie.length;i++)
			{
				var arr = arrCookie[i].split("=");
				if (arr[0] == name) return arr[1];
			}
			return "";
		}
		</script>
