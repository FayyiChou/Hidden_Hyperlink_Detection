				$(document).ready(function(){
					$("#friend_partner").hover(function(){
						$(this).removeClass('button6_hover');
						$(this).addClass('button6');
						$("#friend_link").removeClass('button6');
						$("#friend_link").addClass('button6_hover');
						$("#partner").show();
						$("#friendlink").hide();
					});
					$("#friend_link").hover(function(){
						$(this).removeClass('button6_hover');
						$(this).addClass('button6');
						$("#friend_partner").removeClass('button6');
						$("#friend_partner").addClass('button6_hover');
						$("#partner").hide();
						$("#friendlink").show();
					});
