							if(srcH<=toH)
							{
								ftoW=srcW;
								ftoH=srcH;
							}
							else
							{
								ftoW=(srcW*toH)/srcH;
								ftoH=srcH;
							}
						}
					}
					else{
						if(srcH>toH){
							ftoH=toH;
							ftoW=(srcW*toH)/srcH;
						}else{
							if(srcW<=toW)
							{
								ftoW=srcW;
								ftoH=srcH;
							}
							else
							{
								ftoW=toW;
								ftoH=srcH;
								ftoH=(srcH*toW)/srcW;
							}
						}
					}
				}
				
		        $this.css({"width":ftoW,"height":ftoH}).attr("src",imgsrc).show();
			}	
			
