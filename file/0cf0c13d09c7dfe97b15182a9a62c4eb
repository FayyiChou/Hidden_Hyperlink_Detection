<script type="text/javascript">
    var speed = 30
    mdemo2.innerHTML = mdemo1.innerHTML//克隆demo1为demo2
    function Marquee1() {
        if (mdemo2.offsetHeight - mdemo.scrollTop <= 0)//当滚动至demo1与demo2交界时
            mdemo.scrollTop -= mdemo1.offsetHeight//demo跳到最顶端
        else {
            mdemo.scrollTop++
        }
    }
    var MyMar1 = setInterval(Marquee1, speed)//设置定时器
    mdemo.onmouseover = function() { clearInterval(MyMar1) } //鼠标移上时清除定时器达到滚动停止的目的
    mdemo.onmouseout = function() { MyMar1 = setInterval(Marquee1, speed) } //鼠标移开时重设定时器
    
</script>


            
    
