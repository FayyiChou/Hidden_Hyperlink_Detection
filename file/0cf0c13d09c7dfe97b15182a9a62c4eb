    <script type="text/javascript">
        function Inittpnews(imgUrl, imgLink, imgText, targetId) {
            var xb;
            var pics = "";
            var links = "";
            var texts = "";

            //焦点图顺序调整：改变数组值的顺序即可改变焦点图显示顺序。（注意数组的第一个元素值保持为空，从第二个开始填写顺序。）
            var order = new Array("");
            for (var m = 1; m < imgUrl.length; m++) {
                order[m] = m + "";
            }
            var focus_width = 300;
            var focus_height = 233;
            var text_height = 20;
            var swf_height = focus_height + text_height;

            var j = 0;
            for (i = 1; i < imgUrl.length; i++) {
                xb = order[i];
                if ((imgUrl[xb] != "")) {
                    if (j != 0) {
                        pics = pics + "|";
                        links = links + "|";
                        texts = texts + "|";
                    }
                    pics = pics + imgUrl[xb];
                    links = links + imgLink[xb];
                    texts = texts + imgText[xb];
                    j++;
                }
            }
            var htmstring = '<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0" width="' + focus_width + '" height="' + swf_height + '">';
            htmstring += '<param name="allowScriptAccess" value="sameDomain"><param name="movie" value="fla/pixviewer.swf"><param name="quality" value="high"><param name="bgcolor" value="#eeeeeeee">';
            htmstring += '<param name="menu" value="false"><param name=wmode value="opaque">';
            htmstring += '<param name="FlashVars" value="pics=' + pics + '&links=' + links + '&texts=' + texts + '&borderwidth=' + focus_width + '&borderheight=' + focus_height + '&textheight=' + text_height + '">';
            htmstring += '<embed src="fla/pixviewer.swf" wmode="opaque" FlashVars="pics=' + pics + '&links=' + links + '&texts=' + texts + '&borderwidth=' + focus_width + '&borderheight=' + focus_height + '&textheight=' + text_height + '" menu="false" bgcolor="#eeeeee" quality="high" width="' + focus_width + '" height="' + swf_height + '" allowScriptAccess="sameDomain" type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer" />';
            htmstring += '</object>';
            $("#" + targetId).html(htmstring);
        }
    </script>

