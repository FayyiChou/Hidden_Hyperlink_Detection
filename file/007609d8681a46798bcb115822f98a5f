  </div>
  <!-- 弹层 --> 
</div>

<script type="text/javascript" src="http://newhouse.js.soufunimg.com/newhouse/js/esf/souchang_share.js"></script>


<script>
var citysuo = "nc.";
function sousuo(){
	var key = $("#input_keyw1").val();
	window.open('http://esf.'+citysuo+'fang.com/EsfHouse/Project/list_new.aspx?searchtype=0&keyword='+key);
}
$('.apply-btn').click(function(){
		$(".layer").show();	
		$(".tancengbg").show();			
})

$('.layer-close').click(function(){
		$(".layer").hide();				
		$(".tancengbg").hide();				
})

$('.bottom-close').click(function(){
		$(".bottom-txt").hide();						
})
 </script>
<script type="text/javascript">
$(".photoFloatingwrap .floatext li").click(function(){
$(this).addClass("clickselected").siblings().removeClass("clickselected");
})
</script>
<script type="text/javascript">
var aFloatLi=$(".floatext li").length;

    var arr = [];
    $(function () {
        for (i = 0; i < aFloatLi; i++) {
            arr.push($('#part_' + i).offset().top);//相册左部title,将每一个title居顶部的距离存入数字arr中；
        }
        $(window).scroll(NavTopScroll);
		heightTotal();
    });
	function heightTotal(){
		
				var lineHeight=40+35*i;
				
				var botcircleHeight=40+35*i-15;
				
                   $(".line").css("height",lineHeight);
                   $(".bottomcircle").css("top",botcircleHeight);
		}
    function NavTopScroll() {
        var scrTop = $(window).scrollTop();
        for (var i = 0; i < aFloatLi; i++) {
            if (scrTop <=arr[i]) {
                  var lithat=$('#part_' + i + '_logo').parents("li");
                  lithat.addClass("clickselected").siblings().removeClass("clickselected");
				  $("#circle"+i).addClass("redcircle").closest("li").siblings().children().removeClass("redcircle");
                  break;
            }
        }
    }
</script>
</body></html>