<script type="text/javascript">
	$(document).ready( function () {
		setTimeout(function () {
			$("#blurry_face").fadeOut(1500,function () {/*$("#blurry_face").remove();*/});
		},3000);

		start_blink();
	});

	function start_blink()
	{
		$("#blink_open").show();
		setTimeout(do_blink,((Math.random()*30)+10) * 1000);
	}

	function do_blink()
	{
		 $("#blink_open").hide();
		 setTimeout(start_blink,1000);
	}
</script>
