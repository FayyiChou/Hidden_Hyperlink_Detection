<script>
$(window).load(function () {
	$("input[name='s']").focus(function () {
		if($(this).val() == "Search Site >")
			$(this).val("");
	});

	$("input[name='s']").blur(function () {
		if($(this).val() == "")
			$(this).val("Search Site >");
	});

	$(".select-menu").change(function () {
		var $self = $(this);
		var val = $self.val();
		if(val != "0" && val != "")
		{
			window.location = "/"+$(this).val();
		}
	});


});
</script>
