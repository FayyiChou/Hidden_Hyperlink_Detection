<script>
    function searchkey() {
        var key = $("#keyword").val();
        if (key != "") {
            location.href = "/bao/search.aspx?keyword=" + key + "";
        } else {
            alert("请输入查询关键字");
        }
    }
    function onsearchaaa(key) {

        location.href = "/bao/search.aspx?keyword=" + encodeURI(key) + "";
    }
    function search_key_down(e) {
        /* 处理回车事件 */
        var kc = window.event ? e.keyCode : e.which;
        if (kc == 13) {

            return false;
        }
    }
</script>
