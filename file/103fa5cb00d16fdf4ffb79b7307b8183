<script type="text/javascript">
    var showDialogJspIndex=0;
    $(function () {
        loginSelectEnrollmentYearLoad();
        //placeholder修复ie不支持
        JPlaceHolder.init(); 
        //密码显示隐藏
        $(".show-pw-btn").click(function () {
            if (!$(this).hasClass("blur")) {
                $(this).addClass("blur");
                var pwval = $(this).siblings(".pw-ipt-hide").attr("value");
                $(this).siblings(".pw-ipt-show").attr("value", pwval).show();
                $(this).siblings(".pw-ipt-hide").hide();
            } else {
                $(this).removeClass("blur");
                var pwval = $(this).siblings(".pw-ipt-show").attr("value");
                $(this).siblings(".pw-ipt-hide").attr("value", pwval).show();
                $(this).siblings(".pw-ipt-show").hide();
            }
        })
        //登录注册切换
        $(".lg-rg-switch-tab").delegate("span", "click", function () {
            var $this = $(this);
            if (!$this.hasClass("cur")) {
                $this.siblings("span").removeClass("cur");
                $this.addClass("cur");
                $(".lg-rg-content-wrap").hide();
                $(".lg-rg-content-wrap").eq($this.index()).show();
                userLoginNew.clearLayerHtml();
            }
        })


        //错误提示
//		$(".lg-rg-operate-btn").on("click",function(){
//			$(".form-ipt-error").addClass("is-visible");
//			$(".misc-supplement-wrap").slideDown();
//		})

    })
    /**
     * 加载入学年份
     */
    function loginSelectEnrollmentYearLoad() {
        var year = (new Date()).getFullYear();
        var yearStr = "<option  value='-1' selected='selected'  >入学年份</option> " +
                "<option value='" + year + "'>" + year + "</option>" +
                "<option value='" + (year - 1) + "'>" + (year - 1) + "</option>" +
                "<option value='" + (year - 2) + "'>" + (year - 2) + "</option>" +
                "<option value='" + (year - 3) + "'>" + (year - 3) + "</option>" +
                "<option value='" + (year - 4) + "'>" + (year - 4) + "</option>" +
                "<option value='" + (year - 5) + "'>" + (year - 5) + "</option>" +
                "<option value='" + (year - 6) + "'>" + (year - 6) + "</option>" +
                "<option value='" + (year - 7) + "'>" + (year - 7) + "</option>" +
                "<option value='" + (year - 8) + "'>" + (year - 8) + "</option>" +
                "<option value='" + (year - 9) + "'>" + (year - 9) + "</option>";
        $("#loginSelectEnrollmentYear").html(yearStr);
        $("#registerSelectEnrollmentYear").html(yearStr);
        $("#completeSelectEnrollmentYear").html(yearStr);

    }

    //判断浏览器是否支持 placeholder属性
    var JPlaceHolder = {
	    //检测
	    _check : function(){
	        return 'placeholder' in document.createElement('input');
	    },
	    //初始化
	    init : function(){
	        if(!this._check()){
	            this.fix();
	        }
	    },
	    //修复
    	fix : function(){
	        $(':input[placeholder]').not(".hidden").each(function(index, element) {
	            var self = $(this), txt = self.attr('placeholder');
	            var pos = self.position(), h = self.outerHeight(true), paddingleft = self.css('padding-left'),fontsize = self.css('font-size');
	            var holder = $('<span></span>').text(txt).addClass("placeholder").css({position:'absolute', left:"0px", top:"0px", height:h+'px', lineHeight:h+'px', paddingLeft:paddingleft, color:'#999999',fontSize:fontsize}).appendTo(self.parent());
	            if(!self.val()){
	            	holder.show();
	            }else{
	            	holder.hide();
	            }
	            self.focusin(function(e) {
	            }).focusout(function(e) {
	                if(!self.val()){
	                    holder.show();
	                }
	            });
	            holder.click(function(e) {
	                self.focus();
	            });
                self.keyup(function(){
                	holder.hide();
                });
	        });
	    }
	};
</script>



<script typs="text/javascript" src="https://assets.zhihuishu.com/pace/1.0.2/pace.min.js"></script>
<script type="text/javascript" src="/resources/js/user/common.js"></script>
<script type="text/javascript" src="/resources/js/user/tm_dialog2.1.js"></script>

<script type="text/javascript">
    var hSchoolName = "烟台大学";
    var hSchoolCode = "ytu";
    var hSchoolId = "312";
    var headType = "1";
    var isComplete="";
    var uSchoolId="";
    var uCollegeId="";
    var uGrade="";
    var uCode="";
    var uRealName="";
    var uId="";
    $(document).ready(function () {
    	//点击搜索按钮
        $(".schIndexInner_box .menu-search-btn").bind("click", function () {
            if (!$(this).hasClass("blur")) {
                $(this).addClass("blur");
                $(".menu-search-wrap").fadeIn(300);
                $(".menu-search-ipt input").focus();
            } else {
                $(this).removeClass("blur");
                $(".menu-search-wrap").fadeOut(300);
            }
        });
      	//菜单首屏不显示背景,向下滚动隐藏菜单栏，反之显示
    	var nowScrollTop=0;oldScrollTop=0;
    	var maxScrollValue = document.documentElement.scrollHeight - $(window).height();
        $(window).on('scroll', function(){
        	if($(window).scrollTop()>170){
        		$(".schIndex_head").addClass("schIndex_head_hasbg");
        	}else{
        		$(".schIndex_head").removeClass("schIndex_head_hasbg");
        	}
        	if(navigator.userAgent.indexOf('MSIE') < 0 ){
        		nowScrollTop = document.documentElement.scrollTop || window.pageYOffset || document.body.scrollTop;
                if(oldScrollTop>=0 && nowScrollTop >=0 && oldScrollTop<=maxScrollValue){
                	if(oldScrollTop < nowScrollTop){
                		$(".schIndex_head").css("top","-80px");
                    	if($(".menu-search-btn").hasClass("blur")){
                    		$(".menu-search-btn").removeClass("blur");
                    		$(".menu-search-wrap").hide();
                    	}
	                }
	                else{
	                	$(".schIndex_head").css("top",0);
	                }
                }
                oldScrollTop = nowScrollTop;
        	};
        });
    });
    //重写js
    if (typeof String.prototype.startsWith != 'function') {
        String.prototype.startsWith = function (prefix) {
            return this.slice(0, prefix.length) === prefix;
        };
    }
    if (typeof String.prototype.endsWith != 'function') {
        String.prototype.endsWith = function (suffix) {
            return this.indexOf(suffix, this.length - suffix.length) !== -1;
        };
    }
</script>
