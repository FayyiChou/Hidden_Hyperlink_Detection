    var newTermId = "7";
    $(function () {
        //返回顶部
        $(".go-top").click(function () {
            $('body,html').animate({scrollTop: 0}, 800);
            return false;
        })
        $(window).on('scroll', function () {
            if ($(window).scrollTop() > 800) {
                $('.go-top').show();
            } else {
                $('.go-top').hide();
            }
        });
        //选课完成后导学小视频播放
        $(".study-video-button-play-guide").click(function(){
        	$("#learning-guide-video").html("");
			$("#learning-guide-video").Ableplayer({
