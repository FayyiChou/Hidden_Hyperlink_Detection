//alert(window.navigator.appName)
//alert(window.navigator.appVersion)
function test_iever(){ //检测是否是IE 
	if (window.navigator.appName!='Microsoft Internet Explorer'){
		alert("由于您用的不是Microsoft Internet Explorer浏览器，所以不能顺利报名，请更换。\n如果您用的是IE10或IE11浏览器，请参照首页左下角“IE10/IE11兼容视图设置方法”把本网站添加入兼容视图。")
	}
}
test_iever()
</script>
<script>
ecode=new Array()
ebmurl=new Array()
bmurl=''
function wszx(sf){
	if (sf.length==2){
		for (i=0;i<0;i++){
			if (ecode[i]==sf){
				bmurl=ebmurl[i]
				break
			}
		}
		document.form1.action=bmurl+"/ksbm/wszx.jsp" 
		//alert(document.form1.action)
		document.form1.submit()
	}
}
</script>
