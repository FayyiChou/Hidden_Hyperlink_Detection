function onLuceneSearch(){
    //var select = document.getElementById("select").value;
	if(document.all.skey.value==''){
		alert('请输入关键字');
		return false;
	} else {
		var encodeURI_keyword = encodeURI(encodeURI(document.all.skey.value));
		window.open('/cms/cmsWebapp/jsp/lucene/qwjs_jg.jsp?siteunid=C830971CF883E6DE084BC293558DA1B7&keyword=' + encodeURI_keyword);
	}
}
