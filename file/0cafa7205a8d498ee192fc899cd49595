   <script>
    	$(function(){
			$(".sousuo").focus(function(){
					$(this).val("");
					
				})
			$(".sousuo").blur(function(){
					var vv = $(this).attr("placeholder");
					
					var bv = $(this).val();
					var reg = /^\s+/;
					if(reg.test(bv) || bv == ''){
							$(this).val(vv);
							
						}else{
							$(this).val(bv);
							}
					
				})	
			
			$("input[type='image']").click(function(){
					var v = $(this).parent().prev(".sousuo").val();
					
					var vv = $(this).parent().prev(".sousuo").attr("placeholder");
					
					var reg = /^\s+/;
					if(reg.test(v) || v == vv || v == ''){
							alert("对不起，内容的不能为空");
							return false;
						}
					
				})
			
			
			})
    </script> 
