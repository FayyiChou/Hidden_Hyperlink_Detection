        function _a_recommrela() {
            $('.recommend-box').find('ul').html('');
            
            var url = '/u/xj85176432/blog/RecommRelation?rnd=' + Math.random();
            var ajax = {
                url: url, type: "POST", dataType: "json",
                data: { c: $('#hideArticleCode').val(), t: encodeURIComponent($('#hideTitle').val()),g:encodeURIComponent($('#hideTags').val()),n:8 },
                success: function(data, textStatus) {
                    $.each(data, function(i) {
                        $('<li class="color' + (i > 3 ? 2 : 1) + '"><a href="http://i.sodao.com/'+data[i]._asr_username+'/blog-'+data[i]._asr_code+'.html" target="_blank" title="' + data[i]._asr_title + '">' + data[i]._asr_title + '</a></li>').appendTo($('.recommend-box').find('ul'));
                    });
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    $('<li>数据请求时发生错误，请检查' + errorThrown + '</li>').appendTo($('.recommend-box').find('ul'));
                }
            };
            $.ajax(ajax);
        }
        //复制链接
        function CopyURL(){
            if(window.clipboardData){
                var myHerf = $.trim($("#linksrc").val());
                var ok = window.clipboardData.setData("Text",myHerf);
                if(ok)
                    alert("复制成功!");
            } else {
                alert("对不起，目前此功能只支持IE，请使用Ctrl+C复制地址!");
            }
        }
       
            

