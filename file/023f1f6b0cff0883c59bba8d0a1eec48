    <script type="text/javascript">
        function maxWidthHeight(divbox, maxwidth, maxheight) {
            var maxwidth = parseInt(maxwidth);
            var maxheight = parseInt(maxheight);
            var outwidth;          //输出宽度
            var outheight;         //输出高度
            var width;             //原图片宽度 
            var height;            //原图片高度

            $("#" + divbox + " img").each(function (i, n) {
                width = parseInt($(this).width());
                height = parseInt($(this).height());

                var rateWidth = maxwidth / width;
                var rateHeight = maxheight / height;

                if (rateWidth > rateHeight) {
                    outheight = maxheight;
                    outwidth = width * rateHeight;
                }
                else {
                    outheight = height * rateWidth;
                    outwidth = maxwidth
                }
                $(this).attr("width", outwidth);
                $(this).attr("height", outheight);
            });
        }
    </script>
    
