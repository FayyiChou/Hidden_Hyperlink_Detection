<script type="text/javascript">
    /*临时处理日志评论*/
    $(".blogTit-comNum-hook").text($(".blogCom-comNum-hook").text());

    var currentLoginUserId = "0";
    var cmtdetailpage_pager = new sodaopager(document.getElementById('cmtdetailpager'));
    cmtdetailpage_pager.linkCss = 'page';
    cmtdetailpage_pager.currentCss = 'current';
    cmtdetailpage_pager.prevnextCss = 'turn';
    cmtdetailpage_pager.currentPage = 1;
    cmtdetailpage_pager.pageClick = cmtdetailpage_get;
    var cmtdetailpage_pager_pageSize = 20;
    var cmtdetailpage_pager_totalRows = 3;
    var cmtdetailpage_pager_feedtype = 1;
    var cmtdetailpage_pager_feedid = 0;
    var cmtdetailpage_pager_objcode = "c720ce2ab878dfa49c";
    //first
    cmtdetailpage_pager.pageSize = cmtdetailpage_pager_pageSize;
    cmtdetailpage_pager.currentPage = 1;
    cmtdetailpage_pager.totalRows = cmtdetailpage_pager_totalRows;
    cmtdetailpage_pager.output();
                
    function cmtdetailpage_get(pg) {
      if (pg == null) {
          pg = 1;
      }
        var url = 'http://www.sodao.com/dynamic/commentsinglelist?jsonp=?';
        var ajax = {
        url: url,
        data:{
             "page":pg,
            "pagesize":cmtdetailpage_pager_pageSize,
            "clu": currentLoginUserId,
            "feedtype":cmtdetailpage_pager_feedtype,
            "feedid": cmtdetailpage_pager_feedid,
            "objcode":cmtdetailpage_pager_objcode,
            "rnd":Math.random()
            },
            type: "GET",
            dataType: "jsonp",
            success: function(data, textStatus) {
                if(data.result == 1)
                {
                    $("#cmtdetailpage_container").html(data.html);
                    cmtdetailpage_pager.currentPage = pg;
                    cmtdetailpage_pager.output();
                }
            },
            error: function(XMLHttpRequest, textStatus, errorThrown) {
                }
        };
        $.ajax(ajax);
    }
</script>


