							"abbr,acronym{border:0;}" +
							"sup,sub{line-height:0;}" +
							"pre,code,tt{font:12px 'andale mono', 'lucida console', monospace;line-height:1.5;}" +
							"body{font-size:14px; line-height:1.8; font-family:Tahoma; word-wrap:break-word;}" +
							"img{max-width:650px;}";
			var style = $("#iframe_content")[0].contentWindow.document.createElement("style");
			if ($.browser.msie) {
				style.type = "text/css";
				style.media = "screen"
			};
			if (style.styleSheet) { //ie
				style.styleSheet.cssText = styles;
			} else {
				style.appendChild(document.createTextNode(styles));
			}
			$("#iframe_content")[0].contentWindow.document.getElementsByTagName("head")[0].appendChild(style);					
			$("#iframe_content").contents().find("body").append(content);			
            SetFrameHight();
//           
          
          SDweibo.initCommentTextarea($("#SD-weibo-comment"));
            SDweibo.initCommentFace($("#SD-comment-detailface"), $("#SD-weibo-comment"));
            /*IE6下动态图片缩放处理*/
            if (SOD.ie6test()) {
                $("img.imgicon").LoadImage({
                    scaling: true,
                    width: 150,
                    height: 140,
                    loadpic: "http://css.sodao.com/home/img/weibo-loading.gif"
                });
            }
            
        }

        function SetFrameHight() {
            height = $("#iframe_content").contents().find("body").prop('scrollHeight');
            $("#iframe_content").css("height",height < 100 ? 100 : height);

            //修改打开方式，打开新窗口
            $("a", $(window.frames["iframe_content"].document)).attr('target','_blank');   
            
            setTimeout(function() {
                SetFrameHight();
            }, 1000);
        }
        
        function getTotalHeight(){
             
            if($.browser.msie){
                return document.compatMode == "CSS1Compat"? document.documentElement.clientHeight :
                         document.body.clientHeight;
            }else{
                return self.innerHeight;
            }
        }
        
        /*分享*/
        function _a_share(obj) {
            if (_AG.isLogin == 0) {
                sDialog.loadIFrame("/User/LoginWindows?LoginWinFromUrl="+encodeURIComponent(top.location.href), "搜道网 因为真实 所以精彩！赶快注册帐号登录，体验更多精彩乐趣！", 550, 230, true);
                return false;
            }
            if ($(obj).attr('a_shared') == "1") {
                sDialog.message('已经分享', "提示", 1500, 180, 0);
                return false;
            }
            var content = $(obj).next().html();
            SoDaoShare.ShareBlog('', encodeURIComponent($(obj).attr('a_title')), $(obj).attr('a_url'), _AG.usOwner, function(o) { _a_shareCallback(o,obj) }, encodeURIComponent(content),$(obj).attr('a_code')); 
            return false;
        }
        /*分享回调*/
        function _a_shareCallback(o,obj) {
            if ($(o).val() == "1") {
                $(obj).attr('a_shared','1')
            }
        }
        /*转载*/
        function _a_rep(obj){
            if (_AG.isLogin == 0) {
                sDialog.loadIFrame("/User/LoginWindows?LoginWinFromUrl="+encodeURIComponent(top.location.href), "搜道网 因为真实 所以精彩！赶快注册帐号登录，体验更多精彩乐趣！", 550, 230, true);
                return false;
            }
            if ($(obj).attr('a_reped') == "1") {
                sDialog.message('已经转载', "提示", 1500, 180, 0);
                return false;
            }
            var url = '/blog/detail/rep?rnd=' + Math.random();
            var ajax = {
                url: url, type: "POST", dataType: "json",
                data: { code: 'c720ce2ab878dfa49c', md: '80c7ba1168d65ee0cf24d01eaf3dcc4f' },
                success: function(data, textStatus) {
                    sDialog.hideLoading();
                    if (data.status == -3) {
                        sDialog.loadIFrame("/User/LoginWindows?LoginWinFromUrl="+encodeURIComponent(top.location.href), "搜道网 因为真实 所以精彩！赶快注册帐号登录，体验更多精彩乐趣！", 550, 230, true);
                    }
                    else if (data.status == 1) {
                        $(obj).attr('a_reped','1');
                        sDialog.message("转载成功", "提示", 1500, 180, 0);
                    }
                    else{
                        sDialog.message(data.msg, "提示", 1500, 180, 0);
                    }
                },
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    sDialog.hideLoading();
                    sDialog.message("转载失败，请稍候再试！", "提示", 1500, 180, 0);
                }
            };
            sDialog.loading("数据提交中。。。", 0, 300, 100);
            $.ajax(ajax);
        }
