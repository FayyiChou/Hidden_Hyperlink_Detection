            if (_AG.isLogin == 0) {
                sDialog.loadIFrame("/User/LoginWindows?LoginWinFromUrl="+encodeURIComponent(top.location.href), "搜道网 因为真实 所以精彩！赶快注册帐号登录，体验更多精彩乐趣！", 550, 230, true);
