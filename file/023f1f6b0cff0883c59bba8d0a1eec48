<script type="text/javascript">
/*初始化分类数据*/
function _egl_init() {
    var html = "";
    var url = "/u/xj85176432/blog/group?rnd=" + Math.random();
    var ajax = {
        url: url, type: "GET", dataType: "json",
        success: function(data, textStatus) {
            $('#groupListData').html('');
            $('#forContralmanagetbale').html('');
            $.each(data, function(i) {
                var url = 'http://i.sodao.com/xj85176432/blog-class-' + data[i]._ag_articlegroupid + '-1.html';
                $("<li id='_egl_li_" + data[i]._ag_articlegroupid + "' count='" + data[i]._ag_articlecount + "'><a href='" + url + "'>• " + data[i]._ag_articlegroupname + "（" + data[i]._ag_articlecount + "）</a></li>").appendTo($('#groupListData'));
                html = "";
                html += '<tr gid="' + data[i]._ag_articlegroupid + '">';
                html += '  <td width="63%" height="36" class="bob">' + data[i]._ag_articlegroupname + '</td>';
                html += '  <td width="19%" align="center" class="bob"><a href="javascript:;" onclick="_egl_editItem(this)">编辑</a></td>';
                html += '  <td width="18%" align="center" class="bob"><a href="javascript:;" onclick="_egl_delItem(this)">删除</a></td>';
                html += '</tr>';
                html += '<tr style="display:none">';
                html += '  <td height="50" class="bob"><input type="text" name="textfield2" value="' + data[i]._ag_articlegroupname + '" class="article-class-txt form-input" />';
                html += '    <input type="submit" value="确定" onclick="_egl_confirmeditItem(this)"  maxlength="14" name="button" class="boxy-btn1"/>';
                html += '    <input type="reset" class="boxy-btn2" name="button" onclick="_egl_canceleditItem(this)" value="取消" />';
                html += '    </td>';
                html += '  <td align="center" class="bob"></td>';
                html += '  <td align="center" class="bob"></td>';
                html += '</tr>';
                $(html).appendTo($('#forContralmanagetbale'));
            });
        },
        error: function(XMLHttpRequest, textStatus, errorThrown) {
            $('#groupListData').html('加载失败');
        }
    };
    $('#groupListData').html('分类数据加载中...'); 
    $.ajax(ajax);
}
_egl_init();
function _egl_manage() {
    sDialog.loadId("groupListDataManage", "分类管理", 400, 0);
}
/*删除啊扭*/
function _egl_delItem(obj) {

    sDialog.confirm("确定删除日志分类“ " + $(obj).parent().prev().prev().html() + " ”吗？", function() { _egl_delItem2(obj) }, "提示");
}
function _egl_delItem2(obj) {

    var tr = $(obj).parent().parent();
    var gid = tr.attr('gid');
    var url = '/u/xj85176432/blog/group/del?rnd=' + Math.random();
    var ajax = {
        url: url, async: false, type: "post", dataType: "json",
        data: { ngid: "1", gid: gid },
        success: function(data, textStatus) {
            if (data.status == 1) {
                $('#_egl_li_' + gid).remove();
                tr.next().remove();
                tr.remove();
            }
            else
                sDialog.message(data.msg, "提示", 1500, 180, 0);
        },
        error: function(XMLHttpRequest, textStatus, errorThrown) {
            sDialog.error('数据请求出错，请稍候再试!', '错误提示', 300, 100);
        }
    };
    $.ajax(ajax);
    
}
/*编辑按钮*/
function _egl_editItem(obj) {
    var tr = $(obj).parent().parent();

    tr.hide();
    tr.next().show();
}
/*取消编辑按钮*/
function _egl_canceleditItem(obj) {

    var tr = $(obj).parent().parent();
    tr.hide();
    tr.prev().show();
}
/*确定编辑按钮*/
function _egl_confirmeditItem(obj) {
    var val = $(obj).prev().val();
    //
    var val = $(obj).prev().val();
    if (val.isNullOrEmpty()) {
        sDialog.message("请输入分类名称！", "提示", 1500, 180, 0);
        $(obj).prev().focus();
        return;
    }
    var tr = $(obj).parent().parent();
    var gid = tr.prev().attr("gid");
    var count = $('#_egl_li_' + gid).attr('count');
    var html = "";
    var url = '/u/xj85176432/blog/group/update?rnd=' + Math.random();
    var ajax = {
        url: url, async: false, type: "post", dataType: "json",
        data: { gn: val, gid: gid },
        success: function(data, textStatus) {
            if (data.status == 1) {
                tr.hide();
                tr.prev().children('td:first').html(val);
                tr.prev().show();
                $('#_egl_li_' + gid).children('a').html("• " + val + "（" + count + "）");
            }
            else
                sDialog.message(data.msg, "提示", 1500, 180, 0);
        },
        error: function(XMLHttpRequest, textStatus, errorThrown) {
            sDialog.error('数据请求出错，请稍候再试!', '错误提示', 300, 100);
        }
    };
    $.ajax(ajax);
}
/*添加新分类按钮*/
function _egl_addNewItem(obj) {
    var groupName = $(obj).prev().val();
    if (groupName.isNullOrEmpty()) {
        sDialog.message("请输入分类名称！", "提示", 1500, 180, 0);
        $(obj).prev().focus();
        return;
    }
    var html = "";
    var url = '/u/xj85176432/blog/group/add?rnd=' + Math.random();
    var ajax = {
        url: url, async: false, type: "post", dataType: "json",
        data: { gn: groupName },
        success: function(data, textStatus) {
            if (data.status == 1) {
                html = "";
                html += '<tr gid="' + data.groupid + '">';
                html += '  <td width="63%" height="36" class="bob">' + groupName + '</td>';
                html += '  <td width="19%" align="center" class="bob"><a href="javascript:;" onclick="_egl_editItem(this)">编辑</a></td>';
                html += '  <td width="18%" align="center" class="bob"><a href="javascript:;" onclick="_egl_delItem(this)">删除</a></td>';
                html += '</tr>';
                html += '<tr style="display:none">';
                html += '  <td height="50" class="bob"><input type="text" name="textfield2" value="' + groupName + '" class="article-class-txt form-input" />';
                html += '    <input type="submit" value="确定" onclick="_egl_confirmeditItem(this)"  maxlength="14" name="button" class="boxy-btn1"/>';
                html += '    <input type="reset" class="boxy-btn2" name="button" onclick="_egl_canceleditItem(this)" value="取消" />';
                html += '    </td>';
                html += '  <td align="center" class="bob"></td>';
                html += '  <td align="center" class="bob"></td>';
                html += '</tr>';
                var prevDiv = $(obj).parent().prev().children('table:first');
                $(html).appendTo(prevDiv);
                var url = 'http://i.sodao.com/xj85176432/blog-class-' + data.groupid + '-1.html'
                $("<li id='_egl_li_" + data.groupid + "' count='0'><a href='" + url + "'>• " + groupName + "（0）</a></li>").appendTo($('#groupListData'));
            }
            else
                sDialog.message(data.msg, "提示", 1500, 180, 0);
        },
        error: function(XMLHttpRequest, textStatus, errorThrown) {
            sDialog.error('数据请求出错，请稍候再试!', '错误提示', 300, 100);
        }
    };
    $.ajax(ajax);
    $(obj).prev().val('');
    
}
</script>

