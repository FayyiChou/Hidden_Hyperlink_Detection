<script language="javascript">
function getYJTS(level,type,area)
{
	$.ajax({
	    url:'/index/getYJTS/',// 跳转到 action
	    data:{level:level,type:type,area:area},
	    type:'post',
	    //cache:false,
	    dataType:'json',
	    success:function(data){	       
	    	var obj = eval(data);
	    	var str = '';
            $(obj).each(function(index) {
                var val = obj[index];               
                //str+='<li><a href="http://www.gmgp.gov.cn/article/index/aid/'+val.id+'" class="w240px">'+val.title+'</a><span class="news_date">'+val.public_time+'</span></li>';
				switch(val.level)
				{
					case 'yellow':
						str+='<li><a href="http://www.gmgp.gov.cn/article/index/aid/'+val.id+'" class="w576px"><b class="yellow">[黄色预警]</b>'+val.title+'</a><span class="news_date">'+val.public_time+'</span></li>';
						break;
					case 'orange':
						str+='<li><a href="http://www.gmgp.gov.cn/article/index/aid/'+val.id+'" class="w576px"><b class="orange">[橙色预警]</b>'+val.title+'</a><span class="news_date">'+val.public_time+'</span></li>';
						break;
					case 'red':
						str+='<li><a href="http://www.gmgp.gov.cn/article/index/aid/'+val.id+'" class="w576px"><b class="red">[红色预警]</b>'+val.title+'</a><span class="news_date">'+val.public_time+'</span></li>';	
				}
            });
            $('#yjts').html(str);
	     },
	     error : function(){	          
	          alert("暂无数据！");
	     }
	});
}

function getSCZR(low,judge)
{
	$.ajax({
	    url:'/index/getSCZR/',// 跳转到 action
	    data:{low:low,judge:judge},
	    type:'post',
	    //cache:false,
	    dataType:'json',
	    success:function(data){	       
	    	var obj = eval(data);
	    	var str = '';
            $(obj).each(function(index) {
                var val = obj[index];
				str+='<li><a href="http://www.gmgp.gov.cn/article/index/aid/'+val.id+'" class="w240px">'+val.title+'</a><span class="news_date">'+val.public_time+'</span></li>';
            });
            $('#sczr').html(str);
	     },
	     error : function(){	          
	          alert("暂无数据！");
	     }
	});
}
$(function(){
$('#yjts_level').change(function(){
	var type=$('#yjts_type').val();
	var area=$('#yjts_area').val();
	getYJTS($(this).val(),type,area);});
$('#yjts_type').change(function(){
	var level=$('#yjts_level').val();	
	var area=$('#yjts_area').val();
	getYJTS(level,$(this).val(),area);});
$('#yjts_area').change(function(){
	var level=$('#yjts_level').val();
	var type=$('#yjts_type').val();
	getYJTS(level,type,$(this).val());});

$('#sczr_low').change(function(){
	var low=$('#sczr_low').val();
	var judge=$('#sczr_judge').val();
	getSCZR($(this).val(),judge);});
$('#sczr_judge').change(function(){
	var low=$('#sczr_low').val();
	var judge=$('#sczr_judge').val();
	getSCZR(low,$(this).val());});
		
getSCZR('','');
getYJTS('','','');
});
</script>
