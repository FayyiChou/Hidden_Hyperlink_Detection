<script>
$(document).ready(function(e) {
	$('.window .callme').click(function (e) {
		err=false;
		if($("#call_name").val().length<1)
		{
			err=true;
			$("#call_name").css("border","1px solid #FF0000");	
		}
		else
		{
			$("#call_name").css("border","1px solid #FFF")
		}
		if($("#call_tel").val().length<1)
		{
			err=true;
			$("#call_tel").css("border","1px solid #FF0000");	
		}
		else
		{
			$("#call_tel").css("border","1px solid #FFF")
		}
		
		
		
		if(!err)
		{
			$.get("/ajax/call.php", { name: $("#call_name").val(), tel: $("#call_tel").val(), date: $("#call_date").val(), question: $("#call_question").val()})
			.done(function(data) {
				if(data=="ok")
				{
					
					$('#call_form').hide();
					$('#call_ok').show();
					$("#call_name").val('');
					$("#call_tel").val('');
					$("#call_date").val('');
					$("#call_question").val('');
					setTimeout(function() {
						
						$('#mask').hide();
						$('.window').hide();
						
					}, 1000)
				}
				else
				{
					$('#mask').hide();
					$('.window').hide();
				}
				

			});
		}
	});
	
	$('#call_name').change(function()
	{
		if($("#call_name").val().length<1)
		{
			err=true;
			$("#call_name").css("border","1px solid #FF0000");	
		}
		else
		{
			$("#call_name").css("border","1px solid #FFF")
		}
	});
	$('#call_tel').change(function()
	{
		if($("#call_tel").val().length<1)
		{
			err=true;
			$("#call_tel").css("border","1px solid #FF0000");	
		}
		else
		{
			$("#call_tel").css("border","1px solid #FFF")
		}
	});
	

	
});

</script>
