// Write your javascript
var searchPage = setInterval( function() { 
	if (document.location.pathname.indexOf('listing-category') > -1) {
    b = document.querySelectorAll('main#main')[0];
    b.style.boxShadow = "none";
    clearInterval(searchPage);
  }
}, 1000);

//var threeBox = setInterval( function() {
//  if (document.location.pathname.indexOf('listing/') > -1) {
  //  widgets = document.getElementById('main');
    //if (widgets) {
      //widgets.innerHTML = "<div class='box-class'>" + widgets.innerHTML + "</div>";
  //    clearInterval(threeBox);
    //}
  //}
//}, 1000);
var boxCombine = setInterval( function() {
  if (document.location.pathname.indexOf('listing/') > -1) {
    var main = document.getElementById("main");
    if (main) {
    var wrapper = document.createElement("div");
    wrapper.id = "wrap";
    while (main.children.length > 1) {
    // Note: Appending the element to a new parent removes it from its original
    // parent, so `main.children.length` will decrease by 1
    wrapper.appendChild(main.children[1]);
    }
      main.appendChild(wrapper);

      clearInterval(boxCombine);
    }
  }
});
