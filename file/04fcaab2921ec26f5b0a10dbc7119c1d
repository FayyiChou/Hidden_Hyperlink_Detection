<script>
jQuery(document).ready(function() {
	
	var ajaxurl = 'https://www.worldschoolerconnect.com/wp-admin/admin-ajax.php';
	var latlng = '';
	
	// run geocode api
	function geoCodeLat(address) {
		
		var addy = address.replace(" ", "+");
		var addyurl = "https://maps.googleapis.com/maps/api/geocode/json?key=AIzaSyCeCdhIsARyVF-uwQsbKbsEnXaU3b_NQd8";
		locdata = { address: addy, sensor: true };
				
		jQuery.ajax({
                        async: false,
			url: addyurl,
			data: locdata,
			success:function(data){
				//location.reload();
				//alert(data.results[0].geometry.location.lat);
				latlng = [data.results[0].geometry.location.lat, data.results[0].geometry.location.lng];
			},
			error: function(errorThrown){
				alert('error');
				console.log(errorThrown);
			}
		});
		
		//return arr;
	}
	
	jQuery(".wsc_createprofile").on("click", function(e) {					
		var username = jQuery(".wsc-username-field").val();
		var useremail = jQuery(".wsc-useremail-field").val();
		var userpass = jQuery(".wsc-userpassword-field").val();
		var userloc = jQuery(".wsc-userlocation-field").val();
		var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,5})?$/;
		var passReg = /^(?=.*[a-zA-Z])[a-zA-Z0-9!@#$%^&*]{8,30}$/;
		var errors = 0;
		
		if(username == '') {
			alert("Please enter a username please.");
			errors+=1;
			return false;
		} 
		
		if(useremail == '' || !emailReg.test(useremail)) {
			alert("Please submit a valid e-mail.");		
			errors+=1;
			return false;
		}
		
		if(userpass == '' || !passReg.test(userpass)) {
			alert("Please enter a password that is 8 or more characters long.");		
			errors+=1;
			return false;
		}
		
		if(userloc == '') {
			alert("Please enter your current location, not the full address.");		
		} else {
			geoCodeLat(userloc);
		}
		
		if(errors === 0) {
			// TODO: Form Validation
			userdata = { action: 'process_user_data', process: "create_user", user_name: username, 
			user_email: useremail, user_pass: userpass, user_location: userloc, user_lat_location: latlng[0], user_lng_location: latlng[1] };
			
			jQuery.ajax({
				type: 'post',
				url: ajaxurl,
				data: userdata,
				dataType: 'html',
				success:function(data){
					window.location.href = "/future-destinations/?p="+useremail;
				},
				error: function(errorThrown){
					alert('error');
					console.log(errorThrown);
				}
			});
		} 
				
	});
		
	jQuery(".wsc_submitlocations").on("click", function() {
		var loc_array = new Array();
		var locations = jQuery("form.wsc_locations fieldset");
		var useremail = jQuery("input.hidden-ue").val();
		var errors = 0;
		
		// TODO: Form Validation
		// Make sure no empty fields are passed
		locations.each(function(i) {
			if(jQuery(this).find(".wsc-userlocation-field").val() !== '') {				
				var loc = jQuery(this).find(".wsc-userlocation-field").val();	
				geoCodeLat(loc);
				var sd = jQuery(this).find(".wsc-startdate").val();
				var ed = jQuery(this).find(".wsc-enddate").val();
				arr = [loc, sd, ed, latlng[0], latlng[1]];				
				
				loc_array.push(arr);
			}
		});

		if(loc_array.length > 0) {
			var ja = JSON.stringify(loc_array);
			
			userdata = { action: 'process_user_data', process: "update_user_locations", user_email: useremail, user_location: ja };
					
			jQuery.ajax({
				type: 'post',
				url: ajaxurl,
				data: userdata,
				dataType: 'html',
				success:function(data){
					window.location.href = "/invite-friends/?p="+useremail;
				},
				error: function(errorThrown){
					alert('error');
					console.log(errorThrown);
				}
			});
		} else {
			window.location.href = "/invite-friends/?p="+useremail;
		}
		
	});
	
	jQuery(".wsc_skiplocations").on("click", function() {
		var useremail = jQuery("input.hidden-ue").val();
		window.location.href = "/invite-friends/?p="+useremail;
	});
			
	var nowTemp = new Date();
	var now = new Date(nowTemp.getFullYear(), nowTemp.getMonth(), nowTemp.getDate(), 0, 0, 0, 0);	
	var addBoy = jQuery(".wsc_boyadd");
	var addGirl = jQuery(".wsc_girladd");	
	var girlcount = 2;
	var boycount = 2;
	var bcnt = 1;
	var gcnt = 1;
	
	function locations(date1, date2) {
		var checkin = $(date1).datepicker({
		  onRender: function(date) {
			return date.valueOf() < now.valueOf() ? 'disabled' : '';
		  }
		}).on('changeDate', function(ev) {
		  if (ev.date.valueOf() > checkout.date.valueOf()) {
			var newDate = new Date(ev.date)
			newDate.setDate(newDate.getDate() + 1);
			checkout.setValue(newDate);
		  }
		  checkin.hide();
		  $(date2)[0].focus();
		}).data('datepicker');
		
		var checkout = $(date2).datepicker({
		  onRender: function(date) {
			return date.valueOf() <= checkin.date.valueOf() ? 'disabled' : '';
		  }
		}).on('changeDate', function(ev) {
		  checkout.hide();
		}).data('datepicker');
	}
	
	locations("#wscdate1", "#wscdate2");
		
	addGirl.on("click", function() {
		if(girlcount < 6) {
			var str;
			gcnt = girlcount - 1;
			
			str = '<fieldset class="row girls girlsage'+girlcount+'"><div class="col-md-12"><input id="girls_age" type="text" placeholder="Age of Daugther" maxlength="2" /></div></fieldset>';
			
			jQuery(str).insertAfter(".girlsage"+gcnt);
			
			girlcount += 1;
			
			if(girlcount === 6)
				jQuery(this).remove();
			
		} else {
				jQuery(this).remove();
		}
	});

	addBoy.on("click", function() {
		if(boycount < 6) {
			var str;
			bcnt = boycount - 1;
			
			str = '<fieldset class="row boys boysage'+boycount+'"><div class="col-md-12"><input id="boys_age" type="text" placeholder="Age of Son" maxlength="2" /></div></fieldset>';
			
