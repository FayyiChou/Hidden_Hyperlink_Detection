	// submit family update
	jQuery(".wsc_submitfamilyupdate").on("click", function() {
		var loc_array = new Array();
		var ga_array = new Array();
		var ba_array = new Array();
		var facebook = $("input#facebook_profile").val();
		var twitter = $("input#twitter_profile").val();
		var instagram = $("input#instagram_profile").val();
		var description = $("textarea#family_desc").val();
