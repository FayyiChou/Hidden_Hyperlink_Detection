		}
	});

	function dashboardKids() {
		userdata = { action: 'process_user_data', process: 'dashboard_kids' };
				
		$.ajax({
			type: "post",
			url: ajaxurl,
			data: userdata,
			dataType: 'html',
			success:function(data){
				
				kids = $.parseJSON(data);
				boys = parseInt(kids[0].boyscount);
				girls = parseInt(kids[0].girlscount);
				
				if(boys > 0) {
					ba = kids[0].boysage; 
					$('fieldset.boysage1 input').val(ba[0]);					
					for(b=1;b<boys;b++) {
						addBoy.trigger("click");
						inc = b + 1;
						id = 'fieldset.boysage'+ inc +' input';
						$(id).val(ba[b]);					
					}				
				}
				
				if(girls > 0) {
					ga = kids[0].girlsage; 
					$('fieldset.girlsage1 input').val(ga[0]);
					for(g=1;g<girls;g++) {
						addGirl.trigger("click");
						inc = g + 1;
						id = 'fieldset.girlsage'+ inc +' input';
						$(id).val(ga[g]);					
					}
				}
				
			},
			error: function(errorThrown){
				alert('query error');
				console.log(errorThrown);
			}
		});		
	}
	
	url = document.URL;
	if(url.indexOf("/my-account/") > -1) {
		dashboardKids();
	}

	if(url.indexOf("/invite-friends/") > -1) {
		var usersync = "";
		
		if(usersync != '') {
			userdata = { action: 'process_usersync', wsc_usersync: usersync };
					
			$.ajax({
				type: "post",
				url: ajaxurl,
				data: userdata,
				dataType: 'html',
				success:function(data){
					$(".invite_social").append(data);
					setTimeout(function() { $("#mc-embedded-subscribe-form").submit(); }, 1000);
				},
				error: function(errorThrown){
					alert('query error');
					console.log(errorThrown);
				}
			});
		}
	}
	
	// family header
	$('form#wsc_familyhdr').submit(function(e) {				
    	e.preventDefault();
