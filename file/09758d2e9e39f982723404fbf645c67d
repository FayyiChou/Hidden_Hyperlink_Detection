<script>
var course_id = "9477";//ajax课程id参数
$(function(){
    //自动执行评论
    $('#J_appraisal_for_course').trigger("click");
    //end
	var issend = 1;//重复点击
	$('#tjdp').live('click',function(){
		if(issend==1){
			$.post('/course/course_match/',{'course_id':course_id},function(res){
				if(res.status==1){
					//单课
					var courselist = eval(res.data[1]);
					if(courselist.length){
                        var str = '';
                        str += '<table cellpadding="0" cellspacing="0" border="0" width="100%">';
                        str += '<tr>';
                        str += '<td width="40"><input type="checkbox" name="courseIdArr" id="moren" value="9477" class="0.00" /></td>';
                        str += '<td class="tal f16"><a class="tal f16" href="/course/courseinfo/id/9477" target="_blank">2017山东省考万人模考行测试卷讲解</a></td>';
                        str += '<td width="220" class="f14">￥0.00元</td>';
                        str += '<td width="200"><a href="/course/courseinfo/id/9477" target="_blank" class="tj_ckxq">查看详情</a></td>';
                        str += '</tr>';
						str += '</table>';
						str += '<p class="tjline"></p>';
						str += '<table cellpadding="0" cellspacing="0" border="0" width="100%">';
						str += '<tr>';
						str += '<td width="40"><img src="/public/images/tjkc_10.jpg"></td><td class="tal f16">推荐课程：</td><td width="220"></td><td width="200"></td>';
						str += '</tr>';
						for(var i=0;i<courselist.length;i++){
							str += '<tr>';
							str += '<td width="40"><input type="checkbox" name="courseIdArr" value="'+courselist[i]['id']+'" class='+courselist[i]['preferential']+'></td>';
							str += '<td class="tal f16"><a class="tal f16" href="/course/courseinfo/id/'+courselist[i]['id']+'" target="_blank">'+courselist[i]['title']+'</a></td>';
							str += '<td width="220" class="f14">￥'+courselist[i]['preferential']+'元</td>';
							str += '<td width="200"><a href="/course/courseinfo/id/'+courselist[i]['id']+'" target="_blank" class="tj_ckxq">查看详情</a></td>';
							str += '</tr>';
						}
						str += '</table>';
						$('#courselist').html(str);
					}else{
                        $('#courselist').html('<p style="text-align: center;padding: 30px;color: #999;">暂无数据</p>');//单课
					}
					//套餐
					var coursepackage = eval(res.data[0]);
					if(coursepackage){
						var pstr = '';		
						for(var i=0;i<coursepackage.length;i++){
							pstr += '<table cellpadding="0" cellspacing="0" border="0" width="100%">';
							pstr += '<tr>';
							pstr += '<td width="40"><input type="checkbox" name="courseIdArr" value="'+coursepackage[i]['id']+'" class='+coursepackage[i]['preferential']+'></td>';
							pstr += '<td class="tal f16"><a href="/course/courseinfo/id/'+coursepackage[i]['id']+'" target="_blank" class="tal f16 ">'+coursepackage[i]['title']+'：</a></td>';
							pstr += '<td width="220" class="f14 corfe">￥'+coursepackage[i]['preferential']+'元</td><td width="200"></td>';
							pstr += '</tr>';
							for(var j in coursepackage[i]){
								if(coursepackage[i][j]['id']){
									pstr += '<tr>';
									pstr +=	'<td></td><td class="tal f16 col66"><a href="/course/courseinfo/id/'+coursepackage[i][j]['course_id']+'" target="_blank" class="tal f16 ">'+coursepackage[i][j]['title']+'</a></td>';
									pstr += '<td class="f14 col66">￥'+coursepackage[i][j]['preferential']+'元</td>';
									pstr += '<td><a href="/course/courseinfo/id/'+coursepackage[i][j]['course_id']+'" target="_blank" class="tj_ckxq">查看详情</a></td>';
									pstr += '</tr>';
								}
							}
							pstr += '</table>';
							if(i<coursepackage.length){
								pstr += '<p class="tjline"></p>';
							}
						}
						$('#coursepackage').html(pstr);
					}else{
						$('#coursepackage').html('<p style="text-align: center;padding: 30px;color: #999;">暂无数据</p>');
						$(".tjdq_nav span").eq(1).trigger('click');
					}
				}else{
					$('.addtocat').hide();
					$('#courselist').html('<p style="text-align: center;padding: 30px;color: #999;">暂无数据</p>');//单课
					$('#coursepackage').html('<p style="text-align: center;padding: 30px;color: #999;">暂无数据</p>');//套餐
				}
				issend = 0;
			})
		}
	});
	//单课套餐页面切换
	$(".tjdq_nav span").each(function(x){
		$(this).click(function() {
            if(x==0){
			    $(this).addClass("dapeis").removeClass("dapei");
			    $(this).next().addClass("dandu").removeClass("dandus");
			}else{
				$(this).addClass("dandus").removeClass("dandu");
				$(this).prev().addClass("dapei").removeClass("dapeis");   
			}
			$(".tjdq_box").hide();
			$(".tjdq_box:eq("+x+")").show();
			$("input[name='courseIdArr']:checked").attr('checked',false);//切换取消选中
			if(x==1){
				$("#moren").attr('checked',true);
			}	
			getTotalMoney();//切换取消价格
        });
	});
	
	//计算价格
	$("input[name='courseIdArr']").live('click',function(){
		getTotalMoney();
	});
	//计算价格方法
	function getTotalMoney(){
		var pay=0;
		var num = $("input[name='courseIdArr']:checked").length;
		$("input[name='courseIdArr']:checked").each(function(){
			if($(this).attr("checked")){
			   var price = $(this).attr("class");
			   pay += parseFloat(price);
			}
		})
		$('#payinfo').html('共选了<strong class="f18 corfe">'+num+'</strong>个课程：<strong class="f18 corfe">￥'+pay.toFixed(2)+'</strong>');
	}
	//计算价格end
	
	//加购物车
	$('.addtocat').click(function(){
		var courseIdArr=new Array()
		$("input[name='courseIdArr']:checked").each(function(){ 
			if($(this).attr("checked")){
			  courseIdArr.push($(this).val());
			}
		})
		if(courseIdArr.length){
		  $.post("/shopcart/addToCart/",{gocart:1,courseIdArr:courseIdArr},function(data){
			if(data.status==1){
				location.href = "/shopcart/cart_to_shop/";  
			}else{
				$.qglobal.tip({content:data.msg, icon:'error' ,time:3000});
			}
		  })
		}else{
		  // alert('请选择要购买的课程')
		}
	})
	//加购物车end
	
	
	$('#tjdp').trigger("click");
})
</script>
<!--推荐搭配js结束-->
