    function checkfavorite(courseid){
        $.ajax({
            type : 'POST',
            cache : false,
            dataType : 'json',
            url  : SLPGER.root + 'favorite/add/',
            data : {
				courseid:courseid
			}, 
            success:function(result){
                if(result.status==2){
                    $.qglobal.tip({content:result.msg, icon:'error' ,time:1000});
                }else if(result.status === 1){
                    $("#sc").html('已经收藏');
                    $.qglobal.tip({content:result.msg, icon:'success' ,time:1000});
                }else{
                    $.qglobal.tip({content:result.msg, icon:'error' ,time:1000});
                }
            }
        });

    }
</script>
