    $(function(){
        $('.h2_down').toggle(function(){
            var pid = $(this).attr('pid');
            $('.unit_'+pid).each(function(){
                    ppid = $(this).attr('pid');
                    $(".lesson_"+ppid).hide(500);
                    $(this).children("h3").addClass('h3_down_mb');
		    $(this).children("h3").addClass('h3_up');
            }).hide(500); 
            $(".lesson_"+pid).hide(500);
            $(this).addClass('h2_up');
            $(this).removeClass('h2_nmb');

        },function(){
                var pid = $(this).attr('pid');
                $('.unit_'+pid).each(function(){
                    ppid = $(this).attr('pid');
                    $(".lesson_"+ppid).show(500);
                    $(this).children("h3").removeClass('h3_down_mb');
                    $(this).children("h3").removeClass('h3_up');
                }).show(500);
                $(".lesson_"+pid).show(500);
                $(this).removeClass('h2_up')
                if($(this).next("div").length==0){
                        $(this).addClass('h2_nmb');
                }else{
                        $(this).removeClass('h2_nmb');
