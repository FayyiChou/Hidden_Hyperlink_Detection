	$(function(){

		$(".cont_a ul.typelist li").each(function(index, element) {

            $(this).mouseover(function(){

				$(".cont_a ul.typelist li").removeClass("cur");

				$(this).addClass("cur");

				$(".cont_a_c").hide();

				$(".cont_a_c").eq(index).show();

			});

        });

		$(".cont_b ul.typelist li").each(function(index, element) {

            $(this).mouseover(function(){

				$(".cont_b ul.typelist li").removeClass("cur");

				$(this).addClass("cur");

				$(".cont_b .left .text_list").hide();

				$(".cont_b .left .text_list").eq(index).show();

			});

        });

		$(".cont_c ul.typelist li").each(function(index, element) {

            $(this).mouseover(function(){

				$(".cont_c ul.typelist li").removeClass("cur");

				$(this).addClass("cur");

				$(".cont_c .text_cont").hide();

				$(".cont_c .text_cont").eq(index).show();

			});

        });

	});

</script>


