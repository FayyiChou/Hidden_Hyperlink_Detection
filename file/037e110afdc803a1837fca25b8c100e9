            <script type="text/javascript">
            function doSearch() {
                var type = $(".search_category_text").html();
                var type_val = '';
                if (type == '视频')
                    type_val = 'video';
                else if (type == '用户')
                    type_val = 'user';
                else
                    type_val = 'news';

                $("#search_type").attr('value', type_val);

                var keyword = $("#search_keyword").val();
                if ($.trim(keyword) == '') {
                    $("#search_keyword").focus();
                    return false;
                }

                return true; 
            }
            </script>
