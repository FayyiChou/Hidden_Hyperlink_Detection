 <script type="text/javascript">

		var url = encodeURIComponent(document.location.href);
		var title = encodeURIComponent(document.title);
		
		function share(){
			var sina = document.getElementById("sina");
			var qq = document.getElementById("qq");
			var douban = document.getElementById("douban");
			var renren = document.getElementById("renren");
			var kaixin = document.getElementById("kaixin");
			var qqzone = document.getElementById("qqzone");
			var qqbm = document.getElementById("qqbookmark");
			var baidu = document.getElementById("baidu");
			
			if(sina) sina.href = "http://v.t.sina.com.cn/share/share.php?url=" + url + "&title=" + title;	//新浪微博
			if(qq) qq.href = "http://v.t.qq.com/share/share.php?title=" + title + "&url=" + url;	//腾讯微博
			if(douban) douban.href = "http://www.douban.com/recommend/?url=" + url + "&title=" + title;	//豆瓣
			if(renren) renren.href = "http://share.renren.com/share/buttonshare.do?link=" + url + "&title=" + title;	//人人
			if(kaixin) kaixin.href = "http://www.kaixin001.com/repaste/share.php?rtitle=" + url + "&rcontent=" + title;	//开心 
			if(qqzone) qqzone.href = "http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?url=" + url;	//QQ空间
			if(qqbm) qqbm.href = "http://shuqian.qq.com/post?from=3&title=" + title + "&uri=" + url; //QQ书签
			if(baidu) baidu.href = "http://cang.baidu.com/do/add?it=" + title + "&iu=" + url;	//百度搜藏
		
		}
</script>

<script type="text/javascript">
	share();
</script>
