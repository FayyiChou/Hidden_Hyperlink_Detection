function setTabindex(name,cursel,n){ 
    for(i=1;i<=n;i++){         
        var menus=document.getElementById(name+i);       
        var cons=document.getElementById("con_"+name+"_"+i);
        menus.className=(i==cursel)?"hover":"";        
        cons.style.display=(i==cursel)?"":"none";
    }
}	
