   <tr>
     <td align="center" style="position:relative;padding-top:1px;"><div style="width:980px;position:relative;overflow:hidden;cursor:pointer;" onclick="dianjitop_banner();"><img id="tutop_banner" src="/z/0F5118F0_6D90_4AAB_9351_9BF0B8C2ED59/upfile/150610112300110.jpg" width="980" height="250" border="0" /><div id="anniutop_banner1" style="width:14px;height:14px;right:34px;bottom:10px;position:absolute;background-color:#CCEEFF;line-height:14px;font-size:14px;color:#333333;z-index:2160;" align="center" onmouseover="yidongtop_banner(1);clearInterval(jishitop_banner);" onmouseout="jixutop_banner();"><strong>&nbsp;</strong></div><div id="anniutop_banner2" style="width:14px;height:14px;right:10px;bottom:10px;position:absolute;background-color:#EEEEEE;line-height:14px;font-size:14px;color:#333333;z-index:2160;" align="center" onmouseover="yidongtop_banner(2);clearInterval(jishitop_banner);" onmouseout="jixutop_banner();"><strong>&nbsp;</strong></div><script language="javascript">
function dianjitop_banner(){
   switch(weizhitop_banner){
case 1:break;
case 2:break;
   }
}
function yidongtop_banner(weizhi){
document.getElementById("anniutop_banner2").style.backgroundColor='#EEEEEE';
document.getElementById("anniutop_banner1").style.backgroundColor='#EEEEEE';
   document.getElementById("anniutop_banner"+weizhi).style.backgroundColor='#CCEEFF';
   switch(weizhi){
case 1:document.getElementById("tutop_banner").src="/z/0F5118F0_6D90_4AAB_9351_9BF0B8C2ED59/upfile/150610112300110.jpg";break;

case 2:document.getElementById("tutop_banner").src="/z/0F5118F0_6D90_4AAB_9351_9BF0B8C2ED59/upfile/150611180903100.jpg";break;

   }
   weizhitop_banner=weizhi;
}
function nexttop_banner(weizhi){
   weizhitop_banner=weizhitop_banner+1;
   if(weizhitop_banner>2){weizhitop_banner=1;}
   yidongtop_banner(weizhitop_banner);
}
var weizhitop_banner=1;
var jishitop_banner=setInterval("nexttop_banner(weizhitop_banner)",5500);
function jixutop_banner(){
   jishitop_banner=setInterval("nexttop_banner(weizhitop_banner)",5500);
}
</script></div></td>
   </tr>
