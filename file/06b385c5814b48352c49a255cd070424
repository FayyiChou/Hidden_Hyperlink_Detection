            FGDemo_55678518c31411e58a5b001e6706fc59.onmouseover = function(){ clearInterval(MyMar1_55678518c31411e58a5b001e6706fc59) }
            FGDemo_55678518c31411e58a5b001e6706fc59.onmouseout = function(){
                MyMar1_55678518c31411e58a5b001e6706fc59 = setInterval(Marquee1_55678518c31411e58a5b001e6706fc59, speed_55678518c31411e58a5b001e6706fc59)
            }
