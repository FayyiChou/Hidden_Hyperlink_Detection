	function moreBoxto_n(num)
	{
		var tid = "moreBox"+num;
		document.getElementById(tid).style.display='block';
		if(document.getElementById('mobile').value!='' || document.getElementById('mobile').value!='请输入手机号码')
		{
			document.getElementById('mobile').value = '请输入手机号码';
		}
		if(document.getElementById('yanzheng2').value!='')
		{
			document.getElementById('yanzheng2').value = '';
		}
	}
	function sendmsg()
	{
		var code = document.getElementById('yanzheng2').value;
		var mobile = document.getElementById('mobile').value;
		exp = /^[0-9]{11}$/;
		if(mobile != ''){
			if(mobile.match(exp) == null){
				alert('请输入正确的手机号码')
				return false;
			}
		}
		else
		{
			alert('请输入手机号码')
			return false;	
		}
		if(code=='')
		{
			alert('请输入验证码');
			return false;	
		}
		else
		{
			if(code.match(/^[0-9]{6}$/) == null){
				alert('请正确输入验证码')
				return false;
			}
		}
		var content = encodeURIComponent( document.getElementById('mobilemsg').innerHTML + '[<a href="http://www.fang.com/">搜房网</a>]');
	    Xml.Request("/house/web/doajax.php?code="+code+"&mobile="+mobile+"&newcode=" + newcode + "&type=sendmsg&msg=" + content + "&projname="+ encodeURIComponent(projn)+"&time=" + new Date().getTime(), null,getRs, null);
	}
	function getcode()
	{
		// href="/house/web/mobilecode.htm"	
		if(document.getElementById("sendinfo").style.display=="block")
		{
	　　	document.getElementById("sendinfo").style.display="none";
		}
		else
		{
			document.getElementById("sendinfo").style.display="block";
		}
		document.getElementById('sendinfo').style.height='330px';
        document.getElementById('iframe_sendinfo').style.height='330px';
		document.getElementById("iframe_sendinfo").src ="/house/web/mobilecode.htm?t="+ new Date().getTime();
	}
	function getRs()
	{
		alert(this.responseText);
		if(this.responseText=='楼盘点播成功')
		{
			document.getElementById('moreBox37').style.display="none";
			document.getElementById('mobile').value='请输入手机号';
			document.getElementById('yanzheng2').value='';
		}
	}
	function SetProjSubscriptionDisplay(state)
	{

    	var tempDiv = document.getElementById("ProjSubscriptionDiv");
    	if(tempDiv){
    		if(state == 0){
    			tempDiv.style.display = 'block';
    		}else{
    			tempDiv.style.display = 'none';
    		}
    	}
    }
    
    function addfollow(op,e)
    {
        var act = op==1 ? 'add' : 'del';
        if(Get_Cookie('new_loginid')>0)
        {
            if(op==1)
            {
                Xml.Request("/house/web/microUserFollow.php","act=add&objID="+microID, followRes,op);  
            }else{
                Xml.Request("/house/web/microUserFollow.php","act=del&objID="+microID, followRes,op);
            }
        }else{
            var pos = getPointerPosition(e);
            var vurl = "/house/web/dopost.php?act="+act+"&objID="+microID+"&msg=微博登录";
            var i = document.getElementById("iframe_selecthouse_login");
            var d = document.getElementById("newhouse_login");
            if(window.ActiveXObject){
            if(i.contentWindow.document.readyState == "complete"){
                    document.getElementById("iframe_selecthouse_login").src = vurl;
                    
                    document.getElementById("newhouse_login").style.display="block";
                    d.style.left = pos.x - 420 + 'px';
                    d.style.top = pos.y  + 'px';
                    i.style.display="";
                    i.style.visibility="";
            }else{
                i.style.display="none";
                document.getElementById("newhouse_login").style.display="none";
            } 
        }else{
           document.getElementById("iframe_selecthouse_login").src = vurl;
                    document.getElementById("newhouse_login").style.display="block";
                    i.style.display="";
                    i.style.visibility="";   
        }
        }
    }
    function followRes(op)
    {
        var rsphtml = this.responseText;
        if(rsphtml){
            var obj = eval('(' + rsphtml + ')');
            if(obj.code==100 || obj.code==101){
                alert(obj.display);
                if(op==1){document.getElementById("gzout").innerHTML="<a id=\"fansubmit02\" onclick=\"addfollow(0,event);return false;\">取消关注</a>";
                }else{document.getElementById("gzout").innerHTML="<a id=\"fansubmit01\" onclick=\"addfollow(1,event);return false;\">加关注</a>";}
            }else{
                if(obj.display=='undefined'||obj.display=='' ){
                    alert('请重新添加或取消');
                }else{alert(obj.display);}    
            }
        }
    }
    function followInner(op,code,display){
            if(code==100 || code==101){
                if(op==1){document.getElementById("gzout").innerHTML="<a id=\"fansubmit02\" onclick=\"addfollow(0,event);return false;\">取消关注</a>";
                }else{document.getElementById("gzout").innerHTML="<a id=\"fansubmit01\" onclick=\"addfollow(1,event);return false;\">加关注</a>";}
            }else{
                if(display=='undefined'||display=='' ){
                    alert('请重新添加或取消');
                }else{alert(display);}    
            } 
    }
	function ResultMsg()
	{
		var rsphtml=this.responseText;
		document.getElementById("iframe_selecthouse_login").src='http://www.fang.com/newhousenet/newhouse/newhouse_house_login.aspx?msg='+escape(rsphtml);
	　　if(document.getElementById("newhouse_login").style.display=="none")
	　　		document.getElementById("newhouse_login").style.display="block";
	}
	if (document.all){//IE
        if(document.getElementById("Image1"))
        {
        	document.getElementById("Image1").style.marginTop = '1px';
        }
    }
