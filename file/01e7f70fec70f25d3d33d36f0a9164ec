<script type="text/javascript" language="javascript">
	function submit_byb(){
		var url = newhousedomain+'/house/web/newhouse_compare_house.php?str_id=';
		var num = 0;
		$("#form_byb input[type='checkbox']").each(function(i){
			if($(this).attr("checked")) {
				url += $(this).val() + '|';
				num++;
			}
		});
		if(num < 2 || num > 4){
			alert('请选择2-4个楼盘进行比较！');
			return false;
		}
		document.getElementById('form_byb').action = url;
		return true;
	}
	
	function Show_Cookie(){
        var tmp = new Array();
        Xml.Request("/house/web/ajax_newhouse.php", null, call_back_cookie,tmp);
	}
	function call_back_cookie()
	{
		var coo_data = this.responseText;//获取cookie
		if(coo_data)
		{
			var obj = eval('(' + coo_data + ')');
			var used_=0;
			if(obj.data.length>0)
			{
				var view_template="";
				var recent_tpl = "";
				var show_num = obj.data.length > 9 ? 9 : obj.data.length;
				for(i=0;i<show_num;i++)
				{
					var cookie_proj_code = obj.data[i].keyname;
					var cookie_proj_info = obj.data[i].value.split("ζ");
					var cookie_proj_name = cookie_proj_info[0];
					var cookie_proj_price = cookie_proj_info[1];
					var cookie_houseurl = "";
					if( typeof(cookie_proj_info[3])!='undefined' )
					{
					    cookie_houseurl = cookie_proj_info[3];   
					}
					if(cookie_proj_code != "" && cookie_proj_name != "")
					{
						view_template = view_template + "<div class='bybbkmiddlenr01'><input type='checkbox' value='" + cookie_proj_code + "' class='bybbkmiddleinput' /><strong><a href='"+ newhousedomain +"/"+ newhousedomaintail + cookie_proj_code + ".htm' target='_blank'>" + cookie_proj_name + "</a></strong></div>";
					    if(i<6)
					    {
					        var houselink = cookie_houseurl ? cookie_houseurl : newhousedomain + "/house/" + cookie_proj_code + ".htm";
					        recent_tpl=recent_tpl+'<li><div class="v1"><a target="_blank" href="'+houselink+'">'+cookie_proj_name+'</a></div><div class="v2">'+cookie_proj_price+'</div></li>';
					    }
					}
				}
				view_template = view_template + "<div class='bybbkmiddlenr02'><input type='image' src='http://img1.soufun.com/house/detail_new/images/byb_submit.gif' /></div>";
				document.getElementById('browse_mark4byb').innerHTML = view_template;
				if(document.getElementById('recentHouse')!=null)
				{
                    document.getElementById('recentHouse').innerHTML = recent_tpl;
				}
				//update recentHouse 
			}
		}
	}
	Show_Cookie();
