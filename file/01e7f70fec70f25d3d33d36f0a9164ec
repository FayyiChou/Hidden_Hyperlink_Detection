<script type="text/javascript">
	$("a[name^='this_anchor']").click(function(event){
		event.preventDefault();
		var full_url = this.href;
		var parts = full_url.split("#");
        var trgt = parts[1];
		var anchorTarget = $(this).attr('href') + "_anchor";
		var scrollTopNum = $( anchorTarget ).offset().top - 75;
		$('html, body').animate({
			scrollTop: scrollTopNum
		}, 500);
		window.location.hash ="#"+trgt;
	});
//看房团轮播
var kftcount = "8";
setTimeout(function () {
			kftlunbo(2);
		}, 3000);
function kftlunbo(shownum){
	if(kftcount <= 0){
		return false;
	}
	if(kftcount>shownum){
		$('#kft_list_'+(shownum-2)).hide();
		if(kftcount != 1){
			$('#kft_list_'+(shownum-1)).hide();
		}
		$('#kft_list_'+(shownum)).show();
		if(kftcount - shownum >= 2){
			$('#kft_list_'+(shownum+1)).show();
		}		
		shownum = shownum + 2;
		
	}else{
		$('#kft_list_'+(kftcount-1)).hide();
		$('#kft_list_'+(kftcount-2)).hide();
		$('#kft_list_0').show();
		$('#kft_list_1').show();
		shownum = 2;
	}
	setTimeout(function () {
		kftlunbo(shownum);
	}, 2000);
}
</script>

	
	<script language="javascript">
	function browseUbcode(){
		_ub.city = '北京';
		_ub.biz = 'n';
		_ub.location = 0;
	
	}
	browseUbcode();
	</script>
	
