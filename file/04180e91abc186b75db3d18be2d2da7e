       /*-------------------------鼠标拖动---------------------*/       
        var od = document.getElementById("blog_menu");       
        var dx,dy,mx,my,mouseD;
        var odrag;
        var isIE = document.all ? true : false;
	var _olddivstate=true;
	var _blogId=355778;
        document.onmousedown = function(e){
		if(!_pushstate){
			return true;
		}
                var e = e ? e : event;
                if(e.button == (document.all ? 1 : 0))
                {
                        mouseD = true;                       
                }
        }

        document.onmouseup = function(){
		if(!_pushstate){
			return true;
		}
                mouseD = false;
                odrag = "";
                if(isIE)
                {
                        od.releaseCapture();
                        //od.filters.alpha.opacity = 100;
                }
                else
		{
                        window.releaseEvents(od.MOUSEMOVE);
                        od.style.opacity = 1;
                }
        }
       
       
        //function readyMove(e){       
        od.onmousedown = function(e){
		$("#mtip").hide();
		if(!_pushstate){
			return false;
		}
                odrag = this;
                var e = e ? e : event;
                if(e.button == (document.all ? 1 : 0))
                {
                        mx = e.clientX;
                        my = e.clientY;
				if(!document.all)/*Firefox、Chrome和Safari所加判断*/			    {
					HTMLElement.prototype.__defineGetter__("currentStyle",function(){
						return this.ownerDocument.defaultView.getComputedStyle(this,null);
					}
				);}
				//od.style.marginLeft = parseInt(od.currentStyle.marginLeft)+"px";
				//od.style.marginTop =  parseInt(od.currentStyle.marginTop)+"px";
				if(!od.style.marginLeft){
					od.style.marginLeft = "0px";
				}	
				od.style.marginTop =  parseInt(od.currentStyle.marginTop)+"px";
                        if(isIE)
                        {
                                od.setCapture();
                                //od.filters.alpha.opacity = 50;
                        }
                        else
                        {
                                window.captureEvents(Event.mousemove);
                                od.style.opacity = 0.5;
                        }
                }
        }
	od.onmouseup=function(e){
		if(!_pushstate){
			return false;
		}
                odrag = this;
                var e = e ? e : event;
                if(e.button == (document.all ? 1 : 0))			
		{
			//changeMenu(_blogId,parseInt(od.style.marginLeft),parseInt(od.style.marginTop));
		}	
	}
        document.onmousemove = function(e){
		if(!_pushstate){
			return true;
		}
                var e = e ? e : event;
                if(mouseD==true && odrag)
                {               
                        var mrx = e.clientX - mx;
                        var mry = e.clientY - my;     
			var realx = parseInt(od.style.marginLeft)+mrx;	
			var realy = parseInt(od.style.marginTop)+mry;
			realx=getRealMargin(realx,0);
			realy=getRealMargin(realy,1);
                        od.style.marginLeft = realx + "px";
                        od.style.marginTop = realy + "px";
                        mx = e.clientX;
                        my = e.clientY;
                       
                }
        }
	function getRealMargin(num,state){
		if(state==1){//y的数值
			num=Math.max(num,-10);
			num=Math.min(num,210);	
		}else{//x的数值
			num=Math.max(num,0);
			num=Math.min(num,520);
		}
		return num;
	}
