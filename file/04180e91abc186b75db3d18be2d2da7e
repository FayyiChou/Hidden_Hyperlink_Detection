/*前台文章管理*/
var _pid=0;
var _bid=0;
function showPopEdit(n)
{
	$("#pop_edit"+n).show();
}
function hidePopEdit(n)
{
	$("#pop_edit"+n).hide();
}
function showEditBox(bid,pid){
	$("#pop_editbg").show();
	$("#"+bid).show();
	var boxwidth=300;
	var boxheight=177;
	var bodywidth=$(document.body).width();
	var bgheight=$(document.body).height();
	var bodyheight=$(window).height();
	var scrolltop=$(document).scrollTop();
	var left=(bodywidth-boxwidth)/2;
	var top=(bodyheight-boxheight)/2+scrolltop;
	$("#pop_editbg").css({"height":bgheight});
	$("#"+bid).css({"top":top,"left":left});
	_pid=pid;
	_bid=bid;
}
function hideEditBox(bid)
{
	$("#"+bid).hide();
	$("#pop_editbg").hide();
}
function changecate(){
	var cid=$("#change_article_cate").val();
	$.post("/imgupload/scripts/change_cate.php?v="+Math.random(),{pid:_pid,cid:cid},function(data){
		hideEditBox(_bid);
		location.reload();		
	});
}
