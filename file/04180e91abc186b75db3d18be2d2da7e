{
	if(username){
		var array = new Array("请选择", "今日热文", "童言趣语","育儿知识", "早期教育", "心情随笔", "垃圾文章","孕育里程","成长日记");
		if(document.getElementById("categoryx").value == 0){
			return;
		}else if(confirm("你确定要把该文章推荐到:" + array[document.getElementById("categoryx").value] + "吗?")){
			var href = "http://blog.ci123.com/postadd.php?postid=" + id + "&posttype=" + document.getElementById("categoryx").value + "&username=" + username+"&from="+from;
			window.location.href=href;
		}
	}else{
		alert("对不起你还没有登陆");
		window.location.href="http://blog.ci123.com/"
	}
}

