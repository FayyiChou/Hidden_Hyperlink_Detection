//请求数据
function getMore(el,childClass){
	//var n = getNumber();
	el.className = "wrap";
	$.post("/web/index/article?cate1=0",{"aid":$("#aid").attr("data-role")},function(data)
	{
		
		addList(el,data,childClass);
		//alert(data);
		//changeJeson(el,data,childClass);
	})
}
function changeJeson(el,json,childClass){
	var jsonString = json; // 获取服务器返回的json字符串
	var jsonObj = null;
	try {
		jsonObj = eval('(' + jsonString + ')'); // 将json字符串转换成对象
	} catch (ex) {
		return null;	
	}
	addList(el,jsonObj,childClass);//执行添加节点函数
}
function addList(el,jsonObj,childClass){
	var newBox = [];
	if (jsonObj.length<=0)
	{
		$("#loading").hide();
	}
	for(var i=0;i<jsonObj.length;i++)
	{
		var div = document.createElement("div");
		var src =String(jsonObj[i].url);
		var width =String(jsonObj[i].width);
		var height =String(jsonObj[i].height);
		//$id("aaa1").innerHTML =$id("aaa1").innerHTML + src+","
		
		var html='<p class="pic"><img style="height:auto" onerror="error_img(this);" id="img'+jsonObj[i].aid+'" src="'+jsonObj[i].img+'"/></p><h3 class="tit"><span>';
		html=html+jsonObj[i].title+'</span></h3><p class="txt">'+jsonObj[i].date+'</p>';
		div.className = childClass+" "+"popup_in"; //预留接口
		div.innerHTML = html;
                div.style.top = startH +"px";
				div.style.opacity = 0;
                $(div).bind({
                    mouseover : function(){
                        this.style.opacity = 0.9;
                    },
                    mouseout : function(){
                        this.style.opacity = 1;
                    }
                });
                var a = document.createElement('a');
                a.href = '/web/article/'+jsonObj[i].link+jsonObj[i].aid;
                a.title = jsonObj[i].title;
                a.target = '_blank';
                a.style.display="inline-block";
                a.appendChild(div);
		el.appendChild(a);
		newBox[i]=div;
		order_id = jsonObj[i].order_id;
	}
//        alert(el.innerHTML);
	chackImg(el,newBox);
	sumChild = sumChild+Number(jsonObj.length);
	num++;
	$("#aid").attr('data-role',order_id);
	//alert("sumChild:"+sumChild);
}
function chackImg(el,newBox){
	var imgs =[];
	imgs=el.getElementsByTagName("img");//只遍历新增图片
	
	for(i=sumChild;i<imgs.length;i++){
		var img = new SImage(icall,el,imgs.length,imgs[i].src,newBox,imgs[i].id);
	}
}
function SImage(callback,el,num,url,newBox,imgid) {
    var img = new Image();
    this.img = img;
   
    var appname = navigator.appName.toLowerCase();
    if (appname.indexOf("netscape") == -1) { 
    	//ie
    	this.img.onload=function()
    	{
    		callback(img,el,num,newBox);
    	}
    	this.img.onerror=function(){
    		//用error图片计算占位区域
    		var img1=new Image();
    		img1.onload=function()
        	{
        		callback(img1,el,num,newBox);
        	}
    		img1.onerror=function()
    		{
    			callback(img1,el,num,newBox);
    		}
    		img1.src = error_img;
    		document.getElementById(imgid).src=error_img;
        }
    	this.img.src = url;
		 
      //  img.onreadystatechange = function() {
          //  if (img.readyState == "complete") {
          //      callback(img,el,num,newBox);
           // }
       // };
    } else {
        //firefox
         this.img.src = url;
        img.onload = function() {
            if (img.complete == true) {
            	callback(img,el,num,newBox);			
            }
        }
        img.onerror=function(){
        	url=error_img;
        	img.src=url;
        	document.getElementById(imgid).src=error_img;
        	callback(img,el,num,newBox);
        }
    }
}

function icall(obj,el,num,newBox)
{
	
	t++;
	if(t==num)
	{
		sortNew(el,newBox);
		
		//t=sumChild;
		
	}
}
function sortNew(el,newBox){
	var box = newBox;
	postPosition(el,box,"add");//执行定位函数
	for(var i = 0; i < box.length; i++) {
		box[i].style.visibility = "visible"; //定位完毕后显示新增节点
	}
	gettingdata=0;
	$("#loading").hide();
	startH = h[0];
	if( navigator.userAgent.indexOf("Firefox") > -1)
	{
		scroll(0,startH-700);//火狐滚动条问题，暂无好的解决办法，随便打个补丁先
	}
}
function sortAll(el,childTagName){
	h = []; //每次重排都要重置列高度记录数组
	var box = el.children[0].getElementsByTagName(childTagName);
	postPosition(el,box,"re");//执行定位函数
}

function postPosition(el,box,op){
	var minH = box[0].offsetHeight;
	boxW = box[0].offsetWidth+margin;
	n = (el.offsetWidth+margin) / boxW | 0; //计算页面能排下多少Pin
	el.style.width = n * boxW + "px";
	el.style.visibility = "visible";
	for(var i = 0; i < box.length;i++){
		//排序算法，有待完善
		boxh = box[i].offsetHeight; //获取每个Pin的高度
		boxh=parseInt(boxh);
		if (boxh<10)
		{
			boxh=10;
		}
		if(i < n && op =="re" || (i < n && op =="add" && h.length<n)){ 
			//第一行特殊处理
				h[i] = boxh;
				box[i].style.top = 0 + 'px';
				box[i].style.left = (i * boxW) + 'px';
				box[i].style.opacity = 1;
		}else{
				minH = Array.min(h); //取得各列累计高度最低的一列
				minKey = getarraykey(h, minH);
				h[minKey] += boxh+margin ; //加上新高度后更新高度值
				box[i].style.top = minH+margin + 'px';
				box[i].style.left = (minKey * boxW) + 'px';
				box[i].style.opacity = 1;
		}
	}
	maxH = Array.max(h); 
	maxKey = getarraykey(h, maxH);
	el.style.height = h[maxKey] +"px";
}

Array.min=function(array)
{
    return Math.min.apply(Math,array);
}
Array.max=function(array)
{
    return Math.max.apply(Math,array);
}
/* 返回数组中某一值的对应项数 */
function getarraykey(s, v) {
        for(k in s) {
                if(s[k] == v) {
                        return k;
                }
        }
}
function getNumber(){
	//return Math.floor(document.documentElement.clientWidth/(warpWidth+14));
}
var number=0;
var sUserAgent  =  navigator .userAgent.toLowerCase(); 
if (/android/.test(sUserAgent)) {
	var dis="400";
}else{
if(sUserAgent .match(/iphone os/i) == "iphone os")
{
	var dis="500";
}else
{
	var dis="200";
}
}
window.onscroll = function(){
	var a=document.body.scrollHeight;//整个文档滚动高度
	var b=document.documentElement.clientHeight;//浏览器高度
	var c=document.documentElement.scrollTop + document.body.scrollTop;//滚动距离即当前top位置离最上面的距离
	var d=dis;//当距离屏幕底部100px时触发事件
	
	if (parseInt(b)+parseInt(c)+parseInt(d)>=a&&gettingdata==0)
	{
		number++;
		if (number<20)
		{
			gettingdata=1;//标识当前正在请求中，频繁触发事件不会多次请求
			$("#loading").show();
			getMore(document.getElementById("wrap"),"mode");
		}
	}
}
