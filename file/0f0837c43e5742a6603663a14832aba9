<script>
	$(function(){
		if($(window).scrollTop()==0){
			$('.r_xia').hide();
			$('.r_shang').hide();
		}
		
		 $(window).scroll(function(e){
		 	if($(window).scrollTop()<200){
		 		$('.r_xia').hide();
		 		$('.r_shang').hide();
		 	}else{
		 		$('.r_xia').show();
		 		$('.r_shang').show();
		 	}
		 })
		$(".r_xia").on("click",function(){
				 $('html, body').animate({scrollTop:0}, 'slow');
			})
		
	})
</script>




