var pic_height=178; //图片高度
var button_pos=4; //按扭位置 1左 2右 3上 4下
var stop_time=3000; //图片停留时间(1000为1秒钟)
var show_text=0; //是否显示文字标签 1显示 0不显示
var txtcolor="000000"; //文字色
var bgcolor="DDDDDD"; //背景色
var imag=new Array();
var link=new Array();
var text=new Array();

imag[1]="image/hd01.jpg";
link[1]="";
text[1]="星光集团";
imag[2]="image/hd02.jpg";
link[2]="";
text[2]="星光集团";
imag[3]="image/hd03.jpg";
link[3]="";
text[3]="星光集团";
imag[4]="image/hd04.jpg";
link[4]="";
