<script type="text/javascript">
var show_status_bar = true;
if(show_status_bar) {//don't show it on some pages
	/***
	 **firstly, use cookie to check if login (fast)
	 **/
	var user_name = $.cookie('DRUPAL_USER');
	var state = [0, 0];
	//state = (user_name) ? [1, user_name] : [0];
	//user_bar_update(state.join('|'));
	
	/**
	 **secondly, use ajax to check if login (securtiy)
	 **/
	$.get('?q=user/status&from=' + Drupal.settings.via_common.request_path, function(res){
	  user_bar_update(res);
	});
}
/**
 * res
 * [1/0]|[name]|
 * update status bar
 */
function user_bar_update(res) {
    $('.status-item').hide();
    var result = res.split('|');
    if (result[0] == 1) {
      $('#status-authenticated').show().css('display', 'block');
      if (result[1]) {
        $('#user-status-name').html(result[1]);
      }
      if (result[2]) {
        top.location.href = result[2];
      }
    }
    else {
      $('#status-anonymous').show();
    }
    return result;
}
</script>
