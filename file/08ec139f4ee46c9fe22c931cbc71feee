<script>
jQuery(document).ready(function(){
    var topspace = "37px";
    var zhTopNavBar = "false"
    var zhTopNavBar = CookieUtil.getCookie("zh_topnavbar");
    if (zhTopNavBar == "true"){topspace = "2px";}
    var bodyHeight = document.body.clientHeight;
    var leftPanelHeight = 145;//左浮动高度
    var rightPanelHeight = jQuery("#uiShortcutButton").height(); /* 354 415 */
    if(isNaN(rightPanelHeight) || rightPanelHeight < 200){rightPanelHeight = 415;}//右浮动高度
    var readTop = jQuery(".read_con").position().top;	//内容区顶距

    // 载入时重设浮动条顶距
    jQuery("#top_bar_close").click(function(){
    if(jQuery(this).hasClass('open')){topspace = "2px";}else{topspace = "37px";}
        readTop = jQuery(".read_con").position().top;
        setposition();
    });

    //点击顶部用户条开关时重新获取内容区顶距，重设左右浮动位置
    $(window).scroll(function(){
        setposition();
    });

    //滚动时set定位
    function setposition(){
    var readHeight = jQuery(".read_con").height();	//内容区高度
    var lefttop = readHeight + readTop - leftPanelHeight;//左侧定位事件边界
    var righttop = readHeight + readTop - rightPanelHeight;//右侧定位事件边界
    var scrollTop=$(document).scrollTop();
    var windowHeight = $(window).height();
        if ($.browser.msie && ($.browser.version == "6.0") && !$.support.style){}else{
            $("#rightheight").val(righttop);
            if(scrollTop>readTop && scrollTop<(lefttop)){
                jQuery("#uiDirectoryButton").css({"position":"fixed","top":topspace});
                jQuery("#uiDirectorPanel").css({"position":"fixed","top":+59+"px"});//章节目录比浮动按钮+59
            }else if(scrollTop>(lefttop)){
                jQuery("#uiDirectoryButton").css({"position":"absolute","top":lefttop+"px",bottom:"auto"});
                jQuery("#uiDirectorPanel").css({"position":"absolute","top":lefttop+59+"px",bottom:"auto"});
            }else{
                jQuery("#uiDirectoryButton").css({"position":"absolute","top":readTop});
                jQuery("#uiDirectorPanel").css({"position":"absolute","top":readTop+59+"px"});
            };
            //左
            if(scrollTop>(readHeight + readTop - windowHeight)){
                jQuery("#uiShortcutButton").css({"position":"absolute","top":righttop+"px",bottom:"auto"});
            }else{
                jQuery("#uiShortcutButton").css({"position":"fixed","bottom":"0px","top":"auto"});
            }
            //右
        }
    }
    setposition();
});

jQuery(document).click(function(event){
	var target = $(event.target);
	if(target.attr("id") != 'uiDirectorContentPanel' && !target.hasClass('tit') && !target.parents('#uiDirectorContentPanel,div.tit').length) {
		   $("#uiDirectorPanel span.close").click();
	}
});
</script>
