<script type="text/javascript">
    var alterandoLogin = false;
    $(document).ready(function () {
        var fnLogar = function () {
            usuario.BoxLoginFocado = false;
            if ($("#txtEmailEntrar").val().length > 0 && $("#txtSenhaEntrar").val().length > 0 && !usuario.ErroLogin) {
                usuario.Logar($("#loginForm").serialize(), function (u) {

                    if(u.Captcha != null) {
                        $('#CaptchaImage0').attr('src', u.Captcha.CaptchaImage0);
                        $('#CaptchaDeText0').attr('value', u.Captcha.CaptchaDeText0);
                        $('#CaptchaInputText0').attr('value', '');
                    }

                    if(u.ShowCaptcha) {
                        $(".mst-c").fadeIn();
                        $("#LoginMinhaConta").css({"height":"415px"});
                    }

                    if (!usuario.Logado) {
                        $("#txtSenhaEntrar").val("");
                        alterandoLogin = false;
                    }
                });
            }
        }

        $("a[href='#CaptchaImage0']").css({
            "border": "1px solid #E80000",
            "color": "#fff !important",
            "padding": "3px",
            "float":"right"
        });

        $("#CaptchaImage0").css({"float": "none",
            "margin-bottom": "0px",
            "width": "100%"
        })

        $("#CaptchaInputText0").css({
            "text-transform": "uppercase",
            "width": "252px"});

        usuario.BoxLoginFocado = false;

        $("#LoginMinhaConta input,#LoginMinhaConta a").focus(function () {
            usuario.BoxLoginFocado = true;
            usuario.fnShowHideLogin(true);
        }).blur(function () {
            usuario.BoxLoginFocado = false;
            setTimeout(function () {
                usuario.fnShowHideLogin(false);
            }, 100);
        }).keydown(function () {
            usuario.ErroLogin = false;
        });

        $('body').click(function () {
            usuario.ErroLogin = false;
        })

        $("#txtSenhaEntrar").keydown(function (e) {
            if (e.keyCode == 13)
                fnLogar();
        });

        $("#CaptchaInputText0").keydown(function (e) {
            if (e.keyCode == 13)
                fnLogar();
        });

        $("#btn-entrar").click(function() {
            fnLogar();
        });

        $(".btn-fecharlogin").click(function(e) {
            e.preventDefault();
            usuario.BoxLoginFocado = false;
            usuario.fnShowHideLogin(false);
            return false;
        });

        $(".ecomm-login-box").mouseenter(function () {
            if(!alterandoLogin){
                alterandoLogin = true;
                setTimeout(function() {
                    usuario.fnShowHideLogin(true);
                    alterandoLogin = false;
                }, 100);
            }
        }).mouseleave(function () {
            if(!alterandoLogin){
                alterandoLogin = true;
                setTimeout(function() {
                    usuario.fnShowHideLogin(false);
                    alterandoLogin = false;
                }, 500);
            }
        }).click(function(){
            setTimeout(function() {
                usuario.fnShowHideLogin(true);
                usuario.BoxLoginFocado = true;
            }, 100);
        });

    });
</script>

