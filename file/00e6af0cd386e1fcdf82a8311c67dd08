    <script type="text/javascript">
        var idOponiniao = "0";
        var idCategoriaMenu = "0";
        var url = "/";
        var isMobileDevice = false;
        $(function() {
            $('.popup-with-form').magnificPopup({
                type: 'inline',
                preloader: false,
                callbacks: {
                    beforeOpen: function() { $("#topo_floating").fadeOut(); },
                    open: function() { var magnificPopup = $.magnificPopup.instance; magnificPopup.updateItemHTML(); }
                }
            });
            $('#txtCepCadastrar').attr({ 'onkeyup': 'if ($(this).val().length <= 14) mascara(this, mcpf); else mascara(this, mcnpj);', 'maxlength': '18' });

            usuario.GetCliente();
            fnLocate();

            $(".copyright-logo").click(function () {
                location.href = "/";
            });

            $("#mala_direta_lead button").click(function () {
                $.ajax({
                    url: '/Cliente/CadastrarLead',
                    data: { email: $("#email_news").val(), leadTipoId: 1 },
                    success: function (data) {
                        $("#mala_direta_lead").remove();
                    }
                });
            });

            $("#formNewsletter form").submit(function (e) {
                $.ajax({
                    url: '/Cliente/CadastrarLead',
                    data: { email: $("#receberNews").val(), leadTipoId: 1 },
                    success: function (data) {
                        if ($("#formNewsletter form").find(".alert").length == 0) {
                            $("#formNewsletter form").append("<div class=\"alert alert-success\"><button type=\"button\" class=\"close\" data-dismiss=\"alert\">×</button><p>Muito obrigado por se cadastrar :)</p></div>");
                        }
                        $("#receberNews").val('');
                    }
                });
                e.preventDefault();
            });
        });
    </script>
    
