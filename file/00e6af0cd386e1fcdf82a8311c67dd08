<script type="text/javascript">
    $(function () {
        $("#frmContato").validate("pt-BR");
        $('#btnEnviarEmailContato').click(function () {
            var nome = $('#campoNome').val();
            var email = $('#campoEmail').val();
            var assunto = $('#campoAssunto').val();
            var pedido = $('#campoPedido').val();
            var corpo = $('#campoMensagem').val();
            $.ajax({
                url: '/Cliente/EnviarContato',
                data: { nome: nome, email: email, assunto: assunto, pedido: pedido, corpo: corpo },
                type: "POST",
                success: function (r) {
                    if (r.Sucesso) {
                        alert('Cadastrado com sucesso');
                        location.href = "/";
                    }
                }
            });
            return false;
        });
    });
</script>

