    <form id="modalCadastro" class="popup-cadastro mfp-hide">
        <img src="/Content/Images/icon-cadastro.jpg" style="position: absolute;display: block;right: 30px;" />
        <h1 style="font-size: 20px;font-weight: normal;margin-top: 0px;">Bem-vindo(a), <p style="font-weight: bold;">visitante</p></h1>
        
    <script type="text/javascript">
        $(function () {
            $('#divConteudoCadastro input:text, #divConteudoCadastro input:password').keydown(function (e) {
                if (e.shiftKey && (e.which == 51 || e.which == 56 || e.which == 55))
                    return false;
                if (e.which == 39 || e.keyCode == 192)
                    return false;
                if (e.which == 13 || e.which == 10)
                    return false;
                return true;
            });

            $('input.disablePaste').keydown(function (event) {
                var forbiddenKeys = new Array('c', 'x', 'v');
                var keyCode = (event.keyCode) ? event.keyCode : event.which;
                var isCtrl;
                isCtrl = event.ctrlKey
                if (isCtrl) {
                    for (i = 0; i < forbiddenKeys.length; i++) {
                        if (forbiddenKeys[i] == String.fromCharCode(keyCode).toLowerCase()) {
                            return false;
                        }
                    }
                }
                return true;
            });

            $('#cpfCnpj')
                .attr({ 'onkeyup': 'if ($(this).val().length <= 14) mascara(this, mcpf); else mascara(this, mcnpj);', 'maxlength': '18' })
            .focusout(function () {
                var tipo = $("#cpfCnpj").val().length == 14 ? "fisica" : "juridica";
                $("#tipo").val(tipo);

                if (tipo == "juridica") {
                    $("#nome").attr("placeholder", "Razão Social");
                    $("#sobrenome").fadeIn();
                } else {
                    $("#nome").attr("placeholder", "Nome Completo");
                    $("#sobrenome").fadeOut();
                }
            });

            $('input.disablePaste').bind("cut copy paste", function (e) {
                e.preventDefault();
            });

            $("#modalCadastro").validate("pt-BR");

            $("#modalCadastro").submit(function (e) {
                e.preventDefault();

                if ($(this).validate().valid()) {
                    $("#btnCadastrar").attr("disabled", "disabled");
                    $("#btnCadastrar").text("Processando informações...");
                    var data = $('#modalCadastro').serialize();
                    var oldText = $("#btnCadastrar").text();
                    $.ajax({
                        url: '/Cliente/CadastroRapido',
                        data: data,
                        type: "POST",
                        success: function (r) {
                            $("#btnCadastrar").removeAttr("disabled");

                            if (r.Sucesso) {
                                var nome = $("#nome").val().split(" ")[0];

                                $("#retornoMensagem").css({
                                    "color": "green",
                                    "border-left": "3px solid green",
                                    "padding": "3px",
                                    "font-style": "italic"
                                }).text(r.Mensagem);

                                $("#divCamposCadastro").fadeOut();

                                usuario.MostraTopo(nome);
                                $.magnificPopup.close();
                                usuario.Logado = true;

                                if (fnCadastroComplete != null) {
                                    fnCadastroComplete(nome);
                                }

                            } else {
                                $("#retornoMensagem").css({
                                    "color": "red",
                                    "border-left": "3px solid red",
                                    "padding": "3px",
                                    "font-style": "italic"
                                }).text(r.Mensagem);

                                $("#btnCadastrar").text("Cadastrar-se");
                            }
                        }
                    });
                }
                return false;
            });

            $(".cpf").unmask().mask('999.999.999-99');
            $(".cnpj").unmask().mask('99.999.999/9999-99');
        });
    </script>
<p id="retornoMensagem">
    <b>Preencha os campos:</b>
</p>
