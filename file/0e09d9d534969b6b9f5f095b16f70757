<script language="javascript"> 
	function ecpsc11(Img, pos){
	  document.getElementById(Img).style.top=(document.documentElement.scrollTop)+"px";
	  if (pos == 'r') {
	    document.getElementById(Img).style.right="0px";
	  }
	}
	window.onscroll=function(){
	  ecpsc11('img2_div');
	  ecpsc11('img3_div', 'r');
	};
	window.onresize=function(){
	  ecpsc11('img2_div');
	  ecpsc11('img3_div', 'r');
	};
	ecpsc11('img2_div');
	ecpsc11('img3_div', 'r');
 
</script>
