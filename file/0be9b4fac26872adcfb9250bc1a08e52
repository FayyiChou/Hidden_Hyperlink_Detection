//UC中分享
function shareForUc(){
	var a=$(this).attr("id"),
	d= $("#top").find("h2").text();
	g= $("#shlink").val();
	c="http://img.weijie.so/img/content/150829/0918214657.jpg";
	d.indexOf("%")&&(d=d.replace(/%/gi,"\uff05"));
	var config = {
	            url:g,
	            title: d,
	            desc:d,
	            img: c,
	            img_title:d,
	            from:"全球最火热微博"
	        };
	        var share_obj = new nativeShare(config);
	        $("#vr-share>ul").find("a").click(function(){
	        	var ap = $(this).attr("data-app");
	        	share_obj.share(ap);
	      });
}

//qq浏览器中分享
function shareForQb(){
	var a=$(this).attr("id"),
	d= $("#top").find("h2").text();
	g=$("#shlink").val();
	c="http://img.weijie.so/img/content/150829/0918214657.jpg";
	d.indexOf("%")&&(d=d.replace(/%/gi,"\uff05"));
	var config = {
	            url:g,
	            title: d,
	            desc:d,
	            img: c,
	            img_title:d,
	            from:d
	        };
	        var share_obj = new nativeShare(config);
	        $("#vr-share>ul").find("a").click(function(){
	        	share_obj.share($(this).attr('data-app'));
	      });
}

