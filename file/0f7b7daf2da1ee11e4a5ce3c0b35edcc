		$().ready(function() {  
			$('.kwicks').kwicks({  
				max : 428,
				spacing : 0,
				duration: 400,
				event: 'click'
			}); 
		$(".kwicks li .more").click(function(){
				$(".kwicks li .more").animate({bottom:-40}, 200)
				$(this).next().animate({bottom:0}, 600, "easeOutBack")
				return false
			});
		$(".kwicks li").mouseleave(function(){
				$(".kwicks li .more").stop().animate({bottom:0}, 400)
				$(this).find(".caption").stop().animate({bottom:-200}, 600)
			});
		}); 
	</script>
