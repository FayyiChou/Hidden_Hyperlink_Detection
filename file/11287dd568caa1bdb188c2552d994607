	$(function() {
		//1: pkgusage
		$.get("index/pkgUsage", "type=1&rnd="+Math.random(), function(data) {
			console.log(data.localRemainderStr);
			if (data.localRemainderStr) {
				var localRemainder=data.localRemainderStr;
				var domesticRemainder=data.domesticRemainderStr;
				var directedRemainder=data.directedRemainderStr;
				//TODO 设置当前流量
				$("#localRemainder").text(localRemainder + "MB");
				$("#domesticRemainder").text(domesticRemainder + "MB");
				$("#directedRemainder").text(directedRemainder + "MB");
			} else {
			}
		});
		if($("#modal_div").length > 0){
			var modalHtml=$("#modal_div").html();
			createFmodal(modalHtml,false);
		}
	});
	
	function pkgDetail(id){
		top.location.href="pkgDetail?id="+id;
