<script type="text/javascript">
	function getUrlRelativePath() {
		var url = document.location.toString();
		var arrUrl = url.split("//");
		var start = arrUrl[1].indexOf("/");
		var relUrl = arrUrl[1].substring(start);
		if (relUrl.indexOf("?") != -1) {
			relUrl = relUrl.split("?")[0];
		}
		return relUrl;
	}

	var path = getUrlRelativePath();
	if (path.indexOf("indexv") > -1) {
		var doc = document.getElementById("index_voice");
		doc.className += " active";
	} else if (path.indexOf("indexcc") > -1) {
		var doc = document.getElementById("index_cc");
		doc.className += " active";
	} else if (path.indexOf("circle") > -1) {
		var doc = document.getElementById("index_circle");
		doc.className += " active";
	} else {
		var doc = document.getElementById("index_flow");
		doc.className += " active";
	}
</script></div>
			</div>
