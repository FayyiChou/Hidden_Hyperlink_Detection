var ymurl=window.location.href;   //接收地址
ymurl=ymurl.toLowerCase();
$(".dq-list a").each(function(){
	var dqurl = $(this).attr("href").toLowerCase();
	if(dqurl==ymurl){
		$(".dq-list a[class='active']").removeClass("active");
		$(this).addClass("active");
	}
	
});

