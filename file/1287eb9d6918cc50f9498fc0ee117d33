<script type="text/javascript">
    var isMenuAnimate = false;
    var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
    $(function () {
        $(window).on('scroll', function() {
            if (!isMenuAnimate) {
                var _scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
                if (_scrollTop >= 0) {
                    var _scroll = _scrollTop - scrollTop;
                    if (_scroll > 40 || _scroll < -20) {
                        isMenuAnimate = true;
                        $(".mainMenu").animate({height: _scroll > 0 ? "0px" : "50px"}, 60, function () {
                            if(_scroll > 0) {
                                $(this).css("display", "none");
                            } else {
                                $(this).css("display", "block");
                            }
                            isMenuAnimate = false;
                        });
                        $(document.body).trigger('wptNotice_view:bottom', [_scroll > 0 ? "0px" : "51px"]);
                        scrollTop = _scrollTop;
                    }
                }
            }
        });

        //有新消息
        $.getJSON(
            "/find/hasNewMessage",
            function (res) {
                try {
                    if (res.status.code == 0 && typeof res.type != "undefined") {
                        $(".mainMenu").find("."+ res.type + " .redPoint").show();
                    }
                } catch (e) {
                }
            }
        );

        $('.my, .contactSeller').on('click',function (e) {
            if(typeof WPT != 'undefined' && (WPT.pc || WPT.app)) {
                e.preventDefault();
                var self = $(this);
                var href_a = $(this).parent().parent();
                var href_attr = href_a.attr('href');

                $.getJSON('/scanScope/getRedirectUrl?url=' + href_attr, function (res) {
                    if (res.status.code == 0) {
                        wptRedirect(res.data.url);
                    }
                });
            }
        });
    });
</script>
