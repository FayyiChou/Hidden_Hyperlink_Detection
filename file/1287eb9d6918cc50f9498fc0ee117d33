</style><script type="text/javascript">
    var shopUri = '16101419315HPY3w';
    var tab = 'home';
    var bodyTop = 0;
    var saleEndTimeArr = [];
    var page_num = 16;
    var hasRedpack = 0;

    var wxShare = {
        title: '与这个店铺一言为定，将宝贝一网“拍”尽！',
        desc: '『工艺世家文玩』的微拍堂店铺',
        link: 'http://r.weipaitang.cn/shop/16101419315HPY3w?fuu=guest',
        imgUrl: 'http://cdn01.weipaitang.com/certify/20161017sjY0BB07t7cJP5MtK5zv512RvzRS5AxDy_sp_S_hD9CUyZ4AWLydS1kan82gRie0-W627H640/w/64',
        pics: [],
        summary: '与这个店铺一言为定，将宝贝一网“拍”尽！',
        trigger: function (res) {
            //监听Menu中的按钮点击时触发的方法，该方法仅支持Menu中的相关接口。
        },
        success: function (res) {
            //分享成功回调
            $(document).trigger(
                "wptStatistic:sendData",
                ['{"data":' + JSON.stringify($.extend({
                    'source': 'shop',
                }, _statisticData)) + ', "route": "share/data"}']
            );
        },
        cancel: function (res) {
        },
        fail: function (res) {
        }
    };

    window.wx && wx.config({
        debug: false,
        appId: "wx2aa1f6beaa714b7c",
        timestamp: "0",
        nonceStr: "",
        signature: "",
        jsApiList: [
            "onMenuShareTimeline",
            "onMenuShareAppMessage",
            "onMenuShareQQ",
            "hideOptionMenu",
            "showOptionMenu",
            "closeWindow",
            "chooseImage",
            "uploadImage",
            "previewImage"
        ]
    });

    wx.ready(function () {
        wx.showOptionMenu();
        wx.onMenuShareAppMessage(wxShare);
        wx.onMenuShareTimeline({
            title: wxShare.summary,
            desc: '',
            link: wxShare.link,
            imgUrl: wxShare.imgUrl,
            success: wxShare.success
        });
    });

    function loadNextPage(hide) {
        hide = hide || false;
        if ($(".loadNextPage").length > 0) {
            var tabObj = tab == 'home' ? $("#popularity") : $("#" + tab);
            if (tabObj.attr("page")) {
                loadNextPageByPage(hide, tabObj);
            }
        }
    }

    function loadNextPageByPage(hide, tabObj) {
        var pageloadOver = tabObj.attr("pageloadOver");
        if (pageloadOver == 1) return;
        var page = parseInt(tabObj.attr("page"));
        if (isNaN(page)) return;

        tabObj.attr("pageloadOver", 1);
        var saleItemLength = tabObj.find('.saleItem').length;
        if (!hide && saleItemLength > 0 && !(saleItemLength % page_num)) {
            $(".loadNextPage").show();
        }
        $.get('/shop/16101419315HPY3w?ajax=true&tab=' + tab + '&page=' + page, function (html) {
            $(".loadNextPage").hide();
            if (html.trim()) {
                tabObj.append(html).attr("page", page + 1).attr("pageloadOver", 0);
            }

            var shopEmpty = $('.shopEmpty');
            if ($.trim(tabObj.html())) {
                shopEmpty.hide();
                $(document).trigger("wptLazyLoad:load");

                //触发曝光统计
                $(document).trigger("wptStatistic:sendExposureData");
            } else {
                shopEmpty.show();
                tabObj.attr("pageloadOver", 0);
            }
        });
    }

    function countDown(key) {
        var countDownTime = $('#countDownTime_' + key);
        var leftTime = countDownTime.attr('leftTime');
        if (leftTime >= 0) {
            var hour = parseInt(leftTime / 3600);
            hour = hour < 10 ? '0' + hour : hour;
            var min = parseInt((leftTime % 3600) / 60);
            min = min < 10 ? '0' + min : min;
            var sec = parseInt((leftTime % 3600) % 60);
            sec = sec < 10 ? '0' + sec : sec;
            countDownTime.find('.timeHour').text(hour);
            countDownTime.find('.timeMin').text(min);
            countDownTime.find('.timeSec').text(sec);
            countDownTime.attr('leftTime', leftTime - 1);
        } else {
            clearInterval(saleEndTimeArr["setInterval_" + key]);
        }
    }

    function fixedTabList() {
        var sHeight = document.documentElement.scrollTop || document.body.scrollTop;
        var tabListBoxTop = $('.tabListBox').get(0).offsetTop;
        var tabListBoxFixed = $('.tabListBoxFixed');
        if (tabListBoxTop > 0 && sHeight > tabListBoxTop) {
            tabListBoxFixed.show();
        } else if (sHeight < $('.header').height()) {
            tabListBoxFixed.hide();
        }
    }

    $(function () {
        var tabListBoxOffset = $('.tabListBox').offset();
        $('.shopEmpty').css('top', tabListBoxOffset.top + tabListBoxOffset.height);

        $(".qrcode").on("click", function () {
            var self = $(this);
            wptLoading("正在打开...", function () {
                $.getJSON("/user/getShopQR/" + shopUri + "", function (res) {
                    $(document).trigger("wptLoading_view:hide");
                    if (res.errcode == 0) {
                        var shopQRUrl = res.shopQRUrl + "?t=" + Date.now();
                        if(WPT.pc) {
                            WPT.Model.imgCode(shopQRUrl);
                        } else {
                            wx.previewImage({
                                current: shopQRUrl,
                                urls: [shopQRUrl]
                            });
                        }
                    }
                });
            });
        });

        $('.attention').on('click', function (e) {
            e.preventDefault();
            var self = $(this);
            var data = 'userinfoUri=' + shopUri + '&ajax=1';

            var toggleAttention = function () {
                $.getJSON('/user/attention', data, function (res) {
                    if(res.errcode >= 400){
                        wptAlert(res.errmsg);
                        return false;
                    }
                    self.animate({"opacity": 0}, 300, function () {
                        if (self.hasClass('attentioned')) {
                            self.removeClass('attentioned');
                            self.text("+ 关注");
                        } else {
                            self.addClass('attentioned');
                            self.text("已关注");
                            if(res.errcode == 0) {
                                $(document).trigger("wptStatistic:sendData", ['{"data":{"uu":"' + WPT.userinfo.uri + '","su":"' + shopUri + '"}, "route":"attention"}']);
                            }
                        }
                        self.animate({"opacity": 1}, 300);
                    });
                });
            };
            if (self.hasClass('attentioned')) {
                $(document).off('wptConfirm_view:sure').one('wptConfirm_view:sure', toggleAttention);
                wptConfirm('取消关注,将无法看到该店铺的拍品', {text: "取消关注", class: "btn-delete"});
            } else {
                data = 'userinfoUri=' + shopUri + '&act=attention&ajax=1';
                if (!isSubscribe) {
                    wptSubscribe('', 'http://cdn01.weipaitang.com/res/img/subscribe/shopAttention.png', location.pathname + '#attention_' + shopUri);
                    return false;
                }
                toggleAttention();
            }
        });

        $('.tabBox').on('click', function (e) {
            e.preventDefault();
            var self = $(this);
            tab = self.attr("tab");
            if(tab == 'allSale'){
                wptRedirect('/wpt/index/' + shopUri);
            } else {
                var index = self.index();
                $('.tabBox.selected').removeClass('selected');
                $('.tabListBox').find('.tabBox').eq(index).addClass('selected');
                $('.tabListBoxFixed').hide().find('.tabBox').eq(index).addClass('selected');

                $('.shopEmpty').hide();
                $(".loadNextPage").hide();

                $('.saleMain').hide();
                $('.saleMain[id="' + tab + '"]').show();

                var scrollTop = parseInt($('.saleMain[id="' + tab + '"]').data('scroll'));
                scrollTop = isNaN(scrollTop) ? 0 : scrollTop;
                window.scrollTo(0, scrollTop);

                //更新路径
                var page = parseInt($("#" + tab).attr("page"));
                if (!$.os.android || ignorePhoneModel.indexOf(getPhoneModel()) == -1) {
                    history.replaceState(null, document.title, "?tab=" + tab + "&bodyTop=" + scrollTop + (!isNaN(page) ? "&page=" + page : ""));
                }

                //为空加载内容
                var saleMain = $('.saleMain[id="' + tab + '"]');
                if(tab == 'home') {
                    saleMain = saleMain.find(".popularitySaleMain");
                }
                if (saleMain.attr("page") == 1) {
                    loadNextPage(true);
                }
                fixSupport();
            }
        });

        $('.saleMain').on('click', '.saleItem', function (e) {
            e.preventDefault();
            //更新路径
            var tabObj = tab == 'home' ? $("#popularity") : $("#" + tab);
            var page = parseInt(tabObj.attr("page"));
            if (!$.os.android || ignorePhoneModel.indexOf(getPhoneModel()) == -1) {
                history.replaceState(null, document.title, "?tab=" + tab + "&bodyTop=" + $("body").scrollTop() + (!isNaN(page) ? "&page=" + page : ""));
            }

            var self = $(this);

            if(self.hasClass('fgSaleItem')){
                wptRedirect('/fightGroupSale/' + self.attr("saleUri") + "?r=shop&sc=W00004");
            }else{
                wptRedirect('/uri/' + self.attr('saleUri') + "?r=shop");
            }
        });

        //点赞
        $(".saleMain").on("click", ".like", function() {
            var self = $(this);
            var uri = self.parents('.singleSaleItem').data('uri');
            if(!self.hasClass("liked")) {
                $.getJSON(
                    '/wpt/likeIt/' + uri + '?t=1491066863',
                    function(res) {
                        var likeNum = parseInt((1 * self.text()) + 1);
                        self.addClass("liked").find('span').text(likeNum);
                        self.prev().find('span').text(parseInt(self.prev().text()) + 10);

                        //采集数据 关注
                        if(typeof res.isAttention != 'undefined' && res.isAttention == -1) {
                            $(document).trigger("wptStatistic:sendData", ['{"data":{"u":"' + uri + '","uu":"' + WPT.userinfo.uri + '","su":"' + res.su + '","uc":"' + res.uc + '","usc":"' + res.usc + '"}, "route":"attention"}']);
                        }
                        //采集数据 点赞
                        $(document).trigger("wptStatistic:sendData", ['{"data":{"u":"' + uri + '","uu":"' + WPT.userinfo.uri + '","su":"' + res.su + '","uc":"' + res.uc + '","usc":"' + res.usc + '"},"route":"like"}']);
                    }
                );
            }
        });

        //加载下页
        $(window).on('scroll', function () {
            var sHeight = document.documentElement.scrollTop || document.body.scrollTop;
            var wHeight = document.documentElement.clientHeight;
            var dHeight = document.documentElement.offsetHeight;
            if (dHeight - (sHeight + wHeight) < 340) {
                loadNextPage();
            }

            fixedTabList();
            $('#' + tab).data('scroll', sHeight);
        });

        //初始化tab菜单
        $('.tabBox.selected').removeClass('selected');
        $('.tabBox[tab="' + tab + '"]').addClass('selected');
        fixedTabList();
        //初始化saleMain
        $('.saleMain').hide();
        $('.saleMain[id="' + tab + '"]').show();
        if($('.saleMain[id="' + tab + '"]').html().trim() == "") {
            $('.shopEmpty').show();
        }
        //定位
        if(bodyTop > 0) {
            window.scrollTo(0, bodyTop);
        }

        //加载lazy图片
        $.picLazyLoad({
            seletctor: ".lazyLoad",
            threshold: $(window).height()
        });

        // banner 更多
        $(".singleSaleItem .banner").on("click", function(){
            var url = $(this).attr("url");
            if(url) {
                wptRedirect(url);
            }
        });

        //代理新手红包提示
        if(hasRedpack) {
            var redPackObj = $(".redPack");
            var moneyObj = redPackObj.find(".box").find(".money");
            $.getJSON('/thirdPart/marketingTools/representRedpack/16101419315HPY3w', function (res) {
                if (res.errcode > 0) {
                    wptAlert(res.errmsg);
                }else{
                    $(moneyObj).html(res.fee + "<span> 元</span>");
                    $(".receivedEffect").show();
                    $(".mask").show();
                    $(redPackObj).show().animate({width: "90%", height: "90%", marginTop: "5%", marginLeft: "5%"}, {
                        duration: "slow", easing: 'cubic-bezier(0, .04, 0, 1.53)', complete: function () {
                            $(moneyObj).show();
                            $(redPackObj).find(".box").find(".tip").show();
                            $(".receivedEffect").on("click", function () {
                                $(".receivedEffect").hide();
                                $(".mask").hide();
                                $(".redPack").hide();
                            })
                        }
                    });
                }
            });
        }
    });
</script></head>





