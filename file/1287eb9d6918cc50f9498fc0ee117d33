<script type="text/javascript" src="http://cdn01.weipaitang.com/res/js/zepto.wptStatistic.min.js?v=0.9.1.48"></script>
<script language="javascript">
    var reg = new RegExp("(^|&)sc=([^&]*)(&|$)", "i");
    var matches = window.location.search.substr(1).match(reg);
    var scene = "";
    if (matches != null) {
        scene = unescape(matches[2]);
        sessionStorage.setItem('sc', scene);
    }

    var _statisticData = _behaviorData = {};
    try {
        _statisticData = JSON.parse('{"su":"16101419315HPY3w"}');
        _behaviorData = JSON.parse('{}');
    } catch(e) {}

    var wptStatisticData = null;

    window.WPT = window.WPT || {};
    window.WPT.ip = '101.199.108.58';

    $(function () {
        //统计初始化
        wptStatisticData = $.wptStatistic();
        _statisticData = $.extend(_statisticData, {
            uu: "",
            ref: "aHR0cDovL3cud2VpcGFpdGFuZy5jb20vdXJpLzE3MDMzMDIzMDJkOGwzOGk/cg=="
        });
        $(document).trigger(
            "wptStatistic:sendData",
            ['{"data":' + JSON.stringify(_statisticData) + ', "route": "visit"}']
        );
        //触发曝光统计
        $(document).trigger("wptStatistic:sendExposureData");

        //千人千面数据采集
        if(!$.isEmptyObject(_behaviorData) && WPT && WPT.userinfo && WPT.userinfo.uri) {
            _behaviorData = $.extend(_behaviorData, {
                uu: WPT.userinfo.uri
            });
            setTimeout(function () {
                $(document).trigger(
                    "wptStatistic:sendData",
                    ['{"data":' + JSON.stringify(_behaviorData) + ', "route": "behavior"}']
                );
            }, 3000);
        }

        if(WPT && WPT.userinfo && WPT.userinfo.uri) {
            // 用户变化数据，user_uri, IP，屏幕分辨率, 手机型号
            var screen = '';
            if(window.screen.width && window.screen.height) {
                screen = window.screen.width + "_" + window.screen.height;
            }
            var _data = {
                "user_uri": WPT.userinfo.uri,
                "ip": window.WPT.ip,
                "screen": screen,
                "phone_model": navigator.userAgent
            };

            var _cookieData = getCookie('behaviorUserinfo');
            var _curData = JSON.stringify(_data);
            if(_curData != _cookieData) {
                $.ajax({
                    url: '//ua.weipaitang.com/api/v1.0/updateUserInfo',
                    data: _data,
                    dataType: "jsonp"
                });
                setCookie('behaviorUserinfo', _curData, 7);
            }
        }
    });
</script>

