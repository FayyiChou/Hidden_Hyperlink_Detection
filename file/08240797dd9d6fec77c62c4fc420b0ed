				tmpl: "27", target_id: "js-recommend-bicycle-new", category: ["%E8%87%AA%E8%BB%A2%E8%BB%8A%E6%9C%AC%E4%BD%93%E3%83%BB%E3%83%95%E3%83%AC%E3%83%BC%E3%83%A0"]			}
		}
	});
	(function() {
		var timerID = setInterval(function() {
			if (document.getElementById('js-recommend-bicycle-new').childNodes[1] != undefined) {
				var $e = $('#js-recommend-bicycle-new');
								
				//見出しを置換
				$e.find('.heading').html('新着商品<span>自転車新着商品をチェック！</span>');
				
				//カルーセルスタート
				$e.find('.owl-carousel').owlCarousel({
										navigation: true,
					pagination: false
				});
	
				//タイマー解除
				clearInterval(timerID);
				timerID = null;
			}
		} , 100);
	})();
</script>
