		<script>
			var $menu = $('#navbar .dropdown-menu');
			$menu.menuAim({
				activate: activateSubmenu,
				deactivate: deactivateSubmenu
			});
			function activateSubmenu(row) {
				var $row = $(row),
					submenuId = $row.data('submenu-id'),
					$submenu = $("#" + submenuId),
					height = $menu.parent('.menu-aim').height(),
					width = $menu.outerWidth();
				
				$submenu.css({
					display: "block",
					top: 0,
					left: 177,
					height: height
				});
				$row.find('a').addClass('maintainHover');
			}
			function deactivateSubmenu(row) {
				var $row = $(row),
					submenuId = $row.data('submenu-id'),
					$submenu = $('#' + submenuId);
				
				$submenu.css('display', 'none');
				$row.find('a').removeClass('maintainHover');
			}
			//メニューを開くだけ
			function openSubmenu() {
				if ($('#navbar .menu-aim').is(':hidden')) {
					$('#navbar .cell-category .btn').addClass('active');
					$('#navbar .menu-aim').show();
				}
			}
			//メニューを閉じる
			function closeSubmenu() {
				if ($('body.top').length) {
					if ($('body').width() >= 1200) {
						$('#navbar .submenu').hide();
					} else {
						$('#navbar .cell-category .btn').removeClass('active');
						$('#navbar .menu-aim').hide();
					}
				} else {
					$('#navbar .cell-category .btn').removeClass('active');
					$('#navbar .menu-aim').hide();
				}
			}
			$(function() {
				$('.dropdown-menu li').on('click', function(e) {
					e.stopPropagation();
				});
				$('#navbar .cell-category .btn').on('mouseenter', function() {
					openSubmenu();
				});
				$('#navbar .menu-aim').on('mouseleave', function() {
					closeSubmenu();
				});

				//メニューの状態監視
				var menuon = false;
				$('body').on('mouseover', function() {
					menuon = false;
				});
				$('body').on('mouseover', '#navbar .btn:eq(0), .menu-aim', function() {
					menuon = true;
					return false;
				});
				setInterval(function() {
					if (!menuon) {
						closeSubmenu();
					}
				}, 100);

				//初回起動
				$('#navbar .menu-aim').hide();
				if ($('body.top').length) {
					//ウィンドウサイズの監視
					$(window).setBreakpoints({
						distinct: true,
						breakpoints: [1200]
					});
					$(window).bind('enterBreakpoint1200', function() {
						openSubmenu();
					});
					$(window).bind('exitBreakpoint1200', function() {
						closeSubmenu();
					});
				}
				//ブランドの表示
				$('#navbar .menu-aim .more a').on('click', function() {
					if ($(this).hasClass('open')) {
						$(this).removeClass('open').text('もっと見る');
						$(this).parents('.brand').find('.toggle').removeClass('visible');
					} else {
						$(this).addClass('open').text('閉じる');
						$(this).parents('.brand').find('.toggle').addClass('visible');
					}
					return false;
				});
				//検索ボックスのカテゴリ
				$('#navbar .cell-search-type .btn').on('click', function() {
					$('#navbar .custom-select').toggle();
				});
				//formのaction属性を書き換える
				$('#navbar .custom-select a').on('click', function() {
					$('#navbar .custom-select a').removeClass('active');
					$(this).addClass('active');
					$(this).parents('#navbar').find('.cell-search-type .btn span').text($(this).text());
					$(this).parents('#navbar').find('form').attr('action', $(this).attr('href'));
					$('#navbar .custom-select').hide();
					return false;
				});
				//初回起動
				if (!$('#navbar .custom-select a.active').length) {
					$('#navbar .custom-select a').eq(0).addClass('active');
				}
				$('#navbar .custom-select a.active').trigger('click');
				//サブミット
				$('#navbar .cell-submit .btn').on('click', function() {
					$(this).parents('form').submit();
				});
			});
		</script>


