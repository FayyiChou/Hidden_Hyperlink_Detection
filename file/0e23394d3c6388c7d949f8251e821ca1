<script>
jQuery(function(){
var flower_price = new Array();
flower_price[11] = 1100;
flower_price[99] = 9600;
flower_price[520] = 50000;
flower_price[999] = 96000;
flower_price[1314] = 120000;
jQuery('.fighting-value').hover(function(){
jQuery(this).find('.fighting-value-info').css({'left':'124px'});
},function(){
jQuery(this).find('.fighting-value-info').css({'left':'-9999px'});
});
jQuery('.pmm3__btn').on('click', function(ev){
ev.preventDefault()
var to_uid = jQuery(this).attr('to_uid');
var to_username = jQuery(this).attr('to_username');
jQuery('input[name="to_uid"]').attr("value", to_uid);
jQuery('.pmmbox__title em').html(to_username);
jQuery('#J_pmmmask').show()
jQuery('#J_pmmbox').show()
});
jQuery('#J_pmmbox').find('.pmmbox__item').on('click', function(ev){
jQuery('#J_pmmbox').find('input').attr('checked', false)
jQuery(this).find('input').attr('checked', 'checked');
jQuery('.pmmbox__total em').html(flower_price[jQuery(this).find('input').val()]);
});
jQuery('#J_pmmbox').find('.icon-close').on('click', function(ev){
jQuery('#J_pmmmask').hide()
jQuery('#J_pmmbox').hide()
});
var appendhtml = new Array();
});
</script>
