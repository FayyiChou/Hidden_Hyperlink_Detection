<script type="text/javascript">
function changesig(status) {
var obj = document.getElementsByTagName('div');
  if (document.getElementById('hidesig')) {
if(status>0) {
setcookie('hidesig', 1,  -(86400 * 30 * 1000));
document.getElementById('hidesig').innerHTML = "隐藏签名";
document.getElementById('hidesig').onclick = function () {
changesig(0);
}
}else {
setcookie('hidesig', 1, 86400 * 30 * 365);
document.getElementById('hidesig').innerHTML = "显示签名";
document.getElementById('hidesig').onclick = function () {
changesig(1);
}
}
  }

for (i=0;i<obj.length;i++){
if(obj[i].id.match(/signatures_[0-9]+/i)) {
if(status>0) {
obj[i].style.display='';
} else {
obj[i].style.display='none';
}
}
}
return false;
}
var hidesig = getcookie('hidesig');
if(hidesig>0) changesig(0);


</script>


