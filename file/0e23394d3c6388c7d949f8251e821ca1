jQuery(document).ready(function(){
if(discuz_uid > 0){
jQuery.ajax({
url: ('/home.php?mod=space&do=subscribe&inajax=1&type=subscribe_push_count'),
dataType: 'json',
cache: false,
success: function(data)
{
if(data != 0){
jQuery('#subscribe_push_count').addClass('new');
jQuery('#subscribe_push_count').html('动态('+data+')');
jQuery('#subscribe_push_count').css('background-image','url(../../static/image/common/notice.gif)');
jQuery('#subscribe_push_count').css('background-position','3px 50%;');

}
}
});

var uids = new Array();
jQuery('.bui').children('.btn-follow').each(function(){
uids.push(jQuery(this).attr('touid'));
});
jQuery.ajax({
url: ('/home.php?mod=space&do=subscribe&inajax=1&type=subscribe_check'),
dataType: 'json',
data:{uids:uids},

success: function(data)
{

for(var o in data){
if(data[o] == 1){
jQuery('.bui').children('.btn-follow').eq(o).attr('class','btn-follow is-following');
jQuery('.bui').children('.btn-follow').eq(o).attr('type',"del");
jQuery('.bui').children('.btn-follow').eq(o).html('已关注');
}else if(data[o] == 'self'){
jQuery('.bui').children('.btn-follow').eq(o).html('');
jQuery('.bui').children('.btn-follow').eq(o).addClass('important-hidden-tao');
}

}

}
});
var dom_userRmb = document.getElementById('user_rmb_from_5153');
if (dom_userRmb && g_yyuid && g_yyuid != '0') {
jQuery.ajax({
url: ('/forum.php?mod=ajax&action=getUsrBalanceIn5153'),
dataType: 'json',
cache: false,
success: function(datalist)
{
if (datalist
&& datalist.rs
&& datalist.rs == 1
&& typeof(datalist.balance) == 'number'
&& isNaN(datalist.balance) == false
){
dom_userRmb.innerHTML = datalist.balance;
}
}
});
}

var dw_dwjd = document.getElementById('dw_dwjd');
if (dw_dwjd && g_yyuid && g_yyuid != '0') {
jQuery.ajax({
url: 'http://plus.duowan.com/open/getUserGold?appid=bbs&yyuid=' + g_yyuid+'&time='+jQuery.now(),
dataType: 'jsonp',
cache: false,
success: function(datalist)
{
if (datalist && datalist.code == 0
){
if(typeof datalist.data != 'undefined' && typeof datalist.data.gold != 'undefined')
dw_dwjd.innerHTML = datalist.data.gold;
else dw_dwjd.innerHTML = '0';
}
}
});
}
}
});



</script>


