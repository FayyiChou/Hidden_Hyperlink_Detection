//<!CDATA[ 
function g(o){return document.getElementById(o);} 
function HoverLi(n){ 
//如果有N个标签,就将i<=N; 
//本功能非常OK,兼容IE7,FF,IE6 
for(var i=1;i<=2;i++){g('tb_'+i).className='normaltab';g('tbc_0'+i).className='undis';}g('tbc_0'+n).className='dis';g('tb_'+n).className='hovertab'; 
} 
function HoverLi8(n){ 
//如果有N个标签,就将i<=N; 
//本功能非常OK,兼容IE7,FF,IE6 
for(var i=1;i<=3;i++){g('tb8_'+i).className='normaltabb';g('tb8b_0'+i).className='undisc';}g('tb8b_0'+n).className='disc';g('tb8_'+n).className='hovertabb'; 
}

function HoverLi9(n){ 
//如果有N个标签,就将i<=N; 
//本功能非常OK,兼容IE7,FF,IE6 
for(var i=1;i<=4;i++){g('tb9_'+i).className='normaltabc';g('tb9c_0'+i).className='undisc';}g('tb9c_0'+n).className='disc';g('tb9_'+n).className='hovertabc'; 
}

//如果要做成点击后再转到请将<li>中的onmouseover 改成 onclick; 
//]]> 
</script> 
