$(document).ready(function() {
	jQuery.jqtab2 = function(tab2tit,tab2_conbox,shijian) {
		$(tab2_conbox).find("li").hide();
		$(tab2tit).find("li:first").addClass("thistab2").show(); 
		$(tab2_conbox).find("li:first").show();
	
		$(tab2tit).find("li").bind(shijian,function(){
		  $(this).addClass("thistab2").siblings("li").removeClass("thistab2"); 
			var activeindex = $(tab2tit).find("li").index(this);
			$(tab2_conbox).children().eq(activeindex).show().siblings().hide();
			return false;
		});
	
	};
	/*调用方法如下：*/
	
	$.jqtab2("#tab2s2","#tab2_conbox2","mouseenter");
	
});
</script> 
