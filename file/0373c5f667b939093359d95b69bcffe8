        function show_today() {
            var today = new Date();
            var weeks = new Array("星期日","星期一","星期二","星期三","星期四","星期五","星期六");
            var str = (today.getFullYear()<100 ? today.getFullYear()+1900:today.getFullYear())+"年"+(today.getMonth()+1)+"月"+today.getDate()+"日";
            str = str + " " + weeks[today.getDay()];
            document.write(str);
        }
        show_today();
        </script>
        </dd>
