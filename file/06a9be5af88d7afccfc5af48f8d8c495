    function fun_load_img(obj,_threshold)
    {
        if( typeof obj == "object")
        {
            var _threshold = typeof _threshold != "undefined" ? _threshold : 150;
            obj.lazyload({
                placeholder:"http://img1.xcarimg.com/newcar/defaultimg.png",
                data_attribute:"src",
                threshold:_threshold
            })
        }
    }
