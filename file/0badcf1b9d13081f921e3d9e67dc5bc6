                                        $(document).ready(function () {
                                            $('#usermsg a').click(function () {
                                                if ($('.menu_top').is(':hidden')) {
                                                    $(this).addClass('current');
                                                    $("#forsearch").removeClass('current');
                                                    $('.menu_top').show();
                                                } else {
                                                    $(this).removeClass('current');
                                                    $('.menu_top').hide();
                                                }
                                            });
                                        });
