<div class="banner banner-index" onclick="LinkTo()"></div>
<script src="/News/Banner?Column=Home" type="text/javascript"></script>
<script type="text/javascript">
    var images = [Host + "/content/images/banner01.jpg", Host + "/content/images/banner02.jpg"];
    if (banners.length > 0) {
        $(".banner-index").attr("style", "cursor:pointer;");
        images = [];
        for (i = 0; i < banners.length; i++) {
            images.push(banners[i]["img"]);
        }
    }

    $(function () {
        $('.banner-index').backstretch(images, { fade: 750, duration: 3000 });
    });

    function LinkTo() {
        if (banners.length > 0) {
            for (i = 0; i < banners.length; i++) {
                if ($(".backstretch img").attr("src") == banners[i]["img"] && banners[i]["url"] != "") {
                    window.location.href = banners[i]["url"];
                }
            }
        }
    }

</script>


