        var $tips =  $(".modal-wrap");
          $(window).on("load",function(){
            var _w = $(window).width()/3.5;
            var _h = $(window).height()/2;
            var box_h = _w-$("#modal-wrap").width();
            $("#modal-wrap").css({"top":_h+'px',"left":'50%',"margin-left":-_w});
         });
        $(function() {
        	
        	$(".toso-link").click(function(){
        		var searchInput = $(".header-search-input");
        		var headerNav = $('.am-header-nav');
        		if(searchInput.is(':hidden')){
        			headerNav.hide();
        			searchInput.show();
        		}
        		else{
        			headerNav.show();
        			searchInput.hide();
        		}
        	});
        	
        	var h = new searchHistory();
            $('.click-search-btn').click(function(){
                var s = $.trim($('.s').val());
                if(s==''){
                    alert('请填写搜索关键词');	
                }else{
                    h.add(s);
                    window.location.href="/s-"+s+"-k------1-1.htm";
                }
            });

			$(".soso").keydown(function(e){
				if(e.keyCode===13){
					$('.click-search-btn').trigger("click");
				}
			});


