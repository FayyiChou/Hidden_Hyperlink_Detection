         $(".j-jubao-yz").click(function(){
            //开始举报
            var reason  = $(".jubao-reason").val();
            var url = window.location.href;
            var tid = "4376898";
            var data = "tid="+tid+"&reason="+reason+"&url="+url;
            $.post('/jubao',data,function(data){
