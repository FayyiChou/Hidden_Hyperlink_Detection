         $(window).on({
            load:function(){
                imgLocation();
                //当页面加载完，给预览大图页面添加分享按钮
                $(".am-pureview-actions").append("<span class=\"share-ico1 j-share\"></span>");
                $(".am-icon-chevron-left").attr("data-am-share-close","");
            },
            resize:function(){
                imgLocation();
            },
            //当页面滚动的时候重排瀑布流，防止延迟之后页面混乱
            scroll:function(){
               imgLocation();
            }
        });

        $(".coll-add").click(function(){
            var $this = $(this).find("i");
            if($this.hasClass("am-header-ico4-hover")){
              $this.removeClass("am-header-ico4-hover").addClass("am-header-ico-4");
            }else{
              $this.removeClass("am-header-ico-4").addClass("am-header-ico4-hover");  
            }
            
        });
        $(".m-coll-list").css({"height":__h,"overflow":'auto'})
        /**
         * 手机端触摸兼容
         * obj表示绑定对象，click表示pc端click事件，手机端touchstart触摸事件，fn匿名函数
         * 
         */
        function clicks(obj,click,touchstart,fn){ 
            //var $obj = $(obj);
            var $dom = $(document);
            if(typeof ontouchstart !="undefined"){
                $dom.on(touchstart,obj,fn);
            }else{
                $dom.on(click,obj,fn);
            }
        }
