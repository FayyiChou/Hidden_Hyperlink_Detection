            //线条切换
            var $input = $(".ins input");
            $input.focus(function(){
                var $this = $(this);
                var parent = $this.parent();
                parent.addClass("u-border-red").removeClass("u-border-gray");
                parent.siblings(".ins").removeClass("u-border-red").addClass("u-border-gray");
            });
        });


        var __h = $(window).height()/1.7;
        $(document).on("click",".am-icon-wechat",function(){
            $(".am-pureview").hide();
        });
        $(document).on("click",".am-icon-chevron-left",function(){
