        $(".copy-href").click(function(){
            var href= decodeURI(window.location.href);
            prom(href);
            return false;
        });
        /*复制链接对话框*/
        function prom(names) {  
            var name = prompt("请复制链接", names); //将输入的内容赋给变量 name ，  
        }  

       imgLocation();
       function imgLocation(){
            pubu_container = $(".works");
            pubu_container.imagesLoaded(function(){
                pubu_container.masonry({
                    itemSelector : "li"
                });
            });
        }

        
        function nofind()
        {
            var img=event.srcElement;
            img.src="http://static.redocn.com/usercenter/images/sucai_small_img_test.png";
            img.onerror=null;//控制不要一直跳动
        }
        

        function loadshoucang(){
            if(document.cookie.indexOf("uchome_newauth")!=-1){
                var tid = "4376898";
                var data = "tid="+tid;
                $.post('/favorites/checkifaddfav',data,function(res){
                    //
                    if(res.status==1)
                    {
                        //已经收藏过了
                        $("#shouchang_btn").addClass('u-coll-btn2').text('取消收藏').show();
                    }else{
                        //成功
                        $("#shouchang_btn").removeClass('u-coll-btn2').text('收藏').show();
                    }
                });
            }
        }

        loadshoucang();


     /*提示显示的时间*/
    function timer(){
        var time = null;
        var count = 1;
        time = setInterval(function(){
            if(count<=0){
                clearInterval(time);
                $(".modal-wrap").fadeOut();
            }else{
                count--
            }

        },1000);  
    }



    $("#login_form").submit(function(e){
        //
        var usval = $("[name=u]").val();
        var passwordval = $("[name=p]").val();
        if(usval.length == 0){
            $tips.fadeIn().html("请填写用户名");
            timer();
            return false;
        }
        
        if(passwordval.length == 0){
            $tips.fadeIn().html("请填写密码");
            timer();
            return false;
        }
        $tips.html("");
        e.preventDefault();
        var data  = $(this).serialize();
        $.post('/login/ajaxlogin',data,function(res){
           if(res.status!=1) {
                //失败
                $tips.fadeIn().html("帐户名不存在或者密码错误");
                timer();
            }else{
               window.location.reload();
           }
        },'json');
    })
    
    $("#shouchang_btn").click(function(){
//        判断是否登录
        var tid = "4376898";
        var data = "tid="+tid;
        $.post('/favorites/addfav',data,function(res){
            if(res.status==1)
            {
                //未登录弹出登录款
                $("#doc-modal-4").modal();
            }else if(res.status==2) {
                $("#shouchang_btn").removeClass('u-coll-btn2').text('收藏');
                $tips.fadeIn().html("已取消收藏");
                timer();
            }else if(res.status==3) {
                $("#shouchang_btn").addClass('u-coll-btn2').text('取消收藏');
            }
        });
    });
    function nofind()
        {
            var img=event.srcElement || event.target;
            img.src="http://static.redocn.com/usercenter/images/sucai_small_img.png";
            img.onerror=null;//控制不要一直跳动
    }
