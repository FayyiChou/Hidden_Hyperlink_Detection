        $(".j-cate-yz").click(function(){
            var va = $(".am-input").val();
            (va=='')?alert("请输入分明名称"):null;
        });
        //为瀑布流绑定事件
        $(window).on({
            "load":function(){
                imgLocation();
                //当页面加载完，给预览大图页面添加分享按钮
