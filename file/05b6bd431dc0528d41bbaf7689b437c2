function request(flag){var w=jQuery("#w").val();if(w.substring(0,2)=='请输'||w==""||w==null){alert("请输入产品名称！");return false}if(flag==1){jQuery("#reqForm").attr("action","http://z.hc360.com/cgi-bin/iframe_ls.cgi")}else{jQuery("#reqForm").attr("action","http://miniportal.b2b.hc360.com/miniportal/turbine/template/miniportal,search.html");jQuery("#keyword").val(w)}jQuery("#reqForm").submit()}jQuery(document).ready(function(){jQuery("#reqForm").submit(function(){var w=jQuery("#w").val();if(w.substring(0,2)=='请输'||w==""||w==null){alert("请输入产品名称！");return false}else{return true}});jQuery("#w").keydown(function(e){if(e.keyCode==13){jQuery("#reqForm").attr("action","http://z.hc360.com/cgi-bin/iframe_ls.cgi")}})});
</script>
