function veiwWidth(){
var isWidescreen = window.screen.width;
if(isWidescreen>=1440){$("body").addClass("layout-1190");}
if(isWidescreen>=1024 && isWidescreen<1440){$("body").addClass("layout-1000");}
}
veiwWidth();
