      <li class="slide-item" style="background:url(/Public/upload/201601/itlfc_20160119134301_98934.jpg) no-repeat #fff center 0;"><a href="#" target="_blank" class="link"></a></li><li class="slide-item" style="background:url(/Public/upload/201601/itlfc_20160119134838_68373.jpg) no-repeat #fff center 0;"><a href="#" target="_blank" class="link"></a></li><li class="slide-item" style="background:url(/Public/upload/201601/itlfc_20160119134253_78507.jpg) no-repeat #fff center 0;"><a href="#" target="_blank" class="link"></a></li>    </ul>
    <script language="javascript">
            $(function(){
                var slide_index = 0;
                var slide_len = $(".slide-nav").children().length;
                function play(){
                    $(".slide-item").fadeOut(200);
                    $(".slide-items").find("li:eq("+slide_index+")").fadeIn(600);
                    $(".slide-nav a").removeClass("cur");
                    $(".slide-nav").find("a:eq("+slide_index+")").addClass("cur");
                }
                $(".slide-nav a").click(function(){	
                    if($(this).index()!=slide_index){
                        slide_index = $(this).index();								
                        play();
                    }					
                });
                function auto(){
                    if(slide_index>=slide_len-1){
                        slide_index = 0;
                    }else{
                        slide_index++;
                    }
                    play();
                }
                clearInterval(timer);  
                var timer = setInterval(function(){ auto(); },3000);            
                $(".slide-items").hover(function() { 
                        clearInterval(timer); 
                    }, function() { 
                        timer = setInterval(function(){
                        auto();
                    }, 5000)
                });
			
            });
        </script>
