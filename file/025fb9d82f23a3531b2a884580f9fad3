<script>
function OnSubmit() {
	var webfrm = document.webfrm;
	location.href = webfrm.action+"/"+webfrm.elements[0].value+".html";
	return false;
}
var w = document.webfrm.p;
function fuck(o) {
	if (w.value.length > 0) {
	var h = o.href;
	var q = encodeURIComponent(w.value);
	o.href = "/"+o.id+"/"+q+".html";
	return 1;
	}
}
function questions_add( str ){
	if(str=='请输入关键词'){
		str='';
	}
	this.location.href = '/questions/add/'+encodeURI( str );
	return;
}
</script>
