			function question_del()
			{
				if ( confirm('你真的要删除该问题吗?') )
				{ document.getElementById('form_question_del').submit(); }
				return false;
			}
			function answer_del( id )
			{
				if ( '' != id && 0 != id )
				{	if ( confirm('你真的要删除问题的回答吗?') )
					{
						if ( undefined == document.getElementById('form_answer_del').childNodes[0].value )
						{
							document.getElementById('form_answer_del').childNodes[1].value = id;
						}
						else
						{
							document.getElementById('form_answer_del').childNodes[0].value = id;
						}

						document.getElementById('form_answer_del').submit();
					}
				}
				return;
			}
			function answer_add()
			{
				document.form_answer.Submit.disabled="disabled";
				return true;

			}
