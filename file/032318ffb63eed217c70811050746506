function _$(id){
	return document.getElementById(id);
}
function msg(m, s) {
	if(s=='po_mes') {
		_$('des').value = '';
	}
    if(m) {
    	_$('prompt_msg').innerHTML = m;
    	_$('prompt').style.display = 'block';
    }
}
var odes = '';
function getdes() {
    var des = '';
    var i = 1;
    for(var i=1; i<5; i++) {
        var val = _$('zt'+i).value;
        if(_$('zt'+i).checked) {
        	if( i != 4) {
                des += des? ('、'+val) : val;
            } else {
                des += des? '及其它' : '其它';
            }
        }
    }

    if(!_$('des').value || !odes) {
    	_$('des').value = '我想咨询该商品' + (des?(des+'方面的'):'一些') + '问题，请尽快和我联系！';
    } else if(_$('des').value.indexOf(odes) > -1) {
        _$('des').value = _$('des').value.replace(odes, des);
    }
   	odes = des;
}
function closeprompt() {
	_$('prompt').style.display = 'none';
}
_$('des').value = '';
