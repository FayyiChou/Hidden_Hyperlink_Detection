<script type="text/javascript">
var id = '1469021812';
var search_site = 'http://www.xiami.com';

function loadsimilarartist(){
	$.ajax({
		url:'/ajax/similar-artists?id='+id+'&c=6',
		type: 'GET',
		dataType: 'json',
		timeout: 10000,
		error: function(xml){
			$('#artist_recommend').hide();
		},
		success: function(artists){
			var content = '';
			if(artists&&artists.length>0){
				for(var i = 0;i<artists.length;i++){
					var artist = artists[i];
					var artist_id=artist.artist_string_id;
					var artist_name=artist.name;
					var artist_logo = artist.logo;
					var count_likes = artist.count_likes;
					content +='<li><a href="/artist/'+artist_id+'?from=similar_artist"><img src="'+(artist_logo?artist_logo.replace("_1.","_3."):'http://img.xiami.net/res/img/default/cd55.gif')+'" height="55" width="55" alt="'+artist_name+'"/></a><p><a href="/artist/'+artist_id+'?from=similar_artist" title="'+artist_name+'">'+artist_name+'</a></p></li>';
				}
				$(content).appendTo('#artist_recommend ul');
				$('#artist_recommend').show();
			}else{
				$('#artist_recommend').hide();
			}
		}
	});
}

function linksmouseup() {
	//var length = 0;
	//$("input[name=linkids]").each(function() {
	//	if($(this).attr('checked')) length++;
	//});
	//if(length <3) $("#checklinksall").attr('checked','false');
	//else $("#checklinksall").attr('checked','true');
}
function favartists() {
	var ids = getSelectValues('linkids');
	if(ids.length<1) {
		alert("请先选择喜欢的艺人");
		return false;
	}
	$.post('/ajax/artistlinks',{ids:ids,_xiamitoken:_xiamitoken},function(){
		$("#fav_recommend").hide();
	});
}

$(document).ready(function(){
	$("#checklinksall").click(function() {
		if($(this).attr('checked')) selectAll('linkids');
		else inverse('linkids');
	});
	$("#linksclose").click(function() {
		$("#fav_recommend").hide();
	});

	$('#artist_acts').mouseenter(function(){
		$(this).addClass('acts_hover');
	})
	.mouseleave(function(){
		$(this).removeClass('acts_hover');
	});

	$(".removefav").click(function(){
		var id = $(this).attr('rel');
		if(id.match(/\d+/)==null) return false;
		var $this = $(this);
		var $lo = $('<img height="16" width="16" alt="" src="http://img.xiami.net/res/img/default/loading.gif"/>');
		$this.find('strong').hide().after($lo);
		$.getJSON('/ajax/space-lib-del',
			{'artist_id':id,_xiamitoken:_xiamitoken},
			function(respon){
				$lo.remove();$this.find('strong').show();
				if(respon.code==1&&id==respon.id){
					$("#artist_removefav_"+id).hide();
					$("#artist_getfav_"+id).fadeIn();
				}
			}
		);
	});

	 $.getJSON('/count/getplaycount',{'id':id,'type':'artist'},function(data){
	 	$('#play_count_num').html(data.plays);
	 });

	loadsimilarartist();
});
$('#album_acts').mouseenter(function(){
	$(this).addClass('acts_hover');
	$('.do_fav').mouseenter(function(){
		$(this).find('ul').show();
	})
	.mouseleave(function(){
		$(this).find('ul').hide();
	});
})
.mouseleave(function(){
	$(this).removeClass('acts_hover');
});
$('#more_btn').bind("click", function(){
	if (!$(this).hasClass("song_less")) {
		$(this).html('- 更少').addClass("song_less");
		$('.share_btns').hide();
		$('#full_share').show();
	} else {
		$(this).html('+ 更多').removeClass("song_less");
		$('#full_share').hide();
		$('.share_btns').show();
	}
});


</script>


