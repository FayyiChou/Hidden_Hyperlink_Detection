<script type="text/javascript">
		$(function(){
			v6.http.get(_config.domain + 'index/ajaxAllGame', function(data){
				var timer = 0;
				var hide = function(){
					$('#sobjv5').removeClass('yxdq');
				};
				$('#gamealllist').html(template.compile($('#gameListTemplate').html())({data : data.data.game}));
				$('#sobjv5').on('mouseover', function(e){
					clearTimeout(timer);
					$(this).addClass('yxdq');
				}).on('mouseout', function(){
					clearTimeout(timer);
					timer = setTimeout(hide, 200);
				});

				var dom = $('#qt-info-div'), len = data.data.notice.length, index = 0;
				dom.html(template.compile($('#noticeTemplate').html())({data : data.data.notice}));
				
				if(data.data.notice.length > 1) setInterval(function(){
					index = (index + 1) % len;
					dom.animate({'scrollTop' : index * 26 + 'px'});
				}, 1000 * 3);
				dom.scrollTop(0);
			});
			$(document).on("userGetInfo", function(e, data){
				var message = data.data.message;
				if(message && parseInt(message.unread) > 0){
					$('#noReadNumcote').text(message.unread).show();
				}
			});
			if($('#fixedMenu').length){
				//固定head
				//xinFixed
				var body = $(document.body);
				var doc = $(window);
				var onScroll = function(){
					if(!$('#fixedMenu').length) return;
					var scTop = doc.scrollTop();
					var hasFixed = body.hasClass('xinFixed');
					if(scTop > 40 && !hasFixed){
						body.addClass('xinFixed');
					}else if(scTop <= 40 && hasFixed){
						body.removeClass('xinFixed');
					}
				};
				doc.on('scroll', onScroll);
				$(onScroll);
			}
		});
		</script>
		<script type="text/javascript">
$(function(){
	//函数定义
	var tabChange = function(cls, currentCls, type, func){
		var eles = $(cls);
		var index = 0;
		eles.each(function(key, val){
			$(val).on(type, function(){
				$(this).addClass(currentCls);
				if(key != index){
					$(eles.eq(index)).removeClass(currentCls);
					index = key;
					$.isFunction(func) && func(this, index);
				}
			})
		});
	};
	//精彩活动
	$('#tj-game li.rec-game').on('mouseover', function(){
		$(this).addClass('hover');
	}).on('mouseout', function(){
		$(this).removeClass('hover');
	});
	//开服公告
	tabChange('#objkfnav li', 'hover', 'mouseover');
	//所有游戏
	(function(){
		var eles = null;
		tabChange('#allgameteb .gameall-tit li', 'hot', 'click', function(node, index){
			if(eles == null) eles = $('#allgameteb .gameall-show li');
			var code = $(node).data('code').toLowerCase();
			eles.each(function(key, tag){
				var item = $(tag);
				var c = item.data('code');
				if(c && code.indexOf(c) > -1){
					item.addClass('current');
				}else{
					item.removeClass('current');
				}
			});
		});
	})();
	//合作
	$('#compa li').on('mouseover', function(){
		$(this).addClass('hover');
	}).on('mouseout', function(){
		$(this).removeClass('hover');
	});
	//幻灯片
	(function(){
		var root = $('#sdshowbg');
		var eles = $('#headteb li');
		var tabs = $('#headtebnum li');
		var currentIndex = 0;
		var timer = 0;
		var autoRun = function(){
			run((currentIndex + 1) % eles.length);
		};
		var run = function(index){
			if(index == currentIndex) return;
			clearTimeout(timer);
			$(eles.eq(currentIndex)).removeClass('hot');
			$(tabs.eq(currentIndex)).removeClass('hot');
			$(eles.eq(index)).addClass('hot');
			$(tabs.eq(index)).addClass('hot');
			currentIndex = index;
			root.css('opacity', 0).animate({opacity : 1}, 'normal', 'swing', function(){
				clearTimeout(timer);
				timer = setTimeout(autoRun, 5000);
			});
		};
		tabs.each(function(key, node){
			$(node).on('mouseover', function(){
				run(key);
			});
		});
		timer = setTimeout(autoRun, 5000);
	})();
	//显示登录或者用户信息
	(function(){
		var showExperienceView = false;
		var signLock = false;
		var bindSignClick = function(){
			$('#sign-button').on('click', function(e){
				if(!v6.isLogined()) return v6.user.dialog.login();
				if(signLock) return;
				signLock = true;
				v6.http.post(_config.domain + 'sign/signToday', {}, function(data){
					signLock = false;
					if(data.success){
						showExperienceView = true;
						$(document).trigger("userSignSuccess");
					}
				});
			});
		}

		v6.user.display({
			target : '#user-panel',
			platform : [{"type":"tencent","desc":"\u817e\u8baf","title":"\u817e\u8bafQQ\u8d26\u53f7","url":"http:\/\/puser.funshion.com\/oauth\/qq?app_code=game&u=http%3A%2F%2Fgame.funshion.com%2Fv6%2Fuser%2FthirdLogin%2Fqq%2Fa.funshion.com%2F"},{"type":"sina","desc":"\u5fae\u535a","title":"\u65b0\u6d6a\u5fae\u535a\u8d26\u53f7","url":"http:\/\/puser.funshion.com\/oauth\/sina?app_code=game&u=http%3A%2F%2Fgame.funshion.com%2Fv6%2Fuser%2FthirdLogin%2Fsina%2Fa.funshion.com%2F"}],
			withLoginedServer : true
		});
		if(!v6.isLogined()){
			bindSignClick();
		}
		$(document).on("userGetInfo", function(e, data){
			var sign = data.data.sign;
			sign.view = showExperienceView;
			$('#sign-panel').html(template.compile($('#signTemplate').html())(sign));
			$('#signState').on('mouseover', function(){
				$(this).addClass('hover');
			}).on('mouseout', function(){
				$(this).removeClass('hover');
			});
			$('#signPrize').css('opacity', 0).animate({opacity : 1, top : '-13px'}, 'fast', 'swing').delay(3000).animate({opacity : 0}, {callback : function(){
				$('#signPrize').hide();
			}});
			bindSignClick();
			showExperienceView = false;
		});
		return;
	})();
});
</script>
<script type="text/javascript">
(function(){
	var logined = v6.isLogined();
	var closeFlash = function(type){
		$('#flashAd').remove();
		if(logined || !type){
			//一天里不在展现
			v6.cookie.set('_home', 1, 1);
		}
	};
	var send = function(url){
		if(url && url.substr(0, 4) == 'http') (new Image()).src = url;
	}

	var parseData = function(json){
		if(!json.length || !json[0].ad_list || !json[0].ad_list.length) return;
		var data = json[0].ad_list;
		for(var i = 0, len = data.length; i < len; i++) {
			if (data[i].material && data[i].link){
				var swf = v6.swf.createHTML({
					id : 'homeSwf',
					url : data[i].material,
					width : 900,
					height : 500,
					wmode : 'transparent',
					allowscriptaccess : 'sameDomain',
					allowfullscreen : false
				});
				var html = template.compile($('#gameMessageTemplate').html())({swf : swf, href : data[i].link});
				$(document.body).append(html);
				$('#flashAd .linkto').on('click', function(){
					closeFlash(false);
				});
				$('#flashAd .closeAd').on('click', function(){
					closeFlash(true);
				});
				setTimeout(function(){
					$('#flashAd').remove();
				}, 7000);

				$.each(data[i].monitor.view, function(index, item){
                    if(item.point == 0){
                        send(item.url);
                    }
				});
				//展示内容时上报微距 end
				var clickmonitor = data[i].monitor.click[0].url;
				$('#flashAd a').on('click', function(){
					send(clickmonitor);
					return true;
				});
				break;
			}
		}
	};
	$(function(){
		if(!v6.swf.version) return;
		var cv = v6.cookie.get('_home');
		if(!cv) $.ajax({
			type: "get",
			async: true,
			url: "http://pub.funshion.com/interface/deliver?ap=ws_tips_game&client=&oc=&vchannel=&schannel=&mid=&plid=&vid=&url=&refer=&isvip=&uid=0&mac=&mick=&fck=&screen_w=&screen_h=&deliver_ver=v1",
			dataType: "jsonp",
			success: parseData
		});
	})
})();
</script>
		<script type="text/javascript">
		$(function(){
			var footer = $('#doc-footer'), preDiff = 0;
			var resize = function(){
				var diff = $(window).outerHeight() - $(document.body).outerHeight() + preDiff;
				if(diff > 0){
					preDiff = diff;
					footer.css('marginTop', diff);
				}else{
					preDiff = 0;
					footer.css('marginTop', 0);
				}
			};
			resize();
			$(window).on('resize', resize);
		});
		</script>
