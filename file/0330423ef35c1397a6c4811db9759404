<script type="text/javascript">
var urlnow=top.location.href;
var intervalId = setInterval("hiden_left()", 10000);  
	$(document).ready(function(e) {
		var nowH = window.screen.availHeight;
		var theH = $('.fixed').height();
		var marT = (nowH-theH)/2;
		$('.fixed').css("'top:'+marT+'px','_top:'+marT+'px'");
		var t=true;		
		var isIE=!!window.ActiveXObject;
		var isIE6=isIE&&!window.XMLHttpRequest;
		$('.f_left').click(function(){
			if(t){
				$('.fixed').animate({right:'-121px'},300);
				intervalId();
				t= !t;
			} else{
				$('.fixed').animate({right:'0px'},300);
				t= !t;
			}
		});
		
		$('.f_left').mouseenter(function(){
			hiden_left();
		})
		
		//leavemessage div
		$('.leavemes').on('click', function(){
			var i = $.layer({
				type: 1,
				title: '加盟留言',
				closeBtn: [0 , true],
				shadeClose: true,
				border : [5, 0.5, '#666', true],
				offset:['30px' , ''],
				area: ['700px','600px'],
				fadeIn: 1000,
				shadeClose : false,
				page: {
					dom: '.messageform'
				}
			});
			return false;
		});
		
		// get sugest div
		$('#getmes').on('click', function(){
			var i = $.layer({
				type: 1,
				title: '获取加盟建议',
				closeBtn: [0 , true],
				shadeClose: true,
				border : [5, 0.5, '#666', true],
				offset:['30px' , ''],
				area: ['500px','400px'],
				fadeIn: 1000,
				shadeClose : false,
				page: {
					dom: '.getsugest'
				}
			});
			var $getsugest=$('.getsugest').find('.quest');
			$('.pre').click(function(){
				var $indexele=null;
				$getsugest.each(function(i){
                    if($(this).is(":visible")){
						$indexele=$(this);
					}
                });
				$('.next').text('下一步');
				if($indexele.prev('div').length==1){
					$indexele.hide();
					$indexele.prev().show();
				}
			})
			$('.next').click(function(){
				var $indexele=null;
				$getsugest.each(function(){
                    if($(this).is(":visible")){
						$indexele=$(this);
					}
                });
				if($indexele.attr('index')==4){$(this).text('获取建议')}
				if($indexele.next('div').length==1){
					$indexele.hide();
					$indexele.next().show();
				}else{//check phone
					var questphone=$('#questphone').val();
					if(checkphone(questphone)){
						var questlevel=$('input[name="questlevel"]:checked').val();
						var questmoney=$('input[name="questmoney"]:checked').val();
						var questexperience=$('input[name="questexperience"]:checked').val();
						var questshop=$('input[name="questshop"]:checked').val();
						var provincial=$('#provincial').val();
						var city=$('#city').val();
						//submit messae
						$.post("http://www.520jfw.com/plus/leaguebooksave.php",
						{url:urlnow,action:'guest',cellphone:questphone,questlevel:questlevel,questmoney:questmoney,questexperience:questexperience,questshop:questshop,provincial:provincial,city:city},
						function(data,status){
							if(parseInt(data)==1){
								alert('操作成功啦，我们会在6小时内给您发送加盟建议。')
								location.reload();
							}else{
								alert('操作失败，请拨打我们的免费400电话联系网站管理员及时解决问题，万分感谢。')	
							}
						});
					}else{
						alert('请填写正确的手机号码，否则无法我们无法发送加盟建议到您手机哦~')	
					}
				}
			})
			return false;
		});

		//get_city_ajaxmetod
		$('#provincial').change(function(){
			$.post("http://www.520jfw.com/plus/getcityajax.php",{id:$(this).val()},function(data,status){
				if(data=='err'){
					alert('操作失败，请拨打我们的免费400电话联系网站管理员及时解决问题，万分感谢。')	
				}else{
					$('#city').html(data);
				}
			});
		})
		
		$('#messageleave').click(function(){
			var cellphone=$('#phone').val();
			if(checkphone(cellphone)){
				var name=$('#name').val();
				var money=$('input[name="money"]:checked').val();
				var shop=$('input[name="shop"]:checked').val();
				var content=$('#content').val();
				$.post("http://www.520jfw.com/plus/leaguebooksave.php",
				{url:urlnow,action:'message',cellphone:cellphone,money:money,shop:shop,content:content,name:name},
				function(data,status){
					if(parseInt(data)==1){
						alert('留言成功啦，我们加盟顾问将会尽快联系您。')
						location.reload();
					}else{
						alert('操作失败，请拨打我们的免费400电话联系网站管理员及时解决问题，万分感谢。')	
					}
				});
			}else{
				alert('请填写正确的手机号码，否则无法我们无法发送加盟建议到您手机哦~')	
			}
		})
	});
function trimLeft(s){    
	if(s == null) {    
		return "";    
	}    
	var whitespace = new String(" \t\n\r");    
	var str = new String(s);    
	if (whitespace.indexOf(str.charAt(0)) != -1) {    
		var j=0, i = str.length;    
		while (j < i && whitespace.indexOf(str.charAt(j)) != -1){    
		j++;    
		}    
	str = str.substring(j, i);    
	}    
	return str;    
}      
function trimRight(s){    
	if(s == null) return "";    
	var whitespace = new String(" \t\n\r");    
	var str = new String(s);    
	if (whitespace.indexOf(str.charAt(str.length-1)) != -1){    
		var i = str.length - 1;    
		while (i >= 0 && whitespace.indexOf(str.charAt(i)) != -1){    
			i--;    
		}    
		str = str.substring(0, i+1);    
	}    
	return str;    
} 

function checkphone(questphone){
	if(questphone){
		if(parseInt(trimRight(trimLeft(questphone))).toString().length!=11){
			return false;
		}else{
			return true;
		}	
	}else{
		return false;
	}
}

function hiden_left(){
	$('.fixed').animate({right:'0px'},300);
	t=true;
}
