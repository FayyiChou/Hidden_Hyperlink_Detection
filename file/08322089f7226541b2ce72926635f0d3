<script>
$(document).ready(function(){
	$(window).scroll(function(){
		if($(window).scrollTop()>$('#headerIndexTopHidden').offset().top){
			$('#headerFixedTop').css("position","fixed");
			$('#headerFixedTopHidden').css("height",$('#headerFixedTop').height());
		}else{
			$('#headerFixedTop').css("position","static");
			$('#headerFixedTopHidden').css("height","0");
			}
	});
})
function updateSearchColor(obj,type){
	if(type==1){
		$(obj).css("color","#333");
	}else{
		if($(obj).val()!=""){
			$(obj).css("color","#333");
		}else{
			$(obj).css("color","#d4d4d4");
		}
	}
}
//这里添加后 mobile_one.js 内的方法将被覆盖
function moveTo(id){
	var setheight = $("#"+id).offset().top-$('#headerFixedTop').height();
	var color = "";
	if(id=="bn"){
		color = "#00aaff";
	}
	if(id=="cg"){
		color = "#35bf14";
	}
	if(id=="rn"){
		color = "#f54104";
	}
	if(id=="nj"){
		color = "#ffc700";
	}
	$('p[id^=title]').css("color","#333");
	$('#title'+id).css("color",color);
	$('html,body').animate({scrollTop:setheight }, 1000);
}
</script>

