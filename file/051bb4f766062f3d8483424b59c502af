<script>
	$(function(){
		$("#link_to_select_area").citypicker({
	     province: '广东省',
	     city: '东莞市',
	     level:'city'
	    });
		$(".up_tip_box").mouseover(function(){
			$(".up_tip").show();
		}).mouseout(function(){
			$(".up_tip").hide();
		});
		$("#mainimage").smallslider({"showText":false});
	})
	function gotoArea(areaName){
		console.log(areaName);
		$.post("/pages/DerDistrict/searchByName.do",{"name":areaName},function(data){
			var result = jsonParse(data);
			if(!result.success){
				alert(result.msg);
			}else{
				window.location.href="/p/area/"+result.result+".htm";
			}
		})

	}
</script>
