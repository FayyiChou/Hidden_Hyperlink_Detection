    (function(){
        var marketEle = document.getElementById("local-market");
        var url = "http://{spell}.bitauto.com/cheshi/";
        var location;
        if (Bitauto.location.county!=null) {
           location = Bitauto.location.county;
        }
        else if (Bitauto.location.city !=null) {
            location = Bitauto.location.city;
        }
        if (location && marketEle) {
            marketEle.setAttribute("href", url.replace("{spell}", location.spell));
        }
    })();

