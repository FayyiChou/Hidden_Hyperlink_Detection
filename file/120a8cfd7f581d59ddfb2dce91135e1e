<script type="text/javascript">
    function validate() {
        if (subscription_form.name.value == "Name") {
            alert("Enter your Name")
            subscription_form.name.focus()
            return false;
        }

        if (subscription_form.phone.value == "Phone") {
            alert("Enter your phone Number")
            subscription_form.phone.focus()
            return false;
        }
        if (subscription_form.email1.value == "") {
            alert("Enter your E-Mail ID")
            subscription_form.email1.focus()
            return false;
        }
        if (subscription_form.email1.value.match(/^\s+[a-zA-Z0-9]+/) != null || (subscription_form.email1.value.match(/[a-zA-Z0-9]+/) == null || subscription_form.email1.value.match(/[a-zA-Z0-9]+\@[a-zA-Z0-9\-]+(\.([a-zA-Z0-9]{2}|[a-zA-Z0-9]{3}))+$/) == null)) {
            alert("Enter a valid E-Mail ID")
            subscription_form.email1.value = ""
            subscription_form.email1.focus()
            return false;
        }

    }
   
</script>

