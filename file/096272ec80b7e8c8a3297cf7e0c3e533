<script type="text/javascript">
$(function(){	
	$("#NIE-topBar-menu").hover(function(){
		$(this).addClass("NIE-topBar-menu-hover");	
	},function(){
		$(this).removeClass("NIE-topBar-menu-hover");	
	});
	$("#Tab p").live("hover",function(){
		var index = $(this).index()
		$(this).addClass("on").siblings().removeClass("on");
		$(this).parent("#Tab").next("div").children("div").hide();
		$(this).parent("#Tab").next("div").children("div").eq(index).fadeIn(0);
	})
});
