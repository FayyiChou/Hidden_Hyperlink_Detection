		<script type="text/javascript">
	$(document).ready(function(){
		// 获取自主服务头部尾部
		 setTimeout('$("#mask").css("width","240px")', 500);
		
		//分页
		initPagination('6');
		
		//鼠标经过、离开活动div上时，修改样式
		$(".zyh_img").hover(function(){
	  		$(this).addClass("zyhimghvr");
	  		$(this).find(".class_all_activityType").show();
	  		$(this).find("#fuckxindong").show();
	  		$(this).find("#fuckxin").show();
	  		
	  	},function(){
	  		$(this).removeClass("zyhimghvr")
	  		$(this).find(".class_all_activityType").hide();
	  		$(this).find("#fuckxindong").hide();
	  		$(this).find("#fuckxin").hide();
		});
    });
    
	//下一页---可点击
    function enable_nextPage_top(){
    	$("#id_nextPage_top").removeClass().addClass("fy_x");
		$("#id_nextPage_top").html("<a href='javascript:nextPage_top()'>下一页</a>");
    }
    
	//上一页---可点击
    function enable_upPage_top(){
    	$("#id_upPage_top").removeClass().addClass("fy_x");
    	$("#id_upPage_top").html("<a href='javascript:upPage_top()'>上一页</a>");
    }
	function initPagination(totalPage){
	    var setting = paginationSetting();//分页组件 - 显示设置
	    $("#pagination").pagination(totalPage, setting);
	   if(totalPage<1) {
			$("#pagination").hide();
		}else{
			$("#pagination").show();
		}
	}
	
	/**
	* 分页组件 - 显示设置
	*/
	function paginationSetting(){
	    var opt = {};
	    opt['current_page']=0;
	    opt['items_per_page']=6;//每页个数
	    opt['num_display_entries']=4; //省略号前
	    opt['num_edge_entries']=2; //省略号后
	    opt['prev_text']='上一页';
	    opt['next_text']='下一页';
	    opt['callback']=nextPage;
	    return opt;
	}
	
	function nextPage(pageIndex) {
	 	$("#currPage").attr("value", pageIndex+1);
		$("#activityFormId").submit();
	}
					
	</script>

	

</body></html>