$('.owl-wrapper >.owl-item').on('click',function(){
 var obj=$(this);
 index =  obj.index();
 $('.owl-wrapper >.owl-item').children().children().removeClass('z-crt');
 obj.children().children().addClass('z-crt');
  $('.text').find('.con').fadeOut(100)
  $('.text').find('.con:eq('+index+')').fadeIn(100);
  if($('.text').find('.con:eq('+index+')').hasClass("con1")){
ajaxvote();
  }else if($('.text').find('.con:eq('+index+')').hasClass("con5")){
ajaxask();  
  }else if($('.text').find('.con:eq('+index+')').hasClass("con7")){
ajaxStock();  
  }
});
    });
function ajaxStock(){
ajaxGetHTML('http://appshare.yicai.com/app/ajax/stockList/5258994/000800/2/','stocktable');
}
function ajaxask(){
