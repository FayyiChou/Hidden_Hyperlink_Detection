		<!-- start: custom scripts -->
		<!-- 626775 -->
<script>
$(document).ready(function () {
	renderWcnSelectMenus();
	function renderWcnSelectMenus() {
		var selectMenus = [];
		$('<div/>').load('fetchwcnjobfacets div.vacancy-filter', function () {
			var $sfg = $('.search-filter-group', this);
			if ($sfg.length > 0) {
				$sfg.each(function () {
					var $sft = $('.search-filter-title', this),
						$sfv = $('.search-filter-value', this),
						currentFacet = $.trim($sft.text()),
						fieldname = $sfv.find('input').attr('name'),
						options;
					if (currentFacet == "Job Category" || currentFacet == "Organisation") {
						if ($sfv.length > 0) {
							options = ['<option value="">Choose&hellip;</option>'];
							$sfv.each(function () {
								options.push('<option value="', $('input', this).attr('value'), '">', $.trim($('label', this).text()), '</option>');
							});
							selectMenus.push('<li class="four columns"><label for="', fieldname, '">', currentFacet, '</label><select name="', fieldname, '" id="', fieldname, '">', options.join(''), '</select></li>');
						}
					}
				});
				if (selectMenus.length > 0) $('#searchjobs_keywords').removeClass('eleven').addClass('three').after(selectMenus.join(''));
			}
		});
	}
});
</script>
		<!-- end: custom scripts -->
