	function BindTab(tabs, content) {
		tabs.bind('click', function() {
			var num = tabs.index($(this));

			content.hide();
			content.eq(num).show();

			tabs.removeClass("current");
			tabs.eq(num).addClass("current");

		});
	}
</script>
