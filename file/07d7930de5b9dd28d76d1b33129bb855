<script type="text/javascript">
	$('.more').mouseenter(function(){
        $(this).find('img').attr('src','http://www.sinopharmsx.com//statics/gysx/img/lgd.png');
    });
  	$('.more').mouseleave(function(){
        $(this).find('img').attr('src','http://www.sinopharmsx.com//statics/gysx/img/hgd.png');
    });


    $('.zxdh ul li').mouseenter(function(){
      
        $(this).prevAll().removeClass('select');
        $(this).prevAll().css({'background':'url()'});

        $(this).nextAll().removeClass('select');
        $(this).nextAll().css({'background':'url()'});
        $(this).addClass('select');
    });



    $('.hxyw').mouseenter(function(){
        $(this).find('.pop').css({"display":"block"});
    });
  	$('.hxyw').mouseleave(function(){
        $(this).find('.pop').css({"display":"none"});
    });
    $('.sp').mouseenter(function(){
        $(this).find('.sp_bt').css({"display":"block"});
    });
  	$('.sp').mouseleave(function(){
        $(this).find('.sp_bt').css({"display":"none"});
    });
    $('.carousel').carousel();
    $('.zxdh ul li').mouseenter(function(){
       $(this).css({'background':'url(http://www.sinopharmsx.com//statics/gysx/img/kuang.png)'});

       idname=$(this).attr('id');
       $(".zxdh_detials").hide();
       $(".zxdh_detials[name="+idname+"]").show();

       $(".rimg").hide();
       $(".rimg[name="+idname+"]").show();
    });
    $('.zxdh_detials li').mouseenter(function(){
    	val1=$(this).index();
    	name=$(this).parent().parent().attr('name');
    	$(".rimg img[name="+name+""+val1+"]").prevAll().hide();
    	$(".rimg img[name="+name+""+val1+"]").nextAll().hide();
    	$(".rimg img[name="+name+""+val1+"]").show();
    })
    $(document).ready(function(){
      	$('.slider9').bxSlider({ 
	        slideWidth: 100,
			minSlides: 3,
			maxSlides: 3,
			ticker: true,
	  		tickerHover:true,
			speed: 12000,
			startSlides: 0, 
	        slideMargin: 10
      	});
    });
    
	var i=2;    
	$('.zhankai').on("click",function(){
		if (i%2==0) {
	   		$(this).html("收起<span class='glyphicon glyphicon-chevron-up'></span>");
	    	$('.fzgs_zgs').css({'height':'150px'});
	    	i=i-1;
	    }else{
	    	$(this).html("展开<span class='glyphicon glyphicon-chevron-down'></span>");
			$('.fzgs_zgs').css({'height':'50px'});
			i=i+1;
		}
   	})
   	$('.qqqqq').mouseenter(function(){
   		
	   		$('.znews_tc').show();
	    
	    
   	})

   	$('.qqqqq').mouseleave(function(){
	   	$('.znews_tc').hide();
   	})

	$('.fhdb').click(function(){
		var speed=200;//滑动的速度
        $('body,html').animate({ scrollTop: 0 }, speed);
        return false;
	})
	$('.closeb').on('click',function(){
		$('.bianlan').hide();
	})
	$('.erweima').mouseenter(function(){
		$('.tanchu').show();
	})
	$('.erweima').mouseleave(function(){
		$('.tanchu').hide();
	})
	

		


	
</script>
