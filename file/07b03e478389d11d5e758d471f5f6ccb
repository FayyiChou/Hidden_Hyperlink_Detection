<script>
window.onload = function() {
setTimeout("startmarquee(26, 110, 0, 'marquee')", 5000);
setTimeout("startmarquee(26, 110, 0, 'marquee1')", 5000);
setTimeout("startmarquee(26, 110, 0, 'marquee2')", 5000);
}
function startmarquee(lh,speed,delay,id) {

var t;
var p = false;
var o = document.getElementById(id);
o.innerHTML += o.innerHTML + o.innerHTML + o.innerHTML;
o.onmouseover = function() {
p = true;
}
o.onmouseout = function() {
p = false;
}
o.scrollTop = 0;

function start() {
t = setInterval(scrolling,speed);
if(!p) o.scrollTop += 2;
}

function scrolling() {
if(o.scrollTop%lh != 0) {
o.scrollTop += 2;
if(o.scrollTop >= o.scrollHeight / 2) o.scrollTop = 0;
} else {
clearInterval(t);
setTimeout(start,delay);
}
}
setTimeout(start,delay);
}


</script>


