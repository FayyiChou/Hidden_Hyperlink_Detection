<script>
//滚动条开始 

window.onload=function(){
var bttop=0;
var ctop=0;
var height=$("#dv_scroll_text").height();
$("#dv_scroll_text").css({top:"0px"});

//e鼠标事件 
$(".Scrollbar-Handle").mousedown(function(e)//e鼠标事件 
{
	
var y = e.pageY;//获得鼠标指针离DIV元素上边界的距离 
	
bttop =$(".Scrollbar-Handle").css("top").replace('px', '');
ctop=$("#dv_scroll_text").css("top").replace('px', '');

$(document).bind("mousemove",function(ev)//绑定鼠标的移动事件，因为光标在DIV元素外面也要有效果，所以要用doucment的事件，而不用DIV元素的事件 
 {
	var _y = parseInt(ev.pageY) - parseInt(y);//获得Y轴方向移动的值 
mmove(_y);

});

});
//点击松开
$(document).bind("mouseup", function(event) {
  $(document).unbind("mousemove");
        });
		
//移动
function mmove(_y)
{
	
	var nowtop = (parseInt(bttop)+parseInt(_y)); //计算button的top值
	var contenttop = -(parseInt(nowtop)*(height-500)/500); 
	//计算button的top值
	if(height>500 && nowtop>=0 && nowtop<=500)
	{
	 $(".Scrollbar-Handle").css({top: nowtop +"px"}); 
	 $("#dv_scroll_text").css({top: contenttop +"px"});
	}

}
//
 $('#dv_scroll').mousewheel(function(event, delta) {
						bttop =$(".Scrollbar-Handle").css("top").replace('px', '');
						ctop=$("#dv_scroll_text").css("top").replace('px', '');
						mmove((-delta)*15);
							//alert(delta);
                        event.preventDefault();

                    });
}


//-----------------滚动条结束 
</script>



