			 var link5 ='/html/sndt/index.html';
	        
        /*第一种形式 第二种形式 更换显示样式*/
        function setTab(name, cursel, n) {
            for (i = 1; i <= n; i++) {
                var menu = document.getElementById(name + i);
                var con = document.getElementById("con_" + name + "_" + i);
                menu.className = i == cursel ? "hover" : "";
                con.style.display = i == cursel ? "block" : "none";	
				if (i==cursel ) link5= a[i];		
				
            }
        }
		
		function winopen()
		{
		 window.open(link5);
		}
		
		function getIEVersion()
        {
            if(navigator.appName == "Microsoft Internet Explorer")
            {
               if(navigator.appVersion.match(/7./i)=='7.')
               {                  
		            var headID = document.getElementsByTagName("head")[0];
                    var newCss7 = document.createElement('link');
                    newCss7.type = 'text/css';
                    newCss7.rel = 'stylesheet';
                    newCss7.href = '/Templets/Content/img/index7.css';
                    headID.appendChild(newCss7);
                    return;
               }
               if(navigator.appVersion.match(/6./i)=='6.')
               {
                    var headID = document.getElementsByTagName("head")[0];
                    var newCss = document.createElement('link');
                    newCss.type = 'text/css';
                    newCss.rel = 'stylesheet';
                    newCss.href = '/Templets/Content/img/index.css';
                    headID.appendChild(newCss);
                    return;
               }
               if(navigator.appVersion.match(/6./i)!='6.'|| navigator.appVersion.match(/7./i)!='7.')
               {
                    var headID = document.getElementsByTagName("head")[0];
                    var newCssOther = document.createElement('link');
                    newCssOther.type = 'text/css';
                    newCssOther.rel = 'stylesheet';
                    newCssOther.href = '/Templets/Content/img/index7.css';
                    headID.appendChild(newCssOther);
                    return;
               }
             }  
			    
        }
		
    function openlogin3456()
    {
        var wid=1024 ;
        var hid=459;
        var left=eval(screen.Width-wid)/2;
        var top=eval(screen.Height-hid)/2;
        window.open('http://hb.hbncc.com');
