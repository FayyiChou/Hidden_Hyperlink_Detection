   //會員登入軸//判斷程式
   function ChkData_PI00B_2_LR(){
		OpOk  = "0";
		
		if (OpOk=="0") // 判斷程式繼續
		{
		   if (RCS_LR.C_UserNo.value.length<=0)
         {
            OpOk  = "1";
            alert("【會員帳號】不能空白，請重新輸入！");
            RCS_LR.C_UserNo.focus() ;
         }
         else if (Jistno(RCS_LR.C_UserNo.value)=="0") //檢查是否為英數字元
         {
            OpOk  = "1";
            alert("【會員帳號】字元輸入有誤！必需是英數字組合。");
            RCS_LR.C_UserNo.focus() ;
         }
		}
		
		if (OpOk=="0") // 判斷程式繼續
		{
		   if (RCS_LR.C_UserPass.value.length<=0)
         {
            OpOk  = "1";
            alert("【會員密碼】不能空白，請重新輸入！");
            RCS_LR.C_UserPass.focus() ;
         }
         else if (Jistno(RCS_LR.C_UserPass.value)=="0") //檢查是否為英數字元
         {
            OpOk  = "1";
            alert("【會員密碼】字元輸入有誤！必需是英數字組合。");
            RCS_LR.C_UserPass.focus() ;
         }
		}
		
		//沒問題了
      if (OpOk=="0") 
      {
         RCS_LR.submit();
      }
		
   }
   
   //會員登出軸//判斷程式
   function ChkData_PI00B_4_LR(){
