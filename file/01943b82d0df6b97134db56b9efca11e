		function  multiSearchUB() {
			var selectIndex = document.getElementById('bs_idtype').selectedIndex;
			var searchType = document.getElementById('bs_idtype').options[selectIndex].text;
			if(searchType=='搜论坛'){
				var forumkey = document.getElementById('multiSearchInput').value;
				if(forumkey=="请输入关键字"){
					forumkey='';
				}
				forumkey = Trim(encodeURIComponent(forumkey));
				_ub.collect(1,{'vwg.page':'bbspostpage','vwb.forumkey':forumkey});
			}else{
				var postkey = document.getElementById('multiSearchInput').value;
				if(postkey=="请输入关键字"){
					postkey='';
				}
				postkey = Trim(encodeURIComponent(postkey));
            	_ub.collect(1,{'vwg.page':'bbspostpage','vwb.postkey':postkey});
			}
			
			multiSearch2014();//重新运行search函数
		}
		function Trim(str)
		{ 
			return str.replace(/(^\s*)|(\s*$)/g, "");
		}
