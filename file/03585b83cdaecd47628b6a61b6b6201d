  <script type="text/javascript">
        function SubmitSellCar() {
            var phone = $("#SellerPhone").val();
            $.post("/tool/ajaxtool.ashx?action=SubmitQuickSellCarInfor", {
                "QSI_Phone": phone
            },
                function (data) {
                    data = $.trim(data);
                    if (data != "") {
                        if (data == "1") {
                            art.dialog({
                                wdith: '100',
                                height: '50',
                                icon: 'face-smile',
                                title: '友情提示',
                                drag: false,
                                time: 60,
                                resize: false,
                                top: '15%',
                                content: '恭喜，提交成功！',
                                //close: function () {
                                //    window.location.href = "/buycar/buycar.html";
                                //}
                            });
                            return true;
                        }
                        if (data == "0") {
                            art.dialog({
                                wdith: '100',
                                height: '50',
                                icon: 'face-sad',
                                title: '友情提示',
                                drag: false,
                                resize: false,
                                top: '15%',
                                content: '提交失败，请检查填写内容！'
                            });
                            return false;
                        }
                    }
                    else {
                        art.dialog({
                            wdith: '100',
                            height: '50',
                            icon: 'face-sad',
                            title: '友情提示',
                            drag: false,
                            resize: false,
                            top: '15%',
                            content: '提交失败，请检查填写内容！'
                        });
                        return false;

                    }
                }, "text");
        }
