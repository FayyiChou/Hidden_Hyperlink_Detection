        //此处动态设置文字分类的蓝色下划线标识状态
        $(function () {
            var writename = 'xcdt';
            if (writename.length > 0) {
                $(".news-con .news-nav ul").find("li").each(
                    function () {
                        if ($(this).html().indexOf(writename) > 0) {
                            $(this).find("a").addClass("news-on");
                        }
                        else {
                            $(this).find("a").removeClass("news-on");
                        }
                    });
            }
        })
