#demo80 {
overflow:hidden;
height: 180px;
}
</style>

<script>

var speed8=30; //数字越大速度越慢
var tab80=document.getElementById("demo80");
var tab81=document.getElementById("demo81");
var tab82=document.getElementById("demo82");
tab82.innerHTML=tab81.innerHTML; //克隆demo1为demo2
function Marquee(){
if(tab82.offsetHeight-tab80.scrollTop<=0)//当滚动至demo1与demo2交界时
tab80.scrollTop-=tab81.offsetHeight //demo跳到最顶端
else{
tab80.scrollTop++
}
}
var MyMar=setInterval(Marquee,speed8);
tab80.onmouseover=function() {clearInterval(MyMar)};//鼠标移上时清除定时器达到滚动停止的目的
tab80.onmouseout=function() {MyMar=setInterval(Marquee,speed8)};//鼠标移开时重设定时器
</script>


