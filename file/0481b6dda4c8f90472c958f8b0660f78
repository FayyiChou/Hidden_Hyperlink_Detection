<script>

var speed3=30; //数字越大速度越慢
var tab30=document.getElementById("demo30");
var tab31=document.getElementById("demo31");
var tab32=document.getElementById("demo32");
tab32.innerHTML=tab31.innerHTML; //克隆demo1为demo2
function Marquee(){
if(tab32.offsetHeight-tab30.scrollTop<=0)//当滚动至demo1与demo2交界时
tab30.scrollTop-=tab31.offsetHeight //demo跳到最顶端
else{
tab30.scrollTop++
}
}
var MyMar=setInterval(Marquee,speed3);
tab30.onmouseover=function() {clearInterval(MyMar)};//鼠标移上时清除定时器达到滚动停止的目的
tab30.onmouseout=function() {MyMar=setInterval(Marquee,speed3)};//鼠标移开时重设定时器
</script>


