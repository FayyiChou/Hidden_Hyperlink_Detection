<script>

var speed2=30; //数字越大速度越慢
var tab20=document.getElementById("demo20");
var tab21=document.getElementById("demo21");
var tab22=document.getElementById("demo22");
tab22.innerHTML=tab21.innerHTML; //克隆demo1为demo2
function Marquee(){
if(tab22.offsetHeight-tab20.scrollTop<=0)//当滚动至demo1与demo2交界时
tab20.scrollTop-=tab21.offsetHeight //demo跳到最顶端
else{
tab20.scrollTop++
}
}
var MyMar=setInterval(Marquee,speed2);
tab20.onmouseover=function() {clearInterval(MyMar)};//鼠标移上时清除定时器达到滚动停止的目的
tab20.onmouseout=function() {MyMar=setInterval(Marquee,speed2)};//鼠标移开时重设定时器
</script>

