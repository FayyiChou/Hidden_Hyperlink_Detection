var productspeed=30;
var marquee_product9=document.getElementById("marquee_product9");
var marquee_product8=document.getElementById("marquee_product8");
var marquee_demo7=document.getElementById("marquee_demo7");
marquee_product9.innerHTML=marquee_product8.innerHTML;
function productmarquee(){
if(marquee_demo7.scrollLeft>=marquee_product8.scrollWidth){
marquee_demo7.scrollLeft=0;
}
else{
marquee_demo7.scrollLeft++;
}
}
var productMyMar=setInterval(productmarquee,productspeed);
marquee_demo7.onmouseover=function(){clearInterval(productMyMar);}
marquee_demo7.onmouseout=function(){productMyMar=setInterval(productmarquee,productspeed);}
