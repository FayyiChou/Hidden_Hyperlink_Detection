			function goAttribute(attrStr,rootMenuId,menuId) {
				var _url = CTX+"/attrList/"+encodeURIComponent(attrStr)+"?";
				if(rootMenuId==0){
					_url += "rootMenuId="+menuId;
				}else{
					_url += "subMenuId="+menuId;
				}
        		window.open(_url);
			}
    	    function addHistoryStore(){
    			var menuName = getMenuName(11);
    			var thisobj = {roomId:"914",title:"拾金不昧的拉斐尔才是好炮姐！",videoImgUrl:"http://img2.plures.net/live/screenshots/1100862/59d6/84eb/d598/d3d0/4617/cc6a/e912/e78b_1488088334.jpg",onlineStatus:"0",ancharName:"大坤坤",lookCount:"45890",menuName:menuName};
    			var thisobjlist = {};
    			var addhistory = store.get("jtvhistory");
    			if(addhistory!=null){
    				thisobjlist = addhistory.val;
    			}
    			thisobjlist[914] = thisobj;
    			storeWithExpiration.set("jtvhistory",thisobjlist,600000);
    		}
    		function addHistory(){
    			var memberId = $.cookie('memberId');
	    		if(memberId!=null){
					$.post(FRONT_DOMAIN+"/addHistory.do",{memberId:memberId,roomId:914},function(data,status){});			
				 }
				 var historyRoomIds = store.get('historyRoomIds')||"";
				 if(historyRoomIds != ""){
				 	var subhistoryRoomIds = ","+historyRoomIds+",";
				 	if(subhistoryRoomIds.indexOf(",914,") >= 0){
				 		historyRoomIds = subhistoryRoomIds.replace(",914,", ",");
				 	}else{
						var hisarr = str2Arr(historyRoomIds);
				 		if(hisarr.length > 60){
				 			historyRoomIds = historyRoomIds.substring(0,historyRoomIds.lastIndexOf(","));
				 		}
				 		historyRoomIds = ","+historyRoomIds+",";
				 		addHistoryStore();
				 	}
				 	historyRoomIds = 914 + historyRoomIds.substring(0,historyRoomIds.length-1);
				 }else{
				 	historyRoomIds = 914;
				 	addHistoryStore();
				 }
				store.set('historyRoomIds', historyRoomIds);
			}
			var addhistoryflag = false;
			function initSaveFistory(){
				setTimeout(function(){
					if(!addhistoryflag){
						var barflag = $.cookie('barerrorflag');
						var inithistoryload = $.cookie('inithistoryload');
						if(barflag==null && inithistoryload==null){
							initSaveFistory();
						}else{
							addHistory();
							addhistoryflag = true;
							return;
						}
					}
				},1000);
			}
