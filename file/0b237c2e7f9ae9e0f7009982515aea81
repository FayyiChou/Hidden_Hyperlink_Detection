<script>
var cats = [
	{id:random(), img:"upinkji_ad_1.jpg"},
    {id:63,  img:"upinkji_ad_7.jpg"},
    {id:21,  img:"upinkji_ad_5.jpg"},
    {id:2,  img:"upinkji_ad_2.jpg"},
    {id:5, img:"upinkji_ad_3.jpg"},
    {id:6,img:"upinkji_ad_4.jpg"},
    {id:22, img:"upinkji_ad_6.jpg"},
    {id:77, img:"upinkji_ad_8.jpg"},
    {id:144,img:"upinkji_ad_9.jpg"},
    {cx:true}];
	$(cats).each(function(index, data){
	    $.ajax({
	            url:"pc.php?act=get&",
	            dataType:'json',
	            data:{'kw':data.kw,'cid':data.id,'cx':data.cx},
	            success:function(d){
	                if (!d) {
	                    return false;
	                }
	                var tdata = {list:d};
	                if (typeof data.img) {
	                    tdata.img = data.img;
	                }
	                if (typeof data.id) {
	                    tdata.id = data.id;
	                }
	                $('#con'+index).append(template('tpl'+index,tdata));				
	                loadImg();
	            },error:function(){
	                    confirm('商品加载失败，是否刷新页面？') && location.reload();
	            }
	    });
	})
	random()
	function random(){
		var arr = [63,21,2,5,6,22,77,144];
		leng = arr.length;
		rid = parseInt(Math.random(0,leng)*10);
		return arr[rid];
	}

// 楼层导航
$(function(){
   var AllHet = $(window).height();
   var mainHet= $('.floorCtro').height();
   var fixedTop = (AllHet - mainHet)/2
 //  $('.floorCtro').css({top:fixedTop+'px'}); 
   $('.floorCtro p').click(function(){
		var ind = $('.floorCtro p').index(this)+1;
		var topVal = $('#floor0'+ind).offset().top;
		$('body,html').animate({scrollTop:topVal},1000)
	})
   $(window).scroll(scrolls)
   scrolls()
   function scrolls(){
	   var f1,f2,f3,f4,f5,f6,f7,f8,f9;
	   var fixRight = $('.floorCtro p');
	   var sTop = $(window).scrollTop();
	   fl = $('#floor01').offset().top;
	   f2 = $('#floor02').offset().top;
	   f3 = $('#floor03').offset().top;
	   f4 = $('#floor04').offset().top;
	   f5 = $('#floor05').offset().top;
	   f6 = $('#floor06').offset().top;
	   f7 = $('#floor07').offset().top;
	   f8 = $('#floor08').offset().top;
	   f9 = $('#floor09').offset().top;
	   
		
	   if(sTop<=600){
		   $('.floorCtro').fadeOut(300).css('display','none');
		   }
	   else{
		   $('.floorCtro').fadeIn(300).css('display','block');
		   }
	   
	   	if(sTop>=fl){
		   fixRight.eq(0).addClass('cur').siblings().removeClass('cur');
		   }
	   	if(sTop>=f2-100){
		   fixRight.eq(1).addClass('cur').siblings().removeClass('cur');
		   }
	   	if(sTop>=f3-100){
		   fixRight.eq(2).addClass('cur').siblings().removeClass('cur');
		   }
	   	if(sTop>=f4-100){
		   fixRight.eq(3).addClass('cur').siblings().removeClass('cur');
		   }
	   	if(sTop>=f5-100){
		   fixRight.eq(4).addClass('cur').siblings().removeClass('cur');
		   }
	   	if(sTop>=f6-100){
		   fixRight.eq(5).addClass('cur').siblings().removeClass('cur');
		   }
	   	if(sTop>=f7-100){
		   fixRight.eq(6).addClass('cur').siblings().removeClass('cur');
		   }
		if(sTop>=f8-100){
		   fixRight.eq(7).addClass('cur').siblings().removeClass('cur');
		   } 
		if(sTop>=f9-100){
		   fixRight.eq(8).addClass('cur').siblings().removeClass('cur');
		   }  
     }
});
</script>
