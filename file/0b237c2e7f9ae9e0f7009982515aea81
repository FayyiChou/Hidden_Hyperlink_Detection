// 2016-07-14 头部轮播
$(function(){
	var banner_length=$(".banner_ul li").length;//li长度
	var index= 0  //默认active 位置 0开始
	setInterval(function(){
		toIndex(index);
		if(index != banner_length-1)
		{
			index++; //位置增加
		}
		else
		{
			index=0; //位置重置
		}
	},5000)
	$(".banner_btn span").click(function(){
		index=$(this).index();
		toIndex(index);
	})
	function toIndex(index)
	{
		$(".banner_ul li").eq(index).addClass("active").siblings().removeClass("active");
		$(".banner_btn span").eq(index).addClass("active").siblings().removeClass("active");
	}
})
</script>
