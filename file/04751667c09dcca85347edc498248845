            $(function () {
                     
                     initNav('2');

                     var sTeamID=2;
                     $('#aTeam'+sTeamID).addClass('dian');
        

                     var carrousel = $( ".carrousel" );

                     $( ".portrait" ).click( function(e){
                         showButton($(this));
                         var src = $(this).find(".pic").attr( "data-src-wide" );
                         carrousel.find("img").attr( "src", src );
                         carrousel.fadeIn( 200 );

                     });

                     $('.close').click( function(e){
                         carrousel.find( "img" ).attr( "src", '' );
                         carrousel.fadeOut( 200 );
                     } );
					 $('.wrapper').click( function(e){
                         carrousel.find( "img" ).attr( "src", '' );
                         carrousel.fadeOut( 200 );
                     } );

                     $(".spanPrev").click(function () {
                         showPic(-1);
                     });

                     $(".spanNext").click(function () {
                         showPic(1);
                     });

                     //µ„ª˜…œœ¬∞¥≈•∫Ûœ‘ æ¥ÛÕº
                     function showPic(pre) {
                         var carrousel = $(".carrousel");
                         var imageList = $(".imgs");
                         var flag = true;
                         for (var i = 0; i < imageList.length; i++) {
                             for (var j = 0; j < imageList[i].children.length; j++) {
                                 var selectImage = imageList[i].children[j].firstChild.attributes["data-src-wide"];
                                 if (selectImage.textContent == carrousel.find("img").attr("src")) {
                                     if (j >= 0) {
                                         var index = j;
                                         if (pre == 1)
                                             index++;
                                         else
                                             index--;

                                         carrousel.find("img").attr("src", imageList[i].children[index].firstChild.attributes["data-src-wide"].textContent);

                                         if (imageList[i].children.length == 1) {
                                             $(".spanPrev").hide();
                                             $(".spanNext").hide();
                                         } else {
                                             var maxIndex = imageList[i].children.length;
                                             if (pre == 1)
                                                 maxIndex--;

                                             switch (index) {
                                                 //µ⁄“ª∏ˆ‘™Àÿ
                                                 case 0:
                                                     $(".spanPrev").hide();
                                                     $(".spanNext").show();
                                                     break;
                                                     //◊Ó∫Û“ª∏ˆ‘™Àÿ
                                                 case maxIndex:
                                                     $(".spanPrev").show();
                                                     $(".spanNext").hide();
                                                     break;
                                                 default:
                                                     $(".spanPrev").show();
                                                     $(".spanNext").show();
                                                     break;
                                             }
                                         }



                                         flag = false;

                                         break;
                                     }
                                 }
                             }
                             if (!flag) break;
                         }
                     }

                     //œ‘ æ…œœ¬∞¥≈•
                     function showButton(div) {
                         var imgs = div.parent().find(".portrait");
                         if (imgs.length == 1) {
                             $(".spanPrev").hide();
                             $(".spanNext").hide();
                         } else {


                             var imgIndex = imgs.index(div);
                             switch (imgIndex) {
                                 //µ⁄“ª∏ˆ‘™Àÿ
                                 case 0:
                                     $(".spanPrev").hide();
                                     $(".spanNext").show();
                                     break;
                                     //◊Ó∫Û“ª∏ˆ‘™Àÿ
                                 case imgs.length - 1:
                                     $(".spanPrev").show();
                                     $(".spanNext").hide();
                                     break;
                                 default:
                                     $(".spanPrev").show();
                                     $(".spanNext").show();
                                     break;
                             }
                         }
                     }

                 });
                 (function () {
                     var oDiv = document.getElementById("float");
                     var H = 0,
                     iE6;
                     var Y = oDiv;
                     while (Y) {
                         H += Y.offsetTop;
                         Y = Y.offsetParent
                     };
                     iE6 = window.ActiveXObject && !window.XMLHttpRequest;
                     if (!iE6) {
                         window.onscroll = function () {
                             var s = document.body.scrollTop || document.documentElement.scrollTop;
                             if (s > H) {
                                 oDiv.className = "div1 div2";
                                 if (iE6) {
                                     oDiv.style.top = (s - H) + "px";
                                 }
                             } else {
                                 oDiv.className = "div1";
                             }
                         };
                     }
                 })();
