<script type="text/javascript">
goTopEx();
//回到顶部
function goTopEx(){
	$(window).scroll(function(){  //只要窗口滚动,就触发下面代码 
		var scrollt = document.documentElement.scrollTop + document.body.scrollTop; //获取滚动后的高度 
		if( scrollt >200 ){  //判断滚动后高度超过200px,就显示  
			$("#goTopBtn").fadeIn(400); //淡出
		}else{
			$("#goTopBtn").stop().fadeOut(400); //如果返回或者没有超过,就淡入.必须加上stop()停止之前动画,否则会出现闪动
		}
	});
	
	$("#goTopBtn").click(function(){ //当点击标签的时候,使用animate在200毫秒的时间内,滚到顶部
		$("html,body").animate({scrollTop:"0px"},200);
	});
}

function Clickfree(obj) {
    $(".pup3").show();
}

function group(){
	$(".pup4").show();
}

function gold_xiu(){
	$(".pup5").show();
}

function noInvite () {
    $(".pup6").show();
}

function isExMax () {
    $(".pup7").show();
}

$(function() {
	//金币兑换相关弹窗
	//1.买号弹窗
	$(".bind_hao").click(function(){
		$(".bind_hao_box").fadeIn();
	})
	$(".bind_hao2").click(function(){
		$(".bind_hao_box2").fadeIn();
	})
	$(".bind_hao3").click(function(){
		$(".bind_hao_box3").fadeIn();
	})
	//金币
	$(".gold_pup1").click(function(){
		$(".gold_pup_box1").fadeIn();
	})
	//邀请好友
	$(".invite_pup").click(function(){
		$(".invite_pup_box").fadeIn();
	})
	//天数限制
	$(".day_pup1").click(function(){
		$(".day_pup_box1").fadeIn();
	})
	$(".day_pup2").click(function(){
		$(".day_pup_box2").fadeIn();
	})
	//有兑换未完成的任务
	$(".gold_pup2").click(function(){
		$(".gold_pup_box2").fadeIn();
	})
	
	//所有最新弹窗的关闭
	$(".pup1-butbox_new span").click(function(){
		$(this).parent().parent().parent().fadeOut();
	});
	/*
	$.get('/ajax/shop_desc?task_id=',function(res){
        $("#desc").html(res);
    });
	*/
	
	//点击关闭隐藏二维码
	$(".erweima a").click(function(){
		$(this).parent().hide();
	})
	
	//是否包邮弹窗信息
	$(".price a").mouseover(function(){
		$(this).siblings("div").show();
	})
	$(".price a").mouseout(function(){
		$(this).siblings("div").hide();
	})
	

	$(".shang i").mouseover(function(){
		$(this).siblings("div").show();
	})
	$(".shang i").mouseout(function(){
		$(this).siblings("div").hide();
	})
	//弹出与关闭--免审通过pup
	$(".J-pup1").click(function(){
        $(".pup1").show();
	});

	$(".pupmsg i").click(function(){
		$(this).parent().parent().hide();
	});
	$(".pup1-butbox span").click(function(){
		$(this).parent().parent().hide();
	});
	
	//商品详情页TAB切换
	var $divli=$("div.tab ul li");
	$divli.click(function(){
		$(this).addClass("action").siblings().removeClass("action");
		var index = $divli.index(this);
		$("div.tabbox>div").eq(index).show().siblings().hide();	
	});
	
	//商品详情>>试客晒单图片切换
	var $picli=$(".smallpic-list li");
	$picli.click(function(){
		$(this).addClass("bb").siblings().removeClass("bb");
		var index = $picli.index(this);
		$("div.bigpic>div").eq(index).show().siblings().hide();
	})
	
	//关闭晒单大图
	$(".bigpic i").live("click",function(){
		$(this).parent().hide();
		$(this).parent().parent().siblings(".smallpic-list").children(".bb").removeClass("bb");
	})
	
	//扫描二维码，手机申请
	$(".apply_free_phone").mouseover(function(){
		$(".phone_ewm_box").show();
	})
	
	$(".apply_free_phone").mouseout(function(){
		$(".phone_ewm_box").hide();
	})
})

</script>

</body></html>