		function isTouchDevice() {
		try {
			document.createEvent('TouchEvent');
			return true;
		}
		catch(e) {
			return false;
		}
		}
		var smartphone = isTouchDevice();
		var usite = Number(get_cookie('usite'));
		if(!usite) usite = 0;
		if(usite<3)
			setcookie('usite', usite+1, 2678400, '/');
		$(function(){
			if(!smartphone && usite<2)
				$('#ispeak').css('display', 'block');
		});
		</script>
