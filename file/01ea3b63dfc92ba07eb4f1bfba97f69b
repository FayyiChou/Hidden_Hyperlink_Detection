<script>

function isValidEmail (email, strict)
{
 if ( !strict ) email = email.replace(/^\s+|\s+$/g, '');
 return (/^([a-z0-9_\-]+\.)*[a-z0-9_\-]+@([a-z0-9][a-z0-9\-]*[a-z0-9]\.)+[a-z]{2,4}$/i).test(email);
}

function isValidPhone(phonn)
{
	phonn = phonn.replace(/\(|\)|-|\s+/g, '');
	return (/^[\+]?[(]?[0-9]{3}[)]?[- \s\.]?[0-9]{3}[- \s\.]?[0-9]{4,6}$/im).test(phonn) | (/^(1\s|1|)?((\(\d{3}\))|\d{3})(\-|\s)?(\d{3})(\-|\s)?(\d{4})$/).test(phonn);
}

function TestFormFastOrder()
{
	var name = $("#fast_order form input[name='data[new][lname]']").val();
	var phone = $("#fast_order form input[name='data[new][phone]']").val();
	var em = $("#fast_order form input[name='data[new][email]']").val();
$("#fast_order form input[name='ch_rb']").val('1');
	if(!isValidPhone(phone))
	{
	
		alert("Введите правильный номер телефона!");
		return false;
	}	
	
	if(name=='' || phone==''){
		alert('Заполните имя и номер телефона!');
		return false;
	}
	
	if(em!="" && !isValidEmail(em))
	{
		alert("Введите правильный e-mail адрес!");
		return false;
	}	
	
	$("#fast_order form").submit();
}

function TestFormOrderMaster()
{
	var name = $("#order_master form input[name='data[new][lname]']").val();
	var phone = $("#order_master form input[name='data[new][phone]']").val();
	var em = $("#order_master form input[name='data[new][email]']").val();
	
	$("#order_master form input[name='ch_rb']").val('1');
	if(name=='' || phone=='' || em==''){
		alert('Заполните все поля!');
		return false;
	}

	if(!isValidEmail(em))
	{
		alert("Введите правильный e-mail адресс!");
		return false;
	}	
	
	$("#order_master form").submit();
}

function selProductFast(id)
{
	$('#fast_order form').attr('action', '/emarket/getOneClickOrder/element/'+id+'/');

}

function selProductOrderMaster(id, price)
{
	$('#order_master form').attr('action', '/emarket/getOneClickOrder/element/'+id+'/');
	
	$('input#inp_om_price_master').val(price);
}
</script>
