<script type="text/javascript">
/*鼠标悬浮切换*/
function setTab(name,cursel,n){
	for(i=1;i<=n;i++){
		var menu=document.getElementById(name+i);
		var con=document.getElementById("con_"+name+"_"+i);
		menu.className=i==cursel?"hover":"";
		con.style.display=i==cursel?"block":"none";
	}
}
jQuery(document).ready(function($) {
	index("/","/templates/default/");
	//登录
	$.get('/plus/ajax_user.php?act=loginform', function(data) {
		$("#ajax_login").html(data);
		$('.hot-line').remove();
		// 选择登录方式 
		var wxrun = '';
		$('.loginicon').toggle(function(){
			$('#pcLogin').hide();
			$('#codeLogin').show();
			$('#login-box h1').html('微信登录');
			$(this).attr('title', '用户名登录');
			$(this).removeClass('wx').addClass('pc');
						wxrun = window.setInterval(run, 5000);
			function run(){
			    $.get("/m/login.php?act=waiting_weixin_login",function(data){
			        if(data=="1"){
			           window.location="/";
			        }
			    });
			}
					}, function(){
			$('#pcLogin').show();
			$('#codeLogin').hide();
			$('#login-box h1').html('会员登录');
			$(this).attr('title', '微信登录');
			$(this).removeClass('pc').addClass('wx');
						window.clearInterval(wxrun);
					});
	});
	
	if(""){
		dialog({
	        title: '系统提示',
	        content: '亲爱的用户您好：<br />我们检测到您所在地区为 <font color="#ff6600"></font>，建议您切换至<br /> <a href=""><font color="#ff6600"></font></a>，让我们为您提供更准确的职位信息。',
	        okValue:'同意',
	        ok: function () {
	        	window.location.href="";
	        },
	        cancelValue:'取消',
	        cancel: function () {
	        }
	    }).showModal();
	}
});
</script>
