$(function(){
	$("[lazy-src]").lazyloadanything({
		'timeout':20,
		'includeMargin':true,
		'onLoad': function(e, LLobj) {
			var $ele = LLobj.$element;
			var src = $ele.attr("lazy-src");
			$ele.load(src);
		}
	});
});	
