                var sks = tbs[type] ? tbs[type] : '%E7%BB%BC%E5%90%88';
                var url = "";
                if (type == _res.Search[0][0] || type == _res.Search[0][1] || type == _res.Search[0][2] || type == _res.Search[0][3]) url = "http://sosu.qidian.com/searchresult" + bs + ".aspx?searchkey=" + encodeURIComponent(k) + "&searchtype=" + sks;
                else if (type == _res.Search[0][4] || type == _res.Search[0][5]) url = "http://sosu.qidian.com/searchreview" + bs + ".aspx?searchkey=" + encodeURIComponent(k) + "&searchtype=" + sks;
                else if (type == _res.Search[0][6]) url = "http://" + fbs + ".qidian.com/SearchResult.aspx?key=" + escape(k);
                else if (type == 'yzsc') url = 'http://www.yzsc.com.cn/search.aspx?rcid=10010&q=' + escape(k);
                window.open(url);
            },
            SH: function(url) {//SetHomepage
                if (!url) return false;
                if (document.all) {
                    document.body.style.behavior = 'url(#default#homepage)';
                    document.body.setHomePage(url);
                } else if (window.sidebar) {
                    if (window.netscape) {
                        try {
                            netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
                        } catch (e) {
                            alert("该操作被浏览器拒绝，如果想启用该功能，请在地址栏内输入 about:config,然后将项 signed.applets.codebase_principal_support 值该为true");
                        }
                    };
                    var prefs = Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
                    prefs.setCharPref('browser.startup.homepage', url);
                } else
                    alert('您的浏览器不支持自动自动设置首页, 请使用浏览器菜单手动设置!');

                return false;
            },
            R: { //Recom
                //BookFormat
                BF: ['<div class="pic"><a href="http://www.qdmm.com/MMWeb/{0}.aspx" target="_blank" title="{1}"><img src="',beLogoImageUrl,beBookLogoImage,'{0}/{0}.jpg" onerror="javascript:EP.R.DC(this);" alt="{1}" width="90" height="112" border="0" /></a></div>',
                           '<div class="txt" style=" white-space:nowrap;"><a class="name" href="http://www.qdmm.com/MMWeb/{0}.aspx" target="_blank" title="{1}">《{1}》</a><br />',
                                '类型：<a href="{4}" target="_blank" title="{5}">{5}</a><br />作者：<a href="http://me.qdmm.com/authorIndex.aspx?id={2}" target="_blank" title="{3}">{3}</a><br />',
                                '<a class="red" href="http://www.qdmm.com/MMWeb/{0}.aspx" target="_blank" title="阅读《{1}》">[立刻阅读]</a>',
                            '</div>'].join(''),
                //BookItemFormat
                BIF: '<li onmouseover="this.className=\'active\';EP.R.MO({7});" onmouseout="this.className=\'\'">[<a href="{5}" class="gray" target="_blank" title="{6}">{6}</a>] <a class="name" href="http://www.qdmm.com/MMWeb/{0}.aspx" target="_blank" title="{1}">{2}</a></li>',
                B: function() { //Builder
                if (!MMRecom) return;
                    var _er = EP.R;
                    var _bif = _er.BF;
                    var _blf = _er.BIF;
                    var sbObj = $('showbook');
                    var lbObj = $('listbook');
                    if (!sbObj || !lbObj) return;

                    var lbHtml = ['<ul>'];
                    for (var i = 0; i < MMRecom.length && i < 15; i++) {
                        var _b = MMRecom[i];
                        if (i == 0) {
                            sbObj.innerHTML = _bif.Format(_b.BookId, _b.BookName,
                                                        _b.AuthorId, _b.AuthorName,
                                                        CU(_b.SubcatrgoryId), _b.SubcatrgoryName);
                            sbObj._i = i;
                        };
                        lbHtml.push(_blf.Format(_b.BookId, _b.BookName, Sub(_b.BookName, 15, true),
                                                        0, 0,
                                                        CU(_b.SubcatrgoryId), _b.SubcatrgoryName, i));
                    };
                    lbHtml.push('</ul>');
                    lbObj.innerHTML = lbHtml.join('');
                    
                    try {
                        $('k').focus();
                    } catch (e) { ; }
                },
                MO: function(i) { //MouseOver
                if (!MMRecom || i < 0) return;
                    var sbObj = $('showbook');
                    if (!sbObj || sbObj._i == i) return;
                    var _er = EP.R;
                    var _b = MMRecom[i];
                    sbObj.innerHTML = _er.BF.Format(_b.BookId, _b.BookName,
                                                _b.AuthorId, _b.AuthorName,
                                                CU(_b.SubcatrgoryId), _b.SubcatrgoryName);
                    sbObj._i = i;
                },
                DC: function(o) {//DefaultCover
                    if (!o) return;
                    o.src = beLogoImageUrl + beBookLogoImage + '0.jpg';
                }
            }
        };
        eventBind(window, 'load', EP.R.B);
		
    </script>
    <script type="text/javascript" src="http://script.cmfu.com/Script/MMScript.js"></script>

</body></html>