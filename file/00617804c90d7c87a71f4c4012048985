    //导航默认样式记录，还原初始样式时候需要
    var _position = jQuery("#goods_details_nav").css('position');
    var _top = jQuery("#goods_details_nav").css('top');
    var _right = jQuery("#goods_details_nav").css('right');
    var _left = jQuery("#goods_details_nav").css('left');
    var _zIndex = jQuery("#goods_details_nav").css('z-index');
     jQuery(window).scroll(function(){
        if(jQuery(this).scrollTop() > _defautlTop){
            if(jQuery.browser.msie && $.browser.version=="6.0"){
                jQuery("#goods_details_nav").css({'position':'absolute','top':eval(document.documentElement.scrollTop),'right':_defautlRight,'z-index':9});
                防止出现抖动
                jQuery("html,body").css({'background-image':'url(about:blank)','background-attachment':'fixed'});
            }else{
            	if(_defautlRight != undefined){
                jQuery("#goods_details_nav").css({'position':'fixed','top':0,'right':_defautlRight,'z-index':9});
              }else{
              	jQuery("#goods_details_nav").css({'position':'fixed','top':0,'left':_defautlLeft,'z-index':9});
              }
            }
            jQuery("#scroll_add_to_cart").show();
        }else{
        	 if(_defautlRight != undefined){
               jQuery("#goods_details_nav").css({'position':_position,'top':_top,'right':_right,'z-index':_zIndex});
            }else{
               jQuery("#goods_details_nav").css({'position':_position,'top':_top,'left':_left,'z-index':_zIndex});
            }
            jQuery("#scroll_add_to_cart").hide();
        }
         var topBuffer = [jQuery("#detail_s1").offset().top-30,jQuery("#detail_s2").offset().top-30,jQuery("#detail_s3").offset().top-30,jQuery("#detail_s4").offset().top-30,jQuery("#detail_s5").offset().top-30,jQuery("#detail_s6").offset().top-30,jQuery("#detail_s7").offset().top-30,jQuery("#detail_s8").offset().top-30,jQuery("body").outerHeight()];       
        var curTop = jQuery(this).scrollTop();
        var currentPage = getPosit(curTop, topBuffer);
        jQuery("#goods_details_nav li").removeClass("scroll_nav_current").eq(currentPage-1).addClass("scroll_nav_current");
		    function getPosit(topValue,bufferArr){
		        for(var i= 0, len=bufferArr.length; i<len; i++ ){
		            if(topValue < bufferArr[i]){
		                if(i == 0)return 1;
		                return i;
		            }
		        }
		    }
    });     
   	
})
</script> <p class="p11"> <label>&nbsp;</label><span style="vertical-align:middle;"> <a onclick="addToCart_forSKU(3387)" href="javascript:void(0);"><img src="themes/7zhou/images/add_to_cart.gif" /></a> <a onclick="collect(3387)" style="margin-left:10px;" href="javascript:void(0);"><img src="themes/7zhou/images/save.gif" /></a> </span></p> </td>
            </tr> 
           </tbody>
          </table> </td>
        </tr> 
       </tbody>
