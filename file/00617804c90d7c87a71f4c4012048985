              <div class="clearfix"></div> <script type="text/javascript">
 var svr_date2 = {
 	   minDate: new Date(),
 	   //maxDate: '+6m',
		 showOn: 'focus',  //显示
	   showMonthAfterYear: true, // 月在年之后显示      
	   changeMonth: true,   // 允许选择月份      
	   changeYear: true,   // 允许选择年份
	   onClose: function(dateText, inst) {changePrice();}
	};
 var svr_date = {
 	   minDate: new Date(),
 	   //maxDate: '+6m',
		 showOn: 'focus',  //显示
	   showMonthAfterYear: true, // 月在年之后显示      
	   changeMonth: true,   // 允许选择月份      
	   changeYear: true,   // 允许选择年份
	   onClose: function(dateText, inst) {changePrice(); jQuery('#svr_date2').datepicker( "option" , "minDate" ,dateText);}
	};
 $(function(){
	 jQuery('#svr_date').datepicker(svr_date);
	 jQuery('#svr_date2').datepicker(svr_date2);
	 changePrice();
 });	
 function changeAtt(t) {
   t.lastChild.checked='checked';
   for (var i = 0; i<t.parentNode.childNodes.length;i++) {
        if (t.parentNode.childNodes[i].className == 'cattsel') {
            t.parentNode.childNodes[i].className = '';
        }
    }
   t.className = "cattsel";
   changePrice();
}
