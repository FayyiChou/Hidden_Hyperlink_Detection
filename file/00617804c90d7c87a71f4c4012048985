jQuery(function () {
		jQuery(function () {
				jQuery(window).scroll(function () {
				if (jQuery(window).scrollTop() > 100) {
				  jQuery("#dialog_scrolltotop").fadeIn(100);
				}else {
				  jQuery("#dialog_scrolltotop").fadeOut(100);
				}
				
				
				});
				jQuery(".gotop").click(function () {
				  jQuery('body,html').animate({scrollTop: 0}, 0);
				  return false;
				});
				jQuery(".gobottom").click(function () {
				  jQuery('body,html').animate({scrollTop: jQuery(document).height()}, 0);
				  return false;
				});			
		    jQuery(window).bind('scrollstart', function(){
		        jQuery("#topmenu_kf").removeClass("topmenu_kf").addClass("topmenu_kf2");
		    });
		 
		    jQuery(window).bind('scrollstop', function(e){
		        jQuery("#topmenu_kf").removeClass("topmenu_kf2").addClass("topmenu_kf");
		    });				
					
		});
});
