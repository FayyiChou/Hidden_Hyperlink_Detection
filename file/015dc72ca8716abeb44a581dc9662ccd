var speed=30 
marquePic2.innerHTML=marquePic1.innerHTML 
function Marquee(){ 
if(demos1.scrollLeft>=marquePic1.scrollWidth){ 
demos1.scrollLeft=0 
}else{ 
demos1.scrollLeft++ 
} 
} 
var MyMar=setInterval(Marquee,speed) 
demos1.onmouseover=function() {clearInterval(MyMar)} 
demos1.onmouseout=function() {MyMar=setInterval(Marquee,speed)} 
