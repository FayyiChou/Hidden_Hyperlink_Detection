        function fun_showDetail(n) {  
            LoadHtml(data.productlist[n]);
        }

        function LoadHtml(dataProDetail) {
            seajs.use('http://res.rongzi.com/main/library/' + 'template/template-native.js', function (template) {
                var html = template("tpl_ProDetail", dataProDetail);     
                $.layer({
                    type: 1,
                    shade: [0.3, '#000'],
                    area: ['auto', 'auto'],//
                    title: false,
                    border: [10, 0.3, '#000'],
                    page: { html: html },
                    success: function (layero) {
                        $(".xubox_close").removeAttr("href");
                        $(".application").click(function () {
                            layer.closeAll();
                            var html = template("tpl_ProDetail", dataProDetail);
                            $.layer({
                                type: 1,
                                shade: [0.3, '#000'],
                                area: ['auto', 'auto'],//
                                title: false,
                                border: [10, 0.3, '#000'],
                                page: { html: template("tpl_askAtOnce", window.login) },
                                success: function (layero) {
                                    $('input, textarea').placeholder();
                                    if (window.login) {
                                        if (window.login.isLogin) {
                                            $("#userName4").val(window.login.userName);
                                            $("#userMobile4").val(window.login.cellPhone);
                                            if (0 == window.login.sex || 2 == window.login.sex) {
                                                $("#radiowoman4").prop("checked", true);
                                                $("#radiowoman4").parent().addClass("checked");
                                                $("#radiowoman4").parent().parent().siblings().find(".dwrap-radio").removeClass("checked");
                                            }
                                            else {
                                                $("#radioman4").prop("checked", true);
                                                $("#radiowoman4").parent().addClass("checked");
                                                $("#radiowoman4").parent().parent().siblings().find(".dwrap-radio").removeClass("checked");
                                            }
                                            $(".sec-guide3 #d-forNoLogined").hide();


                                            if (window.login.identity > 0) {
                                                $("#Identity_val4").val(window.login.identity);
                                                switch (window.login.identity) {
                                                    case 1:
                                                        $("#Identity_txt4").val("企业主");
                                                        $('#Identity_val4').val(1)
                                                        break;
                                                    case 2:
                                                        $("#Identity_txt4").val("个体户");
                                                        $('#Identity_val4').val(2)
                                                        break;
                                                    case 4:
                                                        $("#Identity_txt4").val("上班族");
                                                        $('#Identity_val4').val(4)
                                                        break;
                                                    case 8:
                                                        $("#Identity_txt4").val("其他");
                                                        $('#Identity_val4').val(8)
                                                        break;
                                                    default:
                                                        break;
                                                }
                                            }
                                            if (window.login.loanAmount > 0) {
                                                $("#loanMoney_4").val(window.login.loanAmount);
                                            }
                                        }
                                    };
                                    $(".combobox").mouseover(function () {
                                        $(this).find(".dropdown").removeAttr("style");
                                        if ($(this).find(".txtLoanMoney").length > 0 && $(this).find(".txtLoanMoney[readonly]").length == 0) {
                                            $(this).find(".txtLoanMoney").focus();
                                        }
                                        else {
                                            $(this).focus();
                                        }
                                    });
                                    $(".dwrap-radio").find("input[type='radio']").each(function (i, item) {
                                        $(item).on("click", function () {
                                            if ($(this).prop("checked")) {
                                                $(this).parent().addClass("checked");
                                                $(this).parent().parent().siblings().find(".dwrap-radio").removeClass("checked");
                                            }
                                        });
                                    });
                                    index.dropdown("#dropdown_money_4", "#loanMoney_4", "", true);
                                    index.dropdown("#dropdown_Identity4", "#Identity_txt4", "#Identity_val4", false);
                                }
                            });
                        });
                    }
                });
            });
        }

      function table_switch(num) {
          for (i = 1; i < 5; i++) {
              if (i != num) {
                  $("#" + "table_" + i).css('display', 'none');
                  $("#" + "a_" + i).css('color', '#666666');
              }
          }
          $("#" + "table_" + num).css('display', 'block');
          $("#" + "a_" + num).css('color', '#f17a6c');
          switch (num) {
              case "1": {
                  $(".redLine").css('left', '0px');
              };
                  break;
              case "2": {
                  $(".redLine").css('left', '200px');
              };
                  break;
              case "3": {
                  $(".redLine").css('left', '400px');
              };
                  break;
              case "4": {
                  $(".redLine").css('left', '590px');
              };
                  break;
          }
      }
    </script>

        <script src="http://res.rongzi.com/main/scripts/CityChange/citychange.js"></script>
            <script>
            //根据QueryString参数名称获取值
            function getQueryStringByName(name) {
                var result = location.search.match(new RegExp("[\?\&]" + name + "=([^\&]+)", "i"));
                if (result == null || result.length < 1) {
                    return "";
                }
                return result[1];
            }

            var regSelRefer = /^(http:\/\/)?\w+\.rongzi\.com/gm;
            $(document).ready(function () {
                var seoInfo = {
                    "hmsr": getQueryStringByName("hmsr"),
                    "utm_campaign": getQueryStringByName("utm_campaign"),
                    "utm_content": getQueryStringByName("utm_content"),
                    "utm_medium": getQueryStringByName("utm_medium"),
                    "utm_source": getQueryStringByName("utm_source"),
                    "utm_term": getQueryStringByName("utm_term"),
                    "rzm_ref": getQueryStringByName("rzm_ref"),
                    "ouid": getQueryStringByName("ouid"),
                    "refer": document.referrer
                };
                //RecordUserSeoInfo
                if (seoInfo.hmsr != "" || seoInfo.utm_source != "" || seoInfo.rzm_ref != "" || (seoInfo.refer != "" && !regSelRefer.test(seoInfo.refer))) {
                    //todo:
                    $.ajax({
                        type: "POST",
                        url: "/common/RecordUserSeoInfo",
                        data: seoInfo,
                        success: function (data) {
                        },
                        dataType: "JSON"
                    });
                }
            });
        </script>

    
    

    <script>
        $(function () {
            $('input, textarea').placeholder();
            //底部一键委托
            $('#layerPop .close-btn').on('click', function () {
                $('#layerPop').animate({ right: "-100%" }, 800);
                setTimeout(function () {
                    $('#layerPop').hide();
                    $('#layerOpen').show();
                    $("#layerOpen").animate({ right: "0" }, 300);
                }, 700);
            });
            $('#layerOpen .openBtn').on('click', function () {
                $('#layerOpen').animate({ right: "-180px" }, 300);
                setTimeout(function () {
                    $('#layerPop').show();
                    $('#layerOpen').hide();
                    $("#layerPop").animate({ right: "0" }, 800);
                }, 200);
            });

        })
    </script>
