   <script type="text/javascript">
    var InpTextArea=document.getElementById("InpWord"),
	    InpNum=document.getElementById("textnum"),
		len=100-InpTextArea.value.length;
		InpNum.innerHTML=len;
    InpTextArea.onkeyup=function(){
        len =100-InpTextArea.value.length;
		if(len<0){
			InpTextArea.value=InpTextArea.value.substring(0,100);
			len=0;
		}
		InpNum.innerHTML=len;
	};

	var sendFlower 	= $('#toFlowerNum');	// 用户想要赠送的鲜花数
	var gold 		= $('#goldNum');	// 用户需要支付的金币数
	var money 		= $('#payMoney');	// 用户账户内的人民币余额
	var needMoney 	= $("#how_money2");	// 用户还需要支付的人民币金额

   //送鲜花页面显示
   function SendFlower(ToUserid,NickName,ArticleID,Url){
   		//判断是否登陆
	   if($.trim('0') =='0'){
			alert('请先登录!');
			flowerDialog.Close();
		}
	   flowerDialog("FlowerDialog",ToUserid,NickName,ArticleID,Url);

	   getUserMoney();
   }

   function flowerDialog(id,ToUserid,NickName,ArticleID,Url){
	   var dialog=document.getElementById(id).style;
	   var Bg=document.getElementById("flowerWBg").style;
	   Bg.display="block";
	   Bg.height=document.body.offsetHeight+"px";
	   dialog.display="block";
	   flowerDialog.Close=function(){
			dialog.display="none";
			Bg.display="none";
		}
		$('#flowertouser1').html(NickName);
		$('#flowertouser2').html(NickName);
		$('#fromUserid').val('0');
		$('#toUserid').val(ToUserid);
		$('#ArticleID').val(ArticleID);
		$('#ArticleUrl').val(Url);
   }

	// 赠送鲜花
	function sendFlower2()
	{
		var fromuserid="0";
		var baseUrl="http://blog.cnfol.com";
		var touserid=$('#toUserid').val();
		var flowerNum=sendFlower.val();

		var goldNum = parseInt(gold.html()); // 用户金币余额
		var rmbNum = 0;

		var sourceUrl=$('#ArticleUrl').val();
		var sourcetabid=$('#SourceTabID').val();
		var note=$('#InpWord').val();
		if(fromuserid==false)
		{
			MyDialog.Close('FlowerDialog');
			alert('请先登录');
			return;
		}
		if(flowerNum==false)
		{
			MyDialog.Close('FlowerDialog');
			alert('鲜花数不能为空');
			return;
		}
		var regnum = /[0-9]+/;
		if(!regnum.test(flowerNum))
		{
			MyDialog.Close('FlowerDialog');
			alert('鲜花数请输入数字!');
			return;
		}

		// 用户账户余额不足够
		if(needMoney.parent().css('display') == 'block') {

			var buytype = ($('#payMoneyChk').is(':checked')? '1': '0');

			var str = 'fromuserid='+fromuserid+'&touserid='+touserid+'&flowerNum='+flowerNum+'&more=1&buytype='+buytype+'&note='+note+'&sourceUrl='+sourceUrl+'&sourcetabid='+sourcetabid+'&callback='+"http://blog.cnfol.com/shengdanlaoren/liveroom/";
			location.href =  "http://blog.cnfol.com/index.php/myfocus/sendFlowerMoney?"+str;
		}
		// 用户账户余额足够
		else {

			// 用户金币余额小于赠送的鲜花数，用人民币支付差额
			if(flowerNum > goldNum) rmbNum = (flowerNum - goldNum) / 10;

			$.ajax({
				url:baseUrl + "/index.php/myfocus/sendflower",
				type:"post",
				dataType:"json",
				data:{fromuserid:fromuserid,touserid:touserid,flowerNum:flowerNum,rmbNum:rmbNum,note:note,sourceUrl:sourceUrl,sourcetabid:sourcetabid,more:1},
				success:function(result)
				{
					flowerDialog.Close();
					alert(result.info);
					location.reload();
				}
			});
		}
	}

	// 计算金额
	function FlowerKeyUp(obj){//2016-05-24 鲜花数 金币 余额 还需支付 计算

		var sendNum = sendFlower.val(),
				prg = /^[0-9]*$/,
				goldNum = parseInt(gold.html()),
				payMoney = parseFloat(money.html());

		payMoney = parseInt(payMoney * 100); // 人民币余额放大100倍
		var Rmb = (sendNum - goldNum) * 10; // 金币差额放大10倍，相当于人民币差额放大100倍

		if(prg.test(sendNum)){
			if(sendNum > goldNum){//金币不足
				var chk = money.parent().find('input:checked');
				//alert($(chk).length)
				if($(chk).length > 0){//勾选余额
					if(Rmb > payMoney){//余额不足
						Rmb = (Rmb - payMoney) / 100; // 解决浮点数相减尾数不精确的问题
						needMoney.html(Rmb).parent().css('display','block');
					}else{//余额足够
						needMoney.parent().css('display','none');
					}
				}else{//未勾选余额
					needMoney.html((sendNum - goldNum) / 10).parent().css('display','block');
				}
				money.parent().removeClass('BeHide');
			}else{//金币足够
				needMoney.parent().css('display','none');
				money.parent().addClass('BeHide');
			}
		}else{
			alert('请输入数字');
			sendFlower.val(0);
			money.parent().css('display','none');
			needMoney.parent().css('display','none');
		}
	}

	// 选择使用人民币支付
	function payMoneyChk1(obj){

		var sendNum = sendFlower.val(),
				goldNum = parseInt(gold.html()),
				payMoney = parseFloat(money.html());

		payMoney = parseInt(payMoney * 100); // 人民币余额放大100倍
		var Rmb = (sendNum - goldNum) * 10; // 金币差额放大10倍，相当于人民币差额放大100倍

		if(Rmb > 0){
			if($(obj).is(':checked')){
				if(Rmb > payMoney){ // 还需要的人民币大于人民币余额
					Rmb = (Rmb - payMoney) / 100; // 解决浮点数相减尾数不精确的问题
					needMoney.html(Rmb).parent().css('display','block');
				}else{
					needMoney.parent().css('display','none');
				}
			}else{
				needMoney.html((sendNum - goldNum) / 10).parent().css('display','block');
			}
		}else{
			needMoney.parent().css('display','none');
		}
	}

	// 是否成功加载余额
	var hasLoaded = false;

	// 获取用户账户余额
	function getUserMoney() {

		// 如果成功加载，拒绝请求
		if(hasLoaded) {

			// 触发计算金额事件
			FlowerKeyUp(sendFlower);
			return;
		}

		// 请求余额接口
		var url = 'http://blog.cnfol.com/index.php/blogapi/getUserMoney?' + new Date().getTime();

		$.getJSON(url, function(data) {

			if(data.errCode == '00') {

				gold.html(data.golden);
				money.html(data.money);
				hasLoaded = true;
				// 触发选择使用人民币支付事件
				payMoneyChk1($('#payMoneyChk'));
				// 触发计算金额事件
				FlowerKeyUp(sendFlower);
			}
			else {

				alert('查询用户账户失败');
			}
		});
	}
 	
	
