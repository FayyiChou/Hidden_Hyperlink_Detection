    <script type="text/javascript" src="//static.lecai.com/js/lottery.js?v=201611022253"></script>
    <script type="text/javascript" src="//static.lecai.com/js/lottery/ssq/ssq.base.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function() {
        	var current_phase = undefined;
        	
        	function initCurrentPhase() {
        		$.ajaxCurrentPhase({
        			'lottery_type': LotteryType.SSQ,
        			'global' : false,
        			'success': function(obj) {
        				current_phase = obj.data['phase']; // 设置当前期
        			}
        		});
        	}
        	
        	initCurrentPhase();
            //随机
            function ssq_random(){
                var random = function(){
                    var num = 6;
                    var val = $.shuffle({
                        min: C.SSQ.RED_MIN,
                        max: C.SSQ.RED_MAX,
                        count: num,
                        sort: true
                    });
                    var inputs = $('#qb_ssq_select .lot_sn_red span');
                    for(var i = 0 , iMax = inputs.length ; i < iMax ; i ++) {
                        var $input = $(inputs[i]);
                        $input.text(val[i].toString().leftpad(2,'0'));
                    }
                    val = $.shuffle({
                        min: C.SSQ.BLUE_MIN,
                        max: C.SSQ.BLUE_MAX,
                        count: 1,
                        sort: true
                    });
                    inputs = $('#qb_ssq_select .lot_sn_blue span');
                    for(var i = 0 , iMax = inputs.length ; i < iMax ; i ++) {
                        var $input = $(inputs[i]);
                        $input.text(val[i].toString().leftpad(2,'0'));
                    }
                }
                var interval = undefined;
                var count = 0;
                interval = setInterval(function(){
                    count ++;
                    random();
                    if(count > 9) {
                        clearInterval(interval);
                    }
                },50);
            }
            // 随机生成一注
            $('#ssq_random').click(function(){
                ssq_random();
            });
            
            ssq_random();
            
            // 立即投注
            $('.buy-btns').click(function (){
            	if(!UID){
                    $.executeErrorHandler(C.ERROR.UNLOGIN);
                }
            	
            	var is_upload = "";
                var is_chase = false;
                var is_syndicate = false;
                var upload_id = 0;
                var multiple = 1;
                var content = quickBuy();
                
                var submit_data = {
                    lottery_type: LotteryType.SSQ,
                    phase: current_phase,
                    select_type: is_upload ? SelectType.UPLOAD : SelectType.CHOOSE,
                    multiple: multiple,
                    is_chase: is_chase,
                    is_syndicate: is_syndicate,
                    content: content,
                    upload_id: upload_id,
                    success: function(obj) {
                        $.showCountdownSuccess('恭喜您，投注成功！', {
                            detail: '您可以去“<a href="/user/order/normal/">投注记录</a>”查看投注记录及中奖情况！',
                            'close': function() {
                                window.location.href = "/";
                            }
                        });
                    }
                };
                submit_data.amount = C.SSQ.UNIT_PRICE;
                var lottery_submit = new LotterySubmit(submit_data);
                if (lottery_submit.isValid()) {
                    lottery_submit.submit();
                }
                else {
                    return;
                }
            });
            
            
            function quickBuy(){
                var balls = $('#qb_ssq_select .lot_sn_red span');
                var redBalls = [] , blueBalls = [];
                var sb = new StringBuffer();
                sb.append(PlayType.SSQ_DS).append('%');
                for(var i = 0 , iMax = balls.length ; i < iMax ; i ++) {
                    var $ball = $(balls[i]);
                    if($ball.text().length > 0){
                        redBalls.push($ball.text());
                    }
                }
                sb.append(redBalls.join(','))
                var balls = $('#qb_ssq_select .lot_sn_blue span');
                sb.append('|');
                for(var i = 0 , iMax = balls.length ; i < iMax ; i ++) {
                    var $ball = $(balls[i]);
                    if($ball.text().length > 0){
                        blueBalls.push($ball.text());
                    }
                }
                sb.append(blueBalls.join(','));
                if(/5001%[0-9]{2},[0-9]{2},[0-9]{2},[0-9]{2},[0-9]{2},[0-9]{2}\|[0-9]{2}/.test(sb.toString())){
                    return sb.toString();
                }
                return false;
            }
            
            $(".change-number").initLeheBtns();
            $(".buy-btns").initLeheBtns();
        });
    </script>
