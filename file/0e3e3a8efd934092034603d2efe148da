<script type="text/javascript">
document.onkeydown = function() 
{
	var form = document.forms[0];

	if (event.keyCode == 13)
	{
		var usercode =form.user.value;
		if(usercode==""||usercode=="用户名或手机号码或邮箱")
		{
			form.user.focus();
			document.getElementById("errorDIV").innerHTML="请输入用户名！";
			return;
		}
		var userpasswd = document.getElementById("passwd").value;
		if(userpasswd==""||userpasswd=="请输入密码")
		{
			document.getElementById("passwd").focus();	
			document.getElementById("errorDIV").innerHTML="请输入密码！";
			return;		
		}	
		var checkNO =form.checkNO.value;
		if(checkNO=="")
		{
			form.checkNO.focus();	
			document.getElementById("errorDIV").innerHTML="请输入验证码！";
			return;		
		}
		if (usercode!="" && userpasswd!=""&&checkNO!="")
		{
			submitPage(); 
		}
		else
		{
			event.keyCode =9;
		}
	}
}
function submitPage()
{
	var form = document.forms[0];
	var usercode =form.user.value;
	if(usercode==""||usercode=="用户名或手机号码或邮箱")
	{
		form.user.focus();
		document.getElementById("errorDIV").innerHTML="请输入用户名！";
		return;
	}
	var userpasswd = document.getElementById("passwd").value;
	if(userpasswd==""||userpasswd=="请输入密码"||typeof(userpasswd) == "undefined")
	{
		document.getElementById("passwd").focus();	
		document.getElementById("errorDIV").innerHTML="请输入密码！";
		return;		
	}	
	var checkNO =form.checkNO.value;
	if(checkNO=="")
	{
		form.checkNO.focus();	
		document.getElementById("errorDIV").innerHTML="请输入验证码！";
		return;		
	}
	checkPwd();
}
function adjustImg()
{
	var codeImg = document.getElementById("codeImg");
	codeImg.src	="/image.jsp?"+"&ts=" + new Date().getTime(); 
	document.getElementById("errorDIV").innerHTML="";
	var form = document.forms[0];
	form.error.value="N";
}
function checkPwd()
{
	document.getElementById("errorDIV").innerHTML="";
	var form = document.forms[0];
	form.error.value="N";
	
	var url="/loginm.shtml?option=homecheckLogin&user="+form.user.value+"&password="+form.passwd.value+"&checkNO="+form.checkNO.value;		
	exec(handleCheckPwdChange, url);			
}
function handleCheckPwdChange()
{
	var result = xmlHttp.responseText;
	var link = "";
	var sucess = "";
	var pos = result.indexOf("<link>");
	if(pos>0)
	{
		var epos = result.indexOf("<\/link>");
		if(epos>pos)
		{
			link=result.substring(pos+"<link>".length,epos);
		}
	}			
	var ppos = result.indexOf("<error>");
	if(ppos>0)
	{
		var pepos = result.indexOf("<\/error>");
		if(pepos>ppos)
		{
			sucess=result.substring(ppos+"<error>".length,pepos);
		}
	}		
	if(sucess=="false")
	{	
		var form = document.forms[0];
        //系统自动登录
		$.emlink.cookie.set(CLUB_AUTOLOGIN,"auto", {
            topdomain: true,expires:365
        });
		//$.emlink.cookie.set(CLUB_CPWD,loginPassword, {
        //   topdomain: true,expires:365
        //});
		$.emlink.cookie.set(CLUB_CID,form.user.value, {
           topdomain: true,expires:365
        });	        

		form.error.value="N";
		form.option.value="login";
		form.suboption.value="b";
		form.target="_self";
		form.action=link+"/login.shtml";
		//form.action="/login.shtml";
		form.submit();
	}
	else 
	{
		var form = document.forms[0];
		if(sucess=="验证码输入不对!")
		{
			adjustImg();
			form.checkNO.focus();
			document.getElementById("errorDIV").innerHTML=sucess;
		}
		else
		{
			document.getElementById("errorDIV").innerHTML=sucess;
			var form = document.forms[0];
			form.error.value="Y";			
		}
	}
}
function initUser()
{
	var form = document.forms[0]; 
    var usr = $.emlink.cookie.get(CLUB_CID);
    if(usr != null&&usr != "")
    	form.user.value=usr; 	
    //adjustImg();	
}


function isLogin()
{
	var url="/loginm.shtml?option=isLogin";		
	exec(handleCheckIsLoginChange, url);			
}
function handleCheckIsLoginChange()
{
	var result = xmlHttp.responseText;
	var link = "";
	var sucess = "";
	var pos = result.indexOf("<link>");
	if(pos>0)
	{
		var epos = result.indexOf("<\/link>");
		if(epos>pos)
		{
			link=result.substring(pos+"<link>".length,epos);
		}
	}			
	var ppos = result.indexOf("<sucess>");
	if(ppos>0)
	{
		var pepos = result.indexOf("<\/sucess>");
		if(pepos>ppos)
		{
			sucess=result.substring(ppos+"<sucess>".length,pepos);
		}
	}			
	var name = "";
	var npos = result.indexOf("<name>");
	if(npos>0)
	{
		var nepos = result.indexOf("<\/name>");
		if(nepos>npos)
		{
			name=result.substring(npos+"<name>".length,nepos);
		}
	}			
		
	
	if(sucess=="false")
	{	
		$("#indexLogin").show();
		$("#logoutLink").hide();
		$("#nameLink").hide();
		initUser();
		$("#headLink").html("<a href=\""+link+"/login.html\" class=\"hrefs\" >会员登录</a><b></b>");
	}
	else 
	{
		$("#indexLogin").hide();
		
		
		$("#headLink").html("<a href=\""+link+"/account/index.html\" class=\"hrefs\" style='color:red;'>账户首页</a><b></b>");
		$("#logoutLink").html("<a onclick=\"logout()\" style=\"cursor:pointer;color:red;\" >退出登录</a>");
		$("#nameLink").html("尊敬的"+name+"您好！");
		$("#logoutLink").show();
		$("#nameLink").show();
		
	}
}
function logout()
{
	var form = document.forms[0];
	form.option.value="logout";
	form.suboption.value="b";
	form.target="_self";
	form.action="/loginm.shtml";
	form.submit();
}
isLogin();
