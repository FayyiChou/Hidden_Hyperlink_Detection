$(document).ready(function() {
      $('#header_slider').bxSlider({
            mode: 'fade',
            captions: true,
            auto: true,
            autoControls: true
      });

      var fields0={};
    $('.field_text').each(function(){
        fields0[$(this).attr('id')]=$(this).val();
    });
      $('.field_text').focus(function(){
            if($(this).val()==fields0[$(this).attr('id')]) { $(this).val(''); }
    });
      $('.field_text').blur(function(){
            if ($(this).val()==''){$(this).val(fields0[$(this).attr('id')])}
    });

      var dateFormat = 'dd-mm-yy'; // Формат даты (напр. 05-12-2014)
     // $("#input_date").val($.datepicker.formatDate(dateFormat,new Date())); 
      $('#input_date').datepicker({
            showOn: "focus",
         //   gotoCurrent: true,
            dateFormat: dateFormat
      });
    $('#input_date').datepicker({
            showOn: "button",
         //   gotoCurrent: true,
            dateFormat: dateFormat
      });

