<script type="text/javascript">





function scheduleseach()
{
 
var o_id=0;
var n_id=0;
var otpr=document.getElementById("input_otpr").value;
var nazn=document.getElementById("input_nazn").value;
var date=document.getElementById("input_date").value;
var tecdate=document.getElementById("tecdate").value;
var jsonstantionarrid = document.getElementById("jsonstantionarrid").value.split(','); 
var jsonstantionarrname = document.getElementById("jsonstantionarrname").value.split(',');
var num = document.getElementById("numstantion").value;
document.getElementById("otpr").value=otpr;
document.getElementById("nazn").value=nazn;
document.getElementById("datetime").value=date;

var tr1=0;
var tr2=0;
for (var i = 0; i < num; i++)
{
if (otpr==jsonstantionarrname[i])
{o_id=jsonstantionarrid[i]; tr1=1;} 
if (nazn==jsonstantionarrname[i])
{n_id=jsonstantionarrid[i]; tr2=1;} 
}  
if (tr1==0)  o_id=-1;
if (otpr=="Откуда") o_id=0;
if (tr2==0)  n_id=-1;
if (nazn=="Куда") n_id=0;
if (date==tecdate) document.location.href="../schedule?o_id="+o_id+"&n_id="+n_id;
else  document.location.href="../schedule?o_id="+o_id+"&n_id="+n_id+"&date="+date;
}

function trainrouteseach(route)
{
document.getElementById("route").value=route;
var date=document.getElementById("input_date").value;
var tecdate=document.getElementById("tecdate").value;
var o_id=document.getElementById("o_").value;
var n_id=document.getElementById("n_").value;
if (date==tecdate) document.location.href="../schedule?route="+route+"&o_id="+o_id+"&n_id="+n_id;
else  document.location.href="../schedule?route="+route+"&o_id="+o_id+"&n_id="+n_id+"&date="+date; 
}
function changestation()
{
document.getElementById("input_otpr").value=document.getElementById("input_nazn").value;
document.getElementById("input_nazn").value=document.getElementById("otpr").value;
document.getElementById("otpr").value=document.getElementById("nazn").value;
document.getElementById("nazn").value=document.getElementById("input_nazn").value;
var tmp=document.getElementById("n_").value;
document.getElementById("n_").value=document.getElementById("o_").value;
document.getElementById("o_").value=tmp;
}


function date_focus() {
  document.getElementById('input_date').focus();
}


