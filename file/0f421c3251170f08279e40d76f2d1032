<script type="text/javascript">
$(document).ready(function(){
// --- Автозаполнение ---


function liFormat (row, i, num) {
var result = row[0] + '<p class=qnt>' + row[1];
  return result;
}
function selectItem(li) {
  if( li == null ) var sValue = 'А ничего не выбрано!';
  if( !!li.extra ) var sValue = li.extra[2];
  else var sValue = li.selectValue;
  document.getElementById("otpr").value=document.getElementById("input_otpr").value;
  document.getElementById("nazn").value=document.getElementById("input_nazn").value;
  //alert("Выбрана запись с ID: " + sValue);
}

// --- Автозаполнение2 ---
$("#input_otpr").autocomplete("../templates/station.php", {
  delay:10,
  minChars:1,
  matchSubset:1,
  autoFill:true,
  matchContains:1,
  cacheLength:10,
  selectFirst:true,
  formatItem:liFormat,
  maxItemsToShow:20,
  zIndex: 160, // z-index списка
  maxHeight: 400, // Максимальная высота списка подсказок, в пикселях
  width: 165, // Ширина списка
  deferRequestBy: 300, // Задержка запроса (мсек), на случай, если мы не хотим слать миллион запросов, пока пользователь печатает. Я обычно ставлю 300.
  onItemSelect:selectItem

}); 
$("#input_nazn").autocomplete("../templates/station.php", {
  delay:10,
  minChars:1,
  matchSubset:1,
  autoFill:true,
  matchContains:1,
  cacheLength:10,
  selectFirst:true,
  formatItem:liFormat,
  maxItemsToShow:20,
  zIndex: 160, // z-index списка
  maxHeight: 400, // Максимальная высота списка подсказок, в пикселях
  width: 165, // Ширина списка
  deferRequestBy: 300, // Задержка запроса (мсек), на случай, если мы не хотим слать миллион запросов, пока пользователь печатает. Я обычно ставлю 300.
  onItemSelect:selectItem
}); 

$("#input_otpr").change(function() {
var s=document.getElementById("input_otpr").value;
s = s.charAt(0).toUpperCase() + s.substr(1);
document.getElementById("otpr").value=s;
document.getElementById("input_otpr").value=s;
});

$("#input_nazn").change(function() {
var s=document.getElementById("input_nazn").value;
s = s.charAt(0).toUpperCase() + s.substr(1);
document.getElementById("nazn").value=s;
document.getElementById("input_nazn").value=s;
});

$("#input_otpr").blur(function() {
var s=document.getElementById("input_otpr").value;
s = s.charAt(0).toUpperCase() + s.substr(1);
document.getElementById("otpr").value=s;
document.getElementById("input_otpr").value=s;
});

$("#input_nazn").blur(function() {
var s=document.getElementById("input_nazn").value;
s = s.charAt(0).toUpperCase() + s.substr(1);
document.getElementById("nazn").value=s;
document.getElementById("input_nazn").value=s;
});


});




</script>

