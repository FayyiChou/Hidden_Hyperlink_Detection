    <script type="text/javascript">
        function initCheckPostNew() {
            $('#checkPostNew').click(function(){
               window.location.reload(); 
            });
            setTimeout(checkPostNew, 5000);
        }
        
        function checkPostNew() {
            $('#postListForm').ajaxSubmit({
                url: ctx + '/countNew',
                success: function(r) {
                    if(r > 0) {
                        $('#checkPostNew').html(r + '条新采集').slideDown();
                    }
                    setTimeout(checkPostNew, 5000);
                },
                dataType: 'json'
            });
        }
        
        $(function(){
            $('#postListForm').find('[name="offset"]').val(0);
            initMasonry($('#container'), $(window), $(document), 150, $('#postListForm'), $('#postTpl'), $('#loading'), function(imgs){
                initPostBtns(imgs);
            });
            initCheckPostNew();
        });
    </script>
    












