<script type="text/javascript">
    //Focus
    $(function () {
        $("#sousuo").blur(function () {
            var R = $("#sousuo").attr("placeholder");
            if ($("#sousuo").val() == "") {
                $("#sousuo").attr("placeholder", "请输入产品名字");
            }
            else {
                location.href = "/Product/ProductList.aspx?kWords=" + $("#sousuo").val();
            }
        })
        $("#sousuo").focus(function () {
            $("#sousuo").attr("placeholder", "");
        })
    })

    function TiaoZhuan() {
        location.href = "/company/CompanySearch.aspx";
    }
    $(function () {
        var $tab_nav = $(".tab ul li");
        $tab_nav.click(function () {
            $(this).addClass("select").siblings().removeClass("select");
            var index = $tab_nav.index(this);
            $(".tabContent > div").eq(index).show().siblings().hide();
        }).hover(function () {
            $(this).addClass("hover");
        }, function () {
            $(this).removeClass("hover");
        }
				 );
    });
    $(".topNav").click(function () {
        $(".fastMenuShow").slideToggle(300);
    }

    );


</script>

