<script type="text/javascript">
    BizQQWPA.addCustom({ aty: '1', a: '1001', nameAccount: 4006677369, selector: 'bizQQ' }); //企业qq自定义样式
    SetLoginStatus();
    //设置顶部用户登录状态
    function SetLoginStatus() {
        var url = "http://wp.qihuiwang.com/CommonAshx/CheckLoginStatus.ashx?callback=?";
        jQuery.getJSON(url, function (data) {
           
            $("#Isdenglu").val("1");
            if (data != "") {
                $("#loingName").val(data.UserName);
                $("#dengluming").text(data.UserName);
                $("#txtUserId").val(data.UserId);
                $("[name='zhuxiao']").attr("style", "display:");
                $("[name='denglu']").attr("style", "display:none");
            }
            else {
                $("[name='zhuxiao']").attr("style", "display:none");
                $("[name='denglu']").attr("style", "display:");
            }
        });
        if ($("#Isdenglu").val() != "1") {
            $("[name='zhuxiao']").attr("style", "display:none");
            $("[name='denglu']").attr("style", "display:");
        }
    }
    $(".backTop").click(function () {
        $('body,html').animate({ scrollTop: 0 }, 1000);
        return false;
    });
</script>

