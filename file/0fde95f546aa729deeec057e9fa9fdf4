<script>
$(function() {
	ymaps.ready(init);
	function init () {
		var myMap = new ymaps.Map("map",
			{
				center: [55.753559,37.609218],
				zoom: 10
			}
		),
		gCollection = new ymaps.GeoObjectCollection(),
		placemark = [],
		metki =
        [
					{
			center: [55.753559,37.609218],
			name: "Магазин семян",
			desc: "Описание",
			},
				];
		metki.forEach(function (group) {
    		var placemark = new ymaps.Placemark(group.center, {
    			balloonContentBody: '<div class="map-name">'+group.name+'</div>'+'<div class="map-des">'+group.desc+'</div>',
    			hintContent: group.name,
    		});
			gCollection.add(placemark);
		});
			    myMap.controls.add('smallZoomControl');
			    myMap.geoObjects.add(gCollection) ;
	}
});
</script>
