<script>
var dataLayer = window.dataLayer || [];
function kmOnProductClickGTM(product, use_callback) {
    dataLayer.push({
        'event': 'productClick',
        'ecommerce': {
            'click': {
                'products': [{
                    'name': product.name || '',
                    'price': product.price || undefined,
                    'category': product.category,
                    //'variant': productObj.variant,
                    'position': product.position
                 }]
            }
         },
        'eventCallback': function() {
            if (use_callback) {
                document.location = product.url
            }
        }
  });
}
dataLayer.push({
    'event': 'kmEcommerce'
});
function kmOnProductAddRemoveToCartGTM(product) {
    var product_data = {
                'products': [{
                    'name': product.name,
                    'category': product.category,
                    'variant': product.variant,
                    'price': product.price,
                    'quantity': product.quantity
                 }]
            };
    var data = {};
    if (product.action === 'add') {
        data = {
            'event': 'addToCart',
            'ecommerce': {
                'currencyCode': 'RUB',
                'add': product_data
            }
        };

    } else {
        data = {
            'event': 'removeFromCart',
            'ecommerce': {
                'currencyCode': 'RUB',
                'add': product_data
            }
        };
    }
    dataLayer.push(data);
    dataLayer.push({
    'event': 'kmEcommerce'
});
}
dataLayer.push({'page_category':'default'});
dataLayer.push({'user_status':'not logged'});
