        var isVote = false;
        var voteNews = function (type) {
            if (!isVote) {
                isVote = true;
                $.getJSON("/WebService/ViewNums.ashx",
                     { action: "vote", type: type, id: forID, module: module, t: Math.random() },
                     function (json) {
                         $("#" + type + "Num").text(json.Message);
                     });
            }
            else { $.douho.showInfo({ msg: "您已经投过票了" }); }
            return false;
        }
