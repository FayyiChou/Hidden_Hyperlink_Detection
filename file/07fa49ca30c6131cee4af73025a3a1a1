<script type="text/javascript">
$(function(){
	$(".anli_con").find(".anniu").hide();
	$(".anli_con").hover(function(){
		var num = $(this).find("li").length;
		if(num > 10){
	$(this).find(".anniu").show();
		}
},
function(){
	$(this).find(".anniu").hide();
})
}) 
</script>
