 $(".hyul").hover(
function () {
	
	$(this).addClass("hybse");
	
	var obj = $(this).find(".hyzhe");
	
	
obj.show();
	

},
function () {
	
	$(this).removeClass("hybse");
	
	var obj = $(this).find(".hyzhe");
	
	obj.hide();

}
);
 

function hyxshi(a){
	
