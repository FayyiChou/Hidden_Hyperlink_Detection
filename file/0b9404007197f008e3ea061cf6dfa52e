    $(function() {
        $("#flink-more").click(function(){
            if($('#flink-content').css('overflow') == 'hidden') {
                $('#flink-content').css({overflow:""});
                $("#triangle").addClass('triangle-up').removeClass("triangle-down");
                $("#more-text").text("收起");
            }
            else {
                $('#flink-content').css({overflow:"hidden"});
                $("#triangle").addClass('triangle-down').removeClass("triangle-up");
                $("#more-text").text("展开");
            }
        });
    })
