   <script type="text/javascript">
function showNavTab(n){
	var h=document.getElementById("nav").getElementsByTagName("li");
	var d=document.getElementById("cnav").getElementsByTagName("h2");
	for(var i=0;i<h.length;i++){
		if(n-1==i){
			h[i].className+=" thisclass";
			d[i].className+=" block";
		}
		else {
			h[i].className=" ";
			d[i].className=" ";
		}
	}
}
</script>

