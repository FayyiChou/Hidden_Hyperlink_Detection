  <script>
    $(function(){
      $(".no_zhangaiview").click(function(){
        $(".no_zhangai").slideToggle();
if($(this).html() == "无障碍浏览"){
          $(this).html("关闭无障碍");
          $(this).css("color","red");
        }else{
          $(this).html("无障碍浏览");
          $(this).css("color","#000");
        }
      })
    })

    var _now=1;
    $(".pagefangda").click(function(){
      _now = _now*1.1;
      document.body.style.zoom=_now;

      console.log(_now)
    })

    $(".pagesuoxiao").click(function(){
      _now = _now*0.9;
      document.body.style.zoom=_now;

      console.log(_now)
    })

    //文字放大
    $(".fontfangda").click(function() {
      //取得当前字体大小 后缀px,pt,pc
      var currentFontSize = $("#zoom").css("font-size");
      //取得当前字体大小，parseFloat()转为float类型去除后缀
      var currentFontSizeNumber = parseFloat(currentFontSize);
      //新定义的字体大小
      var newFontSize = currentFontSizeNumber * 1.1;
      //重写样式表
      $("*").css("font-size", newFontSize);
      //JavaScript不向下执行
      return false;
    });

    //文字缩小
    $(".fontsuoxiao").click(function() {
      var currentFontSize = $("#zoom").css("font-size");
      var currentFontSizeNumber = parseFloat(currentFontSize);
      var newFontSize = currentFontSizeNumber * 0.9;
      $("*").css("font-size", newFontSize);
      return false;
    });
  </script>

