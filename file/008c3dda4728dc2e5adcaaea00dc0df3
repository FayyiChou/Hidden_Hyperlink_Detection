function ccbTip(tip){
	var opacity=$("#black_layer").css("opacity");
	if(opacity==0){
		$("#black_layer").css("opacity","0.6");
	}
	$("#tip").text(tip);
	$(".cc-PromptBox").show();
    $(".black_layer").show();
}

