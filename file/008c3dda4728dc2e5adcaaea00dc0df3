<script type="text/javascript">
//is app client
var isAppClient=0;
var getAppLoginInfo_custNo = "";

function isApp(msale_way){
	if(msale_way == "ADR" || msale_way == "IOS"){
		return true;
	}
	return false;
}

function appCartNumUpdate(){
	$.ajax({
		url : '/app/getAppLoginInfo',
		timeout:6E4,
		dataType : 'json',
		async:false,
		success : function(data){
			if(data != null){
				if(isApp(data.msale_way)){
					if(typeof(Ocj) != "undefined" ){
						try {Ocj.setShopCount(data.cart_num);}catch(e){};
					}else{
						try {callHandler('ChangeBadgeNumberCallBack', [{'badgeNumber': data.cart_num}],  function(response) {});}catch(e){};
					};
				};
			
			};
		}
	});
}
</script>
