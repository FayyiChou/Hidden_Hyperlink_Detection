<script type="text/javascript">
var cities = "0";
$(document).ready(function() {
    $(".pAddressArea").slick({
        nextArrow:false,
        infinite:false
    });
    $('.pAddressArea').on("beforeChange", function(event, slick, currentSlide, nextSlide){
        var i = nextSlide;
        $(".pAddressTab").removeClass("pAddressTabOn");
        $(".pAddressTab").eq(i).addClass("pAddressTabOn");
    });
    $(".pAddressTab").click(function(){
        $(".pAddressArea").slick("slickGoTo",$(this).index());
    });
    
	if(cities > 0){
		var defTown = null;
		$("#city").find("p").each(function(i){
			var val = $(this).attr("id");
			if(val == ''){
				defTown = $(this);
			}
		});
		if(defTown != null){
			initTown('', '', defTown, '0');
		}
		
		$("#l_group").val('');
		$("#m_group").val('');
		var showAreatxt = "" + sgroup_name;
		if(showAreatxt.length > 14){
			showAreatxt = showAreatxt.substring(0, 14);
		}
		$("#area_sel").html(showAreatxt);
	}
});
function initCity(obj){
	$(".pAddressArea").slick("slickNext");
	lgroup_name = $(obj).html();
	var pid = $(obj).attr("id");
	$("#l_group").val(pid);
	$.ajax({
		url : "/common/initcities",
		type : "GET",
		data : {
			"item_code" : "15106180",
			"provid" : pid
		},
		success : function(data) {
			if(data == 'NODATA'){
				$("#city").html("没有可显示的市");
			}else{
				$("#city").html(data);
			}
		},
		error : function(re) {}
	});
}
function initTown(pid, cid, obj, isqh){
	if(isqh == '1'){
		$(".pAddressArea").slick("slickNext");
	}
	var defsc = "";
	$("#m_group").val(cid);
	lgroup_code = pid;
	mgroup_code = cid;
	sgroup_code = defsc;
	mgroup_name = $(obj).html();
	$.ajax({
		url : "/common/inittowns",
		type : "GET",
		async : false,
		data : {
			"item_code" : "15106180",
			"provid" : pid,
			"cityid" : cid,
			"defScode" : defsc
		},
		success : function(data) {
			if(data == 'NODATA'){
				$("#town").html("没有可显示的区县");
			}else{
				$("#town").html(data);
				$("#town").find("p").each(function(){
					if($(this).attr("class") == 'hover'){
						sgroup_name = $(this).html();
					}
				});
			}
		},
		error : function(re) {}
	});
}
</script></div>
