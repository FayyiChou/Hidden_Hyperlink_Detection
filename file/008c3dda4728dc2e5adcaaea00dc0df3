<script type="text/javascript">
function  myocjhref(link){
	window.location.href = link+"?ts="+new Date().getTime();

}
$(document).ready(function(){	
	changeRegion_cd = function(region_cd, sel_region_cd,substation_code,district_code,area_lgroup,area_mgroup) {
		var expires = new Date();
		expires.setTime(expires.getTime() + 365 * 24 * 60 * 60 * 1000);
		_expires = (typeof hours) == "string" ? "" : ";expires=" + expires.toUTCString();
		path = "/";
		$.fn.cookie('c_code', region_cd, { expires : 365, path : '/',domain:'.ocj.com.cn'});
		$.fn.cookie('sel_region_cd', sel_region_cd, { expires : 365, path : '/',domain:'.ocj.com.cn'});
		$.fn.cookie('substation_code', substation_code, { expires : 365, path : '/',domain:'.ocj.com.cn'});
		$.fn.cookie('district_code', district_code, { expires : 365, path : '/',domain:'.ocj.com.cn'});
		$.fn.cookie('area_lgroup', area_lgroup, { expires : 365, path : '/',domain:'.ocj.com.cn'});
		$.fn.cookie('area_mgroup', area_mgroup, { expires : 365, path : '/',domain:'.ocj.com.cn'});
		var url = "/?substation_code="+substation_code;
		location.href = url;
	}
});
</script>
<script type="text/javascript">

$(document).ready(function(){
	$.ajax({
		url : '/app/getAppLoginInfo?ismobile=ismobile',
		timeout:6E4,
		dataType : 'json',
		success : function(data){
			if(typeof(getAppLoginInfo_custNo) != "undefined" ){
				getAppLoginInfo_custNo = data.cust_no;
			}
			
			//[ruei]设置r_domain区分来自浏览器还是客户端
			var r_domain = $.fn.cookie("r_domain");
			if(r_domain == null || r_domain == ""){
				$.fn.cookie('r_domain', data.msale_way + ".m.ocj.com.cn", { expires : 365, path : '/',domain:'.ocj.com.cn'});
			}
			
			if(data.loginYn == "Y"){
				
				if(isApp(data.msale_way)){
					if(typeof(Ocj) != "undefined" ){
						Ocj.loginSuccess(data.cust_name);
						Ocj.setShopCount(data.cart_num);
					}else{
						try {
							callHandler('ChangeCustNameCallBack', [{'custName': data.cust_name}],  function(response) {});
							callHandler('ChangeBadgeNumberCallBack', [{'badgeNumber': data.cart_num}],  function(response) {});
						}
						catch(e){};
					}

				}

				$('#loginInfo1').html("<h2 class=\"fl\">" + data.cust_name + "</h2><a href=\"javascript:myocjhref('/login/logout');\" class=\"fr\">退出</a>");

				$.fn.cookie('login_yn', 'Y', { expires : 3, path : '/'});
				$.fn.cookie('login_cust_name',data.cust_name, { expires : 3, path : '/'});
				
			}else{
				//$('#loginInfo3').hide();
				if(isApp(data.msale_way)){
					if(typeof(Ocj) != "undefined" ){
						Ocj.loginSuccess("");
						Ocj.setShopCount(0);
					}else{
						try {
							callHandler('ChangeCustNameCallBack', [{'custName': ''}],  function(response) {});
							callHandler('ChangeBadgeNumberCallBack', [{'badgeNumber': data.cart_num}],  function(response) {});
						}
						catch(e){};
					}
				}
				
				$.fn.cookie('login_yn', 'N', { expires : 3, path : '/'});
				$.fn.cookie('login_cust_name','', { expires : 3, path : '/'});
			}
			
			if(isApp(data.msale_way)){
				
				if(typeof(isAppClient) != "undefined" ){
					isAppClient = 1;
					appVersion = data.version_info;
				}
				$('.side_tgl_btn').hide();
				//首页隐藏下载app图片
				if(typeof($('.app_download')) != "undefined" ){
					$('.app_download').hide();
				}
				
			}else{
				//$('.side_tgl_btn').show();
			}
			
		}
	});
	
	
});
</script>
