<script type="text/javascript">
/*是不是微信打开的页面*/

if(is_weixn()){
	if($.cookie('indexFooterDownApp') != 'hitY'){
		$(".hHtmlAdvertisement").show();
	} 
	if(window.location.href.indexOf("app_web.jsp")>0){
		$(".hHtmlAdvertisement").hide();
	}
	if(window.location.href.indexOf("app_web_sc.jsp")>0){
		$(".hHtmlAdvertisement").hide();
	}
	function closeTip(){
	 if($.cookie('indexFooterDownApp') != 'hitY'){
			$.cookie('indexFooterDownApp','hitY',{expires:1.25,path:'/'});
			$(".hHtmlAdvertisement").hide();
		} 
	}
}
	
if(location.href.indexOf('login') == -1&&location.href.indexOf('customers') == -1){
	$.cookie('returnUrlBefore', location.href, { expires : 365, path : '/',domain:'.ocj.com.cn'});
}
function appLoginInfo(){
	return "";
}
function historyGo(){
	var ref = window.document.referrer;
	
	if(ref != undefined && ref != '' && ref != 'undefined' && ref.indexOf("event/niuslanquestion") >= 0)
	{
		window.location.href = "/";
		return;
	}
	if(ref != undefined && ref != '' && ref != 'undefined' && (ref.indexOf("darw/living") >= 0||ref.indexOf("events/yxred") >= 0 ||ref.indexOf("darw/hema") >= 0 ||ref.indexOf("darw/qszcfst") >= 0 ||ref.indexOf("darw/foodtime") >= 0 ||ref.indexOf("darw/goldrain") >= 0 ||ref.indexOf("darw/hotList") >= 0))
	{
		window.location.href = ref;
		return;
	}
	if(ref != undefined && ref != '' && ref != 'undefined' && ref.indexOf("?isPufa=1") > 0)
	{
		window.location.href = ref;
		return;
	}
	if(ref == ''||ref.indexOf("/qr") > 0 ||ref.indexOf("/detail") > 0 ||ref.indexOf("/admin/se_ads/parseandforward") > 0 ){
		window.location.href ="/";
	}else{
		history.go(-1);
	}
}
function is_weixn(){  
    var ua = navigator.userAgent.toLowerCase();  
    if(ua.match(/MicroMessenger/i) == 'micromessenger'){ 
        return true;  
    } else {  
        return false;  
    }  
}  
</script>


<script type="text/javascript">
	var openwin = "";
	function popICC() {
		popICC("");
	}
	function popICC_Side() {
		popICC("");
	}
	function popICC(order_no) {
		var ua = navigator.userAgent.toLowerCase();
		var isvalid = true;
		if (window.ActiveXObject) {
			var version = ua.match(/msie ([\d.]+)/)[1];
			if (parseInt(version) < 8) {
				isvalid = false;
			}
		}
		if (!isvalid) {
			alert("对不起，您的浏览器版本太低，请更新您的浏览器以后再使用东东客服，谢谢！");
		} else {
			var cust_no = "";
			if (typeof (getAppLoginInfo_custNo) != "undefined") {
				cust_no = getAppLoginInfo_custNo;
			} else if (typeof (getWebLoginInfo_custNo) != "undefined") {
				cust_no = getWebLoginInfo_custNo;
			}

			if (cust_no != "") {
				if (openwin.closed == true || openwin == "" || openwin == null) {
					var pram = new Object();
					if (typeof (currItemcode) != "undefined") {
						pram.item_code = currItemcode;
						last_sale_price_gen = "138";
						last_sale_price_otuan = "0";
						if (last_sale_price_gen != "0") {
							pram.last_sale_price = last_sale_price_gen;
						} else if (last_sale_price_otuan != "0") {
							pram.last_sale_price = last_sale_price_otuan;
						}
					}
					if (order_no != "") {
						pram.order_no = order_no;
					}
					var thisurl = window.location.href;
					var thisurls = thisurl.split('?');
					pram.imsource = thisurls[0];
					var popIccUrl = "";
					var popIccLoginYn = "";
					$
							.ajax({
								type : "get",
								url : window.location.protocol + "//"
										+ window.location.host
										+ "/popicc/getPopIccUrl",
								async : false,
								data : pram,
								dataType : "json",
								success : function(data) {
													
					//add#ymy#20161122#start#东东客服，驴妈妈和Hk的详情页面加aimnf参数
					data.url = data.url.indexOf('?') > -1 ? data.url + '&aimnf=N' : data.url;
									popIccUrl = data.url;
									popIccLoginYn = data.login_yn;
								}
							});
					if (popIccLoginYn == "Y" && popIccUrl != "") {
						var msale_code = '51';
						if (msale_code == '51' || msale_code == 'KR') {
							openwin = window.open("about:blank", "",
									"fullscreen=0,resizable=yes");
							openwin.moveTo(0, 0);
							openwin.resizeTo(850, 720);
							openwin.focus();
							openwin.location = popIccUrl;
						} else {
							window.location.href = popIccUrl;
						}
					} else {
						if (popIccLoginYn == "N") {
							alert("登录已超时，请重新登录!");
							window.location.href = window.location.protocol
									+ "//" + window.location.host
									+ "/login?returnUrl="
									+ window.location.href+"&reg_retunyn=Y";
						} else {
							alert("尊敬的顾客，东东客服系统正在升级中，请稍后进行访问。");
						}
					}
				} else {
					openwin.focus();
				}
			} else {
				alert("非常抱歉，使用该功能必须先登录!");
				var isHkShop=$("#isHkShop").val();
				if(isHkShop=='true'){
					window.location.href = window.location.protocol + "//"
					+ window.location.host + "/login/mhklogin?returnUrl="
					+ window.location.href;;
				}else{
					window.location.href = window.location.protocol + "//"
					+ window.location.host + "/login?returnUrl="
					+ window.location.href+"&reg_retunyn=Y";
				}
			}
		}
	}
</script>

