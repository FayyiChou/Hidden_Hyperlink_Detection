function goToLmmCaledar(){
	$("#cartForm").append('<input type="hidden" id="aimnf" name="aimnf" value="" />');
	$("#cartForm").attr('action','/detail/lmmMobileCalendar?ts='+new Date().getTime()).submit();
}
function ok (){};
function buttonDP(operate, flag){
	//建行商盟，不能购买全球购和韩国馆商品
	if(""=="y" && ("false"=="true" || "false"=="true" || "false"=="true")){ 
		ccbTip("对不起，全球购商品不支持建行支付，请下载东方购物APP，使用其他支付方式购买");
		return false;
	}
	//
	if(""=="y" && "2000"!="2000"){
		ccbTip("对不起，四川，黑龙江，湖北暂不支持建行支付，无法参与此活动");
		return false;
	}
	if(""=="y" && flag==2 ){
		ccbTip("对不起，暂不支持预约商品订购");
		return false;
	}
	if("" == "TT"){
		var num = $("#prdNum1").val();
		var amt = (num * parseFloat("138.0")).toFixed(2);
		$.ajax({
			url : "http://ottdata.bestv.com.cn/dfgwapp/ottdetail?version=1"
					+ "&item_code=15106180&item_name=大漠果香  新疆灰枣二等500g*2葡萄干238g*4超值特惠组合&origin=购买&msale_way=HTML"
					+ "&user_id=&mac=&os_type=WEB"
					+ "&order_qty=" + num + "&order_amt=" + amt + "&operate=" + operate + "&timestamp=" + new Date().getTime(),
			dataType:'jsonp',
		});
		setTimeout("execFlag('"+flag + "')", 200);
	}else{
		//add#ymy#20161121#start#当是HK详情页面时，隐藏底部bar
		if("false" == "true" && flag == 0){
			$("#cartForm").append('<input type="hidden" id="aimnf" name="aimnf" value="" />');
		}
		execFlag(flag);
	}
}
function execFlag(flag){
	if(flag == "0"){
		addCart(0);
	}
	if(flag == "1"){
		addCart(1);
	}
	if(flag == "2"){
		addBooking();
	}
	if(flag == "3"){
		historyGo();
	}
}
