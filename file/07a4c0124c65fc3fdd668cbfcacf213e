$(document).ready(function(){
	var obj = $("#pageFlag").val();
	$("#title"+obj).css("color","#ffffff");
	$("#outdiv"+obj).css("background-color","#5AC9FE");
	$("#outdiv"+obj).removeClass("tar_tab");
	$("#outdiv"+obj).addClass("cur_tab");
});
function mouseOver(target){
	$(".tar_tab #move_img"+target).animate({ top: '+=8px' }, 100);
}
function mouserOut(target){
	$(".tar_tab #move_img"+target).animate({ top: '-=8px' }, 100);
}
</script>
