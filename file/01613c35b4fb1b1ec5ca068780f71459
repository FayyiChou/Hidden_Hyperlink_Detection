<script>
    // 返回顶部
    $('.backtop').click(function(){
        $('body,html').animate({scrollTop:0},500);
        return false;
    });
    $('#farmerform').submit(function(){
        var province=$('#farmerform').find("[name='Province'] option:selected");
        var city=$('#farmerform').find("[name='City'] option:selected");
        var area=$('#farmerform').find("[name='Area'] option:selected");
        var address = '';
        if(province.text()) { address = province.text().replace('请选择','') + ','; }
        if(city.text()) { address += city.text().replace('请选择','') + ','; }
        if(area.text()) { address += area.text().replace('请选择','') + ','; }

        $('#address').val(address + $('#address').val());
        return ture;
    });
    $('#customerform').submit(function(){
        var province=$('#customerform').find("[name='Province'] option:selected");
        var city=$('#customerform').find("[name='City'] option:selected");
        var area=$('#customerform').find("[name='Area'] option:selected");
        var address = '';
        if(province.text()) { address = province.text().replace('请选择','') + ','; }
        if(city.text()) { address += city.text().replace('请选择','') + ','; }
        if(area.text()) { address += area.text().replace('请选择','') + ','; }

        $('#cus_address').val(address + $('#cus_address').val());
        return ture;
    });
    $('#jobform').submit(function(){
        var province=$('#jobform').find("[name='Province'] option:selected");
        var city=$('#jobform').find("[name='City'] option:selected");
        var area=$('#jobform').find("[name='Area'] option:selected");
