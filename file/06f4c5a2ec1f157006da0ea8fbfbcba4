bool_succed = false;
$(document).ready(function(){
	$.ajax({
            url : 'http://www.ekwing.com/iframe/commtop',
		    type : 'POST',
		    timeout: 5000,
		    dataType : 'jsonp',
            async:false, 
			jsonp:'callback',
		    success:function(data){
                if (data.status != 0){
                    return;
                }
                data = data.data
                /*
                for(var index=0; index < data.css.length; index++){
                    loadHeadFile(data.css[index], "css");
                }
                */
                for(var index=0; index < data.js.length; index++){
                    loadHeadFile(data.js[index], "js");
                }

                $("#header .header_wrap .header_h25").html(data.html);//;prepend(data.html);
		    },
		    error : function(xhr, type){
		    }
        });
});
