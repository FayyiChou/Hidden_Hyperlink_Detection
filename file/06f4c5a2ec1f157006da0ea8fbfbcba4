$(window).ready(function() {
    $(".service_route").css({width: "220","margin-right": '-171px'});
    $(".service_route").mouseenter(function(){
        $(this).stop();
        $(this).animate({width: "220","margin-right": '0px'});
    });
    $(".service_route").mouseleave(function(){
        $(this).stop();
        $(this).animate({width: "220","margin-right": '-171px'});
    });

    //判断是学生做特殊处理
    if ($ && $("#new_header-s").size() > 0) {
        $(".service_route").addClass("service_route_box");
    }

    $(".hang_wrap").show();
});

