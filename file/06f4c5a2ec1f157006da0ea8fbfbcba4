<script type="text/javascript">
var tea_nav_timeid = null;
$(window).ready(function(){
	$(".jsTeaNav").mouseenter(function(){
			clearInterval(tea_nav_timeid);
			$(".jsTeaNavMenu").hide();
			$(this).find(".jsTeaNavMenu").show();
			});

	$(".jsTeaNav").mouseleave(function() {
		tea_nav_timeid = setTimeout(function() {
			$(".jsTeaNavMenu").hide();
		}, 300);
	});

	$(".jsTeaNavMenu").mouseenter(function() {
		clearInterval(tea_nav_timeid);
		$(this).show();
	});
});
</script>
<script>
$(".login_check").live("click", function(){
	if (curuser == 0){
		var objPPRegSDK = new PP_REG_SDK();
		objPPRegSDK.login();
	}
});


$(document).ready(function(){
	getTeacherClass();
	removeUrl();
});

function removeUrl(){
	if (curuser == 0){
		$(".login_check").each(function(){
			this.href = "javascript:;";
		});
	}
}

function getTeacherClass(){
	var bool_succed_class = false;
    var try_num = 0;
	if (curuser == 0){
		return ;
	}
    while (!bool_succed_class && try_num < 3){
	    $.ajax({
            url : '/doc/getteacherclass',
		    type : 'GET',
		    timeout: 5000,
		    dataType : 'json',
            async:false, 
		    success:function(json){
                if (json.errno != 0){
                    bool_succed_class = false;
                    return;
                }
                bool_succed_class = true;

                data = json.data
                if (data.uid == 0){
					/*
                    var html='<li><a id="login_button" class="icon_mod">请先登录</a></li>';
                    $("#class_list").html(html);
                    $("#homework_list").html(html);
					*/
                    return ;
                }

                if (data.class_list == 0){
					var html = '<li><a href="http://www.ekwing.com/class/regcreateclass" class="icon_mod">创建班级</a></li>';
                    $("#class_list").html(html);
                    $("#homework_list").html(html);
                    $("#zuoye").attr("href","http://www.ekwing.com/class/regcreateclass");
                    return ;
                }
                else{
                    var class_list = data.class_list;
                    var html="";
                    for(var index=0; index < class_list.length; index++){
                        html += '<li><a href="http://www.ekwing.com/class/classManage?cid='+class_list[index]['id']+'" class="icon_mod">'+class_list[index]['name']+'</a></li>';

                    }
                    $("#class_list").html(html);
                }
		    },
		    error : function(xhr, type){
		    }
        });
        try_num++;
    }
}
