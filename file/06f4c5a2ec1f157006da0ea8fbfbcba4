var xiaoneng_uid = "";
var xiaoneng_uname = "游客";
var xiaoneng_vip = "0";
var xiaoneng_userlevel = "0";

var objXiaoNeng = JSON.parse(tq_clientname);
if (objXiaoNeng) {
    xiaoneng_uid = objXiaoNeng.uid;
    xiaoneng_uname = objXiaoNeng.uname;
    xiaoneng_vip = objXiaoNeng.is_vip;
    xiaoneng_userlevel = objXiaoNeng.userlevel;
}
var NTKF_PARAM ={
    "siteid":"kf_9431" /*网站siteid*/,
    "settingid":"kf_9431_1477552391720" /*代码ID*/, 
    "uid": xiaoneng_uid/*会员ID*/,
    "uname": xiaoneng_uname/*会员名*/,
    "isvip": xiaoneng_vip,    
    "userlevel": xiaoneng_userlevel/*会员等级*/
}
