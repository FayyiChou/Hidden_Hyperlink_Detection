			<script type="text/javascript">
				function fnHomeNav(){
					var oNav = document.getElementById('hnav_cont');
					var aNavLi = oNav.getElementsByTagName('li');
					var aNavDiv = oNav.getElementsByTagName('div');
					var i=0;
					for(i=0;i<aNavLi.length;i++){
						aNavLi[i].index=i;
						aNavLi[i].onmouseover=function (){
						for(var j=0;j<aNavLi.length;j++){
							aNavDiv[j].style.display="none";
							}
							aNavDiv[this.index].style.display="block"; 
						}
						aNavLi[i].onmouseout=function (){
						for(var j=0;j<aNavLi.length;j++){
							aNavDiv[j].style.display="none";
							}
							//aNavDiv[this.index].style.display="block"; 
						}
					}
				}fnHomeNav();
			</script>
			
