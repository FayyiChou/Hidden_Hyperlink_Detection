	<script type="text/javascript">

	function SubmitKeyClick(obj, evt) {
	        evt = (evt) ? evt : ((window.event) ? window.event : "")
	        keyCode = evt.keyCode ? evt.keyCode : (evt.which ? evt.which : evt.charCode);
	        if (keyCode == 13) {
	           my_search_infor();
	           //alert("aa");
	        }
	    }
	
	
    function my_search_infor() {


        var target_search = document.getElementById("hnavrt_tx");

        if (target_search != null) {

            if (target_search.value != '' && target_search.value != '请输入您想搜索的内容') {
               // window.location.href = 'http://www.bhgmag.com.cn/index.php?m=search&c=index&a=init&typeid=3&siteid=1&q=' + escape(target_search.value);
            	form1.submit();
            	
            }
            else {
                alert("请输入您想搜索的内容");
            }
        }
    }


</script>
	
