    function h_botpic() {
        var oUl = document.getElementById('h_botpic_cont');
        var aLi = oUl.getElementsByTagName('li');
        var aDiv = oUl.getElementsByTagName('div');
        var i = 0;

        for (i = 0; i < aLi.length; i++) {
            aLi[i].index = i;
            aLi[i].onmouseover = function() { aDiv[this.index].style.display = 'block'; };
            aLi[i].onmouseout = function() { aDiv[this.index].style.display = 'none'; };
        }
    } h_botpic();
