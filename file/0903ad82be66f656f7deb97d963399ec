                        function scheck(){
                            var key=document.sform.key.value;
                            if(key == ""){
                                alert("请输入关键字");
                                document.sform.key.focus();
                                return false;
                            }
                        }
                    </script>
                    <form class="form" name="sform" method="post" action="/index.php?c=default&a=infoX&s=1" onsubmit="return scheck();">
