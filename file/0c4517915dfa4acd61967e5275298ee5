       function initexpire()
       {
       	var defaultexpireInfo = '非常抱歉，您访问的页面已经失效，请点击返回登录！';
       	if(expireInfo != null && expireInfo != '')
       	{
       		$("#expireInfo").html(expireInfo);
       	}else
       	{
       		$("#expireInfo").html(defaultexpireInfo);
       	}
       }
       
    </script>
  </head>
  
