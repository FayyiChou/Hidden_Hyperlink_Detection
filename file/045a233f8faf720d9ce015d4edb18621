<div class="loginDialog2Back login_dialog"></div>

<script type="text/javascript">

    var fn_after_loginSuccess = null;

    function loginResponse(resp, statusText, xhr, form) {
        hideStatus();
        var respUrl = '';
        if (xhr.responseXML != undefined) {
            respUrl = xhr.responseXML.URL;
        }
        var result = eval('(' + resp + ')');
        if (result.Success == 1) {
            closeLogin();
            if (fn_after_loginSuccess != null) { fn_after_loginSuccess(); }
            else {
                alert("登录成功！");
                window.location.href = result.AddtionalData2;
            }
        } else {
            if (result.AddtionalData == "true") {
                $('.login_vcode').show();
                $('#formLogin input[name="VCode"]').addClass("required");
                cimg();
            }
            alert(result.Message);
        }
    }

    function closeLogin()
    {
        $('.login_dialog').hide();
    }

    function OpenLoginDialog(logonType,fn)
    {
        $('#logonType').val(logonType);
        fn_after_loginSuccess = fn;
        $('.login_dialog').show();
        var top = (document.documentElement.clientHeight - $('.loginDialog2').outerHeight()) / 2;
        $('.loginDialog2').css("top", top + "px");
    }

    jQuery(function () {
        $('#formLogin').validate({
            messages: {
                UserName: {
                    required: "请输入用户名，或是手机号，或是电子邮箱",
                    minlength: "登录名不得少于2个字"
                },
                Password: {
                    required: "亲，忘了输入密码啦"
                }
            }
        });

        var formLoginOpts = {
            type: 'post',
            url: '/u/login2.html',
            statusCode: errorHandler,
            success: loginResponse
        };

        $("#formLogin").ajaxForm(formLoginOpts);

        $('#btnLogin').click(function () {
            if ($('#formLogin').valid()) $('#formLogin').submit();
        });
    });
</script>

