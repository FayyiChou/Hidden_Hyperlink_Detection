<script type="text/javascript">

    function confirmJobApply(id,jobname,total)
    {
        var applyJobTitle = "您正在申请<em>" + jobname + "</em>职位。";
        if (total > 0)
            applyJobTitle = "您正在申请<em>" + jobname + "</em>等<em>" + total + "</em>个职位";
        $('#applyJobTitle').html(applyJobTitle);
        $('#formApplyJob2 input[name="selJobId"]').val(id);
        $('.winApplyJob').show();
        var top = (document.documentElement.clientHeight - $('.applyJobDialog2').outerHeight()) / 2;
        $('.applyJobDialog2').css("top", top + "px");
    }

    function closeJobApply() { $('.winApplyJob').hide(); }

</script>
