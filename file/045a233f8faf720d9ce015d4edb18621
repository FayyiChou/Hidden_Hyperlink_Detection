<script type="text/javascript">

    var fn_commondialog_callback;

    function showInfoMessage(message,callback)
    {
        fn_commondialog_callback = callback;
        $('#commondialog .info').html(message);
        $('#commondialog .btnconfirm').hide();
        $('#commondialog .btninfo').show();
        $('#commondialogback').show();
        $('#commondialog').show();
    }

    function closeDialog()
    {
        $('#commondialog').hide();
        $('#commondialogback').hide();
    }

    function showConfirm(message,callback)
    {
        fn_commondialog_callback = callback;
        $('#commondialog .info').html(message);
        $('#commondialog .btnconfirm').show();
        $('#commondialog .btninfo').hide();
        $('#commondialogback').show();
        $('#commondialog').show();
    }

    $(function () {
        $('#commondialog .cancel').click(function () {
            closeDialog();
            if (fn_commondialog_callback != null) {
                fn_commondialog_callback(false);
            }
        });
        $('#commondialog .ok').click(function () {
            closeDialog();
            if (fn_commondialog_callback != null) {
                fn_commondialog_callback(true);
            }
        });
        $('#commondialog .btninfo a').click(function () {
            closeDialog();
            if (fn_commondialog_callback != null) {
                fn_commondialog_callback();
            }
        });
    });
    
</script>    <div id="formStatusBack" style="position: absolute; z-index: 99998; top: 0px; left: 0px; width: 100%; height: 1248px; background: rgba(255,255,255,0.5); background: #fff\9; filter:alpha(opacity=50);opacity:0.8;display: none; "></div>
