<script type="text/javascript">
    function scroll_news(){
            var firstNode = $('#box div');
            firstNode.eq(0).fadeOut('slow', function ()
            {
                $(this).clone().appendTo($(this).parent()).show('slow');
                $(this).remove();
            });
     }
    setInterval('scroll_news()', 5000);



String.prototype.Trim = function(){ 
  return this.replace(/(^\s*)|(\s*$)/g,"");
}
String.prototype.TrimTel = function() {
  var m = this.match(/^\s*(\S+(\s+\S+)*)\s*/);
  return (m == null) ? "" : m[1];
}

String.prototype.isMobile = function(){
  return (/^(?:13\d|15[0123456789])-?\d{5}(\d{3}|\*{3})/.test(this.TrimTel()));
}

String.prototype.isTel = function(){
return (/^(([0\+]\d{2,3}-)?(0\d{2,3})-)(\d{7,8})(-(\d{3,}))?/.test(this.TrimTel()));
}

function validate(){
   if (document.frmmsg.tfname.value.Trim().length < 2) {
    alert("请输入你的真实姓名，以便我们与你联系！");
    document.frmmsg.tfname.focus();
    return false;
   }
   if (document.frmmsg.tfphone.value.Trim().length == 0) {
    alert("请填写联系电话");
    document.frmmsg.tfphone.focus();
    return false;
   }else{
      if (!document.frmmsg.tfphone.value.Trim().isMobile() && ! document.frmmsg.tfphone.value.Trim().isTel()) {
         alert("请输入正确的手机号码或电话号码\n\n例如:13888888888或021-8888888");
		 document.frmmsg.tfphone.value="";
         document.frmmsg.tfphone.focus();
         return false;
      }
   }
    if (document.frmmsg.tfemail.value.Trim().length > 0) {
    if(document.frmmsg.tfemail.value.search("^\\w+([-+.]\\w+)*@\\w+([-.]\\w+)*\\.\\w+([-.]\\w+)*$")!=0){
     alert("请输入正确的邮件格式");
	 document.frmmsg.tfemail.value="";
     document.frmmsg.tfemail.select();
     return false;}
   }
   if (document.frmmsg.tacontent.value.Trim().length < 25) {
    alert("请输入有效留言内容，至少25个字符！");
    document.frmmsg.tacontent.focus();
    return false;
   }
   return true;
}
</script>

