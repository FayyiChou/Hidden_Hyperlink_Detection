<a href="./message.php" class="leftfloor_red" target="_blank" style="outline: none;"> <i class="left_iconfont foot_ico_msg"></i> <em class="two_line">用户反馈</em> </a> <a href="javaScript:void(0);" class="leftfloor_red toTop"> <i class="left_iconfont foot_ico_top"></i> <em class="two_line">返回顶部</em> </a> 
</div>
<script>
$(function(){
	(function(){
		$(".floor_left_box a:not(.leftfloor_red)").click(function(){
			$(".floor_left_box a").removeClass("cur");
			$(this).addClass("cur");
			var index=$(".floor_left_box a").index(this);
			var top=$(".layout_main .mod_floor_title").eq(index).offset().top;
			$("body,html").stop().animate({scrollTop:top});
		});
	
		$(".toTop").click(function(){
			$('body,html').animate({scrollTop:0});
		});
	
		var arr=[];
		$(".layout_main .mod_floor_title").each(function(i){
			arr[i]=$(this).offset().top;
		});
	
		$(window).scroll(function(){
			if ($(window).scrollTop() > 300){
				$(".floor_left_box").fadeIn(500);
			}
			else
			{
				$(".floor_left_box").fadeOut(500);
			}
			
			var top=$(document).scrollTop();
			for(var i=0;i<arr.length;i++){
				if(top>=arr[i] && top<arr[i]+100){
					$(".floor_left_box a").removeClass("cur");
					$(".floor_left_box a").eq(i).addClass("cur");
				}
			}
		});
	
	})();
})
</script>
