<script type="text/javascript">
function deleteCartGoods(rec_id)
{
	Ajax.call('delete_cart_goods.php', 'id='+rec_id, deleteCartGoodsResponse, 'POST', 'JSON');
}

/**
 * 接收返回的信息
 */
function deleteCartGoodsResponse(res)
{

  if (res.error)
  {
	alert(res.err_msg);
  }
  else
  {
	document.getElementById("ECS_CARTINFO").innerHTML = res.content;
	$(".hd_cart_list li").mouseenter(function(){
		if($(".hd_cart_list li").index($(this)) != 0 )
		{
			$(this).addClass("hd_cart_cur");
		}
		else
		{
			$(this).addClass("hd_cart_first");
		}
		
	})
	$(".hd_cart_list li").mouseleave(function(){
		if($(".hd_cart_list li").index($(this)) != 0 )
		{
			$(this).removeClass("hd_cart_cur");
		}
		else
		{
			$(this).removeClass("hd_cart_first");
		}
	})
  }
}

/********迷你购物车加减********/
function modifyBuyNumTop(d, a , r) {
	var b;
    var c;
   if (a == -1) {
        c = $(d).parents(".hd_num_box").find("input");
        b = parseInt(c.val()) || 1;
        if (b <= 1) {
			alert("购买数量不能小于1件");
            return
        } else { 
            c.val(b + a)
        }
    } else {
        c = $(d).parents(".hd_num_box").find("input");
        b = parseInt(c.val()) || 1;
      	c.val(b + a)
    }
	change_goods_number(r,b + a);		
}

function onClickCartNum(a,b)
{
	
	var b = parseInt(b);
	var c = $("#minicart_num_"+a);
	var d = parseInt(c.val());
	if(d < 1 || !$.isNumeric(d))
	{
		alert("请输入正确的商品数量");	
		e = 1;
	}
	
	if(b == -1)		
	{
		if(d == 1)
		{
			alert("购买数量不能小于1件");	
		}
		else
		{
			e = d + b;
		}
	}
	else
	{
		e = d + b;
	}

	change_goods_number(a,e);
}

function keyUpCartNum(a,b)
{
	var c = parseInt($(a).val());
	if(c < 1 || !$.isNumeric(c))
	{
		alert("请输入正确的商品数量");	
		d = $(a).val(1);
	}

	change_goods_number(b,d);

}


function change_goods_number(rec_id, goods_number)
{     
	Ajax.call('flow.php?step=ajax_update_cart', 'rec_id=' + rec_id +'&goods_number=' + goods_number, change_goods_number_response, 'POST','JSON');
}
function change_goods_number_response(result)
{               
	if (result.error == 0)
	{
		var rec_id = result.rec_id;
		$('#minicart_num_' +rec_id).val(result.goods_number);//更新数量		
		$('.hd_total_price strong em').html(result.total_price); //更新合计
		$('.hd_total_pro em').html(result.total_goods_count);//更新购物车数量
		$("#in_cart_num").html("<u>"+result.goods_number+"</u>")
	}
	else if (result.message != '')
	{
		alert(result.message);                
	}
}
	


</script> 
