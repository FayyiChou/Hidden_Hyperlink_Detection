function vaildNum(str, param){
    var _param = param.split(",");
	var _pstr = str.split(",");
    for (var i = 0; i < _param.length; i++)
		for (var j=0; j<_pstr.length; j++){
			if(_pstr[j] == _param[i]){
				return true;
				}
		};
    return false;
}

function getADCookie(name) 
{ 
    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)"); 
    if(arr=document.cookie.match(reg))
        return unescape(arr[2]); 
    else 
        return null; 
} 

