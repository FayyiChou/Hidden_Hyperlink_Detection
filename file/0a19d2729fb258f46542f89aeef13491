<script language="javascript">

function swap_tab5(n){

	for(var i=1;i<=5;i++){

		var curC=document.getElementById("neirq3"+i);

		var curB=document.getElementById("ping3"+i);

		if(n==i){

			curC.style.display="block";

			curB.className="lz1"

		}

		else{

			curC.style.display="none";

			curB.className="lz2";

	    }

	}

}

</script>

<script language="javascript">
function js(form)
{form.action = "http://www.jsgs.gov.cn/jsearch/index.jsp?";
form.style.value = "1";
form.appid.value = "1";
form.q.value = form.query.value;
}

function hello()
{
	 var query = document.userForm.query.value;
	 if(query=="")
