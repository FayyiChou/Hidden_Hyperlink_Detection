        <script>
$(function() {
	var movies = $('#movie h3');
	if (movies.length > 0) {
		$(movies[0]).show();
	}

  $('#movie .movie_list_item').click(function() {
    movies.hide();
    $('#' + $(this).attr('_for')).show();
  });
});
</script>
                  
