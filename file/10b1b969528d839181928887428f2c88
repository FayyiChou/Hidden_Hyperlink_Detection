        function checkcanorder(color, size) {
            var guigeCount= $('.xuanze').length;//大于2，就说明有多个规格选项
            var key1 = "";
            var key2 = "";
            if (guigeCount>2) {
                key1 = color + ',' + size;
                key2 = size + ',' + color;
            }else {
                //只有单个规格
                key1 = color;
                key2 = size;

            }
            return (Spec.hasOwnProperty(key1) && !!Spec[key1].Status) || (Spec.hasOwnProperty(key2) && !!Spec[key2].Status);
        }
        var pattern = /([\^\$\.\*\+\-\?\=\!\:\|\\\/\(\)\[\]\{\}\<\>])/ig;
        var specFiter = function(sel) {
            sel = sel.replace(pattern, "\\$1");
            var a = [];
            var reg = new RegExp("^" + sel + ",|," + sel + "$");
            for (var key in Spec) {
                if (Spec.hasOwnProperty(key) && reg.test(key)) {
                    var size = key.replace(reg, "");
                    !!Spec[key].Status&& a.push(size);
                    // console.log(size, Spec[key].Status);
                }
            }
            return a;
        }

        var specFiter_One = function(sel) {
            // 只有单型号或者单颜色
            sel = sel.replace(pattern, "\\$1");
            var a = true;
            var reg = new RegExp("^" + sel + "|" + sel + "$");
            for (var key in Spec) {
                if (Spec.hasOwnProperty(key) && reg.test(key)) { 
                    
                    a=!!Spec[key].Status; 
                    // console.log(size, Spec[key].Status);
                }
            }
            return a;
        }
        //   console.log(Spec);
        function viewpiace(color, size) {
            var key="";
            var key1="";
            var guigeCount= $('.xuanze').length;//大于2，就说明有多个规格选项
            if (guigeCount>2) {
                key = color + ',' + size;
                key1 = size + ',' + color;
            }else {
                //只有单个规格
                key = color ;
                key1 = size ;
            }



            if (Spec.hasOwnProperty(key)) {
                var obj = Spec[key];
                $("#op_price").text("￥" + (obj.MemberPrice*1).toFixed(2));
                sel_obj.oneprice = Number(obj.MemberPrice);
                $("#to_price").text((sel_obj.oneprice * sel_obj.count).toFixed(2));
            } else if (Spec.hasOwnProperty(key1)) {
                var obj1 = Spec[key1];
                $("#op_price").text("￥" + (obj1.MemberPrice*1).toFixed(2));
                sel_obj.oneprice = Number(obj1.MemberPrice);
                $("#to_price").text((sel_obj.oneprice * sel_obj.count).toFixed(2));
            }
        }

