<script>
	var speed=25; //数字越大速度越慢
	var demo10=document.getElementById("demo10");
	var demo11=document.getElementById("demo11");
	var demo12=document.getElementById("demo12");
	function more(){
	demo12.innerHTML+=demo11.innerHTML;
}
demo12.innerHTML=demo11.innerHTML;
	function Marquee1(){
		var demo10=document.getElementById("demo10");
	var demo11=document.getElementById("demo11");
	var demo12=document.getElementById("demo12");
			 a=demo12.offsetWidth+demo11.offsetWidth-demo10.offsetWidth;
		if(demo10.scrollLeft==a)
			demo12.innerHTML+=demo11.innerHTML;
			
		else{
			demo10.scrollLeft++;
		}
}
function morer(){
var demo11=document.getElementById("demo11");
	var demo12=document.getElementById("demo12");
	demo12.innerHTML=demo11.innerHTML;
}

	var MyMar1=setInterval(Marquee1,speed);//鼠标点上去才播放
	//var MyMar2=setInterval(morer,speed);
	demo10.onmouseover=function() {clearInterval(MyMar1)};
	demo10.onmouseout=function() {MyMar1=setInterval(Marquee1,speed)};//鼠标放上去就停止
</script>