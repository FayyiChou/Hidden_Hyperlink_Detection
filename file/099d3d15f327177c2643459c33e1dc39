<script type="text/javascript">
function Show_AAATab(AAATabid_num,AAATabnum){
	for(var i=0;i<4;i++){document.getElementById("AAATabcontent_"+AAATabid_num+i).style.display="none";}
	for(var i=0;i<4;i++){document.getElementById("AAATabmenu_"+AAATabid_num+i).className="AAATabOff";}
	document.getElementById("AAATabmenu_"+AAATabid_num+AAATabnum).className="AAATab";
	document.getElementById("AAATabcontent_"+AAATabid_num+AAATabnum).style.display="block";
};
</script>



<script>
 $(function(){
      // -- 容器不定 改
      var $news_main = $('.news_main');
      var $TRS_Editor = $( '.TRS_Editor' );

      //ad 改为需要的 广告
      var $ad = $('<div class="fudong" style="float:left;margin: 20px 20px 20px 0;"> </div>');

      // 判断的字数
      var fontNum = 36 * 3, isInsert = false;

      // 编辑 or 抓取
      $TRS_Editor.length ? capture($TRS_Editor) : capture($news_main);

      function capture($container) {
        var textTotal = '';
        var fontTotal = $.trim( $container.text() );

        if ( !$container.find( 'img').length ) {
          if ( fontTotal.length < fontNum ) {
            // 如果总字数木有达到要求, 且无图, 那么在最后添加
            $container.append( $ad );
            return isInsert = true;
          }
          // 无图时, 加在倒数第二段位置
          var $pTwo = $container.find( 'p').eq(-2);
          if ( $pTwo.length ) {
            $ad.insertBefore($pTwo);
            return isInsert = true;
          }
        }

        // 对子节点进行判断
        iterators( $container.children() );

        // 判断 图片位置及文字数量
        //  true 则退出该函数,广告已确定添加or不添加 false 继续寻找
        function judge( $el, index ) {
          if ( $.trim( $el.text()).length + textTotal.length < fontNum  && !$el.find( 'img').length && !$el.is( 'img' )  ) {
            textTotal += $el.text();
            return false;
          }

          if( $el.is( 'img' ) || $el.find( 'img').length ) {
            if ( !textTotal ) {
              // 图片是最深最后的子节点
              return true;
            }
            if ( $el.next().length ) {
              $ad.insertBefore($el.next());
            }
            else {
              $ad.insertBefore( $el.parent().next() );
            }
            return isInsert = true;
          }
          else {
            textTotal += $.trim( $el.text());
            if( textTotal.length < fontNum ) {
              return false;
            }
            $ad.insertBefore($el);
            return isInsert = true; // stop
          }
        }

        // 循环 判断n层节点
        // 这里不断循环每个节点的最后一个节点, 那么问题是: 这种循环调用是否可以改成尾调用???
        //  true 则退出该函数,广告位置已确定 false 继续寻找
        function iterators ( $childs ) {
          for ( var len = $childs.length, k = len -1; k >= 0; k --  ) {
            var $child1 = $childs.eq(k);

            if ( $child1.is( 'p' ) ) {
              var res = judge( $child1);
              if( res ) {
                return true;
              }
              continue;
            }

            // 子节点的子节点
            var $child2 = $child1.children();
            if ( !$child2.length ) {
              var res2 = judge( $child1, k);
              if( res2 ) {
                return true;
              }
              $child2 = null;
              continue;
            }
            else {
              var ans = iterators($child2);
              if( ans ) {
                return true;
              }
            }
            $child2 = null;
          }
          return false;
        }
      }

      if ( isInsert ) {
        // 插入广告内
        $('#bd2850752 script').remove();
        $('.fudong').append($('#bd2850752'));
        $('#bd2850752').show();
      }
    })
</script>
