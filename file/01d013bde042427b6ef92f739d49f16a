(function(){
	new setpLdate('#ldYear','#ldMonth','#ldDay');
	new setpSdate('#sdYear','#sdMonth','#sdDay');
	var A=$(".query_select_box li.select");
	A.each(function(i){
		$(this).mousedown(function(){
			$(this).parent().parent().find(".ldType").val(i)
			A.addClass('sel_die');$(this).removeClass('sel_die')
		})
	})
})();
