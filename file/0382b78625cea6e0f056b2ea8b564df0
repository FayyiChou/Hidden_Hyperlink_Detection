<script type="text/javascript">
var content = $('#arc_container').html();
var look = $('#look').html();
function getarticle()
{
    $('#look').attr('class', 'full_text Loading');
    $('#look').html('页面加载中...');
    var content = $('#arc_container').html();
    content3 = content.match(/【.*】/);
    var url = 'http://china.findlaw.cn/zhishi/index.php?m=Fphpcms&a=getArticle&requestmode=async' ;
    $.post(url, {id:1064312},function(result){
        $('#fenye').css('display', 'none');
        result1 = result.replace(/(((<.*?>)?(\s|(　)|\&nbsp;)*)+)/i, '$1' + content3);
        $('#arc_container').html(result1);
        $('#look').attr('class', 'full_text Return');
        var back = '<a href="javascript:;" onclick="javascript:backpage();">返回分页阅读文章</a>';
        $('#look').html(back);
        location.hash = 'p';
    })
    .error(function() {
        $('#look').html('提取内容失败,请刷新后重新提取...');
    });
}

function backpage()
{
    $('#look').attr('class', 'full_text Loading');
    $('#look').html('页面加载中...');
    $('#arc_container').html(content);
    $('#look').attr('class', 'full_text');
    $('#look').html(look);
    $('#fenye').css('display', '');
    location.hash = 'p';
}
</script>
