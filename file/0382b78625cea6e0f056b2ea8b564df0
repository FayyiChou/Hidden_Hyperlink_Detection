function zhisou(url,m,a,key_inputid, emptyString){
	var emptyString = emptyString==undefined ? '请在此输入您的问题' : emptyString;
	var q = $('#'+key_inputid).val();
	if(q == emptyString){
		q = '';
		var frm = $('<form target="_blank" method="post" action="http://s.findlaw.cn" ></form>');
	}else{
		var inputstr = '<input type="hidden" name="m" value="'+m+'" />';
		inputstr +='<input type="hidden" name="a" value="'+a+'" />';
		inputstr +='<input type="hidden" name="zd" value="1" />';
		inputstr +='<input type="hidden" name="q" value="'+q+'" />';
		var frm = $('<form target="_blank" method="get" action="'+url+'" >'+inputstr+'</form>');
	}
	$('body').append(frm);
	frm.submit();
}

