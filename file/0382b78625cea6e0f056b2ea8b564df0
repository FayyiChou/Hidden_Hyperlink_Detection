$(function(){
		var obj = $('#pinbdao_lu');
		var top  = obj.offset().top;
		window.onscroll = window.onresize = function(){
			var scrollT = Math.max(window.document.documentElement.scrollTop, window.document.body.scrollTop);
			if(scrollT >= top){
				obj.addClass('channelMove');

			}else{
				obj.removeClass('channelMove');
			}

		}
});
$('#right_pindao_lu i a').each(function(){
	cutTextByte( $(this), 12, '' );		//截取字符
});
$(function(){
	$('#askfdk_xiao_totop').click(function(){
		$('#pinbdao_lu').removeClass('channelMove');
	})
});

