          <script>
					function showTab(tab){
						var array = ['jiaoyu','keji','yiyao','jingji','jisuanji','sheke','jianzhu','zonghe'];
						if(tab!=null)
						{
							var selectdiv = document.getElementById("tab_"+tab);
							var selecta = document.getElementById(tab);
							if(selectdiv)
							{
								for (var i=0;i<array.length ;i++ )
								{
									var div = document.getElementById("tab_"+array[i]);
									var a = document.getElementById(array[i]);
									a.className = "";
									if(div)
									{
										div.style.display="none";
									}
								}
								selectdiv.style.display = "block";
								selecta.className="cur";
							}
						}
					}
					function showMore(){
						var array = ['jiaoyu','keji','yiyao','jingji','jisuanji','sheke','jianzhu','zonghe'];
						for (var i=0;i<array.length ;i++ )
						{
							var a = document.getElementById(array[i]);
							if(a.className=="cur")
							{
								window.location.href="/a/fuwu/shougao/"+a.id;
								return;
							}
						}
					}
					</script>
