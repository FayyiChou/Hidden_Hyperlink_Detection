						$(".jsban li").hover(function(){t=clearInterval(t)},function(){t=setInterval(autoshow,10000)})
						var t=setInterval(autoshow,10000);
						function autoshow(){
							n=n>=(cont-1)?0:++n;
							$(".jsbannav span").eq(n).trigger("click");
						}
                    });
