                        var n=0;
						var cont=$(".jsban ul li").length;   
						for(i=0;i<cont;i++){
								$(".jsbannav").append("<span><\/span>");
							}
						$(".jsbannav span:eq(0)").addClass("cur");
						$(".jsban li:not(:first-child)").hide();
						$(".jsbannav span").click(function(){
							 n=$(this).index();
							$(this).addClass("cur").siblings(".jsbannav span").removeClass("cur");
							$(".jsban li").eq($(this).index()).fadeIn(1000).siblings(".jsban li").fadeOut(500);
						   
