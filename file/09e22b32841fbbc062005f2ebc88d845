<script>
function yooh5AdUrlAction(url) {
    var appVersion = window.navigator.appVersion;
    var isIos = appVersion.indexOf("iPhone") > -1 || appVersion.indexOf("Mac") > -1;
    if(isIos && url.toLowerCase().indexOf('apk') > -1) {
        alert('暂时不支持IOS !')
    } else {
        location.href = url;
    }
}
</script>

