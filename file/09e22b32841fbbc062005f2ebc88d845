<script type="text/javascript">
$(document).ready(function() {

    //百度统计
    var _hmt = _hmt || [];
    (function() {
      var hm = document.createElement("script");
      hm.src = "//hm.baidu.com/hm.js?6ba12b646b7deef3f24a9ab0938c2a86";
      var s = document.getElementsByTagName("script")[0];
      s.parentNode.insertBefore(hm, s);
    })();

    //游戏中心统计 页面级
    var gcsa = { "appId":1001, "chanId":chanId };
        if(getCookie('EgretApp')) {
        var egretApp = getCookie('EgretApp');
        var egretIcon = getCookie('EgretIcon');
        gcsa = { "appId":1001,"chanId":chanId,'EgretApp':egretApp,'EgretIcon':egretIcon };
        //alert(JSON.stringify(gcsa));  
    }
    GameCenterSA.init(gcsa);
    GameCenterSA.login();

    //页面加载结束统计
    PageLoadingSA.sendData( { "act":"index_index", "tag":"end" } );

    //统计页面PV来源
    var statPV = "INDEX";
    var getStatPV = egret_gc_getQueryString('statPV');
    if(typeof(getStatPV) != 'undefined' && getStatPV != null) {
        statPV = statPV + '_' + getStatPV;
    }
    statPvFrom(statPV);
    
})
</script>

