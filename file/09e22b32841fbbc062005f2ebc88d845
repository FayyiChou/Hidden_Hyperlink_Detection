<script>
$(document).ready(function() {

    $("img.lazy").lazyload({
         effect : "fadeIn",
         threshold : 180
    });

    //按钮点击统计
    $('.GCSTAT').click(function() {
        var act = $(this).data('act');
        if(typeof act != 'undefined') {
            act = '360-'+act;
            GameCenterSA.action( { "act": act } );
        }
        //return false;
    })

    resizePic('.min-pic img');

    // 调整页面高度
    var screenHeight1=$('window').height();
    (function(){
        var bodyHeight = $(document).outerHeight(true);
        if($('.m-nav').css('display')){
            if (screenHeight1 < bodyHeight) {
                $(".wrapper").append('<div style="height: 5.5rem;"></div>');
            }
        }
    })();

    // 记录A标签点击顺序 记录用户轨迹到redis
    //$("a[href^='/Player']").off().on('click', function(){
    $("body").on('click', "a[href^='/Player']", function(){
        // 统计
        var act = $(this).data('act');
        if(typeof act != 'undefined') {
            GameCenterSA.action( { "act": act } );
        }
        function getUrlRelativePath() {
            var url = document.location.toString();
            var arrUrl = url.split("//");
            var start = arrUrl[1].indexOf("/");
            var relUrl = arrUrl[1].substring(start);//stop省略，截取从start开始到结尾的所有字符 
            return relUrl;
        }
        var url = getUrlRelativePath();

        //圈子定位动态id
        if(url.indexOf('/Group?') > -1) {
            url = url.replace(/&pageId=\d+/, '').replace(/&dtid=\d+/, '');
            if(typeof pageId != 'undefined' && pageId > 1) {
                url += '&pageId='+(parseInt(pageId));
            }
            var dtid = $(this).attr('dtid');
            if(typeof dtid != 'undefined') {
                url += '&dtid='+dtid;
            }
        }
        
        $.getJSON("/Api/backUrl.set?chanId="+chanId+'&url='+encodeURIComponent(url), function(e){
        });
        return true;
    });

    // 返回
    $("a[data-goback='goback']").click(function(e){
        e.preventDefault(); e.stopPropagation();
        $.getJSON("/Api/backUrl.get?chanId="+chanId, function(e){
            var url = "/Group?chanId="+chanId;
            if(e.data.length > 10) {
                url = decodeURIComponent(e.data);
            }
            window.location.href = url;
        });
        return false;
    });
});

//在线时长统计
document.body.addEventListener(
   'touchmove',
   function(event) {
    OnlineSA.setHeartbeat();
   },
   true
);

document.body.addEventListener(
   'touchstart',
   function(event) {
    OnlineSA.setHeartbeat();
   },
   true
);

document.body.addEventListener(
   'touchend',
   function(event) {
    OnlineSA.setHeartbeat();
   },
   true
);

</script>

