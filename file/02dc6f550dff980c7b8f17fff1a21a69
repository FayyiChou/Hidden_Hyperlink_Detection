<script language="javascript">
var myWindow = window.open('https://www.icris.cr.gov.hk/popup_notice.html','網上查冊中心','width=580,height=506');
</script>	
	


<script language="javascript">

hostStringOrig = "http://www.icris.cr.gov.hk:80/csci/index.jsp" ;
hostString = "http://www.icris.cr.gov.hk:80/csci/index.jsp" ;
hostString = hostString.substring(hostString.indexOf(":")+3, hostString.lastIndexOf("/")) ;
httphostString = hostString.substring(0,hostString.indexOf(":"))+ hostString.substring(hostString.indexOf("/"),hostString.length) ;

function loginIguest()
{   
    window.open('https://'+httphostString+'/clearsession.jsp?user_type=iguest','loginIguest','status=1,resizable=yes,scrollbars=yes,left=0,top=0,width=800,height='+screen.height*0.8);
}

function loginSubscriber()
{
    window.open('https://'+httphostString+'/clearsession.jsp?user_type=subscribe','loginSubscriber','status=1,resizable=yes,scrollbars=yes,left=0,top=0,width=800,height='+screen.height*0.8);
}

function ApplyAccount(locale)
{   
    window.open('https://'+httphostString+'/register.do?fromIndexPage=true&localeFromIndexPage='+locale,'ApplyAccount','status=1,resizable=yes,scrollbars=yes,left=0,top=0,width=800,height='+screen.height*0.8);
}


</script> 

