//获取小区成交数据
function getDeals(communityId, page){
    $.ajax({
	type:"GET",
	url:"/exchange/getdeals",
	dataType:"html",
	data:{'communityId':communityId, 'page':page, 'communityname':communityAddress},
	success: function(data){
	    $('#dealinfo').html(data);
	}
    });
}

//获取带看数据
function getLookNotes(communityId, page){
    $.ajax({
	type:"GET",
	url:"/exchange/getLookNotes",
	dataType:"html",
	data:{'houseId':"1062012", 'page':page, 'communityname':communityAddress},
	success: function(data){
	    $('#lookNotesInfo').html(data);
	}
    });
}
$("#shoucang").on('click', function(){
	if(confirm("您要收藏本网站吗？")){
		try {
	       	 	window.external.addFavorite("http://tj.5i5j.com/exchange/"+houseid, "青山湖南钢生活区");//ie的
		}catch (e){
	        	try{
	            		window.sidebar.addPanel("青山湖南钢生活区", "http://tj.5i5j.com/exchange/"+houseid, "");//ff
	       		}catch (e){
	            		alert("加入收藏失败，请使用Ctrl+D进行添加");
	        	}
	    	}
	}
});
$(document).ready(function() {
	$("#auto-loop").lightGallery({
		loop:true,
		auto:false,
		pause:4000
	});
	$('.bigImg').on('click',function(e){
		$("#auto-loop").find("img.lazy").attr("src",function(){return $(this).attr("data-ks-lazyload");});
		var bigImgIndex=$(this).parent('div').children('div.lb-small-pic').find('img').index($('.show'));
		$('#auto-loop li:eq('+bigImgIndex+')').click();
	})
});
