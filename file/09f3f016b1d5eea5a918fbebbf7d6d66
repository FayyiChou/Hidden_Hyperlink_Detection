<script type="text/javascript"><!--
$('#slideshow0').owlCarousel({
	items: 6,
	autoPlay: 5000,
	singleItem: true,
	navigation: true,
	navigationText: ['<i class="fa fa-chevron-left fa-5x"></i>', '<i class="fa fa-chevron-right fa-5x"></i>'],
	pagination: true
});

var slidevideo = new tvp.VideoInfo();
var slidevideoplayer =new tvp.Player();

$('#slideshow0 .item a').click(function(){
  if ($(this).attr('href').indexOf('v.qq.com') > 0) {
    $('#slideshow-video .modal-title').html($(this).children('img').attr('alt'));
    
    var regex = /([^\/]+)(?=\.html)/;
    var videoid = regex.exec($(this).attr('href'));
    slidevideo.setVid(videoid[0]);
    slidevideoplayer.create({
      width:"878",
      height:"494",
      video:slidevideo,
      modId:"slidevideoplayer",
      autoplay:true,
      isVodFlashShowCfg:false,
      isVodFlashShowEnd:false,
      isVodFlashShowSearchBar:false,
