    (function ($) {
      $('.dropdown').mouseover(function(){
        $(this).find('.dropdown-menu').show();
        $(this).addClass('dropdown-active');
      });
      $('.dropdown').mouseout(function(){
        $(this).find('.dropdown-menu').hide();
        $(this).removeClass('dropdown-active');
      });
    })(jQuery);
