$(function(){
	var li = $(".cyf li");
	$.each(li,function(i,item){
		$(item).hover(function() {
			$(item).removeClass("befor");
			$(item).addClass("after");
			var id=$(this).find("div").attr("id");
			$("#"+id).css("display","");
		}, function() {
			$(item).removeClass("after");
			$(item).addClass("befor");
			var id=$(this).find("div").attr("id");
			$("#"+id).css("display","none");
		});
	});
});

