    function onSearch(){
    var input=$("#input").val();
    if(input==""){
        alert('请输入关键词!');
        return;
    }
    document.search.action="gswqZixunWeb-search.whbs";
    document.search.submit();
}
       
       
       
       function lishow(id){
            $.ajax({
			type : "POST",
			url : "gswqZixunWeb-listShow.whbs",
			cache : false,
			dataType : "html",
			data : "zixunId="+id,
			success : function(msg) {
					$("#lishow").html(msg);
			}
		});	
       }
       
       
