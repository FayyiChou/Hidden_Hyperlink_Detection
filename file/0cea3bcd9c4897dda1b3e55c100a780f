								onSearch: function (searchForm) {
									var query = searchForm.query.value ? searchForm.query.value : "";
									var autoSuggestInput = "<input type='hidden' name='src' value='as'/>";
									var searchTags = {
										"ms.interactiontype": msSearchInInteractionValue,
										"ms.searchquery": query,
										"ms.pgarea": msPageAreaValue,
										"ms.searchtype": msSearchTypeDefault
									}

									var $scope = angular.element(searchForm).scope();
									if (!$scope) {
										occe.WedcsSearchEvent(searchTags);
									}

									var activeController = $scope["main"] || $scope["homepageSearch"];
									if (activeController && activeController.getAutoSuggestTags) {
										var autoSuggestTags = activeController.getAutoSuggestTags();
										if (typeof autoSuggestTags !== 'undefined' && autoSuggestTags['ms.searchtype'] !== 'searchbox'){
											$(searchForm).append(autoSuggestInput);
										}
										searchTags = $.extend(searchTags, autoSuggestTags);
									}

									occe.WedcsSearchEvent(searchTags);
								}
							}
						});
				});
			</script>
				


