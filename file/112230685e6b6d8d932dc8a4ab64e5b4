function onUsabillaClick(event){
B[sNSStartup].lightbox.hide();
if (event) {
$(event.target).closest('.user_center_popover').hide()
event.preventDefault();
}
if (!window.usabilla_live) {
preloadUsabilla();
}
window.usabilla_live('data', { custom: { user_id: ""} });
window.usabilla_live('data', { custom: { action: "apps"} });
window.usabilla_live('click');
}
if(window.$) {
$('.provide_website_feedback').click(onUsabillaClick);
}
