            switch(lang){
            case '3':
                arr.splice(2,1);
                break;
            case '2':
                arr.splice(1,1);
                break;
            default:
                arr.splice(0,1);
            }
            $('.langU').html('语言：'+arr.join('/&nbsp;'));
        });
    </script>
    -->
