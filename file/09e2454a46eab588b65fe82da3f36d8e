<script>
    $(function() {
        function getMallSearchUrl(key) {
            sensorsKeywordSearch("首页", key, "否");
            return  "/item/seclist/?key=" + encodeURIComponent(encodeURIComponent(key)).replace(/\'/g,'%27');
        }

        //搜索(enter)
		$('.mall_search').on('keypress',function(event){
			var key = $(this).val();
            if(event.keyCode == "13"){
				if ($.trim(key) === ''){
					return false;
				}
				window.open(getMallSearchUrl(key));
            }
        });
		//搜索(搜索按钮)
		$('.mall_search_btn').on('click',function(){
			var key = $(this).siblings('.mall_search').val();
			if($.trim(key) === ''){
				return false;
			}
			window.open(getMallSearchUrl(key));
		});
        //热门搜索埋点
        $(".authority_search .search_hot").on("click", "a", function () {
            var key = $(this).text();
            sensorsKeywordSearch("首页", key, "是");
        });
    })

</script>


