  <script type="text/javascript">
	jQuery(function($){
		//tab
		$('.tabs_a a.iphone').click(function(){
			$('.tabs_a a.ipad').removeClass('current');
			$(this).addClass('current');
			$('.ipad_list').hide();
			$('.pic_list').show();
		});
		$('.tabs_a a.ipad').click(function(){
			$('.tabs_a a.iphone').removeClass('current');
			$(this).addClass('current');
			$('.pic_list').hide();
			$('.ipad_list').show();
		});
	});
	jQuery(function($){
		$('#flexBtn').bind('click',function(){
			$(this).prev('.readCon').toggleClass('limitHeight');
			if( $(this).prev('.readCon').hasClass('limitHeight') ){
				$(this).html('<i></i>查看更多...')
			}else{
				$(this).html('<i class="up"></i>收起...');
			}			
		});
	});
	</script>  
