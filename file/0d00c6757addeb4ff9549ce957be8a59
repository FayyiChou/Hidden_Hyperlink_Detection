<script>
/*鼠标移过，左右按钮显示*/
$(document).ready(function(e) {
    $(window).resize(function(e) {
		var width=$(this).outerWidth(true);
		$(".top_jdt_ul,.top_jdt_ul li").css("width",width);
    });

$(".top_jdt").hover(function(){
	$(this).find(".prev,.next").fadeTo("show",0.1);
},function(){
	$(this).find(".prev,.next").hide();
})
/*鼠标移过某个按钮 高亮显示*/
$(".prev,.next").hover(function(){
	$(this).fadeTo("show",0.7);
},function(){
	$(this).fadeTo("show",0.1);
})
$(".top_jdt").slide({ titCell:".num ul" , mainCell:".top_jdt_ul" , effect:"fold", autoPlay:true, delayTime:700 , autoPage:true });
});

</script> 
<!-- 代码 结束 --> 

