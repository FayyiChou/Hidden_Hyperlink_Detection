    <div class="clearall reply-bd">
        <div class="cont">
                <div class="fr">
                       <a class="qr-code-img" href="javascript:;"></a><p class="qr-code-tip">下载19楼客户端<br>回帖双倍威望</p>
                        <script type="text/javascript">
                            AM('md5',function() {
                                var str = "http://m.19lou.com/d?threadUrl=//ningbo.19lou.com/forum-797-thread-166871421069359864-1-1.html&showType=1&bdName=13015";
                                var m5 = $.md5(str);
                                var p1 = m5.substr(0,2);
                                var p2 = m5.substr(2,2);
                                var actionUrl = '//att3.citysbs.com/qrcode/'+p1+'/'+p2+'/'+m5+'.png?data='+encodeURIComponent(str);
                                $(".J_qrcode").attr('src',actionUrl);
                            })
                        </script>
