<script>
	$(function(){
		
                $(window).resize(function(){
			auto_window_size();
		});
		auto_window_size();
		function  auto_window_size() {//整体布局宽度自适应
			var win_width = $(window).width(),
				win_body = $('#autoBody');
			//规则当窗口宽度为1700 1240进行变化
			if (win_width < 1700 && win_width >= 1240) {//展示中屏
				win_body.removeClass('s').addClass('m');
				$("._ad_l").hide();
				$("._ad_m").show();
				$("._ad_s").hide();
			} else if (win_width < 1240) {//展示小屏
				win_body.removeClass('m').addClass('s');
				$("._ad_l").hide();
				$("._ad_m").hide();
				$("._ad_s").show();
			} else {//展示大屏
				win_body.removeClass('s').removeClass('m');
				$("._ad_l").show();
				$("._ad_m").hide();
				$("._ad_s").hide();
			}
		}

               //调整其他方式观看底部二维码的位置
		var ml20 = $(".ml20").clone();
		$(".ml20").remove();
		$(".ml10").before(ml20);
	})
</script>
