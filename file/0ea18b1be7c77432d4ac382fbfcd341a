//调国双分享事件
  $(".bdsharebuttonbox>a").click(function(){
      var _chanel;
      if (window._gsTracker) {
         if($(this).index() == 0) _chanel = "微博";
         else if($(this).index() == 1) _chanel = "微信";
         else if($(this).index() == 2) _chanel = "QQ";
         else if($(this).index() == 3) _chanel = "QQ空间";
         else _chanel = "微信";
         
         _gsTracker.trackEvent("分享",_chanel,__INFO__.vid);
      }
  })
</script>

