<!-- 播放页视频框矩形广告位 类型：固定 尺寸：300x250 -->


<script>
//$(function(){
var _scrollresize = true;

adScroll();
$(window).resize(function(){
  	setTimeout(function(){
     	if($("body").hasClass("s")){
	         $(".list-juji").height(440);
	         $(".list-juji-con").height(400);
	         //$(".list-juji-con").scrollTop(150);
	       }else if($("body").hasClass("m")){
	         $(".list-juji").height(510);
	         $(".list-juji-con").height(470);
	         //$(".list-juji-con").scrollTop(200);
	    }else{
	         $(".list-juji").height(716);
	         $(".list-juji-con").height(636);
	         //$(".list-juji-con").scrollTop(250);
	    }
	    $(".list-juji").stop().animate({"top":0},100);
	    adScroll();
   	},200)    
})
	


function adScroll(){
	if( typeof(__INFO__) != 'undefined' && __INFO__.cid == 11 && $(".list-juji-con") != 'undefined'){
        if(_scrollresize ){
            _scrollresize = false;
            var curad = $(".r-ad");
		$(".list-juji").prepend(curad);
		//curad.remove();

         }
	   	
        var _initHei = $(".list-juji-con").height();
        var _initHei2 = $(".list-juji").height();
		var _newad = $(".list-juji .r-ad");
		_newad.css({"width":300,"height":250,"display":"block"});
		$(".list-juji-con").height(_initHei- 250);
		var _scrollflag = true;
		$(".list-juji-con").scroll(function(){
			var _scrolltop = $(".list-juji-con").scrollTop();
		      //console.log("_scrolltop :             "+_scrolltop )
			if(_scrolltop > 500 && _scrolltop < 510){
		               if(_scrollflag){
					 $(".list-juji").stop().animate({"top":-260});
					$(".list-juji-con").stop().animate({"height":_initHei});
					$(".list-juji").css({"height":_initHei2+260});
					_scrollflag = false;
				}
			}else if(_scrolltop > 250 && _scrolltop < 260){
				if(!_scrollflag){
				 	$(".list-juji").stop().animate({"top":0},100);
					$(".list-juji-con").stop().animate({"height":_initHei - 260});
					$(".list-juji").css({"height":_initHei2});
                                              _scrollflag = true;                                       
				}
			}else{}
		})
	}
}
//})
</script>

