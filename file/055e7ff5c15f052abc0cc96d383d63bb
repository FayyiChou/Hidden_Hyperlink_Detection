<script type="text/javascript">
    jQuery(document).ready(function () {
        $("#ckLogin").change(function () {
            if ($("#ckLogin").prop("checked")) {
                $("#ckLogin").next("span").show();
            }
            else {
                $("#ckLogin").next("span").hide();
            }
        })

        //设置内容高度
        function setContentHeight() {
            var cHeight = $(window).height(); //窗口高度
            var bHeight = $("html").height();
            var topHeight = $(".nav-header").height() + $(".navbar").height(); //头部高度
            var bottomHeight = $(".Yzw-footer-wrapper").height(); //底部高度
            var contentHeight = cHeight - topHeight - bottomHeight; //内容高度
            var contentHeight1 = bHeight - topHeight - bottomHeight;
            if (cHeight > bHeight) {
                $(".main-content,.login-bg").css("min-height", contentHeight);
            } else {
                $(".main-content,.login-bg").css("min-height", contentHeight1);
            }
        }
        window.onload = function () { setContentHeight(); }
        window.onresize = function () { setContentHeight(); }

        $("#valiCode").click(function () {
            this.src = "/Login/LoginValidationCode?time=" + (new Date()).getTime();
        });

        $(".login-form input").keydown(function (e) {
            if (e.keyCode == 13) {
                $("#btnsubmit").click();
            }
        });

        $("#btnsubmit").click(function () {
            login();
            return false;
        })

        function login() {
            if ($.trim($("#tbaccount").val()).length == 0) {
                $.showWarning("请输入账号！");
                return false;
            }
            if ($.trim($("#tbpassword").val()).length == 0) {
                $.showWarning("请输入密码！");
                return false;
            }
            if ($.trim($("#tbverifycode").val()).length == 0) {
                $.showWarning("请输入验证码！");
                return false;
            }

            var param = {
                account: $("#tbaccount").val(),
                password: $("#tbpassword").val(),
                verifycode: $("#tbverifycode").val(),
                keepalive: $("#ckLogin").prop("checked")
            };

            function rollbackLoginStatus() {
                $("#btnsubmit").css("pointer-events", "inherit");
                $("#btnsubmit").html(JR('<i class="fa fa-key"></i>登录'));
            }

            $.ajax({
                url: '/Login/Login',
                type: "POST",
                data: param,
                dataType: "json",
                beforeSend: function () {
                    $("#btnsubmit").css("pointer-events", "none");
                    $("#btnsubmit").html(JR('<i class="fa fa-key"></i>登录中...'))
                },
                error: function () {
                    rollbackLoginStatus();
                },
                complete: function () { },
                success: function (data) {
                    if (data && data.Success) {
                        if (data.Data&&!data.Data.LastChangePasswordDate) {//没修过密码，优先导向到修过密码
                            window.location.href = "https://auth.yzw.cn/authportal/profile/password";
                        }
                        else if ("http://jc.yzw.cn/tendermgt/tender/list") {
                            window.location.href = "/Login/VerifyPhone?returnUrl=" + encodeURIComponent("http://jc.yzw.cn/tendermgt/tender/list");
                        }
                        else if ("") {
                            window.location.href = "/Login/VerifyPhone?returnUrl=" + encodeURIComponent("http://jc.yzw.cn/tendermgt/tender/list");
                        }
                        else {
                            window.location.href = "/Login/VerifyPhone?returnUrl=" + encodeURIComponent("https://www.yzw.cn");
                            
                        }
                    }
                    else if (data && data.Success === false) {
                        rollbackLoginStatus();
                        $.showWarning(data.Message);
                        if (data.Code != 1) {
                            //刷新验证码
                            $("#valiCode").click();
                            $("#tbverifycode").val("");
                        }
                    }
                }
            });
        }

        //检测浏览器语言
        var currentLang = navigator.language;   //判断除IE外其他浏览器使用语言
        if (!currentLang) {//判断IE浏览器使用语言
            currentLang = navigator.browserLanguage;
        }
        //判断访问终端
        var browser = {
            versions: function () {
                var u = navigator.userAgent, app = navigator.appVersion;
                return {
                    trident: u.indexOf('Trident') > -1, //IE内核
                    presto: u.indexOf('Presto') > -1, //opera内核
                    webKit: u.indexOf('AppleWebKit') > -1, //苹果、谷歌内核
                    gecko: u.indexOf('Gecko') > -1 && u.indexOf('KHTML') == -1,//火狐内核
                    mobile: !!u.match(/AppleWebKit.*Mobile.*/), //是否为移动终端
                    ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/), //ios终端
                    android: u.indexOf('Android') > -1 || u.indexOf('Linux') > -1, //android终端或者uc浏览器
                    iPhone: u.indexOf('iPhone') > -1, //是否为iPhone或者QQHD浏览器
                    iPad: u.indexOf('iPad') > -1, //是否iPad
                    webApp: u.indexOf('Safari') == -1 //是否web应该程序，没有头部与底部
                };
            }(),
            language: (navigator.browserLanguage || navigator.language).toLowerCase()
        }

        var isMobile = browser.versions.mobile || browser.versions.ios || browser.versions.android || browser.versions.iPhone || browser.versions.iPad;
        var url = isMobile ? "/hp/wap/index.html" : "/hp/pc/index.html";
        $("#aDownloadIndex").attr('href','https://www.yzw.cn' + url);
    });
</script>
</body></html>