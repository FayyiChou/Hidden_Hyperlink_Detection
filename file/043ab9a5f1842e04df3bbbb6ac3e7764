<script type="text/javascript"> 
var n = 0, count = 0;
var cid = 1;
$(function(){
	cartSize();
	initHeadInfo();
	ScrollImgLeft();
	//$('img').lazyload({placeholder : "images/loading.gif",effect : 'fadeIn'});
	 count = $("#banner li").size();
	$('#banner li').mousemove(function(){
		n = $(this).index();	
		$(this).roll(n);		
	});
	
	$.fn.roll = function(num){
		$("#banner li").eq(num).addClass('current').siblings().removeClass('current');
		var rtab = $(this).parent().parent();
		rtab.siblings('img').eq(num).siblings("img").fadeOut("slow");
		rtab.siblings('img').eq(num).fadeIn("slow");
	}
	
	var a = $('#jipin li').length;
	var b = 0;
	var ul = $("#jipin");
	$('#prev').click(function(){
	 	b = b <= 0 ? 0 : --b;
		ul.animate({marginLeft:-237*b}, 'slow',function(){});
	});
	
	$('#next').click(function(){
		b = b < a-3 ? ++b : b;
		ul.animate({marginLeft:-237*b}, 'slow',function(){});
	});
	
//	var a1 = $('#zhuanti li').length;
//	var b1 = 0;
//	var ul1 = $("#zhuanti");
//	$('#prev1').click(function(){
//	 	b1 = b1 <= 0 ? 0 : --b1;
//		ul1.animate({marginLeft:-237*b1}, 'slow',function(){});
//	});
	
//	$('#next1').click(function(){
//		b1 = b1 < a1-3 ? ++b1 : b1;
//		ul1.animate({marginLeft:-237*b1}, 'slow',function(){});
//	});
	
	$('#flink  li,#recommand li,#need li,#article li').mousemove(function(){
		tabs($(this));
	});
	
	function tabs(obj){
		obj.addClass('current').siblings().removeClass('current');
		var tab = obj.parent().parent();
		cls = tab.next().attr('class');
		if (cls.indexOf(" ")>0){
			cls = cls.substr(cls.indexOf(" ")+1);
		}
		tab.siblings("."+cls).eq(obj.index()).show().siblings("."+cls).hide();
	}
	setInterval("gd()", 5000);
	var cpfl_cp_in=false;
	var cpdetail_in=false;
	var mn=0;
	$('#cpfl .cp').mouseover(function(){
		var obj = $(this);
		cid = obj.attr("id");
		if(cid!=39){
			obj.addClass('current').siblings().removeClass('current');
			var height = obj.position().top;
			mn= $(this).prevAll().length;
			if(mn==0){
				$('#cpdetail').css("top",24*mn-1+5);
			}else{
				$('#cpdetail').css("top",24*mn-1);
			}
			var data = getAttrByCid(cid);
			loadDetailAttr(data);
			$('#cpdetail').show();
			cpfl_cp_in=true;
		}
	});
	
	$('#cpfl .cp').mouseout(function(){
		
		$('#cpdetail').hide();		
		cpfl_cp_in=false;
		if(!cpfl_cp_in&&!cpdetail_in){$('#cpfl .cp').removeClass('current');}
	});
	
	$('#cpdetail').mouseover(function(){
		$('#cpdetail').show();
		$('#cpfl .cp:eq('+mn+')').addClass('current');
		cpdetail_in=true;
	});
	
	$('#cpdetail').mouseout(function(){
		$('#cpdetail').hide();
		cpdetail_in=false;
		if(!cpfl_cp_in&&!cpdetail_in){$('#cpfl .cp').removeClass('current');}
	});
	
});

//banner滚动效果
setInterval("autoBanner()", 5000);
function autoBanner(){
   n = n > count ? 0 : ++n;
   $('#banner li').roll(n-1);
}

function sh(arr){
	window.location.href='/category-'+cid+'-b0-min0-max0-attr'+arr+'-1-goods_id-DESC.html';
}

function cartSize(){
	var ret = $.ajax({
		url : "/cartSize.php",
		async : false,
		cache : false
	}).responseText;
	var json = eval("("+ret+")");
	$("#cart_size").html(json.size);
}

function initHeadInfo(){
	var ret = $.ajax({
		url : "/userInfo.php",
		async : false,
		cache : false
	}).responseText;
	var json = eval("("+ret+")");
	if(json.success){
		var username = json.username;
		var info="欢迎来到意绵绵	<strong style=\"color:#CC3300; padding:0 5px; font-weight:bold;\"><a style=\"color: #cc3300;\" href=\"/web/user.php\" target=\"_blank\">"+username+"</a></strong>"+
				 "	<a href=\"/logout.php\" style=\"font-size:12px; color:#FF6600;\" >退出登录</a>";
		$("#index_head").html(info);
	}
}

//最新评论滚动
var len = $("#comments .theccc").length;
var i = 0;
function gd(){
    i = i > len ? 0 : i++;
	$("#comments").animate({marginTop:-110*i},"slow",function(){
		$(this).children(":first").appendTo($(this));
		$(this).css("marginTop",0);
	});
}

function zjpdLjgm(id1,id2,id3,id4){
	if(id1!=undefined){
		$.ajax({
		url : "/cart/add.php",
		data:{
				goodsId : id1,
				num : 1
			},
		async : false,
		cache : false,
		contentType: "application/x-www-form-urlencoded; charset=UTF-8"
		});
	}
	if(id2!=undefined){
		$.ajax({
		url : "/cart/add.php",
		data:{
				goodsId : id2,
				num : 1
			},
		async : false,
		cache : false,
		contentType: "application/x-www-form-urlencoded; charset=UTF-8"
		});
	}
	if(id3!=undefined){
		$.ajax({
		url : "/cart/add.php",
		data:{
				goodsId : id3,
				num : 1
			},
		async : false,
		cache : false,
		contentType: "application/x-www-form-urlencoded; charset=UTF-8"
		});
	}
	if(id4!=undefined){
		$.ajax({
		url : "/cart/add.php",
		data:{
				goodsId : id4,
				num : 1
			},
		async : false,
		cache : false,
		contentType: "application/x-www-form-urlencoded; charset=UTF-8"
		});
	}
	window.open("/cart.php");
}


 //横向滚动
function ScrollImgLeft(){
	var speed=50
	var scroll_begin = document.getElementById("scroll_begin");
	var scroll_end = document.getElementById("scroll_end");
	var scroll_div = document.getElementById("scroll_div");
	scroll_end.innerHTML=scroll_begin.innerHTML
	function Marquee(){
		if(scroll_end.offsetWidth-scroll_div.scrollLeft<=0)
		scroll_div.scrollLeft-=scroll_begin.offsetWidth
		else
		scroll_div.scrollLeft++
	}
	var MyMar=setInterval(Marquee,speed)
	scroll_div.onmouseover=function() {clearInterval(MyMar)}
	scroll_div.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
}

function loadDetailAttr(data){
	$('#cpdetail').html("");
	var atHtml = '';
	if(data!=''){
		var atcs = data.split(';');
		for(var i=0;i<atcs.length;i++){
			var ats = atcs[i].split(':');
			atHtml += "<ul class=\"overf\"><li class=\"tagtitle\">"+ats[0]+"</li>";
			var as = ats[1].split(",");
			for(var j=0;j<as.length;j++){
				atHtml += "<li><a onclick=\"sh('"+as[j]+"')\" href=\"javascript:void 0;\" class=\"link6662\">"+as[j]+"</a></li>";
			}	
			atHtml += "</ul>";
		}
	}
	atHtml += "<div style=\"padding-top:20px;\"><img src=\"/images/index_82.jpg\"></div>";
	$('#cpdetail').append(atHtml);
}

function showinfo(link){
	window.open(link);
}

