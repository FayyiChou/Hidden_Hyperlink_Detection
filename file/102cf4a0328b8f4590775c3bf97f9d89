    <script type="text/javascript">
        function previewCW(cwid, cwt, isNotSupport, viewType) {
            //课件类型不支持
            if (isNotSupport == "1") {
                notisWarning("目前此课件仅支持在PC端学习");
                return;
            }
            switch (viewType) {
                case "0":
                    window.location.href = '/m/Course/CWViewerNew' + '?cwID=' + cwid+'&isAllCanShow=1'+'&no=681694&isControlTime=1';
                    //cs
                    break;
                case "1.1":
                    window.y_bridge_util.play_video({
                        'param': {
                            'orgid': 'b3f98378-05de-4ef2-af24-f29ef19e4c99',
                            'uid': cwid,
                            'type': 'courseware',
                            'islogined': false
                            },
                        'CBPluginName': '',
                        'CBTagName': function () {
                        }//回调函数
                    });
                    break;
                case "2":
                    openAppPlayer(cwid);
                    break;
                default:
                    openAppPlayer(cwid);
                    break;
            }
        }

        function previewNew(cwid, cwt, isNotSupport, viewType)
        {
            //课件类型不支持
            if (isNotSupport == "1") {
                notisWarning("目前此课件仅支持在PC端学习");
                return;
            }
            switch (viewType) {
                case "0":
                    window.location.href = '/m/Course/CWViewerNew' + '?cwID=' + cwid+'&isAllCanShow=1'+'&no=681694&isControlTime=0';

                    //cs
                    break;
                case "1.1":
                    window.y_bridge_util.play_video({
                        'param': {
                            'orgid': 'b3f98378-05de-4ef2-af24-f29ef19e4c99',
                            'uid': cwid,
                            'type': 'courseware',
                            'islogined': false
                            },
                        'CBPluginName': '',
                        'CBTagName': function () {
                        }//回调函数
                    });
                    break;
                case "1.2":
                    window.y_bridge_util.play_doc({
                        'param': {
                            'orgid': 'b3f98378-05de-4ef2-af24-f29ef19e4c99',
                            'uid': cwid,
                            'type': 'courseware',
                            'islogined': false
                            },
                        'CBPluginName': '',
                        'CBTagName': function () {
                        }//回调函数
                    });
                    break;
                case "2":
                    openAppPlayer(cwid);
                    break;
                default:
                    openAppPlayer(cwid);
                    break;
            }
        }

        //提示窗
        function notisWarning(msg) {
            window.Notify.warning(msg);
        }
        //云播下载
        function downloadYunbo() {
            var url = "/m/Download";
            
            window.location.href = url;
        }
        //打开云播
        function openAppPlayer(cwid) {
            var isY = window.y_bridge_public.isY;
            if (!isY) {
                setTimeout(function () { showDownLoad(cwid); }, 1000);
            }
                 setTimeout(function () { showDownLoad(cwid); }, 1000);
            goToYunPlayer(cwid, "courseware");
        }

        var timeoutShow;
        //云播下载提示
        function showDownLoad(cwid) {
            if (cwid) {
                clearTimeout(timeoutShow);
                var yunbo = $(".promptfloat");
                $(yunbo).show();
                timeoutShow = setTimeout(function () {
                    $(yunbo).hide();
                }, 5000);
            }
        }

        //分享获得积分
        function shareCourse(){
            $.get("/m/Share/ShareContent", { masterID: "61940a95-d375-4e39-b52d-aa9923836630", masterName: "我的“第一次”，运营必学课程！", masterType: "course", userID: "" ,doShareUserID:""},
                function (data) {

                });
        }
    </script>

