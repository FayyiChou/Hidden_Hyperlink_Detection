    <script type="text/javascript">
    window.addEventListener("resize", function () {
        var popupleft = ($(window).width() - $(".addpopup").width()) / 2;
        var popupttop = document.body.scrollTop + (document.documentElement.clientHeight - $(".addpopup").height()) / 2;
        $(".addpopup").css({ "left": popupleft, "top": popupttop });
        var promptleft = ($(window).width() - $(".promptpop").width()) / 2;
        var prompttop = document.body.scrollTop + (document.documentElement.clientHeight - $(".promptpop").height()) / 2;
        $(".promptpop").css({ "left": promptleft, "top": prompttop });
    }, false);
    $(document).ready(function () {

        function tab(nav, md, con) {
            var nav_l = nav.find(md);
            nav_l.each(function (index) {
                $(this).click(function () {
                    var styleCss=$(this).parent().find(".selected").eq(0).attr("style");
                    $(this).parent().find(md).removeClass('selected');
                    $(this).parent().find(md).removeClass('d_main_hue');
                    $(this).parent().find(md).removeClass('d_border_color');
                    $(this).parent().find(md).removeClass('d_d');
                    $(this).parent().find(md).removeClass('d_bd');
                    $(this).parent().find(md).removeClass('d_ff');
                    $(this).parent().find(md).attr("style","");
                    $(this).addClass('selected');
                    $(this).addClass('d_main_hue');
                    $(this).addClass('d_border_color');
                    $(this).addClass('d_d');
                    $(this).addClass('d_bd');
                    $(this).addClass('d_ff');
                    $(this).attr("style",styleCss);
                    con.addClass('hidden');
                    con.eq(index).removeClass('hidden');
                    if ($(this).attr("data-type") == "pl") {
                        //goSnapIndex();
                    }

                });
            });


        }
        goSnapIndex();
        tab($('.new-course-nav'), 'li', $('.new-course-con'));

        //页面加载看是否是从试看返回的 若是则跳出提示
        if(0==1)
        {
            alert("试看时间已到，无法继续观看!");
        }

        //倒计时
        //showCountDown($("span[data-id='spcd']:last"), $("span[data-id='spcd']:last").attr("data-second"));
    });
    if ($("#redUrl").val() != '') {
        window.location.href = $("#redUrl").val();
    }
    function goSnapIndex() {
        getComment(false);
    }
    var isloadingComment = false;
    var isEndComment = false;

    function getComment(isClear) {
        if (!isloadingComment && !isEndComment) {
            isloadingComment = true;
            if (!isClear) {
                $("#pageIndex").val(parseInt($("#pageIndex").val()) + 1);

            }

            isloadingComment = false;

            loadEva(isClear);
        }
    }
    
    function loadEva(isClear) {
        $.get("/m/Course/_CommentData1", { courseID: "61940a95-d375-4e39-b52d-aa9923836630", status: "Approved", pageIndex: $("#pageIndex").val(), pageSize: $("#pageSize").val() },
                      function (data) {
                          var temp=JSON.parse(data);
                          var questions =temp.list;
                          var allcount=temp.conunt;
                          var index=parseInt($("#pageIndex").val());
                          var pagesze=parseInt($("#pageSize").val());
                          var maxindex= parseInt((allcount-1)/pagesze)+1;

                          if (index==maxindex) {
                              $("#spnLoadMore").remove();
                          }

                          if (($("#pageIndex").val() == "1" || $("#pageIndex").val() == "0") && questions.length == 0) {
                              var html = templateParse("temppj",1);
                              //var html = '<div class="course-no-comment"><span class="c-no-img"></span><span class="d-block font-size-14 text-lightgrey text-center">暂无评价</span></div>';
                              $("#course-s-list").append(html);
                          }

                          for (var i = 0; i < questions.length; i++) {
                              if (!questions[i].UserPhotoUrl || questions[i].UserPhotoUrl == "") {
                                  questions[i].UserPhotoUrl = "/m/Content/images/decorate/u194.png";
                              }

                              if (!questions[i].UserCName || questions[i].UserCName == "") {
                                  questions[i].UserCName = "匿名";
                              }
                              if (questions[i].CommentTime != "") {
                                  var testDate = new Date(questions[i].CommentTime);
                                  testDate.setTime(testDate.getTime()+1000*60*(testDate).getTimezoneOffset());

                                  questions[i].CommentTime = testDate.format("yyyy-MM-dd hh:mm");
                              }
                              if (questions[i].CommentContentJsonObj) {
                                  questions[i].Content=questions[i].CommentContentJsonObj.Content;
                                  questions[i].ReplyCount=questions[i].CommentContentJsonObj.CommentReplyLst.length;
                              }

                              var htmlS = setStar(questions[i].CommentScore);
                              var htmlQ = templateParse("tempEval", questions[i]);
                              //console.log(questions[i].CommentContentJsonObj.Content);
                              if (questions[i].CommentContentJsonObj) {
                                  htmlQ = $(htmlQ.trim());
                                  //var htmlA = templateParse("tempAsk", questions[i].CommentContentJsonObj);
                                  //$(htmlQ).find('#course-e-detail').append(htmlA);

                                  for (var j = 0; j < questions[i].CommentContentJsonObj.CommentReplyLst.length; j++) {
                                      var repObj=questions[i].CommentContentJsonObj.CommentReplyLst[j];
                                      var dateRep= new Date(repObj.ReplyTime);
                                      //dateRep.setTime(dateRep.getTime()+1000*60*(dateRep).getTimezoneOffset());
                                      repObj.ReplayTime = dateRep.format("yyyy-MM-dd hh:mm");

                                      //console.log(repObj);
                                      var htmlRep=templateParse("tempReply", repObj);
                                      $(htmlQ).find('.course-reply-list').append(htmlRep);
                                  }
                              }
                              $(htmlQ).find('.course-e-star').append(htmlS);
                              $("#course-s-list").append(htmlQ);
                          }
                      });


    }
