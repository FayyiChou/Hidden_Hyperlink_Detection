        $('.try-ins-info').fadeOut();
    },3000);

    //控制星星
    function setStar(score) {
        var htm = "";
        var s = "";
        if (score != 0) {
            for (var i = 1; i <= 5; i++) {

                if (score < i) {
                    s = "fa-star-o  d_main_hue d_d d_ff";
                }
                else {
                    s = "fa-star  d_main_hue d_d d_ff ";
                }
                htm += ' <span class="fa ' + s + ' ss"></span>';

            }
        }

        return htm;
    }

    var url_GetUserType = "/m/GetUserType";//获取用户类型
    var url_AddShoppingCar = "/m/Trans/AddShoppingCart";//添加到购物车
    var url_Login = "/m/Login?returnUrl=%2fm%2fcv%2f681694";//登录网址
    var url_CheckIsFree = "/m/Course/CheckIsFree";//是否免费直播培训
    var url_ShoppingCart = "/m/Trans/ConfirmOrder";//确认订单

    //加入购物车
    $("#btnShoppingCart").bind("click", function () {
        $("#pageIndex").val("0");
        $("#pageSize").val("5") ;
        $("#course-s-list").append("");
        var id = $(this).attr("productid");
        var courseType = $(this).attr("coursetype");

        $.ajax({
            url: url_GetUserType,
            type: "get",
            dataType: "json",
            success: function (data) {
                if (data == -1) {
                    //alert("请先登录");
                    //warning("请先登录");
                    //window.location.href = url_Login;
                    window.location.href = "/m/Login?returnUrl=%2fm%2fcv%2f681694";
                    //Notify.warning("请先登录");
                }
                    //else if (data == -2 || data == -3) {
                    //    //alert("加入购物车失败，请稍后再试。");
                    //    Notify.warning("加入购物车失败，请稍后再试。");
                    //}
                    //else if (data == 1) {
                    //    //alert("暂不开放老师购买或添加课程。");
                    //    Notify.warning("暂不开放老师购买或添加课程。");
                    //}
                else {
                    if(0 == 1){
                        alert1($("#divTips").html());
                        //Notify.warning("非指定类型会员，无法购买");
                        return;
                    }
                    var arr = { goodsID: id, goodsType: courseType };
                    $.ajax({
                        url: url_AddShoppingCar,
                        type: "post",
                        data: arr,
                        dataType: "text",
                        success: function (data) {
                            switch (data) {
                                case "0":
                                    //alert("该商品已在购物车");
                                    Notify.warning("该商品已在购物车");
                                    break;
                                case "1":
                                    var popupleft = ($(window).width() - $(".addpopup").width()) / 2;
                                    var popupttop = document.body.scrollTop + (document.documentElement.clientHeight - $(".addpopup").height()) / 2;
                                    $(".addpopup").css({ "left": popupleft, "top": popupttop });
                                    $("#addSuccess").show();
                                    break;
                                case "2":
                                    //alert("添加失败");
                                    Notify.warning("添加失败");
                                    break;
                                case "-1":
                                    Notify.warning("该商品未销售，无法加入到购物车。");
                                    break;
                                case "-2":
                                    Notify.warning("该商品名额已满，无法加入到购物车。");
                                    break;
                                default:
                                    break;
                            }
                        }
                    });
                }
            }
        });
    });
    var islogin="0";
    //立即购买
    $("#btnBuyAtOnce").bind("click", function () {
        $("#pageIndex").val("0");
        $("#pageSize").val("5") ;
        $("#course-s-list").append("");
        if (islogin=="0") {
            window.location.href = "/m/cvsale/681694";
            return;
        }
        if(0 == 1){
            alert1($("#divTips").html());
            //Notify.warning("非指定类型会员，无法购买");
            return;
        }
        var id = $(this).attr("productid");
        var courseType = $(this).attr("coursetype");
        $(this).button('loading');


        $.post('/m/Trans/BuyAtNow', { productID: id, productType: courseType },
        function (data) {
            if (data.status == 401) {
                if (navigator.userAgent.toLowerCase().indexOf("micromessenger") != -1) {
                    showDealingMsg("登录中...");
                    window.location.href = '/m/BindUser?uctype=wechat&ubtype=username';
                }
                else {
                    window.location.href = url_Login;
                }
            }
                //单一登录跳转
            else if (data.status == 501 && data.message.indexOf("其他地方登录") > -1) {
                window.location.href = '/m/Shared/TokenExpire203';
            }
            if (data.indexOf("ok:") == 0) {
                window.location.href = url_ShoppingCart + "?oid=" + data.replace("ok:", "") + "&goodsType=" + courseType;
            }
            else {
                $("#btnBuyAtOnce").button("reset");
                Notify.warning(data);
            }
        });
    });
    var coursetype="OnlineCourse";

    function scannedNow()
    {
        var url = '/m/Course/DetailForfree';
        url = url + "?no=" +681694;
        $("#redUrl").val(url);
        window.location.href = url;
    }
    //试看
    function goView(no) {
        window.location.href = '/m/Course/DetailForfree' + '?no=' + no;
    }
    function studyNow(obj) {
        $("#pageIndex").val("0");
        $("#pageSize").val("5") ;
        $("#course-s-list").append("");
        if (islogin=="0") {
            window.location.href = "/m/cvsale/681694";
            return;
        }
        if(0 == 1){
            alert1($("#divTips").html());
            //Notify.warning("您不是指定类型会员，无法学习！");
            return;
        }
        $(obj).val("请稍候...");
        $(obj).attr("disabled", "disabled");

        $.post("/m/Trans/BuyCourseForFree", { courseID: "61940a95-d375-4e39-b52d-aa9923836630" },
function (data) {
    if (data.status == 401) {
        //window.location.href = url_Login;
        if (navigator.userAgent.toLowerCase().indexOf("micromessenger") != -1) {
            showDealingMsg("登录中...");
            window.location.href = '/m/BindUser?uctype=wechat&ubtype=username';
        }
        else {
            window.location.href = url_Login;
        }
    }
        //单一登录跳转
    else if (data.status == 501 && data.message.indexOf("其他地方登录") > -1) {
        window.location.href = '/m/Shared/TokenExpire203';
    }
    else if (data == "error") {
        Notify.warning("获取课程失败，请稍后再试。");
        $(obj).val("马上学习");
        $(obj).removeAttr("disabled");
    }
    else {

        var url = '/m/Course/MyCourseIndex';
        url = url + "?scid=" + data;
        if (coursetype=="CoursePackage") {
            url = url + "&coursetype=coursepackage";
        }
        $("#redUrl").val(url);
        url = url + "&isStudyNow=true";
        window.location.href = url;
        $(obj).val("已获得");
        $(obj).css("background-color", "gray");
    }
});
    }

