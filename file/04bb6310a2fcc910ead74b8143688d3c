ce(-1)[0]; if(!($focus = store.get($name))) { smartcrop.crop(img, options, function(result) { var x = ((result.topCrop.x + result.topCrop.width/2)/img.naturalWidth - .5)*2; var y = ((result.topCrop.y + result.topCrop.height/2)/img.naturalHeight - .5)*-2; $('.focuspoint').attr({'data-focus-x':x,'data-focus-y':y,'data-image-w':img.naturalWidth,'data-image-h':img.naturalHeight}).focusPoint(); store.set($name, {x:x,y:y}); }); } else { $('.focuspoint').attr({'data-focus-x':$focus.x,'data-focus-y':$focus.y,'data-image-w':img.naturalWidth,'data-image-h':img.naturalHeight}).focusPoint(); } } }); if($('.pd_zw img[data-img]').length>0){ var colorThief = new ColorThief(); var colors = []; anno.addPlugin('Arting365', { url: 'http://arting365.com/annotations/',index_id: '?index_id=286253'}); var annotations = []; $('.pd_zw img[data-img]').css({ 'background':'url(//static.365imgs.cn/img/pm-loader.gif) #e2e2e2 center center no-repeat', 'width':700, 'height':300 }); $('.pd_zw img[data-img]').appear(); $('.pd_zw').trigger('scroll'); $('.pd_zw').on('appear', 'img[data-img]', function(e, $affected){ if($(this).attr('src') == $(this).attr('data-img')) return; $(this).attr('src', $(this).attr('data-img')).removeAttr('style'); if($(this).parent()[0].nodeName == 'A' && $(this).parent().href != $(this).attr('src')){ $(this).parent().replaceWith($(this).parent().html()); } if($(this).parent()[0].nodeName == 'A'){ return; } var that = $(this); var img = $(this)[0]; var image=$(this).attr('src').replace('http:',location.protocol); $(this).imagesLoaded(function(){ that.wrap('<div class="img-preview"></div>'); anno.makeAnnotatable(img); var arr = img.src.split('/'); var key = '/'+arr.splice(3, arr.length - 3).join('/'); if(annotations[key]){ $.each(annotations[key],function(idx,val){ anno.addAnnotation(val); anno.highlightAnnotation(val); }); } that.after('<div class="img-tools"><a rel="fluidbox" href="'+image+'" title="查看大图">查看大图</a><a class="annotator" href="http://arting365.com/assets/img/annotator.gif?20170328" title="图片评论">点选图片上的元素添加评论</a></div>'); }); }); $('.pd_zw').on('click', '.annotator', function ($e){ $('.annotator').colorbox({ opacity: .7, title: '图片评论演示', onLoad: function(){ $('#cboxOverlay').css('background-color', '#000'); } }); }); $('.pd_zw').on('click', 'a[rel="fluidbox"]', function ($e){ $('.pd_zw a[rel="fluidbox"]') .colorbox({ rel: 'gal', opacity: 1, slideshow: true, slideshowSpeed: 3000, maxWidth: function(){ return $(window).width()-100; }, maxHeight: function(){ return $(window).height()-100; }, title: '僵小鱼他爹的进化之路', onLoad: function(){ var img = $.colorbox.element().attr('href').replace('http:',location.protocol); if(typeof colors[img] !== 'undefined'){ $('#cboxOverlay').css('background-color', 'rgb('+colors[img]+')'); } else { colorThief.getColorAsync(img,function(color, element){ $('#cboxOverlay').css('background-color', 'rgb('+color.join(',')+')'); colors[img] = color.join(','); }); } } }); }); } $('.favoriteIt').on('ajax:success', function(event, data, status, xhr) { if(data.code == 200){ if(data.increment > 0){ $(this).addClass('icon-heart-o'); }else{ $(this).removeClass('icon-heart-o'); } }else if(data.code == 401){ if(!confirm(data.message)){ return false; } location.href=data.url; }else{ alert(data.message); } }).on('ajax:error',function(event, xhr, status, error){ if(xhr.status == 401){ if(!confirm('请登陆后收藏文章')){ return false; } location.href='/auth/login'; }else{ alert('服务器错误，请重试'); } }); var $textarea = $('.form-reply').find('textarea'); $('.replyIt').click(function(e){ e.preventDefault(); $textarea.focus(); $textarea.val('@'+$(this).attr('data-username')+' '+$textarea.val()); $('.form-reply').append('<input type="hidden" name="comment_id" value="'+$(this).attr('data-comment-id')+'" />'); }); $('.deleteIt').on('ajax:success', function(event, data, status, xhr) { if(data.code == 200){ $(this).closest('li').hide(); $(".form-reply input[name=comment_id]").remove(); }else{ alert(data.message); } }).on('ajax:error',function(event, xhr, status, error){ if(xhr.status == 403){ alert('你好像不能删除评论哦~'); }else{ alert('服务器错误，请重试'); } }); $('.form-reply').on('ajax:beforeSend', function(event, xhr, settings) { if($.trim($textarea.val()).length <= 1){ alert('哎呀，忘记写评论内容了~'); return false; } }).on('ajax:success', function(event, data, status, xhr) { if(data.code == 200){ $textarea.val(''); alert(data.message); location.reload(); }else if(data.code == 401){ if(!confirm(data.message)){ return false; } location.href=data.url; }else{ alert(data.message); } }).on('ajax:error',function(event, xhr, status, error){ if(xhr.status == 403){ alert('你好像不能评论哦~'); }else{ alert('服务器错误，请重试'); } }); }); </script><div class="right_box"><ul><li><a href="#top"><i class="iconfont"></i></a></li><li id="wx"><i class="iconfont"></i><h3><a><img src="http://arting365.com/assets/1603/images/wx_2.jpg" width="118" height="118"></a></h3></li></ul></div><div class="footer"><div class="w100"><div class="foot_l"><svg width="189" height="47" class="svg"><image href="http://arting365.com/assets/img/logo.svg" src="http://arting365.com/assets/img/logo_nonsvg.png" width="189" height="47"></image></svg><h2><span>/ </span>关注设计影响力</h2><h3><a href="http://m.arting365.com/about/" target="_blank">关于我们</a> | <a href="http://m.arting365.com/about/#feature" target="_blank">特色板块</a> | <a href="http://m.arting365.com/about/#topic" target="_blank">涵盖主题</a> | <a href="http://m.arting365.com/about/#brand" target="_blank">合作品牌</a> | <a href="http://m.arting365.com/about/#contact" target="_blank">联系合作</a><br> 反馈建议：service @ arting365.com<br> 商务合作：021 - 5027 8342 / 8343<br> 工作时间：工作日 10:00-18:00 </h3></div><div class="foot_r"><img src="http://arting365.com/assets/1603/images/wx_2.jpg">关注微信公众平台</div><div class="foot_c"><h2>战略联盟 / 合作伙伴</h2><ul><li><img src="http://arting365.com/assets/1603/images/shanghaidesign.png"></li><li><img src="http://arting365.com/assets/1603/images/baidumux.png"></li></ul></div><div class="clear"></div></div></div><div class="foot_