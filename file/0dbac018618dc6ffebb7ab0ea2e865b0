    <script type="text/javascript">        
        $(function () {   
            AutoImgCssSize();
        });

        function AutoImgCssSize() {
            $(".left_head").autoIMG();
	
	        var width = 270;
	        var height = 257;
           
            var t = $(".left_head img");
            var src = t.attr("src");
            var nImg = new Image();
            nImg.src = src;

            //宽度大于高度
            if(nImg.width>nImg.height){
                //图片宽度比窗口宽度小
                if (nImg.width < width) {        
		            var nw = Math.floor((width - nImg.width) / 2);
		            t.css("padding-left", nw + "px"); 
                }
                var h = nImg.height*(width/nImg.width);
                if(h<height){
                    var nh = Math.floor((height - h) / 2);                 
		            t.css("padding-top", nh + "px");  		
                }               

            }else{
                if (nImg.height < height) {        
		            var nh = Math.floor((height - nImg.height) / 2);
		            t.css("padding-top", nh + "px"); 
                }

                var w = nImg.width*(height/nImg.height);
                if(w<width){
                    var nw = Math.floor((width - w) / 2);
                    console.log("w:"+w+",width - w:"+(width - w)+",nw:"+nw);
		            t.css("padding-left", nw + "px");  		
                }
            }
        }


