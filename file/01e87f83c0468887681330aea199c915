    function downloadStat(from, word) {
        var url;
        if (typeof word === 'undefined')
        {
            url = 'https://www.meipian.cn/app/http/download.php?from=' + from + '&ua=' + $UA
        }
        else
        {
            url = 'https://www.meipian.cn/app/http/download.php?from=' + from + '&ua=' + $UA + '&word=' + word
        }

        $.ajax({
            type: 'GET',
            url: url,
            success:function() {
                window.location.href = 'http://a.app.qq.com/o/simple.jsp?pkgname=com.lanjingren.ivwen&g_f=991653'
            },
            error: function () {
                window.location.href = 'http://a.app.qq.com/o/simple.jsp?pkgname=com.lanjingren.ivwen&g_f=991653'
            }
