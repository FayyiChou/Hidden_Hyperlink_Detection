                }
                $('.post_admiremoney,.post_othermoney').hide();
                $('.post_img h2').html(price+'元');
               
                playTour(price);
            })
            function playTour(PlayTourPrice){
                $.post('/WeixinPay/setPlayTour',{price:PlayTourPrice,pid:'39271'},function(data){
                    if(data.status==1){
                        if(data.url){
                            $.ajax({url:data.url,type:'GET',success:function(data){
                                    $('body').append(data);
                            },error:function(){
                                minfo('请用微信客户端访问进行支付');
                            }});
                        }else{
                            var url = data.info;
                            //参数1表示图像大小，取值范围1-10；参数2表示质量，取值范围'L','M','Q','H'
                            var qr = qrcode(10, 'M');
                            qr.addData(url);
                            qr.make();
                            $('.post_img .img_code').html(qr.createImgTag());
                            $('.post_img').show();
                        }
                    }
                });
            }
        </script> 
   </div> 
