<script>
    function chooseDate(o){
        var year = $('#date_year .current a').attr('data-val');
        var month = $(o).find('a').attr('data-val');

        $('input[name="date"]').val(year+'-'+month)
        $('.simu-brand').hide();

    }

    function showMonth(o){
        var year = $(o).find('a').attr('data-val');
        if(year==2016){
            $('.month').each(function(i,e){
                if($(e).find('a').attr('data-val')>08){
                    $(e).hide();
                }
            });
        }else{
            $('.month').show();
        }
        $('#date_month').show()
    }

	var _storyTime;
    
    $(function(){
    	$(".story-in").hide();
    	$(".story-in:first").show();
    	$(".story-num a").eq(0).addClass("on");
        var len = $(".story-num > a").length;
    	var index = 0;
    	$(".story-num a").mouseover(function(){
    	   index =   $(".story-num a").index(this);
    	   showStoryImg(index);
    	}); 
    	$('#storybox').hover(function(){
    		 if(_storyTime){
    		 clearInterval(_storyTime);
    		 }
    	},function(){
    		_storyTime = setInterval(function(){
    			showStoryImg(index)
    		 index++;
    		 if(index==len){index=0;}
    		 } , 4000);
    	});
    	var _storyTime = setInterval(function(){
    		showStoryImg(index)
    	   index++;
    	   if(index==len){index=0;}
    	} , 4000);
    })
    function showStoryImg(i){
    	if($(".story-num a").eq(i).attr('class') == 'on') {
    		return;
    	}
    	var ii = i + 1;
       $("#storybox .story-in").filter(":visible").hide().parent().children().eq(ii).fadeIn(1000);
      // $("#idSlider").stop(true,false).animate({top : -168*i},800);
       $(".story-num a")
        .eq(i).addClass("on")
        .siblings().removeClass("on");
    }
</script>


