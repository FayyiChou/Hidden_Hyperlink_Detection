<script>

    var heheBox = null;
    seajs.use('//static.360buyimg.com/finance/financial/common/module/popup/1.0.0/js/popup.js', function(box) {
        heheBox = box;
    });


    $(".w-icon").on("click",function(){
        $(this).parents(".panes").find(".pane1").stop(false,true).animate({"left":"0"},'500');
    });

    $(".returns").on("click",function(){
        $(this).parents(".panes").find(".panes-wraps").stop(false,true).animate({"left":"569"},'500');
    })
    $(".shutsb").on("click",function(){
        $(this).parents(".panes").find(".panes-wraps").stop(false,true).animate({"left":"569"},'500');
    })

    $(".sel-downs-liu").on("click",function()
    {
        $(this).parent().find("wei-tuo-list").show();
    })

    //提交买卖
    $(".qun1").on("click",function()
    {
        //校验数据
        if($("#buyOrderType").attr("dd")!=1)
        {
            if($("#buypriceinput").val()=='')
            {
                $("#buypriceinput").parent().parent().find(".so-text").html("请输入买入价格！&nbsp;&nbsp;"+"<div class='so-xia'></div>");
                $("#buypriceinput").parent().parent().find(".so-text").show();
                return;
            }else
            {
                if(!checkUsMoney($("#buypriceinput").val()))
                {

                    $("#buypriceinput").parent().parent().find(".so-text").html("买入价格不合法！&nbsp;&nbsp;"+"<div class='so-xia'></div>");
                    $("#buypriceinput").parent().parent().find(".so-text").show();
                    return;
                }
                $("#buypriceinput").parent().parent().find(".so-text").hide();
            }
        }

        if($("#buynuminput").val()=='')
        {
            $("#buynuminput").parent().parent().find(".so-text").html("请输入买入数量 ！&nbsp;&nbsp;"+"<div class='so-xia'></div>");
            $("#buynuminput").parent().parent().find(".so-text").show();
            return;
        }else{
            $("#buynuminput").parent().parent().find(".so-text").hide();
        }

        //判断证券代码有效性 TODO
        if($("#inbuy").attr("instrumentid")=='')
        {
            alert("请输入股票代码！");//TODO
            return false;
        }

        if($("#buyOrderType").attr("dd")==1)//市场订单 可以使小数
        {
            if(!checkUsMoney($("#buynuminput").val()))
            {
                $("#buynuminput").parent().parent().find(".so-text").html("买入数量不合法！&nbsp;&nbsp;"+"<div class='so-xia'></div>");
                $("#buynuminput").val(round2( $("#buynuminput").val(),2));
                $("#buynuminput").parent().parent().find(".so-text").show();
                return ;
            }
        }else//其他的都必须是整数
        {
            if(!/^[0-9]*[1-9][0-9]*$/.test($("#buynuminput").val()))
            {
                $("#buynuminput").parent().parent().find(".so-text").html("不支持碎股交易 ！&nbsp;&nbsp;"+"<div class='so-xia'></div>");
                $("#buynuminput").parent().parent().find(".so-text").show();
                return ;
            }
        }

        $("#buynuminput").parent().parent().find(".so-text").html("");
        $("#buynuminput").parent().parent().find(".so-text").hide();



        $(".jzq-sure").attr("tt",1);
        //展示数据
        $("#mingxiname").html($("#inbuy").val());
        $("#mingxiside").html("买入");
        $("#mingxitype").html($("#buyOrderType").html());
        $("#mingxinum").html($("#buynuminput").val());
        if( $("#buyOrderType").attr("dd")==1)
        {
            $("#mingxiall").html("$"+parseFloat($("#buypriceinput").attr("beginprice")*$("#buynuminput").val()).toFixed(2));
        }else
        {
            $("#mingxiall").html("$"+parseFloat($("#buypriceinput").val()*$("#buynuminput").val()).toFixed(2));
        }



        //计算预估佣金
        var num = parseFloat($("#buynuminput").val());
        console.log(num<1.00);
        if(num<1.00)
        {
            $("#mingxiyongjin").html("$"+0.99);
        }

        console.log(398.00>=num>=1.00);
        if(398.00>=num  && num>=1.00)
        {

            $("#mingxiyongjin").html("$"+1.99);
        }

        if(num>398.00)
        {
            var sbb =  1.99+($("#buynuminput").val()-398)*0.005
            $("#mingxiyongjin").html("$"+parseFloat(round2(sbb,2)).toFixed(2));
        }






        $(this).parents(".panes").find(".pane2").stop(false,true).animate({"left":"0"},'500');
    });


    //卖股票时候的显示委托明细
    $(".qun2").on("click",function()
    {
        $(".jzq-sure").attr("tt",2);

        //校验数据合法性

        if($("#sellOrderType").attr("dd")!=1)
        {
            if($("#sellpriceinput").val()<=0)
            {
                $("#sellpriceinput").parent().parent().find(".so-text").html("卖出价格不合法"+"<div class='so-xia'></div>");
                $("#sellpriceinput").parent().parent().find(".so-text").show();
                return ;
            }

            if(!checkUsMoney($("#sellpriceinput").val()))
            {
                $("#sellpriceinput").parent().parent().find(".so-text").html("卖出价格不合法"+"<div class='so-xia'></div>");
                $("#sellpriceinput").parent().parent().find(".so-text").show();
                return ;
            }

            $("#sellpriceinput").parent().parent().find(".so-text").html("");
            $("#sellpriceinput").parent().parent().find(".so-text").hide();
        }



        if($("#sellnuminput").val()<=0)
        {
            $("#sellqianshow").html("卖出数量不合法"+"<div class='so-xia'></div>");
            $("#sellqianshow").show();
            return ;
        }

        if( $("#sellOrderType").attr("dd")==1)//市场订单可以是碎股
        {
            if(!checkUsMoney($("#sellnuminput").val()))
            {
                $("#sellqianshow").html("卖出数量不合法！"+"<div class='so-xia'></div>");
                $("#sellnuminput").val(round2( $("#sellnuminput").val(),2));
                $("#sellqianshow").show();
                return ;
            }
        }else//其他的都是整数
        {
            if(!/^[0-9]*[1-9][0-9]*$/.test($("#sellnuminput").val()))
            {
                $("#sellqianshow").html("不支持碎股交易！"+"<div class='so-xia'></div>");
                $("#sellqianshow").show();
                return ;
            }
        }

        if(parseFloat($("#sellnuminput").val())>parseFloat($("#sellnuminput").attr("maxsellnum")))
        {
            $("#sellqianshow").html("卖出数量大于可卖数量！"+"<div class='so-xia'></div>");
            $("#sellqianshow").show();
            return ;
        }
        //恢复默认的设置
        $("#sellqianshow").html("");
        $("#sellqianshow").hide();















        //展示数据
        $("#mingxiname").html($("#insell").val());
        $("#mingxiside").html("卖出");
        $("#mingxitype").html($("#sellOrderType").html());
        $("#mingxinum").html($("#sellnuminput").val());
        if( $("#sellOrderType").attr("dd")==1)
        {
            $("#mingxiall").html("$"+parseFloat($("#sellpriceinput").attr("beginprice")*$("#sellnuminput").val()).toFixed(2));
        }else
        {
            $("#mingxiall").html("$"+parseFloat($("#sellpriceinput").val()*$("#sellnuminput").val()).toFixed(2));
        }

        //计算预估佣金
        var num = parseFloat($("#sellnuminput").val());
        if(num<1.00)
        {
            $("#mingxiyongjin").html("$"+0.99);
        }

        if(398.00>=num  && num>=1.00)
        {

            $("#mingxiyongjin").html("$"+1.99);
        }

        if(num>398.00)
        {
            var sbb =  1.99+($("#sellnuminput").val()-398)*0.005
            $("#mingxiyongjin").html("$"+parseFloat(round2(sbb,2)).toFixed(2));
        }

        $(this).parents(".panes").find(".pane2 .jzq-sure").addClass("jzq-blue");
        $(this).parents(".panes").find(".pane2").stop(false,true).animate({"left":"0"},'500');
    });

    $(".jzq-qu[dd!=2]").on("click",function(){
        $(this).parents(".panes").find(".panes-wraps").stop(false,true).animate({"left":"569"},'500');
    });


    function mingxiqu(){

        $(this).parents(".panes").find(".panes-wraps").stop(false,true).animate({"left":"569"},'500');
    }


    var mubox = null;
    seajs.use('//static.360buyimg.com/finance/financial/common/module/popup/1.0.0/js/popup.js', function(box) {
        mubox = box;
    });

    function qiehuanUs1()
    {
        $(this).parent().find("li").removeClass("curr");
        $(this).addClass("curr");
        $("#bugsh").show();
    }

    function qiehuanUs2()
    {
        $(this).parent().find("li").removeClass("curr");
        $(this).addClass("curr");
        $("#bugsh").hide();
        $("#sellsh").show();
    }

    function hidemaimai()
    {
        mubox.hideLayer( $("#maimaiDiv"));

        $("#usbuyonebtn").removeAttr("style");
        $("#ussellonebtn").removeAttr("style");
    }

    function showThreenew(event)
    {
        $(this).find(".sbleibie").toggle();
        window.event?window.event.cancelBubble=true:event.stopPropagation();
    }


    //展示买卖类别
    function showThree(event)
    {
        $(this).parent().find(".sbleibie").toggle();
//        $(this).parent().find(".sbleibie").show();
        window.event?window.event.cancelBubble=true:event.stopPropagation();

    }
    function showThree2new(event)
    {
        $(this).find(".sbleibie").toggle();
        window.event?window.event.cancelBubble=true:event.stopPropagation();
    }

    function showThree2(event)
    {
        $(this).parent().find(".sbleibie").show();
        window.event?window.event.cancelBubble=true:event.stopPropagation();
    }

    //收起买卖类别(点击类别)=====》有点意思
    function hideThree(event)
    {
        var con = $(this).text();
        var dd = $(this).attr("dd");
        $(this).parent().parent().find(".sel-text").text(con);
        $(this).parent().parent().find(".sel-text").attr("dd",dd)
        if(dd==1)
        {
            $("#buypriceinput").val("市价");
            $("#buypriceinput").attr("disabled","disabled");
            $("#buyPriceMinusBtn").parent().addClass("value-select-hui");
        }else
        {
            //移除禁用属性
            $("#buyPriceMinusBtn").removeAttr("style");
            $("#buyPriceAddBtn").removeAttr("style");
            $("#buypriceinput").removeAttr("disabled");
            $("#buyPriceMinusBtn").parent().removeClass("value-select-hui");
            if(dd==3)//止损订单 要加上0.05
            {
                var ss2 =  round2(parseFloat($("#buypriceinput").attr("beginprice"))+0.05,2);
                $("#buypriceinput").val(ss2);

                $("#buyPriceMinusBtn").parent().parent().find(".so-text").html("触发价≥市价+0.05美元"+"<div class='so-xia'></div>");
                $("#buyPriceMinusBtn").parent().parent().find(".so-text").show();
                var hehesbsb=null;

                clearInterval(hehesbsb);
                hehesbsb =  setInterval(function(){
                    $("#buyPriceMinusBtn").parent().parent().find(".so-text").hide();
                },2000);


            }else
            {
                $("#buyPriceMinusBtn").parent().parent().find(".so-text").hide();
                $("#buypriceinput").val($("#buypriceinput").attr("beginprice"));
            }
        }
        $(".sbleibie").hide();
        window.event?window.event.cancelBubble=true:event.stopPropagation();
    }


    //卖时候的选择订单类型
    function hideThree2(event)
    {
        var con = $(this).text();
        var dd = $(this).attr("dd");
        $(this).parent().parent().find(".sel-text").text(con);
        $(this).parent().parent().find(".sel-text").attr("dd",dd)
        if(dd==1)//市价订单
        {
            $("#sellpriceinput").val("市价");
            $("#sellpriceinput").attr("disabled","disabled");
            $("#sellPriceMinusBtn").parent().addClass("value-select-hui");
        }else
        {
            $("#sellpriceinput").removeAttr("disabled");
            $("#sellPriceMinusBtn").parent().removeClass("value-select-hui");
            if(dd==3)
            {
                var ss2 =  round2(parseFloat($("#sellpriceinput").attr("beginprice"))-0.05,2);
                $("#sellpriceinput").val(ss2);

                $("#sellPriceMinusBtn").parent().parent().find(".so-text").html("触发价≤市价-0.05美元"+"<div class='so-xia'></div>");
                $("#sellPriceMinusBtn").parent().parent().find(".so-text").show();

                var hehesbsb=null;

                clearInterval(hehesbsb);
                hehesbsb =  setInterval(function(){
                    $("#sellPriceMinusBtn").parent().parent().find(".so-text").hide();
                },2000);

            }else
            {
                $("#sellPriceMinusBtn").parent().parent().find(".so-text").hide();
                $("#sellpriceinput").val($("#buypriceinput").attr("beginprice"));
            }
        }
        $(".sbleibie").hide();
        window.event?window.event.cancelBubble=true:event.stopPropagation();

    }


    //卖出价格加
    function sellPriceAdd()
    {
        var price = $("#sellpriceinput").val();
        //判断是不是数字
        if(!checkUsMoney(price))
        {
            $("#sellpriceinput").val($("#sellpriceinput").attr("beginprice"));
            return
        }
        price  =  (parseFloat(price)  + 1.00).toFixed(2) ;
        //委托类型限制的卖出价格的逻辑
        //止损委托 应该低于当前价格005
        var orderType = $("#sellOrderType").attr("dd");
        if(orderType==3)
        {
            $("#sellpriceinput").val($("#sellpriceinput").attr("beginprice"));
            return;
        }
        $("#sellpriceinput").val(price);
    }

    //卖出价格减
    function sellPriceMinus()
    {
        var price = $("#sellpriceinput").val();
        //判断是不是数字
        if(!checkUsMoney(price) ||  price < 1)
        {
            $("#sellpriceinput").val($("#sellpriceinput").attr("beginprice"));
            return
        }
        //委托类型限制的卖出价格的逻辑(放在提交时候去校验)
        price  = (parseFloat(price) -1.00).toFixed(2) ;
        $("#sellpriceinput").val(price);
    }



    //卖出数量增
    var sbhidesell = null;
    function sellNumAdd()
    {
        if ($("#sellnuminput").val() == '')
        {
            $("#sellnuminput").val(1.00);
            return;
        } else
        {
            var oldNum = $("#sellnuminput").val();
            var selltype = $("#sellOrderType").attr("dd");
            //判断是不是数字
            if (!checkUsMaimaiNum(oldNum))
            {
                if (selltype == 1)//市价订单
                {
                    $("#sellnuminput").val(0.01);
                }else
                {
                    $("#sellnuminput").val(1);
                }
                return;
            }
            if (selltype == 1)
            {
                oldNum = (parseFloat(oldNum) + 1.00).toFixed(2);
            }else
            {
                oldNum = (parseFloat(oldNum) + 1.00).toFixed(0);
            }
            var maxSellNum = $("#sellnuminput").attr("maxsellnum");

            if(oldNum>parseFloat(maxSellNum))
            {
                if (selltype == 1)
                {
                    oldNum = parseFloat(maxSellNum).toFixed(2);
                }else
                {
                    oldNum = parseFloat(maxSellNum).toFixed(0);
                }

            }
            $("#sellnuminput").val(oldNum);
            var heheqian=null;
            if (selltype == 1)
            {
                heheqian =round2(oldNum*parseFloat($("#sellpriceinput").attr("beginprice")),2);
            }else
            {
                heheqian =round2(oldNum*parseFloat($("#sellpriceinput").val()),2);
            }

            $("#sellqianshow").html("约"+heheqian +"不含手续费<div class='so-xia'></div>");
            $("#sellqianshow").show();

            clearInterval(sbhidesell);
            sbhidesell =  setInterval(function(){
                $("#sellqianshow").hide();
            },1200);
        }
    }

    //卖出数量减
    var sbhidesell2 = null;
    function sellNumMinus()
    {
        if ($("#sellnuminput").val() == '')
        {
            $("#sellnuminput").val(1.00);
            return;
        } else
        {
            var oldNum = $("#sellnuminput").val();
            var selltype = $("#sellOrderType").attr("dd");
            //判断是不是数字
            if (!checkUsMaimaiNum(oldNum))
            {
                if (selltype == 1)//市价订单
                {
                    $("#sellnuminput").val(0.01);
                }else
                {
                    $("#sellnuminput").val(1);
                }
                return;
            }
            if (selltype == 1)
            {
                oldNum = (parseFloat(oldNum) - 1.00).toFixed(2);
            }else
            {
                oldNum = (parseFloat(oldNum) - 1.00).toFixed(0);
            }

            if(oldNum<=parseFloat(0))
            {
                if (selltype == 1)
                {
                    oldNum = 0.01;
                }else
                {
                    oldNum = 1;
                }
            }

            $("#sellnuminput").val(oldNum);
            var heheqian=null;
            if (selltype == 1)
            {
                 heheqian =round2(oldNum*parseFloat($("#sellpriceinput").attr("beginprice")),2);
            }else
            {
                 heheqian =round2(oldNum*parseFloat($("#sellpriceinput").val()),2);
            }



            $("#sellqianshow").html("约"+heheqian +"不含手续费<div class='so-xia'></div>");
            $("#sellqianshow").show();

            clearInterval(sbhidesell2);
            sbhidesell2 =  setInterval(function(){
                $("#sellqianshow").hide();
            },1200);

        }
    }







    //买入价格加 （要加上关于委托类型的判断）
    function buyPriceAdd()
    {
        var price = $("#buypriceinput").val();
        //判断是不是数字
        if(!checkUsMoney(price))
        {
            $("#buypriceinput").val($("#buypriceinput").attr("beginprice"));
            return
        }
        price  =  (parseFloat(price)  + 1.00).toFixed(2) ;
        $("#buypriceinput").val(price);
    }


    //买入价格减（要加上关于委托类型的判断）
    function buyPriceMinus()
    {
        var price = $("#buypriceinput").val();
        if(!checkUsMoney(price) ||  price < 1)//判断是不是数字
        {
            $("#buypriceinput").val($("#buypriceinput").attr("beginprice"));
            return
        }
        price  = (parseFloat(price) -1.00).toFixed(2);
        if(price < $("#buypriceinput").attr("beginprice"))
        {
            //假如是止损订单不可以低于当前价格
            var orderType = $("#buyOrderType").attr("dd");
            if(orderType==3)
            {
                $("#buypriceinput").val($("#buypriceinput").attr("beginprice"));
                // 可以在警示框提示用户
                return ;
            }
        }
        $("#buypriceinput").val(price);
    }


    var  sbhide=null;
    //买入数量加
    function buyNumAdd()
    {
        if($("#buynuminput").val()=='')
        {
            $("#buynuminput").val(1.00);
            return;
        }else
        {
            var oldNum = $("#buynuminput").val();
            var buytype = $("#buyOrderType").attr("dd");
            //判断是不是数字
            if(!checkUsMaimaiNum(oldNum))
            {
                if(buytype==1)//市价订单
                {
                    $("#buynuminput").val(0.01);
                }else
                {
                    $("#buynuminput").val(0);
                }
                return ;
            }
            if(buytype==1)
            {
                oldNum = (parseFloat(oldNum)+1.00).toFixed(2);
                //不可以大于最大可买 在提交时候检查
//                if(oldNum > parseFloat($("#buynuminput").attr("maxbuynum")))
//                {
//                    oldNum = $("#buynuminput").attr("maxbuynum");
//                }

            }else
            {
                oldNum = (parseFloat(oldNum)+1.00).toFixed(0);
                //不可以大于最大可买 在提交时候检查
//                if(oldNum>parseFloat($("#buynuminput").attr("maxbuynum")))
//                {
//                    oldNum = $("#buynuminput").attr("maxbuynum");
//                }
            }
            $("#buynuminput").val(oldNum);

            var buyqian = null;
            if(buytype==1)
            {
                buyqian =round2(oldNum*parseFloat($("#buypriceinput").attr("beginprice")),2) ;
            }else
            {
                buyqian =round2(oldNum*parseFloat($("#buypriceinput").val()),2) ;
            }
            $("#showbuyqian").html("约"+buyqian +"不含手续费<div class='so-xia'></div>");
            $("#showbuyqian").show();

            clearInterval(sbhide);
            sbhide =  setInterval(function(){
                $("#showbuyqian").hide();
            },1200);



        }
    }

    var  sbhide2=null;
    //买入数量减（加上价格的因素和可用资金这个三者时间的关系）
    function buyNumMinus()
    {
        if($("#buynuminput").val()=='')
        {
            $("#buynuminput").val(1.00);
            return;
        }else
        {
            var oldNum = $("#buynuminput").val();
            var buytype = $("#buyOrderType").attr("dd");
            //判断是不是数字
            if(!checkUsMaimaiNum(oldNum))
            {
                if(buytype==1)//市价订单
                {
                    $("#buynuminput").val(0.01);
                }else
                {
                    $("#buynuminput").val(1);
                }
                return ;
            }
            if(buytype==1)
            {
                oldNum = (parseFloat(oldNum)-1.00).toFixed(2);

            }else
            {
                oldNum = (parseFloat(oldNum)-1.00).toFixed(0);
            }
            if(oldNum<=parseFloat(0))
            {
                if(buytype==1)//市价订单
                {
                    $("#buynuminput").val(0.01);
                }else
                {
                    $("#buynuminput").val(1);
                }
                return ;
            }
            $("#buynuminput").val(oldNum);

            var buyqian = null;
            if(buytype==1)
            {
                buyqian =round2(oldNum*parseFloat($("#buypriceinput").attr("beginprice")),2) ;
            }else
            {
                buyqian =round2(oldNum*parseFloat($("#buypriceinput").val()),2) ;
            }

            $("#showbuyqian").html("约"+buyqian +"不含手续费<div class='so-xia'></div>");
            $("#showbuyqian").show();

            clearInterval(sbhide2);
            sbhide2 =  setInterval(function(){
                $("#showbuyqian").hide();
            },1200);
        }


    }
    //卖出时候的三个按钮
    function sellNumBtn(tt)
    {
        var buytype = $("#sellOrderType").attr("dd");
        if(tt==1)
        {
            if(buytype==1)
            {
                $("#sellnuminput").val(parseFloat($("#sellnuminput").attr("maxsellnum")).toFixed(2))
            }else
            {
                $("#sellnuminput").val(parseFloat($("#sellnuminput").attr("maxsellnum")).toFixed())
            }
            return;
        }


        if(tt==2)
        {
            if(buytype==1)
            {
                $("#sellnuminput").val(parseFloat($("#sellnuminput").attr("maxsellnum")/2).toFixed(2))
            }else
            {
                $("#sellnuminput").val(parseFloat($("#sellnuminput").attr("maxsellnum")/2).toFixed())
            }
            return;
        }


        if(tt==3)
        {
            if(buytype==1)
            {
                $("#sellnuminput").val(parseFloat($("#sellnuminput").attr("maxsellnum")/3).toFixed(2))
            }else
            {
                $("#sellnuminput").val(parseFloat($("#sellnuminput").attr("maxsellnum")/3).toFixed())
            }
            return;
        }

    }

    //全部买卖  一半买卖  三分之一买卖
    function buyNumBtn(tt)
    {


        var buytype = $("#buyOrderType").attr("dd");

        if(buytype!=1)
        {
            if(!checkUsMoney($("#buypriceinput").val()))
            {
                return;
            }
        }

        if(tt==1)
        {
            if(buytype==1)
            {
                var maxBuyNum = (allUseMoney/parseFloat($("#buypriceinput").attr("beginprice"))).toFixed(2);
                $("#buynuminput").val(maxBuyNum);
            }else
            {
                var maxBuyNum = (allUseMoney/$("#buypriceinput").val()).toFixed(0);
                $("#buynuminput").val(maxBuyNum);
            }
            return;
        }

        if(tt==2)
        {
            if(buytype==1)
            {
                var maxBuyNum = (allUseMoney/parseFloat($("#buypriceinput").attr("beginprice"))).toFixed(2);
                $("#buynuminput").val((maxBuyNum/2).toFixed(2));
            }else
            {
                var maxBuyNum = (allUseMoney/$("#buypriceinput").val()).toFixed(0);
                $("#buynuminput").val((maxBuyNum/2).toFixed(0));
            }
            return;
        }

        if(tt==3)
        {
            if(buytype==1)
            {
                var maxBuyNum = (allUseMoney/parseFloat($("#buypriceinput").attr("beginprice"))).toFixed(2);

                $("#buynuminput").val((maxBuyNum/3).toFixed(2));
            }else
            {
                var maxBuyNum = (allUseMoney/$("#buypriceinput").val()).toFixed(0);
                $("#buynuminput").val((maxBuyNum/3).toFixed(0));
            }
            return;
        }
    }


    function checkUsMoney(obj)
    {
        obj = obj.trim();
        if (/^([1-9][\d]{0,}|0)(\.[\d]{1,2})?$/.test(obj))
        {
            return true;
        } else
        {
            return false;
        }
    }



    function checkUsMaimaiNum(obj)
    {
        obj = obj.trim();
        if (/^([1-9][\d]{0,}|0)(\.[\d]{1,4})?$/.test(obj))
        {
            return true;
        } else
        {
            return false;
        }
    }




    // 卖出，弹框显示
    function justSell()
    {

    }

    // 提交账号和密码
    function usmaimai()
    {

        $(this).addClass("z-hui");
        if($(this).attr("tt")==1)
        {
            var  symbol = $("#inbuy").attr("symbol");
            var instrumentID = $("#inbuy").attr("instrumentID");

            var qty = $("#buynuminput").val();
            if($("#buyOrderType").attr("dd")==1)
            {
                var price = $("#buypriceinput").attr("beginprice");
            }else{
                var price = $("#buypriceinput").val();
            }

            $.ajax({
                type:'post',
                dataType:'json',
                url:"/usTrade/buy",
                data:{
                    symbol:symbol,
                    instrumentID:instrumentID,
                    qty:qty,
                    price:price,
                    orderType:$("#buyOrderType").attr("dd")
                },
                success: function (msg)
                {
                    if(msg.code==0)
                    {
                        heheBox.hideLayer($("#maimaiDiv"));
                        heheBox.showLayer($("#weituook"));
                    }else{
                        heheBox.hideLayer($("#maimaiDiv"));
                        heheBox.showLayer($("#weituoerror"));
                        $("#weituoerror").find(".weituoerrormsg").html(msg.msg);
                    }
                    $(this).removeClass("z-hui");
                },
                error:function(msg)
                {
                    $(this).removeClass("z-hui");
                    alert(msg.msg)
                }
            });
        }else
        {
            //卖
            var  symbol = $("#insell").attr("symbol");
            var instrumentID = $("#insell").attr("instrumentID");

            var qty = $("#sellnuminput").val();
            if($("#sellOrderType").attr("dd")==1)
            {
                var price = $("#sellpriceinput").attr("beginprice");
            }else
            {
                var price = $("#sellpriceinput").val();
            }

            $.ajax({
                type:'post',
                dataType:'json',
                url:"/usTrade/sell",
                data:{
                    symbol:symbol,
                    instrumentID:instrumentID,
                    qty:qty,
                    price:price,
                    orderType:$("#sellOrderType").attr("dd")
                },
                success: function (msg)
                {
                    if(msg.code==0)
                    {
                        heheBox.hideLayer($("#maimaiDiv"));
                        heheBox.showLayer($("#weituook"));
                    }else{
                        heheBox.hideLayer($("#maimaiDiv"));
                        heheBox.showLayer($("#weituoerror"));
                        $("#weituoerror").find(".weituoerrormsg").html(msg.msg);
                    }
                    $(this).removeClass("z-hui");
                },
                error:function(msg)
                {
                    $(this).removeClass("z-hui");
                    alert(msg.msg)
                }
            });

        }

    }

    function round2(number,fractionDigits)
    {
        with(Math)
        {
            return round(number*pow(10,fractionDigits))/pow(10,fractionDigits);
        }
    }


    // 个股详情页面的买按钮函数
    function usbuyone(code,code2)
    {

        $("#usbuyonebtn").css("pointer-events","none");

        var insId=null;
        //需要两个参数 insid 可用资金  可买股票
        $.ajax({
            type:'post',
            dataType:'json',
            url:"/usTrade/getInsID",
            data:{
                code:code
            },
            success: function (msg)
            {
                if(msg.code==0)
                {
                    insId=msg.data;
                    $.ajax({
                        type:'post',
                        dataType:'json',
                        url:"/usTrade/getChiCangOne",
                        data:{
                            code:code
                        },
                        success: function (msg)
                        {
                            if(msg.code==0)
                            {
                                var dd = msg.data;
                                var str= new Array();
                                str = dd.split(":");
                                var keyongqian = str[0];
                                var kemaishu = str[1];
                                allUseMoney = keyongqian;

                                usBug(code,code2,insId,kemaishu);
//                                alert(keyongqian);
//                                alert(kemaishu);
                            }else
                            {

                            }
                        },
                        error:function(msg)
                        {

                        }
                    });

                }else
                {
                    console.log(msg.msg);
                }
            },
            error:function(msg)
            {
                alert(msg.msg);
            }
        });
    }


    // 个股详情页面的卖按钮函数
    function ussellone(code,code2)
    {
        //
        $("#ussellonebtn").css("pointer-events","none");

        var insId=null;
        //需要两个参数 insid 可用资金  可买股票
        $.ajax({
            type:'post',
            dataType:'json',
            url:"/usTrade/getInsID",
            data:{
                code:code
            },
