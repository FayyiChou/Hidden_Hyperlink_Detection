                    $.ajax({
                        type:'post',
                        dataType:'json',
                        url:"/usTrade/getChiCangOne",
                        data:{
                            code:code
                        },
                        success: function (msg)
                        {
                            if(msg.code==0)
                            {
                                var dd = msg.data;
                                var str= new Array();
                                str = dd.split(":");
                                var keyongqian = str[0];
                                var kemaishu = str[1];
                                allUseMoney = keyongqian;

                                usSell(code,code2,insId,kemaishu)
//                                usBug(code,code2,insId,kemaishu);
                            }else
                            {

                            }
                        },
                        error:function(msg)
                        {

                        }
                    });

                }else
                {
                    console.log(msg.msg);
                }
            },
            error:function(msg)
            {
                alert(msg.msg);
            }
        });




    }


    function usBug(code,symbol,instrumentID,ss)
    {
        if(code==null || code=='')
            return;

        //把选项切回买入

        qiehuanUs1.call($("#qiehuanmai"));

        //查询股票行情信息
        $.ajax({
            type:"post",
            dataType:'json',
            url:'/usTrade/getUsCurr',
            data:{code:code},
            success:function(msg)
            {
                if(msg.code==0)
                {
                    var da = msg.data;
                    if(da!=null)
                    {

                        $("#inbuy").val(da.name+" "+da.code)

                        //当前价格设置为两位小数
                        if(da.changeRange>0)
                        {
                            $("#hangbuy").removeClass("col-blues");
                            $("#hangbuy").addClass("col-reds");
                        }else
                        {
                            if(da.changeRange==0)
                            {
                                $("#hangbuy").removeClass("col-blues");
                                $("#hangbuy").removeClass("col-reds");
                                $("#hangbuy").addClass("col-greys");
                            }else{
                                $("#hangbuy").removeClass("col-reds");
                                $("#hangbuy").addClass("col-blues");
                            }

                        }
                        $("#hangbuy").html(round2(da.current,2)+"&nbsp;&nbsp;"+da.changeRangeStr)

                        $("#buypriceinput").val(round2(da.current,2))
                        $("#buynuminput").val()

                        //买卖页面都需要进行显示数据
                        $("#insell").val(da.name+" "+da.code);

                        //当前价格设置为两位小数
                        if(da.changeRange>0)
                        {
                            $("#hangsell").removeClass("col-blues");
                            $("#hangsell").addClass("col-reds");
                        }else
                        {
                            if(da.changeRange==0)
                            {
                                $("#hangsell").removeClass("col-blues");
                                $("#hangsell").removeClass("col-reds");
                                $("#hangsell").addClass("col-greys");
                            }else{
                                $("#hangsell").removeClass("col-reds");
                                $("#hangsell").addClass("col-blues");
                            }

                        }


                        $("#hangsell").html(round2(da.current,2)+"&nbsp;&nbsp;"+da.changeRangeStr)
                        $("#sellpriceinput").val(round2(da.current,2))

                        var bp1 = da.buyPrice1==null? '--':da.buyPrice1;
                        var bn1 = da.buyVolume1==null? '--':da.buyVolume1;
                        var sp1 = da.sellPrice1==null? '--':da.sellPrice1;
                        var sn1 = da.sellVolume1==null? '--':da.sellVolume1;
                        //买卖盘信息展示
                        $(".buyp1").each(function () {
                            $(this).html(bp1);
                        });

                        $(".buyn1").each(function () {
                            $(this).html(bn1);
                        });

                        $(".sellp1").each(function () {
                            $(this).html(sp1);
                        });

                        $(".selln1").each(function () {
                            $(this).html(sn1);
                        });

                        $("#sellpriceinput").attr("beginPrice",round2(da.current,2));
                        $("#buypriceinput").attr("beginPrice",round2(da.current,2));


                        //计算可以买数量
                        var canBuyNum = (allUseMoney/round2(da.current,2)).toFixed(0);

                        $("#buynuminput").attr("placeholder","可买:"+canBuyNum);
                        $("#buynuminput").attr("maxBuyNum",canBuyNum);


                        $("#inbuy").attr("symbol",symbol);
                        $("#inbuy").attr("instrumentID",instrumentID);

                        $("#sellnuminput").attr("placeholder","可卖: "+ss);
                        $("#sellnuminput").attr("maxsellnum",ss);//卖出不可以大于最大可卖数量


                        heheBox.showLayer( $("#maimaiDiv"));
                    }
                }
            },
            error: function (msg) {

            }
        });
    }


    function usSell(code,symbol,instrumentID,ss)
    {
        if(code==null || code=='')
            return;
        //查询股票行情信息
        $.ajax({
            type:"post",
            dataType:'json',
            url:'/usTrade/getUsCurr',
            data:{code:code},
            success:function(msg)
            {
                if(msg.code==0)
                {
                    var da = msg.data;
                    if(da!=null)
                    {
                        $("#inbuy").val(da.name+" "+da.code)
                        $("#hangbuy").html(round2(da.current,2)+"&nbsp;&nbsp;"+da.changeRangeStr)
                        $("#buypriceinput").val(round2(da.current,2))
                        $("#buynuminput").val()

                        //买卖页面都需要进行显示数据
                        $("#insell").val(da.name+" "+da.code);
                        $("#hangsell").html(round2(da.current,2)+"&nbsp;&nbsp;"+da.changeRangeStr)
                        $("#sellpriceinput").val(round2(da.current,2))

                        var bp1 = da.buyPrice1==null? '--':da.buyPrice1;
                        var bn1 = da.buyVolume1==null? '--':da.buyVolume1;
                        var sp1 = da.sellPrice1==null? '--':da.sellPrice1;
                        var sn1 = da.sellVolume1==null? '--':da.sellVolume1;
                        //买卖盘信息展示
                        $(".buyp1").each(function () {
                            $(this).html(bp1);
                        });

                        $(".buyn1").each(function () {
                            $(this).html(bn1);
                        });

                        $(".sellp1").each(function () {
                            $(this).html(sp1);
                        });

                        $(".selln1").each(function () {
                            $(this).html(sn1);
                        });

                        $("#sellpriceinput").attr("beginPrice",round2(da.current,2));
                        $("#buypriceinput").attr("beginPrice",round2(da.current,2));

                        //显示可以卖数量

                        $("#sellnuminput").attr("placeholder","可卖: "+ss);
                        $("#sellnuminput").attr("maxsellnum",ss);//卖出不可以大于最大可卖数量


                        //计算可以买数量
                        var canBuyNum = (allUseMoney/round2(da.current,2)).toFixed(0);
                        $("#buynuminput").attr("placeholder","可买:"+canBuyNum);



                        //qiehuanUs2(); 切换 下面为替换
                        $("#qiehuanmai").removeClass("curr");
                        $("#qiehuanmai2").addClass("curr");
                        $("#bugsh").hide();
                        $("#sellsh").show();

                        $("#insell").attr("symbol",symbol);
                        $("#insell").attr("instrumentID",instrumentID);


                        heheBox.showLayer( $("#maimaiDiv"));
                    }
                }
            },
            error: function (msg) {

            }
        });
    }




</script>

