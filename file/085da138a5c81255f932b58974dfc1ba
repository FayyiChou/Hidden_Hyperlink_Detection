<script>
var id = '61004';
var count = $(".count").val();
loadStock(id);
//加入自选
function addStock(code,obj){
	var $div = $(obj);
	$div.removeAttr("onclick");
	var count = $(".count").val();
	count = parseInt(count);
	seajs.use('//static.360buyimg.com/finance/common/unit/login/1.0.0/login.js', function (login) {
        login(function () {
             $.post("/stock/att_stock.html",{"code":code},function(data){
                if(data.result){
				    $("#cancleAttr-btn-temp").show();
				    $("#attr-btn-temp").hide();
				    $("#attr-btn").hide();
					$(".count").val((count+1)+"");
					$("span.att_count").html( (count+1)+"人加自选" );
                }else
                {
                    var OpenCountBox = null;
                    seajs.use('//static.360buyimg.com/finance/financial/common/module/popup/1.0.0/js/popup.js', function(openCountBox) {
                        OpenCountBox = openCountBox;
                    });
                    $("#alertMsgForAllWindowsTitle").html("添加自选失败！")
                    $("#alertMsgForAllWindowsContent").html("*"+data.msg+"!");
                    OpenCountBox.showLayer($("#alertMsgForAllWindows"));
				}
				var cc = "addStock('"+code+"',this)";
				$div.attr("onclick",cc);
             },"jsonp");
        });
    });
}
function cancleAttr(code,obj){
	var $div = $(obj);
	$div.removeAttr("onclick");
	var count = $(".count").val();
	count = parseInt(count);
	if(count==0){
		count = 1;
	}
	seajs.use('//static.360buyimg.com/finance/common/unit/login/1.0.0/login.js', function (login) {
        login(function () {
             $.post("/stock/c_stock.html",{"code":code},function(data){
                if(data.result){
                    $("#cancleAttr-btn-temp").hide();
                    $("#cancleAttr-btn").hide();
				    $("#attr-btn-temp").show();
					$(".count").val((count-1)+"");
					$("span.att_count").html( (count-1)+"人加自选" );
                }
				var cc = "cancleAttr('"+code+"',this)";
				$div.attr("onclick",cc);
    		},"jsonp");
        });
    });
}
/**
 * 热门行业列表
 */
function loadStock(id){
    $.post("/info/getStock.html",{"id":id},function(data){
        $("#stock").html(data);
		$("#attr-btn-temp").hide();
		$("#cancleAttr-btn-temp").hide();

        //var checkIsJdLoginForTrade = $("#checkIsJdLoginForTrade").val();
        if(checkIsJdLoginForTrade == 'false')//没有登录京东账户
        {

            return;
        }
    });
}
