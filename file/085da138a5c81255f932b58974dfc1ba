<script>

        $(function () {
            var re =  readCookie("isShowChat");
            if(re ==  "noShow")
            {
                $('.barrage').css("display","none");
                $('#chatLayout').addClass("noShowChat");
            }else
            {
                $('#chatLayout').removeClass("noShowChat");
            }
            setTimeout(function(){
                $(".ui-sidebar").delegate(".barrage-btn","click", function () {
                    $('.barrage').css("display","block");
                    $(".barrage-bg").first().show();//背景
                    $(".barrage-btn").first().show();//按钮
                    $('#chatLayout').show();
                    createCookie("isShowChat","show","30","/");
                    //在这里修复2个bug
                    //(1)发表弹幕后，要手动移除显示的样式
                    //(2)显示弹幕的特效，不仅仅只有禁用，还要有恢复模式
                });
            },300);

        });
</script>
