<script type="text/javascript">
var options = {path: '/', expires: 10};     //定义路径，过期时间为10天
var cookie_backColor = $.cookie("backColor"); //定义背景颜色值
var cookie_fontColor = $.cookie("fontColor"); //定义字体颜色值
var cookie_fontSize = $.cookie("fontSize");   //定义字体大小

//如果cookie里有背景颜色、字体颜色、字体大小，就加载相应属性。
$(function(){
  if(cookie_backColor){
    $('#kui-page-read').css('background', cookie_backColor);
  }
  if(cookie_fontColor){
    $('#kui-page-read-txt').css('color', cookie_fontColor);
  }
  if(cookie_fontSize){
    $('#kui-page-read-txt').css('font-size', cookie_fontSize + 'px');
  }
});
function backcolor(num){
  if(num == 1){
    $("#kui-page-read").css('background','#ffffff');
    $.cookie("backColor","#ffffff",options);
  }
  if(num == 2){
    $("#kui-page-read").css('background','#e3e8f7');
    $.cookie("backColor","#e3e8f7",options);
  }
  if(num == 3){
    $("#kui-page-read").css('background','#daebfc');
    $.cookie("backColor","#daebfc",options);
  }
  if(num == 4){
    $("#kui-page-read").css('background','#ebeaea');
    $.cookie("backColor","#ebeaea",options);
  }
  if(num == 5){
    $("#kui-page-read").css('background','#e7e3e6');
    $.cookie("backColor","#e7e3e6",options);
  }
  if(num == 6){
    $("#kui-page-read").css('background','#dedcd8');
    $.cookie("backColor","#dedcd8",options);
  }
  if(num == 7){
    $("#kui-page-read").css('background','#d8d7d7');
    $.cookie("backColor","#d8d7d7",options);
  }
  if(num == 8){
    $("#kui-page-read").css('background','#e6fae4');
    $.cookie("backColor","#e6fae4",options);
  }
}
function fontcolor(num){
  if(num == 1){
    $("#kui-page-read-txt").css('color','#000');
    $.cookie("fontColor","#000",options);
  }
  if(num == 2){
    $("#kui-page-read-txt").css('color','#f00');
    $.cookie("fontColor","#f00",options);
  }
  if(num == 3){
    $("#kui-page-read-txt").css('color','#060');
    $.cookie("fontColor","#060",options);
  }
  if(num == 4){
    $("#kui-page-read-txt").css('color','#00f');
    $.cookie("fontColor","#00f",options);
  }
  if(num == 5){
    $("#kui-page-read-txt").css('color','#600');
    $.cookie("fontColor","#600",options);
  }
}
function fontsize(num){
  switch(num){
    case 1:
      $("#kui-page-read-txt").css('font-size','16px');
      $.cookie("fontSize","16",options);
      break;
    case 2:
      $("#kui-page-read-txt").css('font-size','18px');
      $.cookie("fontSize","18",options);
      break;
    case 3:
      $("#kui-page-read-txt").css('font-size','20px');
      $.cookie("fontSize","20",options);
      break;
    case 4:
      $("#kui-page-read-txt").css('font-size','22px');
      $.cookie("fontSize","22",options);
      break;
    case 5:
      $("#kui-page-read-txt").css('font-size','24px');
      $.cookie("fontSize","24",options);
      break;
  }
}
function defaultCL(){
  $("#kui-page-read").css('background','#E9FAFF');
  $.cookie("backColor", "#E9FAFF", options);
  $("#kui-page-read-txt").css('color','#333');
  $.cookie("fontColor", "#333", options);
  $("#kui-page-read-txt").css('font-size','16px');
  $.cookie("fontSize", "16", options);
}
</script>
<script type="text/javascript">
var lastread=new LastRead();lastread.set('3701', '787131', '乱伦系列', ' 第 1 部分阅读');
</script>

</body></html>