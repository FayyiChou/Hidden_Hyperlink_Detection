                            function MarqueeToLeft()
                            {
                                if(demo1.offsetWidth-demo.scrollLeft<=0)
                                {
                                    demo.scrollLeft-=demo1.offsetWidth;
                                }
                                else
                                {
                                    demo.scrollLeft+=speedStep;
