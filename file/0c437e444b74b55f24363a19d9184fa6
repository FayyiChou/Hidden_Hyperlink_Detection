        <script type="text/javascript">
            jQuery(window).load(function(){ //jQuery(window).load() must be used instead of jQuery(document).ready() because of Webkit compatibility                
    
                // News slider > Vertical
                jQuery(".newslider-vertical").sliderkit({
                    shownavitems:3,
                    verticalnav:true,
                    navitemshover:true,
                    circular:true
                });
                
                // News slider > Horizontal
                jQuery(".newslider-horizontal").sliderkit({
                    auto:true,
                    shownavitems:5,
                    panelfx:"sliding",
                    panelfxspeed:1000,
                    panelfxeasing:"easeInOutExpo", //"easeOutExpo", "easeOutCirc", etc.
                    mousewheel:true,
                    keyboard:true,
                    fastchange:false
                });
    
            });    


$('a.all-news').live('click', function(){
          var url=$(this).attr('href');
          var ajax = $('.news-cont:first').clone(true);
          $(this).hide();
          $('#loader').show();//Какой нибудь значек загрузки, его надо будет добавить
          $(ajax).empty().load(url, function(){
               $('#loader').hide();
               $('.news-cont').append($(ajax).html());
          });
 return false;
     });


        </script>
