	<script>
	var select_registration = 0;
	$(function(){
		$(".qlsc_list2 a").each(function() {
			$(this).click(function(){
				if(select_registration == $(this).attr("data-value")){
					$(this).removeClass("active");
					select_registration = 0;
				}else{
					$(".qlsc_list2 a").removeClass("active");
					$(this).addClass("active");
					select_registration = $(this).attr("data-value");
				}
			});
		});
	});
	</script>

