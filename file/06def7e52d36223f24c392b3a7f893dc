     function secBoard107(g,n,l) 
  { 
    for(i=0;i<l;i++) 
    {
      document.getElementById("tablink"+g+"_"+i).className="sec1"; 
    }
    
    document.getElementById("tablink"+g+"_"+n).className="sec2"; 
   var mainTable = document.getElementById("mainTable_"+g); 
   for(i=0;i<mainTable.tBodies.length;i++)
     mainTable.tBodies[i].style.display="none";
    mainTable.tBodies[n].style.display="block";
  } 
