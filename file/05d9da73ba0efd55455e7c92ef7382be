		<!--底部向下靠-->
		<script type="text/javascript">
		    $(function () {
		        var $ft = $('#footer'),
		                minH = $ft.offset().top + $ft.outerHeight();
		
		        function loginResponsive() {
		            var bh = $(window).height();
		            if (bh > minH) {
		                $ft.css({
		                    marginTop: bh - minH
		                });
		            }
		        }
		
		        loginResponsive();
		
		    });
		</script>
