function fsSlider(){
	var _sliderCur = 0,
		_step = 0,
		_slider = _g("fSlide"),
		_sliders = _g("fSlider").getElementsByTagName("a"),
		_slens = _sliders.length,
		_trigger = _g("fTrigger"),
		_triggerIn = '',
		_timer = null;

	for(var i=0;i<_slens;i++){
		if(i==0){
			_triggerIn += '<i class="cur"></i>';
		}else{
			_triggerIn += '<i></i>';
		}
	}
	_trigger.innerHTML = _triggerIn;

	var _triggers = _g("fTrigger").getElementsByTagName("i"),
		_tlens = _triggers.length;

	for(var j=0;j<_tlens;j++){
		addEvent(_triggers[j],"mouseover",(function(n){
			return function(){ _sliderEx(n); };
		})(j));
	}
	function _sliderEx(t){
		_sliders[_sliderCur].className = _triggers[_sliderCur].className = "";
		_sliders[t].className = _triggers[t].className = "cur";
		_sliderCur = t;
	}

	function _paly(){
		_timer = setInterval(function(){
			_step < _slens-1 ? _step += 1 : _step = 0;
			_sliderEx(_step);
		},3000);
	};

	addEvent(_slider, "mouseover", function(){
		clearTimeout(_timer);
	});

	addEvent(_slider, "mouseout", function(){
		_paly()
	});

	_paly();
}
function msSlider(){
	addEvent(_g("msPrev"),"click",function(){ msSliderNav(-1) });
	addEvent(_g("msNext"),"click",function(){ msSliderNav(1) });
	var _sliderCur = 0,
		_step = 0,
		_timer = null,
		_slider = _g("masters"),
		_sliders = _g("msphoto").getElementsByTagName("a"),
		_slens = _sliders.length;
	function msSliderNav(p){
		var _pos = _sliderCur+p;
		if( _pos === -1 ){ _pos = _slens-1 };
		if( _pos === _slens ){ _pos = 0 };
		_sliderEx(_pos)
	}
	function _sliderEx(t){
		_sliders[_sliderCur].className = "";
		_sliders[t].className = "cur";
		_sliderCur = t;
	}
}

var _spd1 = new Date();
</script>
