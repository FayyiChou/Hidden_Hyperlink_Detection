    function setDoubleStr(str){
        var str=str+"";
        if(str.length==1){
          str="0"+str;
        }else{
          str=str;
        }
        return str;
    }
    var date = new Date();
    _year = date.getFullYear(),  _month = date.getMonth() + 1, _day = date.getDate();
    if(location.hash == '' || location.hash == '#' + _year + setDoubleStr(_month) + setDoubleStr(_day)){
        jQuery('.bodan-daylist').show();
        jQuery(window).scrollTop(0);
    }
    </script>
