                    $('#read-more').show().prev().show();
                    console.log('1')
                }
                $('#read-more').click(function(){
                    var auto_H=$('#wrap').height();
                    var max_H=auto_H+100;
                    var RL_H=auto_H-17;
                    if($(this).hasClass('fold')){
                        $(this).parent().animate({'maxHeight':'114px'});
                        $(this).prev().animate({'height':'27px'});
                        $(this).html('展开').removeClass('fold');
                    }else{
                        $(this).parent().animate({'maxHeight':max_H});
                        $(this).prev().css({'height':RL_H});
                        $(this).html('点击隐藏').addClass('fold');
                    }
                })
