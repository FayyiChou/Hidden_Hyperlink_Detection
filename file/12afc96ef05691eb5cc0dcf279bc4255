<script type="text/javascript">
    $("#queryName").keydown(function (evt){
        if(evt.keyCode == 13) {
            queryName();
        }
    });
    function queryName(){
        var queryName=$("#queryName").val();
        var seltUrl="http://lux.cngold.org/product/cosme/";
        var index=seltUrl.indexOf("?");
        if(index!=-1){
            seltUrl=seltUrl.substr(0,index);
        }
        if(queryName){
            seltUrl+="?keyword="+queryName;
        }
        //window.location.href =seltUrl;
        window.open(seltUrl)

    }
</script>

