<script type="text/javascript">

    function loginTime(){
        var b = new Date ();
        var hour = b.getHours();
        if(hour<13){
            $("#cngoldTimeShow").html("上午好");
        }else if(hour>12 && hour <19){
            $("#cngoldTimeShow").html("下午好");
        }else{
            $("#cngoldTimeShow").html("晚上好");
        }
    }
    //退出
     function logout(){
       var initUrl=window.location.href;                               
        var logoutUrl = 'http://passport2.cngold.org/account/logout.htm?service='+initUrl;
        location.href = logoutUrl;
    }

    $(document).ready(function () {
        var user = getCookieLoginNameLux('USER');
        var id = getCookieLoginNameLux('IDENTITY');
        if(id){
            $('#cngoldLoginName').attr('href', "//my.cngold.org/" + id + "/index.htm");
            $("#cngoldLoginName").text(user);
            $('#loginno').hide();
            $('#login').show();
        }else{
            $('#loginno').show();
            $('#login').hide();
             var loginUrl=$("#passportlogin").attr("href");
			var initUrl=window.location.href;
    		loginUrl=loginUrl+"?service="+initUrl;	
			$("#passportlogin").attr("href",loginUrl);  
        }
    });

    function getCookieLoginNameLux(objName){
        var arrStr = document.cookie.split("; ");
        for(var i = 0;i < arrStr.length;i ++){
            var cookieStr = decodeURIComponent(arrStr[i]);
            var temp = cookieStr.split("=");
            if(temp[0] == objName)
                return unescape(temp[1]);
        }
        return "";
    }



</script>

