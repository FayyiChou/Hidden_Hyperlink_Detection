    //问题管理
    function managequest(num,id){
    	id = parseInt(id);
        switch(num){
            case 1:
                $.dialog({
                    id:'renametitle',
                    position:'center',
                    align:'left',
                    fixed:1,
                    width:300,
                    height:100,
                    title:'修改问题标题',
                    fnOk:function(){document.renameForm.submit();$.dialog.close('renametitle')},
                    fnCancel:function(){$.dialog.close('renametitle')},
                    content:'<div class="mainbox"><form name="renameForm"  action="http://help.wps.cn/question/edittitle.html" method="post" ><input type="hidden" value="32635" name="qid"/>问题标题：<br /><input type="text" name="title" value="WPS表格，工具栏总是自动隐藏，不方便" class="txt" size="45" /><input type="hidden" name="fkey" value="bNaDfyfN26eb526adLe18QeDde00cReed7ai9RaM0fa11d1d0de3"/></form></div>'});
                break;
            case 2:
            	var description=$('#mydescription').html();
                $.dialog({
                    id:'editquesDiv',
                    position:'center',
                    align:'left',
                    fixed:1,
                    width:600,
                    title:'修改问题内容',
                    fnOk:function(){editor.getSource();document.editquescontForm.submit();$.dialog.close('editquesDiv')},
                    fnCancel:function(){$.dialog.close('editquesDiv')},
                    content:'<div>标题：WPS表格，工具栏总是自动隐藏，不方便<br /><form name="editquescontForm"  action="http://help.wps.cn/question/editcont.html" method="post" ><textarea id="mc" name="content" style="width: 95%; padding-top: 1px; font-size: 14px;" rows="10" >'+description+'</textarea><input type="hidden"  value="32635" name="qid"/><input type="hidden" name="fkey" value="bNaDfyfN26eb526adLe18QeDde00cReed7ai9RaM0fa11d1d0de3"/></form></div>'
                });
                editor=showeditor("mc");
                break;
            case 2:
                if(!confirm('确定删除问题？该操作不可返回！')==false){
                    document.location.href="http://help.wps.cn/index/default.html";
                }
                break;
            case 3:
                $.dialog({
                    id:'editcatediv',
                    position:'center',
                    align:'left',
                    fixed:1,
                    width:300,
                    height:100,
                    title:'移动分类',
                    fnOk:function(){document.askform.submit();$.dialog.close('editcatediv')},
                    fnCancel:function(){$.dialog.close('editcatediv')},
                    content:'<div class="mainbox">请选择要移动到的分类：<form name="askform" action="http://help.wps.cn/question/movecategory.html" method="post" ><input type="hidden" name="qid" value="32635" /><select name="category" size=1 style="width:240px" ><option value="16">Office</option><option value="3">WPS文字/WORD</option><option value="4">ET表格/EXCEL</option><option value="6">WPP演示/PPT</option><option value="7">互联网</option><option value="14">&nbsp;&nbsp;|--&nbsp;在线模板</option><option value="15">&nbsp;&nbsp;|--&nbsp;在线素材</option><option value="17">移动Office</option></select><input type="hidden" name="fkey" value="bNaDfyfN26eb526adLe18QeDde00cReed7ai9RaM0fa11d1d0de3"/></form></div>'  });
                break;
            case 4:
                if(!confirm('确定关闭该问题?')==false){
                    document.location.href="http://help.wps.cn/question/close/32635/bNaDfyfN26eb526adLe18QeDde00cReed7ai9RaM0fa11d1d0de3.html";
                }
                break;
            case 5:
                document.location.href="http://help.wps.cn/question/recommend/32635/bNaDfyfN26eb526adLe18QeDde00cReed7ai9RaM0fa11d1d0de3.html";
                break;
            case 6:
                if(!confirm('确定删除问题？该操作不可返回！')==false){
                    document.location.href="http://help.wps.cn/question/delete/32635/bNaDfyfN26eb526adLe18QeDde00cReed7ai9RaM0fa11d1d0de3.html";
                }
                break;
            case 7:
            	var url = "http://help.wps.cn/kf/show/32635.html";
            	if(id>0){
            		url = url.substring(0,url.lastIndexOf(".html"))+"/"+id+".html";
            	}
            	document.location.href=url;
                break;
            default:
                alert("非法操作！");
                break;
        }
    }

