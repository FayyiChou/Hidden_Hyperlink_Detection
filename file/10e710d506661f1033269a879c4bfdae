        obj._eng_index = _nindex;
        $('.lnv_rank_detail').css('top', _nindex * 115 - 35);
        $('.lnv_rank_detail_container').empty();
        ajaxTime = $.post('ajax.aspx?m=EnginnerInfo', rel, function (response) {
            if (response.status == 200) {
                obj._packWeixin_s(response);
                $('.lnv_rank_detail_content').show();
                ajaxTime.abort();
            }
        })
    }, function () {
        hoverflag = true;
    })
    $('.rank_chat>li').hover(function () { //ajax hover排行榜
        if (ajaxTime) {
            ajaxTime.abort();
        };
        if (!hoverflag) return false;
