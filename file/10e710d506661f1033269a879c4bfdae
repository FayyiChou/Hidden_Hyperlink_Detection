            //回复按钮点击提交回复内容
            $('.response').click(function () {
                var me = $(this), val = $.trim($(this).siblings('input').val()), _id = $(this).closest('.comment-detail').find('.comment-id').val(), _ul = $(this).closest('.comment-detail').find('.res_info_del_ul'), _num = $(this).closest('.comment-detail').find('.frist_res_i').children('span');
                if (!val) return false;
                $.post('ajax.aspx?m=SubmitUserComment', {
                    comment_id: _id,
                    content: val
                }, function (res) {
                    if (res.status == 200) {
                        Date.prototype.format = function (format) {
                            var o = {
                                "M+": this.getMonth() + 1, //month 
                                "d+": this.getDate(), //day 
                                "h+": this.getHours(), //hour 
                                "m+": this.getMinutes(), //minute 
                                "s+": this.getSeconds(), //second 
                                "q+": Math.floor((this.getMonth() + 3) / 3), //quarter 
                                "S": this.getMilliseconds() //millisecond 
                            }

                            if (/(y+)/.test(format)) {
                                format = format.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
                            }

                            for (var k in o) {
                                if (new RegExp("(" + k + ")").test(format)) {
                                    format = format.replace(RegExp.$1, RegExp.$1.length == 1 ? o[k] : ("00" + o[k]).substr(("" + o[k]).length));
                                }
                            }
                            return format;
                        }
                        usrname = name_encrypt(usrname);
                        _ul.append('<li class="view"><div class="res_obj">' + usrname + '：</div><div class="res_cont_off">' + val + '<i>（' + new Date().format("yyyy/MM/dd hh:mm:ss") + ' ）</i></li>');
                        _num.text(parseInt(_num.text()) + 1);
                        me.siblings('input').val('');
                    }
                    supportflag = true;
                })
            })
            //点击更多回复按钮
            $('.more_res_cont_res_btn').click(function () {
                var _li = $(this).siblings('ul').children('li');
                _li.addClass('view');
                $(this).hide();
            })



            var tipTimeline;
            var chooseflag = true;
            $('.lnv_engineer_btn').click(function () { //ajax点击立即咨询
                var me = $(this), _id = me.siblings('input[name="engineer-id"]').val(), index0 = me.parents('.lnv_engineer').index('.lnv_engineer'), index1 = me.parents('li').index(), obj = lnv_dp.index;
                obj._eng_id = _id;
                if ($('.lnv_engineer_detail')) $('.lnv_engineer_detail').remove();
                var role = me.attr('data-role');
                var rel = { eng_id: _id }; //工程师id
                if (role == 'chat') { //在线咨询
                    $.post('ajax.aspx?m=EnginnerInfo', rel, function (response) {
                        if (response.status == 200) {
                            if (response.eng_from == 2) {
                                if (response.eng_from == 2 && response.eng_status == 0) {
                                    if (response.loginstatus == 0) {
                                        $("#ResponseURL").val(response.eng_chat);
                                        show_login();
                                    }
                                    else {
                                        OpenWindow(response.eng_chat);
                                    }
                                }
                                else {
                                    if (!chooseflag) return false;
                                    chooseflag = false;
                                    obj._packEngContainer(index0, index1);
                                    obj._packEng(response);
                                }
                            }
                        }
                        chooseflag = true;
                    }, 'JSON')
                } else if (role == 'weixin') { //微信咨询
                    if (!chooseflag) return false;
                    chooseflag = false;
                    obj._packEngContainer(index0, index1);
                    $.post('ajax.aspx?m=EnginnerInfo', rel, function (response) {
                        if (response.status == 200) {
                            if (response.eng_from == 0) {
                                obj._packEng(response);
                            }
                        }
                        chooseflag = true;
                    }, 'JSON');
                } else if (role == 'phone') {
                    if (!chooseflag) return false;
                    chooseflag = false;
                    obj._packEngContainer(index0, index1);
                    $.post('ajax.aspx?m=EnginnerInfo', rel, function (response) {
                        if ('0' == '0') {
                        $("#ResponseURL").val("Home.aspx");
                        show_login();
                    }
                    else {
                        /*******测试数据*******/
                        response.rsv_time = ['9:00-10:00', '10:00-11:00', '11:00-12:00', '12:00-13:00', '13:00-14:00', '14:00-15:00', '15:00-16:00', '16:00-17:00', '17:00-18:00'];
                        if (response.status == 200) {
                            if (response.eng_from == 1) {
                                if (response.loginstatus == 0) {
                                    show_login();
                                }
                                else {
                                    var dtconfig = {
                                        isphone: false,
                                        engId: _id,
                                        day: response.rsv_day,
                                        time: response.rsv_time,
                                        onRender: function (html) {
                                            window.phRsvHtml = html;
                                            obj._packEng(response);
                                        }
                                    }
                                    lnv_dp.dt.init(dtconfig);
                                }
                            }
                        }
                        chooseflag = true;
                    }
                }, 'JSON')
                }
        })

    var hoverflag = true, mouseflag = false, hoverTimeline = null, ajaxTime;
    $('.rank_weixin>li').hover(function () { //ajax hover排行榜
        if (ajaxTime) {
            ajaxTime.abort();
        };
        if (!hoverflag) return false;
        hoverflag = false;
        var liobj = $(this);
        var _nindex = liobj.index(), _id = liobj.find('input[name="engineer-id"]').val(), obj = lnv_dp.index;
