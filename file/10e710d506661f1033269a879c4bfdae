            $('.lnv_banner').hover(function () {
                $('#next,#previous').addClass('opacity_50').removeClass('opacity_0');
            }, function () {
                $('#next,#previous').addClass('opacity_0').removeClass('opacity_50');
            })
            $('#next,#previous').hover(function () {
                $(this).addClass('opacity_100').removeClass('opacity_50');
            }, function () {
                $(this).addClass('opacity_50').removeClass('opacity_100');
            })
            $('.lnv_banner_wall').find('a').hover(function () {
                $(this).find('.info').delay(200).animate({ 'top': 0 }, 200);
            }, function () {
                $(this).find('.info').stop(true).animate({ 'top': 71 + 'px' }, 200);
            })
            $('.lnv_engineer_nav>i').click(function () {
                var _index = $(this).index();
                $('.lnv_engineer_nav>i').removeClass('cur').eq(_index).addClass('cur');
                $('.lnv_engineer').hide().eq(_index).fadeIn();
            })

            var _oindex = 0;
            $('.lnv_rank_nav>i').hover(function () {
                var _nindex = $(this).index();
                $('.lnv_rank_nav>i').removeClass('cur').eq(_nindex).addClass('cur');
            }, function () {
                $('.lnv_rank_nav>i').removeClass('cur').eq(_oindex).addClass('cur');
            })
            $('.lnv_rank_nav>i').click(function () {
                var _nindex = $(this).index();
                $('.lnv_rank_nav>i').removeClass('cur').removeClass('active').eq(_nindex).removeClass('gray-img').addClass('cur').addClass('active');
                $('.lnv_rank_nav>i').find('b').addClass('gray-img');
                $('.lnv_rank_nav>i').eq(_nindex).find('b').removeClass('gray-img');
                $('.lnv_rank_sub').hide().eq(_nindex).fadeIn();
                _oindex = _nindex;
            })
            $('.lnv_rank_tab>span').click(function () {
                var _nindex = $(this).index();
                $('.lnv_rank_tab>span').removeClass('cur').eq(_nindex).addClass('cur');
                $('.lnv_rank_sub').animate({ 'margin-left': 0 - _nindex * 326 + 'px' }, 400);
            })

            var supportflag = true;
            $('.dp140509_content_all_down_l_list_td02_zan_a').click(function () { //ajax精彩评论点赞
                var _id = $(this).attr('data-index'), me = $(this);
                var rel = { comment_id: _id };
                if (!supportflag) return false;
                supportflag = false;
                $.post('ajax.aspx?m=SupportUserComment', rel, function (response) {
                    if (response.status == 200) {
                        me.addClass('dp140509_content_all_down_l_list_td02_zan_a_off').removeClass('dp140509_content_all_down_l_list_td02_zan_a');
                        me.find('span').text(response.goodnum);
                    }
                    supportflag = true;
                })
            });


            //回复这个工程师的回复按钮
            //首先判断用户是否登陆，从cookie里查询用户信息

            $('.frist_res').click(function () {
                var reg = new RegExp("(^|=)UserName=([^&]*)(&|$)");
                if (arr = document.cookie.match(reg)) {
                    var _li = $(this).parent().siblings('.res_info_reply'), _input = _li.find('.res_info_del_form');
                    if (_li.is(':visible') && _input.is(':visible')) {
                        _li.hide();
                        _input.hide();
                    } else {
                        _li.show();
                        _input.show();
                    }
                    window.usrname = unescape(arr[2]);
                } else {
                    show_login();
                }
            });
            //回复下面回复的回复按钮点击
