        <script type="text/javascript">
            $(function () {
                var _box = $('.lnv_banner_box');
                _box.append($('.banner_option:first').clone(true));

                var _n = $('.banner_option').length;
                function setW() {
                    _box.width(_n * 1000);
                }
                $(window).resize(function () {
                    setW();
                })
                setW();

                var _ban = $('.lnv_banner'), _page = $('.pager'), _next = $('#next'), _pre = $('#previous'), _i = 1, _timer = null;
                for (var i = 0; i < _n - 1; i++) {
                    if (i != 0) {
                        _page.append('<i></i>');
                    } else {
                        _page.append('<i class="cur"></i>');
                    }
                }
                function play() {
                    _box.stop(true).animate({ 'margin-left': 0 - _i * 1000 + 'px' }, 400, function () {
                        if (_i == _n - 1) {
                            _box.css('margin-left', 0 + 'px');
                            _i = 1;
                        } else {
                            _i++;
                        }
                        _page.find('i').removeClass('cur').eq(_i - 1).addClass('cur');
                    });
                }
                function start() {
                    _timer = setInterval(function () {
                        play();
                    }, 5000);
                }
                _ban.hover(function () {
                    _timer && clearInterval(_timer);
                }, function () {
                    start();
                });
                _next.click(function () {
                    clearInterval(_timer);
                    setTimeout(function () { play() }, 200);
                })
                _pre.click(function () {
                    clearInterval(_timer);
                    if (_i == 1) {
                        _i = _n - 2;
                    } else {
                        _i = _i - 2;
                    }
                    setTimeout(function () { play() }, 200);
                })
                _page.find('i').click(function () {
                    var _p = $(this).index();
                    _i = _p;
                    _page.removeClass('cur');
                    $(this).addClass('cur');
                    clearInterval(_timer);
                    setTimeout(function () { play(); }, 200);
                })
                start();
            })
            function OpenWindow(Url) {
                var xpwidth = window.screen.width - 20;
                var xpheight = window.screen.height - 80;
                window.open(Url, '_blank', 'resizable=yes,directories=no,top=0,left=0,width=' + xpwidth + ',height=' + xpheight);
            }
            $(document).delegate('.chat_queue', 'click', function () {
                var ButtonName = $(this).html();
                var PageName = '点评网站首页';
                $.post('ajax.aspx?m=QueryStatistics', { ButtonName: ButtonName, PageName: PageName })
            })
        </script>
