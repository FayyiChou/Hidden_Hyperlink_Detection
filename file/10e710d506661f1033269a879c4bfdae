<script type="text/javascript">
    $('#keywords').focus(function () {
        if ($(this).val() == $(this).attr('title')) {
            $(this).val('');
        }
    }).blur(function () {
        var _v = $(this).val(), _o = $(this).attr('title');
        if ($.trim(_v) == '') {
            $(this).val(_o);
        }
    }).keyup(function (e) {
        if (e.keyCode == 13) {
            $('#search').trigger('click');
        }
    })
    $('#search').click(function () {
        if ($('#keywords').val() != $('#keywords').attr('title')) {
            location.href = "/Pages/PC/SearchResult.aspx?k=" + encodeURIComponent($('#keywords').val());//提交搜索结果，并跳转
        }
        return false;
    })
</script>

        
