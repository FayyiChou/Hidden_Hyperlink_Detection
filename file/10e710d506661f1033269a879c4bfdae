        <script type="text/javascript">
            $(function () {

                var _nav = $('#nav'), _content = $('.lnv_content'), _nt = _nav.offset().top, _nh = _nav.height();
                $(window).scroll(function () {
                    var _wt = $(this).scrollTop();
                    if (_wt >= _nt) {
                        _nav.addClass('lnv_navsearch1 active').removeClass('lnv_navsearch0');
                        _content.css('margin-top', _nh + 'px');
                    } else {
                        _nav.removeClass('lnv_navsearch1 active').addClass('lnv_navsearch0');
                        _content.css('margin-top', 0);
                    }

                })


                $('#go-normal').click(function () {
                    $('#dp_loginWx').hide();
                    $('#dp_loginId').show();
                })
                $('#go-weixin').click(function () {
                    $('#dp_loginId').hide();
                    $('#dp_loginWx').show();
                })

                $(".dp_login_nav>span").click(function () {

                    var index = $(this).index();
                    switch (index) {
                        case 0: $('#dp_login_content_a').show();
                            $('#dp_login_content_b').hide();
                            break;
                        case 1: $('#dp_login_content_b').show();
                            $('#dp_login_content_a').hide();
                            break;
                    }
                    $(this).css({ "border-color": "#ECECEC", "border-top-color": "#FF5a54" });
                    $(this).siblings("span").css({ "border-color": "#333", "border-top-color": "#ECECEC" });

                })
                $(".dp_loginSuccess_yes").click(function () {
                    window.location.reload(true);
                })

                $("#dp_login_top_input0").focus(function () {
                    $("#dp_login_top_label0").hide();
                })
                $("#dp_login_top_input1").focus(function () {
                    $("#dp_login_top_label1").hide();
                })
                $("#dp_login_top_input2").focus(function () {
                    $("#dp_login_top_label2").hide();
                })
                $("#dp_login_top_input3").focus(function () {
                    $("#dp_login_top_label3").hide();
                })
                $("#dp_login_name").focus(function () {
                    $("#dp_login_top_label4").hide();
                })
                $("#dp_login_phone").focus(function () {
                    $("#dp_login_top_label5").hide();
                })
                $("#dp_login_email").focus(function () {
                    $("#dp_login_top_label6").hide();
                })

                $("#dp_login_name").blur(function () {
                    if (!$(this).val().length)
                        $("#dp_login_top_label4").show();
                })

                $("#dp_login_phone").blur(function () {
                    if (!$(this).val().length)
                        $("#dp_login_top_label5").show();
                })

                $("#dp_login_email").blur(function () {
                    if (!$(this).val().length)
                        $("#dp_login_top_label6").show();
                })

                $("#dp_login_top_input0").blur(function () {
                    if (!$(this).val().length)
                        $("#dp_login_top_label0").show();
                })
                $("#dp_login_top_input1").blur(function () {
                    if (!$(this).val().length)
                        $("#dp_login_top_label1").show();

                })
                $("#dp_login_top_input2").blur(function () {
                    if (!$(this).val().length)
                        $("#dp_login_top_label2").show();
                })
                $("#dp_login_top_input3").blur(function () {
                    if (!$(this).val().length)
                        $("#dp_login_top_label3").show();

                })
            })
    </script>
    </form>



</body></html>