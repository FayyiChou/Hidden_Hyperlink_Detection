            $(function () {
                if ($("html").hasClass("csstransitions")) {
                    $(".live-channel-panel ul li").addClass("tran");
                }
                Imgload.init();
                var tr = 0;
                $(".catalog-tab-more").on("click", function () {
                    if (tr == 0) {
                        $(".ui-catalog-body").addClass("catalog-all");
                        tr++;
                    } else {
                        $(".ui-catalog-body").removeClass("catalog-all");
                        tr--;
                    }
                });
                if ($(".ui-tab .active").position().top > 30) {
                    $(".ui-catalog-body").addClass("catalog-all");
                    tr++;
                }
            });
