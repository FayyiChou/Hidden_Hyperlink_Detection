<script src="/static/home/wgts/js/jquery.SuperSlide.2.1.1.js"></script>
<script>
    $(function () {
        $(".video-play>i").on("click", function () {
            $(this).closest(".video-main").addClass("active")
        })
        $(".banner").addClass("active")

        /**
         * 新闻
         * @param {type} param
         */
        $.ajax({
            url: 'http://news.yuantuan.com/news/apinews?limit=5',
            dataType: "jsonp",
            jsonp: "callback",
            success: function (data) {
                var $ul = '';
                var $_li = '<li class="clear">' +
                        '<a target="_blank" href="{{url}}" class="ds-list-pic fl">' +
                        '<img src="{{thumb}}" alt="" />' +
                        '</a>' +
                        '<div class="ds-list-txt fl">' +
                        '<h3>' +
                        '<a target="_blank"  href="{{url}}">{{title}}</a>' +
                        '</h3>' +
                        '<span>{{created}}</span>' +
                        '</div>' +
                        '</li>';
                var $_div = '<a href="{{url}}" target="_blank"  class="news-ds-pic fl">' +
                        '<img src="{{thumb}}" alt="" />' +
                        '</a>' +
                        '<div class="news-ds-t-txt fr">' +
                        '<h3>' +
                        '<a target="_blank"  href="{{url}}">{{title}}</a>' +
                        '</h3>' +
                        '<p>{{description}}</p>' +
                        '<b>{{created}}</b>' +
                        '<a href="http://news.yuantuan.com" target="_blank" class="district-link-roud db fr">下一篇</a>' +
                        '</div>';
                $_div = $_div.replace('{{url}}', data[0].url);
                $_div = $_div.replace('{{url}}', data[0].url);
                $_div = $_div.replace('{{created}}', data[0].created);
                $_div = $_div.replace('{{title}}', data[0].title);
                $_div = $_div.replace('{{thumb}}', data[0].thumb);
                 $_div = $_div.replace('{{description}}', data[0].description);
                $('.news-ds-top').html($_div);
                delete(data[0]);
                $.each(data, function (i, v) {
                    if (i === 0) {
                        return true;
                    }
                    var _t = '';
                    _t = $_li.replace('{{url}}', v.url);
                    _t = _t.replace('{{url}}', v.url);
                    _t = _t.replace('{{created}}', v.created);
                    _t = _t.replace('{{title}}', v.title);
                    _t = _t.replace('{{thumb}}', v.thumb);
                    $ul += _t;
                });
                $(".news-ds-list").html($ul);
            }
        });

    })
    jQuery(".friends-wrap").slide({
        titCell: ".friends-slider-nav ul",
        mainCell: ".friends-main",
        autoPage: "<li><a></a></li>",
        effect: "leftLoop",
        autoPlay: true
    });
    $(function () {
        var pxX = 0;
        var pxY = 0;
        $(".content02").mousemove(function () {
            getMousePos()
            //console.log(.x;)

        })
        function getMousePos(event) {
            var e = event || window.event;
            if (e.screenX - pxX > 80 || e.screenY - pxY > 80) {
                $(".ytfcloud-ecology").addClass("right");
                $(".ytfcloud-ecology").removeClass("left");
                pxX = e.screenX;
                pxY = e.screenY;
                return false
            } else if (e.screenX - pxX < -80 || e.screenY - pxY < -80) {
                $(".ytfcloud-ecology").addClass("left");
                $(".ytfcloud-ecology").removeClass("right");
                pxX = e.screenX;
                pxY = e.screenY;
                return false
            }

        }
    })

    //换一组
    $(".change-group").click(function () {
        var Timer;
        var iNow = 0

        $(".case-show-list").removeClass("active");
        var str = '<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img01.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img02.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img04.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img03.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img05.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img06.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img07.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img07.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img07.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img07.png" alt="">' +
                '				</a>' +
                '			</li>' +
                '			<li>' +
                '				<a href="javascript:void(0)">' +
                '					<img src="/images/case/case-img07.png" alt="">' +
                '				</a>' +
                '			</li>'


        setTimeout(function () {
            $(".case-show-list").addClass("active").html(str)
            var caseShowLi = $(".case-show-list").find("li");
            Timer = setInterval(function () {
                iNow++
                if (iNow - 1 == caseShowLi.length) {
                    clearInterval(Timer);
                    return false;
                }
                caseShowLi.eq(iNow - 1).attr("class", "case0" + (iNow) + "")
            }, 100);
            $(".case-show-list").addClass("active");

        }, 200)

    });

</script>

