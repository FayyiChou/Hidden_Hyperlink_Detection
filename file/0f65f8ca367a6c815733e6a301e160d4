        // info:{title/img/sum/rate/url/name/value}
        var goods_Info = {
	
            title   : "青岛盈捷投资咨询有限公司",
            img     : "https://daixm.b0.upaiyun.com/logo/Avatar3.png",
            sum     : "",
            rate    : "",
	
            url     : "/qingdao/detail/product/17466367978516/",
            name    : "id",
            value   : "17466367978516"
        }

        var page = window.page || {};

        page.INFO = {
            // Sm: small
            porhead_border_Show_JQ: [".right-section .company", ""],
            SR_ClassN: "searchResults",
            summary: "",
            simple_summary:""
        }

        page.MethodObj = function () { };
        page.MethodObj.prototype = (function () {
            var I = page.INFO;
            var credit, block_time,
                fee_rate = ((goods_Info.rate || 0) / 100).toFixed(5);
            var _input = ".left-section .block1 .demand input";
            var _hideMenu = ".left-section .block1 .hide-menu";

            var waste_lb10214 = function () {
                $(".similar .info-block").mouseenter(function () {
                    $(this).find(".hide-border").show();
                }).mouseleave(function () {
                    $(this).find(".hide-border").stop().hide();
                });
                $(".another-similar .info-block").mouseenter(function () {
                    $(this).find(".hide-border").show();
                }).mouseleave(function () {
                    $(this).find(".hide-border").stop().hide();
                });
            };
            var waste_lb43005 = function () {
                $(".left-section .block1 .hole").height($(".details .left-section .block1").height());
            }
            var waste_lb23008 = function () {
                $.daixiaoMi.common_Text_input_bind(false, function () {
                    $(_hideMenu).hide();
                });

                $("#sum").focus(function () {
                    $(_hideMenu + ":last").hide();
                    $(_hideMenu + ":first").show();
                })
                $("#date").focus(function () {
                    $(_hideMenu + ":first").hide();
                    $(_hideMenu + ":last").show();
                })

                $(_hideMenu + " li").mouseenter(function () {
                    $(this).addClass("fix").siblings().removeClass("fix");
                }).click(function () {
                    $(this).parent().parent().prevAll("input").val($.regexNum($(this).text()));
                    $(this).closest(".hide-menu").hide();
                })
            }

            var waste_lb33590 = function () {
                $(".btn.narrow-btn").click(function () {
                    if($(this).hasClass("disable"))
                        return;
                    var key = goods_Info.name,
                        value = goods_Info.value;

                    $(".btn.narrow-btn").addClass("disable bkg").html("<i></i>");
                    $.daixiaoMi.common_bottom_bar.Api_ajax.sendAjax_add_Goods(key, value, goods_Info, function () {
                        $(".btn.narrow-btn").removeClass("bkg").html("已加入关注");
	                ga('send', 'event', 'follow', 'detail');
                    })
                })
            }

            var waste_lb65004 = function () {
                var arr = [".left-section .block1 .hide-menu li",
                    ".left-section .block1 .hide-menu",
                    ".left-section .block1 .demand input"
                ]
                $.documentClick(arr, function () {
                    $(".left-section .block1 .hide-menu").hide();
                })
            }

            var waste_3395206 = function () {
                var _val = $("#summary i").text();
                if (_val.length>90) {
                    I.summary = _val;
                    I.simple_summary = _val = _val.slice(0, 86) + " ...";
                    $("#summary i").text(_val);
                    $(".showme").css({ "display": "block" });
                }
                $(".showme").click((function () {
                    var sign = true;
                    var text1 = $("#summary .showme").text(),
                        text2 = "点击收起";
                    return function () {
                        if (sign)
                            $("#summary i").addClass("open").text(I.summary).siblings(".showme").text(text2);
                        else
                            $("#summary i").removeClass("open").text(I.simple_summary).siblings(".showme").text(text1);
                        sign = !sign;
                        return false;
                    }
                })())
            }
            
            return {
                attackEvent: function () {
                    // document click Event
                    waste_lb65004();
                    // put hole class element height
                    waste_lb43005();
                    // hide menu Event
                    waste_lb23008();
                    // por-head border show
                    waste_lb10214();
                    // join concern 
                    waste_lb33590();
                    // overlong company summary show & hide
                    waste_3395206();
                    // seecial event bind
                    this.attackSpecialEvent();
                },

                attackSpecialEvent: function () {
                    var that = this;
                    $(_input).blur(function () {
                        that.calculated_amount();
                    })
                    $(_hideMenu).find("li").click(function () {
                        that.calculated_amount();
                    })
                    // attach input attr
                    $("input:text").input_text_autocomplete();
                },

                calculated_amount: function () {
                    credit = parseFloat($("#sum").val()) * 10000;
                    block_time = parseInt($("#date").val());
                    var result = $.daixiaoMi.Calculated_Amount([credit, block_time, fee_rate]);

                    var num1 = parseInt(result[0] / 10) * 10,
                        num2 = parseInt(result[1] / 100) / 100;
                    $(".left-section .block1 .set:first").find(".colorful").text(num1).next().text("利息总额：" + num2 + "万元");
                },

                initial: function () {
                    this.attackEvent();
                    //this.animation_ini();
                    this.calculated_amount();
                }
            };
        })();
        page.iniMethod_Bridge = function () {
            var o = new page.MethodObj();
            o.initial();
        }

        $(function () {
            page.iniMethod_Bridge();
        });
	$.daixiaoMi.navigator.IE6_css("/static/css/homesite/prodetail/ie6.css");
