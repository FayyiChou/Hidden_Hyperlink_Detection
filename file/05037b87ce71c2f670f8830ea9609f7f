<script>

var _index = 0;
var nDay = new Date();
var m = nDay.getMonth();
var d = nDay.getDate();

if(m<=2){
	_index = 0;
}else if(m==3){
   if(d<2){
       _index = 0;
   }else if(d==2 ||d==3 || d==4 || d==5){
	_index = d-2;
   }else{
      _index = 3;
   }
}else{
	_index = 3;
}

//alert(_index);

$('#rcTabs li').removeClass('on');
$('#rcTabs li:eq('+_index+')').addClass('on');
$('#rcBody li').removeClass('on');
$('#rcBody li:eq('+_index+')').addClass('on');

$('#rcTabs li').click(function(i,n){
	$('#rcTabs li').removeClass('on');
	$(this).addClass('on');
	$('#rcBody li').removeClass('on');
	var _index = $(this).index();
	$('#rcBody li:eq('+_index+')').addClass('on');
});
</script>

