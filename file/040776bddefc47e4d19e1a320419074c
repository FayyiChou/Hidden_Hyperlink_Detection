    $('.J_vodeoPlayBtn').click(function(){
        $('.pop-wrap,.pop-video').show();
        videoNum = $(this).attr('data-index')
        $('.pop-video').find('.J_miPlayerBox').miPlayer({
          showTitle: false,
          autoPlay: true,
          width: 855,
          height: 480,
          title: '',
          poster: videoPic[videoNum], //第一桢图片地址
          videos: videoList[videoNum] //视频地址
        });
    })
    $('.J_vodeoPlayClose').click(function(){
      $('.pop-video').find('.J_miPlayerBox').empty();
      $('.pop-wrap,.pop-video').hide();
    })

//广告轮播
$(function(){
        $('#slides_new').slides({
            generatePagination: false,
