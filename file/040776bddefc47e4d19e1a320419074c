  $(".btn_ul").find('li').each(function(i){
    $(this).click(function(){
        $(this).siblings().removeClass('on')
        $(this).addClass('on')
        $(".con-i").eq(i).siblings().hide();
        $(".con-i").eq(i).show();
    });
  });
  
    $(".ren_btn").find('li').each(function(i){
    $(this).click(function(){
        $(this).siblings().removeClass('on')
        $(this).addClass('on')
        $(".con_tent_i").eq(i).siblings().hide().removeClass('on');
        $(".con_tent_i").eq(i).show().addClass('on');
    });
  });
 
//点击播放声音 
  $(".ak_voice_btn").click(function(){
    play_voice(this);
  });

  var play_voice = function(e) {
    var that = e;
    $(".audio_play").remove();
    var voice_name = name = $(that).attr('data-voice');
    var src = 'http://gcg.xoyo.com/hd/yuyue/voice/'+voice_name+'.mp3';

    if(navigator.userAgent.indexOf("Chrome") > -1){
      $(that).append('<audio class = "audio_play" id="myaudio_'+ name +'" src="'+ src +'" controls="controls" hidden="true"></audio>');
      document.getElementById('myaudio_' + name).play();
    }else if(navigator.userAgent.indexOf("Firefox")!=-1){
      $(that).append('<embed class = "audio_play" id="myaudio_'+ name +'" src="'+ src +'" controls="controls" hidden="true" mastersound></embed>');
      document.getElementById('myaudio_' + name).play();
    }else if(navigator.appName.indexOf("Microsoft Internet Explorer")!=-1 && document.all){
      $(that).append('<object classid="clsid:22D6F312-B0F6-11D0-94AB-0080C74C7E95" class = "audio_play"><param name="AutoStart" value="1" /><param name="Src" id="myaudio_'+ name +'" src="'+ src +'"value="'+ src +'"></object>');
      document.getElementById('myaudio_' + name).play();
    }else if(navigator.appName.indexOf("Opera")!=-1){

    }
  }
  //坑
  $(".linkul li")[3].style.marginRight = 0;
