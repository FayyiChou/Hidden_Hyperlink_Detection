							}
							else
							$("#return").html("<div class=bbs_error>" + data + "</div>");
                     submit_disable($id('LeadBBSFm'),1);
                    }
		);



//$.ajax(options); 
return false; 
}); 
} 
);

function resetAnnounce()
{
	edit_clear();
	if($id("ForumNumber"))
	{
		$("#ForumNumber").val("");
		$("#verifycode").attr("src",$("#verifycode").attr("src")+"&1");
	}
}
