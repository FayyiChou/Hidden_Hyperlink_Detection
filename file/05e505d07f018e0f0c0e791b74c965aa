<script>
//记录前前页
var url="http://m.shejiben.com/ask/wenda/247664.html";
var url_act="discussions";
function Writepage(rurl,purl){
        $.ajax({
            url:'http://m.shejiben.com/user/writepage?'+Date.parse(new Date()),
            type:'POST',
            data:{page_url:url,url_act:url_act,rurl:rurl},
            dataType:'html',
            success:function (data){
              if(purl != undefined && purl != ''){
                  location.href=purl;
              }
            }
        });
 }
 Writepage('','');
function Getuid(url){
    $.ajax({
        url:'http://m.shejiben.com/user/checklogin?'+Date.parse(new Date()),
        type:'POST',
        data:{},
        dataType:'html',
        success:function (data){
           if(data == undefined || data == '' || !data){
               var purl='http://m.shejiben.com/user/login?'+Date.parse(new Date());
               Writepage(url,purl);  //记录下一页面地址，再跳去登录  URL 回跳地址  purl 跳转地址
           }else{
               location.href=url;
           }
        }
    });
}

//提问跳转
 jQuery('.go_ask_box').bind('click',function(){
     var url='http://m.shejiben.com/user/submitask';
     Getuid(url);
 });
//回答跳转
jQuery('body').on('click','.answer_reply',function(){
    var id=jQuery(this).attr('pid');
    var toid=jQuery(this).attr('toid');
    var aid=jQuery(this).attr('aid');
    var url='http://m.shejiben.com/user/repqt?type=2&id='+id+'&aid='+aid+'&toid='+toid;
     Getuid(url);
});
//回复跳转
jQuery('.go_answer').bind('click',function(){
    var id=jQuery(this).attr('pid');
    var url='http://m.shejiben.com/user/repqt?type=1&id='+id;
    Getuid(url);
});
</script>
