<script type="text/javascript">
function praise(thisClass,id)
{
    $.post('http://m.shejiben.com/ask/praise', {'id':id}, function(data){
        if(data.replace(/\s*/g, "")=='ok'){
            praiseNum = parseInt($(thisClass).text()) + 1; 
            $(thisClass).parent('.answer-icon').addClass('already-praise');
            $(thisClass).text(praiseNum);
        }else{

            alert(data);
        }
    }, 'text');
}
</script>
