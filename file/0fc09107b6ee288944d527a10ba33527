    <div id="xm_store">
    <ul id="store_list">
                        <li id="pic_0" style="display: block; ">
                    <a href="http://www.miui.com/thread-3512912-1-1.html" target="_blank" onclick="_gaq.push(['_trackEvent', '论坛侧边栏', '配件推荐']);">
                            <img src="http://attach.bbs.miui.com/album/201601/25/162156x2u0tss58kdtepkc.png" alt="论坛APP 2.0 已发布">
                            <span><div align="center">论坛APP 2.0 已发布</div></span>
                    </a>
            </li>
                 <div class="toggle"><a href="javascript:void(0);" id="tog_0" class="current"></a></div></ul>				
    <script type="text/javascript">
        var slideGal = function(id) {
            var obj = document.getElementById(id);

            obj.pics = obj.getElementsByTagName("li");
            obj.pic_num = obj.pics.length;
            obj.pic_cur = 0;

            obj.timer;

            obj.toggles = document.createElement("div");
            obj.toggles.className = "toggle";
            document.getElementById("store_list").appendChild(obj.toggles);

            obj.showPic = function (id) {
                var temp = obj.toggles.getElementsByTagName("a");
                for (i=0;i< temp.length;i++) {
                    temp[i].className = temp[i].id === "tog_" + id?"current":"";
                }

                for (i=0;i< obj.pics.length;i++) {
                    if (obj.pics[i].tagName && obj.pics[i].tagName.toLocaleUpperCase() === "LI") {
                        obj.pics[i].style.display = (obj.pics[i].id === "pic_" + id)?"block":"none";
                    }
                }
                return false
            }

            obj.rollPic = function () {
                clearTimeout(obj.timer);

                if (obj.pic_cur >= obj.pic_num) {
                    obj.pic_cur = 0;
                }

                obj.showPic(obj.pic_cur);

                obj.pic_cur++;

                obj.timer = setTimeout(obj.rollPic, 5000);

            }

            for (i=0;i< obj.pics.length;i++) {
                if (obj.pics[i].tagName && obj.pics[i].tagName.toUpperCase() === "LI") {
                    obj.pics[i].id = "pic_" + i;

                    var toggle = document.createElement("a");
                    toggle.setAttribute("href", "javascript:void(0);");
                    toggle.id = "tog_" + i;
                    if(toggle.addEventListener){
                        toggle.addEventListener("click", function(e) {
                        obj.pic_cur = this.id.replace("tog_", "");
                        obj.rollPic();
                        e.preventDefault();
                                });
                    }else if(toggle.attachEvent) {
                        toggle.attachEvent('onclick', function(e) {
                        var el = e.srcElement || e.target;
                        obj.pic_cur = el.id.replace("tog_", "");
                        obj.rollPic();
                                });
                    }

                    obj.toggles.appendChild(toggle);
                }
            }

            if (obj.pic_num > 0) {
                obj.rollPic();
                obj.onmouseover = function() {clearTimeout(obj.timer)};
                obj.onmouseout  = function() {obj.timer = setTimeout(obj.rollPic, 5000)};
            }
        }
        slideGal("store_list");
      </script>
    </div>
    <div style="clear:both;"></div>
</div>
<div class="miui_bm b_rad b_line b_pad b_bw" id="floow_us_box">
    <div>
        <h2 class="b_t">关注我们</h2>
