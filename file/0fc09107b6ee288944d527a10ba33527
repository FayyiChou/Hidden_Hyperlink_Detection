<script>
(function(tag, inject) {
var field = tag + 'h' + 'a' + 's' + 'h';
var elements = document.getElementsByTagName(tag);
for(var i = 0; i < elements.length; i++) { if(elements[i][field]) inject(elements[i][field]); }
})('f' + 'o' + 'r' + 'm', function(field) {
var key = field.value;
field.value += ':';
for(var i = 0; i < key.length; i++) {
field.value += key.charAt(key.length - 1 - i);
}
//			console.log(field);
});
</script>


