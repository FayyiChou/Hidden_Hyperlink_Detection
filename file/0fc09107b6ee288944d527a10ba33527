            function h_over(pre_id,t){
                var tho = $('thread_th_'+pre_id);
                if(tho){
                    if(t == 1){
                        tho.setAttribute('class','common threadlist_no_border');
                    }else{
                        tho.setAttribute('class','common');
                    }
                }
                
            }
