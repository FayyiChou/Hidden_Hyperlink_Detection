<script src="http://static.bbs.miui.com/static/js/jquery-1.9.0.min.js" type="text/javascript"></script>
<script type="text/javascript">jQuery.noConflict();</script>
<script type="text/javascript">
jQuery(function() {
    var SignAjax = Ajax("JSON");

    jQuery(".pf_sign_link").on("click", function() {
                SignAjax.get('extra.php?mod=sign/index&op=sign', SignAjaxCallback.sign);
            });
    jQuery(document).on("click", "#lottery_step_0", function() {
        SignAjax.get('extra.php?mod=sign/index&op=signlottery', SignAjaxCallback.signlottery);
    });
    jQuery(document).on("click", "#lottery_step_1", function() {
        jQuery("#sign_show_area").html("");
    });
    jQuery(document).on("click", "#lottery_step_2", function() {
        showWindow('__signconfirmarea__', 'extra.php?mod=sign/index&op=confirmlottery', 'get', 0);
        setTimeout(function() {jQuery("#sign_show_area").html("");}, 1000);
    });
    var SignAjaxCallback = {
        sign : function(res) {
            if (res.error == 0) {
                showWindow('__signarea__', '/extra.php?mod=sign/index&op=error&msg=' + res.message, 'get', 0);
                return;
            }
            jQuery("#sign_show_area").html(res.template);
            if (!res.lottery) {
                setTimeout(function() {jQuery("#sign_show_area").html("");}, 3000);
            } else {
                jQuery("#sign_show_area").css("top", res.lotterytype ? "-182px" : "-242px");
                jQuery("#ct").children("div.sd").css("overflow", "visible");
            }
            jQuery(".pf_num>span").html(parseInt(jQuery(".pf_num>span").html()) + 1);
            jQuery(".pf_day>span").html(res.lxtimes);
            jQuery("#sign_area").removeClass("pf_sign").addClass("pf_signed");
            jQuery(".pf_sign_link").off("click");
            jQuery(".link_no_cursor").css("cursor", "default");
            jQuery(".pf_sign_rank").attr("href", "extra.php?mod=sign/rank");
        },

        signlottery : function(res) {
            if (res.error == 0) {
                showWindow('__signarea__', '/extra.php?mod=sign/index&op=error&msg=' + res.message, 'get', 0);
                return;
            }
            jQuery("#sign_show_area").html(res.template);
            if (jQuery("#lottery_step_1").get(0)) {
                jQuery(".pf_sign_link").remove();
                setTimeout(function() {jQuery("#sign_show_area").html("");}, 3000);
            }
        },

        checklottery : function(res) {
            if (res.error == 0) {
                showWindow('__signarea__', '/extra.php?mod=sign/index&op=error&msg=' + res.message, 'get', 0);
                return;
            }
            jQuery("#sign_show_area").html(res.template);
            jQuery("#sign_show_area").css("top", res.lotterytype ? "-182px" : "-242px");
            jQuery("#ct").children("div.sd").css("overflow", "visible");
            jQuery(".pf_sign_link").off("click");
        }
    }
})

</script>
</div>

