                <script type="text/javascript">
            var u_atarget = getcookie('atarget');
            if(u_atarget > 0){$('open_new_window').style.backgroundPosition = '-25px -93px';}else{$('open_new_window').style.backgroundPosition = '0px -93px';}
            _attachEvent($('open_new_window'),'click',function(event){
                var u_atarget = getcookie('atarget');
                if(u_atarget > 0){
                    $('open_new_window').style.backgroundPosition = '0px -93px';
                    setcookie('atarget', -1, 2592000);
                }else{
                    $('open_new_window').style.backgroundPosition = '-25px -93px';
                    setcookie('atarget', 1, 2592000);
                }
            });
