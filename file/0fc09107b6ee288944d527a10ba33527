        // clear first
        $('phone_device').setAttribute('selecti', 0);
        $('phone_device').options[0].value = 0;
        $('phone_device_ctrl').innerHTML = '手机型号';


        var brand = $('phone_brands').getAttribute('selecti');
        var ul = document.createElement('ul');
        
        if ( brand > 0 ) {
            selectId = 'phone_device';
            for (var i=1; i<_phones[brand].length; i++) {
                if (! isUndefined(_phones[brand][i]['pname'])) {
                    var li = document.createElement('li');
                    li.innerHTML = _phones[brand][i]['pname'];
                    li.k_id =  _phones[brand][i]['pid'];
                    li.k_value =  _phones[brand][i]['pid'];
                    if ($(selectId).getAttribute('selecti') == li.k_id) {
                        li.className = 'current';
                    }
                    li.onclick = function() {
                                                var lis = ul.getElementsByTagName('li');
                        for (var j=0; j<lis.length; j++) {
                            if (lis[j].className != '') {
                                lis[j].className = '';break;
                            }
                        }
                        //li.className = 'current';
                        $(selectId + '_ctrl').innerHTML = this.innerHTML;
                        $(selectId).setAttribute('selecti', this.k_id);
                        $(selectId).options.length = 0;
                        $(selectId).options[0] = new Option('', this.k_value);
                        eval($(selectId).getAttribute('change'));
                        //hideMenu(menuObj.id);
                        hideMenu(ul.parentNode.id);
                        return false;
                    };
                    ul.appendChild(li);
                }
           }
        }

        $('phone_device_ctrl_menu').innerHTML = '';
        $('phone_device_ctrl_menu').appendChild(ul);

    },

    setPhone : function() {
        var device = parseInt($('phone_device').value);
        var rsyncphone = 0;
                showWindow('phone_set_btn', "/forum.php?mod=ajax&action=setphone&device="+device+"&rsyncphone="+rsyncphone, 'get', 0);
    },

    changePhone : function(brand, device,t) {
        if(t == 1){
            display("set_p_box");
            display("set_c_box");
        }else{
            display("set_c_box");
            display("phone_info_box");
        }
    }
    
}

if($('rsync_true_device')){
_attachEvent($('rsync_true_device'),'click',function(event){
    if($('rsync_true_device').getAttribute('rsyn') == 1){
$('rsync_true_device').style.backgroundPosition = '-25px -93px';
$('rsync_true_device').setAttribute('rsyn','0');
$('phone_brands_ctrl').style.color = '#ccc';
$('phone_device_ctrl').style.color = '#ccc';
$('fd_miui_bg').style.display = 'block';
    }else{
$('rsync_true_device').style.backgroundPosition = '0px -93px';
$('rsync_true_device').setAttribute('rsyn','1');
$('phone_brands_ctrl').style.color = '#666666';
$('phone_device_ctrl').style.color = '#666666';
$('fd_miui_bg').style.display = 'none';
    }
});
}

_attachEvent($('weixin_title'), 'click', function (event) {
    var t = $('weixin_logo');
    var te = $('weixin_title');
    event.stopPropagation();
    if(t.style.display != 'block'){
        t.style.display = 'block';
        te.style.fontWeight = 'bold';
        te.style.color = '#4c4c4c';
    }else{
        t.style.display = 'none';
        te.style.fontWeight = 'normal';
        te.style.color = '#909090';
    }
});
if($('nv_forum')){
_attachEvent($('nv_forum'),'click',function(){
    var t = $('weixin_logo');
    var te = $('weixin_title');
    if(t.style.display == 'block'){
        t.style.display = 'none';
        te.style.fontWeight = 'normal';
    }
    
    if(parseInt($('phone_brands').value) > 0){
        $('phone_brands_ctrl').style.border = '0px';
    }
    if(parseInt($('phone_device').value) > 0){
        $('phone_device_ctrl').style.border='0px';
    }
});
}
_attachEvent($('floow_us_box'),'click',function(){
    event.stopPropagation();
});
var star_p = 0;
function getStarUser(){
    if(star_p>6)star_p = 0;
    var x = new Ajax();
    x.recvType = 'HTML';
    x.get('forum.php?mod=ajax&action=starusers&p='+star_p+'&fid=',function(s,x){
if(s){
$('star_list').innerHTML = s;
}else{
$('star_block').style.display = 'none';
}
    });
    star_p++;
}
getStarUser();
//_attachEvent($('weixin_title'), 'mouseout', function () {$('weixin_logo').style.display='none'; });
//_attachEvent($('weixin_logo'), 'mouseover', function () {$('weixin_logo').style.display='block';});
//_attachEvent($('weixin_logo'), 'mouseout', function () {$('weixin_logo').style.display='none'; });
_attachEvent($('sider_video'),'click',function(){
var screenHeight = screenWidth = 0;
if(document.documentElement.clientHeight){
screenHeight = document.documentElement.clientHeight;
screenWidth = document.documentElement.clientWidth;
}else{
screenWidth = document.body.clientWidth;
screenHeight = document.body.clientHeight;
}
showDialogVideo();
});
function showDialogVideo(){
var screenHeight = screenWidth = 0;
var video_src = "<embed width='700' height='480' wmode='opaque' type='application/x-shockwave-flash' src='http://static.video.qq.com/TPout.swf?vid=q01247uzeig&auto=1' allowscriptaccess='sameDomain' allowfullscreen='true'>";
if(document.documentElement.clientHeight){
screenHeight = document.documentElement.clientHeight;
screenWidth = document.documentElement.clientWidth;
}else{
screenWidth = document.body.clientWidth;
screenHeight = document.body.clientHeight;
}
$("z_index_dialog").style.display='block';
var content_width = screenWidth/2-350;
$("dialog_content").style.left=content_width+'px';
$("videocontent").innerHTML=video_src;
$("dialog_content").style.display='block';
_attachEvent($('dialog_close'),'click',dialogClose);
}
function dialogClose(){
$("z_index_dialog").style.display='none';
$("dialog_content").style.display='none';
$("videocontent").innerHTML='';
}


</script>

