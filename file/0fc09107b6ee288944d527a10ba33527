<script>

_attachEvent($('fans_station_switch'), 'click', fans_station_click);
function fans_station_click(){						
if($('fans_list_retract').style.display == 'none'){
fans_station_switch(0);
setcookie('fans_station_display', 0, 2592000);
}else{
fans_station_switch(1);
setcookie('fans_station_display', 1, 2592000);
}
}
function fans_station_switch(type){
var ps = $('fans_station_ps');
var list_div = $('fans_list');
var list_div_retract = $('fans_list_retract');
if(type==1){
list_div_retract.style.display = 'none';
list_div.style.display = 'block';
$('fans_station_switch').style.backgroundPosition = '-220px -84px';
ps.style.display = 'block';
$('miui_fans_station').style.paddingBottom = '25px';
$('fans_station_switch').title='收起';
}else if(type==0){
list_div.style.display = 'none';
list_div_retract.style.display = 'block';
ps.style.display = 'none';
$('fans_station_switch').style.backgroundPosition = '-183px -84px';
$('miui_fans_station').style.paddingBottom = '0px';
$('nv_forum').style.height = '100%';
$('fans_station_switch').title='展开';
}
}
var fans_station_display = getcookie('fans_station_display');
if(fans_station_display == 1){
fans_station_switch(1);
}
</script>
	
