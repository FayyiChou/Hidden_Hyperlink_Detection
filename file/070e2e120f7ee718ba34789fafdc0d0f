<script type="text/javascript">
	var _pMethod = {
	    $: function() {
	        var elements = new Array();
	        for (var i = 0; i < arguments.length; i++) {
	            var element = arguments[i];
	            if (typeof element == 'string') element = document.getElementById(element);
	            if (arguments.length == 1) return element;
	            elements.push(element);
	        }
	        return elements;
	    },
	    addEvent: function(node, type, listener) {
	        if (!(node = _pMethod.$(node))) {
	            return false;
	        }
	        if (node.addEventListener) {
	            node.addEventListener(type, listener, false);
	            return true;
	        } else if (node.attachEvent) {
	            node['e' + type + listener] = listener;
	            node[type + listener] = function() {
	                node['e' + type + listener](window.event);
	            }
	            node.attachEvent('on' + type, node[type + listener]);
	            return true;
	        }
	        return false;
	    },
	    getNextSibling: function(node) {
	        //获取下个兄弟节点
	        if (!(node = _pMethod.$(node))) {
	            return false;
	        }
	        if (node.nextSibling.nodeType ==3) {
	            return _pMethod.getNextSibling(node.nextSibling);
	        } else {
	            return node.nextSibling;
	        }
	    },
	    getClassNames: function(element) {
                //取得包含元素类名的数组
                if (!(element = _pMethod.$(element))) return false;
                return element.className.replace(/\s+/, ' ').split(' ');
        },
	    addClassName: function(element, className) {
	        //为元素添加类            
	        if (!(element = _pMethod.$(element))) return false;
	        element.className += (element.className ? ' ' : '') + className;
	        return true;
	    },
	    removeClassName: function(element, className) {
	        //从元素中删除类
	        if (!(element = _pMethod.$(element))) return false;
	        var classes = _pMethod.getClassNames(element);
	        var length = classes.length
	        for (var i = length - 1; i >= 0; i--) {
	            if (classes[i] === className) {
	                delete(classes[i]);
	            }
	        }
	        //由于在getClassNames中移除了空格 取得了数组 所以要重新用空格分隔
	        element.className = classes.join(' ');
	        return (length == classes.length ? false : true);
	    },
	    stopPropagation: function(eventObject) {
	        //阻止事件冒泡
	        eventObject = eventObject || window.event;
	        if (eventObject.stopPropagation) eventObject.stopPropagation();
	        else eventObject.cancelBubble = true;
	    },
	    getElementsByClassName: function(className, tag, parent) {
	        parent = parent || document;
	        if (!(parent = _pMethod.$(parent))) {
	            return false;
	        }
	        var allTags = (tag == "*" && parent.all) ? parent.all : parent.getElementsByTagName(tag);
	        var matchingElements = new Array();
	        className = className.replace(/-/g, "-");
	        var regex = new RegExp("(^|s)" + className + "(s|$)");
	        var element;
	        for (var i = 0; i < allTags.length; i++) {
	            element = allTags[i];
	            if (regex.test(element.className)) matchingElements.push(element);
	        }
	        return matchingElements;
	    }
	};
	(function(){
		/**
		 * 显示楼盘证书
		 * @param  {Object} yszBtns 证书小图片元素
		 * @return {undefined}         
		 */
		function showYSZ(yszBtns) {
			for (var i = 0, il = yszBtns.length; i < il; i++) {
				(function(J) {
					_pMethod.addEvent(J, 'mouseenter', function(e) {
						var nextnode = _pMethod.getNextSibling(J)
						_pMethod.removeClassName(nextnode, 'none');
						_pMethod.stopPropagation(e);
					});
					_pMethod.addEvent(J.parentNode, 'mouseleave', function(e) {
						_pMethod.addClassName(_pMethod.getNextSibling(J), 'none');
						_pMethod.stopPropagation(e);
					});
				})(yszBtns[i]);
			}
		}
		showYSZ(_pMethod.getElementsByClassName('yszIcon', 'i', _pMethod.$('tuijian')));
	})()
</script>
