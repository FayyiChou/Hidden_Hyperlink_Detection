var demos= 8;
var maininitialized= false;
var indemofunc= false;
var timedemo;

function changelang(i, sw) {
	var p;

	p= document["ilang"+ i];
	p.src= (!sw ? plang[i-1].src:plangover[i-1].src);
	}

function langover(i) {
	changelang(i, 1);
	}

function langout(i) {
	changelang(i, 0);
	}

function timedemonext() {
	if(maininitialized) {
		if(!indemofunc) {
			indemofunc= true;
			if(timedemo) clearTimeout(timedemo);
			if(++pdemono>= demos) pdemono= 1;
			switch(jvsRunway()) {
				case 3:
				case 1:
					pdemo.src= pdemos[pdemono].src;
					break;

				case 2:
					pdemo.filters.blendTrans.Apply();
					pdemo.src= pdemos[pdemono].src;
					pdemo.filters.blendTrans.Play();
					break;
				}


			timedemo= setTimeout("timedemonext()", 5000);
			indemofunc= false;
			}
		}
	}

function timedemonext2() {
//	focus();
	timedemonext();
	}

function main() {
	pdemos= new Array();
	for(i=1; i<=demos; i++) {
		pdemos[i]= new Image();
		pdemos[i].src= "image/hellodemo"+ i+ ".jpg";
		}
	pdemono= 0;
	pdemo= document["idemo"];
	maininitialized= true;

	timedemonext();
	}
