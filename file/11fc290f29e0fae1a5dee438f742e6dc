      function AutoScroll(obj){
        var lineH = $(obj).find("li:first").height();
        $(obj).animate({
          marginTop:"-"+lineH+"px"
        },500,function(){
          $(this).css({marginTop:"0px"}).find("li:first").appendTo(this);
        });
      }

