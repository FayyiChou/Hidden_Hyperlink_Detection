        var speed3=30//速度数值越大速度越慢
		var str72 = document.getElementById("str72");
		var str71 = document.getElementById("str71");
		var str7 = document.getElementById("str7");
        str72.innerHTML=str71.innerHTML
        function Marquee(){
        if(str72.offsetWidth-str7.scrollLeft<=0)
        str7.scrollLeft-=str71.offsetWidth
        else{
        str7.scrollLeft++
        }
        }
        var MyMar=setInterval(Marquee,speed3)
        str7.onmouseover=function() {clearInterval(MyMar)}
