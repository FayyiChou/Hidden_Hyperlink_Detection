function selectIt(){
	var oSelect=document.getElementById('sgid'), val=Request('sgid');
	for(var i=0,len=oSelect.options.length; i<len; i++){
		if(oSelect.options[i].value == val){oSelect.options[i].selected = true; break;}
	}
}
