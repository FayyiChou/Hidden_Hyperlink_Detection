<script>
	var speed=20;
	var GD52649=document.getElementById('demo52649'); 
	var GD526491=document.getElementById('demo526491');
	var GD526492=document.getElementById('demo526492');
	GD526492.innerHTML=GD526491.innerHTML;
	function Marquee52649(){
		if(GD526492.offsetWidth-GD52649.scrollLeft<=0){
			GD52649.scrollLeft-=GD526491.offsetWidth;
		}else{
			GD52649.scrollLeft++;
		}
	}
	var MyMar52649=setInterval(Marquee52649,speed);
