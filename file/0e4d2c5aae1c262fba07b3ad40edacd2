<script>
$(function(){
	var direc = '1';
	if(direc=='1'){
	if($('#demo20750 #boxNews').length==0) {
		setTimeout(function(){ 
				$('#88598406a3a4acb55beaa9984390517a').vTicker({speed:500,pause:2000,showItems:5});
		}, 1000);
	}
	}else if(direc=='2'){ 
		setTimeout(function(){
			var boxNewsList = $('#demo20750 #boxNewsList');
			boxNewsList.width('10000px');
			var i = $('li', boxNewsList).length;
			var li = $('li', boxNewsList);
			var liWidth = li.width();
			var width = i * liWidth + 20;
			li.first().attr('first', '1');

			var cloneli = $('li', boxNewsList).clone();
			boxNewsList.append(cloneli);
			

			boxNewsList.queue(move());
			boxNewsList.queue(move());

			$('li', boxNewsList).mousemove(function(event) {
				boxNewsList.stop();
			}).mouseout(function(event) {
				boxNewsList.queue(move());
				boxNewsList.queue(move());
			});;

			function move(){
				var j = (i - Math.abs(parseInt(boxNewsList.css('margin-left')))/liWidth) * 2000;
				j = Math.max(j, 2000);
				boxNewsList.animate({'margin-left': '-'+width+'px'},j,function(){
					boxNewsList.append($('li[first=1]:last', boxNewsList).prevAll('li')).css('margin-left', '-20px');
					boxNewsList.queue(move());
				});
			}
		}, 1000);
	}
});
</script>

