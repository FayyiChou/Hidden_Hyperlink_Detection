<script type="text/javascript">
    $(function () {
        $("#lis1").click(function () {
            for (i = 1; i <= 3; i++) {
                $("#message" + i).hide();
            }
            $("#message1").show();
        });
        $("#lis2").click(function () {
            for (i = 1; i <= 3; i++) {
                $("#message" + i).hide();
            }
            $("#message2").show();
        });
        $("#lis3").click(function () {
            for (i = 1; i <= 3; i++) {
                $("#message" + i).hide();
            }
            $("#message3").show();
        });

        $("#submit_btn").click(function () {
            $("#dv1").css("display", "none");
            $("#feedback_success").css("display", "block");
        })
        $("#feedback_agin").click(function () {
            $("#dv1").css("display", "block");
            $("#feedback_success").css("display", "none");
        })
        for (var i = 1; i <= 3; i++) {
            $("#message" + i).focus(function () {
                $(this).val("");
            });
        }
        $(".number").focus(function () {
            $(this).val("");
        });
        $(".tag li").click(function () {
            $(this).parent().find("li").removeClass("on");
            $(this).addClass("on");
        });
        $("#submit_btn").click(function () {
            var Email = $("#contact").val();
            var styleContent = $("#message1").val();
            var moduleContent = $("#message2").val();
            var bugContent = $("#message3").val();
            var Satisfaction = $(".content input[name='score']:checked").val();
            $.ajax({
                type: "Post",
                url: "ashx/advice.ashx",
                data: { "Satisfaction": Satisfaction, "Email": Email,
                    "styleContent": styleContent, "moduleContent": moduleContent, "bugContent": bugContent
                },
                success: function (data) {
                    alert(data);
                },
                error: function (data) {
                    alert(data);
                }
            });
        });

    });
    function ShowDIV(thisObjID) {
        $("#BgDiv").css({ display: "block", height: $(document).height() });
        var yscroll = document.documentElement.scrollTop;
        // $("#" + thisObjID).css("top", "150px");
        $("#" + thisObjID).css("display", "block");
        document.documentElement.scrollTop = 0;

    }
    function closeDiv(thisObjID, obj) {
        if (obj == "ok") {
            $("#BgDiv").css("display", "none");
            $("#" + thisObjID).css("display", "none");
        }
        else {
            window.location.href = "../Index2.aspx";
        }
    }
</script>


