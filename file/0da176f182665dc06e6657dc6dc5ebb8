                            var curJh = $(this).parent().parent().prev().find("a");
                            var chrJhtext = curJh.text();
                            var jihuaCount = chrJhtext.substring(chrJhtext.lastIndexOf('(') + 1, chrJhtext.lastIndexOf(')')); //获取计划的个数
                            
