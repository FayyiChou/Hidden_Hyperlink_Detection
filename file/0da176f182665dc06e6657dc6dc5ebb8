    <script type="text/javascript">
        /*遮罩div js*/
        var popupStatus = 0;
        var $place = function (selector) { //目标居中
            var $t = 0;
            var $l = 0;
            $t = ($(window).height() - $(selector).height()) / 2 + $(document).scrollTop();
            $l = ($(window).width() - $(selector).width()) / 2;
            $(selector).css({ "left": $l + "px", "top": $t + "px" });
            $("#screen").css({ "height": $("body").height() + "px" });
        };
        $(function () {
            var isProfessional=0;
            if(isProfessional==1){
                $("#mlinkid").click(function() {
                    showdiv();
                    return false;
                })
            }

            $("#FeeReading").click(function () {
                showdiv();
            });
            //取消
            $("#btnquxiao").click(function () {
                //仅在开启标志popupStatus为1的情况下去除     
                if (popupStatus == 1) {
                    $("#screen").fadeOut("slow");
                    $("#showdiv").fadeOut("slow");
                    popupStatus = 0;
                }
            });
        });
        function showdiv() {
            $place($("#showdiv"));
            $(window).resize(function () { $place($("showdiv")); }) //调整浏览器大小 
            loadPopup();
        }
        function loadPopup() {
            //仅在开启标志popupStatus为0的情况下加载 
            if (popupStatus == 0) {
                $("#screen").css({
                    "opacity": "0.5"
                });
                $("#screen").fadeIn("slow");

                $("#showdiv").fadeIn("slow");

                $("body").css({ "overflow": "hidden" });

                popupStatus = 1;
            }
        }
    </script>
    
