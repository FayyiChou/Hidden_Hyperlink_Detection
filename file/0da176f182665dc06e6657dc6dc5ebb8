 <script type="text/javascript">
        //nzl 9-46读取版权，备注
        $(function () {
            var xmlhttp;
            var txt, val, node, i, j;
            txt = "";
            $.ajax({
                url: "../../UserControl/XMLSource/XMLfooter.xml",
                dataType: 'xml',
                type: 'GET',
                timeout: 2000,
                error: function (xmlhttp) {
                    //                val=$(this).attr("NAME");
                    ;

                },
                success: function (xmlhttp) {
                    $(xmlhttp).find("NODE").each(function (i) {
                        val = $(this).children("DISPLAY").text();
                       // alert(xmlhttp.length)
                        if (val != "换行") {
                          
                            if (val != "不显示")
                                txt += val + "：" + $(this).children("VALUE").text();
                            else
                                txt += $(this).children("VALUE").text();
                            txt += "&nbsp;&nbsp;&nbsp;&nbsp;";
                           
                        }
                        else
                            txt += "<br/>";

                    });
//                      alert(txt);
//                  if(txt!=null)
                    document.getElementById("txtfooter").innerHTML = txt;
                }
            });
           
            
           
        });
    </script>
