    <script type="text/javascript">
        function unuserFun() {
            var section = '湖南';
            if (confirm("是否申请" + section + "正式用户")) {

                $.post("ashx/casInsertUser.ashx", { action: "islogin" }, function (data) {

                    var da = eval(data);
                    if (da > 0) {
                        window.location.reload();
                    }
                });
            }
        }
        //cas登录地址
        var casServerLoginUrl = 'http://tyrz.nstrs.cn/login';
        //本地默认地址
        var defaultUrl = 'http://www.hnstrs.cn/Professional.aspx?flag=r';
        var casServerUrlPrefix = 'http://tyrz.nstrs.cn/';
        var currentUser = ''; //alert(currentUser);
        var IsCasVerify = 'true';

        var userType = "dz";
        $(function () {
            var currentUrl = document.URL;
            var url = "http://" + "www.hnstrs.cn:80" + "/ashx/header.ashx";
            var zhongxinurl = "http://" + "www.hnstrs.cn:80" + "/User/MemberCenter.aspx";
            if (IsCasVerify == "true") {
                $("#login").attr('href', casServerLoginUrl + '?service=' + defaultUrl);
                $("#login").attr('onclick', '');
            }
            else {
                $("#login").attr('onclick', 'loginByLocation()');
                $("#login").attr('href', 'javascript:void(0)');
            }
            posthead();
            $("#logout").click(function () {
                $.post("../ashx/header.ashx", { action: "logout" }, function (data) {
                    if (data == "1") {
                        //alert("退出日志记录完毕");
                        if (IsCasVerify == "true") {
                            //cas地址

                            window.location.href = casServerUrlPrefix + "/logout?service=" + defaultUrl;

                        } else {
                            window.location.href = defaultUrl;
                        }
                    }
                });
            });

        });

        function posthead() {
        ///
            var currentUrl = document.URL;
            var url = "http://" + "www.hnstrs.cn:80" + "/ashx/header.ashx";
            var zhongxinurl = "http://" + "www.hnstrs.cn:80" + "/User/MemberCenter.aspx";
            $.post("../ashx/header.ashx", { action: "islogin" }, function (data) {
                var d = eval("(" + data + ")");
                if (d.code == "1" || d.code == "3") {
                    $("#login").hide();
                    $("#register").hide();
                    $("#LoginMsg").show();
                }
                if (d.pd == 1) {
                    if (d.code == "1") {
                        $("#MenberCenter").show();
                        $("#message").html("欢迎您<a href='" + zhongxinurl + "'>" + d.name + "</a>!");
                    } else if (d.code == "3") {
                        $("#MenberCenter").hide();
                        $("#message").html("欢迎您&nbsp&nbsp&nbsp&nbsp" + d.name + "<a  href='" + "" + "' style=' cursor: pointer;'>本地审核未通过用户</a>");
                    }

                    if (d.isAdmin == "True") {
                        $("#userMrg").show();
                    }

                } else {
                    $("#MenberCenter").hide();
                    $("#message").html("欢迎您&nbsp&nbsp&nbsp&nbsp" + d.name + "<a onclick=\"unuserFun()\" style=' cursor: pointer;'>非本地用户</a>");
                }
                if (d.code == "0") {
                    $("#LoginMsg").hide();
                }
            });
        }

        function getUrl1() {
            var resulturl = "";
            var url = document.URL;
            var arr = url.split('?');
            var ar = arr[0];
            if (arr[1].split("=")[0] == "categoryId") {
                ar += "?categoryId=" + arr[1].split("=")[1];
                resulturl = ar;
            }
            else {
                resulturl = ar;
            }
            return resulturl;
        }
        function getUrl() {
            var url = document.URL;
            var arr = url.split('?');
            var ar = arr[0];
            return ar;
        }

        function funparent() {
            posthead();
        }


        //本地登录
        function loginByLocation() {
            window.location.href = "../login.aspx?ReturnUrl=" + encodeURIComponent(getUrl());
        }
        //Cas登录
        function loginByCas() {

        }

        //nzl 254-283社会，专业，管理
        function alerUerWord() {
            gltel();
            var xmlhttp, i, val, type;
            $.ajax({
                url: "UserControl/XMLSource/XMLindex2UserWord.xml",
                dataType: 'xml',
                type: 'GET',
                timeout: 2000,
                error: function (xmlhttp) { },
                success: function (xmlhttp) {
                    $(xmlhttp).find("index2WORD").each(function (i) {
                        val = $(this).children("USER").text();
                        switch (val) {
                            case "社会公众":
                                type = "shgz"; break;
                            case "专业人员":
                                type = "zyry"; break;
                            case "管理人员":
                                type = "glry"; break;

                        }
                        document.getElementById(type).innerHTML = "<b>" + $(this).children("TITLE").text() + "</b> </br> &nbsp;&nbsp;&nbsp;&nbsp;"
                            + $(this).children("TEXT").text();

                    }
                )
                }

            });
        }
        function gltel() {
            var xmlhttp, i, val, type;
            $.ajax({
                url: "UserControl/XMLSource/xiangxiBGTELandURL.xml",
                dataType: 'xml',
                type: 'GET',
                timeout: 2000,
                error: function (xmlhttp) { },
                success: function (xmlhttp) {

                    document.getElementById("gltel").innerHTML = $(xmlhttp).find("XIANGXIGB").first().children("TEL").text() + "。";
                    // alert(document.getElementById("gltel").innerHTML);
                }

            });
        }
    </script>
    
