                            $('#shang-content').fadeOut();
                        });

                        $('.shang-submit').click(function() {
                            $(this).prop('disabled', true);
                            $('.shang-iframe-container').append('<iframe width="100%" class="pay_frame" name="alipay" frameborder="0" scrolling="no"></iframe>');
                            $('#shang-iframe').fadeIn(function() {
                                setTimeout(function() { $('#form-shang').attr('target', 'alipay').submit(); }, 200);
                            });
                        });
                    })($);
                </script>
