     <div class="article-content"> 
      <script src="/ckplayer/ckplayer_common.php?id=1" language="javascript"></script> 
      <script type="text/javascript">
var videoinfo={
	basehost:'http://www.fulididai.com',
	path:'',
	vurl:'',
	html5url:'',
	title:'日本人年均性生活45次 国际均匀程度103次',
