<script type="text/javascript">
    $(function () {
        //$(".J-verifycode").on("click", changeVerifyCode);
        var username = NI.queryString("username");
        if (username) {
            $("#UserName").val(decodeURIComponent(username));
            $("#PassWord").focus();
        }
        changeVerifyCode1();
        $(".J-verifycode").on("click", changeVerifyCode1);

        function changeVerifyCode1() {
            var isIE6IE7 = $.browser.msie && ($.browser.version == "6.0" || $.browser.version == "7.0");
            if (isIE6IE7) {
                $("#verifycodeImg").attr("src", "/Account/VerifyCode3?v=" + Math.random());
            } else {
                $.ajax({
                    "type": "POST",
                    "url": "/Account/VerifyCode2",
                    "data": {},
                    "success": function (r) {
                        $("#verifycodeImg").attr("src", "data:image/png;base64," + r);
                    },
                    "error": function () {
                    }
                });
            }
        }
    });
    
    
</script>
