    <script type="text/javascript">
        var s_down = false;
        $("#s_down").click(function () {
            var s_w = $("#s_down_word").html();
            var s_d = $("#s_down_word").attr("data");
            if (!s_down) {
                $("#s_down_list").show();
                jQuery.each($("#s_down_list li"), function (i, n) {
                    if ($(n).attr("data") == s_d) {
                        $("#s_down_list li").removeClass("hover");
                        $(n).addClass("hover");
                        return;
                    }
                });
                s_down = true;
            }
            else {
                $("#s_down_list").hide();
                s_down = false;
            }
        });
        $("#s_down_list li a").click(function () {
            var s_w = $(this).html();
            var s_d = $(this).attr("data");
            $("#s_down_word").html(s_w);
            $("#s_down_word").attr("data", s_d);
        })
        function searchKey() {
            var s_d = $("#s_down_word").attr("data");
            var key = $("#s_i_search").val();
            switch (s_d) {
                case "1":
                    window.location.href = "/news?key=" + encodeURI(key);
                    break;
                case "2":
                    window.location.href = "/case?key=" + encodeURI(key);
                    break;
                case "3":
                    window.location.href = "/house?key=" + encodeURI(key);
                    break;
            }
        }
    </script>
