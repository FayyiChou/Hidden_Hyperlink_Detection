            <script type="text/javascript">
                function SubmitCacl() {
                    var name = $("#cacl_UserName").val();
                    var phone = $("#cacl_UserTel").val();
                    var house = $("#cacl_House").val();
                    var area = $("#cacl_Area").val();
                    var code = $("#cacl_vCode").val();
                    if ($.trim(code) == "") {
                        layer.alert("请输入验证码！");
                        return false;
                    }
                    if ($.trim(name) == "" || $.trim(area) == "" || $.trim(phone) == "" || $.trim(house) == "") {
                        layer.alert("请仔细填写，您还有未填的必填项！");
                        return false;
                    }
                    if (phone.length != 11) {
                        layer.alert("请输入正确的手机号码！");
                        return false;
                    }
                    var remark = "装修区域：" + $("#cacl_AreaAddr").val();
                    $("#cacl_Remark").val(remark);
                    $("#cacl_form").ajaxSubmit({
                        url: "/home/cacl",
                        type: "post",
                        dataType: "json",
                        beforeSubmit: function () {
                            layer.load(1, { shade: [0.1, '#000'] });
                        },
                        error: function () {
                            layer.closeAll('loading');
                            layer.alert("请求失败");
                        },
                        success: function (data) {
                            layer.closeAll('loading');
                            if (data != null) {
                                if (data.Success) {
                                    $("#cacl_hx").val("0");
                                    $("#cacl_form input[type='text']").val("");
                                    $("#cacl_result").val(data.Source);
                                }
                                else
                                    layer.alert(data.Msg);
                            }
                            else
                                layer.alert("提交失败");
                        }
                    });
                    return false;
                }
            </script>

