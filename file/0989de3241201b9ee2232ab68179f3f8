            <script type="text/html" id="temp_des">
                <div class="des_photo">
                    <a href="/designer/{id}"><img src="{photo}" /></a>
                </div>
                <div class="des_info">
                    <div class="des_info_top">
                        <div class="des_info_top_l">
                            <h3>{name}</h3>
                            <p class="good">{cname}</p>
                            <p class="star"></p>
                            <a href="/designer/{id}">预约设计师</a>
                        </div>
                        <div class="des_info_top_r">
                            <p><span>工作年限：</span>{cydate}年</p>
                            <p><span>现任职务：</span>{cname}</p>
                            <p><span>设计理念：</span>{title}</p>
                            <p><span>设计风格：</span>{goodat}</p>
                        </div>
                    </div>
                    <div class="des_info_bom">
                        {cases}
                    </div>
                </div>
            </script>
            <script type="text/javascript">
                $('#des_list').bxCarousel({
                    display_num: 5,
                    move: 1,
                    auto: true,
                    controls: true,
                    auto_interval: 4000,
                    margin: 7,
                    auto_hover: false,
                    next_text: "",
                    prev_text: ""
                });
                var showDesInfo = function (id) {
                    $.ajax({
                        url: '/home/GetDesInfo',
                        type: "post",
                        dataType: "json",
                        data: { id: id, status: status },
                        //beforeSend: function () {
                        //    layer.load(1, { shade: [0.1, '#000'] });
                        //},
                        error: function () {
                            layer.closeAll('loading');
                            layer.alert("请求失败");
                        },
                        success: function (data) {
                            layer.closeAll('loading');
                            if (data != null) {
                                if (data.Success) {
                                    var temp = $("#temp_des").html();
                                    temp = temp.replace(/{id}/g, data.Source.ID);
                                    temp = temp.replace(/{photo}/g, data.Source.Photo);
                                    temp = temp.replace(/{name}/g, data.Source.Name);
                                    temp = temp.replace(/{cname}/g, data.Source.cName);
                                    temp = temp.replace(/{cydate}/g, data.Source.cyDate);
                                    temp = temp.replace(/{title}/g, data.Source.Title);
                                    temp = temp.replace(/{goodat}/g, data.Source.GoodAt);
                                    var case_html = [];
                                    if (data.Source.DesCase != null && data.Source.DesCase.length > 0) {
                                        $.each(data.Source.DesCase, function (i, n) {
                                            if (i < 5) {
                                                case_html.push("<a href=\"/case/" + n.Id + "\"><img src=\"" + n.ImageSmall + "\" /></a>");
                                            }
                                        });
                                    }
                                    temp = temp.replace(/{cases}/g, case_html.join(""));
                                    $("#desPanel").html(temp);
                                }
                                else {
                                    layer.alert(data.Msg);
                                }
                            }
                            else {
                                layer.alert("请求失败");
                            }
                        }
                    });
                };
            </script>
        </div>
