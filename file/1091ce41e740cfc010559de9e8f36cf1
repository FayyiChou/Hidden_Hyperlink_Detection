    <script type="text/tmpl" id="js-tmpl-top10">
      {{each list as item }}
      <tr>
        <td><span class="{{$index | formatIndex}}">{{$index + 1}}</span>{{item.appShortName}}</td>
        <td class="{{item.currentGainsDouble | formatPriceClass}}">{{item.currentGainsDouble | formatCode}}{{item.curPrice}}</td>
        <td class="{{item.currentGainsDouble | formatPriceClass}}">{{item.currentGainsPercent}}</td>
        <td><a href="{{item.openAccountUrl}}" target="_blank">开户</a></td>
      </tr>
      {{/each}}
    </script>
    <script>
      (function (){
        template.helper('formatIndex', function (index) {
          if (index < 3) {
            return 'f-bcr';
          }
          return 'f-bcg'
        });

        template.helper('formatPriceClass', function (value) {
          if (value > 0) {
            return 'f-fcr';
          } else if (value < 0) {
            return 'f-fcg'
          } else {
            return '';
          }
        });

        template.helper('formatCode', function (value) {
          if (value > 0) {
            return '+';
          } else if (value < 0) {
            return '-'
          } else {
            return '';
          }
        });
        var dataInterval = 60000;
        var list;
        var initData = function () {
          $.ajax({
            url: 'http://60.205.166.190/YBK58/ybk58ws/data/json/getExchangeTotalMarketValueTop',
            type: 'post',
            dataType: 'json',
            success: function (msg) {
              if (msg.code == 10000) {
                if (typeof dataInterval != 'undefined' && dataInterval > 6000) {
                  dataInterval = msg.dataInterval;
                  list = msg.resultList.sort(function (a, b) {
                    return b.currentGainsDouble - a.currentGainsDouble;
                  });
                  $('#js-top10').html(template('js-tmpl-top10', {list:list}));
                  setTimeout(function () {
                    initData();
                  }, dataInterval)
                }
              }
            }
          })
        };
        var init = function () {
          initData();
        };
        init();
      }());
    </script>
