<script  type="text/javascript" language="javascript">
function checkcookie(uu) {
if($.cookie(uu)=="clicked"){ 
document.getElementById(uu).style.display="none";


} else {
	document.getElementById(uu).style.display="inline";
   }
}
function setcookie(ww) {
  $.cookie(ww,"clicked",{expires:7,path:"/"})

}
</script>

