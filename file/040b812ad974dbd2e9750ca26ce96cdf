<script>
$(function(){
	var isDetailPage = false;
	var currentUrl = window.location;
	var detailRe = /\/q\/\d+\.html/;
	var switchStop = true;
	var recommendSearchContent = document.getElementById("recommendSearchContent").innerHTML;
	if (typeof $(".detailhistory").get(0) != "undefined"){
		var oriHistoryHTML = $(".detailhistory").get(0).outerHTML;
		var oriHistoryINNERHTML=$(".detailhistory").get(0).innerHTML;
		//console.log(oriHistoryHTML);
	}
	
	if (detailRe.test(currentUrl)){
		isDetailPage = true;
	}
	//首页搜索提示
	var oJ_keywords = document.getElementById("J_keywords");
	//oJ_keywords.focus();
	oJ_keywords.onfocus = function(){
		var searchBox = document.getElementById("zSearchSuggest");
		var searchResultBox = document.getElementById("searchResultList");
		if (searchResultBox.children.length != 0){
			searchBox.style.display = "block";
		}
	}
	oJ_keywords.onclick=function(e){
		if ( e && e.stopPropagation ){
　　		// 因此它支持W3C的stopPropagation()方法
　		　e.stopPropagation();
		} else {
　　//否则，我们需要使用IE的方式来取消事件冒泡
　　		window.event.cancelBubble = true;
		return false;
		}
	};
	if (oJ_keywords != null || true) {	
		var bind_name = 'input';
        if (navigator.userAgent.indexOf("MSIE") != -1){
        	bind_name = 'propertychange';
    	}
		$(oJ_keywords).keyup(function(event){
			//判断li的数量，如果小于0不做处理，如果大于0，找到最大位置和当前位置，
			if(event.keyCode == 40 || event.keyCode == 38){
				var moveLength = $("#searchResultList li").length;
				if(moveLength < 0){
					return;
				} else {
					if(event.keyCode == 40){
						var moveDirection = 1;
					} else if(event.keyCode == 38) {
						var moveDirection = -1;
					}
					//判断当前高亮的位置
					var curIndex = $("#searchResultList").find(".active").index();
					var newCurIndex = curIndex + moveDirection;
					$("#searchResultList li").removeClass("active");
					$("#searchResultList li").eq(newCurIndex).addClass("active");
					return;	
				}				
			} else if (event.keyCode == 13){
				var judgeCurIndex = $("#searchResultList").find(".active").index();
				if(judgeCurIndex >= 0){
					if(window.event){ //判断IE浏览器
					  window.event.returnValue = false;
					}
					else{
					  event.preventDefault();
					};
					event.stopPropagation()
					$("#searchResultList li").eq(judgeCurIndex).get(0).click();
				}
				return;
			}
			
			
		
			var oSearchBox = document.getElementById("zSearchSuggest");
			var keyWords = $.trim(this.value);
			if(keyWords != "" && switchStop){
				console.log(keyWords);
				var oriKeyword = keyWords;
				keyWords = encodeURIComponent(keyWords);
				$.post("/ajax/new/getSearchResult.php",{"keywords":keyWords},function(response){
					switchStop = false;
					if (response.status) {
						if(document.getElementById("J_keywords").value == ""){
							switchStop = true;
							return false;
						}
						$("#searchResultList").removeClass("search-ranking");
						var data = eval(response.data);
						var dataLength = data.length;
						var searchRes = "";
						for (var i = 0; i < dataLength; i++){
							searchRes += '<li class="" onclick="location.href=\'/q/'+data[i].z_id+'.html\'" onmouseover="this.className=\'active\'" onmouseout="this.className=\'\'"><a>'+data[i].z_title+'<span>'+data[i].z_reply+'个回答</span></a></li>';
						}
						if	(searchRes){
							searchRes += '<li onclick="location.href=\'/new/search.php?kword='+oriKeyword+'\'" class="last"><a id="searchlast" href="/new/search.php?kword='+oriKeyword+'">搜索  “'+oriKeyword+'” </a></li>';
							$("#searchResultList").html(searchRes);
						}
						oSearchBox.style.display = "block";
					} else {
						//oSearchBox.style.display = "none";
					}
					switchStop = true;
				},"json");
			} else {
				
				if(oriHistoryINNERHTML){
					oSearchBox.innerHTML = oriHistoryHTML;
				}else{
					oSearchBox.innerHTML = recommendSearchContent;
					$("#zSearchSuggest ul").attr("id", "searchResultList");
					console.log("empty le");
					//oSearchBox.innerHTML = "";
				}
				oSearchBox.style.display = "block";
			}
	   });
	}
	
	document.onclick = function(){
		var oSearchBox = document.getElementById("zSearchSuggest");
		if (oSearchBox) {
			oSearchBox.style.display = "none";
		}
	}

	$(".del").click(function(){
		$.post("/ajax/new/changeCookie.php",{"keyword":$(this).attr("name")},function(d){
		
		});
		console.log($("#searchResultList li").length);
		var oSearchBox = document.getElementById("zSearchSuggest");
		if($("#searchResultList li").length == 2){
			//$(this).parent().remove();
			//$(".last").remove();
			$("#searchResultList").html("");
			oriHistoryHTML = $(".detailhistory").get(0).outerHTML;
			oriHistoryINNERHTML = $(".detailhistory").get(0).innerHTML;
			oSearchBox.innerHTML = recommendSearchContent;
			$("#zSearchSuggest ul").attr("id","searchResultList");	
			oSearchBox.style.display="block";
		}else{
			$(this).parent().remove();
			oriHistoryHTML = $(".detailhistory").get(0).outerHTML;
			oriHistoryINNERHTML = $(".detailhistory").get(0).innerHTML;
		}	
		return false;
	});

	$("#clearallcookie").click(function(e){
		if(window.event){ //判断IE浏览器
		  window.event.returnValue = false;
		}
		else{
		  event.preventDefault();
		};
		event.stopPropagation()
		var oSearchBox = document.getElementById("zSearchSuggest");
		var cookieUse = new cookieHandler();
		//cookieUse.delCookie("search_content");
		oSearchBox.innerHTML = recommendSearchContent;
		$("#zSearchSuggest ul").attr("id","searchResultList");	
		
		document.getElementById("J_keywords").focus();
		oSearchBox.style.display="block";
	});

		
});
</script><script src="http://icon.zol-img.com.cn/ask/js/guide.js"></script>
