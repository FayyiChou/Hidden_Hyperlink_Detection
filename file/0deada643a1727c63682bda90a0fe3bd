	<script>
		$(function(){
			var showweixin = getCookie("showweixin");
			if(showweixin == null || showweixin==''){
				setTimeout(function(){
					$(".left_nav_top").animate({bottom:'0'},"slow");
				},3000)
				$(".left_nav_top .close").click(function(){
					$(".left_nav_top").animate({bottom: '-160px'},"slow");
					if (($.browser.msie) && ($.browser.version == "6.0")){
						$(".left_nav_top").hide();
					}
					setCookie("showweixin","1");
				})
			}
		})
	</script>
