$(function(){

	$("img").lazyload({
        effect: "fadeIn"
    });
	$(".y_ALo li .rsp").hide();	
	$(".y_ALo li").hover(function(){
		$(this).find(".rsp").stop().fadeTo(500,0.5)
		$(this).find(".text").stop().animate({left:'0'}, {duration: 500})
	},
	function(){
		$(this).find(".rsp").stop().fadeTo(500,0)
		$(this).find(".text").stop().animate({left:'176'}, {duration: "fast"})
		$(this).find(".text").animate({left:'-176'}, {duration: 0})
	});
});

$(function(){
	// 域名更换
	$(".J_close").click(function(){
		$(".popup_div").fadeOut();
		$("#spm").fadeOut();
	});

	setTimeout(function(){
		$(".popup_div").fadeOut();
		$("#spm").fadeOut();
	},20000)

	// 倒计时
	$(".timer li").hover(function(){
		$(this).addClass("active");
	},function(){
		$(this).removeClass("active");
	})

     // 高分获奖
	var $div_li =$("div.y_hj ul li");
	$div_li.hover(function(){
		$(this).addClass("active").siblings().removeClass("active");
	    var index =$div_li.index(this);  
            $("div.xxk > div.y_xxk")     
                    .eq(index).show()   
                    .siblings().hide();
	});
	
	//礼品中心
	var $div_li2 =$("div.y_hj2 ul li");
	$div_li2.hover(function(){
		$(this).addClass("active").siblings().removeClass("active");
	    var index2 =$div_li2.index(this);  
            $("div.xxk2 > div.y_xxk2")     
                    .eq(index2).show()  
                    .siblings().hide(); 
	});

	// 酒美达人
	$(".jm_cont dl").hover(function(){
		$(this).addClass("active");
	},function(){
		$(this).removeClass("active");
	});
	
	// 热销排行榜
	$(".bar_hot .item").hover(function(){
		$(this).parent().find(".item").removeClass("on");
		$(this).addClass("on");
	});
	
	// 友情链接
	$("#foot .text s").toggle(function(){
		$("#foot .text p").addClass("hg_auto");
		$(this).addClass("current");
	},function(){
		$("#foot .text p").removeClass("hg_auto");
		$(this).removeClass("current");

	})

	// 礼品中心 进口葡萄酒 烈酒 酒具
	$(".index_mod_tab .mod_cont li").hover(function(){
		$(this).addClass("active");
	},function(){
		$(this).removeClass("active");
	})

	// 焦点图
	var defaultOpts = { interval: 8000, fadeInTime: 300, fadeOutTime: 100 };
	var _titles = $(".focus .focus_btn li");
	var _bodies = $(".focus .change_cont li");
	var _count = _titles.length;
	var _current = 0;
	var _intervalID = null;
	var stop = function() { window.clearInterval(_intervalID); };
	var slide = function(opts) {
		if (opts) {
			_current = opts.current || 0;
		} else {
			_current = (_current >= (_count - 1)) ? 0 : (++_current);
		};

		_bodies.filter(":visible").fadeOut(defaultOpts.fadeOutTime, function() {
			_bodies.eq(_current).fadeIn(defaultOpts.fadeInTime);
			_bodies.removeClass("active").eq(_current).addClass("active");
		});
		_titles.removeClass("active").eq(_current).addClass("active");

	};

	var go = function() {
		stop();
		_intervalID = window.setInterval(function() { slide(); }, defaultOpts.interval);
	}; 

	var itemMouseOver = function(target, items) {
		stop();
		var i = $.inArray(target, items);
		slide({ current: i });
	}; 

	_titles.hover(function() { if($(this).attr('class')!='active'){itemMouseOver(this, _titles); }else{stop();}}, go);
	_bodies.hover(stop, go);
	go();


	// 公告向上滚动
	function upAndDown(){
		var chidHtml=$(".notice ul").html();
		$(".notice ul").append(chidHtml);
		var timer;
		var move=function(){
			clearTimeout(timer);
			$(".notice ul").each(function(index, element) {
				var height=$(this).outerHeight();
	           var top=parseInt($(this).css("top"));
			   top=top-32;
			   if(top<(2-height/2))
			   {
				   $(this).css("top",top+height/2+32);
				   top=0;
			   }
			   $(this).animate({top:top},100);
			   
	        });
			timer=setTimeout(move,4000);
		}
		timer=setTimeout(move,4000);
		$(".notice").hover(function(){
			clearTimeout(timer);
		},function(){
			setTimeout(move,4000);
		})
		
	}

	upAndDown();

	$(".J_daren_box").tabs("daren_btn li","daren_cont","active");
	$(".J_jm_box").tabs("jm_btn li","jm_cont","active");
	$("#lp_box").tabs("lp_btn li","J_lp_cont","active");
	$("#lp_box_2").tabs("lp_btn_2 li","J_lp_cont_2","active");
	$(".jz_list_c").rotationClick("jz_list_c","Scrcont","J_pic","left_ico","right_ico",true);
        //glide.layerGlide(true,'ranking_focus_click','ranking_focus_chang',218,8,0.1,'left');
        //glide.layerGlide(true,'ranking_focus_click_two','ranking_focus_chang_two',218,10,0.1,'left');
})

var changeDomain = getCookie("showchangedomain");
if(changeDomain == null || changeDomain==''){
	//$(".popup_div").show();
	//$("#spm").show();
	//$("#hd_banner").show();
	//setCookie("showchangedomain","1");
}else{
	$(".bigImg").slideDown('slow');
	setTimeout(function(){
		$(".bigImg").slideUp();
	},15000)
	setTimeout(function(){
		$(".smallImg").show();
	},15100)
}

function clearys(type){
	if(type=='1'){
		$("#orderNo").val("");
	}else{
		$("#shren").val("");
	}
}

function getCookie(name) {  
    var strCookie = document.cookie;  
    var arrCookie = strCookie.split("; ");  
    for ( var i = 0; i < arrCookie.length; i++) {  
        var arr = arrCookie[i].split("=");  
        if (arr[0] == name) {  
            return arr[1];  
        }  
    }  
    return "";  
}  
function setCookie(name, value) {  
    var Days = 30;
    var exp = new Date();
    exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);
    document.cookie = name + "=" + escape(value) + ";expires=" + exp.toGMTString();  
} 

function buy(product_no,n) {
	var quantity = "";
	if(n == null || n == ""){
		quantity = 1;
	}else{
		quantity = n;
	}
	addShoppingCart(product_no,quantity);
}

function goCartManager(){
	var url = window.location.href;
	$.getJSON('/json/shoppingcart_saveUrlCookie.shtml?m=' + Math.random(),{"urll":url},function(data){
		window.location.href="/cart_showShoppingCart.shtml";
	});
}

var cuxiaoEmail=/^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/; 
function  cuxiaoEmailsubscribe(){
	//cuxiaoEmail
	var cuxiaoEmailValue = $.trim($("#cuxiaoEmail").val());
	if(cuxiaoEmailValue=="" || cuxiaoEmailValue.length<=0){
		alert("促销邮件不能为空!");
		return;
	}else if(!cuxiaoEmail.test(cuxiaoEmailValue)){
		alert("促销邮件格式非法!");
		return;	
	}else{
		$.post('/user/user_subscribeCuXiao.dhtml?m=' + Math.random(),{"cxEmail":cuxiaoEmailValue},function(data){
				var errorMsg = data.errorMsg;
				if(errorMsg==1){
				alert("促销邮件格式非法!");
				return;
				}else if(errorMsg==2){
				alert("此邮件已设置促销推送,请勿重复设置!");
				return;
				}else if(errorMsg==-1){
				alert("系统繁忙,请稍后再试!");
				return;
				}else if(errorMsg==3){
				alert("恭喜您,促销推送邮箱设置成功!");
				return;
				}
			});
	}
}

