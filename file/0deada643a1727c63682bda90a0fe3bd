<script>

    // 倒计时
    function pro_countdown(endtime,textid,now1,starttime){
        var now=new Date();
        if(now1 != null){
            now = new Date(now1);
        }
        var newdate=null;
        var lefttime=null;
        var isEnd = 0;
        var isStart = 1;
        var newdate=new Date(endtime);

        lefttime=Math.floor((newdate-now)/1000);
        if(lefttime<0)	{
            isEnd = 1;
        }

        if(!isEnd && starttime != null && starttime != ""){
            newdate = new Date(starttime);
            lefttime=Math.floor((now-newdate)/1000);
            if(lefttime < 0){
                isStart = 0;
                lefttime = Math.abs(lefttime);
            }
        }
        if(isStart && !isEnd){
            newdate = new Date(endtime);
            lefttime=Math.floor((newdate-now)/1000);
        }

        function countdata(){
            lefttime = lefttime - 1;
            var text;
            if(lefttime>=0){
                var day=Math.floor(lefttime/3600/24);
                var hour=Math.floor(lefttime/3600%24);
                var minutes=Math.floor((lefttime%3600)/60);
                var second=Math.floor(lefttime%60);
                var li=$("#"+textid+" em");
                if(hour<10){hour="0"+hour ;}
                if(minutes<10){minutes="0"+minutes;}
                if(second<10){second="0"+second;}
                day=day.toString();
                hour=hour.toString();
                minutes=minutes.toString();
                second=second.toString();
                li.eq(0).html(day);
                li.eq(1).html(hour);
                li.eq(2).html(minutes);
                li.eq(3).html(second);

            }else{
                //	if(textid != 'clockUl'){
                $("#"+textid).parent().parent().parent().parent().addClass("old");
                $("#"+textid).parent(".t").html("<div class='old'>已结束!</div>");
                //}
            }
        }
        countdata();
        setInterval(countdata,1000);
    };

    pro_countdown("2017/02/24,20:00:00","time_1")
    pro_countdown("2017/02/24,20:00:00","time_2")
    pro_countdown("2017/02/24,20:00:00","time_3")
    pro_countdown("2017/02/24,20:00:00","time_4")
    pro_countdown("2017/02/24,20:00:00","time_5")
    pro_countdown("2017/02/24,20:00:00","time_6")
    pro_countdown("2017/02/24,20:00:00","time_7")
    pro_countdown("2017/02/24,20:00:00","time_8")
    pro_countdown("2017/02/24,20:00:00","time_9")
    pro_countdown("2017/02/24,20:00:00","time_10")

</script>

