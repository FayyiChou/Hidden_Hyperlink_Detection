	function questionnaire(){
		 $.ajax({  
	           type: "get",
	           url: "/questionnaire",
	           //返回数据成功，将返回的json数据解析  
	           success: function (json) {  
		           	if (json.code == "0") { //正常
		           		var html = "<div style='float:right;position:fixed;_position:absolute; bottom:0px;_bottom:0px;right:30px'>"
					             + "<a href='" + json.questionnaireLink + "'  title='" + json.questionnaireTitle + "' target='_blank'><img src='" + json.questionnaireImg + "' border=0/></a>"
					             + "</div>";
						$("body").append(html); 
		           	}
	           }  
	     }); 
	}
