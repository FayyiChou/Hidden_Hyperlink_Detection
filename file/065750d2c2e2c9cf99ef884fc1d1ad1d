    <script type="text/javascript">
        var jsonpcallbackForUcar = function (data) {
            data = data.CarListInfo;
            if (data.length <= 0) return;
            var strArray = new Array();
            strArray.push("<h2><a target=\"_blank\" href=\"" + (data[0].MoreCarUrL != 'undefined' ? data[0].MoreCarUrL : "") + "?leads_source=p009011\">相关二手车</a></h2>");
            strArray.push("<div class=\"used-car-box\">");
            for (var i = 0; i < data.length; i++) {
                strArray.push("<div class=\"img-info-layout img-info-layout-14093\">");
                strArray.push("<div class=\"img\"><a title=\"" + data[i].BrandName + "\" target=\"_blank\" href=\"" + data[i].CarlistUrl + "&leads_source=p009011\">");
                strArray.push("<img src=\"" + data[i].PictureUrl + "\" /></a></div>");
                strArray.push("<ul class=\"p-list\"><li class=\"name\"><a title=\"" + data[i].BrandName + "\" target=\"_blank\" href=\"" + data[i].CarlistUrl + "&leads_source=p009011\">" + data[i].BrandName + " </a></li>");
                strArray.push("<li class=\"info\">" + data[i].BuyCarDate + "上牌 " + data[i].DrivingMileage + "公里</li>");
                strArray.push("<li class=\"price\"><a title=\"" + data[i].BrandName + "\" target=\"_blank\" href=\"" + data[i].CarlistUrl + "&leads_source=p009011\">" + data[i].DisplayPrice + "</a></li></ul>");
                strArray.push("</div>");
            }
            strArray.push("</div>");
            if (document.getElementById("line_boxforucar_box")) {
                document.getElementById("line_boxforucar_box").innerHTML = strArray.join("");
            }
        }
        
    </script>
    
