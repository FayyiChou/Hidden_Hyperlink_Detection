else if (carlevel == '概念车' || carlevel == 'MPV' || carlevel == '面包车' || carlevel == '皮卡' || carlevel == '其它') {
    adBlockCode = '0ef28a62-8aee-43c4-a2e5-25a9d953a7f1';
}
else if (carlevel == 'SUV') {
    adBlockCode = '0bcd1944-cfc5-4a77-a4b1-c8b85bb57f6b';
}

