  <script type="text/javascript">
    var Login = function(){
        return{
               'login' : function(){
				var isFirstLogin_ = "";
                var enResult = encode64($("#index_login_pwd").val());
				login.loginHandler(enResult,isFirstLogin_);
            },

            /**
              * 刷新验证码
             */
            'reload' : function(){
                var now = new Date();
                var captcha = document.getElementById("captcha");
                captcha.src = "/module-portalweb/captcha.jpg?a="+ now.getTime();
            },
            
            'showRandomCode' :function(){
            	document.getElementById("captchaDiv").style.display="";
            },
            
            'hiddenRandomCode' :function(){
            	document.getElementById("captchaDiv").style.display="none";
            }
        };
    }();

    $().ready(function(){
        var isFirstLogin_ = "";
        if(isFirstLogin_ == ''){
         	Login.hiddenRandomCode();
        }else{
        	Login.showRandomCode();
        }
    });
    </script>



