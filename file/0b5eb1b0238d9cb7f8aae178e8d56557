<script language="javascript">
function _check()
{
    
	var bo=true;
    if(is_null(document.form2.name,1,"请填写用户名！\n\n"))
    {
        bo=false;
		return bo;
    }
    if(is_null(document.form2.pwd,1,"请填写密码！\n\n"))
    {
        bo=false;return bo;
    }
    return bo;
}
function _login()
{   
	if(_check()){
		document.form2.action="logining.jsp";
		document.form2.submit();
    }

}
function checkkey(keys)     
{
      if(keys.keyCode == 13){ 
	  	 _login(); 
     } 
}
function _forgetPwd()
{
	window.location.href="./PortalControl?flag=forgetPwd";
}
function _register()
{
	window.location.href="./PortalControl?flag=term";
}
</script>

		
        
