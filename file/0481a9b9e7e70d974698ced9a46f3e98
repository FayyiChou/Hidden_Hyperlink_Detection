	<section class="clearfix">
		<!--主体左侧部分-->
		<div class="section_left">
           <dl class="online_catTitle clearfix">
                <dt>网上办事</dt>
                <dd></dd>
           </dl>
           <div class="online_container">
                <script type="text/javascript">
               		// 事项内容页链接
               		var serviceArLink = "/gzjdw/wsbs/bsdt/list-1.shtml";
               		// 获取事项列表（页码、条数、部门、业务、对象、标题、类型）
               		var getWsmpServiceitemJson = function(pageNum, pageSize, deptName, toBusiness, toObject, serviceitemName, itemType, typeIndex){
               			$.get("/pro/prj/wsmpServiceitem/list", {pageNum:pageNum, pageSize:pageSize, deptName:deptName, toBusiness:toBusiness, toObject:toObject, serviceitemName:serviceitemName, itemType:itemType}, function(data){
               				var serviceContent = "";
               				for(var i=0; i<data.item.length; i++){
               					var otherLink = '';
               					if(data.item[i].deptName == '网络安全监察分局') {
               						otherLink = '?servicecode='+ data.item[i].dpcode +'&return_no_login_url=http://www.gzjd.gov.cn/gsmpro/web/webuser/login.jsp';
               					}
               					
               					var color = '';
               					var downLink = 'href="' + serviceArLink + '#' + data.item[i].serviceId + '&bgxz' + '"';
               					if(data.item[i].ishaveDownfile == 0) {
               						color = 'cold9d9d9';
               						downLink = '';
               					}
               					
               					var color2 = 'cold9d9d9';
               					var wssqLink = '';
               					if(data.item[i].extend2 == 0) {
               						if(data.item[i].extend1 == 1) {
               							wssqLink = 'href="http://www.gzjd.gov.cn/gsmpro/form1.servlet?action=passForm&serviceId=' + data.item[i].serviceId + '&itemid=' + data.item[i].itemId + '"';
               							color2 = '';
               						} else if(data.item[i].extend1 == 0) {
               							wssqLink = '';
               						}
               					} else if(data.item[i].extend2 == 3) {
               						if(data.item[i].appurl != null) {
               							wssqLink = 'href="' + data.item[i].appurl + otherLink + '"';
               							color2 = '';
               						} else {
               							wssqLink = '';
               						}
               					}
               					
               					serviceContent += "<dl"+ (i==0?" class='on'":"") +">"
               								 				+ "<dt class='clearfix'>"
               								 					+ "<span class='title'>"+ (data.item[i].serviceitemName.length>34?data.item[i].serviceitemName.substring(0,34)+"...":data.item[i].serviceitemName) +"</span>"
               								 					+ "<span class='depart'>"+ data.item[i].deptName +"</span>"
         								 					+ "</dt>"
               								 				+ "<dd>"
               								 					+ "<ul>"
               								 						+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg0.jpg'/></li>"
          								 							+ "<li><a href='"+ (serviceArLink+"#"+data.item[i].serviceId+"&bszn") +"' target='_blank'><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_icon0.jpg'/>办事指南</a></li>"
           								 							+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg1.jpg'/></li>"
       								 							+ "</ul>"
               								 
               								 					+ (downLink == '' ? "" : ("<ul>"
               								 						+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg0.jpg'/></li>"
               								 						+ "<li><a class='"+ color +"' "+ downLink +" target='_blank'><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_icon1.jpg'/>表格下载</a></li>"
               								 						+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg1.jpg'/></li>"
           								 						+ "</ul>"))
               								 
               								 					+ "<ul>"
               								 						+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg0.jpg'/></li>"
               								 						+ "<li><a href='http://wsbs.gz.gov.cn/gz/hotline/gotoIndex.action' target='_blank'><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_icon2.jpg'/>网上咨询</a></li>"
               								 						+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg1.jpg'/></li>"
           								 						+ "</ul>"
               								 
               								 					+ (wssqLink == '' ? "" : ("<ul>"
               								 						+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg0.jpg'/></li>"
               								 						+ "<li><a class='"+ color2 +"' "+ wssqLink +" target='_blank'><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_icon3.jpg'/>网上申请</a></li>"
               								 						+ "<li><img alt='' src='/data/tms/gzjd_website/images/online_table_item_btn_bg1.jpg'/></li>"
           								 						+ "</ul>"))
               								 
               								 				+ "</dd>"
           								 				+ "</dl>";
               				}
               				$(".table_content").eq(typeIndex).html(serviceContent);
               				pageInit(pageNum, data.totalPage, data.count, typeIndex);
               			}, "json");
               		};
               		// 获取事项总数（部门、业务、对象、标题、类型）
               		var getWsmpServiceitemCount = function(deptName, toBusiness, toObject, serviceitemName, itemType, typeIndex){
               			$.get("/pro/prj/wsmpServiceitem/count", {deptName:deptName, toBusiness:toBusiness, toObject:toObject, serviceitemName:serviceitemName, itemType:itemType}, function(data){
               				var typeTitle = $(".online_submenu li a").eq(typeIndex);
               				typeTitle.html(typeTitle.data("title")+"（"+data.count+"）");
               				var status = $(".online_submenu li:eq("+typeIndex+") a").text().indexOf("0")
               				if(status == -1){
               					$(".online_submenu li:eq("+typeIndex+")").attr("style","display:block");
               				}else{
               					$(".online_submenu li:eq("+typeIndex+")").attr("style","display:none");
               				}
               				if(data.count > 0)
               					$(".online_submenu li").eq(typeIndex).click();
               			}, "json");
               		};
               		// 分页初始化（页码、总页数、总条数）
               		var pageInit = function(pageNum, totalPage, totalNum, typeIndex){
               			var pageContent = "";
               			var pageNumContent = "";
               			var startPageNum = 1;
               			var endPageNum = totalPage;
               			if(totalPage > 5){
               				endPageNum = 5;
               				if(pageNum > 3){
               					if(pageNum+2 >= totalPage){startPageNum = totalPage - 4; endPageNum = totalPage;}
               					else{startPageNum = pageNum - 2; endPageNum = pageNum + 2;}
               				}
               			}
               			for(var i=startPageNum; i<=endPageNum; i++){pageNumContent+="<a"+(i==pageNum?" class='on'":"")+" onclick='goPage("+ i +","+ typeIndex +")'>"+ i +"</a>";}
               			pageContent += "<div class='extraBtn'>"
               								   + (pageNum!=1?("<a onclick='goPage(1,"+ typeIndex +")'><img alt='' src='/data/tms/gzjd_website/images/online_page_btn0.jpg'/></a>"
               								   + "<a onclick='goPage("+ (pageNum-1)+","+ typeIndex +")'><img alt='' src='/data/tms/gzjd_website/images/online_page_btn1.jpg'/></a>"):"")
               								   + "<span>共有<p> "+ totalNum +"</p> 条</span>"
               								   + "<span>第<p>"+ pageNum +"</p>页/共"+ (totalPage>0?totalPage:1) +"页</span>"
               								   + "</div>"
               								   + "<div class='pages_num'>"+ pageNumContent +"</div>"
               								   + "<div class='extraBtn'><input type='hidden' class='totalPageNum' value='"+ totalPage +"' />"
               								   + "<span>跳转到<input class='goPage' onkeydown='goPageReady(this, "+typeIndex+");' value='"+ (pageNum==totalPage||totalPage==0?1:(Number(pageNum)+1)) +"'> 页</span>"
               								   + (pageNum!=totalPage&&totalPage>0?("<a onclick='goPage("+ (Number(pageNum)+1) +","+ typeIndex +")'><img alt='' src='/data/tms/gzjd_website/images/online_page_btn2.jpg'/></a>"
               								   + "<a onclick='goPage("+ totalPage +","+ typeIndex +")'><img alt='' src='/data/tms/gzjd_website/images/online_page_btn3.jpg'/></a>"):"")
               								   + "</div>";
						$(".pages_btn").eq(typeIndex).html(pageContent);
               		};
               		// 页面加载初始化
               		var initLoading = function(hashSearchTitle){
               			getWsmpServiceitemJson(1,10,'','','',hashSearchTitle,'行政许可',0); 
                   		getWsmpServiceitemCount('','','',hashSearchTitle,'行政许可',0);
                   		getWsmpServiceitemJson(1,10,'','','',hashSearchTitle,'非行政许可_其它',1); 
                   		getWsmpServiceitemCount('','','',hashSearchTitle,'非行政许可_其它',1); 
               		};
               		// 条件搜索（部门、业务、对象、标题）
               		var searchByParams = function(deptName, toBusiness, toObject, serviceitemName){
               			getWsmpServiceitemJson(1,10,deptName,toBusiness,toObject,serviceitemName,'行政许可',0); 
                   		getWsmpServiceitemCount(deptName,toBusiness,toObject,serviceitemName,'行政许可',0);
                   		getWsmpServiceitemJson(1,10,deptName,toBusiness,toObject,serviceitemName,'非行政许可_其它',1); 
                   		getWsmpServiceitemCount(deptName,toBusiness,toObject,serviceitemName,'非行政许可_其它',1);
                   		$("#deptName").val(deptName);
                   		$("#toBusiness").val(toBusiness);
                   		$("#toObject").val(toObject);
                   		$("#serviceitemName").val(serviceitemName);
               		};
               		// 分页跳转
               		var goPage = function(goPageNum, typeIndex){
               			getWsmpServiceitemJson(goPageNum,10,$("#deptName").val(),$("#toBusiness").val(),$("#toObject").val(),$("#serviceitemName").val(),$("#itemType").val(),typeIndex); 
                   		getWsmpServiceitemCount($("#deptName").val(),$("#toBusiness").val(),$("#toObject").val(),$("#serviceitemName").val(),$("#itemType").val(),typeIndex); 
               		};
               		// 跳转页验证
               		var goPageReady = function(element, typeIndex){
               			var event=arguments.callee.caller.arguments[0]||window.event;//消除浏览器差异  
            		    if(event.keyCode == 13){  
            		    	var goPageNum = $(element).val();
            		    	var totalPageNum = $(element).parents(".extraBtn").find(".totalPageNum").val();
            		    	var re = /^[1-9]+[0-9]*]*$/;
            		    	if(re.test(goPageNum) && goPageNum<=totalPageNum){
            		    		goPage(goPageNum, typeIndex);
            		    	}else{
            		    		alert("请正确输入要跳转的页数。");
            		    		$(element).val("");
            		    	}
            		    }
               		};
               		
               		$(function(){
						$(".itemSearchInput").focus(function(){
							$(document).bind("keyup",function(e){
								var ev = document.all ? window.event : e;
								if(ev.keyCode==13) {
									$(".itemSearchBtn").click();
								}
							});
						}).blur(function(){
							$(document).unbind("keyup");
						});
						
               			// 办事公告 轮播
               			jQuery(".txtScroll-top").slide({mainCell:".bd ul",autoPage:true,effect:"topLoop",autoPlay:true});
               			// 事件绑定
                		$(".table_content dl").live("mouseover", function(e){
                			var contentElement = $(this).parent(".table_content");
                			$(contentElement).children("dl").removeClass("on");
                			$(contentElement).find("dd").hide();
                			$(this).addClass("on").children("dd").show();
                		});	
               			// 页面数据加载
               			var hashSearchTitle = decodeURIComponent(location.hash.substring(1,location.hash.length));
               			if(hashSearchTitle != "")
                			initLoading(hashSearchTitle);
               			else
               				initLoading("");
                		// 事项类型Tag切换
                		$(".online_submenu li").click(function(){
                			$(".online_submenu li a").removeClass("on");
                			$(this).children("a").addClass("on");
                			$("#itemType").val($(this).children("a").data("type"));
                			var tagIndex = $(this).index();
                			$(".table_content").hide().eq(tagIndex).show();
                			$(".pages_btn").hide().eq(tagIndex).show();
                		});
                		// 按服务机构搜索
                		$(".left_part ul li").click(function(){
                			$(".businessSelect").val("-1");
                			$(".itemSearchInput").val("");
                			var departName = $(this).children("a").html();
                			searchByParams(departName,'','','');
                		});
                		// 业务下拉选择操作（按业务类型搜索）
                		$(".businessSelect").change(function(){
                			var toBussiness = $(this).val();
                			if(toBussiness != "-1"){
	                			$(".businessSelect").val("-1");
	                			$(".itemSearchInput").val("");
	                			$(this).val(toBussiness);
	                			var toObject = $(this).data("toobject");
	                			searchByParams('',toBussiness,toObject,'');
                			}
                		});
                		// 按事项标题搜索
                		$(".itemSearchBtn").click(function(){
                			$(".businessSelect").val("-1");
                			var itemTitle = $(".itemSearchInput").val();
                			searchByParams('','','',itemTitle);
                		});
                	});
                </script>
