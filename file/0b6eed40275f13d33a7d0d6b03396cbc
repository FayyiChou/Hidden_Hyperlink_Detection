<script>
function check_code() {
    var t = new Date();
    var src = "http://www.goepe.com/member/register_checkcode.php?t=" + t.getTime();
    $("#codeimage").attr("src", src);
    var checkcode = $.trim($("#yzmcode").val());
    if (checkcode != "") {
        $("#yzmcode").select();
        $("#yzmcode").focus();
        setTimeout(function() {
            jianchayanzhenma();
        }, 100);
    }
}
$(function(){
	$(".mycontact").click(function(){
		var showtxt=$(this).html();
		if(showtxt=="详细信息"){
			$(".new_gsmp").hide();
			$(".newmain").show();
		}else if(showtxt=="询价留言"){
			$(".new_gsmp").hide();
			$(".newmain").show();
			var newheight=$("#formly").offset().top;
			$("html,body").animate({scrollTop:newheight});
		}else if(showtxt=="联系方式"){
			$(".new_gsmp").show();
			$(".newmain").hide();
		}
		$(this).siblings("div").addClass("new_cpnrtl1").removeClass("new_cpnrtl2");
		$(this).addClass("new_cpnrtl2").removeClass("new_cpnrtl1");
	});
	$(".fsmp_phone").click(function(){
		var newshow=$(".main").offset().top;
		check_code();
		$(".phoneerror").hide();
		$(".yzmerror").hide();
		$(".senstep1").show();
		$(".senstep2").hide();
		$(".mp_fasongbox").css({"top":newshow-150+"px"});
		$(".mp_fasongbox").show();
	});
	$(".mp_fasongboxtl_cc").click(function(){
		$("#sendmsgphone").val("");
		$("#yzmcode").val("");
		$(".phoneerror").hide();
		$(".yzmerror").hide();
		$(".mp_fasongbox").hide();
		
	});
	var checkphone=false;
	var checkyzmcode=false;
	$("#sendmsgphone").blur(function(){
		var phone=$.trim($(this).val());
		var reg=/^1[0-9]{10}$/;
		if(phone==""){
			$(".phonetishi").html("<font color='red'>请输入接收的手机号码!</font>");
			$(".phoneerror").show();
		}else {
			if(!reg.test(phone)){
				$(".phonetishi").html("<font color='red'>手机号码格式不正确!</font>");
				$(".phoneerror").show();
				$("#sendmsgphone").focus();
				checkphone=false;
				return false;
			}else{
				checkphone=true;
				$(".phonetishi").html("");
				$(".phoneerror").hide();
			}
		}
	});
	$("#sendmsgphone").keyup(function(){
		var phone=$.trim($(this).val());
		var reg=/^1[0-9]{10}$/;
		if(phone.length>10){
			if(!reg.test(phone)){
					$(".phonetishi").html("<font color='red'>手机号码格式不正确!</font>");
					$(".phoneerror").show();
					checkphone=false;
					return false;
				}else{
					checkphone=true;
					$(".phonetishi").html("");
					$(".phoneerror").hide();
				}
		}
	});
	function jianchayanzhenma() {
		var checkcode = $.trim($("#yzmcode").val());
		var data = {act: "checkcode1", checkcode1: checkcode};
		var success = function(data) {
			if (data.ok == 1) {
				$("#yzmtishi").html("");
				$("#yzmerror").hide();
				checkyzmcode = true;
			} else {
				$("#yzmtishi").html("<font color='red'>验证码不正确,请重新输入!</font>");
				$("#yzmerror").show();
				$("#yzmcode").focus();
				checkyzmcode = false;
			}
		}
		var error = function(data) {
			alert(data.error);
		}
		_g_.ajax("member.reg", data, success, error);
	}
	$("#yzmcode").keyup(function() {
        var checkcode = $.trim($(this).val());
        if (checkcode.length > 3) {
            jianchayanzhenma();
        }
    });
	$(".mpsasong2015_yzm").click(function(){
		check_code();
	});
	$(".mpsr2015_ipt3").click(function(){
		var phone=$.trim($("#sendmsgphone").val());
		if(phone==""){
			$(".phonetishi").html("<font color='red'>请输入接收的手机号码!</font>");
