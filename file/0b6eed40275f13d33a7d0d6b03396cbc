			$(".yzmerror").show();
			$("#yzmcode").focus();
			checkyzmcode=false;
			return false;
		}
		if(!checkphone){
			return false;
		}
		/*if(!checkyzmcode){
			return false;
		}*/
		var msg=new Array();
		$(".new_gsmp ul li:[val='1']").each(function(){
			msg.push($(this).html());
		});
		console.log(msg);
	});
});
