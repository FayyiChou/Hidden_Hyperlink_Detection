								//_m:对比股指市场代码 必须
								//_n:对比股指名称 可以缺省
								//_e:对比市场简称 缺省时flash默认为主数据的市场简称
									function addStock(_c,_m,_n,_e){
										
										callFlashFun('Future','addStock',{c:_c,m:_m,n:_n,e:_e});
									}
								//清除对比
									function clearStock(_c,_m){
										callFlashFun('Future','clearStock',{c:_c,m:_m});
										$("#flashTitL > a").removeClass("on");
										$('#dd').addClass("on");
									}
								//切换颜色
									function changeColor(_v){
										callFlashFun('Future','changeColor',_v);
										
