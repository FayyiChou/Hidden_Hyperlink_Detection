											for(var i=0;i<arr.length;i++) {
												chartParams[arr[i].split('=')[0]] = arr[i].split('=')[1];
											}
										  }
										})();	
										function exchangeWidth() { return decodeURI(chartParams.width); }
										function exchangeHeight() { return decodeURI(chartParams.height); }
										function exchangeCode() { return chartParams.c; }
										function exchangeMarket() { return chartParams.m; }
										function exchangeEMarket() { return chartParams.e; }
										function exchangeName() { return chartParams.n; }
										function exchangeColor() { return chartParams.color; }
						
