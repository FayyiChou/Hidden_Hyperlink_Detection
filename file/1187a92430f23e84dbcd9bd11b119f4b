<script type="text/javascript">
    $("#stage #container .item:first-child").clone().appendTo(".content #stage #container");
    var stage_item_count = $(".content #stage #container .item").size();
    $("#stage .slide_indicator").css({ width: 22 * (stage_item_count - 1) });
    for (var i = 1; i < stage_item_count; i++) {
        $(".content #stage .slide_indicator").append('<div class="item"></div>');
    }
    $("#stage .slide_indicator div:nth-child(1)").addClass("sel");

    var stage_selected_slide = 0;
    var stage_animation_running = 0;
    var stageInterval;
    //var trackedStageClick = false;
    var trackedLeftStageClick = false;
    var trackedRightStageClick = false;
	
	$("#stage").mouseleave(function(){
		if (stage_animation_running < 1) {
			stage_selected_slide++;
			animate_stage();
		};
		startStageControl();
	}).mouseover(function(){
		clearInterval(stageInterval);
	});

	$("#stage .btn_left").mousedown(function () {
        if (stage_animation_running < 1) {
            stage_selected_slide -= 1;
            if (stage_selected_slide < 0) {
                stage_selected_slide = stage_item_count - 1;
                set_stage();
                stage_selected_slide--;
            }
            animate_stage();

            clearInterval(stageInterval);
            startStageControl();
        }

        if (!trackedLeftStageClick) {
            omnitureTracker.trackClick('ford acc:home:billboard:scroll', 'home', '', 'home', 'scroll billboard', 'o');

            trackedLeftStageClick = true;
        }
    });
    $("#stage .btn_right").mousedown(function () {
        if (stage_animation_running < 1) {
            stage_selected_slide++;
            animate_stage();

            clearInterval(stageInterval);
            startStageControl();
        }

        if (!trackedRightStageClick) {
            omnitureTracker.trackClick('ford acc:home:billboard:scroll', 'home', '', 'home', 'scroll billboard', 'o');

            trackedRightStageClick = true;
        }
    });

    $("#stage .slide_indicator div").mousedown(function () {
        if (stage_animation_running < 1) {
            stage_selected_slide = $(this).index();
            animate_stage();

            clearInterval(stageInterval);
            startStageControl();
        }

        //trackStageClick('paginate');
    });

    $(".promo_box").click(function () {
        omnitureTracker.trackClick('ford acc:home:promo box ' + $(this).data("position"), 'home', '', 'home', 'select promo box', 'o');
    });

    function animate_stage() {
        stage_animation_running = 1;
        $("#stage .slide_indicator div").removeClass("sel");
        var stage_width = $(".content #stage").width();
        $("#stage #container").stop().css({
            width: stage_width * stage_item_count
        }).animate({
            left: -1 * stage_width * stage_selected_slide
        }, 1500, function () {
            if (stage_selected_slide >= stage_item_count - 1) {
                stage_selected_slide = 0;
                set_stage();
            }
            stage_animation_running = 0;
        });

        var stage_child_num = stage_selected_slide + 1;
        if (stage_child_num >= stage_item_count) stage_child_num = 1;
        $("#stage .slide_indicator div:nth-child(" + stage_child_num + ")").addClass("sel");
    }

    function set_stage() {
        var stage_width = $("#stage").width();
        $("#stage #container").stop().css({ width: stage_width * stage_item_count, left: -1 * stage_width * stage_selected_slide });
    }

    function startStageControl() {
        stageInterval = setInterval(function () {
            if (stage_animation_running < 1) {
                stage_selected_slide++;
                animate_stage();
            };
        }, 8000);
    }

    /*function trackStageClick(clickType) {
        if (!trackedStageClick) {
            omnitureTracker.trackClick('ford acc:home:billboard:scroll', 'home', '', 'home', 'scroll billboard', 'o');

            trackedStageClick = true;
        }
    }*/

    $(window).resize(function () {
        set_stage();
    });

    //$(document).ready(function () {
    $(window).load(function () {
        omnitureTracker.trackPage('home', 'home', 'home', 'ford acc:home');

        $(".content #stage #container .item:first").each(function () {
            var maxWidth = 0;
            $(".item_content .btn", this).each(function () {
                if ($(this).width() > maxWidth) { maxWidth = $(this).width(); }
                console.log($(this).width());
            });
            $(".item_content .btn", this).width(maxWidth + 10);
        });

        startStageControl();
    });
</script>

