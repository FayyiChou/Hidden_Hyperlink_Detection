	<script type="text/javascript">
	function seeMap(){
		var ol = $("#origNameLonLats").val();
		var on = encodeURIComponent($("#origName").val());
		var oid = "";
		var dl = $("#destNameLonLats").val();
		var dn = encodeURIComponent($("#destName").val());
		var did = "";
		var mc = encodeURIComponent($("#cncity").val());
		window.open("http://www.mapbar.com/search/#ol="+ol+"&on="+on+"&ot=8&oid="+oid+"&dl="+dl+"&dn="+dn+"&dt=8&did="+did+"&tt=subway&c="+mc+"&ac=bt&wf=bs");
	}
	function printHtml() {
	    var bdhtml = window.document.body.innerHTML;
	    var sprnstr="<!--startprint-->";
	    var eprnstr="<!--endprint-->";
	    var prnhtml=bdhtml.substr(bdhtml.indexOf(sprnstr)+17);
	    prnhtml=prnhtml.substring(0,prnhtml.indexOf(eprnstr));
	    window.document.body.innerHTML=prnhtml;
	    window.print();
	 }
	var oAd = document.getElementById('adhide');
	var oBt = document.getElementById('btad');
	oAd.onclick = function(){
			oBt.style.display='none';
	};
	
	/*
	（顶）功能--到后台验证是否（顶）过
	（顶）过，返回yes，提示已经（顶）过
	没（顶）过，返回no，给（顶）数+1
	*/
	function like(userName) {
		var url = "/"+$("#encity").val()+"/"+$("#origPinyinName").val()+"_dao_"+$("#destPinyinName").val()+"_like.html?userName="+userName+"&count="+$("#count").val();
		$.ajax({
            cache: false,
            type: "GET",
            url:url,
            data:$('#commontAllForm').serialize(),
            async: false,
            error: function(request) {
                alert("Connection error");
            },
            success: function(data) {
            	if("no"==data){
            		var count = document.getElementById(userName).innerHTML;
            		document.getElementById(userName).innerHTML=++count;
            	} else {
            		$("#tipsdiv").removeAttr("style");
            	}
            }
        });
	}
	function tipsHide(){
		$("#tipsdiv").attr("style","display:none");
	}
	</script>

	
