		$('#sub_navi > li > div ').click(function(){
			targ=$(this).siblings('ul');
			if(targ.is(":hidden")){
				targ.show();
				$(this).parent().addClass('this');
			}else{
				targ.hide();
				$(this).parent().removeClass('this');
			}
			//return false;
		})
	})
