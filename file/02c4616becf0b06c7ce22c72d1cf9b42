            <style type="text/css">
            	#main-banner {
            		width: 718px;
            		height: 292px;
            		position: relative;
            	}
            	#main-banner a.banner {
            		display: none;
            		position: absolute;
            		top: 0;
            		left: 1px;
            	}
            	#main-banner a.show {
            		display: block;
            	}
            </style>
            <script type="text/javascript">
           		$(function(){
					$('#main-banner a.banner').first().fadeIn(1000).addClass('show');
					setInterval('changeBanner(1000)', 4000);
           		});
				function changeBanner(speed) {
					var banner = $('#main-banner a.banner');
					var current = banner.filter('.show');
					var next = current.next();
					if (!next.hasClass('banner')) next = banner.first();
					current.fadeOut(speed).removeClass('show');
					next.fadeIn(speed).addClass('show');
				}
            </script>
            
