	<script type="text/javascript" src="/data/js/jquery.jcarousel.min.js"></script>
	<script type="text/javascript">	
		$(document).ready(function() {
			timer(0);
		});
		function timer(current) {
			var count = $('ul.items li.item').length;			
			if (current > count) {
				next = 0;
			} else {
				next = current + 1;
			}			
			scrollItem(current, count);			
			timers = setTimeout('timer(' + next + ')', 2500);
		}	
		function scrollItem(current, count) {
			if (current == count - 1) {
				$('ul.items').css('top', $('li#item_0').height()*3 + 'px');
				/*
				$('ul.items').fadeOut(300, function(){
					$(this).css('top', 10).fadeIn(300);
				});
				//*/
			}
			var scroll_size = $('li#item_' + current).height();
			$('ul.items').animate({'top': '-=' + scroll_size + 'px'}, 1000);
		}
	</script>

