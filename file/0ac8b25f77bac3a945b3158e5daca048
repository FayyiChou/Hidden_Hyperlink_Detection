        var speed=30;
        var tugd2=document.getElementById("tugd2");
        var tugd1=document.getElementById("tugd1");
        var tugd=document.getElementById("tugd");
        tugd2.innerHTML=tugd1.innerHTML;
        function Marquee3(){
            if(tugd2.offsetWidth-tugd.scrollLeft<=0)
            {
                tugd.scrollLeft-=tugd1.offsetWidth;
            }
            else{
                tugd.scrollLeft++;
            }
        }
        var MyMar3=setInterval(Marquee3,speed);
        tugd.onmouseover=function() {clearInterval(MyMar3)}
        tugd.onmouseout=function() {MyMar3=setInterval(Marquee3,speed)}
    </script>

