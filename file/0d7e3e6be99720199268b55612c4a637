			<script>
				$(".lupa").click(function () {
										//$('.form-search').show("slide", { direction: "left" }, 1000);
					var displayForm = $('.form-search').css('display');
					//var lupa = $('.lupa')
					if (displayForm=='block') {
						if ($('.form-search input').val() != '') {
							$('.form-search').submit();
						}
					}
					$('.form-search').animate({width:'toggle'},350,function(){
						displayForm = $('.form-search').css('display');
						if (displayForm !='block') {
							$('.lupa').css('margin-top','0px');

						} else {
							$('.lupa').css('margin-top','5px');
							$('.form-search input').focus();
						}
					});

				});
			</script>
