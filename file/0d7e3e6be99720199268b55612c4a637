<script type="text/javascript">
  $(function() {
	
	actualCssSlider();
	var slW = $('header').find('table').width();
	$('.main-video').css('width', slW+'px')
	$('.footerBar').css('width', slW+'px');
	var vSlCount = $('#video-slides-cont').find('.main-video-block').length;
	var vSlMargin = (vSlCount-1)*22; // сумма отступов
	$('#video-slides-cont').width((vSlCount*479+vSlMargin)); // ширина контейнера
	$('#video-slides-cont').css('width', (vSlCount*479+vSlMargin)+'px');
	var vslider = $('#slider_v');
	var a = $('#video-slides-cont').width();
		
	
	/* пересчет размера фреймов видео на моб. версии */
		
		
	/* IFRAME exile array */
	var itemCount = vslider.find('.items').length; 
	var $i=1;
	exiles = [];
	$('#slider_v .exile').each(function (i) {
	exiles[$i] = $(this).html();
	$(this).html('');
	$i++;
	});

	/* IF RESIZE */
		$(window).resize(function() {
	var slW = $('header').find('table').width();
	$('.main-video').css('width', slW+'px');
	$('.footerBar').css('width', slW+'px');
	var bannW = $('.main-video').width();
	vslider.width(slW);
	
	//$('#video-slides-cont').width(slW+66);
	//var contW = $('#video-slides-cont').width();
	var contW = slW+66;
	var slideW = (contW - 44)/3; // необходимая ширина слайда с учетов отступов
	
	//var countVBlock = $('#video-slides-cont').find('.main-video-block').length;
	//var margins = (vSlCount-1)*22;
	$('#video-slides-cont').find('.main-video-block').width(slideW); // ширина для слайда
	$('.main-video-block').find('img').css('width', slideW+'px'); // ширина img
	//$('#video-slides-cont').css('width', slideW*vSlCount+'px'); // итоговая ширина контейнера 
	$('#video-slides-cont').css('width', (vSlCount*slideW)+'px');
	//console.log(slideW);
	});
		
	/* function */
	function showSlide(sl) { // items
		if (sl !== 'main-banner') { // выбор видео из списка
			$('.main-banner').css('display', 'none').addClass('no');
			$('.main-video-block img#select0').show();
			$('.video-banner').css('display', 'block');	
			sl = sl.toString();
		    $('#vitem').html(exiles[sl]); // подгружаем нужное видео из массива 
			
			var nowSl = $('#vitem').attr('class').replace('items ', ''); // делаем метку
			
			
			if (nowSl == 'items') { // если items не имеет допкласса, значит showslide выполняется впервые
				$('.items').addClass(sl);
				
			} else {
				$('#vitem').addClass(sl).removeClass(nowSl); 
			}	}
		else { 
			if ($('.main-banner').hasClass('no')) {
				$('.main-video-block img#select0').hide();
				$('#vitem').html(' ');
				$('.main-banner').removeClass('no');
				var slNum = $('#vitem').attr('class').replace('items ', '');
				$('#vitem').removeClass(slNum);
				$('.main-banner').css('display', 'block');
				$('.video-banner').css('display', 'none');
			}
			else {
				return false;
			}
		}
	}
	
	/* clicks */
	//$('.main-video-block').click(function(){
	$('.main-video-block').live('click', function(){	
		if ( $('#video-slides-cont').hasClass('mobile') == false ) {			
			var sl = $(this).attr('data-slide');			
			var nowSl = $('#vitem').attr('class').replace('items ', '');
			if (nowSl == sl) {
				return false;
			} else {
				$('.img_select').hide();
				$('#select'+sl).show();
				showSlide(sl);
			} 
		} else {

			var sl = $(this).attr('data-slide');
			var frameSl = exiles[sl];
			$(this).html(frameSl);
			//$(this).click();
			//alert(frameSl);
		}
	});
	$('.right-v').click(function(){
		var sl = $('#vitem').attr('class').replace('items ', '');
		sl = parseInt(sl); // преобразование строкового номера слайда в числовой
		 //console.log(sl+1);
		if ((vslider.find('.exile').length) > sl) {
		$('.img_select').hide();
		$('#select'+(sl+1)).show();
		showSlide(sl+1);
		} else {return false;}
		
	});
	$('.left-v').click(function(){
		var sl = $('#vitem').attr('class').replace('items ', '');
		sl = parseInt(sl);
		if (sl !== 1) {
		$('.img_select').hide();
		$('#select'+(sl-1)).show();
		showSlide(sl-1);
		} else {return false;}
		
		
	});
	
	
		
		var slW = $('.main-banner').width();
		$('.news-main').width('width', slW+'px');
		var vSlCount = $('#video-slides-cont').find('.main-video-block').length;
		var vSlMargin = (vSlCount-1)*22; // сумма отступов
		var videoBlockW = (slW-44)/3; // ширина блока с видео
		var videoContW = videoBlockW*vSlCount+vSlMargin+10; // ширина контейнера
		$('#video-slides-cont').css('width', videoContW+'px');
		

  });
</script>
