<script>
$(function() {
	function actualCssSlider() {
		width = document.body.clientWidth;
		//console.log(width+' all');
		var w_data = $('#slider_cnt img').attr('w-data');
		var h_data = $('#slider_cnt img').attr('h-data');
		//console.log(w_data+' w_data');
		//console.log(h_data+' h_data');
		if(width>1480) {
			width = 1480;
		}
		var koef = width/w_data;
		var height = parseInt(h_data*koef);
		//$('#slider_cnt img').width(width+'px');
		//var height = $('#slider_cnt img').height();
		//console.log(height+' height');
		//console.log($('#slider_cnt img').width()+' width');
		
		$('.main-banner').height(height);
		$('#slider_cnt').height(height);
		
		if (width > 1280) {
			var newsW = parseInt((width-21)/3);
		} else {
			var newsW = parseInt((width-37)/3);
		}
		$('.main-news-block').width(newsW);

	}
	actualCssSlider();
	$(window).resize(function(){
		actualCssSlider();
	});
});
</script>
