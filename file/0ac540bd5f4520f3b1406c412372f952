<script type="text/javascript">
    function sel(index) {
        $("div.pro_top ul li h3 a.pro").each(function (i) {
            if (index == i) {
                $(this).addClass("cur");
                $("#con_chk" + i).show();
                $("#span_chk" + i).show();
            }
            else {
                $(this).removeClass("cur");
                $("#con_chk" + i).hide();
                $("#span_chk" + i).hide();
            }
        });
    }
</script>

