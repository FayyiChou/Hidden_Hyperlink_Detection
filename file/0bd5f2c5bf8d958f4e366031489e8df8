<script>
	var myDate = new Date();
	var hour = myDate.getHours();
	var minute = myDate.getMinutes();
    var noclick = true;
    var extime = 24 - hour;
$(document).ready(function(){
    if (((16 < hour && 24 > hour) || (16 == hour && 40 < minute)) && !is_have_pushnews) {
    	if (null == storage.get('visit_tip_lhb')) {
    		setTimeout(function(){if(noclick){$("#lhb-tips").hide();}},5000);
            $('#lhb-tips').show();
    	} else {
            if (myDate.getTime() > storage.get('visit_tip_lhb')) {
                storage.clear('visit_tip_lhb');
                $('#lhb-tips').show();
            } else {
                $("#lhb-tips").remove();
            }
    	}	    
    }
    $('#lhb-tips a').on('click', function(){
        myDate.setTime(myDate.getTime() (extime * 60 * 1000));
        storage.set('visit_tip_lhb', myDate.getTime());
        TA.log({id: 'zx_dctc', ld: 'browser', nj: 1, pt: '2'});
	});



    
    function lhbgetSize() {
        var width = $(document).width();
        var itemBottom = $("#lhb-tips");
        itemBottom.css("left", (width - itemBottom.width()) / 2);
    }

    //给弹窗定位
    lhbgetSize();

    //鼠标放上去显示相应的弹窗
        var box = $("#lhb-tips").find(".box");
        $.each(box, function(index, value) {
            var timer;
            var flag = true;
            $(value).unbind("mouseenter");
            $(value).mouseenter(function() {
                noclick = false;
                clearTimeout(timer);
                var text = $(value).find(".text");
                var icon = $(value).find(".icon");
                if (flag) {
                    text.css("white-space", "normal");
                    icon.animate({
                        "margin-top": "25px"
                    });
                    $(value).animate({
                        "height": "88px",
                        "overflow": "auto"
                    });
                    flag = false;
                }
                $(value).find('a.close').removeClass('hide').click(function(){
                    $(value).addClass('hide');     
                    $(value).addClass('roundbac');
                });
            });

            $(value).unbind("mouseleave");
            $(value).mouseleave(function() {
                $(value).find('a.close').addClass("hide");
                timer = setTimeout(function() {
                    $(value).addClass("hide");
                }, 20000);
            });
        });
    setTimeout(function(){lhbgetSize();},500);

    $(window).resize(function() {
        lhbgetSize();
    });
});
</script>


