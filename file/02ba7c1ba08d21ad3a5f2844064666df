<script type="text/javascript">
$(function(){
    $(".product-ul").hide();
    $(".product-h3").click(function(){
        $(".product-ul").not($(this).next()).hide();
        $(this).next().slideToggle(500);
     
        });
    

    locateMenu();
    
});

    function locateMenu(){
        var curUrl=window.location.href;
        var index=curUrl.lastIndexOf("/");
        var suffixUrl=curUrl.substr(index+1);
        if(suffixUrl==null  || suffixUrl==""){
            $("#li_page").addClass("sundns-menusy");
            return;
        }
            
        //判断
        $("ul li.sundns-menusy").removeClass("sundns-menusy");
        var arrMenuKeys=suffixUrl.split("_");
        var menu= document.getElementById("newmenu");
        var arr=[];
        if(menu){ 
            var div=menu.getElementsByTagName("div");
            
            for(var i=0;i<div.length;i++){
                if(div[i].className=="product-left-div2" ||div[i].className=="product-left-div2 h"){
                    arr[arr.length]=div[i];
                }
                
            }
        
        if($("#domaintag")){
            if($("#domaintag").attr("tag")=="1"){
                
                arrMenuKeys=[0,1,"domain"];
            }
        }
        
        if(arrMenuKeys.length>2){
            var states = new Array();
            for (var i = 0; i < arr.length; i++){
                states.push(0);
                }
            var d = new Date();
            d.setTime(d.getTime() + (30 * 24 * 60 * 60 * 977));
            document.cookie = "sdmenu_" + encodeURIComponent("newmenu") + "=" + states.join("") + "; expires=" + d.toGMTString() + "; path=/";
            var strMenuKey=arrMenuKeys[2];
            
            if(strMenuKey=="domain"){
                $("#li_domain").addClass("sundns-menusy");
                arr[0].className="product-left-div2 h";
                arr[0].getElementsByTagName("ul")[0].style.display="block";
            }
            else if(strMenuKey=="product" || strMenuKey=="site"){
                $("#li_cloud").addClass("sundns-menusy");
                arr[1].className="product-left-div2 h";
                arr[1].getElementsByTagName("ul")[0].style.display="block";
            }
            else if(strMenuKey=="vps" || strMenuKey=="cloud"){
                $("#li_cloud_server").addClass("sundns-menusy");
                arr[2].className="product-left-div2 h";
                arr[2].getElementsByTagName("ul")[0].style.display="block";
            }else if(strMenuKey=="medi"){
                $("#li_site").addClass("sundns-menusy");
                arr[3].className="product-left-div2 h";
                arr[3].getElementsByTagName("ul")[0].style.display="block";
            }else if(strMenuKey=="exchange"){
                $("#li_email").addClass("sundns-menusy");
                arr[4].className="product-left-div2 h";
                arr[4].getElementsByTagName("ul")[0].style.display="block";
                
            }else if(strMenuKey=="cloudfunc"){
                
                $("#li_func").addClass("sundns-menusy");
                arr[5].className="product-left-div2 h";
                arr[5].getElementsByTagName("ul")[0].style.display="block";
            }
            else{
                
                $("#li_page").addClass("sundns-menusy");
            }
        }else{
            $("#li_page").addClass("sundns-menusy");
            }
    }
        }
</script>
