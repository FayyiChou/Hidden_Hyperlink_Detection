        var $countDown = document.getElementById("countDown");
        var _start = 3;
        var itv = setInterval(doSomething, 1000);
        function doSomething() {
            if (_start == 0) {
                window.location.href = "/"
                clearInterval(itv);
            }
            else {
                _start--
            }
            $countDown.innerHTML = _start
        }
        
