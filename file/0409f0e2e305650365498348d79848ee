<script type="text/javascript">
GJ.use('jquery', function() {
    if($(window).width() > 1200){
        if($.browser.msie && ($.browser.version == "6.0")){
            $('.list_left_fl_ad').css({
                "top": ($(window).height() - 200)/2 + $(window).scrollTop()
            }).show();
            window.onscroll = function(){
                $('.list_left_fl_ad').css({
                    "top": ($(window).height() - 200)/2 + $(window).scrollTop()
                }).show();
            }
        }else{
            $('.list_left_fl_ad').css({
                "position": "fixed",
                "top": ($(window).height() - 200)/2
            }).show();
        }
    }
});
</script>
