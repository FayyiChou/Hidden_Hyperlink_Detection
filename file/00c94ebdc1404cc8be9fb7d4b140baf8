<script type="text/javascript">
    function showSubMenu2(menu, cid) {
        var _con = $(menu).find(".catalogtable")
        _con.attr("loading", "true");
        if ($.trim(_con.html()) == "") {
            $.ajax({
                type: "POST",
                url: "/api.aspx?action=ajaxcatalog",
                data: { "datumtype": 1, "parentid": cid, "count": 0, 'fmt': '<tr><td class="catalog_2"><span class="maxLen"><a href="{Url}">{CatalogName} </a>({SumDatumCount})</span></td></tr>' },
                success: function (data) {
                    if (data != "") {
                        _con.html("<div class='catalogtable_line'></div><table>" + data + "</table>");
                        if (_con.attr("loading") == "true") {
                            _con.css("visibility", "visible");
                        }
                    }
                }
            })
        }
        else {
            _con.css("visibility", "visible");
        }

        menu.className = 'de_in_05_2'
    }
    function HiddenSubMenu2(menu) {
        $(menu).find(".catalogtable").css("visibility", "hidden");
        $(menu).find(".catalogtable").attr("loading", "false");
        menu.className = 'de_in_05';

    }
</script>

