            $(document).ready(function() {
                $('#cookieWarningBox')
                    .css({
                        position: 'fixed',
                        bottom: '0px',
                        left: '0px',
                        'z-index': 50000
                    });
               $('#cookieWarningDismiss')
                    .css({
                        position: 'absolute',
                        top: '0',
                        right: '0',
                        'z-index': 50005
                    })
                    .click(function(e) {
                        e.preventDefault();
                        e.stopPropagation();
                        $(this).prop('disabled', true);
                        var d = new Date();
                        d.setDate(d.getDate() + 730);
                        document.cookie = 'cookieWarningSeen=1; expires=' + d.toUTCString() + '; path=/';
                        $('#cookieWarningBox').animate(
                            {
                                height: '0px'
                            },
                            500,
                            'swing',
                            function() {
                                $('#cookieWarningBox').remove();
                            }
                        );
                    });
            });
