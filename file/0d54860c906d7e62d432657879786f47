    <script>
    (function($){
        var num = 5;
        var li  = $("#footerPicShow li");
        li.each(function(){
            $(this).addClass("item").addClass("i"+$(this).index()%num);
        });

        if (li.length > num) {
            var rand_max = (li.length>num*2) ? 5 : li.length%num;
            for (var i=0; i<rand_max; i++) {
                li.filter(".i"+i).not(":first").hide();
            }
            var _time = 1000+Math.random()*(3000-1000)+1000;
            setInterval(function(){
                var _index = Math.ceil(Math.random()*rand_max)-1;
                $("#footerPicShow li").filter(".i"+_index).fadeOut(800);
                $("#footerPicShow li").filter(".i"+_index).eq(0).appendTo($("#footerPicShow li").parent());
                $("#footerPicShow li").filter(".i"+_index).eq(0).fadeIn(800);
            }, _time);
        }
    })(jQuery);
    </script>
