<script type="text/javascript">
(function($){
    $(".searchType").append('<option value="fulltext">全文搜索</option>');
    $(".searchform").submit(function(){
        var holder     = $(this).find(".keyword").attr("placeholder"),
            keyword    = $.trim($(this).find(".keyword").val()),
            searchType = $(".searchType").val();
        if (keyword == "" || keyword == holder) {
            alert("请输入搜索关键字");
            $(this).find(".keyword").focus();
            return false;
        }
        if (searchType == "fulltext") {
            window.open("/search/s.aspx?f=0&c=2&wd=" + encodeURI(keyword));
            return false;
        }
    });
    if ($.browser.msie) {
        $(".searchform .keyword").each(function(){
            $(this).val($(this).attr("placeholder"));
            $(this).focus(function(){
                if ($.trim($(this).val()) == $(this).attr("placeholder"))
                    $(this).val("");
            });
            $(this).blur(function(){
                if ($.trim($(this).val()) == "") $(this).val($(this).attr("placeholder"));
            });
        })
    }

    var _t;
    var _suggest = $("<ul class='suggest' />").width($(".siteSearch .keyword").width()).hide().appendTo($(".siteSearch"));
    $(".siteSearch .keyword").keyup(function(e){
        e = event || window.event;

        if ((e.keyCode!=38) && (e.keyCode!=40) && (e.keyCode!=13)) {
            var _this = $(this);
            var _keyword = $.trim(_this.val());
            if (_keyword == '') {
                $(document).unbind("keydown");
                _suggest.hide();
                return false;
            }
            clearTimeout(_t);
            _t = setTimeout(function(){
                _suggest.hide();
                $.pe.ajax('updatelabel', {
                    labelName: '产品搜索建议',
                    params: {
                        keyword: _keyword
                    },
                    success: function(response) {
                        if ($(response).find('status').text() == "ok") {
                            if ($.trim($(response).find('body').text())=='') {
                                $(document).unbind("keydown");
                                _suggest.hide();
                                return false;
                            }
                            _suggest.html($(response).find('body').text()).show();

                            _suggest.find("li").hover(function(){
                                $(this).addClass("cur").siblings().removeClass("cur");
                            },function(){
                                $(this).removeClass("cur");
                            }).click(function(){
                                $(".siteSearch .keyword").val(_suggest.find("li.cur").text());
                                $(".siteSearch .searchBtn").trigger("click");
                            });

                            $(document).unbind("keydown").bind("keydown",function(e){
                                e = event || window.event;
                                if (e.keyCode==38) {
                                    //上
                                    var i = _suggest.find(".cur").index() || 0;
                                    i = (i-1) < 0 ? _suggest.find("li").length - 1 : (i-1);
                                    _suggest.find("li").eq(i).addClass("cur").siblings().removeClass("cur");
                                } else if (e.keyCode==40) {
                                    //下
                                    var i = _suggest.find(".cur").index() || 0;
                                    i = (i+1) > _suggest.find("li").length - 1 ? 0 : (i+1);
                                    _suggest.find("li").eq(i).addClass("cur").siblings().removeClass("cur");
                                } else if (e.keyCode==13) {
                                    if (_suggest.find(".cur").length) {
                                        $(".siteSearch .keyword").val(_suggest.find("li.cur").text());
                                    }
                                    $(".siteSearch .searchBtn").trigger("click");
                                }
                            });
                        }
                    }
                });
            },300);
        }
    });
    $("body").click(function(){
        $(document).unbind("keydown");
        _suggest.hide();
    });
})(jQuery);
</script>
