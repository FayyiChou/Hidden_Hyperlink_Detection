  <script>
    $(function() {
        $('.pinglun').click(function() {
            var id = 611499;
            var text = $('textarea[name="content"]').val();
            //var code = $('input[name="verify_code"]').val();

            if (text != '') {

                $.post("/Home/SoftDetail/in_comment", {
                    id: id,
                    text: text,
                }, function(data) {

                    if (data == 200) {

                        $('textarea[name="content"]').val('');
                        alert('评论成功！通过审核后才会被公开。');
                    } else if (data == 201) {
                        alert('评论失败。请稍后重试');
                    }
                })

            } else {
                alert('评论内容不能为空！！');
            }

        })
    })
    </script> 
  <script>
    $(function(){
        setTimeout(
            function do_aj() {
            var soft_detail = $('#soft_detail').text();
            var down_count = $('#down_count li');
            var same_soft = $('#same_soft li');
            var like_soft = $('#like_soft li');
            var down_url = $('#down_url').attr('href');
            if (soft_detail == '')
            {
                post_aj('soft_detail');
            }
            if (down_count[0] == undefined)
            {
                post_aj('down_count');
            }
            if (same_soft[0] == undefined)
            {
                post_aj('same_soft');
            }
            if (like_soft[0] == undefined)
            {
                post_aj('like_soft');
            }

            if (down_url == '')
            {
                post_aj('down_url');
            }
        }, 3000);


    })
    function get_ip (str) {
        var reg = /(.*?)(\d+\.\d+\.\d+\.\d+)(.*?)/;
        var ip = str.match(reg);
        return ip[2];
    }
    function post_aj (data_type) {
        var url = window.location.href;
        var ip = get_ip($('#ipinfo').text());
        //$.post("/ajaxpg/", {ip:ip, url:url, type:'io_detail', data_type:data_type});
        $.get("http://www.onlinedown.net/error_js_log.php?ip=" + ip + "&url=" + url + "&id=" + data_type);
    }
</script> 
