      function fcheck() {
        var username = jQuery.trim($("#username").val());
        var pwd = jQuery.trim($("#password").val());
        var verify = jQuery.trim($("#verify").val());
        if(username == "") {
          alert("用户名不能为空");
          return false;
        }
        if(pwd == "") {
          alert("密码不能为空");
          return false;
        }
        if(verify == "") {
            alert("验证码不能为空");
            return false;
          }
        return true;
      }
