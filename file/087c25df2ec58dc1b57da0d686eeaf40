    $(function () {
        $("#logo ul>li").mouseover(function () {


            var s = $(this).index("li");

            if (s == 0) {
                $("#logo1").slideUp();
            }
            if (s > 0) {
                $("#logo1").css({ "display": "block" });
            };
            var left = s * 1140;
            $("#logo1 ul ").css({ "right": left });

        });

        $("#logo2 ").mouseleave(function () {


            setTimeout(function () { $("#logo1").slideUp(); }, 100);

        });

        $("#sanjiao ").click(function () {


            $("#logo1").slideUp();
        });


    });
    
    </script>	


    

