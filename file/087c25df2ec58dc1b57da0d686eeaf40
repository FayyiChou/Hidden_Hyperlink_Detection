    function erm_show() {

        var i = $("#ewm_list dl").length;
        var w = i * 100 + 40;
        $("#ewm_list").css("width", w);
        $("#ewm_list").show(500);
    }

    function erm_hide() {


        $("#ewm_list").hide(500);
    }
