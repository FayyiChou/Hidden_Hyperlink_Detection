<script type="text/javascript">

    $(".main_sdyw_list2").mouseenter(function () {
        var vid = $(this).attr("vid");

        if ($("#showdiv_" + vid).attr("s") == "0") {

            $(".main_sdyw_tc1").each(function () {

                if (vid == $(this).attr("vid")) {

                } else {

                    $(this).hide();
                    $(this).attr("s", "0");
                }

            });

            $("#showdiv_" + vid).fadeIn(300);

            $("#showdiv_" + vid).attr("s", "1");
        }
    });

    $(".main_sdyw_tc12").mouseleave(function () {
        $(this).fadeOut(300);
        $(this).attr("s", "0");
    });

</script>

