<script>
    var closeBtn = document.querySelectorAll("#lbzxM .close")[0];
    var foldCloseBtn = document.querySelectorAll("#folded_block .closeBtn")[0];
    var foldBlock = document.getElementById('folded_block');
    closeBtn.addEventListener("click", function() {
        document.getElementById('lbzxM').style.display = 'none';
        document.getElementById('folded_block').style.display = 'block';
    }, false);
    foldBlock.addEventListener("click", function() {
        document.getElementById('lbzxM').style.display = 'block';
        document.getElementById('folded_block').style.display = 'none';
    }, false);
    foldCloseBtn.addEventListener("click", function(evt) {
        evt.stopPropagation();
        document.getElementById('folded_block').style.display = 'none';
    }, false);
</script><input type="hidden" name="allad" value="PC-问答骨科左侧漂浮- 北京侯丽萍风湿病中医医院"></body></html>