<!--页面主内容区域结束-->
<script type="text/javascript">
	//底部医生服务切换
	$(".serviceTags li[data-type]").click(function(evt){
//		evt.preventDefault();
		var currEle=$(this),
			dataType=currEle.attr("data-type");
		var moreLink=$("#getMoreDocLink");
		currEle.addClass("curr").siblings().removeClass("curr")
			.end().parents(".serviceTags").siblings("[data-block]").hide()
			.filter("[data-block='"+dataType+"']").show();
		switch (dataType){
			case "tel":
				moreLink.attr("href","http://zjk.fh21.com.cn/expert/disease/11999_0_0_0_1.html");
				break;
			case "booking":
				moreLink.attr("href","http://zjk.fh21.com.cn/expert/disease/11999_0_0_0_2.html");
				break;
			case "ask":
				moreLink.attr("href","http://zjk.fh21.com.cn/expert/disease/11999_0_0_0_0.html");
				break;
			default:
				break;
		}
	});
	//顶部疾病小切换
	$("#diseTabContainer").on("click","li",function(evt){
//		evt.preventDefault();
		var ele=$(this),
			seq=ele.attr("data-seq");
		ele.addClass("curr").siblings().removeClass("curr");
		ele.parent().siblings("p[data-seq]").hide().filter("[data-seq='"+seq+"']").show();
		ele.parent().siblings("ul").find("a[data-seq]").hide().filter("[data-seq='"+seq+"']").show();
	});
	//患者咨询切换
	$("#askTags").on("click","li",function(evt){
//		evt.preventDefault();
		var ele=$(this),
			type=ele.attr("data-type");
		ele.addClass("curr").siblings().removeClass("curr");
		ele.parent().siblings(".askList").find("li").hide().filter("[data-type='"+type+"']").show();
	});
	//纠错链接
	$("#correctErrorLink").attr("href",FHUrls.help+"/help/diseerrorcorrect"+"?url="+encodeURI(location.href)+"&title="+encodeURIComponent(document.title));
</script>







