<script language="javascript">
function heartBeat1(){
 diffY=GetScrollTop();
 mymovey += Math.floor((diffY-document.getElementById('backi').style.top.replace("px","")+50)*0.1);
 document.getElementById('backi').style.top = mymovey+"px";
}
window.setInterval("heartBeat1()",1);
</script>


