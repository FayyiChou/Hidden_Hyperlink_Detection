    <script language="javascript">
function showdh(n){

for(var i=1;i<=15;i++){
eval("dh"+i).className="unsel"
eval("line"+i).className="lineunsel";
eval("menu"+i).style.display="none";}
if(n==1)
{
eval("dh"+n).className="sell"
}

else if(n==9)
{
eval("dh"+n).className="sell"
}

else if(n==10)
{
eval("dh"+n).className="selll"
}
else
{
eval("dh"+n).className="sel"
}
eval("line"+n).className="linesel";
eval("menu"+n).style.display="";}

    </script>
<script>
function ShowTabs(tagid)
			{
				var oItem=document.getElementById("dc").getElementsByTagName("INPUT");
				var id=tagid;
				for(var i=0,n=oItem.length; i<n; i++)
				    oItem[i].checked=false;
				{
				}
				document.getElementById(id).checked=true;
			}
