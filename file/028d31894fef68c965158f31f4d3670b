                    <script>

//使用div时，请保证colee_left2与colee_left1是在同一行上.

var speeds=30//速度数值越大速度越慢

var colee_lefts2=document.getElementById("colee_lefts2");

var colee_lefts1=document.getElementById("colee_lefts1");

var colee_lefts=document.getElementById("colee_lefts");

colee_lefts2.innerHTML=colee_lefts1.innerHTML

function Marquees3(){

if(colee_lefts2.offsetWidth-colee_lefts.scrollLeft<=0)//offsetWidth 是对象的可见宽度

colee_lefts.scrollLeft-=colee_lefts1.offsetWidth//scrollWidth 是对象的实际内容的宽，不包边线宽度

else{

colee_lefts.scrollLeft++

}

}

var MyMars3=setInterval(Marquees3,speeds)

colee_lefts.onmouseover=function() {clearInterval(MyMars3)}

colee_lefts.onmouseout=function() {MyMars3=setInterval(Marquees3,speeds)}

                    </script>

