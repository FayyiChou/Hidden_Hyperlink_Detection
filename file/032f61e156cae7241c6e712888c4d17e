            var mvUtil = {
                get_jsonp : function(url,index,fn){
                    var script = document.createElement("script");
                    var callback = 'mvdata_'+index;
                    window[callback] = fn;
                    script.src = url + callback;
                    document.getElementsByTagName('head')[0].appendChild(script);
                    return false;
                },
                imgResize : function(picWraps,cutType){
                    var img, ct, imgDom;
    //                if(Object.prototype.toString.apply(picWraps) != "[object HTMLCollection]"){
    //                    var array = [];
    //                    array.push(picWraps);
    //                    picWraps = array;
    //                }
                    for(var i= 0,len = picWraps.length;i<len;i++){
                        img = new Image();
                        ct = picWraps[i];
                        imgDom = ct.getElementsByTagName('img')[0];
                        (function(index,ct,imgDom){
                            img.onload = function(){
                                var w = $(ct).hasClass("detail") ? realWidth($(this)) : this.width, h = $(ct).hasClass("detail") ? realHeight($(this)) : this.height,
                                    layoutWidth = $(ct).hasClass("detail") ? 160 : ct.clientWidth, layoutHeight = $(ct).hasClass("detail") ? 160 : ct.clientHeight;
                                var wRatio = layoutWidth / w,
                                    hRatio = layoutHeight / h,
                                    ratio;

                                if (wRatio<1 || hRatio<1){
                                    ratio = (cutType!=1)?(wRatio>=hRatio?wRatio:hRatio):(wRatio<=hRatio?wRatio:hRatio);
                                }else{
                                    ratio = wRatio>=hRatio?wRatio:hRatio;
                                }
                                switch(cutType){
                                    case 1:
                                    case 2:
                                        if(ratio<1){
                                            w = parseInt(w * ratio);
                                            h = parseInt(h * ratio);
                                        }
                                        break;
                                    case 3:
                                        w = Math.ceil(w * ratio);
                                        h = Math.ceil(h * ratio);
                                        break;
                                }

                                imgDom.style.height = h + "px";
                                imgDom.style.width = w + "px";

                                imgDom.style["top"] = (layoutHeight - h)/2 + "px";
                                imgDom.style["left"] = (layoutWidth - w)/2 + "px";

                                imgDom.src = this.src;
                                //track exposure
                                if (imgDom.getAttribute('data-imptk') && imgDom.getAttribute('data-imptk') != 'undefined') {
                                    var imptk = JSON.parse(imgDom.getAttribute('data-imptk'));
                                    for (var i = 0; i < imptk.length; i++) {
                                        var img = new Image();
                                        img.src = imptk[i];
                                        img.onerror = (img.onload = (img.onabort = function() {
                                            img = null
                                        }))
                                    }
                                }
                            }
                        })(i,ct,imgDom)
                        if (imgDom) {
                        img.src = imgDom.getAttribute('data-original') || imgDom.getAttribute('src');
                        }
                    }
                },
                
                getUID: function () {
                    function getHash (str) {
                        var hash = 1,
                        charCode = 0,
                        idx;
                        if(str){
                            hash = 0;
                            for(idx = str.length - 1; idx >= 0; idx--){
                                 charCode = str.charCodeAt(idx);
                                 hash = (hash << 6&268435455) + charCode+(charCode << 14);
                                 charCode = hash&266338304;
                                 hash = charCode != 0 ? hash ^ charCode>>21 : hash;
                            }
                        }
                        return hash;
                    }
                    var currentTime = new Date();
                    var todayHour = new Date(currentTime.toDateString()).getTime() + currentTime.getHours() * 60 * 60 * 1000 + parseInt(currentTime.getMinutes() / 5) * 5 * 60 * 1000;
                    return ("" + getHash(document.referrer) + todayHour + getHash(document.cookie.replace(/(?:(?:^|.*;\s*)v\s*\=\s*([^;]*).*$)|^.*$/, "$1"))).substr(0, 32);
                }
            };
            

            function realWidth(obj){
                var clone = obj.clone();
                clone.css("visibility","hidden");
                $('body').append(clone);
                var width = clone.outerWidth();
                clone.remove();
                return width;
            }
            
            function realHeight(obj){
                var clone = obj.clone();
                clone.css("visibility","hidden");
                $('body').append(clone);
                var height = clone.outerHeight();
                clone.remove();
                return height;
            }
            
            function getProductData(){
                for(var i = 0; i < dataRepo.length; i++){
                    (function(obj){
                        obj.url = obj.url.replace("{{showid}}",obj.showid);
                        obj.url = obj.url.replace("{{uid}}",mvUtil.getUID());
                        mvUtil.get_jsonp(obj.url, obj.showid, function(data){
                        if (!data.ads) {
                            dataRepo[0].defaultProducts.sort(function(){ return 0.5 - Math.random() });
                            data.ads = [].concat(dataRepo[0].defaultProducts).slice(0, 6);
                        }
                        else if (data.ads.length < 6) {
                            dataRepo[0].defaultProducts.sort(function(){ return 0.5 - Math.random() });
                            data.ads = data.ads.concat(dataRepo[0].defaultProducts).slice(0, 6);
                        } else {
                            data.ads = data.ads.concat(dataRepo[0].defaultProducts).slice(0, 6);
                        }
                        renderProduct(data);
                        })
                    })(dataRepo[i])
                }
            }
            

            function realWidth(obj){
                var clone = obj.clone();
                clone.css("visibility","hidden");
                $('body').append(clone);
                var width = clone.outerWidth();
                clone.remove();
                return width;
            }
            
            function renderProduct(dataArray){
                //var html = doT.template($('#tmpl-1').html())(dataArray);
                //$('.carousel-container').prepend(html);
                

                function mv_scroll_1(){
                    var mvtjs_1 = 'g7.js';if(mvproduct.makeg7){
                    mvproduct.makeg7(1,'http://www.360.cn',dataArray.ads,298,458,{'logo':'logo.png','entity':'ambition','bgColor':'#000','activeBdColor':'#FFFF00','priceColor':'#00CC00',currencySymbol:'￥',fontFamily:'Microsoft Yahei'},mvtjs_1);
                    clearInterval(scrollStatus_1);}
	                //图片裁剪
	                mvUtil.imgResize($('.pimg'),3);
	                $('.mv_list').attr('data-impurl',dataArray.impurl);
	                if ($('.mv_list').attr('data-impurl')) {
	                    var impArg = [];
	                    $('.item img').each(function() {
	                        if (typeof $(this).attr('data-imparg') !== typeof undefined && $(this).attr('data-imparg') !== false && $(this).attr('data-imparg') !='undefined') {
	                            impArg.push($(this).attr('data-imparg'));
	                        }
	                    });
	                    impArg = impArg.join('.');
	                    var impURL = new Image();
	                    impURL.src = $('.mv_list').attr('data-impurl') + impArg;
	                    impURL.onerror = (impURL.onload = (impURL.onabort = function() {
	                        impURL = null
	                    }))
	                }
                }
                var scrollStatus_1 = setInterval(mv_scroll_1,50);
                
            }
            getProductData();
            
            $('body').on('click', '.item', function(e) {
                if (typeof $(this).attr('data-clktk') !== typeof undefined && $(this).attr('data-clktk') !== false && $(this).attr('data-clktk') !=='undefined') {
                    var clktk = JSON.parse($(this).attr('data-clktk'));
                    for (var i = 0; i < clktk.length; i++) {
                        var img = new Image();
                        img.src = clktk[i];
                        img.onerror = (img.onload = (img.onabort = function() {
                            img = null
                        }))
                    }
                }
            });
            
        }();
    </script>

