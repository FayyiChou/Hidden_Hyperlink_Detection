function loadqrcode()
{
	var url = 'http://www.liuxd.com/qrcode/index.php?jsoncallback=?';
	$.getJSON(
            url,
			function(json){
			$('#Leftfloat').html(json+'分享该内容到手机');
			}
		)
}
loadqrcode();

