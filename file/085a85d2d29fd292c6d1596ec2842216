var speed5s=40//速度数值越大速度越慢
demos50.innerHTML=demos40.innerHTML
function Marquee(){
if(demos50.offsetWidth-demos90.scrollLeft<=0)
demos90.scrollLeft-=demos40.offsetWidth
else{
demos90.scrollLeft++
}
}
var MyMar=setInterval(Marquee,speed5s)
demos90.onmouseover=function() {clearInterval(MyMar)}
demos90.onmouseout=function() {MyMar=setInterval(Marquee,speed5s)}
