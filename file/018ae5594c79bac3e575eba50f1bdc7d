<script>
(function($){
	var added = false;
	
	function addCopyEvent(){
		if(added) return true;
		$('#copy_url_act,#copy_url').zclip({
			path:'/loadJs/ZeroClipboard.swf',
			copy:$('#copy_url').val(),
			afterCopy:function(){
				alert('已复制成功');
				$('#copy_url').select();
			}
		});
		$('#copy_pic_ubb_act,#copy_pic_ubb').zclip({
			path:'/loadJs/ZeroClipboard.swf',
			copy:$('#copy_pic_ubb').val(),
			afterCopy:function(){
				alert('已复制成功');
				$('#copy_pic_ubb').select();
				buildGamePic();
			}
		});
		$('#copy_pic_html_act,#copy_pic_html').zclip({
			path:'/loadJs/ZeroClipboard.swf',
			copy:$('#copy_pic_html').val(),
			afterCopy:function(){
				alert('已复制成功');
				$('#copy_pic_html').select();
				buildGamePic();
			}
		});
		$('#copy_game_ubb_act1,#copy_game_ubb_act2').zclip({
			path:'/loadJs/ZeroClipboard.swf',
			copy:$('#copy_game_ubb').text(),
			afterCopy:function(){
				alert('已复制成功');
				$('#copy_game_ubb_act1').select();
				buildGamePic();
			}
		});
		added = true;
	}
	
	function buildGamePic(){
		$.get('http://tvgdb.duowan.com/pic/buildGamePic?id=16133&alias=gba');
	}
	
	$('.bbs-b a').click(function(){
		$('.mod-tips-s2').show('fast',addCopyEvent);
		return false;
	});
	$('.J_btnClose').click(function(){
		$('.mod-tips-s2').hide('fast');
		return false;
	});
})(jQuery);
</script>
