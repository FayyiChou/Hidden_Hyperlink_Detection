<script>
//var skin = 'skin_white'; 
$(function($){
	changeGame();
	$('.download a').click(function(){
		var downLink = $(this).attr('href');
		if(downLink != 'undefined'){
			$.get('http://tvgdb.duowan.com/gameVersion/down?id=21099&alias=gba');
		}
	});
	$( '#article-list .head a' ).live( 'click' , 
			function (){ 
				$.ajax({ url:$( this ).attr( 'href' ), 
				success: function (html){ 
					$( '#article-list' ).html(html); } 
			}); 
			return false ; 
	}); 
	$( '#article-list .ft a' ).live( 'click' , 
			function (){ 
				$.ajax({ url:$( this ).attr( 'href' ), 
				success: function (html){ 
					$( '#article-list' ).html(html); } 
			}); 
			return false ; 
	}); 
	$( '#change' ).live( 'click' , 
			function (){ 
				changeGame();
				return false ; 
	}); 
	
	var hasArt = false;
	if(hasArt == true){
		$.ajax({ url:'http://tvgdb.duowan.com/article/getArt?gid=16133&alias=gba',
				success:function(articles){
					$('#article-list').html(articles);
				}
			});
	};
});
function changeGame(){
	$.get('http://tvgdb.duowan.com/gameBase/getSimilar?gid=16133&gtype=14&type=3&alias=gba'+'&math='+ Math.random(),
	function(data){
		$('#similar').html(data);
		});
}
function viewArt(id){
	$.get('http://tvgdb.duowan.com/article/artView?alias=gba',{"id":id});
}
$('.click_wb').click(function(){
	$.get('http://dwtvg.sinaapp.com/dwtvg/?r=stat/stat&site=1');
});
$('.click_kk').click(function(){
	$.get('http://dwtvg.sinaapp.com/dwtvg/?r=stat/stat&site=2');
});
</script>

