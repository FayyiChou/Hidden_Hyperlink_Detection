<script type="text/javascript">
//menu add the number
$(function(){
$(".sl_menu > li").each(function(i,val){
    $(this).addClass("sl_menu_"+ (i+1));
    if ($(this).children("ul").length > 0){
        $(this).children("ul").children("li").each(function(i2,val2){
            $(this).addClass("sl_menu_"+ (i+1)+"_"+(i2+1));
        });
    }else{
        $(this).addClass("noChild");
    }
});
});
//show the menu
$(document).ready(function(){
  $('.sl_menu li').mousemove(function(){
	  if ($("#menuIcon").is(":hidden")) {
  	$(this).find('ul').slideDown(2);
	  }
	  });
  $('.sl_menu li').mouseleave(function(){
	  if ($("#menuIcon").is(":hidden")) {
  	$(this).find('ul').slideUp("fast");
	  }
	  });
});
//menu 切换成小菜单展开动画
$(document).ready(function(){
	$("#menuIcon,.sl_menu > li").click(function(){
		if ($("#menuIcon").is(":visible")) { //防止宽屏上点击
			$(".sl_menu").toggle(300);
			$(".sl_menu > ul").toggle(300);
			};
	});
	$(window).resize(function(){
		if (!$(".sl_menu").is(":visible")){
			$('.sl_menu').attr('style', function(i, style){
			return style.replace(/display[^;]+;?/g, '');
			});
		};
	});
})
//翻页增加class
 $(function(){ var obj = $('.Page_Number'); obj.each(function(i){ $(this).addClass("Page_Number_"+(i+1)); }) });
</script> 
<script>
//侧边二级分类点击展开三级分类动画
 $(function() {
var acmenli = $(".acmenu > li"), acmenliul = $(".acmenu > li > ul"), acmenlili = $(".acmenu > li > ul li"), hoverTimer;
var acmena = $(".acmenu a");
acmena.click(function(){
if($(this).next("ul").children("li").length>0)
{
if($(this).children("ul:hidden").length>0)
$(this).next("ul").slideUp(300);
else
$(this).next("ul").slideToggle(300);
return false;
}
});
})
//Job list
$(function(){
$(".job_list li").click(function(){
	$(".job_list li").removeClass("on");
	$(this).addClass("on");
  });
});
</script> 
