<script type="text/javascript">
	var link_begin='https://mima.163.com/16163app?action=forumpost&fid=';
	var link_center='&tid=';
	var link_end='&gameid=2190';
	var url = window.location.href;//获取url
      
    var tidindex = url.indexOf('tid=');//获取tid位置
	
	//识别是否为动态链接
	if(tidindex!=-1){
		var tidNumIndex= tidindex +4;//获取tid数字位置
		var tidnew = url.substring(tidNumIndex)//以tid数字为开头的url
		var value_url = tidnew.split(/[^0-9]/); //以非数字分割线
		var tid =value_url[0];//获取tid
	}else{
		var value_url = url.split(/[\-]/); //静态，以‘-’为分界线
		var tid =value_url[1];//获取tid
	}

      var fid = document.getElementsByName("srhfid")[0].value; // 获取fid

      var text_link=link_begin+fid+link_center+tid+link_end; //打开app对应链接
	  
	var qrcode = new QRCode('qrcode', {     
		text: text_link,
		width: 150,
		height: 150,
		correctLevel : QRCode.CorrectLevel.H
	});
