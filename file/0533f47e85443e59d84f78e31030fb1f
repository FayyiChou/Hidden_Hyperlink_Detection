function bannerImgCenter(){
	var winW = document.body.clientWidth;
	$('.banner .autoimg').each(function(){
		var mleft = (1920 - winW)/2;
		$(this).css('margin-left',-mleft);
	});
	
}
bannerImgCenter();
$(window).resize(function() {
	bannerImgCenter();
});
$('.banner .olduser').css('right',$('.info').css('margin-right'));
