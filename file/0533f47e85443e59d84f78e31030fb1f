  	$('.banner .olduser').css('right',$('.info').css('margin-right'));
});
$(".part7 .play").on('click',function(){
	$(this).hide();
	$(".part7 .close").show();
	$("[name='media']")[0].play();
})
$(".part7 .close").on('click',function(){
	$(this).hide();
	$(".part7 .play").show();
	$("[name='media']")[0].pause();
	$("[name='media']")[0].currentTime=0;
})


