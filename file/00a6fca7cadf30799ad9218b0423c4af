<script type="text/javascript">
    AM(function(){
        $('.readLevel-2').hover(function () {
            var position = $(this).position();
            $(this).nextAll('.level-win-2').show().css("left", position.left).css("top", position.top + $(this).outerHeight());
        }, function () {
            return;
        });
        $('.level-win-2').hover(function () {
        }, function () {
            $(this).hide();
        });
        $('.lev-win-2').click(function () {
            $('.level-win-2').hide();
        });
        $('.readLevel').hover(function () {
            var position = $(this).position();
            $(this).nextAll('.level-win').show().css("left", position.left).css("top", position.top + $(this).outerHeight());
        }, function () {
            return;
        });
        $('.level-win').hover(function () {
        }, function () {
            $(this).hide();
        });
        $('.lev-win').click(function () {
            $('.level-win').hide();
        });

        //广播台交互效果
        var picObj=$(".pic-list-title").find("span");
        picObj.hover(function(){
            picObj.removeClass('hover');
            $(this).addClass("hover");
            var picUl=$(".pic-list-con").find("ul"),picIndex=picObj.index($(this)[0]);
            picUl.hide();
            picUl.eq(picIndex).show();
        })
    })

    //分享加威望的方法
    function postData(){
        var dataUrl = "//suzhou.19lou.com/forum-2014-thread-192151430225624702-1-1.html";
        var post_url = "//suzhou.19lou.com/topic/share/";
        var idNum = "f037ee3a09eb9c2524b02b90b2507495";
        var awordId = 0;
        jQuery.ajax({
            type: "POST",
            url: post_url,
            data: {jsonUrl:dataUrl,IDNum:idNum,aword:awordId},
            dataType: "json",
            success:function(data){
                //不做提示
            }
        });
    }
</script>

