<script>
	$('.weixin').click(function(){
		$('.erweimaBox').fadeIn('fast');
	})
	$('.erweimaBox').click(function(){
		$(this).fadeOut('fast');
	})
	
	$('.show_hide_btn').click(function(){
		var  status= $(this).attr('status');
		if(status==0){
			$('.show_hide_btn').css('background','url("/images/show_pic.png") no-repeat 17px 11px #eee');
			$('.position_sidebar').animate({
				'left':'0px'
			})
			$(this).attr('status',1);
		}else{
			$('.show_hide_btn').css('background','url("/images/hide_pic.png") no-repeat 17px 11px #eee');
			$('.position_sidebar').animate({
				'left':'-45px'
			})
			$(this).attr('status',0);
		}
	})

</script>

