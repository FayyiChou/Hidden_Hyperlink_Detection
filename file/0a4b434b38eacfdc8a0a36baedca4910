    var typename = "";
    var domain = "m.android.tgbus.com";
    var KeyWord = "keyword|2066||sort|0||sortmode|1||type|0||pageno|2";
    var GAMEID = "";
    var pageno = 1;
    var arcid = "556405";
    var pagenum = "1";

    /**
    * [本页地址有source=bd，则添加新样式，及页面所有链接加上后缀source=bd]
    */
    function addCharacter() {

        var nowUrl = window.location.search;

        if (nowUrl.indexOf("source=bd") == -1) {
            return;
        }

        var aArray = $("a");
        $.each(aArray, function (index, val) {
            var href = $(val).attr("href"),
				regexp = /^http/;

            if ((typeof href == "string") && (regexp.test(href)) && href.indexOf("source=bd") == -1) {
                var index = href.indexOf("?");

                if (index > -1) { //有"?"
                    href = href.concat("&source=bd");
                } else {
                    href = href.concat("?source=bd");
                }

                $(val).attr('href', href);
            }

        });


    }

    function showbody(obj) {
        var txt = $(obj).text();
        if (txt == "查看全文") {
            $.get("http://m.tgbus.com/api/action.ashx", { type: "body", domain: domain, typename: typename, arcid: arcid }, function (data) {
                if (data.ret > 0) {
                    $("#bodytxt").html(data.data);
                }

            }, "jsonp");
            $("[data=page]").hide();
            $(obj).text("关闭全文");
        } else {
            $.get("http://m.tgbus.com/api/action.ashx", { type: "body", domain: domain, typename: typename, arcid: arcid, pagenum: pagenum }, function (data) {
                if (data.ret > 0) {
                    $("#bodytxt").html(data.data);
                }

            }, "jsonp");
            $(obj).text("查看全文");
            $("[data=page]").show();
        }
    }
    function showpagelist() {
        $.get("http://m.tgbus.com/api/action.ashx", { type: "xg", domain: domain, typename: typename, KeyWord: KeyWord, pageno: pageno }, function (data) {
            if (data.ret > 0) {
                var htmltxt = '';
                for (var i = 0; i < data.data.length; i++) {
                    var _aHref = data.data[i].MUrl;

                    console.log();

                    htmltxt += "<dl class=\"cl\">";
                    htmltxt += "<dt class=\"fl\">";
                    htmltxt += "<a href=\"" + data.data[i].MUrl + "\">";
                    htmltxt += "<img alt=\"\" src=\"" + data.data[i].Icon + "\">";
                    htmltxt += "</a>";
                    htmltxt += "</dt>";
                    htmltxt += "<dd>";
                    htmltxt += "<h3><a href=\"" + data.data[i].MUrl + "\">" + data.data[i].Title + "</a></h3>";
                    htmltxt += "<p class=\"cl\">";

                    htmltxt += "<em>" + data.data[i].StrDataLine + "<span>电玩巴士</span></em>";
                    htmltxt += "</p>";
                    htmltxt += "</dd>";
                    htmltxt += "</dl>";
                }
                $("#listpage").before(htmltxt);
                pageno = data.ret;
            }

        }, "jsonp")
    }
    showpagelist();
    if ("undefined" != typeof GAMEID) {
        var gameid = parseInt(GAMEID);
        if (!isNaN(gameid) && gameid > 0) {

            $.get('http://gf.tgbus.com/Api/GameData.ashx', { gameid: gameid }, function (data) {
                if ("undefined" != typeof data) {
                    var html = "<div class=\"list8\">";
                    html += "<dl class=\"cl\">";
                    html += "<dt class=\"fl\">";
                    html += "<a href=\"" + data.GamePageUrl + "\">";
                    html += "<img alt=\"\" src=\"" + data.FirstVersion.Box.ImageUrl + "\">";
                    html += "</a>";
                    html += "</dt>";
                    html += "<dd>";
                    html += "<h3><a href=\"" + data.GamePageUrl + "\">" + data.CNTitle + "</a></h3>";
                    html += "<h4>" + data.ENTitle + "</h4>";
                    html += "<p>出品公司：" + data.Company.FullName + "</p>";
                    html += "<p>类型：" + data.Genre.CNTitle + "</p>";
                    html += "<p>当前版本：" + data.Platform.Title + "</p>";
                    html += "<a href=\"" + data.GamePageUrl + "\" class=\"lk_info\">资料库</a>";
                    html += "</dd></dl></div>";

                    $('#tg_ad').before(html);
                }
            }, "jsonp")

        }
    }

</script>
<script type="text/javascript">
    $(function () {
        //var bd_width = $(window).width();
        var bd_width = parseInt(document.body.clientWidth);
        if (isNaN(bd_width) || bd_width <= 0) {
            bd_width = parseInt(document.documentElement.clientWidth);
        }

        $(".article iframe,.article embed").each(function (i, n) {
            var i_width = getAttValue($(this), "width");
            var i_height = getAttValue($(this), "height");
            if (i_width > 0 && i_height > 0) {
                if (!isNaN(bd_width) && bd_width > 20) {
                    var all_width = bd_width - 20;
                    var all_height = parseInt((i_height / i_width) * all_width);
                    $(this).css({ "width": all_width + "px" });
                    $(this).css({ "height": all_height + "px" });
                }
            }
            $(this).show().css({ "display": "block" });
        });

        if ($("#youku_TGBus").length > 0) {
            var total = 0;
            var timer = window.setInterval(function () {
                total++;
                if ($("#youku_TGBus").width() != 0) {
                    $("#youku_TGBus").css({ "width": (bd_width - 20) + "px", "height": (bd_width - 20) * 0.58 + "px" });
                    window.clearInterval(timer);
                }
                else if (total == 100) {
                    window.clearInterval(timer);
                }
            }, 70);
        }

        function getAttValue(objdom, attname) {
            var itemvalue = objdom.attr(attname);
            if (itemvalue == "") {
                itemvalue = objdom.css(attname);
            }
            if (itemvalue != "" && itemvalue != "100%") {
                itemvalue = parseInt(itemvalue);
                if (!isNaN(itemvalue)) {
                    return itemvalue;
                }
            }

            return 0;
        }


    });
</script>
