	// <![CDATA[
	FrontNews_list01['FrontNews_list01-1456903503140_init'] = function(){
		var showDegree=0;
		var showQuestionTag=0;
		var showMessageCount=0;
		var otherShowDegree=0;
		var otherShowMessageCount=0;
		var otherShowQuestionTag=0;
			if(showDegree == '1' || showQuestionTag == '1' || showMessageCount == '1' ){
					FrontNews_list01.getAjaxValueByNewsId("478","false",showDegree,showMessageCount,showQuestionTag,"FrontNews_list01-1456903503140_VisitedCount_1","FrontNews_list01-1456903503140_CommentCount_1","FrontNews_list01-1456903503140_showQuestionTag_1");
					FrontNews_list01.getAjaxValueByNewsId("477","false",showDegree,showMessageCount,showQuestionTag,"FrontNews_list01-1456903503140_VisitedCount_2","FrontNews_list01-1456903503140_CommentCount_2","FrontNews_list01-1456903503140_showQuestionTag_2");
