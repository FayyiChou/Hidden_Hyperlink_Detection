    <script type="text/javascript" language="javascript">
        function Search() {
            var search = $(".select_showbox").html();
            var searchText = $("#put_search").val();
            // alert(searchText);
            if (searchText != "") {
                if (search == "新闻动态") location.href = "News.aspx?search=" + encodeURI(searchText);
                if (search == "图书产品") location.href = "Book.aspx?search=" + encodeURI(searchText);
                if (search == "教材") location.href = "TeachingList.aspx?search=" + encodeURI(searchText);
                if (search == "教辅") location.href = "TeachingAidsList.aspx?search=" + encodeURI(searchText);
            } else {
                alert("请输入搜索关键词！");
            }
        }

        function UpdateTeaching() {
            $("#Teaching").find("dd").find("a").each(function () {
                var urlId = $(this).attr("data-id");
                var urlCid = $(this).attr("data-cid");
                /*if (urlId == 15 || urlId == 17) $(this).attr("href", "Teaching.aspx?Cid=" + urlCid + "&Id=" + urlId);
                else*/ $(this).attr("href", "TeachingList.aspx?Cid=" + urlCid + "&Sid=" + urlId);
            });
        }

        UpdateTeaching();

        function UpdateTeachingAids() {
            $("#TeachingAids").find("dd").find("a").each(function () {
                var urlId = $(this).attr("data-id");
                var urlCid = $(this).attr("data-cid");
                /*if (urlId == 15 || urlId == 17) $(this).attr("href", "TeachingAids.aspx?Cid=" + urlCid + "&Id=" + urlId);
                else*/ $(this).attr("href", "TeachingAidsList.aspx?Cid=" + urlCid + "&Sid=" + urlId);
            });
        }

        UpdateTeachingAids();
    </script>

