    <script type="text/javascript">
        $(function () {
            var pval = unescape(window.sessionStorage.getItem('plugDomValue'));
            var pid = window.sessionStorage.getItem('plugDomID');
            cookie.defaults.domain = '.zhaopin.com';
            if (pval === "null" || pval === "" || pval === null) {
                $("#nowspan").text("无");
            } else {

                $("#nowspan").text(pval);
            }

            $('body').on('click', ".j_selectControl dt", function () {
                if ($(this).find("span").hasClass("icon_arrowB")) {
                    var h = $(this).siblings().length * 35 + 41;
                    $(this).parent().animate({
                        height: h + "px"
                    }, 500, 'ease-out', function () {
                        $(this).find("span").removeClass("icon_arrowB").addClass("icon_arrowT");
                    });
                } else {
                    $(this).parent().animate({
                        height: "41px"
                    }, 500, 'ease-out', function () {
                        $(this).find("span").removeClass("icon_arrowT").addClass("icon_arrowB");
                    });
                }
            });

            function appendValue(doom) {
                var parentId;
                var parentValue;
                if (doom.parents().hasClass("j_hotCityList")) {
                    parentId = doom.data("parentid") || "";
                    parentValue = doom.data("parentvalue") || "";
                } else {
                    if (doom.hasClass("j_unParent")) {
                        parentId = "";
                        parentValue = "";
                    } else {
                        parentId = doom.siblings("dt").attr("id");
                        parentValue = escape(doom.siblings("dt").text());
                    }
                };
                var v = escape(doom.text()),
                    i = doom.attr("id") || "";
                var ppid = cookie.get("plugDomID");

                switch (ppid) {
                    case "selectMajor":
                        cookie.set({ "selectMajor": v + "_" + i + "_" + parentId + "_" + escape(parentValue) });
                        break;
                    case "localCity":
                        cookie.set({ "localCity": v + "_" + i + "_" + parentId + "_" + escape(parentValue) });
                        break;
                    case "nowCity":
                        cookie.set({ "nowCity": v + "_" + i + "_" + parentId + "_" + escape(parentValue) });
                        break;
                    default:
                }

                window.location.href = "/msite/MicroResume?from=sel";

                //window.history.back();
            };

            $(".j_selectControl dd").live("click", function () {
                appendValue($(this));
            });


        });


    </script>

</body></html>