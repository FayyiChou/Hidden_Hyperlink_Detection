<script>
//var fistad=$("index-imgs-box src").first().attr("src");


$(".footer_right").hover(function(){
		$(".weibo").slideDown();
	},function(){
		$(".weibo").slideUp();
})

$(document).ready(function(){
	
	var w = $(window).width();
	var h = $(window).height();
	var c = h-70-66
	$(".banner").height(h);
	$(".bannerBox").height(h);
	$(".newsBarBox").height(c);
	$(".newsBarC").height(c);
	$(".newsBar2").height(c);
	$(".newsBar").css({"top":(c/2-31)+"px"});
	var rnewsh = $(".newsBarC ul").height();
		if(c > rnewsh )
		{
			//$(".newsBarC ul").css({"marginTop":((c-rnewsh)/2)+"px"});
			$(".newsBarC ul").css({"marginTop":(10)+"px"});
		}
	});
	
	$(".newsBar2").click(function(){
		if($(".newsBarC").width() > 200)
		{	
			 $(".newsBarC").animate({width:'0px'},100);
			 $(".newsBar2").removeClass("newsBar2x");
			
		}
		else
		{
		 $(".newsBarC").animate({width:'220px'},100);
		  $(".newsBar2").addClass("newsBar2x");
		}
	})

</script>


