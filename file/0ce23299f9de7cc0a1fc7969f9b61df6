function rzfun1()
{
var v=$("#wd1").val();
if(v.length>0)
{
location.href="sjsrz/rz/"+v;
}else{
location.href="sjsrz/default.htm";
}
}
function rzfun2()
{
var v=$("#wd2").val();
if(v.length>0)
{
location.href="dmrz/rz/"+v;
}else{
location.href="dmrz/default.htm";
}
}
</script>
