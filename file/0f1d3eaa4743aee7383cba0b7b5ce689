    <script>
        $(function(){

            $("#curr_month_60").mouseover(function(){
                $(this).addClass('cur');
                $("#last_month_60").removeClass('cur');
                $(".turn_table_60").eq(0).show();
                $(".turn_table_60").eq(1).hide();
            });
            $("#last_month_60").mouseover(function(){
                $(this).addClass('cur');
                $("#curr_month_60").removeClass('cur');
                $(".turn_table_60").eq(1).show();
                $(".turn_table_60").eq(0).hide();
            });
        });
    </script>
    
    <script>
        $(function(){

            $(".reward_btn").on('click', function(){
                var month = $(this).attr('month');
                var rank_type = $(this).attr('rank_type');
                var rank = $(this).attr('rank');
                var data = {month: month, rank_type: rank_type, rank: rank};

                $.ajax({
                    url: '/discuz/reward_rank',
                    type: 'POST',
                    data: data,
                    dataType: 'json',
                    success: function(data){
                        if(data.status == 0)
                        {
                            alert('领取成功！');
                            location.reload();
                        }else{
                            alert(data.msg);
                        }
                    }
                });
            });

            $(document).on('click', '.pagination li a', function(){

                var url = $(this).attr('href');
                var dom = $(this).parent().parent().parent().parent();
                
                if($(this).parent().hasClass('active')){
                    return false;
                }
                $.ajax({
                    url: url,
                    type: 'get',
                    dataType: 'html',
                    success: function(html){
                        dom.empty();
                        dom.append(html);
                    }
                });

                return false;
            }); 

        })
    </script>


