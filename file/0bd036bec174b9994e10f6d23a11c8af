        //图片上传
        //用户图像
        jQuery("#himages").click(function() {
            if (CheckPicNum()) {
                jQuery("#intRoom").val(jQuery("#SelRoom").val());
                jQuery("#intHall").val(jQuery("#SelHall").val());
                jQuery("#intToilet").val(jQuery("#SelToilet").val());
                if (jQuery("#txtEstate").val() == "" || jQuery("#txtEstate").val() == "输入楼盘，小区") {
                    alert("输入楼盘，小区！");
                }
                else {
                    jQuery("#pHouse").html(""); //清空户型图
                    jQuery("#spanMsg").html(""); //清空户型图结果
                    jQuery("#himage").css("display", "");
                    jQuery("#selectQ").html(jQuery("#txtEstate").val());
                    jQuery("#hxs").click();
                }
            }
        })
        jQuery("#himageh,#chimage").click(function() {
            jQuery("#himage").css("display", "none");
            jQuery("#imagesnum").html("");
