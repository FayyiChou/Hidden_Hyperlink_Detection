    <script type="text/javascript">
    
            
 
 function Pub(o)
 {      
        if(o==6)
        {
            if(jQuery("#LogUserName").val().length==0)
            {
                 jQuery("#LogUserName_error").css("display","");
                jQuery("#LogUserNametip").css("display","none");
                 jQuery("#LogUserName_right").css("display","none");
            }
            else
            {
              jQuery("#LogUserName_error").css("display","none");
                jQuery("#LogUserNametip").css("display","none");
                 jQuery("#LogUserName_right").css("display","");
            }
            if(jQuery("#LogUserPwd").val()=="")
            {
                jQuery("#LogUserPwd_error").css("display","");
                jQuery("#LogUserPwd_tip").css("display","none");
                 jQuery("#LogUserPwd_right").css("display","none");
            }
            else
            {
             jQuery("#LogUserPwd_error").css("display","none");
                jQuery("#LogUserPwd_tip").css("display","none");
                 jQuery("#LogUserPwd_right").css("display","");
            }
        }
    if(CheckCaseSubmit())
    {
     if(CheckPic()==false)
           {
                 alert("请上传户型图！");
                 return false;
           } 
           
         if(jQuery("#mread").attr("checked")==false)
                { 
                    msg="请阅读发布协议";
                    alert(msg);
                     jQuery("#mread").focus();
                    return false;
                }

            jQuery("#optType").val(o);
          
        jQuery("#task_fabu").submit();
        return true;
    }
    else
    {
        return false;
    }
 }
 
function CheckPic()
{
    var o=false;
    jQuery("#ImageRegion_1").children("div").each(function(){
            
        o= true;
    });
    return o;

}

function CheckPicNum()
{
    var o=true;
    jQuery("#ImageRegion_1").children("div").each(function(){
        alert("只能上传一张户型图！");        
        o= false;
    });
    return o;

}
