    <script type="text/javascript">
        var twyz = false;//是否需要图文验证，true需要 false不需要
        function changeMobile() {
            jQuery("#trYzm").show();
            jQuery("#txtMobile").css("display", "");
            jQuery("#lbread").css("display", "none");
            jQuery("#aC").css("display", "none");
            

        }

        function MobileUpdate() {
            var oldmobile = "";
            var newmobile = jQuery("#txtMobile").val();
            if (oldmobile != newmobile) {
                jQuery("#isendMobile").attr("disabled", "")
                jQuery("#isendMobile").css("cursor", "pointer");
            }
            else {
                jQuery("#isendMobile").attr("disabled", "disabled")
                jQuery("#isendMobile").css("cursor", "");
            }
        }
        var checknameandphone = false;
        var checkFirst = new Array(5);
        var resultArray = [0, 0, 0, 0, 0];
        var checkFunction = ["CheckMobile('txtMobile')", "CheckCall('txtCall')", "CheckYzm('txtValid')", "CheckCity('slcity')", "CheckEstate('txtEstate')"];
        function ShowTip(KeyName, flag) {

            var tipName = KeyName + "_tip";
            if (flag) {
                jQuery("#" + tipName).show();
                ShowError(KeyName, false);
                ShowRight(KeyName, false);
            } else {
                jQuery("#" + tipName).hide();
            }
        }
        function LenObject(key) {
            var l = escape(key), len;
            len = l.length - (l.length - l.replace(/\%u/g, "u").length) * 4;
            l = l.replace(/\%u/g, "uu");
            len = len - (l.length - l.replace(/\%/g, "").length) * 2;
            return len;
        }
        function ChangeDivClass(KeyName, classN) {
            var divName = KeyName + "Div";
            if (KeyName == "slcDistrict" || KeyName == "slcComareas" || KeyName == "txtAddress")
                divName = "slcCityDiv";
            jQuery("#" + divName).removeClass();
            jQuery("#" + divName).addClass(classN);
        }
        function ShowError(KeyName, flag, msg) {
            var ErrorName = KeyName + "_error";
            if (flag) {
                jQuery("#" + ErrorName).show();
                document.getElementById(ErrorName).innerHTML = "<img src='" + imgurl + "images/register/reg_07.gif'/>&nbsp;" + msg;
                ShowTip(KeyName, false);
                ShowRight(KeyName, false);
                ChangeDivClass(KeyName, 'onwred');
            } else {
                jQuery("#" + ErrorName).hide();
                ChangeDivClass(KeyName, '');
            }
        }
        function ShowRight(KeyName, flag, msg) {

            var RightName = KeyName + "_right";
            if ((KeyName == "txtUserName") && flag)
                checknameandphone = true;
            else if ((KeyName == "txtMobile" || KeyName == "txtUserName") && !flag)
                checknameandphone = false;
            if (flag) {
                jQuery("#" + RightName).show();
                if (msg != null) {
                    jQuery("#" + RightName).html("<img src='" + imgurl + "images/register/reg_08.gif'/>&nbsp;" + '<label class="ok">' + msg + '</label>');
                } else {
                    jQuery("#" + RightName).html("<img src='" + imgurl + "images/register/reg_08.gif'/>&nbsp;" + '<label class="ok">' + '已输入' + '</label>');
                }
                ShowTip(KeyName, false);
                ShowError(KeyName, false);
            } else {
                jQuery("#" + RightName).hide();
            }
        }
        //检查是否正确的数字
        function checkNum(strpage) {
            strpage = strpage.replace(/(^\s*)|(\s*$)/g, ""); //去除前后空格
            re = new RegExp("^[1-9]*[1-9][0-9]*$"); //是否正整数							
            return re.test(strpage);
        }
        function checkFloat(strpage) {
            if (checkNum(strpage)) {
                return true;
            }
            if (strpage == "0.00") {
                return false;
            }
            strpage = strpage.replace(/(^\s*)|(\s*$)/g, ""); //去除前后空格
            re = new RegExp(/^[0-9]*\.[0-9]{1,2}$/); //是否正整数			

            return re.test(strpage);
        }
        function ShowDiv(i) {
            for (var j = 1; j <= 4; j++) {
                jQuery("#" + 'Div' + j).hide();
            }
            jQuery("#" + 'Div10').hide();
            jQuery("#" + 'Div' + i).show();
        }

        function CheckProvince(KeyName) {
            if (jQuery("#" + KeyName).val() == 0 || jQuery("#" + KeyName).val() == -1) {
                ShowError(KeyName, true, "请选择省份");
                return false;
            }
        }

        function CheckUserName(KeyName) {
            resultArray[5] = 1;
            jQuery("#" + KeyName).val(jQuery("#" + KeyName).val().toLowerCase());
            var value = jQuery("#" + KeyName).val();
            if (value == "") {
                ShowError(KeyName, true, "请输入姓名");
                checkFirst[3] = KeyName;
                return false;
            }

            ShowRight(KeyName, true);
            resultArray[5] = 2;
            checkFirst[5] = "";
            return true;
        }


        function CheckYzm(KeyName, Mobile) {

            return true;
        }

        function CheckEmail(KeyName) {
            resultArray[8] = 1;
            jQuery("#" + KeyName).val(jQuery("#" + KeyName).val().toLowerCase());
            var value = jQuery("#" + KeyName).val();
            if (value == "") {
                ShowError(KeyName, true, "请输入邮箱");
                checkFirst[8] = KeyName;
                return false;
            }
            if (value.length > 50) {
                ShowError(KeyName, true, "输入太长");
                checkFirst[8] = KeyName;
                return false;
            }
            var regEmail = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/.test(value);
            if (!regEmail) {
                ShowError(KeyName, true, "邮箱格式不正确");
                checkFirst[8] = KeyName;
                return false;
            }

            ShowRight(KeyName, true);
            resultArray[8] = 2;
            checkFirst[8] = "";
            return true;
        }


        function CheckMobile(KeyName) {

            
            resultArray[0] = 1;
            var mobile = jQuery("#" + KeyName).val();
            if (mobile.length == 0) {
                ShowError(KeyName, true, "手机号码不能为空");
                checkFirst[0] = KeyName;
                return false;
            }
           
            if (mobile.length != 11) {
                ShowError(KeyName, true, "手机号码必须为11位数字");
                checkFirst[0] = KeyName;
                return false;
            }
//            if (!myreg.test(mobile)) {
//                ShowError(KeyName, true, "手机号码格式不正确");
//                checkFirst[0] = KeyName;
//                return false;
//            }

            jQuery("#lbts").html("如果1分钟内没有收到验证码短信，您可以通过“语音”方式获取验证码，此服务免费。");
            ShowRight(KeyName, true);
            resultArray[0] = 2;
            checkFirst[0] = "";
            return true;
        }

        function CheckCall(KeyName) {

            resultArray[1] = 1;
            var value = jQuery("#" + KeyName).val();
            if (value == "") {
                ShowError(KeyName, true, "请填写您的称呼");
                checkFirst[1] = "txtCall";
                return false;
            }

            ShowRight(KeyName, true);
            resultArray[1] = 2;
            checkFirst[1] = "";
            return true;


        }
        function CheckYzm(KeyName) {
            resultArray[2] = 1;
            var value = jQuery("#" + KeyName).val();

            if (value == "" && jQuery("#txtMobile").css("display") != "none") {
                ShowError(KeyName, true, "请输入验证码");
                checkFirst[2] = "txtValid";
                return false;
            }

            ShowRight(KeyName, true);
            resultArray[2] = 2;
            checkFirst[2] = "";
            return true;
        }
        function CheckCity(KeyName) {
            resultArray[3] = 1;
            var value = jQuery("#" + KeyName).val();
            if (value == 0 || value == -1) {
                ShowError(KeyName, true, "请选择城市");
                checkFirst[3] = "slcity";
                return false;
            }
            else {
                var city = jQuery("#" + KeyName).val();
                jQuery("#citeCity").val(city);
                ShowRight(KeyName, true);
                resultArray[3] = 2;
                checkFirst[3] = "";
                return true;
            }
        }
        function CheckEstate(KeyName) {
            resultArray[4] = 1;
            jQuery("#" + KeyName).val(jQuery("#" + KeyName).val().toLowerCase());
            var value = jQuery("#" + KeyName).val();

            if (LenObject(value) < 1 || value == "输入楼盘，小区") {
                ShowError(KeyName, true, "请输入楼盘，小区");
                checkFirst[4] = KeyName;
                return false;
            }

            ShowRight(KeyName, true, "为保证您能获得优质的装修服务，小区名称一定要填写正确哦");
            resultArray[4] = 2;
            checkFirst[4] = "";
            return true;
        }
        function CheckArea(KeyName) {
            resultArray[1] = 1;
            jQuery("#" + KeyName).val(jQuery("#" + KeyName).val().toLowerCase());
            var value = jQuery("#" + KeyName).val();
            if (value == "") {
                ShowError(KeyName, true, "请输入建筑面积");
                checkFirst[1] = "txtArea";
                return false;
            }
            else {
                if (!checkNum(value)) {
                    ShowError(KeyName, true, "请输入整数");
                    checkFirst[1] = "txtArea";
                    return false;
                }
            }
            ShowRight(KeyName, true);
            resultArray[1] = 2;
            checkFirst[1] = "";
            return true;
        }
        function CheckPrice(KeyName) {
            resultArray[2] = 1;
            jQuery("#" + KeyName).val(jQuery("#" + KeyName).val().toLowerCase());
            var value = jQuery("#" + KeyName).val();
            if (value == "") {
                ShowError(KeyName, true, "请输入装修总预算");
                checkFirst[2] = "txtPrice";
                return false;
            }
            else {
                if (!checkNum(value)) {
                    ShowError(KeyName, true, "请输入整数");
                    checkFirst[2] = "txtPrice";
                    return false;
                }
            }
            ShowRight(KeyName, true);
            resultArray[2] = 2;
            checkFirst[2] = "";
            return true;
        }
        function CheckCaseDescription(KeyName) {
            resultArray[3] = 1;
            jQuery("#" + KeyName).val(jQuery("#" + KeyName).val().toLowerCase());
            var value = jQuery("#" + KeyName).val();


            if (LenObject(value) > 300) {
                ShowError(KeyName, true, "具体要求已超过150个汉字");
                checkFirst[3] = KeyName;
                return false;
            }

            ShowRight(KeyName, true);
            resultArray[3] = 2;
            checkFirst[3] = "";
            return true;
        }

        //建材产品分类
        function CheckJCCategory(KeyName) {
            if ($("input[name^='JCPub']")[0].checked) {
                resultArray[2] = 1;
                var ischecked = false;
                $("input[name^='chkCategory']").each(function(i) {
                    if (this.checked) {
                        ischecked = true;
                        return;
                    }
                });
                if (!ischecked) {
                    ShowError(KeyName, true, "请选择类别");
                    resultArray[2] = 0;
                    checkFirst[2] = "";
                    return false;
                }
            }
            ShowRight(KeyName, true);
            resultArray[2] = 2;
            checkFirst[2] = "";
            return true;
        }
        function CheckJCContent(KeyName) {
            if ($("input[name^='JCPub']")[0].checked) {
                resultArray[3] = 1;
                var value = jQuery("#" + KeyName).val();
                if (value == "") {
                    ShowError(KeyName, true, "请填写您要求购商品的材质\品牌\预算等信息");
                    checkFirst[3] = "productContent";
                    return false;
                }
            }
            ShowRight(KeyName, true);
            resultArray[3] = 2;
            checkFirst[3] = "";
            return true;
        }

        var Time = 60; //设置时间　单位：秒
        var h, m, s, t = null;

        function e(o, v, o1, v1) {

            Time -= 1;

            if (Time == 0) { v.style.display = "none"; o.style.display = ""; v1.style.display = "none"; o1.style.display = ""; clearInterval(t); }
            else {
                v.style.display = "";
                v.value = Time + "秒后可重新获取";
                v1.style.display = "";
                v1.value = Time + "秒后可重新获取";
            }
        }
        function starttime() {
            Time = 60;
            var o = document.getElementById("isendMobile");
            o.style.display = "none";
            var v = document.getElementById("sv");
            v.style.display = "";
            v.value = Time + "秒后可重新获取";

            var o1 = document.getElementById("btnyy");
            o1.style.display = "none";
            var v1 = document.getElementById("yysv");
            v1.style.display = "";
            v1.value = Time + "秒后可重新获取";

            t = setInterval(function() { e(o, v,o1,v1) }, 1000);
           
            if (v.style.display != "") {
                o.style.display = "none";
            }
            if (v1.style.display != "") {
                o1.style.display = "none";
            }
            
        }
        

        function starttime1() {
//            Time = 60;
