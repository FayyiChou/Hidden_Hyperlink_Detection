        function MaiMaInfo() {
            var call = $("#txtCall").val();
            var sex = $("#sex").val();
            var Mobile = $("#txtMobile").val();
            var city = $("#slcity").val();
            var Estate = $("#txtEstate").val();
            var selelable = $('#task_fabu input[name="SelRoom"]:checked ').val();
            var Email = $("#txtEmail").val();
            var CaseStyle = $("#radCaseStyle").val();
            var Area = $("#txtArea").val();
            var Price = parseInt($("#txtPrice").val()) * 10000;
            var selelableInfo = "";
            var CaseStyleInfo = "";
            if (selelable == 1) {
                selelableInfo = "一居";
            } else if (selelable == 2) {
                selelableInfo = "二居";
            }
            else if (selelable == 3) {
                selelableInfo = "三居";
            }
            else if (selelable == 4) {
                selelableInfo = "其他";
            }
            if (CaseStyle == 1)
            { CaseStyleInfo = "现代简约"; }
            else if (CaseStyle == 2)
            { CaseStyleInfo = "田园风格"; }
            else if (CaseStyle == 3)
            { CaseStyleInfo = "中式古典"; }
            else if (CaseStyle == 4)
            { CaseStyleInfo = "西式古典"; }
            else if (CaseStyle == 5)
            { CaseStyleInfo = "欧美风情"; }
            else if (CaseStyle == 6)
            { CaseStyleInfo = "东南亚风格"; }
            else if (CaseStyle == 7)
            { CaseStyleInfo = "混合型风格"; }
            else if (CaseStyle == 8)
            { CaseStyleInfo = "日韩风格"; }
            else if (CaseStyle == 9)
            { CaseStyleInfo = "中式风格"; }
            else if (CaseStyle == 10)
            { CaseStyleInfo = "简欧风格"; }
            else if (CaseStyle == 11)
            { CaseStyleInfo = "新古典风格"; }
            else if (CaseStyle == 12)
            { CaseStyleInfo = "混搭风格"; }
            else if (CaseStyle == 13)
            { CaseStyleInfo = "地中海风格"; }
            else if (CaseStyle == 14)
            { CaseStyleInfo = "其它"; }
            var emcode = ",\"vwh.name\":\"" + encodeURIComponent(call) + "\",\"vwh.sex\":\"" + encodeURIComponent(sex) + "\",\"vwh.phone\":\"" + Mobile + "\",\"vwh.housetype\":\"" + encodeURIComponent(selelableInfo) + "\"" + ",\"vwh.community\":\"" + encodeURIComponent(Estate) + "\"" + ",\"vwh.mail\":\"" + encodeURIComponent(Email) + "\",\"vwh.style\":\"" + encodeURIComponent(CaseStyleInfo) + "\",\"vwh.area\":\"" + encodeURIComponent(Area) + "\",\"vwh.budget\":\"" + encodeURIComponent(Price) + "\"";
            var code = "{\"vwg.page\":\"tender\",\"vwh.city\":\"" + encodeURIComponent(city) + "\"" + emcode + "}";
            var obj = $.parseJSON(code);
            _ub.city = '深圳'; //所在城市（中文） 
            _ub.biz = 'h'; //家居‘h’
            _ub.location = 0;
            _ub.collect(71, obj);
        }
    </script>

