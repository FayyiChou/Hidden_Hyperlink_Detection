        }

        function CheckSubmit() {
            var flag = true;
            var checkcount = 5;
            //    if($("input[name^='JCPub']")[0].checked)
            //    {
            //        checkcount=4;
            //    }
            for (var i = 0; i < checkcount; i++) {
                if (resultArray[i] == 0) {
                    eval(checkFunction[i]);
                }
            }
            for (var i = 0; i < checkcount; i++) {
                if (resultArray[i] == 1) {
                    flag = false;
                    if (checkFirst[i] != "" && document.getElementById(checkFirst[i])) {
                        document.getElementById(checkFirst[i]).focus();
                    }
                    break;
                }
            }
            return flag;
        }
        function Submit() {
            //jQuery.watermark.hideAll();
            if (CheckSubmit()) {
                var oldMobile = "";
                var newMobile = jQuery("#txtMobile").val();

                if (oldMobile != newMobile || oldMobile == "") {
                    var code = jQuery("#txtValid").val();
                    Passport.LoginVerifysms(newMobile, code, "", "", "", 0, funregsuc, funregfail);
                }
                else {
                    MaiMaInfo();
                    document.forms["task_fabu"].submit();
                }

            }
            return false;
        }

        function funregsuc() {
            MaiMaInfo();
            document.forms["task_fabu"].submit();
            return false;
        }
        function funregfail(errormsg) {
            alert(errormsg);
            return false;
        }


        //获取验证码
        function GetYzm() {

//            SendPassportCode()

            refresh_code();
            jQuery("#yzmtc").css("display", "inline");
           

        }

        function refresh_code() {
            var v_random = Math.round(Math.random() * 10000);
            jQuery("#loginValiCodeImg").attr("src", 'http://captchas.fang.com/Display?type=soufangbang&width=72&height=26&r==' + v_random);
        }
        function getyyyzm() {
            
            var txtValiCode = jQuery("#txtValiCode").val();            
           
            var mobile = jQuery("#txtMobile").val();


            if (CheckMobile("txtMobile") == true) {
                if (twyz == true) {
                    if (txtValiCode == "") {
                        alert("请输入图文验证答案");
                        return;
                    }
                }
                
                Passport.LoginSendMsm(mobile, "", "", 0, 1, funsucyy, funfail);

              
            }
        }


        function SendPassportCode() {
//            var mobile = jQuery("#txtMobile").val();


//            if (CheckMobile("txtMobile") == true) {
//                Passport.LoginSendMsm(mobile, "", "", 0, 0, funsuc, funfail);


//            }

            
            var mobile = jQuery("#txtMobile").val();
            var txtValiCode = jQuery("#txtValiCode").val();
           
            if (CheckMobile("txtMobile") == true) {
                if (twyz == true) {
                    if (txtValiCode == "") {
                        alert("请输入图文验证答案");
                        return;
                    }
                }
                jQuery("#lbts").html("如果1分钟内没有收到验证码短信，您可以通过“语音”方式获取验证码，此服务免费。");
               
                Passport.LoginSendMsm(mobile, txtValiCode, "", 0, 0, funsuc, funfail);
                

            }




        }
        function funsucyy() {
            starttime();
            starttime1();
            var mobile = jQuery("#txtMobile").val();
            mobile = left(mobile, 3) + "****" + right(mobile, 4);
            refresh_code();
            jQuery("#lbts").html("房天下向您的手机：<span style=\"color:red\">" + mobile + "</span>拨打电话并播报验证码。来电号码<span style=\"color: #d00000\">010-58544536</span>。如果1分钟内没接到语音验证码电话，请点击按钮重新获取。");


         }
         function left(mainStr, lngLen) {
             if (lngLen > 0) { return mainStr.substring(0, lngLen) }
             else { return null }
         }

         function right(mainStr, lngLen) {
             // alert(mainStr.length) 
             if (mainStr.length - lngLen >= 0 && mainStr.length >= 0 && mainStr.length - lngLen <= mainStr.length) {
                 return mainStr.substring(mainStr.length - lngLen, mainStr.length)
             }
             else { return null }
         }
         function mid(mainStr, starnum, endnum) {
             if (mainStr.length >= 0) {
                 return mainStr.substr(starnum, endnum)
             } else { return null }
             //mainStr.length 
         }
        function funsuc() {
            starttime();
            starttime1();
            refresh_code();
            jQuery("#tryy").css("display", "");

            alert("手机验证码已发送");
        }
        function funfail(errormsg, isShowMathCode) {

            if (isShowMathCode == "true") {
                refresh_code();
                jQuery("#yzmtc").show();
                twyz = true;
               
            }
            else {
                alert('发送失败！' + jQuery.trim(errormsg));
            }
            //            refresh_code();
            jQuery("#tryy").css("display", "");
            
        }

    </script>

