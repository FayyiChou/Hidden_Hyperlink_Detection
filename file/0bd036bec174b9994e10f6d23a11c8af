        $(document).ready(function() {
            $("body").bind("click", function(e) {
                if (e.target.id != "txtEstate") {
                    document.getElementById('txtSelectType').value = "1";

                    //suggestionDiv.style.display = 'none';
                    setTimeout("suggestionDiv.style.display = 'none'", 100)
                }
            });
        });



