        })

        jQuery("#intRoom,#intHall,#intToilet,#intKitchen").change(function() {
            var str = "";
            if (jQuery("#intRoom").val() != "") {
                str += jQuery("#intRoom").val() + "室";
            }
            if (jQuery("#intHall").val() != "") {
                str += jQuery("#intHall").val() + "厅";
            }
            if (jQuery("#intToilet").val() != "") {
                str += jQuery("#intToilet").val() + "卫";
            }
            if (jQuery("#intKitchen").val() != "") {
                str += jQuery("#intKitchen").val() + "厨";
            }
            jQuery("#selectQ").html(jQuery("#txtEstate").val() + str);
        })
        //户型图 搜索户型图
        var imageJ;
        var imageList = 0;
        jQuery("#hxs").click(function() {
            jQuery("#s_picdes").html("加载中...");
            jQuery("#pHouse").html("");
            jQuery("#spanMsg").html("");
            imageJ = new Object();
            imageList = 0;
            var room = jQuery("#intRoom").val();
            var hall = jQuery("#intHall").val();
            var toilet = jQuery("#intToilet").val();
            var kitchen = jQuery("#intKitchen").val();
            var area = jQuery("#intBuildingarea_sel").val();
            var newcode = jQuery("#txtEstateID").val();
            // var Province=escape((jQuery("#ddlProvince").val()));//省
            var City = escape("深圳"); //市
            var District = escape(jQuery("#ddDistrict").find("option:selected").text()); //区
            var RealEstate = escape(jQuery("#txtEstate").val()); //楼盘名称
            var urla = "http://newhousemgn.fang.com/newhousenet/datainterface/newhouse_4_second_pic.aspx";
            var mdata = "RealEstate=" + RealEstate + "&District=" + District + "&City=" + City + "&newcode=" + newcode + "&room=" + room + "&HALL=" + hall + "&TOILET=" + toilet + "&KITCHEN=" + kitchen + "&buildingarea_s=" + area + "&buildingarea_m=" + (parseInt(area) + parseInt(50));
            jQuery("#confirmImages,#confirmImages2").attr("src", "http://jj.js.soufunimg.com/home/weiketest/images/loadingAnimation.gif")
            jQuery.ajax({
                type: "POST",
                url: "http://home.sz.fang.com/zhuangxiu/ashx/housemgn.ashx",
                data: mdata,
                dataType: "json",
                cache: false,
                timeout: 9000,
                success: function(msg) {
                    imageJ = msg;
                    changeImage();
                    jQuery("#spanMsg").html("1/共" + msg.length + "张");
                    jQuery("#confirmImages,#confirmImages2").attr("src", "http://jj.js.soufunimg.com/home/weiketest/images/task/button15.gif")
                },
                error: function(e) {
                    jQuery("#s_picdes").html("抱歉，未找到户型图，请上传");
                    jQuery("#confirmImages,#confirmImages2").attr("src", "http://jj.js.soufunimg.com/home/weiketest/images/task/button15.gif");
                }
            });

        })
        //上一张
        jQuery("#imUp").click(function() {
            if (imageJ.length > 0) {
                if (imageList > 0) {
                    imageList--;
                }
                changeImage();
            }
        })
        jQuery("#imDown").click(function() {
            if (imageJ.length > 0) {
                imageList++;
                if (imageList > imageJ.length - 1) {
                    imageList = 0;
                }
                changeImage();
            }
        })
        //图片下一张
        jQuery("#hitNext,#mhouse").click(function() {
            jQuery("#imDown").click();
        })
        //确定搜房网户型图
        jQuery("#confirmImages,#confirmImages2").click(function() {
            jQuery("#himage").css("display", "none");
            jQuery("#imagesnum").html("");
            jQuery("#confirmImages,#confirmImages2").attr("src", "http://jj.js.soufunimg.com/home/weiketest/images/task/button15.gif");
            try {
                if (imageJ.length > 0) {
                    if (jQuery("#pPhotoList span[isrealimg='realimg']").length < 4) {
                        //                    alert("选择成功");
                        var urlsExtend = 'w:0+h:0+s:0';
                        UploadImageFinish(imageJ[imageList].Images, urlsExtend);
                    }
                    else {
                        jQuery("#himages").css("display", "none");
                        jQuery("#picturesUploaderN,#picturesUploaderS").css("display", "none");
                        jQuery("#imgMax").css("display", "inline");
                        jQuery("#imgMax1").css("display", "inline");
                        alert("对不起，您最多是能上传4个附件");
                    }
                }
                else {
                    alert("选择失败");
                }
            }
            catch (err) { alert('选择失败1'); }
            return false;
        })
        function changeImage() {
            if (jQuery("#pHouse").length > 0) {
                var room = imageJ[imageList].Room;
                var hall = imageJ[imageList].Hall;
                var toilet = imageJ[imageList].Toilet;
                var kitchen = imageJ[imageList].Kitchen;
                var area = imageJ[imageList].Area;
                var ImageName = imageJ[imageList].ImageName;
                jQuery("#pHouse").html("");
                jQuery("#aHousePicname").val(ImageName); //户型图片名称
                if (room != 0) {
                    if (area != 0) {
                        jQuery("#s_picdes").html("" + room + "室" + hall + "厅" + toilet + "卫" + kitchen + "厨  面积" + area + "平米"); //显示

                    }
                    else {
                        jQuery("#s_picdes").html("" + room + "室" + hall + "厅" + toilet + "卫" + kitchen + "厨"); //显示
                    }
                }
                else {
                    if (area != 0) {
                        jQuery("#s_picdes").html("面积" + area + "平米"); //显示
                    }
                }
                if (room != 0 && area != 0) {
                    jQuery("#aHousePicDis").val("" + room + "室" + hall + "厅" + toilet + "卫" + kitchen + "厨  面积" + area + "平米"); //提交
                }
                else if (room == 0 && area != 0) {
                    jQuery("#aHousePicDis").val("面积" + area + "平米"); //提交
                }
                else if (room != 0 && area == 0) {
                    jQuery("#aHousePicDis").val("" + room + "室" + hall + "厅" + toilet + "卫" + kitchen + "厨"); //提交
                }
                jQuery("#pHouse").html("<img style=\"cursor:pointer;\" id=\"mhouse\" onclick=\"jQuery('#imDown').click();\"   src=\"" + imageJ[imageList].Images + "\"   onload=\"imgReSize(this,600,600)\" />"); //显示图片
                jQuery("#mhouse").attr("src", imageJ[imageList].Images);
            }
            if (jQuery("#spanMsg").length > 0) {
                jQuery("#spanMsg").html("");
                jQuery("#spanMsg").html("" + (imageList + 1) + "/共" + imageJ.length + "张");
            }
        }


        function HandleUpload(uploader) {
            if (uploader.thumbs.length > 0) {
                //原图 urls
                var txtPicturesUrl_thumb_value = uploader.urls[0]; //原图URL集合

                for (index = 0; index < uploader.thumbs.length; index++) {
                    txtPicturesUrl_thumb_value += ("," + uploader.urls[index]);
                    if (jQuery("#pPhotoList span[isrealimg='realimg']").length < 4) {
                        UploadImageFinish(txtPicturesUrl_thumb_value, 'un', '', 1);
                    }
                    else {
                        jQuery("#himages").css("display", "none");
                        jQuery("#picturesUploaderN,#picturesUploaderS").css("display", "none");
                        jQuery("#imgMax").css("display", "inline");
                        jQuery("#imgMax1").css("display", "inline");
                        alert("对不起，您最多只能上传4个附件");
                        return;
                    }
                }
                return false;
            }
        }

        //图片模板
        jQuery("span a.delPic").click(function() {
            if (confirm("是否删除图片!")) {

                jQuery(this).closest("span").remove();

                if (jQuery("#pPhotoList span[isRealImg='realImg']").length < 4) {
                    jQuery("#himages").css("display", "inline");
                    jQuery("#picturesUploaderN,#picturesUploaderS").css("display", "inline");
                    jQuery("#imgMax").css("display", "none");
                    jQuery("#imgMax1").css("display", "none");
                }
            }
            return false;
        });
    </script>

