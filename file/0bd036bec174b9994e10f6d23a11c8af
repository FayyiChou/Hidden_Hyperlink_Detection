<script type="text/javascript">
    //顶端搜索
//    function searchcaseKey() {
//        //记录搜索日志
//        //搜索结果
//        if (jQuery("#caseKey").val() == "请输入户型/功能间/风格/楼盘/设计师/品牌等关键词") {
//            jQuery("#caseKey").val("");
