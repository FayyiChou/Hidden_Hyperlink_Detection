    var adkey = '请输入户型/功能间/风格/楼盘/设计师/公司等关键词';
    var adlink = '';
    adsearchlink();
    function adsearchlink() {
        var isAdSearch = '0';
        if (parseInt(isAdSearch) == 1) {
            var adSearchKey1 = '请输入户型/功能间/风格/楼盘/设计师/公司等关键词';
            var adSearchKeyLink1 = '';
            var adSearchKey2 = '请输入户型/功能间/风格/楼盘/设计师/公司等关键词';
            var adSearchKeyLink2 = '';
            if (adSearchKeyLink2 != '') {
                var numid = 0;
                var adcookie = getCookie('homeadsearchzx');
                if (adcookie != null) {
                    numid = adcookie;
                }
                if (parseInt(numid) % 2 == 0) {
                    adkey = adSearchKey1;
                    adlink = adSearchKeyLink1;
                    //numid = 1;
                    addCookie('homeadsearchzx', '1', '1');

                } else {
                    adkey = adSearchKey2;
                    adlink = adSearchKeyLink2;
                    //numid = 0;
                    addCookie('homeadsearchzx', '0', '1');

                }
            }
            jQuery("#zxbsearchkey").val(adkey);
        } else {
        jQuery("#zxbsearchkey").val('请输入户型/功能间/风格/楼盘/设计师/公司等关键词');
        }
    }
    function adsearchfocus() {
        if (jQuery("#zxbsearchkey").val() == adkey) {
            jQuery("#zxbsearchkey").val('');
        }
    }
    function adsearchblur() {
        if (jQuery("#zxbsearchkey").val() == '') {
            jQuery("#zxbsearchkey").val(adkey);
        }
    }
    
    
    function searchSubmitNew() {
        var caseKey = jQuery("#zxbsearchkey").val();
//        var adkey = '请输入户型/功能间/风格/楼盘/设计师/公司等关键词';
//        var adlink = '';
        if (adlink != '') {
            if (caseKey == '' || caseKey == '请输入户型/功能间/风格/楼盘/设计师/公司等关键词' || caseKey == adkey) {
                window.open(adlink);
                return false;
            }
        }

        if (caseKey == "请输入户型/功能间/风格/楼盘/设计师/公司等关键词") {
            caseKey = "";
        }
        window.open("/zhuangxiu/casesearch.aspx?k=" + caseKey);
    }

    function addCookie(name, value, days) {
        var str = name + "=" + escape(value);
        if (days > 0) {
            var date = new Date();
            var ms = days * 24 * 3600 * 1000;
            date.setTime(date.getTime() + ms);
            str += ";path=/;domain=" + document.domain + ";expires=" + date.toGMTString();
        }
        document.cookie = str;
    }
    function getCookie(name) {
        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
        if (arr = document.cookie.match(reg))
            return unescape(arr[2]);
        else return null;
    }
</script>


