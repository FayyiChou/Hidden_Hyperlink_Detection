  function PrePub()
 {

    if(CheckCaseSubmit())
    {
     if(CheckPic()==false)
           {
                 return false;
           } 
           
             if(jQuery("#mread").attr("checked")==false)
                { 
                    msg="请阅读发布协议";
                    alert(msg);
                     jQuery("#mread").focus();
                    return false;
                }
        var taskid=0;
       
        if(taskid>0)
          { jQuery("#optType").val(4);
          }
        else 
            jQuery("#optType").val(1);
        jQuery("#task_fabu").submit();
        return true;
    }
    else
    {
        return false;
    }
 }
 
