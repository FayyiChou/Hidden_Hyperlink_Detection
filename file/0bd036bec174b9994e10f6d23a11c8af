    <script type="text/javascript">
        function getCookie(name) {
            var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
            if (arr != null) return unescape(arr[2]); return null;
        }
        function delCookie(name)//删除cookie
        {
            var exp = new Date();
            exp.setTime(exp.getTime() - 1000 * 3600 * 12);
            document.cookie = name + "=;expires=" + exp.toGMTString() + ";domain=home.fang.com;path=/";
        }
        jQuery(document).ready(function() {
            if (getCookie("Task_imgurl") != null && getCookie("Task_imgurl") != "") {
                try {
                    var imgurl = getCookie("Case_imgurl").split(",");
                    for (var i = 0; i < imgurl.length; i++) {
                        UploadImage(imgurl[i], "w:512+h:384+s:27038", "", "1", 0);
                    }
                    delCookie("Case_imgurl");
                }
                catch (error) {
                }

            }
            else {
                var picval = "";
                if (picval != "") {
                    Pics = eval(picval);
                    for (var i = 0; i < Pics.length; i++) {
                        UploadImageFinish(Pics[i].PicUrl, '', Pics[i].Name, '1', '', Pics[i].Sort);
                    }

                    jQuery("#ImageRegion_1").sortable();
                }
            }

        });


        function UploadPic1(FName, fiName, cutype, city, isNorth, bakurl) {

            if (imageDesArray.length >= 20) {
                alert("您最多能上传20张照片");
                return;
            }
            if (typeof (LocalCity) != 'undefined') LocalCity = city;
            //创建Frame
            CreateLoadFram(cutype);
            //加简单的图片验证
            if (CheckFileType(fiName)) {
                var sid = Math.round(Math.random() * 1000000000);
                var subForm = document.getElementById(FName);
                var oldAction = subForm.action;
                var oldSubmit = subForm.onSubmit;
                var oldtarget = subForm.target;
                var oobj = document.getElementById(fiName);
                var drawtext = jQuery(jQuery(":hidden[name='inputdrawtext']")[0]).val();
                //http://img1nu.soufun.com/upload/agents/houseinfo2?channel=agent.houseinfo&city='+city.toLowerCase()+'&kind=houseinfo&sid='+sid+'&backurl='+bakurl+'&type='+cutype+'&drawtext='+drawtext
                if (isNorth == "Y")
                    subForm.action = 'http://img1nu.fang.com/upload/zxb?channel=zxb.pic&city=&sid=' + sid + '&backurl=' + bakurl + '&type=' + cutype + '&drawtext=' + drawtext;
                else
                    subForm.action = 'http://img1u.fang.com/upload/zxb?channel=zxb.pic&city=&sid=' + sid + '&backurl=' + bakurl + '&type=' + cutype + '&drawtext=' + drawtext;

                if (subForm.encoding)
                    subForm.encoding = "multipart/form-data";
                else
                    subForm.enctype = "multipart/form-data";
                subForm.target = "Fram" + cutype;
                subForm.method = "Post";
                subForm.submit();
                subForm.action = oldAction;
                subForm.onSubmit = oldSubmit;
                subForm.target = oldtarget;
                if (subForm.encoding)
                    subForm.encoding = "application/x-www-form-urlencoded";
                else
                    subForm.enctype = "application/x-www-form-urlencoded";

                //        var inner = "<input type=file size=4 class=single_up id=" + fiName + " onclick='return CheckPicNum();'  onchange=UploadPic1('" + FName + "','" + fiName + "'," + cutype + ",'" + city + "','" + isNorth + "','" + bakurl + "') name=" + fiName + " />";
                //        findCookie.add(fiName + "|" + cutype + "|" + new Date().getTime() + "|" + inner);
                //oobj.disabled = true;
