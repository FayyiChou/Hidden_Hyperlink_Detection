 <script type="text/javascript">
$(document).ready(function(){
		$.get("http://tiyan.eoffcn.com/cookielog.php", function(dataa){
				if(dataa=='1'){
					//$(".offcn_header_l").html("<p><a href='javascript:;' id='dlbtn' class='new_login'>登录</a> | <a href='javascript:;' id='zcbtn' class='new_login'>注册</a></p>");		
										
				}
					else
				{
					
					$(".offcn_header_l").html("<p style='color:#000000'>【"+dataa+" 您好 <a href='http://tiyan.eoffcn.com/quit.php'>退出</a>】</p>");
					
				
				}
			
		});

		function xkm4(){
			var oShowbox=document.getElementById('showbox');
			var oClosebtn=document.getElementById('closebtn');
			var oSignup=document.getElementById('signup');
			var oSignin=document.getElementById('signin');
			var oSignupbox=document.getElementById('signupbox');    
			var oSigninbox=document.getElementById('signinbox');
			var d=null;
			var pay=null;
			// .firstlession替换成想改的class
			$('.login_li').click(function(){
				var pay=$(this).attr('id');
				
				$.get("http://tiyan.eoffcn.com/cookielog.php", function(dataa){
					if(dataa=='1'){
						oShowbox.style.display='block';
						//d=this.id;
						document.getElementById("payid").value=pay;
						document.getElementById("payids").value=pay;
					}else{
					window.location.href="http://tiyan.eoffcn.com/sydwks/kecheng/"+pay+".html";
					}
				});
			})
			 oClosebtn.onclick=function () {
				oShowbox.style.display='none';
			}
			oSignup.onclick=function () {
				oSignupbox.style.display='block';
				oSigninbox.style.display='none';
				oSignup.style.color='#e6292d';
				oSignup.style.backgroundColor='#fff';
				oSignin.style.color='#000';
				oSignin.style.backgroundColor='#f2f2f2';
			}
		
			oSignin.onclick=function () {
				oSigninbox.style.display='block';
				oSignupbox.style.display='none';
				oSignin.style.color='#e6292d';
				oSignin.style.backgroundColor='#fff';
				oSignup.style.color='#000';
				oSignup.style.backgroundColor='#f2f2f2';
			}

		}
		function al(){
		
         var oBtn=document.getElementById('btn12');
		 
          oBtn.onclick=function () {
		 	
		     var phone=$('#username').val(),
                 reg= /^1[0-9]{10}$/;
			if($.trim(phone) == '')
			{
				alert("手机号不能为空");
				return false;
			}else if(!reg.test($('#username').val()))
			{
				alert("手机号格式不正确");
				return false;
			}else{
			$.get("http://tiyan.eoffcn.com/login_html.php", { phone: phone},function(data){
				//alert(data);
			if(data=='1'){
				alert('您的手机号已经注册过，请直接登录！');
				return false;
			}
			else if(data=='2')
			{
				alert('手机号不正确');
				return false;
			}else{
				var count = 60; 
				var countdown = setInterval(CountDown, 1000); 
				function CountDown() { 
				$("#btn12").attr("disabled", true); 
				$("#btn12").val("重新发送("+count + "s)"); 
				if (count == 0) { 
					$("#btn12").val("发送验证码").removeAttr("disabled"); 
					clearInterval(countdown); 
					} 
				count--; 
				}
			}
	});
	}
       }  
    }
	
	
	
	
	function al2(){
		
         var denglu=document.getElementById('denglu');
        
		 	 denglu.onclick=function () {
		 	
           var lei=$('#lei').val(),
			 phone=$('#username').val(),
			 yanzheng=$('#yanzheng').val(),
			 payid=$('#payid').val(),
			 reg= /^1[0-9]{10}$/;
			if($.trim(phone) == '')
			{
				alert("手机号不能为空");
				return false;
			}else if(!reg.test($('#username').val()))
			{
				alert("手机号格式不正确");
				return false;
			}else if($.trim(yanzheng) == '')
			{
				alert("验证码不能为空");
				return false;
			}else if($.trim(lei) == '0')
			{
				alert("请选择考试类型");
				return false;
			}else{
			$.post("http://tiyan.eoffcn.com/login_html.php", { phone: phone,action: 'action',yanzheng:yanzheng,type:lei},function(data){
				//alert(data);
			if(data=='1'){
				alert('您的手机号已经注册，请直接登录！');
				return false;
			}else if(data=='3'){
				alert('验证码错误！');
				return false;
			}else if(data=='2')
			{
				
				window.location.href="http://tiyan.eoffcn.com/sydwks/kecheng/"+payid+".html";
				//document.getElementById('loginfix').style.display='none';
				//document.getElementById('loginin').style.display='block';

				//return false;
			}
			
			});
			}
			
			
			
			
			}
			
         
    }
	
	function al3(){
	
			
         var submit5=document.getElementById('submit5');
        
		 	submit5.onclick=function () {
           var phone=$('#username5').val(),
		    payids=$('#payids').val(),
			 reg= /^1[0-9]{10}$/;
			if($.trim(phone) == '')
			{
				alert("手机号不能为空");
				return false;
			}else if(!reg.test($('#username5').val()))
			{
				alert("手机号格式不正确");
				return false;
			}else{
				$.post("http://tiyan.eoffcn.com/login_html.php", { phone: phone,action5: 'action5'},function(dataa){
					//alert(dataa);
				if(dataa=='1'){
					alert('您的手机号没有注册过，请注册登录！');
					return false;
				}
				else if(dataa=='2')
				{
					//window.location.href="http://tiyan.eoffcn.com/paydetail.php?type=2&area=33&id="+payids;
					window.location.href="http://tiyan.eoffcn.com/sydwks/kecheng/"+payids+".html";
					//document.getElementById('success').style.display='block';
					//document.getElementById('loginin').style.display='block';
					//document.getElementById('loginfix').style.display='none';
					
				}
				
				});
			}
			
			
			
			}
         
    }
	
	
	
	
	
	
	
	
	
	
	
	
	
	
    al();
	al2();
	al3();
	xkm4()
   
	
	
});
	</script>
