 <script type="text/javascript">
	//window.onload=function () {
		function altt(){
		
         var oBtn=document.getElementById('btn12t');
		 
        
		 	
		     var phone=$('#usernamet').val(),
                 reg= /^1[0-9]{10}$/;
			if($.trim(phone) == '')
			{
				alert("手机号不能为空");
				return false;
			}else if(!reg.test($('#usernamet').val()))
			{
				alert("手机号格式不正确");
				return false;
			}else{
			$.get("http://tiyan.eoffcn.com/login_html.php", { phone: phone},function(data){
				//alert(data);
			if(data=='1'){
				alert('您的手机号已经注册过，请直接登录！');
				return false;
			}
			else if(data=='2')
			{
				alert('手机号不正确');
				return false;
			}else{
				var count = 60; 
				var countdown = setInterval(CountDown, 1000); 
				function CountDown() { 
				$("#btn12t").attr("disabled", true); 
				$("#btn12t").val("重新发送("+count + "s)"); 
				if (count == 0) { 
					$("#btn12t").val("发送验证码").removeAttr("disabled"); 
					clearInterval(countdown); 
					} 
				count--; 
				}
			}
	});
	}
        
    }
	
	
	
	
	function al2t(){
         var denglut=document.getElementById('denglut');
        
           var lei=$('#leit').val(),
			 phone=$('#usernamet').val(),
			 yanzheng=$('#yanzhengt').val(),
			 reg= /^1[0-9]{10}$/;
			if($.trim(phone) == '')
			{
				alert("手机号不能为空");
				return false;
			}else if(!reg.test($('#usernamet').val()))
			{
				alert("手机号格式不正确");
				return false;
			}else if($.trim(yanzheng) == '')
			{
				alert("验证码不能为空");
				return false;
			}else if($.trim(lei) == '0')
			{
				alert("请选择考试类型");
				return false;
			}else{
			$.post("http://tiyan.eoffcn.com/login_html.php", { phone: phone,action: 'action',yanzheng:yanzheng,type:lei},function(data){
				//alert(data);
			if(data=='1'){
				alert('您的手机号已经注册，请直接登录！');
				return false;
			}else if(data=='3'){
				alert('验证码错误！');
				return false;
			}else if(data=='2')
			{
				
				document.getElementById('toplogin').style.display='none';
				if($("#loginfix").length>0){
				document.getElementById('loginfix').style.display='none';
				document.getElementById('loginin').style.display='block';
				$("#uname").append(phone);
				}
				$(".offcn_header_l").html("<p style='color:#000000'>【"+phone+" 您好 <a href='quit.'>退出</a>】</p>");
				return false;
			}
			
			});
			}
			
			
         
    }
	
	function al3t(){
	
			
         var submit5=document.getElementById('submit5t');
        
		 	
           var phone=$('#username5t').val(),
			 reg= /^1[0-9]{10}$/;
			if($.trim(phone) == '')
			{
				alert("手机号不能为空");
				return false;
			}else if(!reg.test($('#username5t').val()))
			{
				alert("手机号格式不正确");
				return false;
			}else{
				$.post("http://tiyan.eoffcn.com/login_html.php", { phone: phone,action5: 'action5'},function(dataa){
					//alert(dataa);
				if(dataa=='1'){
					alert('您的手机号没有注册过，请注册登录！');
					return false;
				}
				else if(dataa=='2')
				{
					//window.location.href="http://tiyan.eoffcn.com/paydetail.?type=2&area=33&id=1011";
								window.location.reload();
								}
				
				});
			}
			
			
			
			
        
    }
	
	</script>
