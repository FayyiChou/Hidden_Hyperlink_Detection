<script>
    // 火车站点信息
    var trainStations = [];

    // 火车票
    $(function(){
        var allStations = {};
        $.ajax({
			 url: '/js/search/train_station_20160408.json',
		     dataType: 'json',
		     async:false
		 }).done(function (data) {
			 allStations = data.d;
		 });

        // TODO 通过ajax请求获取站点数据
        $(".lv_s_tab li[data-type='TRAIN']").on("click", function(){
            if(trainStations.length==0) {
            	$.ajax({
       				 url: '/js/search/train_station_20160408.json',
       			     dataType: 'json',
       			     async:false
       			 }).done(function (data) {
       			     trainStations = data.d;
       			 });
                var searchCondition = window.localStorage.getItem('searchCondition');
                if(searchCondition == null || searchCondition ==''){
                    $("#from_station_cn").val('北京');
                    $("#fromstation").val('北京');
                    $(".train_tomorrow").html(getDayOfWeek('2017-04-03'));
                }else{
                    fillSearchCondition();
                }
            }
        });

        $(".tt-search-btn").on("click",function(e){
            e.stopPropagation();
            if(validateTtInput()){
                var traindate = $("#input_train_date").val();
                var from_station_cn =$("#from_station_cn").val();
                var to_station_cn = $("#to_station_cn").val();
                var from_station_cn_text = from_station_cn;
                var to_station_cn_text = to_station_cn;
                var fromstationflag = false;
                var tostationflag = false ;
                var from_station = $("#fromstationcode").val();
                var to_station = $("#tostationcode").val();

                for(var i=0;i<allStations.length;i++){
                    if(allStations[i].name == from_station_cn){
                        from_station_cn=allStations[i].pinyin;
                        $("input[name='fromstation']").val(from_station_cn);
                        fromstationflag = true;
                        break;
                    }
                }

                for(var i=0;i<allStations.length;i++){
                    if(allStations[i].name ==to_station_cn){
                        to_station_cn=allStations[i].pinyin;
                        $("input[name='tostation']").val(to_station_cn);
                        tostationflag = true;
                        break;
                    }
                }
                var fromstationpinyin = from_station_cn;
                var tostationpinyin = to_station_cn;
                var traindate = $("#input_train_date").val();
                var trainTYpe=$("#traintype");
                var url = "http://train.lvmama.com/booking/"+fromstationpinyin+"-"+tostationpinyin;
                if(trainTYpe.attr("class") == "train-type-gd"){
                    url = url+"?trainType=gaotie-dongche&date1="+traindate;
                } else {
                    url = url+"?date1="+traindate;
                }
                if(fromstationflag == false || tostationflag == false){
                    $("input[name='fromstation']").val("UNKNOW");
                    $("input[name='tostation']").val("UNKNOW");
                }else{
                    writeSearchRecord();
                }
                url = url+"&from_station="+from_station+"&to_station="+to_station+"&losc=101192&ict=i";
                cmCreateElementTag("首页搜索_火车票_" + from_station_cn_text + "_" + to_station_cn_text, "首页搜索框");
                cmCreateElementTag("train", "homepage_search");
                window.location=url;
            }
        });
    })

    //记录搜索历史
    function writeSearchRecord(){
        var searchCondition = $("#fromstation").val()+"|"+$("#tostation").val()+"|"+$("#input_train_date").val();

        if(window.localStorage){
            //console.log('记录搜索条件='+searchCondition);
            window.localStorage.setItem('searchCondition',searchCondition);
        }
    }

    function fillSearchCondition(){
//根据用户搜索历史记录，再次进入的时候，只要cookie不清，搜索条件回填进相应的框
        if(window.localStorage){
            var searchCondition = window.localStorage.getItem('searchCondition');
            if(searchCondition != null && searchCondition != ''){
                var fromstationcn = searchCondition.split('|')[0];
                var tostationcn = searchCondition.split('|')[1];
                var input_train_date = searchCondition.split('|')[2];

                $("#fromstation").val(fromstationcn);
                for(var i=0;i<trainStations.length;i++){
                    if(trainStations[i].pinyin == fromstationcn){
                        var from_station_cn = trainStations[i].name;
                        $("#from_station_cn").val(from_station_cn);
                        break;
                    }
                }

                $("#tostation").val(tostationcn);
                for(var i=0;i<trainStations.length;i++){
                    if(trainStations[i].pinyin == tostationcn){
                        var to_station_cn = trainStations[i].name;
                        $("#to_station_cn").val(to_station_cn);
                        break;
                    }
                }
                //当前时间是否大于缓存时间
                var nowstr = "2017-04-01";
                var endStr = "2017-04-03";

                var r=input_train_date.match(/^(\d{1,4})(-|\/)(\d{1,2})\2(\d{1,2})$/);
                if(r==null){//如果不是正确日期格式则给默认值
                    $("#input_train_date").val(endStr);
                    $(".train_tomorrow").html(getDayOfWeek(endStr));
                }else{
                    if(nowstr > input_train_date){
                        $("#input_train_date").val(endStr);
                        $(".train_tomorrow").html(getDayOfWeek(endStr));
                    }else{
                        $("#input_train_date").val(input_train_date);
                        $(".train_tomorrow").html(getDayOfWeek(input_train_date));
                    }
                }
                return;
            }
        }
    }
</script>

<script type="text/javascript">

    /*老用户登录弹窗*/
    $(function(){
        $.ajax({
            type: "POST",
            url: "/homePage/showCouponList.do",
            dataType: "json",
            success: function (map) {
                if (map.json == "success") {
                    $("#totalUseRecords").append(map.totalUseRecords); //用户可用优惠券数量
                    var userCouponList = map.userCouponList;   //用户可用优惠券
                    for (var i in userCouponList) {
                        if (i < 3) {   //最多显示三张优惠券
                            $("#userCouponList").append
                            ("<li class='clearfix'id='userCouponli" + i + "\'>" +
                                    "<div class='ad-regist-account-left fl'id='userCouponL" + i + "\'>" +
                                    "<p class='ad-regist-account-sequence'>" + userCouponList[i].couponCode + "</p>"); //显示优惠券券码
                            //显示优惠券金额或者打折
                            if (userCouponList[i].favorType != null && userCouponList[i].favorType == "AMOUNT_AMOUNT_WHOLE") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'><em>¥</em>" + userCouponList[i].argumentY / 100 + "</p>");
                            } else if (userCouponList[i].favorType != null && userCouponList[i].favorType == "AMOUNT_AMOUNT_INTERVAL") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'><em>¥</em>" + userCouponList[i].argumentZ / 100 + "</p> ");
                            } else if (userCouponList[i].favorType != null && userCouponList[i].favorType == "AMOUNT_QUANTITY_WHOLE") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'><em>¥</em>" + userCouponList[i].argumentY / 100 + "</p>");
                            } else if (userCouponList[i].favorType != null && userCouponList[i].favorType == "AMOUNT_QUANTITY_INTERVAL") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'><em>¥</em>" + userCouponList[i].argumentZ / 100 + "</p>");
                            } else if (userCouponList[i].favorType != null && userCouponList[i].favorType == "AMOUNT_QUANTITY_PRE") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'><em>¥</em>" + userCouponList[i].argumentY / 100 + "</p>");
                            } else if (userCouponList[i].favorType != null && userCouponList[i].favorType == "DISCOUNT_AMOUNT_WHOLE") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'>" + userCouponList[i].argumentY + "<em>折</em></p>");
                            } else if (userCouponList[i].favorType != null && userCouponList[i].favorType == "DISCOUNT_QUANTITY_WHOLE") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'>" + userCouponList[i].argumentY + "<em>折</em></p>");
                            } else if (userCouponList[i].favorType != null && userCouponList[i].favorType == "DISCOUNT_QUANTITY_PRE") {
                                $("#userCouponL" + i).append("<p class='ad-regist-account-price'>" + userCouponList[i].argumentZ + "<em>折</em></p>");
                            }
                            $("#userCouponL" + i).append("<p class='ad-regist-account-tip'>" + userCouponList[i].couponName + "</p></div> ");
                            $("#userCouponli" + i).append("<div class='ad-regist-account-right fl' id='userCouponR" + i + "\'>");
                            //显示优惠券可以时间
                            if (userCouponList[i].startTime != null && userCouponList[i].endTime == null) {
                                $("#userCouponR" + i).append("<p>" + userCouponList[i].startTime + "至</p>");
                            } else if (userCouponList[i].startTime == null && userCouponList[i].endTime != null) {
                                $("#userCouponR" + i).append("<p>至" + userCouponList[i].endTime + "</p>");
                            } else if (userCouponList[i].endTime != null && userCouponList[i].startTime != null) {
                                $("#userCouponR" + i).append("<p>" + userCouponList[i].startTime + "<br>至" + userCouponList[i].endTime + "</p>");
                            } else if (userCouponList[i].startTime == null && userCouponList[i].endTime == null) {
                            }
                            //显示优惠券使用范围
                            if (userCouponList[i].couponTarget != null && userCouponList[i].couponTarget == "PRODUCT") {
                                $("#userCouponR" + i).append("非全场通用</div> </li>");
                            } else if (userCouponList[i].couponTarget != null && userCouponList[i].couponTarget == "ORDER") {
                                $("#userCouponR" + i).append("全场通用</div> </li>");
                            }
                        }
                    }
                    var _scrolltop = $(document).scrollTop() + 150,
                            height_w = $(document).height();
                    $('.pop_body_bg').css({'height': height_w, 'width': $(document.body).width()}).show();
                    $('.ad-regist-account-pop').show().css({'top': _scrolltop});
                    $('.close').click(function () {
                        $('.pop_body_bg,.ad-regist-account-pop').hide();
                    });

                }
            }
        })
    });


</script>

<script>
// 底通红包
$(function(){
	var $document = $(document);

	$document.on("click", ".footBar-qm", function(){
			$.ajax({
				 url: 'http://www.lvmama.com/homePage/ajaxGetCouponForQingMing.do',
				 dataType: 'json'
			}).done(function (data) {
				 if (data.code == '0') {
					showElevenResultDialog(true);
				 } else if (data.code == '200002') {
					window.location="https://login.lvmama.com/nsso/login?service="+encodeURIComponent(document.location);
				 } else if (data.code == '200003') {
					showElevenResultDialog(false);
				 } 
			});
	});

	 $document.on("click", ".footBar-pickup-rule", function(){
		 	$(".eleven-overlay, .eleven-dialog-rule").show();
		 });
 
	$document.on("click", ".eleven-dialog .eleven-close", function(){
		$(".eleven-overlay, .eleven-dialog").hide();
	});


    function showElevenResultDialog(isSuccessful) {
			var $img = $(".eleven-dialog-result").find("img");
			var $text = $(".eleven-dialog-result").find("p.eleven-result-text");
			var $date = $(".eleven-dialog-result").find("p.eleven-result-date");
			var $tip = $(".eleven-dialog-result").find("p.eleven-result-tip");
			if(isSuccessful) {
				$img.attr("src", "http://s1.lvjs.com.cn/img/v6/myspace/lv_success.png");
				$text.html("恭喜您领取成功！");
				// 提示
				$tip.html('优惠券使用时间：3.29-4.4<br>邮轮优惠券自领取日起15日内有效，<a href="http://www.lvmama.com/myspace/account/coupon.do">立即使用</a>');
			} else {
				$img.attr("src", "http://s2.lvjs.com.cn/img/v6/myspace/lv_no.png");
				$text.html("您已领取过该红包！");
				// 提示
				$tip.html('优惠券使用时间：3.29-4.4<br>邮轮优惠券自领取日起15日内有效，<a href="http://www.lvmama.com/myspace/account/coupon.do">立即使用</a>');
			}
			$(".eleven-dialog-result, .eleven-overlay").show();
	}
});

</script>



