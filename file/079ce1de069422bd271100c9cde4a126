$(".z").on('click',function(){

        var Oa=$(this);

        var id=Oa.attr('id');//获取id属性

        var vl=document.getElementById("zan_z").innerHTML;

        /*alert(vl);*/

            vl=parseInt(vl)+1;    

        $.post("/Index-zan.html",{id:id},function(data){

            if(data.status==1){ 

                alert('感谢您的支持！');//模拟异步数据加1

                $("#zan_z").text(vl);

            }else{

                alert('您已经点过赞了,不要重复哦！');

            }

        },'json'); 

    })

 

