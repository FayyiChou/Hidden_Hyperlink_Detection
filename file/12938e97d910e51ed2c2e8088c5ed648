<script type="text/javascript">
$(function(){
	var moveHeight = 22,
		moveTime = 200,
		curMove = null;
	$('.hover_animate').hover(function (e){
		var curLi = $(this);
		curMove = setTimeout(function (){
			curLi.children('div.hoverdiv_nol').animate({ top: '-' + moveHeight + 'px' }, moveTime);
			curLi.children('div.hoverdiv_act').animate({ top: '0px' }, moveTime);
		}, 0);
	},function(e){
		clearTimeout(curMove);
		$(this).children('div.hoverdiv_nol').animate({ top: '0px' }, moveTime);
		$(this).children('div.hoverdiv_act').animate({ top: moveHeight + 'px' }, moveTime);
	});
});
</script>
