<script>
	$(function(){
		$('.wwlw').click(function(){
			$('#mask').css('display','none');
		});
		$('#myw').live('click',function(){
			var _height = $(window).height(); //获取当前窗口高度
			
			
			$('#wlw_zy > div').css('height',_height+'px');
			
			$('#mask').css('display','block');
			$('#wlw_pic li').css('line-height',_height+'px'); //跟随窗口高度
			$('#wlw_pic li:eq(0)').css({display:'block'}); //第一个li显示，其它隐藏
			
			
			//滚动条变0
			$(window).scrollTop(0);
			iwt();
			
		});
		function iwt(){
			var _imgWeight = $('#wlw_pic img:eq(0)').width();//获取当前图片宽度
			//如果当前图片宽度大于1024则图片最大宽度为1024
			if(_imgWeight > 1024){
				$('#wlw_pic li:eq(0) img').css('width','1024px');
			}
		}
		$(window).resize(function(){
			var _height = $(window).height(); //获取当前窗口高度
			$('#wlw_pic li').css('line-height',_height+'px'); //跟随窗口高度
		});
		//添加鼠标形状
		$('.d_left').mousemove(function(e){
			$(this).css('cursor','url("http://cdn.03.k618.cn/web/images/entername/picshow/left.cur"),pointer')
		});
		$('.d_right').mousemove(function(e){
			$(this).css('cursor','url("http://cdn.03.k618.cn/web/images/entername/picshow/right.cur"),pointer')
		});
		
		i=0;
		var li_length = $('#wlw_pic li').length;  //总共多少张图片
		$('.wlw_tit').empty().html('当前是第<span class="red">1</span>张，总共<span class="red">'+li_length+'</span>张');
		$('#wlw_zy > .d_left').click(function(){
			
			i--;
			
			if(i >= 0){
				my_uu();
				my_left(x);
				$('#wlw_zy > .d_right').show();
			}else{
				i++;
				$(this).hide();
				alert('这是第一张图片！');
			}
		});
		
		
		$('#wlw_zy > .d_right').click(function(){
			
			i++;
			
			if(i < li_length){
				my_uu();
				my_right(x);
				$('#wlw_zy > .d_left').show();
			}else{
				i--;
				$(this).hide();
				alert('这是最后一张图片！');
			}
		});
		function my_uu(){
			x = i;
			$('.wlw_tit').empty().html('当前是第<span class="red">'+(x+1)+'</span>张，总共<span class="red">'+li_length+'</span>张');
		}
		function my_left(u){
			$('#wlw_pic > li').css('display','none');
			$('#wlw_pic li:eq('+u+')').css('display','block');
			
			var _imgWeight = $('#wlw_pic li:eq('+u+') img').width();//获取当前图片宽度
			//如果当前图片宽度大于1024则图片最大宽度为1024
			if(_imgWeight > 1024){
				$('#wlw_pic li:eq('+u+') img').css('width','1024px');
			}
			
		}
		function my_right(u){
			$('#wlw_pic > li').css('display','none');
			$('#wlw_pic li:eq('+u+')').css('display','block');
			
			var _imgWeight = $('#wlw_pic li:eq('+u+') img').width();//获取当前图片宽度
			//如果当前图片宽度大于1024则图片最大宽度为1024
			if(_imgWeight > 1024){
				$('#wlw_pic li:eq('+u+') img').css('width','1024px');
			}
		}
	});
</script>
