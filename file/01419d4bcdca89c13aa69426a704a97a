<script>
    var _cookie_id = "58e0231672b8c";
    var _log_name = 'wap_notice_a129gxATnkg_';
    /**
     * 记录上报日志
     * @param click
     */
    function saveLog(click) {
        var url = "/index/statis?name=" + _log_name + "&isIOS=" + isIOS +"&click=" + click + '&cookie_id=' + _cookie_id;
        $.get(url, function() {});
    }
