    // 访问日志
    saveLog('load');
    window.onload = function(){
        gotoAPP();
        if (isMiaopai) {
            $(".more").css("padding-bottom", 0);
        } else {
            $(".more").css("padding-bottom", $(".footer_banner img").height()+20);
        }
    }

    // android 版本判断
    var u = window.navigator.userAgent;
    function getAndroidVersion() {
        return u.substr(u.indexOf('Android') + 8, 3);
    }

    /**
     * android 唤起操作
     * @param ecduri
     */
    function android_open(ecduri) {
        var ifr = document.createElement('iframe');
        ifr.src = ecduri;
        ifr.style.display = 'none';
        document.body.appendChild(ifr);
    }

</script>
<script>

    function getData(custom){
        $.ajax({
            url:custom['url'],
            dataType:"jsonp",
            success:function(data){
                if(typeof custom['fn'] == "function"){
                    custom['fn'](data);
                }
                gotoAPP();
            }
        })
    }

    // 分享微博
    $("#weibo_share").click(function(e) {
        e.preventDefault();
        // 上报日志
        saveLog('weibo_share');
        var _url = 'http://service.weibo.com/share/share.php?appkey=2637646381&url='+CurrentUrl+'&title='+Title+'&sourceUrl=http%3A%2F%2Fwww.miaopai.com&content=utf8&searchPic=false';
        window.location.href = _url;
    });

    function gotoAPP(){
        $(document).delegate(".openApp","click",function(e){
            e.preventDefault();
            var __this = $(this);
            var _id = __this.attr('data-id');
            // 点击统计名
            var _click = 'wap_click_' + _id;
            // 下载统计名
            var _download = 'wap_download_' + _id;
            if(isWxOrQQ){
                if(isIOS9()){
                    var ios9_url = __this.attr("data-ios");
                    window.location.href="https://app.yixia.com/miaopai/applinks/index.html?redirectURI="+ios9_url;
                }else{
                    $(".dialog").show();
                }

                return;
            }

            // 点击统计
            saveLog(_click);
            if (__this.attr("data-ios") != '' && __this.attr("data-adr") != '') {
                if(isIOS){
                    var ios_url = __this.attr("data-ios");
                    if(!isMiaopai && isIOS9()){
                        window.location.href = "https://app.yixia.com/miaopai/applinks/index.html?redirectURI="+ios_url;
                        return;
                    }else{
                        window.location.href = ios_url;
                    }
                }else{
                    var url = __this.attr("data-adr");
                    android_open(url);
                }
            }
            if (!__this.attr("data-h5")&&!isMiaopai){
                setTimeout(function(){
                    // 下载统计
                    saveLog(_download);
                    window.location.href ="http://m.miaopai.com/v/downType/weibo_toutiao";
                },2000);
                return;
            }else if(!__this.attr("data-h5")&&isMiaopai){
                return;
            }
            var timeout = setTimeout(function(){
                // 下载统计
                saveLog(_download);
                window.location.href = __this.attr("data-h5");
            }, 3000);
        });
    }
</script>


