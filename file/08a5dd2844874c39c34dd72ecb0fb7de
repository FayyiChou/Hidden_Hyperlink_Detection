function onLoadComment(){
	var obj = document.getElementById('theRes');
	var dom = (document.compatMode &&document.compatMode=="CSS1Compat")?document.documentElement:document.body;
	var scrollTop = dom.scrollTop||document.body.scrollTop||0;
	if (obj.offsetTop-scrollTop-dom.clientHeight<100){
		//加载评论
		new Ajax.Request ('http://www.verycd.com/ajax/folder/comments?id=430274', {
        method:'get',
        onComplete: function(transport){
        	if (transport.status==200){
				old_get('userCommentDiv').innerHTML=transport.responseText;
				dynamicTime.updatePage();
				hover_tips.ini(old_get('userCommentDiv'));
				setTimeout(memberHonor.updatePage(),100);
			}else{
				old_get('commentLoading').innerHTML = '';
			}
		}
		});
		return true;
	}
	setTimeout(onLoadComment,100);
}
onLoadComment();

