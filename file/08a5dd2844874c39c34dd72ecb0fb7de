<script type="text/javascript">
function showComment(id) {
    old_get(id+'_show').style.display='';
    old_get(id+'_hidden').style.display='none';
}
function hiddenComment(id) {
    old_get(id+'_hidden').style.display='';
    old_get(id+'_show').style.display='none';
}

var favFwindow;
var favBwindow;
var fromClick = false;
function VC_popup(dom){
    try {
        VeryCD.Track("/stat/miniShow/");
    } catch(e) {}
    fromClick=true;
    path_to_root_dir = "http://v4.vcimg.com/";
    var loginBox = new GlassBox();
    if (navigator.userAgent.toLowerCase().indexOf("msie") != -1) {
        loginBox.init( 'favWindow', '480px', '250px', 'false', 'facebookstyle', true, false );
        loginBox.apos(old_get("addFav").cumulativeOffset().left-14+'px',old_get("addFav").cumulativeOffset().top+12+'px')
    }else{
        loginBox.init( 'favWindow', '480px', '235px', 'false', 'facebookstyle', true, false );
        loginBox.apos(old_get("addFav").cumulativeOffset().left-14+'px',old_get("addFav").cumulativeOffset().top-186+'px')
    }
    old_get("favIframe").src = dom;
    return false;
}
function verycdlogin(dom){
    try {
        VeryCD.Track("/stat/miniShow/");
    } catch(e) {}
    fromClick=true;
    path_to_root_dir = "http://v4.vcimg.com/";
    var loginBox = new GlassBox();
    if (navigator.userAgent.toLowerCase().indexOf("msie") != -1) {
        loginBox.init( 'favWindow', '480px', '250px', 'false', 'facebookstyle', true, false );
        loginBox.apos(old_get("collectli").cumulativeOffset().left-14+'px',old_get("collectli").cumulativeOffset().top+12+'px')
    }else{
        loginBox.init( 'favWindow', '480px', '235px', 'false', 'facebookstyle', true, false );
        loginBox.apos(old_get("collectli").cumulativeOffset().left-14+'px',old_get("collectli").cumulativeOffset().top-186+'px')
    }
    old_get("favIframe").src = dom;
    return false;
}



function hideLoading(){
    window.setTimeout(function () {
        old_get("loading").style.display="none";
    },800);
}
function addPicClass() {
    if(old_get("cancelFav").className == "") {
        old_get("cancelFav").className = "cancelFav";
        old_get("cancelFav").title = "取消收藏";
        new JP.mouseover.auto({
            css_mouseover_class: 'cancelFav',
            element_types: ['img', 'input'],
            root_node: 'moreInfo',
            show_broken_img: true
        });
    }
}
function add2fav(){
    //加为收藏的统计
    try {
        VeryCD.Track('/stat/topicFavAdd/');
    } catch(e) {}
    if(!fromClick) return false;

    
    /* 相关收藏 */
            path_to_root_dir = "http://v4.vcimg.com/";
        var favBox = new GlassBox();
        favBox.init( 'favBox', '540px', '256', 'false', 'facebookstyle', true, false );
        favBox.lbo( false, 0.5 );
        favBox.apos(old_get("addFavModule").cumulativeOffset().left-14+'px',old_get("addFavModule").cumulativeOffset().top+12+'px');
        

    var thisUrl = '/topics/430274/favor';
    new Ajax.Request (thisUrl, {
        method:'post',
        onComplete: function(transport){
            var favnum =  old_get('favoriteNumber').getAttribute('favorites');
            favnum++;
            if (favnum < 10000) {
            	old_get('favoriteNumber').innerHTML = '  ' + favnum + '次 收藏';
            } else {
            	old_get('favoriteNumber').innerHTML = '  10000+次 收藏';
            }
            old_get('favoriteNumber').setAttribute('favorites', favnum);
            old_get('favoriteNumber').style.display = '';
            old_get('visitTimes-title').style.display = 'inline-block';
            var re = YAHOO.lang.JSON.parse(transport.responseText);
            if (re.status == 0 || re.status == 4) {
                old_get('addFavModule').innerHTML ='<img src=http://v4.vcimg.com/images/favorite/myFavorite.gif?20090409 class=\'\' onMouseout=\'addPicClass()\' id=\'cancelFav\' title=\'我已收藏了\'/>';
                if(re.status == 0) {
                    popoupNotemsg('该资源收藏成功.');
                } else if(re.status == 4) {
                    popoupNotemsg('该资源已经被收藏过.');
                }
                _enableBtn();
            } else if(re.status == 1) {
                VC_popup(formHtml);
                popoupNotemsg('该资源收藏失败.');
            }
        }
    });
}
     _issinat = false;
     _iscollect_base2sinat = false;

    if(VeryCD.Member.Id>0){
        get_sinat_info();
    }
    function get_sinat_info() {
    	if(!VeryCD.Member.Id) {
			return false;
		}
        var thisUrl = '/api/account/check_sinat_setting';
		new Ajax.Request (thisUrl, {
        	method:'post',
        	parameters: '',
            onComplete: function(transport) {
            	var re = YAHOO.lang.JSON.parse(transport.responseText);
   				if(re.collect_base == undefined && re.collect_folder == undefined && re.comment_folder == undefined){return false}

        		_issinat = true;
            	if (true == re.collect_base) {
            		_iscollect_base2sinat = true;
                }

            }
        });
    }

function del4fav(){
    //取消收藏的统计
    try {
        VeryCD.Track('/stat/topicFavCancel/');
    } catch(e) {}
    var thisUrl = '/topics/430274/unfavor';
    new Ajax.Request (thisUrl, {
        method:'post',
        onComplete: function(transport){
            var re = YAHOO.lang.JSON.parse(transport.responseText);
            if (re.status == 0) {
                old_get('addFavModule').innerHTML ='<img src=http://v4.vcimg.com/images/favorite/favorite.gif?20090409 class=\'addFav\' id=\'addFav\' title=\'收藏该资源\' />';
                var favnum =  old_get('favoriteNumber').getAttribute('favorites');
                favnum--;
                if (favnum < 10000) {
                	old_get('favoriteNumber').innerHTML = '  ' + favnum + '次 收藏';
                } else {
                	old_get('favoriteNumber').innerHTML = '  10000+次 收藏';
                }
                old_get('favoriteNumber').setAttribute('favorites', favnum);
                old_get('favoriteNumber').style.display = '';
                old_get('visitTimes-title').style.display = 'inline-block';
                popoupNotemsg('取消收藏成功.');
                _enableBtn();
            } else if(re.status == 1) {
                VC_popup(formHtml);
                popoupNotemsg('取消收藏失败.');
            }
        }
    });
}

function _enableBtn(){
    var _favinterval = setInterval(function() {
        try{
            if(old_get("addFav")!=null){
                fromClick=true;
                old_get("addFav").onclick=add2fav;
            }
            if(old_get("cancelFav")!=null){
                old_get("cancelFav").onclick=del4fav;
            }
        }catch(e){
            alert(e);
        }
    }, 100);
    window.setTimeout(function () {
        clearInterval(_favinterval);
    },3000);
}
</script>

<script language="javascript" type="text/javascript">
// 生成升级的内容提示到文档中
insertContent();

var postEntry = {
    commentId 	: 0,
    upNumber	: 0,
    downNumber	: 0,
    upNumberHt	: null,
    downNumberHt: null,
    postScore  : 0
}

function upNumberStat(commentId) {
    var hotUrl = "http://stat.verycd.com/counters/folder_cmt_diggup/"+commentId+"";
    var object = new JSONscriptRequest(hotUrl);
    object.buildScriptTag();
    object.addScriptTag();
}
function upNumber(commentId) {
    hiddenVoteButtons(commentId);
    postEntry.commentId = commentId;
    upNumberComplete();
    var thisUrl = 'http://www.verycd.com/topics/430274/digup/' + commentId;
    new Ajax.Request (thisUrl, {
    	method:'post',
        onComplete: function(transport) {
            if ('failure' == transport.responseText) {
                popoupNotemsg('请不要重复投票.');
            }
        }
    });
    upNumberStat(commentId);
}
function downNumber(commentId) {
    hiddenVoteButtons(commentId);
    postEntry.commentId = commentId;
    downNumberComplete();
    var thisUrl = 'http://www.verycd.com/topics/430274/digdown/' + commentId;
    new Ajax.Request (thisUrl, {
    	method:'post',
        onComplete: function(transport) {
            if ('failure' == transport.responseText) {
                popoupNotemsg('请不要重复投票.');
            }
        }
    });
}

function upNumberComplete() {
    if (old_get('uvb_hot_' + postEntry.commentId)){
        var uvbHot = old_get('uvb_hot_' + postEntry.commentId);
    }
    if (old_get('uvb_new_' + postEntry.commentId)){
        var uvbNew = old_get('uvb_new_' + postEntry.commentId);
    }
    if (uvbHot){
        var upNumber 	= uvbHot.getAttribute("score");
    }
    if (uvbNew) {
        var upNumber 	= uvbNew.getAttribute("score");
    }
    upNumber = parseInt(upNumber) + parseInt(1);
    if (old_get('post_score_hot_up_'+postEntry.commentId)) {
		//　针对　IE
		if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){
       		old_get('post_score_hot_up_'+postEntry.commentId).innerHTML = '&nbsp;[+'+upNumber+']&nbsp;';
			var now= "&nbsp;"+old_get('post_score_hot_down_'+postEntry.commentId).childNodes[0].innerHTML+"&nbsp;";
		}else{
       		old_get('post_score_hot_up_'+postEntry.commentId).innerHTML = '[+'+upNumber+']';
			var now= old_get('post_score_hot_down_'+postEntry.commentId).childNodes[1].innerHTML;
		}
		old_get('post_score_hot_down_'+postEntry.commentId).innerHTML = now;
   }
    if (old_get('post_score_new_up_'+postEntry.commentId)) {
		if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){
       		old_get('post_score_new_up_'+postEntry.commentId).innerHTML = '&nbsp;[+'+upNumber+']&nbsp;';
			var now= "&nbsp;"+old_get('post_score_new_down_'+postEntry.commentId).childNodes[0].innerHTML;
		}else{
       		old_get('post_score_new_up_'+postEntry.commentId).innerHTML = '[+'+upNumber+']';
		    var now= old_get('post_score_new_down_'+postEntry.commentId).childNodes[1].innerHTML;
		}
		old_get('post_score_new_down_'+postEntry.commentId).innerHTML = now;
    }

}

function downNumberComplete(){
    if (old_get('dvb_hot_' + postEntry.commentId)){
        var dvbHot = old_get('dvb_hot_' + postEntry.commentId);
    }
    if (old_get('dvb_new_' + postEntry.commentId)){
        var dvbNew = old_get('dvb_new_' + postEntry.commentId);
    }
    if (dvbHot){
        var downNumber 	= dvbHot.getAttribute("score");
    }
    if (dvbNew) {
        var downNumber 	= dvbNew.getAttribute("score");
    }
    downNumber = parseInt(downNumber) - parseInt(1);

    if (old_get('post_score_hot_down_'+postEntry.commentId)) {
	   if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){
       		old_get('post_score_hot_down_'+postEntry.commentId).innerHTML = '&nbsp;['+downNumber+']&nbsp;';
			var now= "&nbsp;"+old_get('post_score_hot_up_'+postEntry.commentId).childNodes[0].innerHTML+"&nbsp;";
		}else{
       		old_get('post_score_hot_down_'+postEntry.commentId).innerHTML = '['+downNumber+']';
			var now= old_get('post_score_hot_up_'+postEntry.commentId).childNodes[1].innerHTML;
		}
	   old_get('post_score_hot_up_'+postEntry.commentId).innerHTML = now;
    }
    if (old_get('post_score_new_down_'+postEntry.commentId)) {
	   if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){
			var now= "&nbsp;"+old_get('post_score_new_up_'+postEntry.commentId).childNodes[0].innerHTML+"&nbsp;";
       		old_get('post_score_new_down_'+postEntry.commentId).innerHTML = '&nbsp;['+downNumber+']&nbsp;';
		  }else{
			now= old_get('post_score_new_up_'+postEntry.commentId).childNodes[1].innerHTML;
			old_get('post_score_new_down_'+postEntry.commentId).innerHTML = '['+downNumber+']';
		  }
	   old_get('post_score_new_up_'+postEntry.commentId).innerHTML = now;
    }


}

function hiddenVoteButtons(commentId) {
    if (old_get('uvb_hot_' + commentId)){
        var uvbHot = old_get('uvb_hot_' + commentId);
        uvbHot.innerHTML = '<img src="http://v4.vcimg.com/images/file/gotop01.gif" class="gobot" />';
    }
    if (old_get('dvb_hot_' + commentId)){
        var dvbHot = old_get('dvb_hot_' + commentId);
        dvbHot.innerHTML = '<img src="http://v4.vcimg.com/images/file/gobot01.gif" class="gobot" />';
    }
    if (old_get('uvb_new_' + commentId)){
        var uvbNew = old_get('uvb_new_' + commentId);
        uvbNew.innerHTML = '<img src="http://v4.vcimg.com/images/file/gotop01.gif" class="gobot" />';
    }
    if (old_get('dvb_new_' + commentId)){
        var dvbNew = old_get('dvb_new_' + commentId);
        dvbNew.innerHTML = '<img src="http://v4.vcimg.com/images/file/gobot01.gif" class="gobot" />';
    }
}

</script>


