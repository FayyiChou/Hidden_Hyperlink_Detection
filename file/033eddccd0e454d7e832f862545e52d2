     <script type="text/javascript">
    	$(function () {
           //手机导航
            jQuery.getJSON('http://my.lotour.com/login/login/islogin?random=' + (new Date()) + '&callback=?', function (data) {
            loginUserData = data;
            if (parseInt(data.uid) > 0) {
                $("a.wap-menu-right img").attr("src", data.photo != "" ? "http://img.lotour.com/UserPhoto/" + data.photo.replace(".jpg", "_50.jpg") : "http://img.lotour.net/UserPhoto/zxp01_20150801_50.jpg");
                $("a.wap-menu-right").unbind('click').bind('click', function () {
                    window.location.href = "http://passport.lotour.com/user/userInfo.html?memberId=" + data.uid;
                });
            }
            else {
                $("a.wap-menu-right img").attr("src", "http://img1.lotour.com/wap-nav/top_right_01.png");
                $("a.wap-menu-right").unbind('click').bind('click', function () {
                    jQuery.getScript("http://js.lotour.net/login/floatlogin_v3.js", function () {
                        var loginReg = new LoginRegV3();
                        loginReg.init();
                        loginReg.showLogin();
                        loginReg.regCallBack = function (data) {
                            window.location.reload();
                        };
                        loginReg.loginCallBack = function (data) {
                            window.location.reload();
                        };
                    });
                });
            }
        });

        $(".have-second").click(function () {
            $(this).next().slideToggle();
        });
        $(".wap-menu-left").on('click', function () {
            var targe = $('header');
            if (targe.css('margin-left') == '5.6rem') {
                $(".allwap-header").animate({ 'margin-left': '0px' });
                $("#wapleftmenu").animate({ 'left': '-5.6rem' });
                $(".wapleft").hide();
                $(".wapmask").hide();
                $("html,body").css("overflow", "auto");
            } else {
                $("header").animate({ 'margin-left': '5.6rem' });
                $("header").css("position", "static");
                $("#wapleftmenu").animate({ 'left': '0px' });
                $(".wapmask").css("left", "auto");
                $(".wapmask").css("right", "-0.8rem");
                $(".wapmask").css("height", "99.6%");
                $(".wapleft").show();
                $(".wapmask").show();
                $("html,body").css("overflow", "hidden");
            }

        })
        $(".wapmask").on('click', function () {
            $("html,body").animate({ scrollTop: 0 }, 100); ;
            $("header").animate({ 'margin-left': '0px' });
            $("#wapleftmenu").animate({ 'left': '-5.6rem' })
            $(".wapmask").hide();
            $("html,body").css("overflow", "auto");
        })
    });
</script>
