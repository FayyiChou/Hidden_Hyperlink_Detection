    function delClose2() {
        $("#deldiv2").hide().addClass("sphl");
        $("#layerBg").hide();
    }

    function editArticle(footId, href) {
        jQuery.ajax({
            url: 'http://my.lotour.com/i/common/IsInHome',
            type: "get",
            data: { "footId": footId },
            cache: false,
            dataType: "jsonp",
            contentType: "application/x-www-form-urlencoded; charset=utf-8",
            success: function (result) {
                var data = result.Result.Data;
                if (!data.result) {
                    //window.open(href);
                    location.href = href;
                }
                else {
                    $('#layerBg').show();
                    $('#deldiv2').show().removeClass("sphl");
                }
            },
            error: function () {
                location.href = href;
                //window.open(href);
            }
        });
    }
</script>
