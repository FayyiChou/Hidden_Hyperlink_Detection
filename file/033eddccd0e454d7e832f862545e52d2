<!--=E 768分辨率下的底部通栏-->
<script type="text/javascript">
    var pageViewUrl = '/i/Detail/Detail/GetRegionInterestedNum?articleId=1021639&articleType=1&viewNum=4531&regionId=201927';
    //左侧悬浮固定
    function leftFix() {
        var hh = $(".ia-left").outerHeight(true),
		bdRt = $(".ia-left").offset().top,
		bhh = $(".ia-youji").offset().top;
        $(window).scroll(function () {
            var st = $(document).scrollTop(),
			bhh = $(".ia-youji").offset().top;
            if (st - bdRt >= 0) {
                $(".ia-left").addClass("ia-left-fix");

            } else if (st - bdRt < 0) {
                $(".ia-left").removeClass("ia-left-fix");
            }
            if (st >= bhh - hh) {
                $(".ia-left").removeClass("ia-left-fix");
            }
        });
    }

    //左侧小屏高度调整
    function lefts() {
        if ($(window).height() < 600) {
            $(".ia-left").addClass("ia-left-s");
        } else {
            $(".ia-left").removeClass("ia-left-s");
        }
    }

    //相关阅读
    function relRead() {
        $('.idx-brand ul li .item').hover(function () {
            if ($(this).find('p').length < 0) { return false }
            $(this).stop().animate({
                'bottom': 0
            },
        300)
        },
    function () {
        if ($(this).find('p').length < 0) { return false }
        $(this).stop().animate({
            'bottom': -28
        },
        500)
    });
        if ($(window).width() > 768) {
            $(".idx-brand ul li").css("height", $(".idx-brand ul li .item").height() - 28);
        } else {
            $(".idx-brand ul li").css("height", "auto");
        }
    }


    //顶部固定
    function topFix() {
        var scrollFunc = function (e) {
            e = e || window.event;
            if (e.wheelDelta) {
                if (e.wheelDelta > 0) {
                    $('header').addClass("header-fix");
                    $(".ia-left").addClass("ia-left-fix2");
                    $(".lvwen-tit").addClass("lvwen-tit2");
                    $(".ia-banner").addClass("ia-banner2");
                    $(".foot-nav").css("display", "block");
                }
                if (e.wheelDelta < 0) {
                    $('header').removeClass("header-fix");
                    $(".ia-left").removeClass("ia-left-fix2");
                    $(".lvwen-tit").removeClass("lvwen-tit2");
                    $(".ia-banner").removeClass("ia-banner2");
                    $(".foot-nav").css("display", "none");
                }
            } else if (e.detail) {
                if (e.detail > 0) {
                    $('header').removeClass("header-fix");
                    $(".ia-left").removeClass("ia-left-fix2");
                    $(".lvwen-tit").removeClass("lvwen-tit2");
                    $(".ia-banner").removeClass("ia-banner2");
                    $(".foot-nav").css("display", "none");
                }
                if (e.detail < 0) {
                    $('header').addClass("header-fix");
                    $(".ia-left").addClass("ia-left-fix2");
                    $(".lvwen-tit").addClass("lvwen-tit2");
                    $(".ia-banner").addClass("ia-banner2");
                    $(".foot-nav").css("display", "block");
                }
            }
        }

        if (document.addEventListener) {
            document.addEventListener('DOMMouseScroll', scrollFunc, false);
        }
        window.onmousewheel = document.onmousewheel = scrollFunc;


        $(window).scroll(function () {
            if ($(document).scrollTop() == 0) {
                $('header').addClass("header-fix2");
                $(".lvwen-tit").addClass("lvwen-tit3");
            } else {
                $('header').removeClass("header-fix2");
                $(".lvwen-tit").removeClass("lvwen-tit3");
            }
        });

    }

    $(function () {
        leftFix();
        lefts();
        topFix();
        $('html').css({ 'font-size': 100 / 640 * $(window).width() + 'px' });
        //赞
        $(".articleZanBtn").click(function () {
            postprise(this);
        });

        $(".articleZanBtn").live("hover", function () {
            $(this).toggleClass("zanHover");
        });
        $('.articleZanBtn').click(function () {
            $(this).addClass("zaned");
        });

        //收藏
        $('.ia-collect').live("click", function () {
            posttrans(this);
        });

        //评论
        $(".commentBtn").live("hover", function () {
            $(this).toggleClass("tihsOn");
        });
        $(".ltCommentList li").live("hover", function () {
            $(this).children(".commnLstL").toggleClass("atMeIcoOn");
        });

        $(window).resize(function () {
            relRead();
            lefts();
            $('html').css({ 'font-size': 100 / 640 * $(window).width() + 'px' });
            if ($(window).width() < 768) {
                $("#edi_del").hide();
            }
        });


        //图片延迟加载
        (function () {
            $("div.ia-youji,div.ia-text").find("img").lazyload({
                effect: "fadeIn",
                threshold: 200
            });

            //解决jquery-1.8.3.min.js中的懒加载和jquery.lazyload.js冲突的问题
            $(".idx-friend-bg").find('img[original]').each(function (n, m) {
                $(this).attr("src", $(this).attr("original"));
            });
        })();

        //更新文章浏览数，获取地域浏览数
        $.getJSON(pageViewUrl, function (data) {
            $("#rmfinum").html(data);
        })


        if ($("#ilist").length == 1) {
            var ids = $("#ilist").attr("deffer");
            $.getJSON("http://api.lotour.com/rss/pageviews.ashx?ids=" + ids + "&type=6&callback=?", function (data) {
                var result = data.result;
                $.each(result, function (i, val) {
                    var pv = val.pageview;
                    if (pv >= 10000) {
                        pv = Math.round(pv / 10000 * 10) / 10 + "万";
                    }
                    $("span.sn[deffer=" + val.id + "]").html(pv);
                });
            });
        }

        if ($(".ia-youji ul li").length == 6) {
            $(".ia-youji ul li").show();
        }

        //手机端评论、赞、返回js 20115-9-22  ------Start 
        //768分辨率 点赞
        $(".foot-nav-a2").click(function () {
            var obj = $(this);
            var prise = { "articalId": $("#appId").val(), "articalType": 1, "articalMemberId": $("#appMemberId").val() };
            jQuery.ajax({
                url: 'http://my.lotour.com/i/common/AddPrise/?t=' + Math.random(),
                type: "get",
                data: prise,
                cache: false,
                dataType: "jsonp",
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                success: function (result) {
                    var data = result.Result.Data;
                    var _this = $(obj);
                    if (data.flag == 1) {
                        var num = $(".ia-num-zan").html().replace(/[^0-9]/ig, "");
                        num = parseInt(num) + 1;
                        $(".ia-num-zan").html(num);
                        obj.find("span").text(num).show();
                        //点赞效果
                        obj.find("em").addClass("yidian");
                        obj.find("p").hide();
                        obj.find("em").show().animate({ "top": "-40px", "opacity": 0 }, 500);
                        obj.find("img").attr("src", "http://img1.lotour.net/i-article/foot-fix-2-on.png");
                    }
                    else if (data.flag == "2") {//已赞
                        //已点赞效果
                        obj.find("p").fadeIn().fadeOut(500);
                        obj.find("img").attr("src", "http://img1.lotour.net/i-article/foot-fix-2-on.png");
                    }
                    else {
                        alert("系统繁忙，请稍后重试！");
                    }
                },
                error: function () {
                    alert("系统繁忙，请稍后重试！");
                }
            });
        });

    });
    //手机端滑动效果
    function touchFix() {
        var beginY, moveY, moveYold;
        $(document).on('touchstart', function (e) {
            //event.preventDefault();
            if (event.targetTouches.length == 1) {
                var touch = event.targetTouches[0];
                beginY = touch.pageY;
                $(this).stop(true, false);
            }
        })
        $(document).on('touchmove', function (e) {
            //event.preventDefault();
            if (event.targetTouches.length == 1) {
                var touch = event.targetTouches[0];
                moveY = touch.pageY;
                if (beginY - moveY <= 0) {
                    $('.bottom-footer').addClass("foot-nav");
                } else if (beginY - moveY > 0) {
                    $('.bottom-footer').removeClass("foot-nav");
                }
            }
        })
    }
    touchFix();

    function goback() {
        var home = 'http://www.lotour.com';
        (document.referrer == '' || document.referrer == null || document.referrer.indexOf('www.lotour.com') > -1) ? window.location.href = home : window.history.go(-1);
    }

    function gocomment() {
        $('body,html').animate({ 'scrollTop': $("#commentsite").offset().top - 50 });
    }

    //手机端评论、赞、返回js 20115-9-22  ------End

    //微信分享
    var title = '延庆百里画廊2日全家游',
    desc = '我发现了一篇#乐途游记#《' + title + '》，为自己的视野猛开窗户这件事，可千万不能停！>>>',
    link = 'http://my.lotour.com/i/zuji/1021639',
    imgUrl = $("img[realsrc*=Photo]").first().attr("realsrc");

    //获取前5个picurl
    var picList = "";
    $("img[realsrc*=Photo]").slice(0, 5).each(function () {
        picList += $(this).attr("realsrc") + "|";
    });
    picList = picList.substring(0, picList.length - 1);
    // alert(picList);

    var share_title = {
        wb: "",
        db: "精彩游记---《" + title + "》",
        qz: "精彩游记---《" + title + "》",
        qt: ""
    };

