<script language="JavaScript" type="text/JavaScript">
function clockchartimagego()
    {
		if (window.XMLHttpRequest)
		  {// code for IE7+, Firefox, Chrome, Opera, Safari
		  xmlhttp=new XMLHttpRequest();
		  }
		else
		  {// code for IE6, IE5
		  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		  }
		xmlhttp.onreadystatechange=function()
		  {
		  if (xmlhttp.readyState==4 && xmlhttp.status==200)
			{
			document.getElementById("myImg").src=xmlhttp.responseText;
			}
		  }
		  
		var RightNow = new Date();
		var RightNowStr = "" + RightNow.getYear() + "-" + (RightNow.getMonth() + 1) + "-" + RightNow.getDate() + "-" + RightNow.getHours() + "-" + RightNow.getMinutes() + "-" + RightNow.getSeconds();
  
		xmlhttp.open("GET","ChartImage_Data.asp?id=" + RightNowStr,false);
		xmlhttp.send();  
    }
function clockchartimageclear()
	{
		clearInterval(clockchartimage);
		clearInterval(clockchartimagestop);
	}
	
clockchartimagego();


</script>          
 
                
