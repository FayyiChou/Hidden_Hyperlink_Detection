<script type='text/javascript'>
function mastermenu(id){
var i;
for (i=1;i<=13;i++) {
if (i == id) {
document.getElementById('mastermenu_' + i).style.display='block';
} else {
document.getElementById('mastermenu_' + i).style.display='none';
}
}
}
</script>

