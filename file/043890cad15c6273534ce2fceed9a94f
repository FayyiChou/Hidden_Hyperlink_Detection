<script language="javascript">


function winopen(url){
	var str = "網址： " + url + "\n\n將連結到其他網站，\n\n請問是否確定？";
	var str2 = "&nbsp;<br />網址： " + url + "&nbsp;<br />&nbsp;<br />將連結到其他網站，請問是否確定？&nbsp;<br />&nbsp;<br />";
	if (document.getElementById('div_popupbox') == null) {
		if (confirm(str))
		{
			window.open(url);
		}
	} else {
		location.href = "#";
		document.getElementById('popupbox_title').innerHTML = "連結外網警示";
		document.getElementById('popupbox_comment').innerHTML = str2;
		document.getElementById('popupbox_url').value = url;
		document.getElementById("div_popupbox").style.display = "block";
	}
}
</script>
