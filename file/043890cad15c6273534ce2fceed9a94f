        #headerbanner_pic .control {
                position: absolute;
                right: 4px;
                bottom: 1px;
                z-index:99;
        }
        #headerbanner_pic .control a {
                display: inline-block;
                margin: 0 3px;
                
                width:12px; height:20px;
				color:#fff;
				font-size:1 px;
