 <script type="text/JavaScript">		
	function jumpMenu(targ,selObj,restore){ 
	 eval(targ+".location='"+selObj.options[selObj.selectedIndex].value+"'");
	 if (restore) selObj.selectedIndex=0;
	}
</script>		
	
	<script type="text/javascript" src="/data/tosend/resource/template/jiangsubank/script/top.js"></script>
	<script type="text/javascript">
	  var url=window.location.toString();  
	      
      if(url.split("?").length!=1)
      {
		   var ary=url.split("?")[1];     
	       var details=decodeURI(ary.split("=")[1]);
	       var type=Number(details);      
		   $("#myTab0_Content"+type).css("display","block");
		   $("#content_list > div").each(function(index){
		     	if(index==type)
				 	{
				  	$("#myTab0_Content"+index).css("display","block");
				 	}
				  else
				  {
				  	$("#myTab0_Content"+index).css("display","none");
				  }
		   });
		   $("#myTab0 > a").each(function(index){
		      if(index==type)
				  {
				  $("#myTabb"+index).attr("class","active");
				  }
				  else
				  {
				  $("#myTabb"+index).css("class","normal");
				  }
		   });
	  }    	   
	  </script>
	  <!--如果二级栏目没有页面，则默认转到第一个三级栏目-->
	  <script type="text/javascript">
				window.onload = function doIt(){
					var secondchannelUrl = document.getElementById("secondchannelUrl").value;							
					var secondchannelSize = document.getElementById("secondchannelSize").value;			
					if(secondchannelSize !=0)
					window.location.href=secondchannelUrl;
				}
				/*干掉可忽略的错误*/
				function killErr(){
        return true;
			    }
			    window.onerror=killErr;
		</script>		

