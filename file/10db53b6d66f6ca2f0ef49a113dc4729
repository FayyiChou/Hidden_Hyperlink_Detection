<script type="text/JavaScript">
function getcontent(postid,usernick) {
	var userStatusValue = $("#userStateId").val();
	if(loginCheckedFn.isLogin()== false || null ==userStatusValue || userStatusValue.length==0 || userStatusValue== 3){
		loginAlert('ZUMA_TOPIC_NOTLOGIN_CITE','1');
		return;
	}
	var userNick = (usernick==null || usernick.length==0 || '匿名用户'==usernick)?'&nbsp;':'@'+usernick + '&nbsp;';
	clearmsg('div_seccode');
	var content = document.getElementById(postid).innerHTML;
  	content = content.replace(/_950\.(jpg|png|gif)/ig, '_140.$1');
  	content = content.replace(/width="?550"?/ig, '');
  	content = content.replace(/width:550px/ig, 'width:140px');
  	content = '<div class=\"commentBox\"><div class=\"co-author\">'+ usernick +'的原帖: </div>' + content +'</div><p class=\"co-content\">' + userNick + '</p>';
  	jQEditor.setHTML(content);
  	jQEditor.focus('last');
  	jQEditor.plugin('HdImage').resizeImageDivWidth();
}
$("table.jqe-table").each(function(i, el){
	if( $(el).width() > 550 ){
		$(el).css('width', '100%');
	}
});
</script>
