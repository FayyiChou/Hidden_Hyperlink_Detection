<script>
function chanEpRank() {
    var elem = document.getElementById("hotthreadpic"),
        _html = '',
        _title;
    if ( !elem ) { return false };
    if ( typeof(hotpic_json) != "object" ) { return false };

    for ( var i=0; i<5; i++ ) {
        if ( !hotpic_json[i] ) { break };
        _title = unescape(hotpic_json[i].title).replace("(图)", '<span class="title_blue">图</span>');
        _html += '<li><a href="'+ hotpic_json[i].url +'" target="_blank">'+ _title +'</a></li>';
    };
    elem.innerHTML = _html;
};
chanEpRank();
