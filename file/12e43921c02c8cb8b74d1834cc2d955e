var flag=true;
var curUrl=location.pathname.trim().toLowerCase();

      
if (curUrl == "/") {
	curUrl = "/home";
}      
      
$("li[name='navIndex']").each(function(){
	var _this = $(this);
	var _alias = _this.attr("alias").trim();
	if(_alias != null && (("/"+_alias).toLowerCase() == curUrl)){
		_this.addClass("on");
		flag=false;
		return;
	}
});    
 if(flag){
	var _pageData = window.parent.pageData;
	$("li[name='navIndex']").each(function(){
		var _this = $(this);
		var _alias = _this.attr("alias");
		if(_pageData){
			if(_alias != null && (_alias.toLowerCase() == _pageData.split('-')[0].toLowerCase() || (_alias.toLowerCase()=='article1' && _pageData.split('-')[0].toLowerCase()=='article'))){
				_this.addClass("on");
			}
		}
	})
} 
	
	

