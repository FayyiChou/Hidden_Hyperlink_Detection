<script>
var advList = 'adv_float_div=wap_adv_home_float_'.split(',');
function getDevicetype(){
	var ua = navigator.userAgent.toLowerCase();
	var os_type="other";
	if(ua.indexOf("android") > -1 || ua.indexOf("linux") > -1){
		os_type="android";
	}else if(!!ua.match(/\(i[^;]+;( U;)? cpu.+mac os x/)){
		os_type="ios";
	}else{
	}
	return os_type;
}
var os_type=getDevicetype();
var l = ['CN11','CN31','CN4401','CN4403'];
var c = document.cookie;
for (var i = 0; i < l.length; ++i) {
	if(new RegExp('IPLOC='+l[i]).test(c)) {
		advList = [];break;
	}
}
for (var i = 0; i < advList.length; ++i) {
	var tmp = advList[i].split('=');
	$('#'+tmp[0]).load('/adv/'+tmp[1]+os_type+'.html').show();
}
</script>
