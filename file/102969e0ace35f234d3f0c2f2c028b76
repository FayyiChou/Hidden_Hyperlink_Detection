<script type="text/javascript">
    function address(type) {
        var s = escape(type);
        location.href = "/website/NewsList.aspx?newtype=" + s;
    }

    $(function () {
        loadMenu(null);
    })

    function loadMenu(pid) {
        $.ajax({
            url: '/website/usercontrol/AjaxPostBack.ashx',
            async: false,
            dataType: "json",
            type: "POST",
            data: {
                Action: "LoadMenu",
                Parent: pid == null ? "" : pid
            },
            success: function(data) {
                var htm = "";
                for (var i = 0; i < data.length; i++) {
                    htm += "<li class='menulevel' id='menulevel_" + (i + 1) + "' pid='" + data[i].id + "'><a href='" + data[i].link + "'>" + data[i].name + "</a><ul id='sl_" + (i + 1) + "' class='sub_nav_01' style='display:none;'>";
                    for (var j = 0; j < data[i].submenu.length; j++) {
                        htm += "<li><a href='" + data[i].submenu[j].link + "' title='" + data[i].submenu[j].name + "'>" + data[i].submenu[j].name + "</a></li>";
                    }
                    htm += "</ul></li>";
                }
                $("#nav").html(htm);
                $(".menulevel").each(function() {
                    var pid = $(this).attr("pid");
                    var index = $(this).attr("id").split('_')[1];
                    var child = "sl_" + index;
                    var left_span = $(this).offset().left;
                    var left_ul = left_span;
                    var num = $(".menulevel").index($(this));
                    if ($(".menulevel").length - num <= 3) {
                        var navright = $("#nav").width() + $("#nav").offset().left;
                        left_ul = navright - 82 * $("#" + child + " li").length;
                    }
                    else {
                        left_ul -= 60;
                    }
                    $(this).bind("mouseover", function() {
                        $(".menulevel ul").hide();
                        $(".Triangle_con").hide();
                        if ($("#" + child + " li").length > 0) {
                            $(".Triangle_con").css({ "left": left_span + 35 + "px", "top": "70px" }).show();
                        }
                        $("#" + child).css("top", "78px").css("left", left_ul + "px");
                        $("#" + child).show();
                    });
                });
                $(".menulevel ul").bind("mouseleave", function() { $(this).hide(); $(".Triangle_con").hide(); });
            }
        });
    }
</script>
