    //退出用户登录
    function loginOutAccount() {
        AutohomeJsLoad({
            url: '//account.che168.com/login/logoutjson?backvar=cheloginout&timestamp=' + Math.random(),
            callBack: function () {
            }
        });
        AutohomeJsLoad({
            url: '//account.autohome.com.cn/login/logoutjson?backvar=autologinout&timestamp=' + Math.random(),
            callBack: function () {
                window.location.reload();
            }
        });
    };


    function _login(name, pwd,callback,isdialog,objTextareaId) {
        var isTrue = 'false';
        jQuery.ajax({
            type: "GET",
            cache: false,
            url: "//shuoke.autohome.com.cn/ashx/user/ValidateAccount.ashx",
            data: { logincode: escape(name.replace(/\+/gi, '%2B')), pwd: hex_md5(pwd), validcode: "",opertype: "login"},
            dataType: "json",
            success: function (data) {
                var json = data;
                if (json.returncode != "0") {
                    if (json.returncode == "2010202") {
                        alert('您的账户和密码不匹配，为了您的账户安全，请到登录页重新登录。');
                        window.location = "//account.autohome.com.cn/login/";
                        return false;
                    }
                    alert('账号或密码错误');
                    return false;
                } else {
                    if (!data.result || !data.result.PcpopClub) {
                        alert('账号或密码错误');
                        return false;
                    }
                    AutohomeJsLoad({
                        url: "//account.che168.com/login/OtherLoginTemp?pc=" + data.result.PcpopClub,
                        callBack: function() {}
                    });
                    AutohomeJsLoad({
                        url: "//account.autohome.com.cn/login/OtherLoginTemp?pc=" + data.result.PcpopClub,
                        callBack: function() {
                            var name = getUNcookie() == true ? cookieUN : "";
                            autoHeaderObj = new AutoHeader(); //头部登录
                            jQuery("#reply-login").html("欢迎您，<span class=\"red\">" + name + "</span> | <a target=\"_self\"  href=\"javascript:loginOutAccount();\">退出登录</a> ")
                                .show()
                                .addClass('post-comment-user');
                            jQuery("#reply-nologin").hide();
                            jQuery('.user-comment-list-post .post-comment-login').html(name+' | <a href="javascript:loginOutAccount();" target="_self">退出登录</a> ');
                            if (typeof ModifyRegisterUrl == "function") {
                                ModifyRegisterUrl();
                            }
                            if (!isdialog) {
                                callback();
                                return true;
                            } else {
                                var userstatus = AutoShuokeUser.getUserStatus();
                                if (userstatus > 0) {
                                    AutoBlogReply.showBindinfoTip(userstatus, jQuery("#" + objTextareaId));
                                } else {
                                    callback();
                                    return true;
                                }
                            }
                        }
                    });
                }
            },
            error: function (err) {
                alert(err);
                //return isTrue;
            }
        });
        return isTrue;
    }

    //顶
    AutoBlogReply["up"] = function (replyid, obj) {
        jQuery(obj).attr("class", "link999");
        jQuery(obj).attr("onclick", "");

        setTimeout(function () {
            jQuery.ajax({
                url: '//reply.autohome.com.cn/ReceiveRequest/upcomment.ashx?appid=7&replyid=' + replyid + '&datatype=jsonp',
                dataType: "jsonp",
                jsonp: "callback",
                jsonpCallback: "jsonpCallback",
                success: function (data) {
                    if (data.Result == "true") {
                        jQuery(obj).children(':first').text("(" + (parseInt(jQuery(obj).children(':first').text().replace("(", "").replace(")", "")) + 1) + ")");
                        jQuery(obj).parent().find("em.plus").animate({
                            marginTop: '-30px',
                            opacity: 'show'
                        }, 1000).animate({ opacity: 'hide' }, 300);
                    } else {
                        alert(data.errorMessage);
                    }
                }
            });
        }, 0);
    };

    AutoBlogReply["getComments"] = function(pageIndex, count) {
        count = count || 10;
        var html = "", memberIds = [];
        jQuery.ajax({
            url: '//reply.autohome.com.cn/api/comments/shownew.json?appid=7&id=573062&page=' + pageIndex + '&count=' + count + '&orderfield=top&t=' + Math.random() + '&datatype=jsonp&',
            dataType: "jsonp",
            jsonp: "callback",
            jsonpCallback: "jsonpCallback",
            success: function(data) {
                if (!data || !data.commentlist || data.commentlist.length == 0) {
                    return;
                }
                jQuery("#reply-list").show();
                jQuery("span[name='reply-count']").text(data.commentcount);

                //少于10条或者大于两页不显示再加载
                if (data.commentlist.length < count || pageIndex >= 3 || count >= 30) {
                    jQuery("#reply-more-box").hide();
                    jQuery("#reply-more-bottom").show();
                } else {
                    jQuery("#reply-more-box").show();
                    jQuery("#reply-more-bottom").hide();
                }

                jQuery.each(data.commentlist, function(index, item) {
                    var commentItemHtml=AutoBlogReply.getCommentItemHtml(index,item);
                    html+=commentItemHtml;
                    if (!memberIds || jQuery.inArray(item.RMemberId, memberIds) == -1) {
                        memberIds.push(item.RMemberId);
                    }
                });
                if (pageIndex == 1) {
                    jQuery('#reply-list').html(html);
                } else {
                    jQuery('#reply-list').append(html);
                }
                if (autoHeaderObj.user.isLogin) {
                    commentTop.addClickEvent();
                }
                commentTop.addCommentTipEvent();

                AutoBlogReply.showAuthorCommentTip(memberIds, (pageIndex - 1) * count);
            },
            error: function() {

            }
        });
    };

    AutoBlogReply["getCommentTopIcon"] = function(index, topStatus, topType) {
        if (!topStatus) {
            return '';
        }
        var txtTop = '', spanTop = '';
        if (topType == 2) {
            txtTop = '<span class="pop-vie pop-vie-org">有观点</span>';
        } else if (topType == 1) {
            txtTop = '<span class="pop-vie pop-vie-green">作者赞</span>';
        }
        if (txtTop) {
            if (index == 0) {
                spanTop = '<span class="pop-vie-box pop-vie-nobor">' + txtTop + '</span>';
            } else {
                spanTop = '<span class="pop-vie-box">' + txtTop + '</span>';
            }
        }
        return spanTop;
    };
    AutoBlogReply["getUserSource"] = function(sptype) {
        var type = '';
        if (!sptype) {
            return '';
        }
        var sptypeLower = sptype.toLowerCase();

        if (sptypeLower.indexOf("iphone") > -1) {
            type += '<span class="grey">来自 <a href="//www.autohome.com.cn/apps/1.html">' + sptype + '</a></span>';
        } else if (sptypeLower.indexOf("android") > -1) {
            type += '<span class="grey">来自 <a href="//www.autohome.com.cn/apps/3.html">' + sptype + '</a></span>';
        } else if (sptypeLower.indexOf("ipad") > -1) {
            type += '<span class="grey">来自<a href="//www.autohome.com.cn/apps/2.html">' + sptype + '</a></span>';
        } else if (sptypeLower.replace(" ", "").indexOf("windowsphone") > -1) {
            type += '<span class="grey">来自<a href="//www.autohome.com.cn/apps/8.html">' + sptype + '</a></span>';
        } else {
            type += '<span class="grey">来自<a href="//www.autohome.com.cn/apps/m">' + sptype + '</a></span>';
        }
        return type;
    };
    //作者评论加标注
    AutoBlogReply["showAuthorCommentTip"] = function(memberIds, start) {
        if (!memberIds) {
            return;
        }
        jQuery.get("//shuoke.autohome.com.cn/ashx/user/checkisauthor.ashx?v=1.1&opertype=checkids&ids=" + memberIds, function(data) {
            result = eval("(" + data + ")");
            if (result && result.status == 0 && result.body.authors != undefined) {
                for (var i = 0; i < result.body.authors.length; i++) {
                    var item = result.body.authors[i];
                    if (item.Name) {
                        var span = jQuery("a[name='member']:not(:lt(" + start + "))[memberid=" + item.Id + "]");
                        var authorLink = '';
                        if (item.Id == '17024564') {
                            authorLink = jQuery('<a target="_blank" href="//shuoke.autohome.com.cn/author/' + item.Id + '.html" class="comment-popup-sm comment-popup-blue">\
                                <div class="pop-arrow-sm">\
                                    <span class="arrow-bg-sm">◆</span>\
                                </div>\
                                <div class="pop-content-sm">本文作者:' + item.Name + '</div></a>');
                        } else {
                            authorLink = jQuery('<a target="_blank" href="//shuoke.autohome.com.cn/author/' + item.Id + '.html" class="comment-popup-sm comment-popup-gray">\
                                <div class="pop-arrow-sm">\
                                    <span class="arrow-bg-sm">◆</span>\
                                </div>\
                                <div class="pop-content-sm">说客作者:' + item.Name + '</div></a>');
                        }
                        authorLink.insertAfter(span);
                    }
                }
            }
        });
    };

    //查看对话
    AutoBlogReply["getdialogue"] = function(replyid) {
        jQuery.ajax({
            url: '//reply.autohome.com.cn/ReceiveRequest/GetDialogue.ashx?appid=7&replyid=' + replyid + '&datatype=jsonp',
            dataType: "jsonp",
            jsonp: "callback",
            jsonpCallback: "jsonpCallback",
            success: function(data) {
                if (data == null) {
                    return;
                }
                var html = ' <dl class="user-comment-list">';
                jQuery.each(data, function(i, v) {
                    var userLink = '';
                    if (v["rmemberid"] == getUNcookie() == true ? cookieUID : "") {
                        userLink += '<a class="red" href="//i.autohome.com.cn/' + v["rmemberid"] + '/home.html">' + v["rmembername"] + '</a>';
                    } else {
                        userLink += '<a href="//i.autohome.com.cn/' + v["rmemberid"] + '/home.html">' + v["rmembername"] + '</a>';
                    }

                    if (i > 0) {
                        if (v["rtargetmemberid"] == getUNcookie() == true ? cookieUID : "") {
                            userLink += ' 回复 <a class="red" href="//i.autohome.com.cn/' + v["rtargetmemberid"] + '/home.html">' + v["targetname"] + '</a>';
                        } else {
                            userLink += ' 回复 <a href="//i.autohome.com.cn/' + v["rtargetmemberid"] + '/home.html">' + v["targetname"] + '</a>';
                        }
                    }

                    html += '<dt><em>' + (i + 1) + '</em></dt><dd>\
                            <div class="dialog-box-userbar"><span class="fn-left">' + userLink + '</span><span class="fn-right">' + v["replydate"] + '</span></div>\
                            <p>' + v["rcontent"] + '</p>\
                            <div class="text-right"><a hidefocus="true" target="_self" onclick="AutoBlogReply.jubao('+ v["replyid"] +','+v["rmemberid"]+',\''+v["rmembername"]+'\')" href="javascript:void(0)">举报</a> | <span class="pos-plus"><em class="plus" style="display:none;">+1</em><a hideFocus="true" target="_self" onclick="AutoBlogReply.up(' + v["replyid"] + ',this)" href="javascript:void(0)">顶<span class="grey">(' + v["rup"] + ')</span></a></span> | <a hideFocus="true" target="_self" href="javascript:void(0)" onclick=AutoBlogReply.PostDiv(1,this,' + v["replyid"] + ',null,1)>回复</a></div>\
                        </dd>';
                });

                html += '<dt class="dislog-arrow"></dt><dd class="dislog-line"></dd></dl>';
                jQuery("#dialogBox").html(html);
                popdialog.show("dhbox", function(b) {
                    var obj = document.getElementById("dhbox");
                    if (b.isIpad && obj.className.indexOf('dialogipad') == -1) {
                        obj.className += ' dialogipad';
                    }
                });
            }
        });
    };
    //回复框
    AutoBlogReply["PostDiv"] = function(isShow, obj, replyid, width, isdialog) {
        isdialog = isdialog || 0;
        var par = jQuery(obj).parents("dd");
        var dialogin = jQuery(par).find('.user-comment-list-post');
        if (dialogin.length > 0) {
            jQuery(dialogin).remove();
            return;
        } else {
            jQuery(this.lastopendiv).find('.user-comment-list-post').remove();
            var loginhtml = "";

            if (autoHeaderObj.user.isLogin) {
                var name = autoHeaderObj.user.un == null ? "" : autoHeaderObj.user.un;
                loginhtml = '<div class="post-comment-login">' + name + ' | <a target=\"_self\"  href="javascript:loginOutAccount();">退出登录</a> </div>';
            } else {
                loginhtml = '<div class="post-comment-login">\
                                    <div class="fn-left">用户名<input id="txtname" type="text" class="form-text" value="">密码<input id="txtword" onkeydown="AutoBlogReply.Message.keydown(event,this,' + isdialog + ')" type="password" class="form-text"><a target="_self" onclick="AutoBlogReply.Message.login(this,' + isdialog + ');return false;" href="javascript:void(0);" class="btn btn-small">登 录</a></div>\
                                    <div class="fn-right"><a href="//account.autohome.com.cn/password/find">忘记密码？</a> | <a href="//account.autohome.com.cn/register?rt=1">注册</a></div>\
                                </div>';
            }
            html = ' <div class="user-comment-list-post">' + loginhtml + '<div class="textareabox">\
                                <textarea id="textarea' + replyid + '" data-toggle="textarea" data-length="20"  class="form-textarea">' + AutoBlogReply["defaultMessage"] + '</textarea>\
                               <div class="textareabox-submitbar"><span class="fn-left" id="reply-item-content-length">0/5000字</span><span class="fn-right"><a href="javascript:void(0);" target="_self" id="btn' + replyid + '"  class="btn btn-default btn-disabled">提 交</a></span></div>\
                             </div>\
                        </div>';
            par.append(html);

            this.lastopendiv = par;

            var content = jQuery("#textarea" + replyid);
            autoTextarea(content[0], 0, 150);
            content.focus(function() {
                if (!isdialog) {
                    AutoShuokeUser.checkUserStatus();
                }
                if (jQuery(this).text() == AutoBlogReply["defaultMessage"]) {
                    jQuery(this).text("");
                    jQuery(this).css("color", "black").addClass("form-textarea-active");
                }
            })
                .blur(function() {
                    if (jQuery(this).text() == "") {
                        jQuery(this).text(AutoBlogReply["defaultMessage"]);;
                        jQuery(this).css("color", "#CCCCCC").removeClass("form-textarea-active");
                    }
                })
                .keydown(function(evt) {
                    evt = evt ? evt : (window.event ? window.event : null);
                    if (13 == evt.keyCode && evt.ctrlKey) {
                        evt.returnValue = false;
                        evt.cancel = true;
                        jQuery(par).find("a.btn.btn-default").click();
                    }
                })
                .keyup(function() {
                    var maxChars = 5000;
                    var which = jQuery(this)[0];
                    if (which.value.length > maxChars)
                        which.value = which.value.substring(0, maxChars);
                    var curr = maxChars - which.value.length;
                    jQuery("#reply-item-content-length").html((5000 - curr) + "/5000字");
                    if (which.value.length == 0) {
                        jQuery("#btn" + replyid).addClass("btn-disabled").removeClass("btn-blue").unbind();
                    } else {
                        jQuery("#btn" + replyid)
                            .addClass("btn-blue").removeClass("btn-disabled")
                            .unbind().click(function() {
                                AutoBlogReply.Comment(replyid, isdialog);
                                return false;
                            });
                    }
                });

            if (autoHeaderObj.user.isLogin) {
                content.focus();
            } else {
                jQuery("#txtname").focus();
            }

            var userstatus = AutoShuokeUser.getUserStatus();
            if (userstatus > 0 && isdialog) {
                AutoBlogReply.showBindinfoTip(userstatus, content);
            }
        }
    };
    AutoBlogReply["reload"] = function() {
        loadCount = 1;
        AutoBlogReply.init();
    };
    AutoBlogReply["Comment"] = function (id,isdialog) {
        var userName = '', userPwd = '';
        var textarea = AutoBlogReply.$("textarea" + id);
        if (!autoHeaderObj.user.isLogin) {
            if (id == 0) {
                userName = AutoBlogReply.$("txtusername").value;
                userPwd = AutoBlogReply.$("txtpassword").value;
                if (userName == "") {
                    alert("请输入用户名");
                    AutoBlogReply.$("txtusername").focus();
                    return;
                }
                else if (userPwd == "") {
                    alert("请输入密码");
                    AutoBlogReply.$("txtpassword").focus();
                    return;
                }
            }
            else {
                userName = AutoBlogReply.$("txtname").value;
                userPwd = AutoBlogReply.$("txtword").value;
                if (userName == "") {
                    alert("请输入用户名");
                    AutoBlogReply.$("txtname").focus();
                    return;
                }
                else if (userPwd == "") {
                    alert("请输入密码");
                    AutoBlogReply.$("txtword").focus();
                    return;
                }
            }
        }

        function PostData() {
            if (textarea.value == AutoBlogReply["defaultMessage"] || textarea.value.replace(/ /gi, "").replace(/\r/gi, "").replace(/\n/gi, "") == "") {
                alert("评论内容不能为空！");
                textarea.focus();
                return;
            }
            if (textarea.value.length > 5000) {
                alert("评论内容不能超过5000字");
                return;
            }
                
            if (!isdialog) {//查看对话不检测
                var validate=AutoShuokeUser.checkUserStatus();
                if (!validate)
                    return;
            }
            var btnSubmit = AutoBlogReply.$("btn" + id);
            var submitevent;
            if (btnSubmit != null) {
                submitevent = btnSubmit.onclick;
                btnSubmit.onclick = "";
            }
            jQuery.post("//shuoke.autohome.com.cn/ashx/ajaxPostComment.ashx?v1.1", {
                objid: '573062',
                txtcontent: escape(textarea.value.replace(/\n/g, '<br>')),
                backurl: document.URL,
                TargetReplyId: id,
                name: escape(userName),
                pwd: userPwd
            }, function(data) {
                if (data == null) {
                    alert('评论失败');
                    setTimeout(function() { window.location.href = backOrUrl; }, 1000);
                    return;
                }
                var flag = eval("("+data+")");
                if (flag == null) {
                    alert('评论失败');
                    setTimeout(function() { window.location.href = backOrUrl; }, 1000);
                    return;
                }
                if (flag.returncode == 0) {
                    setTimeout(function() {
                        if(isdialog||id==0){
                            AutoBlogReply.init();
                        }else{
                            var par = jQuery("#btn"+id).parents("dd");
                            var dialogin = par.find('.user-comment-list-post');
                            if (dialogin.length > 0) {
                                jQuery(dialogin).remove();
                            }
                            var commentItemHtml=AutoBlogReply.getCommentItemHtml(0,flag.result);
                            jQuery("dd[data-id='"+id+"']").after(commentItemHtml);
                        }
                        textarea.value = "汽车之家温馨提示您：留言中请不要恶意攻击国家、其他用户及工作人员，不要发布任何广告性质的留言，我们会在第一时间永久封杀违反以上规定的ID。流畅沟通、观点鲜明、善意提醒是我们推崇的良性留言氛围。";
                        textarea.style.color = "rgb(204, 204, 204)";
                        textarea.blur();
                        if (id == 0) {
                            jQuery("#chklen").text("0/5000字");
                        } else {
                            jQuery("#reply-item-content-length").text("0/5000字");
                        }
                        jQuery(("#btn" + id)).unbind().addClass("btn-disabled").removeClass("btn-blue");
                    }, 800);
                    if (isdialog) {
                        popdialog.hide();
                        return false;
                    }
                } else if (flag.returncode == 124&&!isdialog) {
                    skPopmobilebind.init();
                    btnSubmit.onclick = submitevent;
                } else if (flag.returncode == 125&&!isdialog) {
                    skPopUpNickName.init();
                    btnSubmit.onclick = submitevent;
                } else {
                    alert(flag.message);
                    btnSubmit.onclick = submitevent;
                }
            });
            }

        function delegate() {
            if (!autoHeaderObj.user.isLogin) {
                _login(userName, userPwd, PostData,isdialog,"textarea" + id);
            } else {
                PostData();
            }
        }
        setTimeout(delegate, 0);
    };

        AutoBlogReply["getCommentItemHtml"]=function(index,item){
            var sexClass = "icon-userman";
            if (item.RMemberSex == 1) {
                sexClass = "icon-userwoman";
            }

            var topStatus = item.TopStatus || 0;
            var topType = item.TopType || 0;
            var btnCommentTop = '';

            if (topStatus == 1) {
                if (topType == 1) {
                    btnCommentTop = '<span class="praise-icon-box" style="display:none"><a href="javascript:void(0);" name="btnAuthorUp" target="_self" data="1">取消赞</a><i class="icon-praise"></i> |</span> ';
                } else if (topType == 2) {
                    btnCommentTop = '<a href="javascript:void(0);" name="btnAdminUp" style="display:none" target="_self" data="1">取消置顶</a><span style="display:none" name="btnAdminUp"> | </span>';
                }
            } else {
                btnCommentTop = '<a href="javascript:void(0);" name="btnAdminUp" style="display:none" target="_self" data="0">置顶</a><span name="btnAdminUp" style="display:none"> | </span>';
                btnCommentTop += '<span class="praise-icon-box" style="display:none" ><a href="javascript:void(0);" name="btnAuthorUp" target="_self" data="0">赞这条评论</a><i class="icon-praise"></i> |</span> ';
            }

            var html = ' <dt>\
                       ' + AutoBlogReply.getCommentTopIcon(index, topStatus, topType) + '\
                       <span class="fn-left"><a name="member" memberid=' + item.RMemberId + ' class="user-fl" href="//i.autohome.com.cn/' + item.RMemberId + '/home.html"><i class="' + sexClass + '"></i>' + item.RMemberName + '</a>\
                       ' + AutoBlogReply.getUserSource(item.SpType) + '</span>\
                       <span class="fn-right">' + item.replydate + ' [<span class="red">' + item.RFloor + '楼</span>]</span></dt>\
                       <dd data-id="' + item.ReplyId + '">\
                       <p>' + item.RContent + '</p>';

            if (item.RTargetReplyId > 0) {
                html += '<div class="reply">';
                if (item.Quote != null) {
                    html += ' <p><span class="grey">原评论</span> <a class="grey666" href="//i.autohome.com.cn/' + item.Quote.RMemberId + '/home.html">' + item["Quote"]["RMemberName"] + '</a> <span class="grey">于</span>' + timeFormat(item["Quote"]["RReplyDate"]) + '<span class="grey">发表在</span> <span class="grey666">' + item["Quote"]["RFloor"] + '楼</span></p>\
                                      <p>' + item["Quote"]["RContent"] + '</p>';
                } else {
                    html += ' <p>原帖已删除</p>';
                }
                html += '</div>';
            }

            var dialog = '';
            if (item.Duihua > 1)
                dialog = '<a target="_self" hideFocus="true" href="javascript:void(0);"onclick="AutoBlogReply.getdialogue(' + item["ReplyId"] + ');return false;">查看对话</a> | ';

            html += ' <div class="text-right"><a hidefocus="true" target="_self" onclick="AutoBlogReply.jubao('+ item["ReplyId"] +')" href="javascript:void(0)">举报</a> | ' + btnCommentTop + dialog + '<span class="pos-plus"><em style="display:none" class="plus">+1</em><a hideFocus="true" target="_self" onclick="AutoBlogReply.up(' + item["ReplyId"] + ',this)" href="javascript:void(0)">顶<span class="grey">(' + item.RUp + ')</span></a></span> | <a hideFocus="true" target="_self" onclick=AutoBlogReply.PostDiv(1,this,' + item["ReplyId"] + ',"width:586px")  href="javascript:void(0)">回复</a></div></dd>';
            return html;
        }

        //查看对话完善信息
        var hasBindinfo = false,intervalBindinfo = null;
        AutoBlogReply["showBindinfoTip"] = function(userstatus,objTextarea) {
            if(userstatus<=0){
                return;
            }
            var tipword = "";
            if (userstatus == 1) {
                tipword = "需要绑定您的手机后才能继续操作";
            } else if (userstatus == 2) {
                tipword = "抱歉，您需要先完善个人信息";
            }
            var objTipBindinfo = jQuery(' <a id="tipBindInfo" target="_blank" href="//i.autohome.com.cn/setting/index" style="display:none;" class="reda" ></a>');
            objTipBindinfo.click(function() {
                if (intervalBindinfo == null) {
                    intervalBindinfo = setInterval(function() {
                        hasBindinfo= AutoBlogReply["hideBindinfoTip"]();
                        if (hasBindinfo) {
                            clearInterval(intervalBindinfo);
                            intervalBindinfo = null;
                        }
                    }, 2000);
                }
            });
            jQuery('.user-comment-list-post .post-comment-login').append(objTipBindinfo);
            jQuery("#tipBindInfo")
                .html(tipword)
                .show();
            objTextarea.css("color", "#CCCCCC")
                .removeClass("form-textarea-active")
                .attr("disabled", "disabled");
        };
        AutoBlogReply["hideBindinfoTip"] = function() {
            var userstatus = AutoShuokeUser.getUserStatus();
            if (userstatus == 0) {
                jQuery("#tipBindInfo").hide();
                jQuery("textarea")
                    .removeAttr("disabled")
                    .css("color", "black")
                    .addClass("form-textarea-active")
                    .focus();
            }
            return userstatus <= 0;
        };
        AutoBlogReply["jubao"] = function(replyId,memberId,memberName) {
            var $objMember = jQuery("dd[data-id='"+replyId+"']").prev();
            memberId = memberId || $objMember.find("a[name='member']").attr("memberid");
            memberName = memberName || $objMember.find("a[name='member']").text();
            window.open('//jubao.autohome.com.cn/ComplaintMain/CreateCom?cfgId=15&appId=1000&objId='+replyId+'&subObjId=7&BizShowName='+encodeURIComponent('举报')+'&AccusedMId='+memberId+'&AccuseMName='+encodeURIComponent(memberName)+'&url='+encodeURIComponent(backOrUrl));
        };

        AutoBlogReply.reload();
        autoHeaderObj.logoffFunctionArray.push(AutoBlogReply.Message.logoff);

        /**
        * 文本框根据输入内容自适应高度
        * @param		{HTMLElement}	输入框元素
        * @param		{Number}		设置光标与输入框保持的距离
        * @param		{Number}		设置最大高度(可选)
        */
        var autoTextarea = function (elem, extra, maxHeight) {
            extra = extra !== undefined ? extra : 0;
            var isFirefox = !!document.getBoxObjectFor || 'mozInnerScreenX' in window,
                isOpera = !!window.opera && !!window.opera.toString().indexOf('Opera'),
                addEvent = function (type, callback) {
                    elem.addEventListener ?
                        elem.addEventListener(type, callback, false) :
                        elem.attachEvent('on' + type, callback);
                },
                getStyle = elem.currentStyle ? function (name) {
                    var val = elem.currentStyle[name];

                    if (name === 'height' && val.search(/px/i) !== 1) {
                        var rect = elem.getBoundingClientRect();
                        return rect.bottom - rect.top -
                            parseFloat(getStyle('paddingTop')) -
                            parseFloat(getStyle('paddingBottom')) + 'px';
                    };

                    return val;
                } : function (name) {
                    return getComputedStyle(elem, null)[name];
                },
                minHeight = parseFloat(getStyle('height'));

            elem.style.maxHeight = elem.style.resize = 'none';

            var change = function () {
                var scrollTop, height,
                    padding = 0,
                    style = elem.style;

                if (elem._length === elem.value.length) return;
                elem._length = elem.value.length;

                if (!isFirefox && !isOpera) {
                    padding = parseInt(getStyle('paddingTop')) + parseInt(getStyle('paddingBottom'));
                };

                elem.style.height = minHeight + 'px';
                if (elem.scrollHeight > minHeight) {
                    if (maxHeight && elem.scrollHeight > maxHeight) {
                        height = maxHeight - padding;
                        style.overflowY = 'auto';
                    } else {
                        height = elem.scrollHeight - padding;
                        style.overflowY = 'hidden';
                    };
                    style.height = height + extra + 'px';
                    elem.currHeight = parseInt(style.height);
                };
            };

            addEvent('propertychange', change);
            addEvent('input', change);
            addEvent('focus', change);
            addEvent('keyup', change);
            change();
        };

        var text = document.getElementById("textarea0");
        if (text != null) {
            autoTextarea(text, 0, 150); // 调用
        }
</script>

