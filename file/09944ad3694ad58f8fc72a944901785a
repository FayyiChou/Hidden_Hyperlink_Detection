    (function LimitHeight() {
        var tags = document.getElementById("tags");
        var width = tags.offsetHeight;
        if (width > 164)
            tags.style.height = "164px";
    })();
</script>
<script type="text/javascript">
    //为标签添加样式
    function AddTagStyle() {
        var div = document.getElementById("tags");
        if (div) {
            if (div.childNodes.length == 0) {
                return;
            }
            for (var i = 0, k = 0; i < div.childNodes.length; i++) {
                var a = div.childNodes[i];
                if (a.nodeName == "A") {
                    a.className = "tag-color" + (k % 5 + 1);
                    k++;
                }
            }
        }
    }
    AddTagStyle();
</script>
