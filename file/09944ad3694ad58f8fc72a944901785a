                            url: "//account.che168.com/login/OtherLoginTemp?pc=" + data.result.PcpopClub,
                            callBack: function (){}
                        });
                        return true;
                    }
                },
                error: function (err) {
                    console.log(err);
                },
                complete: function (json) {
                    //AutoBlogReply.reload();
                    //jQuery(loginbar).html("欢迎您，" + json.name + " | <a href='javascript:loginOutAccount();' target='_self'>退出登录</a>");
