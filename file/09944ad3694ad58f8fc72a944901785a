        click: function (obj, id) {
            if (!autoHeaderObj.user.isLogin) {
                alert("请您先登录");
                return; 
            }
            var M = AutoBlogReply["Message"];
            var itemKey = "fastreply_" + id;
            var lastItmeKey = "fastreply_" + M.LastId;
            if (M.LastId == id) {
                if (M.IsOpen) {
                    M.IsOpen = false;
                    M.Item[itemKey].obj.style.display = "";
                    M.Item[itemKey].Message.style.display = "none";
                }
                else {
                    M.IsOpen = true;
                    M.Item[itemKey].obj.style.display = "none";
                    M.Item[itemKey].Message.style.display = "";
                }
            }
            else {
                if (!M.Item[itemKey]) {
                    var div = M.createObj(id);
                    obj.style.display = "none";
                    obj.parentNode.appendChild(div);
                    M.Item[itemKey] = { "Message": div, "obj": obj };
                } else {
                    M.Item[itemKey].obj.style.display = "none";
                    M.Item[itemKey].Message.style.display = "";
                }
                if (M.LastId != null && M.Item[lastItmeKey]) {
                    M.Item[lastItmeKey].obj.style.display = "";
                    M.Item[lastItmeKey].Message.style.display = "none";
                }
                M.IsOpen = true;
                M.LastId = id;
            }
        },
        keydown: function (e, obj,isdialog) {
            e = e ? e : window.event;
            if (13 == e.keyCode) {
                e.returnValue = false;
                e.cancel = true;
                AutoBlogReply.Message.login(obj,isdialog);
            }
        },
        onkeydown: function (e, id) {
            e = e ? e : window.event;
            if (13 == e.keyCode && e.ctrlKey) {
                e.returnValue = false;
                e.cancel = true;
                AutoBlogReply["Comment"](id);
            }
        },
        onkeyup: function (which) {
            var maxChars = 5000;
            if (which.value.length > maxChars)
                which.value = which.value.substring(0, maxChars);
            var curr = maxChars - which.value.length;
            AutoBlogReply.$("chklen").innerHTML = (5000 - curr) + "/5000字";
            if (which.value.length == 0) {
                jQuery("#btn0").addClass("btn-disabled").removeClass("btn-blue").unbind();
            } else {
                jQuery("#btn0")
                    .addClass("btn-blue").removeClass("btn-disabled")
                    .unbind().click(function() {
                        AutoBlogReply.Comment(0);
                    });
            }
        },
        onfocus: function (obj) {
            AutoShuokeUser.checkUserStatus();
            if (obj.value == AutoBlogReply["defaultMessage"] || obj.value == "") {
                obj.value = '';
                obj.style.color = "black";
            }
            jQuery(obj).addClass("form-textarea-active");
        },
        onblur: function (obj) {
            if (obj.value == "" || obj.value == AutoBlogReply["defaultMessage"]) {
                obj.style.color = "#cccccc";
                obj.value = AutoBlogReply["defaultMessage"];
            }
            jQuery(obj).removeClass("form-textarea-active");
        },
        reset: function (id) {
            var textarea = AutoBlogReply.$("textarea" + id);
            if (textarea) {
                textarea.value = "";
                textarea.focus();
            }
        },
        createObj: function (id) {
            var div = document.createElement("div");
            div.className = "replyarea";
            div.id = "fastreply_" + id;
            div.innerHTML = AutoBlogReply.Template["replybox"].replace(/{replyid}/gi, id).replace(/{clubusername}/gi, autoHeaderObj.user.un);
            return div;
        }
    };
    AutoBlogReply.init = function (count) {

        AutoBlogReply["Message"].Item = {};
        AutoBlogReply["Message"].LastId = null;
        AutoBlogReply["Message"].IsOpen = false;
        jQuery(".user-comment-list-post").remove();
        //头部
        if (autoHeaderObj.user.isLogin) {
            var name = autoHeaderObj.user.un == null ? "" : autoHeaderObj.user.un;
            var userStatus = "欢迎您，<span class=\"red\">" + name + "</span> | <a target=\"_self\"  href=\"javascript:loginOutAccount();\">退出登录</a> ";
            jQuery("#reply-login").html(userStatus).show().addClass("post-comment-user");
            jQuery("#reply-nologin").hide();
        }
        else {
            var userStatus = '<div class="fn-left">用户名<input type="text" id="txtusername" value="" class="form-text">密码<input type="password" id="txtpassword" onkeydown="AutoBlogReply.Message.keydown(event,this,0)" class="form-text"><a class="btn btn-small" href="javascript:void(0);" target=\"_self\" onclick="AutoBlogReply.Message.login(this,0);return false;" id="btn_login">登 录</a></div><div class="fn-right"><a href="//account.autohome.com.cn/password/find">忘记密码？</a> | <a href="//account.autohome.com.cn/register?rt=1">注册</a></div>';
            jQuery("#reply-nologin").html(userStatus).show().addClass('post-comment-login');
            jQuery("#reply-login").hide();
        }
  
        //评论列表
        loadCount = loadCount||1;
        if (count) {
            AutoBlogReply.getComments(1,count);
        } else {
            AutoBlogReply.getComments(1);
        }
    };

    function GetTenMore() {
