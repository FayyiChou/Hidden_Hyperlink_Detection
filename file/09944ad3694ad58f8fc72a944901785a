<script language="javascript" type="text/javascript">
    AutohomeJsLoad({ url: "/ashx/article/HArticleClick.ashx?id=573062&t=" + Math.random() });

    function Logoff(){
        if('1'=='2'){
            window.location.href="//shuoke.autohome.com.cn";
        }
        autoHeaderObj = new AutoHeader(); 
    }

    autoHeaderObj.logoffFunctionArray.push(Logoff);
