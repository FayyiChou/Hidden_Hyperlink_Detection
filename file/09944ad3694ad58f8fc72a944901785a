    $.fn.extend({
        share: function (obj) {
            window.sharetitle = obj.sharetitle;
            window.shareUrl = obj.shareUrl;
            window.pic = obj.pic;
            window.summary = obj.summary;
            //分享到微信
            $('.share-weixin').click(function () {
                weixinshare()
            });
            $('.share-friend').click(function () {
                weixinshare()
            });
            function weixinshare() {
                $('<div id="bsWXBox" class="bsBox" style="display: block; left: 50%; top: 50%; margin-left: -110px; margin-top: -122.5px; position: fixed; height: 245px; width: 220px;"><div class="bsClose" style="top: 8px; right: 20px;">X</div><div class="bsTop" style="width: 220px;"><span style="margin-left: 8px;">扫描分享到微信好友或微信朋友圈</span><span class="bsPlatName"></span></div><div class="bsFrameDiv"><div style="height: 220px; width: 220px; display: block;"><img style="width:178px;height:178px;margin:21px;display:inline-block;" src="//athm.cn/qrcode?size=256&url=' + window.shareUrl + '"></div></div></div>').appendTo($("body"));
                $(".bsClose").on('click', function () {
                    $("#bsWXBox").remove();
                })
            }
            //分享到微博
            $('.share-weibo').click(function () {
                (function (s, d, e) { try { } catch (e) { } var f = 'http://v.t.sina.com.cn/share/share.php?', u = window.shareUrl, p = ['url=', e(u), '&title=', e(window.sharetitle), '&appkey=2924220432', '&pic=', e(window.pic), '&url=', e(window.shareUrl)].join(''); function a() { if (!window.open([f, p].join(''), 'mb', ['toolbar=0,status=0,resizable=1,width=620,height=450,left=', (s.width - 620) / 2, ',top=', (s.height - 450) / 2].join(''))) u.href = [f, p].join(''); }; if (/Firefox/.test(navigator.userAgent)) { setTimeout(a, 0) } else { a() } })(screen, document, encodeURIComponent);
            });
            //分享qq空间
            $(".share-qzone").click(function () {

                (function (s, d, e) { try { } catch (e) { } var f = 'http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?', u = window.shareUrl, p = ['url=', e(u), '&title=', e(window.sharetitle), '&appkey=2924220432', '&pics=', e(window.pic), '&summary=', e(window.summary), '&url=', e(window.shareUrl)].join(''); function a() { if (!window.open([f, p].join(''), 'mb', ['toolbar=0,status=0,resizable=1,width=620,height=450,left=', (s.width - 620) / 2, ',top=', (s.height - 450) / 2].join(''))) u.href = [f, p].join(''); }; if (/Firefox/.test(navigator.userAgent)) { setTimeout(a, 0) } else { a() } })(screen, document, encodeURIComponent);
            })
            //分享qq
            $(".share-qq").click(function () {
                (function (s, d, e) { try { } catch (e) { } var f = 'http://connect.qq.com/widget/shareqq/index.html?', u = window.shareUrl, p = ['url=', e(u), '&title=', e(window.sharetitle), '&appkey=2924220432', '&pics=', e(window.pic), '&summary=', e(window.summary), '&url=', e(window.shareUrl)].join(''); function a() { if (!window.open([f, p].join(''), 'mb', ['toolbar=0,status=0,resizable=1,width=620,height=450,left=', (s.width - 620) / 2, ',top=', (s.height - 450) / 2].join(''))) u.href = [f, p].join(''); }; if (/Firefox/.test(navigator.userAgent)) { setTimeout(a, 0) } else { a() } })(screen, document, encodeURIComponent);
            })
            $(".bsClose").on('click', function () {
                $("#bsWXBox").hide();
            })
        }
    })
   



    var currenthref = window.location.href;
    //bShare.addEntry({
    //    url: "" + currenthref.substr(0, currenthref.indexOf("html") + 4) + "?pvareaid=2028132"
    //});
    var url = encodeURIComponent(currenthref);
    var uid = autoHeaderObj.user.isLogin ? autoHeaderObj.user.uid : 0;
    jQuery(document).ready(function () {
        jQuery("#share-weixin").click(function () {
            _trackEvent.push({ 'eid': '1|1|227|0|200121|300159', 'val': 1, 'a1': uid, 'curl': url, 'rurl': url, time: new Date().getTime() });
        });
        jQuery("#share-friends").click(function () {
            _trackEvent.push({ 'eid': '1|1|227|0|200121|300158', 'val': 1, 'a1': uid, 'curl': url, 'rurl': url, time: new Date().getTime() });
        });
        $("#share-list").share({
            sharetitle: document.title,
            shareUrl: "" + currenthref.substr(0, currenthref.indexOf("html") + 4) + "?pvareaid=2028132",
            pic: '',
            summary: "",
        });
        $('.btn-more').on('click', function () {
            $('.car-line-item').show();
            $(this).hide();
            $('.btn-fold').show();
        });

        $('.btn-fold').on('click', function () {
            $('.car-line-item:gt(2)').hide();
            $(this).hide();
            $('.btn-more').show();
        });
    });
</script>


