                type: "get",
                cache: false,
                url: "//shuoke.autohome.com.cn/ashx/user/ValidateAccount.ashx",
                data: { logincode: escape(jQuery(name).val().replace(/\+/gi, '%2B')), pwd: hex_md5(jQuery(pwd).val()), validcode: "", opertype: "login" },
                dataType: "json",
                success: function (data) {
                    var json = data;
                    if (json.returncode != "0") {
