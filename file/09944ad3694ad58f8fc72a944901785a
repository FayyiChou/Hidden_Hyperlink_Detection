        var hotComment = new HotComment('573062');
        hotComment.init();
    });
</script>


<script type="text/javascript">
    function toggleTipDiv(objBtn, objDivId) {
        var $objDiv = jQuery("#" + objDivId);
        var $objBtn = jQuery(objBtn);
        var currentPosition = $objDiv.attr("position"), btnPosition = $objBtn.attr("position");
        var ishidden = $objDiv.is(":hidden");

        if (ishidden || currentPosition != btnPosition) {
            var top = $objBtn.offset().top + 23;
            var left = $objBtn.offset().left - 0.43 * $objDiv.width() + 0.5 * $objBtn.width();
            $objDiv.css({ "left": left, "top": top, "z-index": 10 }).show();
            if (btnPosition) {
                $objDiv.attr("position", btnPosition);
            }
        } else {
            $objDiv.hide();
        }
    }

    jQuery(document).ready(function() {
            
        jQuery("a[name='btnWeixin'].btn-org").click(function(event) {
            var e = window.event || event;
            if (e.stopPropagation) {
                e.stopPropagation();
            } else {
                e.cancelBubble = true;
            }

            toggleTipDiv(this, "tipWeixinInfo");
            _hmt.push(['_trackEvent','微信按钮','点击','最终页下面']);
        });
        jQuery("a[name='btnWeixin'].btn-orange").click(function(event) {
            var e = window.event || event;
            if (e.stopPropagation) {
                e.stopPropagation();
            } else {
                e.cancelBubble = true;
            }

            toggleTipDiv(this, "tipWeixinInfo");
            _hmt.push(['_trackEvent', '微信按钮', '点击', '最终页右上']);
        });

        jQuery("#tipWeixinInfo").click(function(event) {
            var e = window.event || event;
            if (e.stopPropagation) {
                e.stopPropagation();
            } else {
                e.cancelBubble = true;
            }
        });

        jQuery("a[name='btnWeibo'].btn-gray,a[name='btnWeixin'].btn-gray").mouseover(function() {
            toggleTipDiv(this, "tipSet");
        }).mouseout(function() {
            toggleTipDiv(this, "tipSet");
        });

        document.onclick = function () {
            jQuery("#tipWeixinInfo").hide();
        };

        //相关阅读
        jQuery("#moreRelate").click(function() {
            if (jQuery(this).text() == '展开更多') {
                jQuery("#divRelate ul li:not(.first)").show();
                jQuery(this).text('收起更多');
            } else {
                jQuery("#divRelate ul li:not(.first)").hide();
                jQuery(this).text('展开更多');
            }
        });
    });

   

        
    var skSubscribe;
    seajs.use(["subscribe","newarticle"], function (subscribe,newarticle) {
        skSubscribe = subscribe;
        subscribe.init($constant.authorid, $winobj,$constant.url);
        subscribe.initStatus();

        var hotArticleIds = '572757,572453,573056,572546,572725,573062,572641,536016,572553,572925';
        newarticle.init(hotArticleIds, 10);
        newarticle.getArticles();
            
        newarticle.setCookieView($constant.articleid);
        
    });

        
    var objPraise;
    seajs.use(["praisearticle","login","cookieutil"], function(praisearticle) {
        objPraise = praisearticle;

        jQuery(document).ready(function() {
            praisearticle.init($winobj, $constant.authorcount, $constant.articleid);
            praisearticle.initPraise();
            $winobj.$support_box.click(function() {
                praisearticle.clickSupport();
                return false;
            });

            $winobj.$sup_list.find(".mt10 a").click(function() {
                $winobj.$sup_list.find("ul.s-plist2").show();
                $winobj.$sup_list.find(".mt10").hide();
                return false;
            });

            autoHeaderObj.logoffFunctionArray.push(function() {
                praisearticle.getPraiseState();
            });

            //提示赞
            var knowsupport = subcookieUtil.get("iknow", "support");
            if (!knowsupport) {
                $winobj.$sup_tip.show();
            } else {
                $winobj.$sup_tip.hide();
            }

            $winobj.$sup_tip.find("a").click(function() {
                $winobj.$sup_tip.hide();
                setCookieIknow("support",1);
            });

            if (skSubscribe != null) {
                $winobj.$support_login.click(function() {
                    skSubscribe.setposition(0);
                });
            }
        });
    });
        
    //弹窗登录
    var callBackFunAccountPopLogin;
    var skPopLogin;
    seajs.use(['popLogin', 'dialog'], function( popLogin) {
        document.domain = "autohome.com.cn";
        var registerType = 0; //0简单注册，1完整注册
        var backurl = "";
        skPopLogin = popLogin;
        $winobj.$sup_btnlogin.click(function() {
            popLogin.init(registerType, backurl);
        });

        callBackFunAccountPopLogin = function() {
            var name = getUNcookie() == true ? cookieUN : "";
            autoHeaderObj = new AutoHeader(); //头部登录

            if (autoHeaderObj.user.isLogin) {
                jQuery("#reply-login")
                    .html("欢迎您，<span class=\"red\">" + name + "</span> | <a href='javascript:loginOutAccount();' target='_self'>退出登录</a> ")
                    .show()
                    .addClass("post-comment-user");
                jQuery("#reply-nologin").hide();
                jQuery('.user-comment-list-post .post-comment-login').html(name + ' | <a href="javascript:loginOutAccount();" target="_self">退出登录</a> ');

                commentTop.addClickEvent();
                commentTop.addCommentTipEvent();

                if (typeof ModifyRegisterUrl == "function") {
                    ModifyRegisterUrl();
                }
            }
            objPraise.getPraiseState();
            if (skSubscribe != null) {
                skSubscribe.initStatus();
            }
            var loginposition = 0;
            if (skSubscribe != null) {
                loginposition = skSubscribe.getposition();
            }
            if (loginposition == 0) {
                setTimeout(function() {
                    objPraise.support();
                }, 200);
            } else {
                skSubscribe.subscribe(loginposition);
            }
        };
    });
      
    //更新登录状态
    function updateLoginStatus() {
        var name = getUNcookie() == true ? cookieUN : "";
        autoHeaderObj = new AutoHeader(); //头部登录

        if (autoHeaderObj.user.isLogin) {
            jQuery("#reply-login")
                .html("欢迎您，<span class=\"red\">" + name + "</span> | <a href='javascript:loginOutAccount();' target='_self'>退出登录</a> ")
                .show()
                .addClass("post-comment-user");
            jQuery("#reply-nologin").hide();
            jQuery('.user-comment-list-post .post-comment-login').html(name + ' | <a href="javascript:loginOutAccount();" target="_self">退出登录</a> ');
            var userstatus = AutoShuokeUser.getUserStatus();
            if (userstatus > 0 && isdialog) {
                AutoBlogReply.showBindinfoTip(userstatus, textarea);
            }

            commentTop.addClickEvent();
            commentTop.addCommentTipEvent();

            if (typeof ModifyRegisterUrl == "function") {
                ModifyRegisterUrl();
            }
        }
        objPraise.getPraiseState();
    }
</script>
<script type="text/javascript">
    var ids = '573062.573062.572641.536016.572553.572925';
    if (ids.length > 0) {
        AutohomeJsLoad({ url: "//reply.autohome.com.cn/api/getData_ReplyCounts.ashx?appid=7&objids=" + ids + "&callback=callback&dateType=jsonp&r=" + Math.random() });

        function callback(data) {
            var count = data.commentlist.length;
            for (var i = 0; i < count; i++) {
                var li = data.commentlist[i];
                var obj=document.getElementById("reply_" + li.objid);
                if(obj) {
                    obj.innerHTML = "<i class=\"icon-infor\"></i>" + li.replycountall;
                }
            }
        }
    }

