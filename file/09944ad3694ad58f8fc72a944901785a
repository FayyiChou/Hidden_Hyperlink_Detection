    var povote = function (id, returnId) {
        var obj = document.getElementById(id),
  returnObj = document.getElementById(returnId),
  isIE6 = !!(window.attachEvent && !window.opera && navigator.userAgent.indexOf("MSIE 6") > -1);
        var init = function () {
            var st = document["documentElement"].scrollTop + document["body"].scrollTop;
            returnObj.style.display = st > 0 ? 'block' : 'none';
            if (isIE6) {
                obj.style.position = 'absolute';
                obj.style.top = (st + parseInt(document.documentElement.clientHeight) - 213) + 'px';
            }
        };
        window.onscroll = init;
    };
    povote("floatDiv", "returnTop");
