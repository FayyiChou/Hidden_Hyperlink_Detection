        //分布式注册
        function ModifyRegisterUrl() {
            var status = AutoShuokeUser.getUserStatus();
            var objRegist = jQuery("a[name='registshuoke']");
            if (status == 1) {
                objRegist.attr({ "href": "javascript:void(0);", "target": "_self" })
                    .click(function () {
                        skPopmobilebind.init();
                        return false;
                    });
            } else if (status == 2) {
                objRegist.attr({ "href": "javascript:void(0);", "target": "_self" })
                    .click(function () {
                        skPopUpNickName.init();
                        return false;
                    });
            } else {
                objRegist.attr({ "href": "//shuoke.autohome.com.cn/author/Regist.html#pvareaid=101683", "target": "_blank" });
            }
            return false;
        }
    </script> 
  
<script type="text/javascript" src="//x.autoimg.cn/blog/js/dialog.140704.js?v2.1"></script>
<script type="text/javascript" src="//x.autoimg.cn/blog/js/common/subcookieutil.js?v1.0"></script>
<script type="text/javascript">
    var subcookieUtil = new subCookieUtil();
    //退出，修改导航中菜单链接
    autoHeaderObj.logoffFunctionArray.push(function () {
        jQuery("#applyLink").html('<ul><li class="current"><a href="//shuoke.autohome.com.cn/author/Regist.html#pvareaid=101683" name="registshuoke"><i></i>投稿</a></li></ul>');
    });
    
    var hasImage, hascompleteinfo;
    //初始化导航链接
    function initUserMenu() {
        //未登录
        if (!autoHeaderObj.user.isLogin) {
            return;
        }
        //已登录
        jQuery.getJSON("//shuoke.autohome.com.cn/ashx/user/usercheck.ashx", { r: Math.random() }, function (data) {
            if (data.returncode == 0) {
                var userinfo = data.result;
                hasImage = userinfo.hasimage;
                hascompleteinfo = userinfo.hascompleteinfo;
                //审核通过
                if (userinfo && userinfo.status == 1) {
                    //加载入口链接
                    var html = '<ul><li><a target="_self" href="javascript:void(0);" id="btnArticle">发表文章</a></li><li>|</li><li><a target="_blank" href="/authorcenter/index.html">我的说客</a></li></ul>';
                    jQuery("#applyLink").html(html);
                    //发表文章事件
                    var objBtnArticle = jQuery("#btnArticle");
                    var cookieKnow = subcookieUtil.get("iknow", "myinfo");
                    var uid = autoHeaderObj.user.isLogin ? autoHeaderObj.user.uid : 0;
                    objBtnArticle.click(function () {
                        _trackEvent.push({ 'eid': '1|1|227|0|200121|300159', val: uid });
                    });
                    if (cookieKnow == 1 || (hasImage && hascompleteinfo)) {
                        objBtnArticle.attr({ "href": "//shuoke.autohome.com.cn/author/BlogArticleEditor.html", "target": "_blank" });
                    } else {
                        objBtnArticle.attr({ "href": "javascript:void(0);", "target": "_self" })
                            .click(function () {
                                //弹窗提示完善信息
                                showPopDialog('dialog', '这么好的文章，总得让大家知道是出自哪位高人之手，请先完善您的个人信息！', true, function() {
                                    popdialog.hide(function() {
                                        //点击确定后设置cookie
                                        setCookieIknow("myinfo", 1);
                                        //没有头像，跳转到头像设置页面
                                        if (!hasImage) {
                                            window.location.href = "/authorcenter/portrait.html";
                                            return false;
                                        } else { //跳转到说客设置页面
                                            window.location.href = "/authorcenter/set.html";
                                            return false;
                                        }
                                    });
                                }, true, function() {
                                    popdialog.hide();
                                    window.location.href = "/author/BlogArticleEditor.html";
                                    return false;
                                });
                            return false;
                        });
                    }
                }
            }
        });
    }

    initUserMenu();

    //搜索
    var defaultSerarchq="请输入关键字";
    function beforeSearch(){
    document.charset='gb2312';
    var text=jQuery("#searchq").val();
    if(text==defaultSerarchq){
        jQuery("#searchq").val("");
    }
    }

    //弹出浮层提示
    function showPopDialog(id, msg, showOk, okevent, showCancel, cancelevent) {
        id = id || "tipDialog";
        msg = msg.replace(/\r\n/g, '<br/>');
        var btn = '';
        if (showOk) {
            btn = '<a href="javascript:void(0);" class="btn btn-small btn-orange" target="_self" id="popBtnOk">确认</a>';
        }
        if (showCancel) {
            btn += '<a href="javascript:void(0);" class="btn btn-small ml10" target="_self" id="popBtnCancel">取消</a>';
        }

        var html = '<div class="layer layer-warning" id="' + id + '">\
                            <div class="layer-content">\
                                <div class="layer-warning-content"><i class="icon32-vie"></i><div>'+ msg + '</div></div>\
                            </div>\
                            <div class="layer-bottom layer-warning-bottom">'+ btn + '</div>\
                        </div>';
        jQuery("body").append(jQuery(html));
        popdialog.show(id);
        if (okevent) {
            jQuery("#popBtnOk").click(function () { okevent(); return false; });
        }
        if (cancelevent) {
            jQuery("#popBtnCancel").click(function () { cancelevent(); return false; });
        }
    }
    //设置iknow子cookie
    function setCookieIknow(subcookiename, cookievalue) {
        var expire_days = 3600;
        var expire_date = new Date();
        var ms_from_now = expire_days * 24 * 60 * 60 * 1000;
        expire_date.setTime(expire_date.getTime() + ms_from_now);
        subcookieUtil.set("iknow", subcookiename, cookievalue, expire_date, "/", "shuoke.autohome.com.cn");
    }

    jQuery(document).ready(function () {
        if (typeof ModifyRegisterUrl == "function") {
            ModifyRegisterUrl();
        }

        //导航提示：新上线作者top100
        var objtipTopAuthor = jQuery("#tipTopAuthor");
        var ckSkTopKnow = subcookieUtil.get("iknow", "top100");
        if (!ckSkTopKnow) {
            objtipTopAuthor.show();
            var objTopAuthorKnow = jQuery("#btnKnowTopAuthor");
            objTopAuthorKnow.click(function () {
                objtipTopAuthor.hide();
                setCookieIknow("top100", 1);
                return false;
            });
            var objViewTopAuthor = jQuery("#btnViewTopAuthor");
            objViewTopAuthor.click(function () {
                objtipTopAuthor.hide();
                setCookieIknow("top100", 1);
                window.location.href = "//shuoke.autohome.com.cn/author/rank.html";
                return false;
            });
        }
        
        jQuery("#searchq").val(defaultSerarchq)
        .focus(function(){
                var text=jQuery(this).val();
                if(text==defaultSerarchq){
                jQuery(this).val("");
            }
        })
    .blur(function(){
        var text=jQuery.trim(jQuery(this).val());
        if(text==""){
            jQuery(this).val(defaultSerarchq);
        }
    });
    });

    //导航栏鼠标经过效果
    function AddNavStyle() {
        var navUl = document.getElementById("ulNav");
        var navLi = navUl.getElementsByTagName("li");
        for (var i = 1; i <= 4; i++) {
            var className;
            if (i == '2') {
                className = "nav-item current";
            } else {
                className = "nav-item";
            }
            var li = navLi[i - 1];
            li.className = className;
            li.onmouseover = function () {
                this.className = "nav-item current";
            };
            li.onmouseout = function () {
                if (this.id != "li2")
                    this.className = "nav-item";
            };
        }

        navUl.onmouseout = function () {
            var li = document.getElementById("li2");
            if (li) {
                document.getElementById("li2").className = "nav-item current";
            }
        };
    }

    AddNavStyle();

    //导航栏浮动
    jQuery(function () {

        var $win = jQuery(window),
            $nav = jQuery('#nav'),
            lastTop = $win.scrollTop(),
            isIE6 = !!window.ActiveXObject && !window.XMLHttpRequest,
            up = false, navTop = 155, wtop, timer;

        $win.scroll(function () {
            timer && clearTimeout(timer);
            timer = setTimeout(function () {
                wtop = $win.scrollTop();
                if (lastTop == wtop) {
                    return;
                }
                up = lastTop > wtop ? true : false;
                lastTop = wtop;
                if (up && wtop > navTop) {
                    $nav.addClass('navFixed');
                    isIE6 && $nav.css('top', wtop);
                }
                else {
                    $nav.removeClass('navFixed');
                    isIE6 && $nav.css('top', 0);
                }
            }, 13);
        });
    });
</script>
    <!--导航end-->

