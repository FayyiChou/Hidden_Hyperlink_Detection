    //function shareInit(e) { var t = function (t, r) { this.elem = e(t), this.options = r }; t.prototype = { constructor: t, toggle: function () { return this[this.isShown ? "hide" : "show"]() }, show: function () { var t = this, r = e.Event("show"); t.elem.trigger(r), t.isShown || r.isDefaultPrevented() || (t.isShown = !0, t.elem.show().trigger("shown"), "click" == t.options.trigger && setTimeout(function () { e(document).on("click.overlay.d", function (r) { var a = e(r.target).closest(t.elem); 0 == a.length && a != t.elem && (t.hide(), e(document).off("click.overlay.d")) }) }, 0)) }, hide: function (t) { t && t.preventDefault(); var r = this; t = e.Event("hide"), r.elem.trigger(t), r.isShown = !1, r.elem.hide().trigger("hidden"), e(document).off("click.overlay.d") } }, e.fn.overlay = function (r) { return this.each(function () { var a = e(this), n = a.data("overlay"), o = e.extend({}, e.fn.overlay.defaults, a.data(), "object" == typeof r && r); n || a.data("overlay", n = new t(this, o)), "string" == typeof r ? n[r]() : o.show && n.show() }) }, e.fn.overlay.Constructor = t, e.fn.overlay.defaults = { show: !0 }, e(function () { e(document).on("click.overlay.data-api", '[data-trigger="click"][data-toggle="overlay"]', function () { var t, r = e(this), a = e(r.attr("data-target") || (t = r.attr("href")) && t.replace(/.*(?=#[^\s]+$)/, "")), n = a.data("overlay") ? "toggle" : e.extend({}, a.data(), r.data()); a.overlay(n) }), e(document).on("hover.overlay.data-api", '[data-trigger="hover"][data-toggle="overlay"]', function (t) { var r, a, n = e(this), o = e(n.attr("data-target") || (r = n.attr("href")) && r.replace(/.*(?=#[^\s]+$)/, "")), i = function () { a = o.data("overlay"), a && (a._hoverTimer = setTimeout(function () { o.overlay("hide") }, 50)) }; switch (t.type) { case "mouseenter": n.data("timer", setTimeout(function () { o.overlay("show"), a = o.data("overlay"), a.hasHoverEvent || (o.hover(function () { a._hoverTimer && clearTimeout(a._hoverTimer) }, i), a.hasH