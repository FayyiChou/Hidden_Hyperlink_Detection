        jQuery(function() {
            jQuery('span[vid]').each(function() {
                var vid = jQuery(this).attr("vid");
                var videoid = jQuery(this).attr("videoid");
                var id = jQuery(this).attr("id");
                if (videoid) {
                    jQuery.ajax({ url: "//p-vp.autohome.com.cn/api/player?vid=" + videoid + "&container=" +id+ "&direct=1", dataType: "jsonp" });
