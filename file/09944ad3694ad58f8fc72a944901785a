    (function() {
        if (!autoHeaderObj.user.isLogin) {
            return;
        }
        var $articleTitle = jQuery("#articleTitle"),
            $txtRecommend = $articleTitle.find("#txtRecommend"),
            $tipRecommend = $articleTitle.find("#tipRecommend"),
            $spanDraft = $articleTitle.find("#spanDraft"),
            $modifyArticle = $articleTitle.find("#modifyArticle"),
            $deleteArticle = $articleTitle.find("#deleteArticle");

        var uid, isAuthor;
        uid = autoHeaderObj.user.uid == null ? "" : autoHeaderObj.user.uid;
        isAuthor = uid && uid == '17024564';

        if (isAuthor) {
            $txtRecommend.show();
            $tipRecommend.show();
            $spanDraft.show();
            if ($txtRecommend.length == 0) {
                $modifyArticle.show();
                $deleteArticle.show();
            }
        } else {
            jQuery.ajax({
                url: "//shuoke.autohome.com.cn/ashx/user/CheckPrivilege.ashx",
                data: { opertype: "getadminp" ,r:Math.random()},
                type: "get",
                success: function(json) {
                    if (!json) {
                        return;
                    } else {
                        json = eval('(' + json + ')');
                        if (json && json.returncode == 0&&json.result.pid) {
                            var arrpid = json.result.pid.split(',');
                            if (jQuery.inArray("1", arrpid) > -1) {
                                $spanDraft.show();
                                $modifyArticle.show();
                            }
                            if (jQuery.inArray("2", arrpid) > -1) {
                                $spanDraft.show();
                                $deleteArticle.show();
                            }
                        }
                    }
                },
                error: function() {
                }
            });
        }

        $tipRecommend.mouseover(function() {
            jQuery(this).next().show();
        }).mouseout(function() {
            jQuery(this).next().hide();
        });
    })();

    
    (function() {
        jQuery('#articlecontent img').each(function (index, item) {
            var src = jQuery(item).attr('src');
            var href = '//shuoke.autohome.com.cn/img/?img=' + src;
  
            var parent = jQuery(item).parent();
            if (parent[0].nodeName == 'A') {
                parent.attr({ "href": href,"title":href});
            } else {
                var a = jQuery('<a href="' + href + '"></a>');
                jQuery(item).wrap(a);
            }
        });
    })();
    

    function Delete(id) {
        var ifDelete = confirm("确认删除此篇文章？");
        if (ifDelete) {
            jQuery.ajax({
                url: "/authorcenter/ashx/HDeleteArticle.ashx",
                data: { BlogArticleid: id },
                type: "post",
                success: function(result) {
                    if (!result) {
                        alert('删除失败');
                        return;
                    }
                    result = eval('(' + result + ')');
                    if (result && result.returncode == 0) {
                        alert('删除成功!');
                        window.location.href = '/author/17024564.html';
                    } else if (result.message) {
                        alert('删除失败！' + result.message);
                    }
                },
                error: function() {
                    alert('删除失败');
                }
            });
        }
    }

