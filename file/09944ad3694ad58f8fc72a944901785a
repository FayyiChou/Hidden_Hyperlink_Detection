    AutoBlogReply["Replace"] = function (T, D) {
        var sb = T;
        for (var o in D) {
            var b = "" + D[o];
            var b = new RegExp("{" + o + "}", "gi");
            sb = sb.replace(b, D[o]);
        }
        return sb;
    };
