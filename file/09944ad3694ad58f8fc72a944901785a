        login: function (obj,isdialog) {
            var par = jQuery(obj).parents("div.fn-left");
            var name = jQuery(par).find("input")[0];
            var pwd = jQuery(par).find("input")[1];
            var textarea = jQuery(obj).parents(".post-comment-login").next().find("textarea");
            if (jQuery(name).val() == "") {
                if (focus != false) name.focus();
                alert("用户名不能为空");
                return false;
            }
            if (jQuery(pwd).val() == "") {
                if (focus != false) pwd.focus();
                alert("密码不能为空");
                return false;
            }
            if (jQuery(pwd).val().length < 6 || jQuery(pwd).val().length > 25) {
                alert("账户密码不匹配");
                if (focus != false) pwd.focus();
                return false;
