<script type="text/javascript">
	$('.videowh embed').css({
		width:450,
		height:280,
	})
</script>
	<script type="text/javascript">

		jQuery(".focusBox").slide({
			mainCell : ".pic",
			effect : "left",
			autoPlay : true,
			delayTime : 300
		});
		/*   $(function() {
			$('.index-main-box').css('margin-top', '28%');
		});   */

		/*鼠标移动显示详细信息--照片墙*/
		$('.ps-list').on('mouseover', function() {
			$(this).find('div').css('display', 'block');
		});
		$('.ps-list').on('mouseout', function() {
			$(this).find('div').css('display', 'none');
		});
		var oPhotoImgWidth = $('.ps-list img').width();

		/*产品中心*/
		 $('.zj-product-ul li').css('height',
				$('.zj-product-ul li').eq(0).width()); 
		var oBackgroungImg;
		$('.zj-product-ul li a')
				.on(
						'mouseenter',
						function() {
							oBackgroungImg = $(this).parent('li').css(
									'background');
							$(this)
									.parent('li')
									.css(
											'background',
											'url(/static/home/images/images/product-index-bg.png) no-repeat center center');
							$(this).css('opacity', '1');
						});

		$('.zj-product-ul li a').on('mouseleave', function() {
			$(this).parent('li').css('background', oBackgroungImg);
			$(this).css('opacity', '0');
		});

		$('.zj-index-act li').each(function() {
			$(this).on('mouseenter', function() {
				$(this).find('img').stop().animate({
					'opacity' : 0.4
				}, 1000);
			});
			$(this).on('mouseleave', function() {
				$(this).find('img').stop().animate({
					'opacity' : 1
				}, 1000);
			});
		});

		/*企业视频*/

		$('.zj-shiping').on('mouseenter', function() {
			$(this).find('img').stop().animate({
				'opacity' : '0.5'
			});
		}).on('mouseleave', function() {
			$(this).find('img').stop().animate({
				'opacity' : '1'
			});
		});
		
		$(window).scroll(function(){
			if($(window).scrollTop() > 900){
				$('.zj-qi-video').css({
					width:300,
					height:235,
					left : 0,
					top:($(window).height()-$('.zj-qi-video').height()) - 30,
				});

				$('.index-smal h4').css({
					'display':'block',
				});
				$('.videowh embed').css({
					width:270,
					height:200,
				});
			}else{
				$('.zj-qi-video').css({
					width:480,
					height:380,
					left: ($(document).width() - $('.zj-qi-video').width()) / 2,
					top:($(window).height()-$('.zj-qi-video').height())/2,
				});

				$('.index-smal h4').css({
					'display':'block',
				});
				$('.videowh embed').css({
					width:450,
					height:280,
				});
			};
		});
		
		$('.zj-shiping').each(function(i) {
			$(this).on('click', function() {
				$('#vod-bg').show();
				$('.zj-qi-video').eq($(this).index()).fadeIn(300);
			});
		});

		$('.close-video').on('click', function() {
			$('#vod-bg').hide();
			$('.zj-qi-video').hide();
		});
		
		
		/* 集团简介 */
		$('.index-jibox img').css('width', $(window).width());
		$('.index-jip').css({
			'left':( $(window).width()-$('.index-jip').width())/2,
			'top':($('.index-jibox img').height()-$('.index-jip').height())/2
			});
		
		$('.kg-a1').on('mouseenter',function(){
			$(this).find('.kg-span').stop().animate({'opacity':1},600)
			}).on('mouseleave',function(){
				$(this).find('.kg-span').stop().animate({'opacity':0},600);;
		})
		
		
	</script>
	
	<script type="text/javascript">
				$('#joinUs').mouseover(function(){
					$('#joinUs').css('color','red');
				});
				$('#joinUs').mouseout(function(){
					$('#joinUs').css('color','yellow');
				});
			</script>
