	<script type="text/javascript">
	$(function(){
		$('.right-navBox').css({
			'top':($(window).height()-100)/2
		});
		$('.wxgzpt').css({
			'top':($(window).height()-100)/2,
		});

		$('.scr-a1').on('mouseenter',function(){
			$('.wxgzpt').show();
		}).on('mouseleave',function(){
			$('.wxgzpt').hide();
		});
		$(window).scroll(function(){
			if ($(window).scrollTop() > 1){
				$('.right-navBox').show(150);
			}else{
				$('.right-navBox').hide(150);
			}
		});
		$('.scr-a4').on('click',function(){
			$('html body').animate({
				scrollTop:0,
			});
		});
	
	})
	</script>
