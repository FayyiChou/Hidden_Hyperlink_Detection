		$(function(){
			var a_link_div_html = "";
	    	    a_link_div_html += "<span class=''>友情链接：</span>";
	    	    a_link_div_html += "<span id='linklist'>";
			$.ajax({ 
			    url:'/linklist',   
			    type:'get',   
			    async : false, //默认为true 异步   
			    success:function(data){   
			       var data_links = eval(data); 
			       if(data_links.length > 0){
		    		   for ( var i in data_links) {
				    		if(i==0){
				    			a_link_div_html += "<a href=\" "+ data_links[i].url + "\" class='' target='_blank'>" + data_links[i].name + "</a>";
				    		}else{
				    			a_link_div_html += "<a href=\" "+ data_links[i].url + "\" id='a_link' target='_blank'>" + data_links[i].name + "</a>";
				    		}
						}
		    		   a_link_div_html += "</span>";
		   	    		$('#a_link_div').html(a_link_div_html);
			       }
				}
			});
			
		});
		
		/*
		*读取cookie
		*/
		function getCookie(name){ 
		    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
		    if(arr=document.cookie.match(reg)){
		        return unescape(arr[2]); 
		    }else{
		        return null; 
		    } 
		} 
	</script>


<script>
	$(window).scroll(function() {
		if ($(window).scrollTop() > 500) {
			$('.returntop').show();
			$('.mmrBox').show();
		} else {
			$('.returntop').hide();
			$('.mmrBox').hide();
		}
	});

	$('.returntop').click(function() {
		$('html,body').animate({
			scrollTop : 0,
		});
	});

	function tabgm(){
		$('.mmrBox').animate({
			bottom:160,
		},400,function(){
			$('.mmrBox').animate({
				bottom:140,
			},400);
		});
	};

	var timermm = setInterval(tabgm,800);
	$('.mmrBox').on('mouseenter',function(){
		clearInterval(timermm);
	}).on('mouseleave',function(){
		timermm = setInterval(tabgm,800);
	});
</script>
