<script type="text/javascript">
//热门资讯
showTabs(".artR_tablist li","","on","hover");
//文字标签_5切选卡
showTabs("#artRtab2 .arttabH a","#artRtab2 .tabb","on","hover");
//文字标签_4切选卡
showTabs("#artRtab3 .arttabH a","#artRtab3 .tabb","on","hover");
//查找药品选卡
showTabs(".drug_search_data .drug_tabs span",".drug_search_data .drug_list","on","hover");

$(function(){
	//右侧多彩内容块滑动效果
	var rtimer=[];
	$(".block_win8_list li").mouseenter(function(){
		var _this=$(this);
		_this.find("span").animate({opacity:"0"},300);
		rtimer[1]=window.setTimeout(function(){
			_this.find("span").hide();
		},300);
		return false;
	});
	$(".block_win8_list li").mouseleave(function(){
		
		var _this=$(this);
		_this.find("span").show().animate({opacity:"1"},300);
		window.clearTimeout(rtimer[1]);
		return false;
	});
	
	//查找药品入口
	$(".drug_cx_btn").click(function(e) {
		if(!$(".drug_search").hasClass("drug_search_on")){
			$(".drug_search").addClass("drug_search_on");
			$(".drug_search").find(".drug_search_wrap").width(670);
			$(".drug_search").find(".drug_search_box").stop(true,true).animate({marginLeft:"0"},250);
			window.clearTimeout(rtimer[2]);
			window.clearTimeout(rtimer[3]);
		}else if($(".drug_search").hasClass("drug_search_on")){
			$(".drug_search").removeClass("drug_search_on");
			rtimer[2]=window.setTimeout(function(){
				$(".drug_search").find(".drug_search_box").stop(true,true).animate({marginLeft:"-670px"},250);
			},250);
			rtimer[3]=window.setTimeout(function(){
				$(".drug_search").find(".drug_search_wrap").width(0);
			},500);
		}
	});
});
</script>
