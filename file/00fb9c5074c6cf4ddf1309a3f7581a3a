<div class="art_search2">
    <form id="search_art_bot" method="get" action="http://so.39.net/s" target="_blank">
	<div class="s_option"><span>综合</span></div>
	<input type="text" name="words" class="s_input" value="" maxlength="30" autocomplete="off">
    <input type="submit" value="" class="s_sou">
    <ul class="sbArea" style="display:none;">
		<li class="now" data-name="words" data-val="" data-url="so.39.net/s">综合</li>
		<li data-name="words" data-val="高血压" data-url="so.39.net/jbk.aspx">疾病</li>
		<li data-name="words" data-val="六味地黄丸" data-url="so.39.net/ypk.aspx">药品</li>
		<li data-name="name" data-val="中山大学附属第一医院" data-url="yyk.39.net/hospitals/">医院</li>
		<li data-name="name" data-val="" data-url="yyk.39.net/doctors/">医生</li>
    </ul>
    </form>
</div>
<script>
//search
jQuery(function(){
	var searchVal = $('#search_art_bot .s_input').val();
	$('#search_art_bot .s_option').click(function(){
		$('#search_art_bot .sbArea').toggle();
		return false;
	});
	$('#search_art_bot .sbArea li').click(function(){
		$(this).addClass('now').siblings().removeClass('now');
		$('#search_art_bot .sbArea').hide();
		$('#search_art_bot').attr('action', 'http://' + $(this).attr('data-url')).find('.s_input').attr('name', $(this).attr('data-name')).val($(this).attr('data-val'));
		$('#search_art_bot .s_option span').html($(this).text());
		searchVal = $(this).attr('data-val');
		return false;
	});
	$('#search_art_bot .s_input').focus(function(){
		$(this).val($(this).val() == searchVal ? '' : $(this).val()).addClass('focus');
	}).blur(function(){
		$(this).val($(this).val() == '' ? searchVal : $(this).val()).removeClass('focus');
	});
});
</script>
