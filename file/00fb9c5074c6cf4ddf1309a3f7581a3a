	var rTstimer=[];
	$(".rTsearch").hover(function(){
		$(".rTsearch").find(".rTs_box").stop(true,true).animate({width:"198px"},250);
		window.clearTimeout(rTstimer[1]);
	},function(){
		rTstimer[1]=window.setTimeout(function(){
