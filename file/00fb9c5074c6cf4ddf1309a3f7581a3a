<script type="text/javascript">
$(function() 
{
	$("#btndrug").click(function () {
	   if ($('#phardrugname').val() == "" || $('#phardrugname').val() == "请输入要查询的药品") {
			gotoWithKeyword('');
		}
		else {
			gotoWithKeyword($('#phardrugname').val());
		}
	})
    function gotoWithKeyword(word) {
		var url="http://ypk.39.net/search/all?k="+word;
        if (!/*@cc_on!@*/0) {
            window.open(url, '_new');
        } else {
            var a = document.createElement('a');
            a.href = url;
            a.target = '_new';
            document.body.appendChild(a);
            a.click();
        }
    }	
});
</script>
