        <script type="text/javascript">
            var comic_search_open_more = 1;
            window.searchBox = $(".hidden-search");
            window.menuBox = $(".hidden-menu");
            function showSearch() {
                checkShow(1);
                if (comic_search_open_more > 0) {
                    comic_search_open_more = 0;
                    searchBox.removeClass('hidden-search');
                    searchBox.addClass('show-search');
                } else {
                    comic_search_open_more = 1;
                    searchBox.removeClass('show-search');
                    searchBox.addClass('hidden-search');
                }
            }
            var comic_menu_open_more = 1;
            function showMenu() {
                checkShow(0);
                if (comic_menu_open_more > 0) {
                    comic_menu_open_more = 0;
                    menuBox.removeClass('hidden-menu');
                    menuBox.addClass('show-menu');
                } else {
                    comic_menu_open_more = 1;
                    menuBox.removeClass('show-menu');
                    menuBox.addClass('hidden-menu');
                }
            }

            function checkShow(menuId) {
                if (menuId == 1) {
                    checkMenu = $(".show-menu");
                    if (checkMenu) {
                        comic_menu_open_more = 1;
                        menuBox.removeClass('show-menu');
                        menuBox.addClass('hidden-menu');
                    }
                } else {
                    checkMenu = $(".show-search");
                    if (checkMenu) {
                        comic_search_open_more = 1;
                        searchBox.removeClass('show-search');
                        searchBox.addClass('hidden-search');
                    }
                }
            }
        </script>
