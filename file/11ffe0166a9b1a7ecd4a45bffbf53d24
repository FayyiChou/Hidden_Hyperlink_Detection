<!-- Google Display Ads Remarketing Code ENDS -->

<!-- DYNAMIC REMARKETING THE COMPLETE CODE CONFIGURED FOR VOLUSION -->
<script type="text/javascript">
<!-- DEFAULT CUSTOM PARAM VALUES-->
var pagetype = 'site-view'
var googleprice = '';
var googleprodid ='';
<!--URL DETAILS-->
var pathname = window.location.pathname;
<!--If home page-->
if(pathname == "/" || pathname == "Default.asp" || pathname == "default.asp"){
var pagetype = 'home';
var googleprice = '';
var googleprodid ='';
};

<!--If Category Page-->
if(pathname.indexOf('-s/') > -1){
var pagetype = 'category';
var googleprice = '';
var googleprodid ='';
}

<!--If Product Page-->
if(pathname.indexOf('-p/') > -1){
var pagetype = 'product';
var googleprice = $('span[itemprop="price"]').html().split('<')[0].replace('&#163;','').replace(',','').trim();
var googleprodid = global_Current_ProductCode;
};

<!--If Cart Page-->
if(pathname.indexOf('shoppingcart') > -1 || pathname.indexOf('ShoppingCart') > -1){
var pagetype = 'cart';
var googleprice = $(".pricecolor.colors_productprice b").html().replace(/[^0-9.]/g, '');
var productIdArray = new Array();
var cartquantity = $('a .cart-item-name').length;
for (var i=0; i < cartquantity; i++) {
var link = $('a.cart-item-name').eq(i).attr('href');
var substr = link.split('&');
var pcode = substr[0].split('=');
googleprodid = pcode[1].replace("%","-");
productIdArray.push(googleprodid);
};
googleprodid = productIdArray;
};

<!--If Order Complete-->
if(pathname.indexOf('OrderFinished') > -1 || pathname.indexOf('orderfinished') > -1){
var pagetype = 'purchase';
var googleprice = Order[2];
var productIdArray = new Array();
for (var i=0; i < OrderDetails.length; i++) {
	productIdArray.push(OrderDetails[i][2]);
}
googleprodid = productIdArray;
};
<!--GOOGLE TAG PARAM INITIALIZATION-->
var google_tag_params = {
ecomm_prodid: googleprodid, 
ecomm_pagetype: pagetype,
ecomm_totalvalue: googleprice
};
</script>


<!-- Google Code for Remarketing Tag -->
<script type="text/javascript">
/* <![CDATA[ */
var google_conversion_id = 1061500509;
var google_custom_params = window.google_tag_params;
var google_remarketing_only = true;
/* ]]> */
</script>
