/*增加的首页切换*/
$(document).ready(function(){

	  	$('.nydt').each(function(i){
		$('.nydt').each(function(index,element){
			$(this).bind('mouseover',function(){ 
				$('.nydt').removeClass('qh');
				$('.nydt').eq(index).addClass('qh');
				$('.xnxw').removeClass('dsb');
				$('.xnxw').eq(index).addClass('dsb');
			});
		});
	});
	
	$('.nykj').each(function(i){
		$('.nykj').each(function(index,element){
			$(this).bind('mouseover',function(){ 
				$('.nykj').removeClass('nj');
				$('.nykj').eq(index).addClass('nj');
				$('.njkp').removeClass('dsb');
				$('.njkp').eq(index).addClass('dsb');
			});
		});
	});
	
	$('.qyzt').each(function(i){
		$('.qyzt').each(function(index,element){
			$(this).bind('mouseover',function(){ 
				$('.qyzt').removeClass('lt');
				$('.qyzt').eq(index).addClass('lt');
				$('.qync').removeClass('dsb');
				$('.qync').eq(index).addClass('dsb');
			});
		});
	});
	
});
	</script>
