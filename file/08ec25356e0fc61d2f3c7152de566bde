  <script>
    $(document).ready(function() {
      var page = '2';
      $('.loadmore').on('click',function(){
        if( $(this).html() == "点击加载更多...")
        {
          $("#loading").show();
          $.ajax({
            type:'POST',
            url:'/forum/posts/detail_comments?pid='+'2467339'+'&page='+page+',
            data:{'page':page,'pid':2467339},
            success:function(s){
              eval("var dataobj=" + s + "");
              var html = '';
              if (dataobj.code == 1) {
                $.each(dataobj.msg, function(key, val){
                  html += '<li><div class="user"><i class="i-cmt pop_up"></i><img class="user-logo" src="'+val['avatar_src']+'" alt=""><div class="user-info"><div class="user-name">'+val['nickname'];
                  if(val['uid'] == 9320387){
                    html += '<i class="user-owner">楼主</i>';
                  }
                  html += '<div class="user-time">'+val['add_time']+'</div></div></div></div>';
                  html += '<div class="apply-cnt">'+val['message'];
                  html += '</div>';
                  html += '</div>';
                  if(val['reply_list'].length > 0){
                    html +='<ul class="apply-sub" style="height:98px" name="content_box" id="'+val['pid']+'">';
                    $.each(val['reply_list'], function(k, v){
                      html += '<li>';
                      if(v['rrid'] != 0){
                        html += '<span class="apply-sub-user">'+v['nickname']+'</span>&nbsp;评论&nbsp;<span class="apply-sub-user">'+v['rnickname']+'</span>：'+v['message'];
                      }else{
                        html += '<span class="apply-sub-user">'+v['nickname']+'</span>：'+v['message'];
                      }
                      html += '</li>';
                    });
                    html += '</ul>';
                    if(val['reply_list'].length > 3){
                      html +='<div class="apply-sub-more highlight" name="'+val['pid']+'" sid="'+val['reply_list'].length+'" fid="1"><span>查看全部'+val['reply_list'].length+'条评论</span></div>';
                    }
                  }
                  html +='</li>';
                });
                $('#commet_box').append(html);
                $("#loading").hide();
                page++;
              }else{
                var html = '<div class="loadmore">没有更多了</div>';
                $('.loadmore').html(html);
                $("#loading").hide();
              }
              $('.apply-sub-more').unbind();
              $('.apply-sub-more').on('click',function() {
                if ($(this).attr('fid') == 1) {
                  var l = $(this).attr('sid');
                  $(this).html("<span>收起全部" + l + "条评论</span>");
                  contentBox = document.getElementById($(this).attr('name'));
                  contentBox.style.height = "auto";
                  $(this).attr('fid',"2");
                }else{
                  var l = $(this).attr('sid');
                  $(this).html("<span>查看全部" + l + "条评论</span>");
                  contentBox = document.getElementById($(this).attr('name'));
                  contentBox.style.height = "98px";
                  $(this).attr('fid',"1");
                }

              });
            }
          });
        }
      });
      $('.apply-sub-more').on('click',function() {
        if ($(this).attr('fid') == 1) {
          var l = $(this).attr('sid');
          $(this).html("<span>收起全部" + l + "条评论</span>");
          contentBox = document.getElementById($(this).attr('name'));
          contentBox.style.height = "auto";
          $(this).attr('fid',"2");
        }else{
          var l = $(this).attr('sid');
          $(this).html("<span>查看全部" + l + "条评论</span>");
          contentBox = document.getElementById($(this).attr('name'));
          contentBox.style.height = "98px";
          $(this).attr('fid',"1");
        }

      });
      var toArray = function (arrayLikeObject) {
        return [].slice.call(arrayLikeObject);
      };

      var applyMore = document.querySelectorAll('.pop_up')
          , mask = document.getElementById('mask')
          , close = document.getElementById('close')
          , wrap = document.getElementById('wrap')
          , flag = true
          ;

      for (var i = 0; i < applyMore.length; i++) {
        applyMore[i].addEventListener('click', function () {
          mask.style.display = "block";
          wrap.style.height = "100%";
          wrap.style.overflow = "hidden"
        })
      }
      ;

      close.addEventListener('click',function(){
        mask.style.display = "none";
        wrap.style.height = "auto";
        wrap.style.overflow = "auto";
      })

      // 查看全部评论
      /*
      box4.onclick = function () {
        if (flag) {
