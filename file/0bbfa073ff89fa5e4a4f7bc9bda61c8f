//----滚动品牌-----

// 品牌个数
var manuTotal = $('.brandList').children('li').size();
// 每个品牌宽度 
var perWidth = 130;
// 总共可偏移宽度
var manuAllWidth = - (manuTotal-7) * perWidth;
// 初始化margin-left值
var margin = 0; 
// 初始化左右是否可点
var couldClick = true;
// 每次移动的宽度
var oneClickMove = 5 * perWidth; 

// 右移
$('.brandR').click(function () {
    // 当前偏移量(如-800) > (如-1000：总可偏移量)
    if (margin > manuAllWidth && couldClick) { 
        couldClick = false;
        // 当前偏移量
        var curMargin = parseInt($('.brandList').css('margin-left')); 
        // 较当前偏移量再偏移多少
        if ((curMargin - manuAllWidth)  < oneClickMove) {
            var moveWidth = curMargin - manuAllWidth;
        } else {
            var moveWidth = oneClickMove;
        }
        // 新偏移量
        var newMargin = (parseInt(curMargin) - parseInt(moveWidth));  
        $('.brandList').animate({'margin-left':newMargin + 'px'}, "fast", function () {
            couldClick = true;
        }); 
        margin -= moveWidth;  
    } 
});
// 左移
$('.brandL').click(function () {
    if (margin < 0 && couldClick) {
        couldClick = false;
        // 当前偏移量
        var curMargin = parseInt($('.brandList').css('margin-left')); 
        // 较当前偏移量再偏移多少 
        if ((curMargin  + oneClickMove) > 0) {
            var moveWidth = Math.abs(curMargin); 
        } else {
            var moveWidth = oneClickMove;
        }
        // 新偏移量
        var newMargin = (parseInt(curMargin) + parseInt(moveWidth));  
        $('.brandList').animate({'margin-left':newMargin + 'px'}, "fast", function () {
            couldClick = true;
        }); 
        margin += moveWidth; 
    }  
});



//倒计时
;(function($){
    $.fn.extend({
        Countdown: function(start, end) {
            var TIMEER,
            	$this = $(this),
                start_time = start.getTime(),end_time = end.getTime(),
                _tempTime,day,hour,minute,sec,
                $day = $this.find('.day'),$hour = $this.find('.hour'),$minute = $this.find('.minute'),$sec = $this.find('.sec');
	        if(start_time>end_time){return false;}
	        TIMEER = setTimeout(timing,1000);
	        function timing(){
	        	  clearTimeout(TIMEER);
		          end_time-=1000;
		          if(end_time<start_time){$this.addClass('timeover');return false;}
		          _tempTime = end_time - start_time;
		          day = Math.floor(_tempTime/86400000);
		          _tempTime -= day*86400000;
		          hour = Math.floor(_tempTime/3600000);
		          _tempTime -= hour*3600000;
		          minute = Math.floor(_tempTime/60000);
		          _tempTime -= minute*60000;
		          sec = Math.floor(_tempTime/1000);
		          if(Number(hour)<10){hour='0'+hour;}
		          if(Number(minute)<10){minute='0'+minute;}
		          if(Number(sec)<10){sec='0'+sec;}
		          $day.text(day);
		          $hour.text(hour);
		          $minute.text(minute);
		          $sec.text(sec);
		          TIMEER = setTimeout(timing,1000);
	        }
            return this;
        }
    });
})(jQuery);
 
</script>
