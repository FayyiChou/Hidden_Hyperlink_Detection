<script type="text/javascript">
//列表li滑过加灰色边框
$(function () {
	$('.goodsinfo').hover(function () {
	    $(this).addClass('active');
	}, function () {
		$(this).removeClass('active');
	});
});
//倒计时
(function($){
    $.fn.extend({
        Countdown: function(start, end) {
            var TIMEER,
            	$this = $(this),
                start_time = start.getTime(),end_time = end.getTime(),
                _tempTime,day,hour,minute,sec,
                $day = $this.find('.day'),$hour = $this.find('.hour'),$minute = $this.find('.minute'),$sec = $this.find('.sec');
	        if(start_time>end_time){return false;}
	        TIMEER = setTimeout(timing,1000);
	        function timing(){
	        	  clearTimeout(TIMEER);
		          end_time-=1000;
		          if(end_time<start_time){$this.addClass('timeover');return false;}
		          _tempTime = end_time - start_time;
		          day = Math.floor(_tempTime/86400000);
		          _tempTime -= day*86400000;
		          hour = Math.floor(_tempTime/3600000);
		          _tempTime -= hour*3600000;
		          minute = Math.floor(_tempTime/60000);
		          _tempTime -= minute*60000;
		          sec = Math.floor(_tempTime/1000);
		          if(Number(hour)<10){hour='0'+hour;}
		          if(Number(minute)<10){minute='0'+minute;}
		          if(Number(sec)<10){sec='0'+sec;}
		          $day.text(day);
		          $hour.text(hour);
		          $minute.text(minute);
		          $sec.text(sec);
		          TIMEER = setTimeout(timing,1000);
	        }
            return this;
        }
    });
})(jQuery);

</script>
