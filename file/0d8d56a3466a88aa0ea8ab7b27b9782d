                    form:$(".searchcontent form"),
                    init:function(){
                        var box = this;
                        box.target.click(function(){
                            box.ul.slideToggle(150);
                        });
                        $("li",box.ul).click(function(){
                            var _this = $(this),
                                _index = _this.index();
                            if(_this.hasClass("on"))return;
                            _this.addClass("on").siblings("li").removeClass("on");
                            box.target.text(_this.text());
                            box.form.hide().eq(_index).show();
