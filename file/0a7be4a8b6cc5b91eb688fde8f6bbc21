        $("#jd_date").change(function() {
                 var now_day = getDayStr(0);
                 var time_t = $("#jd_date").val();
                 if ("undefined" == time_t || '' == time_t) {
                     time_t = now_day;
                 }
                 $("#start_time").val(time_t);
                 var l_d = $("#end_time").val();
                 var d_time = l_d.split('-');
                 var li = new Date(d_time[0], d_time[1]-1, d_time[2]);
                 var l_time = time_t.split('-');
                 var tmp = new Date(l_time[0], l_time[1]-1, l_time[2]);
                 var is_f = false;
                 if (tmp.getTime() >= li.getTime())
                 {
                     is_f = true;
                 }
                 tmp = new Date(tmp.getTime()+1 * 24 * 60 * 60 * 1000);
                    $("#ld_date").datepicker('option','minDate', new Date(tmp.getFullYear(),tmp.getMonth(),tmp.getDate()));
                    if (is_f) {
                        var dy = tmp.getDate();
                        var mt = tmp.getMonth() + 1;
                        if (dy < 10) {
                            dy = '0'+dy;
                        }
                        if (mt < 10) {
                            mt = '0'+mt;
                        }
                        $("#ld_time").html(mt+'-'+dy);
                        $("#ld_week").html('&#40;'+transforWeek(tmp)+'&#41;');
                        $("#ld_date").datepicker('option', 'currentText', tmp.getFullYear()+'-'+mt+'-'+dy);
                        $("#end_time").val(tmp.getFullYear()+'-'+mt+'-'+dy);
                    }
                 $("#jd_time").html(l_time[1]+'-'+l_time[2]);
                 if (now_day == time_t) {
                     $("#jd_week").html("&#40;今天&#41;");
                 } else {
                     $("#jd_week").html("&#40;"+transforWeek(time_t)+"&#41;");
                 }
                 var end = $('end_time').val();
                 call_hotel(time_t, end);
             });
             $("#ld_date").change(function() {
                 var now_day = getDayStr(1);
                 var time_t = $("#ld_date").val();
                 if ("undefined" == time_t || '' == time_t) {
                     time_t = now_day;
                 }
                 $("#end_time").val(time_t);
                 var l_time = time_t.split('-');
                 $("#ld_time").html(l_time[1]+'-'+l_time[2]);
                 $("#ld_week").html("&#40;"+transforWeek(time_t)+"&#41;");
                 var start = $('start_time').val();
                 call_hotel(start, time_t);
             });
             function call_hotel(start, end) {
    var kid = $("#kid").val();
    var elongid = $("#elongid").val();
    var elongcityid = $("#elongcityid").val();
    var url = '/biz.d';
    var param = 'm=hotellist&bid=1227135465-421816583&kid='+kid+
            '&elonghotelid='+elongid+'&elongcityid='+elongcityid+
            '&checkInDate='+start+'&checkOutDate='+end;
    $.ajax({
        url:url,
        data:param,
        dataType:'html',
        success:function(data) {
            $("#ab_d_sj_jd").html(data);
        }
    });
}
function transforWeek(date) {
    var day = new Date(Date.parse(date));
    var wdays = Array('星期日','星期一','星期二','星期三','星期四','星期五','星期六');
    return wdays[day.getDay()];
}
function getDayStr(count) {
    var d = new Date();
    d.setDate(d.getDate() + count);
var vYear = d.getFullYear();
var vMon = d.getMonth() + 1;
var vDay = d.getDate();
return vYear+'-'+(vMon<10 ? "0" + vMon : vMon)+'-'+(vDay<10 ? "0"+ vDay : vDay);
}
