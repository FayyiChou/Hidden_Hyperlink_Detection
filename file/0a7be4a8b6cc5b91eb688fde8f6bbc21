	function showBg() {
        var bh = $('body').height()+100;
        var bw = $('body').width();
        $("#fullbg_comm").css({
            height:bh,
            width:bw,
            display:"block"
        });
        //$("#diolog").show();
        $("#diolog").css('visibility', 'visible');
        $("#diolog").css('top', '50%');
    }
 function closeBg() {
        $("#diolog").css('top','-1000px');
        $("#diolog").css('visibility', 'hidden');
        $("#fullbg_comm").hide();
    }
	
