	});
}	

function baidu_afs_request_done(datas, format) {
	if (typeof datas == "undefined" || typeof format === 'undefined') return;
	var length = datas.length;
	if (length <= 0) return;
	var str = '';
        var picid_idx = 0;
        var picid_length = bd_adv_picids.length;
	var i, data, item, result = [], picid = '';
	var div_idx = 0;
	for (i = 0; i < length; i++) {
		var data = datas[i];
                var picid = bd_adv_picids[picid_idx % picid_length] || '';
		var picurl = "";
		if (picid != '') {   
			picurl = "http://img0.aibangjuxin.com/ipic/"+picid+"_8.jpg";
                        picid_idx++;
		}
                else {
                    picurl = "/images/common/default_pic_small.png";
                }
                var is_blocked = false;
                for (var j = 0; j < block_word.length; j++) {
                    if (data.tit.indexOf(block_word[j]) >= 0) {
                        is_blocked = true;
                        break;
                    }
                }
                if (is_blocked) {
                    continue;
                }
		var item = {
			'title':data.tit.substr(0, 9),
			'desc':data.desc,
			'url':data.curl,
			'surl':data.surl,
                        'picurl':picurl,
                        'display':'none'
		};
		if(typeof format[div_idx] === 'object')
		{
                        if (result.length == 0){
                            item['display'] = 'block';
                        }
                        item['divid'] = format[div_idx].divid;
                        item['index'] = result.length || '0';
			result.push(_templateRender(bd_adv_tpl_list[format[div_idx]['tpl']], item));

			if (format[div_idx]['cnt'] <= result.length || i == length - 1) {
                            if ('function' === typeof format[div_idx]['callback']){
                                format[div_idx]['callback'](result.join(""));
                            }else {
                                var obj = document.getElementById(format[div_idx]['divid']);
                                if (obj) {
                                    obj.innerHTML = result.join("");
                                }
                            }
                            
                            result = [];
                            div_idx++;
                       }
		} else {
			break;
		}
                if (i == length - 1 && 'object' === typeof format[div_idx]) {
                    i = -1;
                }
	}

}
// [{'divid':'','cnt':'','tpl':..},{}...
function callback_preappend_to(html, div) {
    var div_set = jQuery('#'+div);
    if (div_set) {
        jQuery(html).insertBefore(div_set);
    }
}

function callback_append(html, div) {
    var div_set = jQuery('#'+div);
    if (div_set) {
        div_set.append(html);
    }
}
function callback_prepend(html, div) {
    var div_set = jQuery('#'+div);
    if (div_set) {
        div_set.prepend(html);
    }
}
function bizlist_baidu_afs_done(datas) {
	baidu_afs_request_done(datas, [
	{'divid':'cont_list','cnt':'1','tpl':'bizlist',
        'callback':function(html){callback_prepend(html, 'cont_list');}},
	{'divid':'page_clear','cnt':'2','tpl':'bizlist',
        'callback':function(html){callback_preappend_to(html,'page_clear');}}
	]);
}

function bizdetail_baidu_afs_done(datas) {
    baidu_afs_request_done(datas, [
        {'divid':'baidu_adv','cnt':'2','tpl':'bizdetail',
        'callback':function(html){callback_append(html, 'baidu_adv');}},
        {'divid':'baidu_adv','cnt':'3','tpl':'bizdetail1',
        'callback':function(html){
            if (!html.length) {
                jQuery('#show_more_adv').css('display', 'none');
                return;
            }
            jQuery('#baidu_adv_div').show();
            callback_append(html,'baidu_adv');
            jQuery('#show_more_adv').click(function(){
                var text = jQuery('#show_more_adv a').html();
                if (text == '查看更多') {
                    jQuery('.hide_baidu_adv').css('display', 'block');
                    jQuery('#show_more_adv a').removeClass('adv_show');
                    jQuery('#show_more_adv a').addClass('adv_hide');
                    jQuery('#show_more_adv a').html('收起');
                } else {
                    jQuery('.hide_baidu_adv').css('display', 'none');
                    jQuery('#show_more_adv a').removeClass('adv_hide');
                    jQuery('#show_more_adv a').addClass('adv_show');
                    jQuery('#show_more_adv a').html('查看更多');
                }
            });
        }}
    ]);
}
