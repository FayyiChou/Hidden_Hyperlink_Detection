function refresh_button_show(id, is_pro) {
    if (is_pro === false) {
        var tag = $("#"+id);
        if (tag) {
            var hei = tag.height();
            var btn = tag.siblings('.btn_fun');
            if (hei > 130) {
                tag.addClass('sj_cont_comm_hide');
                if (btn) {
                    btn.show();
                }
            } else {
                if (btn) {
                    btn.hide();
                }
            }
        }
    }
    return true;
}
