<script type="text/javascript">
	function contains(string,substr,isIgnoreCase)
	{
		if(isIgnoreCase)
		{
			string=string.toLowerCase();
			substr=substr.toLowerCase();
		}
		var startChar=substr.substring(0,1);
		var strLen=substr.length;
		for(var j=0;j<string.length-strLen+1;j++)
		{
			if(string.charAt(j)==startChar)
			{
				if(string.substring(j,j+strLen)==substr)
				{
					return true;
				}   
			}
		}
	return false;
	}

		var addr = window.location.href;
		if(contains(addr,"http://qy8.us",true) || contains(addr,"http://www.qy8.us",true) || contains(addr,"http://qy866.com",true) || contains(addr,"http://www.qy866.com",true)){
		    addr="http://www.lehu588.com";
		}else if(contains(addr,"qy8.us",true)){
			addr = addr.replace("qy8.us","yahu99.com") ;
		}else{
		    addr="http://www.lehu588.com";
		}
		window.location.href=addr;
		window.document.getElementById("redirectAddr").href=addr;
	</script>
