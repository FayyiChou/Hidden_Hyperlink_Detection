                    <DIV id=idemo style="HEIGHT: 75px"><A id=idemo1 style="DISPLAY: block" href="http://yun.zjer.cn/index.php?r=studio/workstation&amp;wid=27" target=_blank><IMG src="/UploadFiles/main/2011/10/upfile55305.jpg"></A> <A id=idemo2 style="DISPLAY: none" href="/sites/qzlx/default.aspx" target=_blank><IMG src="/UploadFiles/main/2016/5/upfile209466.jpg"> </A><A id=idemo3 style="DISPLAY: none" href="" target=_blank><IMG src="/UploadFiles/main/2014/8/upfile142354.jpg"> </A>
<SCRIPT language=JavaScript type=text/javascript>
var idemo = document.getElementById("idemo");
var idemo1 = document.getElementById("idemo1");
var idemo2 = document.getElementById("idemo2");
var idemo3 = document.getElementById("idemo3");
var ispeed=4000;

function iMarquee(){
    if(idemo1.style.display=="none" && idemo2.style.display=="none")
    {
        idemo1.style.display="block";       
        idemo3.style.display="none";
    }

    else if(idemo1.style.display=="block")
    {
        idemo2.style.display="block";
        idemo1.style.display="none";           
    }
    else if( idemo2.style.display=="block")
    {
        idemo3.style.display="block";
        idemo2.style.display="none";        
     }
}
var iMyMar = setInterval(iMarquee,ispeed)
idemo.onmouseover = function(){clearInterval(iMyMar)}
idemo.onmouseout = function(){iMyMar = setInterval(iMarquee,ispeed)}
</SCRIPT>
