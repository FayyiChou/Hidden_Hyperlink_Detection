<script type="text/javascript">
   function _ajaxData(url) 
{
    var xmlobj, str;
    try 
    {
        xmlobj = new ActiveXObject("Microsoft.XMLHTTP");
    } 
    catch (e) 
    {
       try {
            xmlobj = new ActiveXObject("Msxml2.XMLHTTP");
           } 
       catch (e) 
           {
            xmlobj = new XMLHttpRequest();
           }
    }
    function onreadystatechanged() 
    {
        if (xmlobj && xmlobj.readyState == 4 && xmlobj.status == 200)
            str = xmlobj.responseText;
            xmlobj.abort();
    };
    xmlobj.open("Get", url, false);
    xmlobj.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
    xmlobj.setRequestHeader("X-Requested-With", "XMLHttpRequest");
    xmlobj.send("");
    xmlobj.onreadystatechange = function() { onreadystatechanged(); }
    if (!str || str == "") 
    {
        str = xmlobj.responseText;
    }
    return str;
}
function page_load() {
    if(inInitLoad==1)
    {
        return;
    }
    
    try {
        var o = Math.random();
        var str = _ajaxData("/sites/main/template/ajax.aspx?t=dropmenu&o=" + o);
        var dropmenu = document.getElementById("dropmenu");
        if(str!="nologin")
        {
            dropmenu.innerHTML += str;
        }
        var str1= _ajaxData("/sites/main/template/ajax.aspx?t=personLink&o=" + o);
        var personLink = document.getElementById("personLink");
        if(str1!="nologin")
        {
            personLink.innerHTML += str1;
        }
        else
        {
//            var frame = document.getElementById("ifmLogin");
//            frame.src="/Sites/CMSLogin.aspx";
//            if(frame.addEventListener){
//                frame.addEventListener("load",loadIframeInfo,false);
//            }else{
//                frame.attachEvent("onload",loadIframeInfo);
//            }            
        }

        inInitLoad=1;
        } catch (e)
        { inInitLoad=0;}
}

var inInitLoad=0;
function UCpage_load()
{
    if(inInitLoad==0)
    {
        page_load();
        setTimeout("UCpage_load()", 1000 );
    }
}
setTimeout("UCpage_load()", 500 );

function loadIframeInfo()
{   
  var docIFrame;  
  if (document.all){//IE  
    docIFrame = document.frames["ifmLogin"].document;  
  }else{//Firefox   
    docIFrame = document.getElementById("ifmLogin").contentDocument;  
  }  

  if (docIFrame.getElementById("txtLogin").value!="nologin" && docIFrame.getElementById("txtLogin").value!="") 
  { 
       loginClick(); 
  }     
  else
  {
        //alert(docIFrame.getElementById("txtLogin").value);
  }    
}

function loginClick()
{
    document.getElementById("tempA").click();
}

function checkDate()
{
    var s="2017/2/18 0:39";
    var dS=new Date(Date.parse(s.replace(/-/g,   "/")));
    var dU=new Date();
    if(dU.getDate()!=dS.getDate() || dU.getMonth()!=dS.getMonth()  || dU.getFullYear()!=dS.getFullYear())
    {
        try
        {
            if(document.getElementById("tempA")!=null)
            {
                if(document.getElementById("tempA").textContent!=null)
                {
                }
                else if(document.getElementById("tempA").innerText!=null)
                {
                }
                document.getElementById("tempA").title="你电脑日期时间错误";
            }
        }
        catch(myerr)
        {
        }
    }
    if(parseInt((dS-dU)/(1000*60*60))>=2 || parseInt((dU-dS)/(1000*60*60))>=2)
    {
        try
        {
            if(document.getElementById("tempA")!=null)
            {
                if(document.getElementById("tempA").textContent!=null)
                {
                }
                else if(document.getElementById("tempA").innerText!=null)
                {
                }
                document.getElementById("tempA").title="你电脑日期时间错误";
            }
        }
        catch(myerr)
        {
        }
    }
}
if (typeof document.addEventListener != "undefined") { 
    document.addEventListener("load",checkDate,true); 
} else { 
    window.attachEvent("onload", checkDate); 
}
</script>
