        <div id='FloatAdv' style='position:absolute; z-index:99;width:100px;right:5px;height:214px;'><div style='width:100px;background:white;height:20px;line-height:20px;'><a href='javascript:closead();' title='关闭'>×关闭</a></div><a href='http://study.gsjy.net/zsjypt/index.action' target='_blank'><img style=width:100px;' alt='2014年1月1日运行使用' src='/UploadFiles/main/2014/3/2014030310154817.jpg'/></a></div> 
<script language='javascript'>
var advdiv=document.getElementById('FloatAdv');
var ypos=document.documentElement.clientHeight-50-214;
advdiv.style.top=ypos+'px';
var lastScrollY=0;
function changePosition()
{ diffY=document.documentElement.scrollTop;
percent=.1*(diffY-lastScrollY);
if(percent>0)
percent=Math.ceil(percent);
else 
percent=Math.floor(percent);
 advdiv.style.pixelTop+=percent;
 lastScrollY=lastScrollY+percent; 
}
 function closead(){
 document.getElementById('FloatAdv').style.display='none';
}
setInterval('changePosition()',1); 
</script> 
      
