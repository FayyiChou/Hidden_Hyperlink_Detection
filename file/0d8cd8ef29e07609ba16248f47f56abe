<script>
    var pageUrlPath = window.location.pathname;
    var pageUrl = window.location.href;
    var homePage = (pageUrlPath=='/' || pageUrlPath=='/index.php' || pageUrlPath=='/index.html');
    var productPage = (jQuery('.product_page').size() > 0);
    var categoryPage = (jQuery('.product_list').size() > 0);
    var searchPage = (pageUrl.match(/\?q=/) != null);
    var cartPage = (pageUrlPath == '/basket/');
    var purchasePage = (pageUrl.match(/\?ORDER_ID/) != null);
    if(homePage) {
        var google_tag_params = {
            ecomm_pagetype: 'home'
        };
    }else if(productPage) {
        var google_tag_params = {
            ecomm_prodid: jQuery('.product_page .pp_cart_row').data('id'),
            ecomm_pagetype: 'product',
            ecomm_totalvalue: Number(jQuery('.product_page .pi_price').data('price'))
        };
    }else if(categoryPage) {
        var ids = [];
        jQuery('.product_list .product_item .btn').each(function(){ids.push(jQuery(this).data('id'))});
        var val = 0;
        jQuery('.product_list .product_item .pi_price').each(function(){ val = val + Number(jQuery(this).text().match(/(\d+)(\.\d{2})*/)[0])});
        var google_tag_params = {
            ecomm_prodid: ids,
            ecomm_pagetype: 'category',
            ecomm_totalvalue: val
        };
    }else if(searchPage) {
    var ids = [];
    jQuery('.product_list .product_item .btn').each(function(){ids.push(jQuery(this).data('id'))});
    var val = 0;
    jQuery('.product_list .product_item .pi_price').each(function(){ val = val + Number(jQuery(this).text().match(/(\d+)(\.\d{2})*/)[0])});
    var google_tag_params = {
        ecomm_prodid: ids,
        ecomm_pagetype: 'searchresults',
        ecomm_totalvalue: val
    };
    }else if(purchasePage) {
var prodId = prodId || '';
var totalValue = totalValue || '';
        var google_tag_params = {
			ecomm_prodid: prodId,
            ecomm_totalvalue: totalValue,
            ecomm_pagetype: 'purchase'
        };
    }else if(cartPage) {
          var google_tag_params = {
            ecomm_prodid: ids,
            ecomm_pagetype: 'cart',
            ecomm_totalvalue: Number(jQuery('.co_total b:contains("руб")').text().replace(/\s|руб\./g, ''))
        };
    }else {
        var google_tag_params = {
            ecomm_pagetype: 'other'
        };
    }
</script>
