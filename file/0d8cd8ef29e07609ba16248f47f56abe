(function($){
	clTrack('div:contains("Добавить в корзину")');
	function clTrack(e,d){var d=d||!1;$("html").on({click:function(e){
    var c= e.target.innerText? e.target.innerText: e.target.value,
        a= e.type,
        l= e.target.baseURI + ' ' + e.target.formAction,
