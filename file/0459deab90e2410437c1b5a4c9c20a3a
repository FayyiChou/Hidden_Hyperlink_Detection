		else
		{
			$("#queryResult").empty();
			$("#queryResult").hide();
			$("#errorInfo").empty()
			var errorInfo = "";
			errorInfo += '<dt>&nbsp;</dt>';
			errorInfo += '<dd>';
			errorInfo += '<div class="notfound">';
			if(value.info=='websiteerror')
			{
				errorInfo += '<h4>网站维护中...</h4>';
			}
			else
			{
				errorInfo += '<h4>'+value.info+'</h4>';
			}
			errorInfo += '</div>';
			errorInfo += '</dd>';
			
			$("#errorInfo").append(errorInfo);
			$("#errorInfo").show();
		}
		$(".ret_box").fadeIn();
		$('#sch_form').find('i').eq(0).hide();
	},
	error:function()
	{ 
		alert("服务器忙，请稍后重试");
	}
	});
}

var captchaFlag = true;
function getCaptcha()
{
if(!captchaFlag)
{
	return;
}
$.ajax({
	type:'post',
	url: "index.php?r=appleinfo/getCaptcha",
	data:{},
	dataType:"json",
	cache: false,
	async: true,
	beforeSend:function()
	{
		captchaFlag = false;
	},
	complete:function()
	{
		captchaFlag = true;
	},
	success: function(value) 
	{  
		if(value.success=='1')
		{
			$("#verification_code").attr('src', value.info.captcha);
			$("#timestamp").val(value.info.timestamp);
		}
		else
		{
			alert(value.info);
		}
		$('#sch_form').find('i').eq(0).hide();
	},
	error:function()
	{ 
		alert("服务器忙，请稍后重试");
	}
});
}
</script>
