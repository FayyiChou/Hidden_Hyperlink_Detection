var sn_no = '';
var app_key = '';
var disabledb = '0';
jQuery(function(){

var timestamp = $.trim($("#timestamp").val());
if(timestamp.length == 0)
{
	$("div.verification-code").hide();
}else
{
	$("div.verification-code").show();
}

var 
form = $('#sch_form'),
label = form.find('label').eq(0),
label2 = $('.verification-code').find('label').eq(0),
txtField = form.find('input').eq(0),
txtField2 = $('.verification-code').find('input').eq(0);
txtField.focus(function(){
	label.hide();
})
.blur(function(){
	txtField.val() == '' && label.fadeIn();
});
txtField2.focus(function(){
	label2.hide();
})
.blur(function(){
	txtField2.val() == '' && label2.fadeIn();
});

form.submit(function(e){
	e.preventDefault();
	form.find('i').eq(0).show();
	//some coding for post to search
	querySn();
	
});

if(sn_no != undefined && sn_no != "")
{
	$("#sch_txt").focus();
	//$(".apple_info form label").hide();
	label.hide();
	$("#sch_txt").val(sn_no);
	form.find('i').eq(0).show();
	//some coding for post to search
	//var timestamp = $.trim($("#timestamp").val());
	/*if(timestamp.length == 0)
	{
		getCaptcha();
	}else
	{
		querySn();
	}*/
	querySn();
}

//counter
var 
cHtml = '',
counter = $('#num_inner'),
countTxt = '' + counter.html();
$.each(countTxt.split(''), function(i, n){
	cHtml += '<i class="n' + (n === ',' ? 'n': n) + '"></i>';
});
counter.html(cHtml);

$("a.verification").on('click',function(){
	getCaptcha();
});
});


var completeFlag = true;
function querySn()
{
if(!completeFlag)
{
	return;
}
$("#sn_no").html($("#sch_txt").val());
