	type:'post',
	url: "index.php?r=appleinfo/search",
	data:{'sn':$("#sch_txt").val(),'app_key':app_key,'disabledb':disabledb,'timestamp':$("#timestamp").val(),'captcha':$("#sch_code").val()},
	dataType:"json",
	cache: false,
	async: true,
	beforeSend:function()
	{
		completeFlag = false;
	},
	complete:function()
	{
		completeFlag = true;
	},
	success: function(value) 
	{  
		if(value.success=='1')
		{

			//if(value.info == '获取验证码成功')
			if(typeof value.info.captcha != undefined && value.info.captcha != null)
    		{
				$("#verification_code").attr('src', value.info.captcha);
				$("#timestamp").val(value.info.timestamp);
				$("div.verification-code").show();
        	}else{
			
			$("#queryResult").empty();
			$("#errorInfo").empty()
			$("#errorInfo").hide();
			var info = value.info;
			var html = "";
			if(info.PROD_IMAGE_URL != '')
			{
    			html +='<dt>';
    			html +='<img src="'+info.PROD_IMAGE_URL+'" alt="" height="120" width="120" />';
    			html +='</dt>';
			}
			if(info.PART_DESCR != '')
			{
    			html +='<dd>';
    			html +='<label for="opt1">设备型号：</label><span id="opt1">'+info.PART_DESCR+'</span>';
    			html +='</dd>';
			}
			if(info.SERIAL_ID != '')
			{
    			html +='<dd>';
    			html +='<label for="opt2">序列号：</label><span id="opt2">'+info.SERIAL_ID+'</span>';
    			html +='</dd>';
			}
