<script type="text/javascript">
$(".xingqu .cha").click(function(){
	location.reload();
})
$(".xingqu li").click(function(){
	$(this).find("i").toggleClass("on");
	if($(".xingqu li").find(".on").length >= 3){
		$(".liji-dl .lijidl").text("启动兴趣引擎");
	}else{
		$(".liji-dl .lijidl").text("选择3个兴趣标签，启动兴趣引擎");
	}
})
$('.lijidl').click(function(){
    if($(".xingqu li").find(".on").length < 3){
        //tanc2();
        return false;
    } else {
        //保存兴趣标签
        var ids = [];
        $(".xingqu ul li").each(function(){
            if($(this).find("i").hasClass("on")){
                ids.push($(this).attr("id"));
            }
        });
        $.getJSON("http://m.my.lotour.com/user/saveInterests?callback=?&ids=" + ids, function(result){
        	location.reload();
        });
    }
})
$('.login_coin').on('click',function(){
    $(this).toggleClass('on');
    $('.out_txt,.in_txt').toggleClass('disnone');
    if($(this).hasClass("on")){
        inputId = "out_txtemail"
    }else{
        inputId = "txtemail"
    }
})
</script>

