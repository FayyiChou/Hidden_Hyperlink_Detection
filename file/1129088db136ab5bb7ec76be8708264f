var count=0;
var pic_arr=new Array("/templates/default/cn/images/banner/1.jpg","/templates/default/cn/images/banner/2.jpg","/templates/default/cn/images/banner/3.jpg");
function changebg(){
 var pic_div=document.getElementById("banner");
 pic_div.style.background="url("+pic_arr[count]+") no-repeat";
 count++;
 if(count>=3)
 {
  count=0;
  pic_div.style.background="url("+pic_arr[count]+") no-repeat";
 }
 setTimeout(changebg,5000);
}
window.onload=function(){
 changebg();
}
</script>
                    
