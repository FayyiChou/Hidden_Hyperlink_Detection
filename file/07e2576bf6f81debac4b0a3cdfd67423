<script type="text/javascript">
    var count=1;
    // 定义回调方法
    function LoginCallback(cbtype, data) {
        if (cbtype == "check") {
            // 用户名密码的检查结果
            if (!data.Success)
                alert(data.Info);
        }
        else if (cbtype == "login") {
            if (data.Success) {
                // 登录成功
                var url = '/sex/topic/1186377-4.html';
                  window.location.href = url;

              } else {
                  // 登录失败
                alert(data.Info);
                if (count >= 10) {
                    $('#SafeCodeArea').show();
                    $('#safeCodeImg').show();
                }
                count++;
              }
          }
          else if (cbtype == "error") {
              // 服务器错误
              alert("服务器错误:" + data.Info);
          }
  }
    function loginjs() {
        if ($("#SafeCodeArea").is(":hidden") == false) {
            if ($('#txtSafeCode').val().length == 0) {
                alert("验证码不能为空");
                $('#txtSafeCode').focus();
            }
            else {
                CheckSafeCode($('#txtSafeCode').val());
            }
        }
        else {
            Passport.Login($("#uname").val(), $("#pwd").val(), LoginCallback);
        }
    }

    function updateSC() {
        var src = "http://my.39.net/other/SafeCodeImg.aspx?1&ts=" + (new Date()).valueOf();
        $("#safeCodeImg").attr("src", src);
        if ($("#txtSafeCode").val() != "")
            $("#txtSafeCode").val("");
    }

    function CheckSafeCode(str) {
        jQuery.ajax({
            url: "http://my.39.net/MemberAction.aspx",
            type: "get",
            jsonp: "jsonpcallback",
            data: { FunctionName: "CheckSafeCode2", T: 1, SafeCode: str, ts: (new Date()).valueOf() },
            dataType: "jsonp",
            async: false,
            success: function (data) {
                if (data.Success) {
                    ispass = true;
                } else {
                    ispass = false;
                }
                if (!ispass) {
                    alert("验证码错误");
                    updateSC();
                }
                else {
                    Passport.Login($("#uname").val(), $("#pwd").val(), LoginCallback, $('#txtSafeCode').val());
                }
            }
        });
    }

    function useImg(url) {       
        nEditor.tinsertUBB('[img]' + url + '[/img]');
    }
