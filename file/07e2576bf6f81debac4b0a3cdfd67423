                $('#txtCaptcha').focus();
                return;
            }
        }  
        var Body = nEditor.tGetUBB();
 
        addReply(tid,Body,isforced,captchaCode)


    }
    function addReplyBak(json) {
        if (json.Success) { window.location.href = json.Redirect+"?r="+Math.random()+"#lastreply"; }
        else
        {
            if (json.TypeID==-1 && loginbox)
            {
                loginbox.Show();
            }else if (json.TypeID==-2)
            {
                loginbox.Close();
                alert("输入验证码或验证码错误。")
                $("#divCaptchaOut").show();
            }            
            else{
                document.getElementById('imgCaptcha').src='/GetCaptchaImage.aspx?rd='+Math.random(); 
                alert(json.Info);}
        }     
    }
    function clearMsgBox()
