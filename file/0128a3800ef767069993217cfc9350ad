    <div id="adimglist" style="width:100%; overflow:hidden; height:325px;">
        <div style="height:325px; width:10000px;" id="adlist">
                    
            <div class="scrollpic" style="background-image:url(../uploadfile/20121009175220452.jpg);"></div>
                    
            <div class="scrollpic" style="background-image:url(../uploadfile/20121009191223839.jpg);"></div>
                    
            <div class="scrollpic" style="background-image:url(../uploadfile/20121009193209402.jpg);"></div>
                    
            <div class="scrollpic" style="background-image:url(../uploadfile/20121009172524875.jpg);"></div>
                    
