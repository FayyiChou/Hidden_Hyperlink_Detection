<script>
$(".shows").hover(
	function(){
		$("#"+$(this).attr("rev")).show();
		$(this).find("a").eq(0).addClass("active");
	},function(){
		$("#"+$(this).attr("rev")).hide();
		if($(this).attr("rel")!="index"){
			$(this).find("a").eq(0).removeClass("active")
		}
	}
)
</script>

