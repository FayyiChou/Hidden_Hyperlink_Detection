<script>
function banner(er){
    for(ba=1;ba<7;ba++){
        rightbigimgsub=document.getElementById('rightbigimgsub'+ba);
        rightbigimg=document.getElementById('rightbigimg'+ba);
        newslefttit=document.getElementById('newslefttit'+ba);
        date=document.getElementById('date'+ba);
        if(er==ba){
            rightbigimgsub.style.bottom='0px';
            newslefttit.style.display='block';
            date.style.display='block';
            $(rightbigimg).fadeIn();
        }else{
            rightbigimgsub.style.bottom='-24px';
            newslefttit.style.display='none';
            date.style.display='none';
            $(rightbigimg).fadeOut();
        }
    }
}
var er=1;
function starad(){
    banner(er);
    er++;
    if(er==7){
        er=1;
    }
}
var speed=3000;
var scrollbox = document.getElementById("scrobox");
//定时效果
var star=setInterval(starad,speed)
scrollbox.onmouseover=function() {clearInterval(star)}
scrollbox.onmouseout=function(){star=setInterval(starad,speed)}
//定时效果
</script> 
                    
