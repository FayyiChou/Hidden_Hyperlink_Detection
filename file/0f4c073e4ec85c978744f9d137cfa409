  function switchmodTag(modtag,modcontent,modk) {
    for(i=1; i <3; i++) {
      if (i==modk) {
        document.getElementById(modtag+i).className="current";document.getElementById(modcontent+i).className="list";}
      else {
        document.getElementById(modtag+i).className=" ";document.getElementById(modcontent+i).className="list_none";}
    }
  }
