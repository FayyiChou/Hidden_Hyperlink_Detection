<script>
	var gameId = '13019';
	function bookGame() {
		$.ajax({
			url:"http://hao.pcgames.com.cn/action/bookGame.jsp",
            dataType:"json", 
            type:"post", 
            data:{gameId:gameId},
            error: function(msg){  
            	new  popBox({
	                content: '抱歉，系统繁忙，请稍后重试',
	                boxClass : 'popBox error',
	                closeDelay : 2000
            	});
            },
            success: function(json) {
            	if (json.result == -2) {
            		unLogin();
            	} else if (json.result == -1 || json.result == -3 || json.result == 0) {
            		new popBox({
            			title: '提示信息',
            			content: json.msg,
            			okButton: true,
            			okcallback:function() {
            				window.location.reload();
            			}
            		});
            	} else if (json.result == 1) {
            		$("div.gn2").html(json.html);
            		var gameBookCount = parseInt($("#gameBookCount").html()) + 1;
            		$("#gameBookCount").html(gameBookCount);
            	}
            }
		});
	}
</script>

<script>
	function unLogin() {
		new  popBox({
            title: '提示信息',
		    content: '您还没登录，请先登录',
		    okButton: true,
		    cancelButton: true,
		    okText: '马上登录',
		    focusOk: true,
		    okcallback: function(){
            	window.location.href = "http://my.pcgames.com.cn/passport/login.jsp?return=" + window.location;
		    }
        });
	}
</script>
