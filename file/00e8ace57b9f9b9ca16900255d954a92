<script>
function iFrameHeight(this_id){
	var ifm = document.getElementById(this_id);
	var subWeb = document.frames ? document.frames[this_id].document : ifm.contentDocument;
	if(ifm != null && subWeb != null) {
		ifm.height = subWeb.body.scrollHeight;
		ifm.width = subWeb.body.scrollWidth;
	}
}
</script>

