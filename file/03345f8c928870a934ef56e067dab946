<script type="text/javascript">
  var speed1=20//速度数值越大速度越慢
  document.getElementById("demo8").innerHTML=document.getElementById("demo7").innerHTML
  function Marquee1(){
  if(document.getElementById("demo8").offsetWidth-document.getElementById("demo6").scrollLeft<=0)
  document.getElementById("demo6").scrollLeft-=document.getElementById("demo7").offsetWidth
  else{
  document.getElementById("demo6").scrollLeft++
  }
  }
  var MyMar1=setInterval(Marquee1,speed1)
  document.getElementById("demo6").onmouseover=function () {clearInterval(MyMar1)}
  document.getElementById("demo6").onmouseout=function () {MyMar1=setInterval(Marquee1,speed1)}
</SCRIPT>
