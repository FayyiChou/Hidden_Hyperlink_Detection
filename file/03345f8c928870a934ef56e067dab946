<script>  
    $(function(){  
        window.onload = function()  
        {  
            var $a = $('#pinpai_tab li');  
            var $b = $('#pinpai_content ul');  
                          
            $a.hover(function(){  
                var $this = $(this);//$(this)表示把$a转换为jQuery对象，然后赋值给$this这个变量  
                var $t = $this.index();//相当于$this的兄弟元素赋值给 $t  
                $a.removeClass();  
                $this.addClass('pinpai_current');  
                $b.css('display','none');  
                $b.eq($t).css('display','block');//遍历$t  
            })  
        }  
    });  
</script>  

