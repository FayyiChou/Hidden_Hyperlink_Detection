<script type="text/javascript">
    var izvele = false;
    $(function(){
        answ = {};
        $('.answ li').click(function(){
            if (izvele == true) return;
            
            izvele = true;
            qNr = parseFloat($(this).attr('data-question'));    
            
            nextQ = qNr+1;
            
            //Aicis removed new
            //if (qNr == 5) return;
            
            answ[qNr] = $(this).attr('data-answ');

            // Saglabājam atbildes hidden laukā
            $('input[name=answ_'+qNr+']').val(answ[qNr]);
            
            // Kolbas līmenis
            $('#eliksirs .kolba_container .liquid').animate({
                top: 299-qNr*30-qNr*10  
            }, 700, "linear");
            
            if(qNr > 2) $('#eliksirs .kolba_container .liquid').css('height', '226px');
            
            var bg = $(this).css('background-image');
            bg = bg.replace('url(','').replace(')','').replace('"', '').replace('"', '');
            bgPositiong = 280-qNr*30;
            bgLeft = 270+Math.floor((Math.random()*66)+1);
            
            // Ieliekam ikonu kolbā
            $('.kolba_container').append('<img src="'+bg+'" id="'+bgPositiong+'" style="top:0px; left:'+bgLeft+'px;" />');
            $('img#'+bgPositiong).animate({ top: bgPositiong+'px'}, 600);
            
            switch (qNr){
                case 1:swing1(bgPositiong);break;
                case 2:swing2(bgPositiong);break;
                case 3:swing3(bgPositiong);break;
                case 4:swing4(bgPositiong);break;
                case 5:swing5(bgPositiong);break; //added new aicis
            }
            
            //Random angle new image
            var angle = Math.floor((Math.random()*60)+1);
            $('.kolba_container img').last().css( 'transform', 'rotate(' + angle + 'deg)' );   
            $('.kolba_container img').last().css( '-moz-transform', 'rotate(' + angle + 'deg)' );   
            $('.kolba_container img').last().css( '-webkit-transform', 'rotate(' + angle + 'deg)' );
            $('.kolba_container img').last().css( '-o-transform', 'rotate(' + angle + 'deg)' );
            
            // Ja pēdējais jautājums, rādam samaisīt pogu
            if (qNr == 2){
                $('#bubbling').fadeIn(300);
            }
            if(qNr < 5){ //old one if(qNr < 4){
                $('.question_counter').text(nextQ+'/5');
                $('*[data-question="'+qNr+'"]').fadeOut(400, function(){
                    $('*[data-question="'+(nextQ)+'"]').fadeIn();
                    izvele = false;
                });
            }else{
                //Aicis 
                $('.question_counter').text(nextQ+'/5');
                $('*[data-question="'+qNr+'"]').fadeOut(400, function(){
                    $('*[data-question="'+(nextQ)+'"]').fadeIn();
                });
                
                //last();
                
                var properties = { color : '#f36f21' };
                var el = $('.samaisit');
                el.pulse(properties, { pulses : 2000, interval : 500, duration: 1000 });
                $('.samaisit, .reset').fadeIn(250);
                $('[data-question="5"], .question_counter').fadeOut(400);
                
                //Added new aicis
                izvele = false;
            }
        });
        
        
        // Samaisīt poga, parādām rezultātu
        $('.samaisit, .reset').click(function(){
        $('.kolba_container').css('height',503+'px');
        
            $('.reset').hide();
            $('#eliksirs .questAndAnsw .samaisit').fadeOut();
 
            window.clearInterval(int1);
            window.clearInterval(int2);
            window.clearInterval(int3);
            window.clearInterval(int4);
            window.clearInterval(int5);
            window.clearInterval(int6);
            $('.kolba_container img').vibrate({ reverse:true, speed:40 });

            setTimeout(function(){
                var imgTags = $(".kolba_container img");
                
                var i1 = $('#'+id1);
                var i2 = $('#'+id2);
                var i3 = $('#'+id3);
                var i4 = $('#'+id4);
                var i5 = $('#'+id5);
                var i6 = $('#'+id6);
                
                imgTags.unwrap();
                imgTags.unwrap();
                $('.kolba_container img').remove();
                
                $('.kolba_container').append(i1);
                $('.kolba_container').append(i2);
                $('.kolba_container').append(i3);
                $('.kolba_container').append(i4);
                $('.kolba_container').append(i5);
                $('.kolba_container').append(i6);
                
                swing1(id1);
                swing2(id2);
                swing3(id3);
                swing4(id4);
                swing5(id5);
                swing6(id6);
                $('#eliksirs .questAndAnsw').fadeOut(500, function(){
                    //$('.reset').fadeOut();
                    $('#eliksirs .result').fadeIn();

                    // Json objekts no config faila
                    var obj = $.parseJSON(combo);

                    // Atrodam vērtību, balstoties uz atbildem
                    var answ1 = 'v'+$('[name=answ_1]').val();
                    var answ2 = 's'+$('[name=answ_2]').val();
                    $('.result h1 span').html(obj[answ1][answ2]['title_lv']);

                    // Atrodam attēlu, balstoties uz pirmo 3 jaut. atbildēm
                    var answ3 = $('[name=answ_3]').val();
                    var img = obj[answ1][answ2][answ3];

                    $('.result').attr('style', 'height:460px;background: url(/assets/images/eliksirs/'+img+') no-repeat right bottom;');
                });
            },5000);
                 
        });       

        $('.answ li').mouseenter(function() {
            //if ($(this).data('question') != 5)//aicis new
                $(this).addClass('active');
        }).mouseleave(function() {
            //if ($(this).data('question') != 5)//aicis new
                $(this).removeClass('active');
            
        });
        
        $('.openEleksNoteik').click(function(){
            $('#eliksirs .eliksiraNoteikumi').fadeIn();
        });
        
        $('#eliksirs .eliksiraNoteikumi .closeNoteikumi').click(function(){
            $('#eliksirs .eliksiraNoteikumi').fadeOut();
        });
    });
    
    function last(){
            setTimeout(function(){
                $('.step_5_1').addClass('step_5_1 active');
                
                // Kolbas līmenis
                $('#eliksirs .kolba_container .liquid').animate({
                    top: 299-5*30-5*10  
                }, 700, "linear");
                
                var bg = $('.step_5_1').css('background-image');
                bg = bg.replace('url(','').replace(')','').replace('"', '').replace('"', '');
                bgPositiong = 280-5*30;
                bgLeft = 270+Math.floor((Math.random()*66)+1);
                
                // Ieliekam ikonu kolbā
                $('.kolba_container').append('<img src="'+bg+'" id="'+bgPositiong+'" style="top:0px; left:'+bgLeft+'px;" />');
                $('img#'+bgPositiong).animate({ top: bgPositiong+'px'}, 600);
                
                swing5(bgPositiong);
            }, 1500);

            setTimeout(function(){
                $('.step_5_2').addClass('step_5_2 active');
                
                // Kolbas līmenis
                $('#eliksirs .kolba_container .liquid').animate({
                    top: 299-5.1*30-5.1*10  
                }, 700, "linear");
                
                var bg = $('.step_5_2').css('background-image');
                bg = bg.replace('url(','').replace(')','').replace('"', '').replace('"', '');
                bgPositiong = 280-6*30;
                bgLeft = 270+Math.floor((Math.random()*66)+1);
                
                // Ieliekam ikonu kolbā
                $('.kolba_container').append('<img src="'+bg+'" id="'+bgPositiong+'" style="top:0px; left:'+bgLeft+'px;" />');
                $('img#'+bgPositiong).animate({ top: bgPositiong+'px'}, 600);
                swing6(bgPositiong);
                
                setTimeout(function(){
                    var properties = { color : '#f36f21' };
                    var el = $('.samaisit');
                    el.pulse(properties, { pulses : 2000, interval : 500, duration: 1000 });
                    $('.samaisit, .reset').fadeIn(250);
                    $('[data-question="5"], .question_counter').fadeOut(400);
                    
                    $('.questAndAnsw h1').html('&nbsp;');
                },500);
            }, 3000);
    }
    
    var up = false;
    var int1;
    var id1;
function swing1(id){
    id1=id;
    int1 = setInterval(function(){
        if (up == false){
            up = true;
            $('#'+id).animate({"margin-top": '-15px'}, 1400);
        }else {
            $('#'+id).animate({"margin-top": '0px'}, 1400);
            up = false;
        }
        
    },1400);
}

var up2 = false;
var int2;
var id2;
function swing2(id){
    id2=id;
    int2 = setInterval(function(){
        if (up2 == false){
            up2 = true;
            $('#'+id).animate({"margin-top": '-10px'}, 1000);
        }else {
            $('#'+id).animate({"margin-top": '0px'}, 1000);
            up2 = false;
        }
        
    },1000);
}

var up3 = false;
var int3;
id3=id;
function swing3(id){
    id3=id;
    int3 = setInterval(function(){
        if (up3 == false){
            up3 = true;
            $('#'+id).animate({"margin-top": '-10px'}, 1500);
        }else {
            $('#'+id).animate({"margin-top": '0px'}, 1500);
            up3 = false;
        }
        
    },1500);
}

