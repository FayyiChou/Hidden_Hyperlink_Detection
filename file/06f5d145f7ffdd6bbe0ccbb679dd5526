		$('.popupImg').click(function(){
			var vid = '<iframe width="800" height="445" src="http://www.youtube.com/embed/Zi64bCXBeAE?rel=0" frameborder="0" allowfullscreen></iframe>';
			$('.vide2-vimeo').html(vid);
			$('.videoOverlay2 , .vide2-vimeo iframe').fadeIn();
			$('.logo').css({'z-index' : 0});
			$('.video-popup2 .close').fadeOut();
			mute();
		});
		
		$('.videoOverlay2').click(function(){
			$(".sound-off").trigger('click');			
			$('.videoOverlay2').fadeOut();
			$('.vide2-vimeo iframe').remove();
			$('.video-popup2 .close , .logo').fadeIn();
		});
		
				$('#nacBottm1').live('click', function(){
