<script type="text/javascript">
                
$(function(){
        //关闭弹出层
        $('.appPop .closeBtn').click(function(){
            deletePopupType();
            $('.appPopWrap').hide();
            $('.appPop').hide();
        });

        //关闭弹出层
        $('#apply_a').click(function(){
            deletePopupType();
            $('.appPopWrap').hide();
            $('.appPop').hide();
        });

        $("#companyType").change(function () {
            $("#companyTypeMsg").html("");
        });

        $("#workProperty").change(function () {
            $("#workPropertyMsg").html("");
        });
        initVipInfo('','');
    });
    //提交调研结果
        function sumbitSurvey() {
            //验证企业类型
            var companyType=$.trim($("#companyType").val());
            if(null==companyType||companyType==""){
                $("#companyTypeMsg").html("企业类型不能为空");
                return;
            }
            //验证工作性质
            var workProperty=$.trim($("#workProperty").val());
            if(null==workProperty||workProperty==""){
                $("#workPropertyMsg").html("工作性质不能为空");
                return;
            }
            $.ajax({
                type: "POST",
                url: "/sumbitSurvey.ajax",
                dataType: 'HTML',
                data:{companyType:companyType,workProperty:workProperty},
                success:function (data) {
                    $('.appPopWrap').hide();
                    $('.appPop').hide();
                },
                error:function () {
                    $('.appPopWrap').hide();
                    $('.appPop').hide();
                }
            });
        }

function initVipInfo(expireDate,userRole){
	/* if(userRole!="budgeter"){
		return false;
	} */
    var title=null;
    var msg=null;
	if(expireDate!=null&&expireDate!=""){
        var json=jQuery.parseJSON(expireDate);
		if(json.expireDate<getCurentDate()){
			if(!json.isComeFromApply){
				$("#img_vip").show();
				title="提示：VIP权限已于"+json.expireDate+"到期，请联系客服。";
				//msg="VIP权限已于"+json.expireDate+"到期  <a class='vipMsg'>联系客服</a>";
				msg="VIP权限已于"+json.expireDate+"到期，请联系客服";
			}else{
				if($("#vipApplyCount").val()==1){
					title="第一次VIP试用已到期,您可以再次申请试用";
					msg="第一次VIP试用已到期  <a id='a_apply' onclick='userBehavior('2119','a_apply');' href='"+basePath+"/huodong/shiyong/index.html'>再次申请试用</a>";
				}else if($("#vipApplyCount").val()==2){
					//title="第二次VIP试用已到期,您可以咨询购买";
					title="第二次VIP试用已到期,您可以在线购买";
					//msg="第二次VIP试用已到期  <a>购买咨询</a>";
					msg="第二次VIP试用已到期，<a href='http://shop.glodon.com/search?categoryId=1500'>在线购买</a>";
				}else{
					title="您好，普通用户可以申请VIP试用";
					msg="您好，普通用户可以<a id='a_apply' onclick='userBehavior('2119','a_apply');' href='"+basePath+"/huodong/shiyong/index.html'>申请VIP试用</a>";
				}
			}
		}else{
			if(!json.isComeFromApply){
				title="提示：VIP权限到期时间："+json.expireDate;
				msg="VIP权限到期时间："+json.expireDate;
			}else{
				title="提示：VIP试用到期时间："+json.expireDate;
				msg="VIP试用到期时间："+json.expireDate;
			}
		}
		$("#a_userinfo").attr("title",title);
		$("#img_vip").attr("title",title);
		$("#label_vipInfo").html(msg);
	}else{
		title="您好，普通用户可以申请VIP试用";
		msg="您好，普通用户可以<a id='a_apply' onclick='userBehavior('2119','a_apply');' href='"+basePath+"/huodong/shiyong/index.html'>申请VIP试用</a>";
		$("#a_userinfo").attr("title",title);
		$("#img_vip").attr("title",title);
		$("#label_vipInfo").html(msg);
	}
}

function getCurentDate()
{ 
    var now = new Date();
    var year = now.getFullYear();       //年
    var month = now.getMonth() + 1;     //月
    var day = now.getDate();            //日
    var date = year + "-";
    if(month < 10)
    date += "0";
    date += month + "-";
    if(day < 10)
    date += "0";
    date += day;
    return(date); 
} 

function deletePopupType(){
	$.ajax({
		type: "POST",
		url: "/deletePopupType.ajax",
		dataType: 'HTML',
		success: function(data, textStatus){
		},
		error: function(){
		}
	});
}

    //初始化未读消息
    function initUnreadedMsg() {
        $.ajax({
            url: basePath + "/membercenter/getMessageInfo.ajax",
            type: "get",
            async: true,
            dataType: "json",
            success: function (data) {
                var amount = data.amount;
                var messageList = data.messageList;
                if (typeof(amount) == "number" && amount && messageList.length > 0) {
                    /*$("#a_myMsg font.inforFont").text(amount);*/
                    $("#a_myMsg").append("<font class=\"inforFont\">" + amount + "</font>");
                    $.each(messageList, function (i, message) {
                        $(".seeMore").before("<span class=\"infoMain\"><span class=\"infoSpan\">" + message.content + "</span><p class='date'>" + message.sendDate + "</p>" + "</span>");
                    });
                    initTooltip();
                } else {
                    $("span.inforWrap").remove();
                    return;
                }


            }
        });
    }

    //初始化未读提示框消息
    function initTooltip(){
        if(!parseInt($('.inforFont').text()) || parseInt($('.inforFont').text()) == 0){
            $('span.inforWrap').remove();
            return;
        }
        $(".inforWrap").hover(function (){
            $(this).children('.inforBox').stop().show();
        },function (){
            $(this).children('.inforBox').stop().delay(500).hide(0);
        });

        var infoFontNum = parseInt($('.inforFont').text());
        $('.inforSpan h3 font').text(infoFontNum);
        if(infoFontNum > 9){
            $('.inforFont').css({
                'width':'27px',
                'background-position':'0 7px'
            });
            if(infoFontNum > 99){
                $('.inforFont').text(" 99+");
                $('.inforFont').css({
                    'padding-left':'1px'
                });
            }
        }
        var mainLen = $('.infoMain').length;
        for(var i = 0;i< mainLen;i++){
            var mainFontTet = $('.infoMain').eq(i).children('span').text();
            var mainFontTetLen = $('.infoMain').eq(i).children('span').text().length;
            if(mainFontTetLen > 60){
                var mainFontStr = mainFontTet.substr(0,60)
                $('.infoMain').eq(i).children('span').text(mainFontStr+"...");
            }else{
                $('.infoMain').eq(i).children('span').text(mainFontTet);
            }
        }
    }
</script>
