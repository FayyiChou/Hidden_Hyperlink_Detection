 document.onkeydown= function(ev){
                var ev = ev || window.event;
                if(ev.keyCode==37 && $("#pic_pre").length > 0){
                    window.location.href=$("#pic_pre").attr("href");
                }
                if(ev.keyCode==39 && $("#pic_next").length > 0){
                    window.location.href=$("#pic_next").attr("href");
                }
            };
