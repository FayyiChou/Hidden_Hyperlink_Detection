    <script type="text/javascript">
        var Ishidden = true;//是否隐藏悬浮搜索框
        var hisSearch = new Array(5);//搜索历史和活动标签关系
        //trim()
        String.prototype.trim = function () {
            return this.replace(/(^\s*)|(\s*$)/g, "");
        }

        $(window).scroll(function () {
            if ($(window).scrollTop() >= 1000) {
                Ishidden = false;
            }
            if ($(window).scrollTop() < 1000) {
                Ishidden = true;
            }
        });
        $(function () {
            preHisLoad();//预加载搜索历史&&活动关系

            //#region 搜索记录
            $("#inputTexseaval").click(function () {
                var k = $("#inputTexseaval").val();
                if (k.trim())
                    LoadDimlyMatch('fixed');
                else
                    Html("fixed");
            })
            // 搜索框AutoComplete:1s未输入才匹配
            var tFix;
            $("#inputTexseaval").keyup(function () {
                tFix = setTimeout("LoadDimlyMatch('fixed')", 1000);//1s后执行
				console.log('u'+tFix);
            });
            // 搜索框AutoComplete
            $("#inputTexseaval").keydown(function () {
			if (tFix)
                clearTimeout(tFix);
				console.log('d'+tFix);
            });
            //鼠标离开则隐藏下拉框
            $("#searchMain").mouseleave(function () {
                $("#searchList").hide();
            });
            //#endregion
            //#region 悬浮搜索记录
            $("#inputTexseaval2").click(function () {
                var k = $("#inputTexseaval2").val();
                if (k.trim())
                    LoadDimlyMatch('float');
                Html("float");
            })
            // 搜索框AutoComplete:1s未输入才匹配
            var tFlt;
            $("#inputTexseaval2").keyup(function () {
			if (tFlt)
                tFlt = setTimeout("LoadDimlyMatch('float')", 1000);
            });
            $("#inputTexseaval2").keydown(function () {
                clearTimeout(tFlt);
            });
            $("#topSearchpmg").mouseleave(function () {
                $("#searchList2").hide();
            });
            //#endregion

            $("#mycityp").find("a[cid]").click(function () {
                var cid = $(this).attr("cid");
                var cityName = $(this).html();
                $.post("http://" + location.host + root + "/home/Ajax_Helper/HeadCityManage.ashx", { action: "c", cid: cid, cityname: cityName }, function (a) {//未登录情况下获取省份ID
                    if (location.href.indexOf("AnnouncementList.aspx") >= 0) {
                        location.href = "AnnouncementList.aspx";
                    }
                    else {
                        location.href = location.href;
                    }
                })
            });
            //点击省份事件
            $("a[pid]").click(function () {
                var myDate = new Date();
                addCookie('time1', myDate, 0);
                var pid = $(this).attr("pid");
                var provinceName = $(this).html();
                //#region 置顶公告
                $.ajax({
                    type: 'post',
                    dataType: 'json',
                    url: "http://" + location.host + root + "/home/Ajax_Helper/CmsHeadBanner.ashx",
                    data: { "action": "gg", "pid": pid },
                    success: function (data) {
                        var isExist = false;
                        var gg = "<div class=\"txtScroll-top\">";
                        gg += "<i class=\"iconfont\">&#xe602;</i>";
                        gg += "<div class=\"bd\">";
                        gg += "<ul class=\"infoList\">";
                        $.each(data.ds1, function (index, info) {
                            gg += "<li><a href=\"" + info.Url + "\" target=\"_blank\">" + info.Content + "</a></li>";
                            isExist = true;
                        });
                        gg += "</ul>";
                        gg += "</div>";
                        gg += "</div>";
                        if (isExist)
                            $("#gg_banner").html(gg);
                        $(".txtScroll-top").slide({ mainCell: ".bd ul", effect: "top", autoPlay: true, delayTime: 500, interTime: 5000 });
                        $(".txtScroll-top i").click(function () {
                            $(".txtScroll-top").fadeOut();
                        });
                    }
                });
                //#endregion
                if (pid == 0) {
                    provinceName = "全国站";
                }
                $.post("http://" + location.host + root + "/home/Ajax_Helper/HeadCityManage.ashx", { action: "p", pid: pid, provincename: provinceName }, function (res) { //未登录情况下获取省份ID
                    $("#UserProvince").html(res.Pname);
                    if (location.href.indexOf("AnnouncementList.aspx") >= 0) {
                        location.href = "AnnouncementList.aspx";
                    } else {
                        location.href = location.href;
                    }
                });
                myDate = getCookie('time1');
                addCookie('time2', myDate);
            });
        });

        //#region 回车激发搜索
        $(document).keyup(function (event) {
            if (event.keyCode == 13) {
                if (Ishidden)
                    $("#searchBut").trigger("click");
                else
                    $("#searchBut2").trigger("click");
            }
        });
        //#endregion

        //#region 清空搜索历史
        function deleteAllProduct(txtType) {
            var searchList = "#searchList";
            if (txtType == 'float')
                searchList = "#searchList2";
            //清除所有历史搜索记录
            localStorage.removeItem("SearchCart");
            if (getCookie("_JTHB_SESS") != null) {
                $.ajax({
                    type: 'GET',
                    dataType: 'json',
                    async: false,
                    url: "http://" + location.host + root + "/home/Ajax_Helper/SearchData_Handler.ashx?jsoncallback=?",
                    data: { "action": "RemoveAll" },
                    success: function (result) {
                        if (result != null) {
                            if (result.ErrCode == "0000") {
                            }
                        }
                    }
                });
            }
            $(searchList).hide();
            Html(txtType);
        }
        //#endregion
        //#region 清除特定搜索历史
        function deleteProduct(txtType, name, id, obj) {
            var searchList = "#searchList";
            if (txtType == 'float')
                searchList = "#searchList2";
            cart.deleteproduct(name);
            if (getCookie("_JTHB_SESS") != null) {
                $.ajax({
                    type: 'GET',
                    dataType: 'json',
                    async: false,
                    url: "http://" + location.host + root + "/home/Ajax_Helper/SearchData_Handler.ashx?jsoncallback=?",
                    data: { "action": "RemoveById", "SearchId": id },
                    success: function (result) {
                        if (result != null) {
                            if (result.ErrCode == "0000") {
                            }
                        }
                    }
                });
            }
            obj.parentNode.style.display = 'none';
            if (cart.getproductlist().length == 0)
                $(searchList).hide();
        }
        //#endregion

        //#region 搜索历史记录层加载事件
        function Html(txtType) {
            var searchElemt = "#searchList";
            if (txtType == 'float')
                searchElemt = "#searchList2";
            var productlist = {};
            if (getCookie("_JTHB_SESS") == null) {
                productlist = cart.getproductlist();//取出购物车商品
            }
            else {
                $.ajax({
                    type: 'GET',
                    dataType: 'json',
                    async: false,
                    url: "http://" + location.host + root + "/home/Ajax_Helper/SearchData_Handler.ashx?jsoncallback=?",
                    data: { "action": "Query" },
                    success: function (result) {
                        if (result != null) {
                            if (result.ErrCode == "0000") {
                                productlist = result.data;
                            }
                        }
                    }
                });
            }
            if (productlist.length > 0) {
                var strhtml = "<h3>搜索记录 <q class=\"iconfont\" id=\"searchCloseAll1\" onclick=\"deleteAllProduct('" + txtType + "')\">&#xe60a;</q></h3><ul>";
                for (var i = 0; i < productlist.length; i++) {
                    strhtml += "<ol>";
                    strhtml += "<q class=\"iconfont\"onclick=\"javascript:deleteProduct('" + txtType + "','" + productlist[i].name + "','" + productlist[i].id + "',this);\" style=\"float: right;position: static;line-height: 25px;margin-left: 10px;\">&#xe60d;</q>";//删除历史记录
                    strhtml += "<em>约<b>" + hisSearch[i].CommmodityAbout + "</b>个商品</em>";
                    strhtml += "<li onclick=\"SearchBtnClick('" + productlist[i].name + "','http://" + location.host + root + "/OrderMake/productlist.aspx?keywords=" + productlist[i].name + "')\">";
                    strhtml += "<span>" + productlist[i].name + "</span>";//显示单条历史记录
                    strhtml += "</li>";
                    //绑定关联的活动
                    if (hisSearch[i].ActivityTagList) {
                        if (hisSearch[i].ActivityTagList.length > 0) {
                            strhtml += "<dl>";
                            //针对每一条搜索历史进行活动匹配
                            $.each(hisSearch[i].ActivityTagList, function (i, item) {
                                if (!item.PCUrl)
                                    item.PCUrl = "http://" + location.host + root + "/OrderMake/productlist.aspx?keywords=" + item.TagName + "&tagtype=1";
                                strhtml += "<dd onclick=\"SearchBtnClick('" + item.TagName + "','" + item.PCUrl + "')\"><span>在<b>" + item.TagName + "</b>中搜索</span></dd>";
                            });
                            strhtml += "</dl>";                          
                        }
                    }                                       
                    //层显示
                    strhtml += "</ol>";
                }
                strhtml += "</ul>";
                $(searchElemt).html(strhtml);
                $(searchElemt).show();
            }
        }
        //#endregion
        //#region 搜索历史预加载
        function preHisLoad() {
            var productlist = {};
            if (getCookie("_JTHB_SESS") == null) {
                productlist = cart.getproductlist();//取出购物车商品
            }
            else {
                $.ajax({
                    type: 'GET',
                    dataType: 'json',
                    async: false,
                    url: "http://" + location.host + root + "/home/Ajax_Helper/SearchData_Handler.ashx?jsoncallback=?",
                    data: { "action": "Query" },
                    success: function (result) {
                        if (result != null) {
                            if (result.ErrCode == "0000") {
                                productlist = result.data;
                            }
                        }
                    }
                });

            }
            if (productlist.length > 0) {
                getActivity(productlist[0].name, 0);
            }
            if (productlist.length > 1) {
                getActivity(productlist[1].name, 1);
            }
            if (productlist.length > 2) {
                getActivity(productlist[2].name, 2);
            }
            if (productlist.length > 3) {
                getActivity(productlist[3].name, 3);
            }
            if (productlist.length > 4) {
                getActivity(productlist[4].name, 4);
            }
        }
        //获取活动标签
        function getActivity(k, i) {
            $.ajax({
                url: "/home/Ajax_Helper/SearchAutoComplete.ashx",
                type: "post",
                data: { KeyWords: encodeURIComponent(k), ActivityTagNumber: 3, BrandTagNumber: 0, CommmodityTagNumber: 5 },
                dataType: "json",
                success: function (result) {
                    if (result.ErrCode == "0000") {
                        hisSearch[i] = result;
                    }
                }
            });
        }
        //#endregion
        //#region 模糊匹配
        function LoadDimlyMatch(txtType) {
            //默认固定搜索框
            var searchList = "#searchList";
            var searInput = "#inputTexseaval";
            if (txtType == 'float')//悬浮搜索
            {
                searchList = "#searchList2";
                searInput = "#inputTexseaval2";
            }
            var k = $(searInput).val();
            var strhtml = "";
            $(searchList).hide();
            $.post("/home/Ajax_Helper/SearchAutoComplete.ashx", { KeyWords: k, ActivityTagNumber: 5, BrandTagNumber: 5, CommmodityTagNumber: 5 }, function (result) {
                if (result.ErrCode == "0000") {
                    //#region 活动
                    if (result.ActivityTagList) {
                        if (result.ActivityTagList.length > 0) {
                            strhtml += "<dl class=\"searchdownBox\">";
                            strhtml += "<h5><span><b>活动</b></span></h5>";
                            //针对输入进行活动标签匹配
                            $.each(result.ActivityTagList, function (i, item) {
                                if (!item.PCUrl)
                                    item.PCUrl = "http://" + location.host + root + "/OrderMake/productlist.aspx?keywords=" + item.TagName + "&tagtype=1";
                                strhtml += "<dd onclick=\"SearchBtnClick('" + item.TagName + "','" + item.PCUrl + "')\"><span>在<b>" + item.TagName + "</b>中搜索</span></dd>";
                            });
                            strhtml += "</dl>";
                        }
                    }
                    //#endregion
                    //#region 品牌
                    if (result.BrandAbout) {
                        if (result.BrandTagList.length > 0) {
                            var strst = JSON.stringify(result.BrandTagList);
                            strhtml += "<dl class=\"searchdownBox\">";
                            strhtml += "<h5><span><b>品牌</b></span></h5>";
                            $.each(result.BrandTagList, function (i, item) {
                                if (item.TotalRecord > 0) {
                                    var tagParam = "";
                                    if (item.TagID)
                                        tagParam = "&tagid=" + item.TagID;
                                    strhtml += "<dd onclick=\"SearchBtnClick('" + item.TagName + "','http://" + location.host + root + "/OrderMake/productlist.aspx?keywords=" + k + tagParam + "&tagtype=2')\"><span>" + item.TagName + "</span><em>约<b>" + item.TotalRecord + "</b>个商品</em></dd>";
                                }
                            });
                            strhtml += "</dl>";
                        }
                    }
                    //#endregion
                    //#region 商品
                    if (result.CommmodityAbout) {
                        strhtml += "<dl class=\"searchdownBox\">";
                        strhtml += "<h5><span><b>商品</b></span></h5>";
                        $.each(result.CommmodityTagList, function (i, item) {
                            if (item.TotalRecord > 0) {
                                var tagParam = "";
                                if (item.TagID)
                                    tagParam = "&tagid=" + item.TagID;
                                else
                                    tagParam = "&tagtype=" + item.TagType;

                                strhtml += "<dd onclick=\"SearchBtnClick('" + item.TagName + "','http://" + location.host + root + "/OrderMake/productlist.aspx?keywords=" + item.TagName + tagParam + "')\"><span>" + item.TagName + "</span><em>约<b>" + item.TotalRecord + "</b>个商品</em></dd>";
                            }
                        });
                        strhtml += "</dl>";

                    }
                    //#endregion
                    $(searchList).html(strhtml);
                    $(searchList).show();
                }
            }, "json");
        }
        //#endregion

        //#region 热门搜索
        function search_link() {
            var searchlist = $("#dl_search,#dl_search2").find("a");
            $.each(searchlist, function (n, obj) {
                $(obj).attr("href", "http://" + location.host + root + "/OrderMake/productlist.aspx?keywords=" + $(obj).text() + "");
            });
        }
        //#endregion

        //#region 操作cookie
        function addCookie(objName, objValue, objHours) {//添加cookie
            var str = objName + "=" + escape(objValue);
            if (objHours > 0) {//为0时不设定过期时间，浏览器关闭时cookie自动消失
                var date = new Date();
                var ms = objHours * 3600 * 1000;
                date.setTime(date.getTime() + ms);
                str += "; expires=" + date.toGMTString();
            }
            document.cookie = str + ";path=/";
        }

        function getCookie(objName) {//获取指定名称的cookie的值
            var arrStr = document.cookie.split("; ");
            for (var i = 0; i < arrStr.length; i++) {
                var temp = arrStr[i].split("=");
                if (temp[0] == objName)
                    return unescape(temp[1]);
            }
        }

        function delCookie(name) {//为了删除指定名称的cookie，可以将其过期时间设定为一个过去的时间
            var date = new Date();
            date.setTime(date.getTime() - 10000);
            document.cookie = name + "=a; expires=" + date.toGMTString();
        }
        //#endregion

        jQuery(function () {
            search_link();//热门搜索
            //点击搜索按钮
            $("#searchBut").click(function () {
                var keyWords = jQuery.trim(jQuery("#inputTexseaval").val());
                var url = "http://" + location.host + root + "/OrderMake/productlist.aspx";
                if (keyWords.trim())
                    url += "?keywords=" + keyWords;
                SearchBtnClick(keyWords, url);
            });
            $("#searchBut2").click(function () {
                var keyWords = jQuery.trim(jQuery("#inputTexseaval2").val());
                var url = "http://" + location.host + root + "/OrderMake/productlist.aspx";
                if (keyWords.trim())
                    url += "?keywords=" + keyWords;
                SearchBtnClick(keyWords, url);
            });

            //用户退出 点击
            $(".iconSize").click(function () {
                location.href = "http://" + location.host + root + "/home/Logout.aspx";
            });
        });
        //判定URL参数
        (function ($) {
            $.getUrlParam = function (name) {
                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
                var r = window.location.search.substr(1).match(reg);
                if (r != null) return r[2]; return "";
            }
        })(jQuery);

        //#region 点击搜索
        function SearchBtnClick(keywords, url) {
            //购搜索历史对象
            var name = keywords;
            var time = new Date()
            var product =
            {
                'name': name,
                'time': time
            };
            var productlist = cart.getproductlist();//取出搜索历史纪录
            var result = false;
            //查找购物车中是否有该商品
            for (var i in productlist) {
                if (productlist[i].name == product.name) {
                    cart.deleteproduct(product.name);
                    result = true;
                }
            }
            if (!result) {
                if (productlist.length > 4) {
                    cart.deleteproduct(productlist[0].name);//删除第一项;
                }
            }
            //搜索记录加入到购物车
            cart.addproduct(product);
            if (keywords != "搜索站内商品") {
                if (url)
                    window.open(url);
                else
                    window.open("http://" + location.host + root + "/OrderMake/productlist.aspx?keywords=" + keywords);
                preHisLoad();
                return false;
            }
        }
        //#endregion
    </script>

    <script type="text/javascript">
        var myDate = new Date();
        addCookie('time1', myDate);
        //获取登录信息
        var par = $.getUrlParam('ticket');
        var uri = location.href;
        jQuery.getJSON("http://" + location.host + root + "/home/ajax.ashx?jsoncallback=?&t=" + (new Date().valueOf()), { g: 'nocachelogin', ticket: par, uri: uri }, function (json) {
            //#region 置顶公告
            $.ajax({
                type: 'post',
                dataType: 'json',
                url: "http://" + location.host + root + "/home/Ajax_Helper/CmsHeadBanner.ashx",
                data: { "action": "gg", "pid": json.UserProvince },
                success: function (data) {
                    var isExist = false;
                    var gg = "<div class=\"txtScroll-top\">";
                    gg += "<i class=\"iconfont\">&#xe602;</i>";
                    gg += "<div class=\"bd\">";
                    gg += "<ul class=\"infoList\">";
                    $.each(data.ds1, function (index, info) {
                        gg += "<li><a href=\"" + info.Url + "\" target=\"_blank\">" + info.Content + "</a></li>";
                        isExist = true;
                    });
                    gg += "</ul>";
                    gg += "</div>";
                    gg += "</div>";
                    if (isExist)
                        $("#gg_banner").html(gg);
                    $(".txtScroll-top").slide({ mainCell: ".bd ul", effect: "top", autoPlay: true, delayTime: 500, interTime: 5000 });
                    $(".txtScroll-top i").click(function () {
                        $(".txtScroll-top").fadeOut();
                    });
                }
            });
            //#endregion
            if (json.Result == "true") {
                $("#arrow").hide();
                $("#beforelogin").hide();
                $("#afterlogin").show();
                $("#beforestatu").hide();
                $("#afterstatu").show();
                $("#cityMenu").hide();
                if (json.UserLoginType == "7") {
                    $("#UserRealName").html(json.CustName);
                    $("#UserName").html(json.CustName + "，");
                    $("#UserProvince").html(json.UserProvinceName);
                    //if (json.CustLevel == "1")
                    //	$(".userLev").html("钻卡用户");
                    //if (json.CustLevel == "2")
                    //	$(".userLev").html("金卡用户");
                    //if (json.CustLevel == "3")
                    //	$(".userLev").html("银卡用户");
                    //else
                    //	$(".userLev").html("普通用户");
                    $("#spanIntegral b").html(json.Integral);
                    $("#spanVoucher b").html(json.Voucher);
                    if (json.Sex == "1")
                        $("#imgHead").attr("src", "http://y.jf.189.cn/home/header/common/images/header/man.jpg");
                    else
                        $("#imgHead").attr("src", "http://y.jf.189.cn/home/header/common/images/header/girl.jpg");

                } else {
                    $("#UserRealName").html(json.UserRealName);
                    $("#UserName").hide();
                    $("#UserProvince").html(json.UserProvinceName);
                    //$(".userLev").hide();
                    $("#spanIntegral").hide();
                    $("#spanVoucher b").html(json.Voucher);
                    $("#imgHead").attr("src", "http://y.jf.189.cn/home/header/common/images/header/man.jpg");
                }
                $(".userpho").html(json.UserRealName);
                var point = parseInt(json.Integral) + parseInt(json.Voucher);
                $("#Exchange").click(function () {
                    location.href = "http://" + location.host + root + "/ordermake/productlist.aspx?pageIndex=1&pid_n=0&pid_m=" + point + "&bid=";
                })
                var htm = $(".cityChange a").first().html();
                $(".cityChange a").first().remove();
                $(".cityChange").prepend(htm);
            }
            else if (json.Result == "redir") {
                location.href = json.Redirect;
            }
            else {
                $("#arrow").show();
                $("#UserProvince").html(json.UserProvinceName);
            }
        });
        myDate = getCookie('time1');
        addCookie('time2', myDate);
    </script>


