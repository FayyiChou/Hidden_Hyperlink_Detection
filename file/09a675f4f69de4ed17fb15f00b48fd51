<script language="javascript" type="text/javascript">
    $(function () {
        var adsdls_pro = new ScrollPicleft();
        adsdls_pro.scrollContId = "Scrolls_pro"; // 内容容器ID
        adsdls_pro.arrLeftId = "GoLeft_pro"; //左箭头ID
        adsdls_pro.arrRightId = "GoRight_pro"; //右箭头ID
        adsdls_pro.frameWidth = 729; //显示框宽度
        adsdls_pro.pageWidth = 243; //翻页宽度
        adsdls_pro.speed = 0.01; //移动速度(单位毫秒，越小越快)
        adsdls_pro.space = 10; //每次移动像素(单位px，越大越快)
        adsdls_pro.autoPlay = true; //自动播放
        adsdls_pro.autoPlayTime = 2; //自动播放间隔时间(秒)
        adsdls_pro.initialize(); //初始化
    });  

    var qidangP1Array = {}; //定义一个数组  前档原价
    var qidangP2Array = {}; //定义一个数组 前档套餐价
    var houdangP1Array = {}; //定义一个数组 侧后挡原价
    var houdangP2Array = {}; //定义一个数组 侧后挡套餐价
    $(function () {
        $.get("/ajax.ashx?action=getProductBySID&t=" + Math.random(), { pids: "14" }, function (rsp) {
            var sta = gav(rsp, "state");
            var sMsg = gav(rsp, "msg");
            if (sta == "1") {
                $("#selqidang").html("<option>请选择</option>" + sMsg);
                var spri = gav(rsp, "pricearr");
                var sUserpri = gav(rsp, "userpricearr");
                qidangP1Array = spri.split("$");
                qidangP2Array = sUserpri.split("$");
            } else {
                $("#selqidang").html("<option>暂无前档产品</option>");
            }
        });
        $.get("/ajax.ashx?action=getProductBySID&t=" + Math.random(), { pids: "15" }, function (rsp) {
            var sta = gav(rsp, "state");
            var sMsg = gav(rsp, "msg");
            if (sta == "1") {
                $("#selHoudang").html("<option>请选择</option>" + sMsg);

                var spri = gav(rsp, "pricearr");
                var sUserpri = gav(rsp, "userpricearr");
                houdangP1Array = spri.split("$");
                houdangP2Array = sUserpri.split("$");
            } else {
                $("#selHoudang").html("<option>暂无侧后挡产品</option>");
            }
        });

    });
    function TCPriceChange() {
        //前档
        var obj01 = document.getElementById('selqidang');
        var index01 = obj01.selectedIndex; //序号，取当前选中选项的序号
        var val01 = obj01.options[index01].value;
        var p1Price = parseInt(qidangP1Array[index01]) == "NaN" ? parseInt("0") : parseInt(qidangP1Array[index01]);
        var p1UserPrice = parseInt(qidangP2Array[index01]) == 'NaN' ?  parseInt("0") : parseInt(qidangP2Array[index01]);

        //侧后档
        var obj02 = document.getElementById('selHoudang');
        var index02 = obj02.selectedIndex; //序号，取当前选中选项的序号
        var val02 = obj02.options[index02].value;
        var p2Price = parseInt(houdangP1Array[index02]) == 'NaN' ? parseInt("0") : parseInt(houdangP1Array[index02]);
        var p2UserPrice = parseInt(houdangP2Array[index02]) == 'NaN' ? parseInt("0") : parseInt(houdangP2Array[index02]);

        var dcprice = p1UserPrice + p2UserPrice;
        $("#p1PriceH").html(p1Price + p2Price +"元");
        $("#p2UsrPriceH").html("特价：" + dcprice + "元");
    }
</script>

