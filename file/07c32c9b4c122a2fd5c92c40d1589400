<script>
//登录
function login(type){
	document.myform_login.action = "?m=member&c=index&a=login";
	document.myform_login.type.value = type;
	document.myform_login.submit();
}
function check_login_data(){
            var url = 'index.php?m=member&c=index&a=public_check_web_login';
            $.ajax({
                url:url,
                type:'post',
                data:$("#myform_login").serialize(),
                dataType:'json',
                async:false,
                success:function(data){
                    if(data['code'] == '0'){
                        $("#myform_login").submit();
                    }else{
                        $('#error_tip').text(data['msg']);
                        return false;
                    }
                }
            });
        }
</script>        <!--<script type="text/javascript" src="http://www.icartoons.cn/statics/web/newweb/js/swiper.jquery.min.js"></script>-->

