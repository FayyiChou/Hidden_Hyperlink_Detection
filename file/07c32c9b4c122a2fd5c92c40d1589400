    var contentcode = '1S'+'201305300200091794';
    $(function () {
        $(".sub_title li").not(":first-child").click(function () {
            var index = $(this).index() - 1;
            $(this).addClass("active").siblings().removeClass("active");
            //					/alert(1);
            $(".cc").eq(index).show().siblings().hide();
        });
        $(".introdu span").toggle(
                function () {
                    $(this).parent().parent(".introdu").css("height", "auto");
                    $(this).css("position", "relative");
                    $(this).html("【收起】");
                }, function () {
            $(this).parent().parent(".introdu").css("height", "47px");
            $(this).css("position", "absolute");
            $(this).html("【展开】");
        });
    })
