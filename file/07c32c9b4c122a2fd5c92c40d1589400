            $(function () {
			var hm = document.createElement("script");
  			hm.src = "//hm.baidu.com/hm.js?a642a749e9f7b53b2f882b562195f11f";
  			var s = document.getElementsByTagName("script")[0]; 
  			s.parentNode.insertBefore(hm, s);

	
                //首页图片翻转
                $('.quickFlip').quickFlip();	
								
				//左右切换图片
				var lenthnum = $(".swiper-slide").length;
				$(".swiper-slide").width("832px");
				var iwd = lenthnum * 832;
				//				/alert(iwd);
				$(".swiper-wrapper").width(iwd);
				var i = 1;
				$(".swiper-button-next").click(function() {
					if(i>=1 && i<lenthnum){
					$(".swiper-wrapper").animate({
						left: "-=832"
					});
					i++;
					if(i>1){
						$(".swiper-button-prev").removeClass("swiper-button-disabled");
					}
					}
					if(i==lenthnum){
						$(this).addClass("swiper-button-disabled").siblings().removeClass("swiper-button-disabled");
						i=lenthnum;
					}
					//alert(i);
				});
				$(".swiper-button-prev").click(function() {	
					if(i>1 && i<=lenthnum){
					$(".swiper-wrapper").animate({
						left: "+=832"
					});
					i--;
					}
					if(i<lenthnum){
						$(".swiper-button-next").removeClass("swiper-button-disabled");
					}
					//alert(i);
					if(i==1){
						$(this).addClass("swiper-button-disabled").siblings().removeClass("swiper-button-disabled");
						i=1;
					}
				})
				//左右切换图片 end
			});
			function placeholderSupport() {
					return 'placeholder' in document.createElement('input');
			}
		
