<script type="text/javascript">
//切换目录 type:1正序 2逆序  groupid:分组id
function change_category(type,groupid){
	if(type == 1){
		$("a[id^='positive_cat_id_']").removeClass('active');
		$("a[id='positive_cat_id_"+groupid+"']").addClass('active');
		$("ul[id^='positive_setinfo_']").hide();
		$("ul[id='positive_setinfo_"+groupid+"']").show();
	}else{
		$("a[id^='negative_cat_id_']").removeClass('active');
		$("a[id='negative_cat_id_"+groupid+"']").addClass('active');
		$("ul[id^='negative_setinfo_']").hide();
		$("ul[id='negative_setinfo_"+groupid+"']").show();
	}
}
function setlist_order(type){
	if(type == 1){
		$("#positive_setlist_div").show();
		$("#negative_setlist_div").hide();
	}else{
		$("#positive_setlist_div").hide();
		$("#negative_setlist_div").show();
	}
}

function do_fav(type){
    var userid = '';
    if (userid == '') {
        window.location.href = 'http://www.icartoons.cn/index.php?m=member&c=index&a=login&forward=http%3A%2F%2Fwww.icartoons.cn%2Fcomic%2F19-200091794.html%3Ftrackid';
        return false;
    }
    
    if(type ==1){
        $.ajax({
            url:'index.php?m=content&c=index&a=public_add_favourite&type=0&id=200091794',
            type:'get',
            success:function(data){
            }
        });
        
        $('#do_fav').hide();
       
        $('#do_unfav').show();
    }else{
        $.ajax({
            url:'index.php?m=content&c=index&a=public_del_favourite&type=0&id=200091794',
            type:'get',
            success:function(data){
            }
        });
        
        $('#do_fav').show();
        
        $('#do_unfav').hide();
    }
}
$(document).ready(function(){
$('.bds_more').css('background-image','url("http://www.icartoons.cn/statics/web/newweb/images/icon2.png")').css('background-position','0 -93px').css('color','#a8a8a8').css('line-height','21px').css('height','21px').css('padding','0 7px 0 19px').css('margin','2px 2px');
$('.bds_more').text('分享');
});
function play_comic(set_is_vip,url){
    var userid = '';
    var is_vip = '';
    if(is_vip != '1' && userid == '' && set_is_vip != 0){
         $('#login').click();
        return false;
    }else{
        window.open(url);
    }
}
