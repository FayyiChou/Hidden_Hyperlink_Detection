<script>
    var userid2 = '';
    //userid2 = '45950632';
    var content_id = contentcode;
    //content_id = '1S201303280200089950';
    var page = 1;
    var loading = false;
    var has_more = true;
    function load_comment(page) {
        //if(!has_more) return false;
        if (loading) {
            return false;
        }
        loading = true;
        var url = "index.php?m=content&c=index&a=public_comment_list&page=" + page + "&content_id=" + content_id;
        $.ajax({
            url: url,
            type: 'get',
            dataType: 'json',
            async: false,
            success: function (data) {
                if (data['code'] != 0) {
                    return false;
                }
                if (data['datas'].length == 0) {
                    has_more = false;
                    return false;
                }
                create_comment(data['datas']);
                $('#pages').html(data['pages']);
                $('#comment_total').text(data['record_cnt']);
            }
        });
        loading = false;

    }
    //创建评论
    function create_comment(datas) {
        var comment = "";
        $("#comment_list").empty();
        $.each(datas, function (k, v) {
           /*comment = '<div class="block_comment_item" id="comment_div_' + v['id'] + '">';
            if (v['userid'] == userid2) {
                comment += '<img class="icon_delete" src="http://www.icartoons.cn/statics/web/newimage/icon_delete.png" onclick="delete_comment(\'' + v['id'] + '\');"/>';
            }
            //头像
            comment += '<p class="box_head_comment"><img src="' + v['photo'] + '" /></p>';
            //评论内容和昵称，评论时间
            comment += '<div class="info_comment"><p class="name_comment">' + v['nickname'] + '</p><p class="text_comment">' + v['text'] + '</p><p class="time_comment">' + v['create_time'] + '</p></div>';
            comment += '</div>';
            $("#comment_list").append(comment);*/
            comment = '<div class="js-citem g-cols--comment citem" id="comment_div_' + v['id'] + '">';
            comment += '<div class="g-col--left citem-hd"><a title="" class="m-comment-avatar-wrapper"  href="javascript:void(0);">';
            comment +='<img alt="" src="' + v['photo'] + '" width="42" height="42" class="avator"></a></div>';
            comment += '<div class="g-col--right citem-bd">';
            comment += '<div class="media-heading"><a class="" title="' + v['nickname'] + '"  href="javascript:void(0);">' + v['nickname'] + '</a> </div>';
            comment += '<div class="citem-content">' + v['text'] + '</div>';
            comment += '<div class="comment-footer citem-footer"><div class="m-comment-time"> <span class="muted time">' + v['create_time'] + '</span>';
            if(v['userid'] == userid2){
                comment += '<a onclick="delete_comment(\''+v['id']+'\');">删除</a>';
            }
            comment += '</div></div>';
            comment += '</div>';
            comment += '</div>'; 
            $("#comment_list").append(comment);
        });
    }

    //页面滚动时加载评论
    /*$(window).scroll(function(){
     if($(window).height() + $(window).scrollTop() >= $(document.body).height()-50){ //距离底部20个像素时加载元素
     load_comment();
     }
     })
     */
   

    //添加评论
    function add_comment() {
        if (userid2 == '') {
            $('#login').click();
            //window.location.href = 'http://www.icartoons.cn/index.php?m=member&c=index&a=login&forward=http%3A%2F%2Fwww.icartoons.cn%2Fcomic%2F19-200091794.html%3Ftrackid';
            return false;
        }
        var text = $.trim($('#text').val());
        if (text == '' || text == '我来说两句') {
            $('#error_tip_comment').text('评论内容不能为空');
            return false;
        }
        var url = 'index.php?m=content&c=index&a=public_add_comment';
        //var data = $('#text').serialize()+'&content_id='+content_id;
        var myData = {
            "text": $('#text').val(),
            "content_id": content_id,
			"resource": 2
        };
        $(this).ajaxSubmit({
            type: 'get',
            url: url,
            dataType: 'json',
            async: true,
            data: myData,
            success: function (data) {
                if (data['code'] == '0') { //添加评论成功之后重新加载整个评论列表
                    page = 1;
                    has_more = true;
                    $("#comment_list").empty();
                    load_comment(1);
                    $('#error_tip_comment').text('');
	            $('#text').val('');
                    document.getElementById("text_num").innerHTML = "还可输入500字";
                } else {
                    $('#error_tip_comment').show();
                    $('#error_tip_comment').text(data['msg']);
                }
            }
        });
        return false;
    }
    //删除评论
    function delete_comment(id) {
        if (id == '')
            return false;
        window.top.art.dialog({id: 'login', title: '提示', content: '确定要删除这条评论吗？', width: "250px", height: '60px',
            button: [{
                    name: '确定',
                    callback: function () {
                        var url = 'index.php?m=content&c=index&a=public_del_comment&comment_id=' + id;
                        $.ajax({
                            url: url,
                            type: 'get',
                            dataType: 'json',
                            success: function (data) {
                                if (data['code'] == '0') {//删除成功
                                    $('#comment_div_' + id).remove();
                                } else {
                                    alert(data['msg']);
                                }
                            }
                        });
                    },
                    focus: true
                }, {
                    name: '取消',
                    callback: function () {
                    }
                }]
        });
    }

    //翻页
    function page_turning(page) {
        load_comment(page);
    }
    
    function checkLength(gm) {
         var maxChars = 500;
         if (gm.value.length > maxChars){
              gm.value = gm.value.substring(0,maxChars);
         }
         var curr = maxChars - gm.value.length;
         document.getElementById("text_num").innerHTML = "还可输入"+curr.toString()+"字";
     }
	  window.onload = load_comment(1);//加载一次评论
</script>
