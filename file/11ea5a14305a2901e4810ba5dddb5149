      <script type="text/javascript">
VeryCD.__image_widget_block_id = -1;
var show_image_add = function(widget_index) {
    if(!VeryCD.Member.Id){
        VeryCD.login();
        return false;
    }

	if(typeof(window.hide_video_add) == 'function') {
		hide_video_add();
    }
	hide_image_add();

	Sizzle('#comment_widget_content_' + widget_index)[0].innerHTML = Sizzle('#widget_image_container')[0].innerHTML;
	Sizzle('#widget_image_container')[0].innerHTML = '';
	VeryCD.iniHtml(Sizzle('#comment_widget_content_' + widget_index)[0]);
	VeryCD.iniHtml(Sizzle('#widget_image_container')[0]);

	VeryCD.__image_widget_block_id = widget_index;

	if (Sizzle('#ul_upload_img')[0].innerHTML != '') {
        VeryCD.__tmp_show_images = VeryCD.__tmp_show_images || 8;
    } else {
        widget_show_images(759886, 'comment');
    }

    Sizzle('#comment_widget_content_' + widget_index)[0].style.display = 'block';
    Sizzle('#widget_block_' + widget_index)[0].style.display = 'block';

    if (VeryCD.is_ie) {
    	Sizzle('#ul_upload_img')[0].attachEvent("onscroll", load_images);
    } else {
    	Sizzle('#ul_upload_img')[0].addEventListener("scroll", load_images, false);
    }

    // IE8 小贴士 bugfix
    if (VeryCD.ie == 8 && Sizzle('#comment_tips')[0]) {
        var tips = Sizzle('#comment_tips')[0];
        tips.style.display = 'none';
        tips.style.display = '';
    }

    VeryCD.TrackEvent("entry", "流口水", "添加图片");
}

var hide_image_add = function() {
	if (VeryCD.__image_widget_block_id != -1) {
		Sizzle('#widget_image_container')[0].innerHTML = Sizzle('#comment_widget_content_' + VeryCD.__image_widget_block_id)[0].innerHTML;
	    Sizzle('#comment_widget_content_' + VeryCD.__image_widget_block_id)[0].innerHTML = '';
        VeryCD.iniHtml(Sizzle('#widget_image_container')[0]);
    	VeryCD.iniHtml(Sizzle('#comment_widget_content_' + VeryCD.__image_widget_block_id)[0]);
    	Sizzle('#comment_widget_content_' + VeryCD.__image_widget_block_id)[0].style.display = 'none';
	    Sizzle('#widget_block_' + VeryCD.__image_widget_block_id)[0].style.display = 'none';
        VeryCD.__image_widget_block_id = -1;
	}
}
function input_file_radio_check() {
    Sizzle('#imgfile_submit')[0].style.display = 'block';
    Sizzle('#imgtxt_submit')[0].style.display = 'none';
    Sizzle('#img_upload_radio')[0].checked = true;
    Sizzle('#img_url_radio')[0].checked = false;
}
function input_txt_radio_check() {
    Sizzle('#imgfile_submit')[0].style.display = 'none';
    Sizzle('#imgtxt_submit')[0].style.display = 'block';
    Sizzle('#img_upload_radio')[0].checked = false;
    Sizzle('#img_url_radio')[0].checked = true;
}
function init_clear_input() {
    if (typeof(clean_select) == 'function') clean_select();
    if(Sizzle('#imgtxt')[0]) Sizzle('#imgtxt')[0].value = '';
    if(Sizzle('#img_url_radio')[0]) Sizzle('#img_url_radio')[0].checked = false;
    if(Sizzle('#imgfile')[0]) Sizzle('#imgfile')[0].value = '';
    if(Sizzle('#img_upload_radio')[0]) Sizzle('#img_upload_radio')[0].checked = false;
}
init_clear_input();
