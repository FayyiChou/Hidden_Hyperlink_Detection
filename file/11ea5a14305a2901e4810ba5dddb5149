function wish_to_sinat_hover(action) {
    if(Sizzle('#wish_to_sinat')[0]) {
        if (action == 'out') {
            VeryCD._wish_to_sinat=setTimeout('Sizzle(\'#wish_to_sinat\')[0].style.display=\'none\';',200);
