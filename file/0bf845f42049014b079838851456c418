<SCRIPT LANGUAGE="JavaScript">
function isempty(){
	if(document.getElementById('search').value==''){
	   alert('关键字不能为空！');
	   return false;
		}
	if(document.getElementById('search').value==0){
		alert('关键字不能为空！');
		return false;
		}
}

</SCRIPT>
