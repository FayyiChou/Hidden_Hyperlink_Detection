<script>
        $(document).ready(function(){
        //127 统计
        ysCS.sendStat('ajax127', 'hz_' + 'qiyi');
    });
        //倒计时
    function timeHtml(obj,time){
        obj.html(time);
    }

        var ggShowTime=8000; //广告显示的时间
    var ggTime=30000; //倒计时开始时间
    
    var countDownNum=ggTime/1000; //倒计时时间
    var J_playPopIvy_time=$('#J_playPopIvy .time');
    timeHtml(J_playPopIvy_time,countDownNum); //倒计时初始化

    var countDownTime=setInterval(function(){
        countDownNum--;
        if(countDownNum == 0){
            closeBtnGg.hide();
            countDownGg.hide();
        }  timeHtml(J_playPopIvy_time,countDownNum);
    },1000);

    //播放过程中倒计时
    var popCountDownObj=$('#popCountDownTime');
    var popDelayTime=35000; //视频播放多久出广告
    var popContinueTime=5000; //广告持续时间
    // 广告显示函数
    function popIframeShow(showTime){
        $('.popIframeBox').show();

        var ggPopTime=showTime/1000;
        var popCountDownTime=setInterval(function(){
            ggPopTime--;
            if(ggPopTime<1){
                clearTimeout(popCountDownTime);

            }
            timeHtml(popCountDownObj,ggPopTime);
        },1000)
        timeHtml(popCountDownObj,ggPopTime);             //倒计时初始化
        setTimeout(function(){
            $('.popIframeBox').hide();
            //$('.popCountDown').hide();
        },showTime);


    }


    //广告显示时间函数
    function countFun(countTime){
        setTimeout(function(){
            popIframeShow(popContinueTime);
        },countTime);
    }

    // 播放器贴片广告
    var playIvy = $('#J_playPopIvy');
    var parentW=playIvy.width();
    var parentH=playIvy.height();
    var closeBtnGg=$('.close-btn-gg');
    var countDownGg=$('#J_playPopIvy .countDown');
    var timer;
    var iframeW=$('#J_playPopIvy iframe').width();
    var iframeH=$('#J_playPopIvy iframe').height();
    var closeBtnR=parseInt((parentW-iframeW)/2)+3;
    var closeBtnT=parseInt((parentH-iframeH)/2)+3;

    if(iframeW!=null){
        if ($('#videoPlayCon>iframe').length>0) {
            var playIframe = $('#videoPlayCon>iframe');
            var _dataUrl = 'http://dispatcher.video.qiyi.com/common/shareplayer.html?vid=dbbf9f28f524d69f0db5bb5d29645ee3&tvId=7491784009&coop=coop_153_2345&bd=1&fullscreen=1&autoplay=1';

            playIframe.attr('src','');
            timer=setTimeout(function(){
                playIvy.remove();
                playIframe[0].src = 'http://dispatcher.video.qiyi.com/common/shareplayer.html?vid=dbbf9f28f524d69f0db5bb5d29645ee3&tvId=7491784009&coop=coop_153_2345&bd=1&fullscreen=1&autoplay=1';
//        clearTimeout(countDownTime);
                countFun(popDelayTime);
            },ggShowTime);

            closeBtnGg.css({'right':closeBtnR,'top':closeBtnT});
            setTimeout(function(){
                closeBtnGg.css({'display':'block'}); //4s后展示关闭按钮
            },4000);
            countDownGg.css({'right':closeBtnR-5,'top':closeBtnT-24,'display':'block'}); //倒计时位置
            closeBtnGg.on('click',function(){
                clearTimeout(timer);
                clearTimeout(countDownTime);
                playIvy.remove();
                playIframe[0].src = 'http://dispatcher.video.qiyi.com/common/shareplayer.html?vid=dbbf9f28f524d69f0db5bb5d29645ee3&tvId=7491784009&coop=coop_153_2345&bd=1&fullscreen=1&autoplay=1';
//      countFun(popDelayTime);
            })
        }else if($('#videoPlayCon>embed').length>0){
            var playIframe = $('#videoPlayCon>embed');

            playIframe.remove();
            timer=setTimeout(function(){
                playIvy.remove();
                var url = 'http://dispatcher.video.qiyi.com/common/shareplayer.html?vid=dbbf9f28f524d69f0db5bb5d29645ee3&tvId=7491784009&coop=coop_153_2345&bd=1&fullscreen=1&autoplay=1';
                $('<embed allownetworking="all" allowscriptaccess="always" src="'+ url +'" quality="high" wmode="opaque" allowfullscreen="true" windowlessVideo="true" allowFullScreenInteractive="true" type="application/x-shockwave-flash">').appendTo('#videoPlayCon');
                clearTimeout(countDownTime);
//        countFun(popDelayTime);
            },ggShowTime);

            closeBtnGg.css({'right':closeBtnR,'top':closeBtnT});
            setTimeout(function(){
                closeBtnGg.css({'display':'block'}); //4s后展示关闭按钮
            },4000);
            countDownGg.css({'right':closeBtnR-5,'top':closeBtnT-24,'display':'block'}); //倒计时位置
            closeBtnGg.on('click',function(){
                clearTimeout(timer);
                clearTimeout(countDownTime);
                playIvy.remove();
                var url = 'http://dispatcher.video.qiyi.com/common/shareplayer.html?vid=dbbf9f28f524d69f0db5bb5d29645ee3&tvId=7491784009&coop=coop_153_2345&bd=1&fullscreen=1&autoplay=1';
                $('<embed allownetworking="all" allowscriptaccess="always" src="'+ url +'" quality="high" wmode="opaque" allowfullscreen="true" windowlessVideo="true" allowFullScreenInteractive="true" type="application/x-shockwave-flash">').appendTo('#videoPlayCon');
//        countFun(popDelayTime);
            })
        }
    }else{

        if ($('#videoPlayCon>iframe').length>0) {

            var playIframe = $('#videoPlayCon>iframe');
            var _dataUrl = 'http://dispatcher.video.qiyi.com/common/shareplayer.html?vid=dbbf9f28f524d69f0db5bb5d29645ee3&tvId=7491784009&coop=coop_153_2345&bd=1&fullscreen=1&autoplay=1';

            playIframe.attr('src',_dataUrl);

        }else if($('#videoPlayCon>embed').length>0){
            var playIframe = $('#videoPlayCon>embed');
            playIvy.remove();
            playIframe.remove();
            var url = 'http://dispatcher.video.qiyi.com/common/shareplayer.html?vid=dbbf9f28f524d69f0db5bb5d29645ee3&tvId=7491784009&coop=coop_153_2345&bd=1&fullscreen=1&autoplay=1';
            $('<embed allownetworking="all" allowscriptaccess="always" src="'+ url +'" quality="high" wmode="opaque" allowfullscreen="true" windowlessVideo="true" allowFullScreenInteractive="true" type="application/x-shockwave-flash">').appendTo('#videoPlayCon');
        }
    }
</script>


