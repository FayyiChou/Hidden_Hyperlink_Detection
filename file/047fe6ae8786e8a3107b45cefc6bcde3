        <script language="JavaScript" type="text/javascript">
    lastScrollY = 0;
    var lovexin12 = null, lovexin14 = null,
    interVal = window.setInterval(function () {
        if (lovexin12 == null) lovexin12 = document.getElementById("lovexin12");
        if (lovexin14 == null) lovexin14 = document.getElementById("lovexin14");
        var diffY = document.body.scrollTop;
        if (document.documentElement && document.documentElement.scrollTop) diffY = document.documentElement.scrollTop;
        percent = .1 * (diffY - lastScrollY);
        if (percent > 0) {
            percent = Math.ceil(percent);
        } else {
            percent = Math.floor(percent);
        }
        lovexin12.style.top = parseInt(lovexin12.style.top) + percent + "px";
        lovexin14.style.top = parseInt(lovexin14.style.top) + percent + "px";
        lastScrollY = lastScrollY + percent;
    }, 1);
</script>
