<script language="javascript" type="text/javascript">
    function searchEvent(obj) {
        var idp = obj.id.split("_");
        var obj = document.getElementById(idp[0]);
        if (obj.value == "" | obj.value == null) {
            alert("请填写搜索关键字");
            return;
        } else {
            obj.value = obj.value.replace(".", "．");
            document.getElementById(idp[0] + "_findForm").submit();
        }
    }
</script>

