    var inter = setInterval(function () {
        if (document.readyState == "complete") {
            clearInterval(inter);
            var kv = [{ id: "search", item: [] }, { id: "SCFXG", item: [] }, { id: "GQXX", item: [] }, { id: "LinkObject", item: [] }, { id: "XXSCJR", item: []}];
            for (var idx = 0; idx < kv.length; idx++) {
                try {
                    var liArr = document.getElementById(kv[idx].id).getElementsByTagName("div");
                    for (var i = 0; i < liArr.length; i++) {
                        var $E = liArr[i];
                        $E.setAttribute("title", $E.innerText);
                        $E.setAttribute("idx", idx);
                        $E.onclick = function () {
                            var idx = $(this).getAttribute("idx");
                            for (var j = 0; j < kv[idx].item.length; j++) {
                                var forname = kv[idx].item[j].ele.getAttribute("for");
                                if (kv[idx].item[j].ele == this) {
                                    kv[idx].item[j].ele.className = "Tab_Head_Item_1";
                                    document.getElementById(forname).style.display = "block";
                                } else {
                                    kv[idx].item[j].ele.className = "Tab_Head_Item"
                                    document.getElementById(forname).style.display = "none";
                                }
                            }
                        }
                        kv[idx].item.push({ check: i == 0, ele: $E });
                    }
                } catch (e) { }
            }
        }
    }, 1);
</script>

