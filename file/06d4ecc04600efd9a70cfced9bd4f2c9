<script type="text/javascript" src="/js/jq_dialog.js"></script>

<script type="text/javascript">
    document.onkeypress = function(e) {
        var theEvent = window.event || e;
        var code = theEvent.keyCode || theEvent.which;
        if (code == 13) {
            $("#login").click();
        }
    }
    $(document).ready(function() {
        //验证
        $("#login").click(function() {
            if ($("#signUserName").val() == "") {
                $.dialog.open({
                    title: '系统提示!',
                    width: 200,
                    content: "请填写：用户名！",
                    drag: true
                });
            }
            else if ($("#signPass").val() == "") {
                $.dialog.open({
                    title: '系统提示!',
                    width: 200,
                    content: "请填写：密码！",
                    drag: true
                });
            }
            else {
                var tsTimeStamp = new Date().getTime();
                $.post("/plus/UserLogin.ashx", { "username": $("#signUserName").val(), "password": $("#signPass").val(), "time": tsTimeStamp },
	 	function(data, textStatus) {
	 	    if (data == "Login_err") {
	 	        $.dialog.open({
	 	            title: '系统提示!',
	 	            width: 200,
	 	            content: "用户名或者密码错误！",
	 	            drag: true
	 	        });
	 	    } else if (data == "Login_Stat") {
	 	        $.dialog.open({
	 	            title: '系统提示!',
	 	            width: 200,
	 	            content: "该用户名已被冻结！",
	 	            drag: true
	 	        });
	 	    }
	 	    else {
	 	        window.location.href = data;
	 	    }
	 	})
            }
            return false;
        });
    });
</script>
