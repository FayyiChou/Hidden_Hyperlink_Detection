  <script>
    function checks() {
        var tdesc = document.getElementById("txtsayto").value;
        if (tdesc == '') {
            alert('留言内容不能为空哦！');
            document.getElementById("txtsayto").focus();
            return false;
        }
        return true;
    }

    function showorcolse() {
        var divstate = document.getElementById("morenews");
        if (divstate.style.display == 'none') {
            divstate.style.display = '';
        }
        else divstate.style.display = 'none';
    }
</script>  
