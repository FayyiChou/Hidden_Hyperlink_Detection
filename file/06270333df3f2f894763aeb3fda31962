/**
获取相关视频
*/
getRelatedseries(sw,function(json){
				var html = '';
				for(var i = 0 ;i < json.length ; i++){
					html += '<li>';
					html += '	<b></b>';
					html += '		<a style="display:block;overflow:hidden;word-break:keep-all;white-space:nowrap;text-overflow:ellipsis; width:150px;" title="' + json[i].seriesName + '" href="/series_' + json[i].seriesId + '.html">';
					html += 			json[i].seriesName;
					html += '		</a>';
					html += '	<span>';
					html += '		<a style="display:block;overflow:hidden;word-break:keep-all;white-space:nowrap;text-overflow:ellipsis;width:50px;" title="' + json[i].teacherName + '" href="javascript:;">'
					html += 			 json[i].teacherName ;
					html += '		</a>';
					html += '	</span>';
					html += '</li>';
				}
				$('#xiangguanvideo').html(html);
			},function(e){
				$('#xiangguanvideo').html('暂无相关视频');
			});

