<script type="text/javascript">
$.ajax({
	url:"http://score.dcjy.chaoxing.com/services/ScoreService/getScore?fid="+fid+"&uid="+uid+"&callBack=?",
	type:"post",
	dataType:"jsonp",
	success:function(datas){
		if ("obj" in datas){
			var score = datas.obj.score;
			var score_used = datas.obj.score_used;
			$("#jf").html(score+score_used);
		}else{
			$("#jf").html("0");
		}
	}
});
</script>	
