<script type="text/javascript"> 
function star(id,sid,_def){
	this.def = parseInt(_def);
	this.oStar = document.getElementById(id);
	this.aLi = oStar.getElementsByTagName("li");
	this.oUl = oStar.getElementsByTagName("ul")[0];
	this.i = iScore = iStar = def;
	if(def > 0){
		for (i = 0; i < def; i++){
			aLi[i].className = "on";
		}
	}
	for (i = 1; i <= aLi.length; i++)
	{
		aLi[i - 1].index = i;
		aLi[i - 1].onmouseover = function ()
		{
			iScore = this.index || iStar;
	
		for (i = 0; i < aLi.length; i++) aLi[i].className = i < iScore ? "on" : "";	
		};
		aLi[i - 1].onmouseout = function ()
		{
			iScore = iStar;
			for (i = 0; i < aLi.length; i++) aLi[i].className = i < iScore ? "on" : "";	
		};
		//点击后进行评分处理
		aLi[i - 1].onclick = function ()
		{
			var score = this.index;
			$.ajax({
					url : "/updateScore.html",
					type : "get",
					data : "score=" + score + "&seriesId=6196",
					dataType : 'json',
					success : function(json){
						if(json.ret == 1){
							var new_score = json.score;
							var f = new_score.split("\.");
							if(f.length == 2){
								var score = document.getElementById('score');
								if(score){
									score.innerHTML = '<b>' + f[0] + '</b>.' + f[1].substring(0,1) + '';
								}
								var score_num = document.getElementById('score_num');
								if(score_num){
									score_num.innerHTML = json.score_num;
								}
								var view_msg = document.getElementById('view_msg');
								if(view_msg){
									view_msg.innerHTML = '被' + json.score_num + '人评分, 平均' + json.score + '分';
								}
							}
							iStar = new_score - 1;
						}else{
							alert(json.msg);
						}
					},
					error : function (e){
						
					}
				});
		}
	}
};
star("star","123",'');
</script>			</div>
