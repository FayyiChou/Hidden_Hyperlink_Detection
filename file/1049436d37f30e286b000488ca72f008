        <script>
            //特殊处理 Begin
            $(function () {
                var timeCount = 3,//总计时, 分钟
                interval = 500,//扫描间隔i==0,
                i = 0,
                timeMs = timeCount * 1000 * 60,
                commTimer = setInterval(function () {
                    //console.log(i, interval, i * interval, timeMs, i * interval >= timeMs);
                    if ($('._ih').length > 0) {
                        $('._ih').hide();
                        clearInterval(commTimer);
                        return false;
                    }
                    if ($('iframe:not(.not-hide):visible').length > 0) {
                        if ($('iframe:not(.not-hide):visible').parent().hasClass('_ih')) {
                            $('iframe:not(.not-hide):visible').parent().hide();
                            clearInterval(commTimer);
                            return false;
                        }
                    }
                    if (i * interval >= timeMs) {
                        clearInterval(commTimer);
                    }
                    i++;
                }, interval);
            });
            //特殊处理 End
        </script>
