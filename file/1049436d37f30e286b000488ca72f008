<script>
    //搜索栏点击box外关闭box
    $('body').bind("click", function (evt) {
        if ($(evt.target).parents('.head > .search-bar').length == 0) {
            $('.search-area-txt').hide();
        }
    });
    //input的下拉box展示 Begin
    $('.search-txt').on('click', function () {
        $('.search-area-txt').show();
    });
</script>


