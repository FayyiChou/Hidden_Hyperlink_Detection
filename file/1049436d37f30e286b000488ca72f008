<script src="/Themes/v3/js/com/nhSlider.js?v=20160827095141"></script>

<script>
    (function () {
        //判断打开浏览器
        testBrowser();

        //右边广告位
       setTimeout(function () {//30秒后隐藏
         $('.frame-right').remove();
       }, 30000);
        $('.frame-right-close').on('click', function () {
            $('.frame-right').remove();
        });
        
        //撕拉广告插入header前面
        $(".header").before($("#sb-comm"));
        $('#sb-comm .close').on("click", function () {
            $("#sb-comm").animate({
                height: 0,
                opacity: 0
            }, 1000, function () {
                $('#sb-comm img').hide();
            });
        });
        //tabs
        $('.areabox').tabs({
            btns: '.naviga-box .area-laber', //所有按钮选择器
            targets: '.areaslist .naviga-city', //所有目标选择器
            model: 1,
            checkFirst: true
        });
        $('.areaslist .naviga-city').hover(function () {
            var num = $('.areaslist .now').index(),
                navigaBox = $('.naviga-box .area-laber');
            navigaBox.eq(num).addClass('now');
        }, function () {
            $(this).parent().siblings('.naviga-box').find('.area-laber').removeClass('now');
        });
        $('.areabox .naviga-box').mouseleave(function () {
            $(this).find('.area-laber').removeClass('now');
        });
        $('.banner').nhSlider({
            //navBtn:false,
            arrowBtn: true,
            fill: false
        });
        //撕拉推荐
        //promImg();

        //抢优惠的图片左右切换
        var $favItems = $('.favbox li'),
            favItemWidth = $favItems.first().width() + 10,
            favBoxWidth = favItemWidth * $favItems.length;
        $('.btn-box .btn-right').on('click', function () {
            var $this = $(this),
                posLeft = $('.favbox ul').position().left,
                left = posLeft - (favItemWidth * 4);
            if (left <= (favItemWidth * 4) - favBoxWidth) {
                $this.parents('.favbox').find('ul').animate({left: 0}, 500);
            }
            else {
                $this.parents('.favbox').find('ul').animate({ left: left }, 500);
            }
        });
        $('.btn-box .btn-left').on('click', function () {
            var $this = $(this),
                posLeft = $('.favbox ul').position().left,
                left = posLeft + (favItemWidth * 4);
            if (left > 0) {
                $this.parents('.favbox').find('ul').animate({ left: (favItemWidth * 4 * 2) - favBoxWidth },500);
            }
            else{
                $this.parents('.favbox').find('ul').animate({ left: left }, 500);
            }    
        });

        //底部推荐
        $('.bottom-comm .btn-close').on('click', function () {
            $('.bottom-comm').css({
                'border-radius': '0 10px 10px 0'
            }).stop().animate({
                width: 150
            }, 300, function () {
                $('.bottom-comm .info-box').hide();
                $('.bottom-comm .btn-min').show();
            });
        });
        $('.bottom-comm .btn-min').on('click', function () {
            $('.bottom-comm .btn-min').hide();
            $('.bottom-comm .info-box').show();
            $('.bottom-comm').css({
                'border-radius': '0'
            }).stop().animate({
                width: '100%'
            }, 300);
        });

        //getRecommend({
        //    code: 'pc_index_yd_tm',
        //    backFunc: function (reObj) {
        //        var itemsArray = reObj, imgUrl = '';
        //        $.each(itemsArray, function (n, i) {
        //            img = i.imageUrl = '<img alt="' + i.title + '" src="http://img.nanhutravel.com' + i.imageUrl + ' "/>';
        //            if (i.title == '大图') {
        //                var box = '<a class="info" href="' + i.link + '" target="_blank">' + img + '</a>'
        //                $('.btn-close').after(box);
        //            }
        //            if (i.title == '小图') {
        //                var box = '<a class="littleimg">' + img + '</a>'
        //                $('.btn-min').append(box);
        //            }
        //        });
        //    }
        //});
    })();
</script>



