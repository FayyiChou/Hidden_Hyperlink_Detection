                                else{
                                    obj.style.left = x_tzgg + document.documentElement.scrollLeft + 'px';
                                    obj.style.top = y_tzgg + document.documentElement.scrollTop + 'px';
                                }
                                x_tzgg = x_tzgg + step_tzgg * (xin_tzgg ? 1 : -1);
                                if (x_tzgg < L) { xin_tzgg = true; x_tzgg = L }
                                if (x_tzgg > R) { xin_tzgg = false; x_tzgg = R }
                                y_tzgg = y_tzgg + step_tzgg * (yin_tzgg ? 1 : -1);
                                if (y_tzgg < T) { yin_tzgg = true; y_tzgg = T }
                                if (y_tzgg > B) { yin_tzgg = false; y_tzgg = B }
                            }
