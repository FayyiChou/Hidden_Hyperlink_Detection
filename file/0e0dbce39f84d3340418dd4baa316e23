                                if (x < L) { xin = true; x = L }
                                if (x > R) { xin = false; x = R }
                                y = y + step * (yin ? 1 : -1);
                                if (y < T) { yin = true; y = T }
                                if (y > B) { yin = false; y = B }
                            }
