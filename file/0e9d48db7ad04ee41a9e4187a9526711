                                        <div onclick="clearAuto();Mea(0)" class="Swich-on">
                                            1</div>
                                        <div onclick="clearAuto();Mea(1)" class="Swich-off">
                                            2</div>
                                        <div onclick="clearAuto();Mea(2)" class="Swich-off">
