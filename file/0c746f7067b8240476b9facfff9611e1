jQuery(function(){ 
	var mapcontainer = jQuery('#mapcontainer');
	if (mapcontainer.length > 0) {
		var pos = mapcontainer.attr("data").split(',');
		var l0 = pos[0];
		var l1 = pos[1];
		loadGoogleMap(mapcontainer,l1,l0);
		loadGoogleMapStyle();
	};
	function loadGoogleMap(mapcontainer,l0,l1){
		var mapOptions = {
			credentials: "AkdJWYXVIyECXAt2XtJPar1msOEOBUar6heRuX_3csAvHb79SKfQ170dXoNEi40Z",
			center: new Microsoft.Maps.Location(l0, l1),
			mapTypeId: Microsoft.Maps.MapTypeId.auto,/* road, birdseye */
			zoom: 13,
			showDashboard: false,
			showMapTypeSelector: true,
			showScalebar: true,
			disableBirdseye: true,
			showCopyright: false,
			enableSearchLogo: false,
			enableClickableLogo: false
		}
		var map = new Microsoft.Maps.Map(mapcontainer[0], mapOptions);
		var center = map.getCenter();
		var pin1 = new Microsoft.Maps.Pushpin(center, {text: 'Qu'}); 
		map.entities.push(pin1);
		// var curPos = remote_ip_info['country'];
		// if (curPos == "\u4e2d\u56fd") {
		// 	// alert('1');
		// } else {
		// 	// alert('0');
		// };  
		jQuery('#mapcontainer_parent .mapButton').on('click', function(){
			var layer = jQuery('#mapcontainer_parent .mapLayer');
			if (layer.is(":hidden")) {
				layer.show();
			} else{
				layer.hide();
			};
		});
	}
	function loadGoogleMapStyle(){
		// jQuery('#roomsType tr td:eq(0)').width(99);
		// jQuery('#roomsType tr').children().width(99);
		jQuery("#roomsType tr").each(function(){
			jQuery(this).children().eq(0).width(60);
			jQuery(this).children().eq(1).css({"text-align":"left"});
			jQuery(this).children().eq(2).css({"width":"15%"});
			jQuery(this).children().eq(3).css({"width":"15%"});
		});
		jQuery('.dlhorizon dt').css({"float":"left","width":"30%"});
		jQuery('.dlhorizon dd').css({"float":"left","width":"70%"});
		jQuery.fn.raty.defaults.path = '/wp-content/themes/hueman/ext/lib_jquery_raty/images';
		jQuery('#stars').raty({ readOnly: true, score: jQuery('#stars').attr('data')/*, starOff : 'star-off-big.png', starOn  : 'star-on-big.png'*/ });
		jQuery("#slider").responsiveSlides({manualControls: '#slider-pager',maxwidth: 800 });
	}
}); 
