            return false;
        });
    });

    function errorhandle_recommendadd(msg, param) {
        if (param['recommendv'] == '+1') {
            var objv = $('#recommend_add');
            objv.css("display", "");
            objv.html(parseInt(objv.html()) + 1);
        }
    }
    function succeedhandle_favbtn(url, msg, param) {
        if ($.trim(msg) == '信息收藏成功') {
            clearInterval(setTimeout_location);
            $('.favbtn span').text(parseInt($('.favbtn span').text()) + 1);
            setTimeout(function() {
                popup.close();
            }, '1000');
        }
    }
    function succeedhandle_attch_all(url, msg, param) {
        if ($.trim(msg) == "本帖所有附件购买成功") {
            $('.attachment_buy').remove();
            $('.wq_attachments').each(function(i) {
                $(this).attr('class', null).attr('href', $(this).attr('data'));
            });
            setTimeout(function() {
                popup.close();
            }, '1000');
        }
    }
    function errorhandle_recommendsubtract(msg, param) {
        if (param['recommendv'] == '-1') {
            var objv = $('#recommend_subtract');
            objv.css("display", "");
            objv.html(parseInt(objv.html()) + 1);
        }
    }
