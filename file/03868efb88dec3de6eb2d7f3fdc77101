function delay_js(url){var type=url.split("."),file=type[type.length-1];if(file=="css"){var obj=document.createElement("link"),lnk="href",tp="text/css";obj.setAttribute("rel","stylesheet");}else var obj=document.createElement("script"),lnk="src",tp="text/javascript";obj.setAttribute(lnk,url);obj.setAttribute("type",tp);file=="css"?document.getElementsByTagName("head")[0].appendChild(obj):document.body.appendChild(obj);return obj;};
function request(paras) { var url = location.href; var paraString = url.substring(url.indexOf("?") + 1, url.length).split("&"); var paraObj = {}; for (i = 0; j = paraString[i]; i++) { paraObj[j.substring(0, j.indexOf("=")).toLowerCase()] = j.substring(j.indexOf("=") + 1, j.length); }; var returnValue = paraObj[paras.toLowerCase()];; if (typeof (returnValue) == "undefined") { return ""; } else { return returnValue; }}
function setOpacity(elem,num){if(elem.filters){elem.style.filter="alpha(opacity="+num+")"}else{elem.style.opacity=num/100}}function fadeIn(elem,aspeed){elem.style.display=elem.$oldDisplay||"";setOpacity(elem,0);for(var j=0;j<=100;j+=0.5){(function(){var pos=j;setTimeout(function(){setOpacity(elem,pos)},(pos+1)*aspeed)})()}}function loadroll(objImgId,objBtnId){var t=4500,toleft=null,toright=null,objImg=document.getElementById(objImgId),objBtn=document.getElementById(objBtnId),arrImg=objImg.getElementsByTagName('li');var arrBtn=objBtn.getElementsByTagName('li'),intIni=0,setInter=setInterval(funSetime=function(){intIni++;if(intIni>=arrImg.length){intIni=0};funSW(intIni,0)},t);var funSW=function(n,b){var oImg=arrImg[n].firstChild;for(var i=arrBtn.length-1;i>=0;i--){arrBtn[i].className='';if(arrImg[i].className=="s"){arrImg[i].className=''}else{arrImg[i].className=''}};if(n>=3&&toleft==null){clearInterval(toright);toright=null;toleft=setInterval(function(){if(parseInt(objBtn.style.marginLeft)>-187){objBtn.style.marginLeft=parseInt(objBtn.style.marginLeft)-1+"px"}else{clearInterval(toleft);toleft=null;}},5)};if(n<=2&&toright==null){clearInterval(toleft);toleft=null;toright=setInterval(function(){if(parseInt(objBtn.style.marginLeft)<0){objBtn.style.marginLeft=parseInt(objBtn.style.marginLeft)+1+"px"}else{clearInterval(toright);toright=null;}},5)};arrBtn[n].className='on';if(b==1){fadeIn(arrImg[n],0)}else{fadeIn(arrImg[n],10)}arrImg[n].className='s'};for(var i=arrBtn.length-1;i>=0;i--){arrImg[i].cnt=arrBtn[i].cnt=i;arrImg[i].onfocus=function(){this.blur()};arrImg[i].onmouseover=function(){clearInterval(setInter)};arrImg[i].onmouseout=function(){intIni=this.cnt;setInter=setInterval(funSetime=function(){intIni++;if(intIni>=arrImg.length){intIni=0};funSW(intIni,0)},t)};arrBtn[i].onmouseover=function(){clearInterval(setInter);if(this.className!="on"){funSW(this.cnt,1)}};arrBtn[i].onfocus=function(){this.blur()};arrBtn[i].onmouseout=function(){intIni=this.cnt;setInter=setInterval(funSetime=function(){intIni++;if(intIni>=arrImg.length){intIni=0};funSW(intIni,0)},t)}};arrImg[0].className="s";arrBtn[0].className='on'};function tagbox(is,nav,box) {navbox=document.getElementById(nav).getElementsByTagName("li");for(i=0;i<navbox.length;i++) {if(i==is){navbox[i].className="on";document.getElementById(box+i).style.display="block";}else{navbox[i].className="";document.getElementById(box+i).style.display="none";}}}
function setevent(nav,box) {objs=document.getElementById(nav).getElementsByTagName("li");for(i=0;i<objs.length;i++) {objs[i].int=i;objs[i].onmouseover=function(){tagbox(this.int,nav,box)}}};
var /*_nav=$("#nav"),*/_index=$("#index"),/*_main_bg=$("#main_bg"),*/_main=$("#main"),_winHeight,_direction=0,_scrollTop,_navTop,_navHeight=178,to1=false,to2=false,ie6=!-[1,]&&!window.XMLHttpRequest;
function mainRun(){loadroll("roll_box","roll_nav");/*delay_js("base.js");*/}
window.onload=function(){document.getElementById("main").innerHTML=document.getElementById("main_inner").value;mainRun();if(request("p")=="main"){$('html,body').animate({scrollTop:_winHeight}, 0 );}}
function down(){$('html,body').animate({scrollTop:_winHeight}, "slow" );$('.logo a').eq(0).attr("href","javascript:toPage(-1);");$('#nav a').eq(0).attr("href","javascript:void(0);");_nav.animate({top:0}, "slow" );var rollPage=setInterval(function(){if(!$('html,body').is(":animated")){_nav.css("position","absolute").css("top",_winHeight-0+"px");clearInterval(rollPage);_direction=0;setBg(1);setNav(true);if(!to2){pgvSendClick({hottag:'index.screen.main'});to2=true;}}},100);}
function up(){$('html,body').animate({scrollTop:0}, "slow" );$('.logo a').eq(0).attr("href","javascript:void(0);");$('#nav a').eq(0).attr("href","javascript:toPage(1);");var _newtop=$(window).height()-_navHeight-0>_navTop?$(window).height()-_navHeight:_navTop;_nav.animate({top:_newtop+"px"}, "slow" );setBg(0);setNav(false);var rollPage=setInterval(function(){if(!$('html,body').is(":animated")){if(_newtop!=_navTop&&!ie6){_nav.css("position","fixed").css("top","auto").css("bottom","0")};clearInterval(rollPage);_direction=0;if(!to1){pgvSendClick({hottag:'index.screen.index'});to1=true;}}},100);}
function toPage(direction){if(direction==1){down();}else if(direction==-1){up();}}
