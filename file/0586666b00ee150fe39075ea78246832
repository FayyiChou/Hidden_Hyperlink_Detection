  $(document).ready(function() {
  $('.change_city .main_city').click(function(elem){
  var scope = angular.element("body").scope();
  scope.$apply(function (_scope) {
      var message = [
  "<h1>Обращаем Ваше Внимание!</h1>",
      "<div class='indent_top_20'>При смене региона в момент оформления заказа Вы будете перемещены на главную страницу сайта.<br/> Процесс оформления заказа будет утерян.</div>"
      ].join(' ');
      _scope.showConfirm(message);
      _scope.ng_click_confirm = function(){
        location.href=$(elem.currentTarget).attr('href');
      }
  });
  return false;
  });
  if ($.cookie('city_id') && $.cookie('city_confirmed') != '1') {
        
            $("#confirmCityName").text($.cookie('city_name'));
            $("#confirmCityLink").attr('href', '/User/SetCity/'+$.cookie('city_id')+'/'+$.cookie('city_name'));
            $("#confirmCity").show();
        }
    });
</script>

