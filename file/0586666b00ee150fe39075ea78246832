        <script type="text/javascript">
    $(document).ready(function() {
        $(".main_filter .select_step.step0").change(function() {
            $(".main_filter .select_step.step1").trigger("change");
        });

        $(".main_filter .select_step.step1").change(function() {
            var mfaId = $(this).val();
            var year = $(".main_filter .select_step.step0").val();
            if (mfaId != '') {
                $('.str-bubble').hide();
//                var brand = $(this).find("option:selected").text();
//                $(".main_filter .logo").attr("src", "/images/brandLogo/"+brand+".png");
                $.ajax({
                    url: "/Catalog/Main.json/"+mfaId,
                    success: function(response) {
                        if (response.models) {
                            $(".main_filter .select_step.step2").html("<option value=''>Модель</option>").val("");
                            $.each(response.models, function(k, v) {
                                if ((year == "" || v.mod_pcon_start == 0|| Math.floor(v.mod_pcon_start / 100) <= year) &&
                                        (year == "" || v.mod_pcon_end == 0 || Math.floor(v.mod_pcon_end / 100) >= year)) {
                                    $(".main_filter .select_step.step2").append("<option value='"+v.mod_id+"' data-logo='"+v.logo+"'>"+v.mod_name+"</option>");
                                }
                            });
                            $(".main_filter .select_step.step2").removeAttr("disabled");

                            $(".main_filter .logo").attr("src", response.manufacturer_logo);

                        }
                    }
                });
            }
            $(".main_filter .select_step.step3").val("").attr("disabled", "disabled");
            $(".main_filter .link").hide();
            var lax = $(this).find("option:selected").attr("data-original");
            if (lax) {
                $(".main_filter .laximo_link").attr("href", "/Laximo/Cars/"+lax).show();
            }
        });

        $(".main_filter .select_step.step2").change(function() {
            var mfaId = $(".main_filter .select_step.step1").val();
            var year = $(".main_filter .select_step.step0").val();
            var modId = $(this).val();
            if (modId != '') {
                var logo = $(this).find("option:selected").attr("data-logo");
                $(".main_filter .logo").attr("src", logo);

                $.ajax({
                    url: "/Catalog/Main.json/"+mfaId+"/"+modId,
                    success: function(response) {
                        if (response.types) {
                            $(".main_filter .select_step.step3").html("<option value=''>Модификация</option>").val("");
                            $.each(response.types, function(k, v) {
                                if ((year == "" || v.typ_pcon_start == 0 || Math.floor(v.typ_pcon_start / 100) <= year) &&
                                        (year == "" || v.typ_pcon_end == 0 || Math.floor(v.typ_pcon_end / 100) >= year)) {
                                    $(".main_filter .select_step.step3").append("<option value='"+v.typ_id+"'>"+v.typ_mmt_cds+" "+v.typ_hp_from+"л.с.</option>");
                                }
                            });
                            $(".main_filter .select_step.step3").removeAttr("disabled");
                        }
                    }
                });
            }
            $(".main_filter .link").hide();
        });

        $(".main_filter .select_step.step3").change(function() {
            var typId = $(this).val();
            if (typId != '') {
                $(".main_filter .link").attr("href", "/tree/"+typId).show();
            }
        });

        $('.tab_lnk a').click(function(e){
            e.preventDefault();
            $('.tab_lnk a').removeClass('active');
            $(this).addClass('active');
            $('.tab_content .tab').addClass('hidden');
            $('.tab_content .'+ $(this).attr('href').substr(1)).removeClass('hidden');
        });

        /*Menu with car*/
        $('.list_parts .main_item').hover(function(){
            $('.list_parts_img').attr('src', '/images/avto_part/' + $(this).attr('data-index') + '.jpg');
        }, function(){
            $('.list_parts_img').attr('src', '/images/avto_part/0.jpg');
        });

        $('.list_parts .main_item .title').click(function(){
            var current = $(this).parent();
            if(current.hasClass('open')) {
                $('.list_parts .sub_menu').addClass('hidden');
                $('.list_parts .main_item').removeClass('open');
            }
            else{
                $('.list_parts .main_item').removeClass('open');
                $('.list_parts .sub_menu').addClass('hidden');
                $('.sub_menu', current).removeClass('hidden');
                current.addClass('open');
            }
            
        });
    });
</script>
