<script>
function show(n)
{
var o = document.getElementById('zxqy_title').getElementsByTagName("a");
var c = document.getElementById('zxqy_content').getElementsByTagName("div");
for(i = 0; l = o.length, i < l; o[i].className = '',c[i].style.display = 'none',i++ );
o[n].className = 'over';
c[n].style.display = 'block';
}
</script>
