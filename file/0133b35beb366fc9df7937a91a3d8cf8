  	function turnPage(nextPage){		
		window.location.href = "/portal/news/notice?pid=0&typeid=0&pageNum="+nextPage+"&searchKey=";
    } 
	
	function toInfo (newsid) {
		var refer = encodeURIComponent("/portal/news/notice?pid=0&typeid=0&pageNum=1");
				location.href = "/portal/news/info?id="+newsid+"&refer="+refer;
