function adjustAd(ad,top,left){ad.style.position='absolute';var dom=null;if(document.compatMode=='CSS1Compat'){dom=document.documentElement;}else{dom=document.body;}if(top){if(top<=1){ad.style.top=dom.scrollTop+top*dom.clientHeight+'px';}else{ad.style.top=dom.scrollTop+top+'px';}}if(left){if(left<=1){ad.style.left=dom.scrollLeft+left*dom.clientWidth+'px';}else{ad.style.left=dom.scrollLeft+left+'px';}}}
function getBrowserInfo(){var br={};var ua=navigator.userAgent.toLowerCase();var ma;(ma=ua.match(/msie ([\d.]+)/)) ? br.ie=ma[1]:(ma=ua.match(/firefox\/([\d.]+)/)) ? br.firefox=ma[1]:(ma=ua.match(/chrome\/([\d.]+)/)) ? br.chrome=ma[1]:(ma=ua.match(/opera.([\d.]+)/)) ? br.opera=ma[1]:(ma=ua.match(/version\/([\d.]+).*safari/)) ? br.safari=ma[1]:0;return br;}

var adBox = document.getElementById('inner_ads');
if(adBox != null) {
	var adDiv = adBox.getElementsByTagName('div');
	var ad1 = adDiv[0];
	var ad2 = adDiv[1];
