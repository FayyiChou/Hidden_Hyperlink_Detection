                function cnLength(Str) {
	                var escStr = escape(Str);
	                var numI = 0;
	                var escStrlen = escStr.length;
	                for (i = 0;  i < escStrlen;  i++) 
	                if(escStr.charAt(i) == '%') 
	                if(escStr.charAt(++i) == 'u')  numI ++;
	                return Str.length+numI;
                }
