    <link rel="stylesheet" href="/Public/static/css/footerstyle.css" />
    <link rel="stylesheet" href="/Public/static/css/colorbox.css" />
    <script type="text/javascript" src="/Public/static/js/jquery.colorbox.js"></script>
     <script type="text/javascript">
     $("#feed_question").colorbox({
         title: "",
         inline: true,
         transition: "none",
         width: "920px",
         
         href: ".feedback-modal",
         onClosed:function(){
        	 $('.feedback_advice').show();
             $('.feedback-success').hide();
         }
     });
     
     function feedbackAdvice(){
    	
    	var  u_number = $('#u_number').val();//反馈人
    	var  ef_title = $('#ef_title').val();//标题
    	var  ef_content = $('#ef_content').val();//内容
    	var  ef_contact = $('#ef_contact').val();//联系方式
    	if(!$.trim($("#ef_title").val())){
  			$("#ef_title_waring").css('display','block');
  			$("#ef_title_waring").text('请输入反馈主题');
  			$("#ef_title").focus();
  			return false;
  		}else{
  			$("#ef_title_waring").css('display','none');
  			$("#ef_title_waring").text('');
  		}
    	if(!$.trim($("#ef_content").val())){
  			$("#ef_content_waring").css('display','block');
  			$("#ef_content_waring").text('请输入反馈内容');
  			$("#ef_content").focus();
  			return false;
  		}else{
  			$("#ef_content_waring").css('display','none');
  			$("#ef_content_waring").text('');
  		}
    	$.ajax({
            url:'/index.php/Index/feedback',                
            datatype:"json",
            type: "post", 
            data:{
            u_number:u_number,
            ef_title:ef_title,
            ef_content:ef_content,
            ef_contact:ef_contact
        	},
            success:function(result){  
         		$('.feedback_advice').hide();
                $('.feedback-success').show();
            }
    	});
     } 
    </script>
    <!-- 底部 -->       
