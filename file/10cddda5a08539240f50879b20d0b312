<script type="text/javascript">
$(function()
{
	$("#btn_search").click(function()
	{
		var kw=$("#kw").val();
		kw= kw=="请输入产品关键词" ? "" : kw;
		kw= kw=="keywords" ? "" : kw;
		var types=$("#types").val();

		location.href="/index.php/product/search/kw/"+kw;
	});
	
	$("#btn_search").hover(function(){
		$(this).css("background-color","#400303");
	},function(){
		$(this).css("background-color","#AB0102");
	});
	
	$("#kw").click(function()
	{
		if($(this).val()=="请输入产品关键词"||$(this).val()=='keywords')
		{
			$(this).attr("value","");
		}
	});
	
	$("#kw").blur(function()
	{
		if($(this).val()=="")
		{
			$(this).attr("value","请输入产品关键词");
		}
	});
});
</script>

