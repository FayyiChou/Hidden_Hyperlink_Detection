<!--高级筛选-->
<script type="text/javascript">
$(function () {
    $(".filter a").click(function(event) {
        var i = $(".filter a").index(this)
        if(i!=$(".filter a").length-1){
            var m = $(this).find("span");
            if(!m.hasClass('arrow')){
                if(m.hasClass('arrowDown')){
                    m.removeClass().addClass('arrowUp')
                }else{
                    m.removeClass().addClass('arrowDown')
                }
            }
            $(".filter a").removeClass('cur order')
            $(this).addClass('cur order') 
        }
    });
    //高级筛选显示隐藏
     $("#af").click(function () {
        $(".advancedFilter").fadeIn(500);
        $(".af").removeClass("fadeOutRightBig").addClass("fadeInRightBig");
     })
     $(".afTop a").click(function () {
        $(".af").removeClass("fadeInRightBig").addClass("fadeOutRightBig");
        $(".advancedFilter").delay(200).fadeOut();
     })
    /////////////////////////////////////////////////////////////////
    //编辑信息
    var name = "";
    $(".sideMenu h3").click(function () {
        if (name == "" || name != $(this).attr("name")) {
            $(this).addClass("on");
            name = $(this).attr("name");
            //展开信息
            $("#h_" + name).slideDown(500);
        }
        else if (name == $(this).attr("name")) {
            $(this).removeClass("on");
            //收起信息
            $("#h_" + name).slideUp(500);
            name = "";
        }
    });
    $(".more").click(function(event) {
       $(".PdList ul li").show(); 
       $(this).hide();
    });
    
});
</script>
