<script>

var websiteCode = "main"; //站点Code
var categoryCode = "jsj"; //产品栏目Code
var ajaxFlag = true; //请求开关
var b = [] ;

$(function () {
    $(".more").click(function(event) {
       $(".PdList ul li").show(); 
       $(this).hide();
    });
    getProductList(1);
});

//从自定义属性数据中获取属性值
function getAttrVal(jsonObj, attrKey){
    var result = "";
    for(var i = 0; i < jsonObj.length; i ++){
        if(jsonObj[i].name == attrKey){
            result =  jsonObj[i].value;
            break;
        }
    }
    return result;
}
//获取搜索参数
function getSearchParams(pageNum){
    var params = new Object();
    //搜索面板过滤条件
    var attrsKey = new Array();
    var attrsValue = new Array();

    for(var i = 0;i<$(".sideMenu ul").length;i++){
        var key = $(".sideMenu ul").eq(i).data("key");
        var value = $('input[name='+(i+1)+']:checked').val();
        console.log(key+","+value)
        if(value != "不限"){
            attrsKey.push(key);
            attrsValue.push(value);
        }
    }
    // attrsKey.push("特色功能")
    // attrsValue.push("WiFi控制")
    // attrsKey.push("特色功能")
    // attrsValue.push("除PM2.5")
    
    params.attrsKey = attrsKey;
    params.attrsValue = attrsValue;

    //搜索排序方式
    var orderType = $(".order").data("word");
    if(orderType != ""){
        params.orderAttrKey = orderType;
    }
    var orderAttrModel = "desc";
    if($(".order").find("span").hasClass('arrowUp')){
        orderAttrModel = "asc";
    }
    params.orderAttrModel = orderAttrModel;

    //搜索分页参数
    params.pageNum = 1;
    params.pageSize = 999;
    //栏目及站点参数
    params.websiteCode = websiteCode;
    params.categoryCode = categoryCode;
    return params;
}
//立刻搜索
function changeOrder(){
    getProductList(1);
}
//获取产品数据列表
function getProductList(pageNum){
    if(ajaxFlag){
        var num = 0;
        ajaxFlag = false;
        $.post("/prj/product/getList", getSearchParams(pageNum), function(json){
            ajaxFlag = true;
            $("#searchProductCon .product_box").remove(); //清除已有数据
            if(json.status == 1 && json.data.productTotal > 0){
                $(".PdList").html('<ul class="clearfix"></ul>')
                
                var productHtml = ""; //产品数据列表HTML
                for(var i = 0; i < json.data.productList.length; i ++){
                    var f = true;
                    var productData = json.data.productList[i];
                    var productPhoto = productData.photoList.length > 0 ? productData.photoList[0].path : "";
                    var sp = getAttrVal(productData.attributeList, "特色功能");
                    for(var j= 0;j<b.length;j++){
                        if(sp.indexOf(b[j]) == -1){
                            f = false;
                            break;
                        }
                    }
                    if(f){
                        num++
                        productHtml+='<li>'+
                            '<a href="'+productData.detailLink+'">'+
                            '<div class="middle"><img src="'+productPhoto+'"></div>'+
                            '<div class="name">'+
                                '<h3> '+ productData.name +' </h3>'+
                                '<div class="grayF">'+getAttrVal(productData.attributeList, "型号") +'&nbsp;&nbsp;' + getAttrVal(productData.attributeList, "能效等级") +'</div>'+
                            '</div>'+
                            '</a>'+
                        '</li>';
                    }           
                }

                $(".PdList ul").append(productHtml); //插入新数据

                $(".PdList ul li").hide(); //隐藏结果提示
                
                $(".PdList ul li:lt(4)").show(); 
                if(num<=4){
                    $(".more").hide();
                }else{
                    $(".more").show();
                }
            }else{
                $(".more").hide();
                $(".PdList").html("<div style='text-align:center'>抱歉，在产品库中没有找到相关产品。</div>").show(); //显示结果提示
            }
        }, "json");
    }
}
</script>
