		var popSearch = function(){
			var obj = $('.inner_header_pop');
			if(obj.is(":hidden")){
				obj.slideDown()
			}else{
				obj.slideUp()
			}
		}	
		function goSearch(){
			var searchWord = $("#keyWord").val().trim();
			if(searchWord == ""){
				alert("请输入搜索关键字");
				$("#keyWord").focus();
				return false;
			}
			window.location.href = '/zwydb/qtsjb/ssjgsjb1/index.shtml#'+encodeURI(searchWord);								
		}
	</script>
