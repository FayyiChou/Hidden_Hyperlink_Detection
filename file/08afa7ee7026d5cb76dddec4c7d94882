    //跳转的URL地址
    function jump2url()
    { 
    jumpUrl=cpADlink[adNum];
    jumpTarget='_blank';
    if (jumpUrl != '')
    { 
        if (jumpTarget != '')
            window.open(jumpUrl,jumpTarget);
        else location.href=jumpUrl;
        } 
