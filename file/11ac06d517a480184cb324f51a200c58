<script language="JavaScript">
function resizepic(thispic)
{
if(thispic.width>900){thispic.height=thispic.height*900/thispic.width;thispic.width=900;}
}
function bbimg(o)
{
return false;
}
function getTags(parentobj, tag)
{
	if (parentobj == null){
		return new Array();
	}else if (typeof parentobj.getElementsByTagName != 'undefined'){
		return parentobj.getElementsByTagName(tag);
	}else if (parentobj.all && parentobj.all.tags){
		return parentobj.all.tags(tag);
	}else{
		return new Array();
	}
}
function ReSizeAllPic(){
	var imgs=getTags(document.getElementById("content"),"img");
	for(i=0;i<imgs.length;i++){
		var img = imgs[i];
		if(img .width>900){
			img .height=img .height*900/img .width;
			img .width=900;
		}
	}
}
