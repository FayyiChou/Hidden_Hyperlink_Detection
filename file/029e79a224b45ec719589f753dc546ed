	$(".lyd .lmain2 .lbtn").click(function(){
		var t = $(window).scrollTop();
		var hei=$(".lyd .lmain1").outerHeight(); 
		$('body,html').stop().animate({'scrollTop':hei},300)
	})

	var bian3=0;
  	$(".lyd .lmain3 .lbtn div").bind("mouseenter",function(){
		bian3=$(this).index();
		var x=-400*bian3
		$(".lyd .lmain3 ul").stop().animate({marginLeft:x+"px"});
		$(".lyd .lmain3 .lbtn div").removeClass("on");
		$(".lyd .lmain3 .lbtn div").eq(bian3).addClass("on");
	});
	function show3(){
		bian3++;
	    if (bian3==$(".lyd .lmain3 li").length-2) {
			bian3=0;
		}
		var x=-400*bian3
		$(".lyd .lmain3 ul").stop().animate({marginLeft:x+"px"});
		$(".lyd .lmain3 .lbtn div").removeClass("on");
		$(".lyd .lmain3 .lbtn div").eq(bian3).addClass("on");
	}
	var dodo3=setInterval(show3,5000);

	function test3() 
	{ 
		dodo3=setInterval(show3,5000);
	} 
	$(".lyd .lmain3 *").bind("mouseenter",function(){
		clearInterval(dodo3);
	});
	$(".lyd .lmain3 *").bind("mouseleave",function(){
	    clearInterval(dodo3);
		test3();
	});
</script>
