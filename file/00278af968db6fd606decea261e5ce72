	<script type="text/javascript">
	var runtime = 0;
	var banner_width,banner_height;

	function page_init(){

		$(window).bind("scroll", function(event){
			$(".lazyanimate").each(function(){
			   var fold = $(window).height() + $(window).scrollTop();
			   if( fold > $(this).offset().top){
			       $(this).trigger("appear");
			   }
		    });
		});

		$(".lazyanimate").on("appear",function(){
			$(this).addClass('appear');
		});

		$img = $('#banner img');
		var loadimgtime = 0;
	    var imgnum = $img.size();
	    if(imgnum == 0){
	      weiche_banner_init();
	    }else{
	      $img.each(function(i) {
	      $(this).on('load error',function(){
	            loadimgtime++;
	           // console.log(loadimgtime);
	            if(loadimgtime>=imgnum){
	              weiche_banner_init();
	            }
	        });
	      });
	    }
		window.onresize=weiche_banner_init;
	}

	$(document).ready(function(){
		$('.wechat-ico').hover(function(){
			$('#footer .wechat').stop().show().animate({left:105,opacity:1},500);
		},function(){
			$('#footer .wechat').stop().animate({left:0,opacity:0},500,function(){$(this).hide();});
		});

		$('.qq-ico').hover(function(){
			$('#footer .qq').stop().show().animate({top:-45,opacity:1},500);
		},function(){
			$('#footer .qq').stop().animate({top:0,opacity:0},500,function(){$(this).hide();});
		});
		var hashchange = function(){
			var hashnow = location.hash.replace(/#/, "");
			if(hashnow == ''){
				hashnow = "weiche";
			}

			$("#container").load(hashnow+".html",page_init);
			$('#header li').removeClass('current');
			$('#header li>a[href="#'+hashnow+'"]').parent().addClass('current');
		};
		window.onhashchange= hashchange;
		hashchange();
	});
	</script>
