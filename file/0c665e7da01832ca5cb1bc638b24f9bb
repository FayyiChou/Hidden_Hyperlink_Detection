<script type="text/javascript">
    var companyId  = '47535121';
    var hq_userId = '676517923';
    GJ.use('jquery', function(){
        $.ajax({
            type: "POST",
            url: "/ajax.php?_pdt=zhaopin&module=getHighQualityCompanyInfo",
            data:{ company_id: companyId,user_id:hq_userId},
            dataType: "json",
            timeout: 5000,
            success: function(data){
                if(data.code == 1){
                    //登录时间
                    var curData = data.body;
                    var loginDate = "";
                    if(curData['login_time'] == 0){
                        loginDate = "今天";
                    }else if(curData['login_time'] > 0 && curData['login_time'] <= 7){
                        loginDate = curData['login_time'] + "天前";
                    }else if(curData['login_time'] > 7 && curData['login_time'] <= 14){
                        loginDate = "一周前";
                    }else if(curData['login_time'] > 14 && curData['login_time'] <= 30){
                        loginDate = "两周前";
                    }else if(curData['login_time'] > 30 && curData['login_time'] <= 60){
                        loginDate = "一个月前";
                    }else if(curData['login_time'] > 60){
                        loginDate = "两个月前";
                    }
                    $(".last_login_time").html(loginDate);
                    $("#online_post").html('在招职位：' + curData['postsCount'] + '个');
                    $("#online_resume").html('已收简历：' + curData['resumeCount'] + '份');
                    $("#online_phone").html('电话求职：' + curData['callNum'] + '人');
                }
            }
        });
    });
</script>

