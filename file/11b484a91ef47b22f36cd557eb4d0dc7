	<script src="//g.alicdn.com/sj/lib/jquery/dist/jquery.min.js" charset="UTF-8"></script>
	<script src="//g.alicdn.com/sj/qn-portal/0.1.1/script/app-mobile.js" charset="UTF-8"></script>
	<script>
		KISSY.use('node,anim,dom',function(S,Node,Anim,Dom){
			
			/*当屏幕宽度小于1400的时候，浮层贴边*/
			if(document.body.clientWidth<1400){
				S.all(".hs-fix-floor").css("right","0px");
			}
			var DOM = S.DOM;
			/* 当前是第一页吗？*/
			var isFirstPage = true;
			/*离顶部的距离*/
			var t = 0;
			/*是否开始过动画*/ 
			var finshAdmin= [false,false,false,false,false];
			S.all(".hs-fix-li1 a").on('click', function(event) {
				event.preventDefault();
				S.all(".two-code").show();
			});
			S.all(".two-code  .close-btn").on('click', function(event) {
				event.preventDefault();
				S.all(".two-code").hide();
			});
			S.all(".to-top").on('click', function(event) {
				toTop();
				/* Act on the event */
			});
			function toTop(){
				S.all("body").removeClass('scrollable');
				new Anim(".top-div",{top:"0"},1,"easing",function(){
					isFirstPage = true;
					new Anim(".hs-feiji",{top:"160px",left:"914px"},1,"easing",function(){}).stop().run();
				}).stop().run();
			}
			new Anim(".hs-feiji",{top:"160px",left:"914px"},1,"easing",function(){}).stop().run();
			S.all("body").on('mousewheel',function(e) {
				if(Anim.isRunning(".top-div")){
					return;
				}
				if(isFirstPage){
					Anim.stop(".hs-feiji");
					Dom.css(".hs-kong-div1",{display:"block"});
					Dom.css(".hs-kong-div2",{display:"block"});
					Dom.css(".hs-kong-div3",{display:"block"});
					new Anim(".top-div",{top:"-200%"},1,"easing",function(){
						isFirstPage = false;
						S.all("body").addClass('scrollable');
						doMyAdmin(0,0,false);
					}).stop().run();
				}else{
					if(t==0&&e.deltaY>0&&!isFirstPage){
						new Anim(".top-div",{top:"0"},1,"easing",function(){
							isFirstPage = true;
							S.all("body").removeClass('scrollable');
							new Anim(".hs-feiji",{top:"160px",left:"914px"},1,"easing",function(){}).stop().run();
						}).stop().run();
					}
				}
			});
			window.onscroll = function(){
			      t = document.documentElement.scrollTop || document.body.scrollTop; 
			      var num = showIndex(t);
				var isStart = finshAdmin[num];
				var realnum = reallndex(t);
				doMyAdmin(num,realnum,isStart);
			}  
			/*执行第num屏幕动画 ，boolean 如果是true表示已经开始了动画不在开始*/ 
			function doMyAdmin(num,realnum,isStart){
				Anim.stop();
				if(isStart){
					return;
				}else{
					getPower(num,0,4);
					if(num ==realnum){
						returnAnimCss();
					}
					switch(num){
						// case 0:{
							// new Anim(".hs-feiji",{top:"310px",left:"914px"},1,"easing",function(){}).stop().run();
						// } break;
						case 0:{
							setTimeout(function(){S.all(".hs-kong-div1").fadeOut()},600);
							setTimeout(function(){S.all(".hs-kong-div2").fadeOut()},1200);
							setTimeout(function(){S.all(".hs-kong-div3").fadeOut()},1800);
							finshAdmin[num] = true;
						} break;
						case 1:{
							setTimeout(function(){showNum(236,"hs_n1",1);},400);
							setTimeout(function(){showNum(66,"hs_n2",2);},800);
							setTimeout(function(){showNum(89,"hs_n3",3);},1200);
							setTimeout(function(){showNum(38,"hs_n4",4);},1600);
							setTimeout(function(){S.all(".hs-lingling").show();showNum(6541,"hs_n5",5);},2000);
							setTimeout(function(){showNum(12,"hs_n6",6);},2400);
							finshAdmin[num] = true;
						} break;
						case 2:{
							new Anim(".hs-box4-li1",{opacity:'1',top:"461px"},{duration: 1, delay: 0.2},function(){}).stop().run();
							new Anim(".hs-box4-li2",{opacity:'1',top:"431px"},{duration: 1, delay: 0.4},function(){}).stop().run();
							new Anim(".hs-box4-li3",{opacity:'1',top:"408px"},{duration: 1, delay: 0.6},function(){}).stop().run();
							new Anim(".hs-box4-li4",{opacity:'1',top:"391px"},{duration: 1, delay: 0.8},function(){}).stop().run();
							new Anim(".hs-box4-li5",{opacity:'1',top:"520px"},{duration: 1, delay: 1},function(){}).stop().run();
							finshAdmin[num] = true;
						} break;
						case 3:{
							S.all(".hs-box5-4").fadeIn(2);
							finshAdmin[num] = true;
						} break;
						case 4:{
							new Anim(".hs-box6-1",{left:"80px"},{duration: 1, delay: 0.5},function(){}).stop().run();
							new Anim(".hs-box6-2",{right:"80px"},{duration: 1, delay: 1.5},function(){}).stop().run();
							new Anim(".hs-box6-3",{right:"80px"},{duration: 1, delay: 1},function(){}).stop().run();
							finshAdmin[num] = true;
						} break;
					}
					
				}
			}
			/*打开动画权限*/
			function getPower(num,minNum,maxNum){
				if(num==minNum){
					finshAdmin[num+1] = false;
				}else if(num == maxNum){
					finshAdmin[num-1] = false;
				}else {
					finshAdmin[num+1] = false;
					finshAdmin[num-1] = false;
				}
			}
			/*还原所有的动画所需CSS*/
			function returnAnimCss(){
				Dom.css('.hs-feiji', { top: '425px', left: '1200px'});
				Dom.css(".hs-kong-div1",{display:"block"});
				Dom.css(".hs-kong-div2",{display:"block"});
				Dom.css(".hs-kong-div3",{display:"block"});
				S.all(".hs_num").html("");
				Dom.css(".hs-lingling",{display:"none"});
				Dom.css(".hs-box4-li1",{opacity:'0',top:"0"});
				Dom.css(".hs-box4-li2",{opacity:'0',top:"0"});
				Dom.css(".hs-box4-li3",{opacity:'0',top:"0"});
				Dom.css(".hs-box4-li4",{opacity:'0',top:"0"});
				Dom.css(".hs-box4-li5",{opacity:'0',top:"0"});
				Dom.css(".hs-box4-li6",{opacity:'0',top:"0"});
				Dom.css(".hs-box5-4",{display:'none'});
				Dom.css(".hs-box6-3",{right:"-1000px"});
				Dom.css(".hs-box6-2",{right:"-1000px"});
				Dom.css(".hs-box6-1",{left:"-1000px"});
			}
			function showIndex(num){
				/*加400的目的是为了提前显示 */
				var index = parseInt((num+400)/800);
				return index;
			}  
			function reallndex(num){
				var index = parseInt(num/800);
				return index;
			}
			function showNum(n,dom,n2){
				_class = "."+dom;
				var it = S.all(_class).children('i'); 
				var len = String(n).length; 
				for(var i=0;i<len;i++){ 
				       if(it.length<=i){ 
				            S.all(_class).append("<i><img class='hs-img-hehe"+n2+"' src='//gtms02.alicdn.com/tps/i2/TB1gnZrIFXXXXXDXpXXz6JTFXXX-14-560.png'  width='100%' /></i>"); 
				        } 
				       var num=String(n).charAt(i); 
				       var y = -parseInt(num)*60; //y轴位置 
				       var obj = S.all(".hs-img-hehe"+n2).item(i); 
				       obj.animate({
				       	marginTop: {
				       		value:y+"px",
				       	}
				       });
				} 
			}
		});
	</script>
      		
