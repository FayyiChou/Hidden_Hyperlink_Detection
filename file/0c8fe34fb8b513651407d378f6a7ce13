;(function($) {
    var base64CurrentUrl = "aHR0cDovL2NzLmZhbmcuYW5qdWtlLmNvbS9sb3VwYW4veGlhbmdjZS00MDA3NDMveGd0Lmh0bWw/ZnJvbQ==";
   $(document).ready(function() {
        var href;
        if(base64CurrentUrl) {
            $("body").on("click.logout", "a", function(e) {
                var elm = $(this);
                if(elm.html() == "退出" && (href = elm.attr("href")) ) {
                    e.preventDefault();
                    window.location.href = href + (href.indexOf("?") >= 0 ? "&" : "?") + "history=" + base64CurrentUrl;
                }
            });
        }
    }); 
})(jQuery);
