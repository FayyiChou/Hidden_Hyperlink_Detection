<script type="text/javascript">
function displayTime(){
    //正在发生的
    $.ajax({
        type : 'POST',
        cache : false,
        dataType : 'json',
        url  : SLPGER.root + '/type/getunderway/pid_id/101',
        data : '',
        success:function(data){ 
              $('#con1').html(data.data);
        }
    });
}
$(document).ready(function(){
    displayTime();
    setInterval("displayTime()",30000);  
})
</script>
<script type="text/javascript">
    //登录注册显示隐藏
    $("#J_sy_deng").click(function(){
        var email = $("#email_dl").val();
        if(!email){
            $.qglobal.tip({content:'邮箱不能为空', icon:'error' ,time:2000});
            return false;
        }              
        var password = $("#password_dl").val();
        if(!password){
            $.qglobal.tip({content:'密码不能为空', icon:'error' ,time:2000});
            return false;
        }
        var verify = 'sy';
        $.ajax({
            type : 'POST',
            cache : false,
            dataType : 'json',
            url  : SLPGER.root + '/auth/login',
            data : {'email':email,'password':password ,'verify':verify}, 
            success:function(result){

                if(result.status === 1){
                    
                    history.go(0);
                }else{
                   $.qglobal.tip({content:result.msg, icon:'error' ,time:2000});
                }
            }
        });         
    })
$(document).ready(function(){
    $(".z_ul li").each(function(x){
        $(this).mouseover(function(){
            $(".z_ul li").removeClass("lixz")
            $(this).addClass("lixz")
            $(".divbox").animate({'margin-top':-300*x },300)
        })
    })
})    
</script>
