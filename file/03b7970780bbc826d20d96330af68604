        <!--输入了多少个文字验证 -->
<script type="text/javascript">
   window.onload = function(){
	var oDiv = document.getElementById('admin');
	var oP = document.getElementById('text_warm');
	var oT = oDiv.getElementsByTagName('textarea')[0];
	var oBtn=document.getElementById('submit');
    var ie = !-[1,];
	var bBtn = true;
	var timer = null;
	var iNum = 0;
    if(ie){
		oT.onpropertychange = toChange;
	}
	else{
		oT.oninput = toChange;
	}
	
	function toChange(){
		
		var num = Math.ceil(getLength(oT.value)/2);
		var oSpan = oDiv.getElementsByTagName('strong')[0];
		
		if(!oSpan){return}
		
		if(num<=250){
			oSpan.innerHTML ='您还可以输入'+(250 - num)+'字';
			oSpan.style.color = '';
		}
		else{
			oSpan.innerHTML = '您已经超出'+(num - 250)+'字';
			oSpan.style.color = 'red';
		}
		if(num>250){
			oBtn.className = 'dis';
		}
		else{
		oBtn.className = '';
		}
	}
	
	oBtn.onclick=function(){
	if(this.className == 'dis'){
	
	alert('您输入的文字已超出！');
	return false;
	}
	
	}
	function getLength(str){
		return String(str).replace(/[^\x00-\xff]/g,'aa').length;
	}
};


</script> 
