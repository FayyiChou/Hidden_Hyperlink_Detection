	<script type="text/javascript">
function getByClass(oParent, sClass)
{
	var aEle=document.getElementsByTagName('*');
	var i=0;
	var aResult=[];
	
	for(i=0;i<aEle.length;i++)
	{
		if(aEle[i].className==sClass)
		{
			aResult.push(aEle[i]);
		}
	}
	
	return aResult;
}

document.onmousemove=function (ev)
{
	var oEvent=ev||event;
	var oSd=document.getElementById('sd');
	var aAc = getByClass(oSd,'AccHide');
    var scrollTop=document.documentElement.scrollTop||document.body.scrollTop;
	var scrollLeft=document.documentElement.scrollLeft||document.body.scrollLeft;
	for(var h=0;h<aAc.length;h++){
	aAc[h].style.left=oEvent.clientX+scrollLeft+10+'px';
	aAc[h].style.top=oEvent.clientY+scrollTop+10+'px';

	}
	}
addEvent(window,'load',function(){
 var oAcc=document.getElementById('Acces');
  var aAccHide=getByClass(oAcc,'AccHide');
  var aBt=getByClass(oAcc,'AccName');
 for(var i=0;i<aBt.length;i++)
  {
    aBt[i].index=i;
    aBt[i].onmouseover=function(){
	  for(var j=0;j<aBt.length;j++)
  {
    
	aAccHide[j].style.display='none';
	}
	aAccHide[this.index].style.display='block';
    }
	   aBt[i].onmouseout=function(){
	  for(var j=0;j<aBt.length;j++)
  {
	aAccHide[j].style.display='none';
	}
	
	}
	   }
})

</script>
