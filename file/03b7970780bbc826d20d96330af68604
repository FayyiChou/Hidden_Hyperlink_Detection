<script>
function proCity(pro) {
	$.ajax({
		  type: "POST",
		  url: "/product/getLowerRegion",
		  data: "regionid="+pro,
		  success: function(data){
			 province_onchange(data, "city", "area");
		  }
		});
}
function cityArea(cit) {
	//alert('aa');
	$.post("/product/getLowerRegion", {
		regionid : cit
	}, function(data) {
		city_onchange(data, "area");
	});
}
function comment_data() {
	var price = document.getElementById('local_price').value;
	var province = document.getElementById('province').value;
	var message = document.getElementById('message').value;
	if (isNaN(price)) {
		alert('价格必须为数字');
		return false;
	}
	if (province == "") {
		alert('请选择地区!');
		return false;
	}
	if (message == "" || message.replace(/^\s*|[\x00-\x1f]|\s*$/g, '') == ''
			|| message == "仅供发表个人评价，提问题请发到论坛。不允许发布广告、转让、求购等信息。") {
		alert('请填写内容!');
		return false;
	}

}

</script>
