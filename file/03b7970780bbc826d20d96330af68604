<div class="comment" id="wine_comment">
		<script type="text/javascript">
addEvent(window,'load',function(){
function getByClass(oParent, sClass)
{
	var aEle=document.getElementsByTagName('*');
	var i=0;
	var aResult=[];
	
	for(i=0;i<aEle.length;i++)
	{
		if(aEle[i].className==sClass)
		{
			aResult.push(aEle[i]);
		}
	}
	
	return aResult;
}
  var oPilu=document.getElementById('wine_comment');
  var Repeat=getByClass(oPilu,'user_repeat');
  var neirong=oPilu.getElementsByTagName('strong');
  for(var i=0;i<Repeat.length;i++)
  {
    Repeat[i].index=i;
	Repeat[i].onclick=function(){
	if(neirong[this.index].style.display=='block'){
    neirong[this.index].style.display='none';}
	else{
	neirong[this.index].style.display='block';
	}
	}
	   }
})
</script>


<script type="text/javascript">
var zanUrl = "/product/commentZan";
var replyUrl = "/product/commentReply";
	$(function() {
		
		//对于评论点赞的JS
		$(".cozan").click(function() {
			var commentclass = $(this).attr('class');
			if(commentclass == 'cozanAc') {
				return false;
			}
			var commentid = $(this).attr('id');
			var zan = $.ajax({
				url:zanUrl,
				data:{'commentid':commentid},
				dataType:'json',
				type:'post',
			});
			zan.done(function(data){
				$("#"+commentid).html("("+data['count']+")");
				if(data['zan'] == 'yz') {
					alert('您已支持！');
				}
				$("#"+commentid).attr('class','cozanAc');
				$("#"+commentid).attr('title','已支持');
			});
		});
		//回复判断
		var length = 0;
		$("textarea[class=replay_txt]").val("");
		$("textarea[class=replay_txt]").keyup(function(){
			var content = $(this).val();
			var user_repeatid = $(this).next('input').attr('id');
			length = String(content).replace(/[^\x00-\xff]/g,'aa').length;
			length = Math.ceil(length/2);
			if(length > 250) {
				$("#user_repeat"+user_repeatid).html('字数超出');
			}else{
				$("#user_repeat"+user_repeatid).html('回复');
			}
				
		});
		//提交回复
		$('.replay_button').click(function(){
			if(length > 250  || length <= 0) {
				alert("输入不符合要求，不能为空，不能超过250字");
				return false;
			}
			var commentid = $(this).attr('id');
			var content = $("#reply"+commentid).val();
			var productid = $('input[name=h_pid]').val();
			var reply = $.ajax({
				url:replyUrl,
				data:{'content':content,'pid':commentid,'productid':productid},
				dataType:'html',
				type:'post'
			})
			reply.done(function(data){
				 document.write(data)
				 document.close();
			});
		});
	});
	function getLength(str){
		return String(str).replace(/[^\x00-\xff]/g,'aa').length;
	}
</script>
