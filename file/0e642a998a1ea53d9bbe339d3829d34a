   	<script src="/js/bootstrap.min.js"></script>
    <script src="/js/jquery.validate.min.js"></script>
    <script type="text/javascript" src="/js/jquery.colorbox-min.js"></script>
    <script type="text/javascript" src="/js/jquery.form.min.js"></script>
    <script type="text/javascript" src="/js/contact.js"></script>
    <script src="/js/jquery.cycle2.js"></script>
    
	<script type="text/javascript" src="/js/jquery.cycle2.js"></script>


<script type="text/javascript">
$(document).ready(function(){
	$('form').validate();
    $("select[name=number_of_players]").change(function(){
        var num_players = $(this).val();
        var first_row = $(this).parents('div.row:first').index() + 1;
        var last_row = parseInt(first_row) + parseInt(num_players);
        var total_rows = first_row + 4;
        for(i=first_row;i<=total_rows;i++){
			if(i <= last_row){
				$("div.row:nth-child(" + i + ")").show();
				$("div.row:nth-child(" + i + ") input").addClass('required');
			}else{
				$("div.row:nth-child(" + i + ")").hide();
				$("div.row:nth-child(" + i + ") input").removeClass('required');
			}
		}
    });

    $("select[name=number_of_tickets_banquet]").change(function(){
        var num_players = $(this).val();
        var first_row = $(this).parents('div.row:first').index() + 1;
        var last_row = parseInt(first_row) + parseInt(num_players);
        var total_rows = first_row + 10;
        for(i=first_row;i<=total_rows;i++){
			if(i <= last_row){
				$("div.row:nth-child(" + i + ")").show();
				$("div.row:nth-child(" + i + ") input").addClass('required');
			}else{
				$("div.row:nth-child(" + i + ")").hide();
				$("div.row:nth-child(" + i + ") input").removeClass('required');
			}
		}
    });

    $("select[name=number_of_tee_signs]").change(function(){
        var num_tees = $(this).val();
        var first_row = $(this).parents('div.row:first').index() + 1;
        var last_row = parseInt(first_row) + parseInt(num_tees);
        var total_rows = first_row + 5;
        for(i=first_row;i<=total_rows;i++){
			if(i <= last_row){
				$("div.row:nth-child(" + i + ")").show();
				$("div.row:nth-child(" + i + ") input").addClass('required');
			}else{
				$("div.row:nth-child(" + i + ")").hide();
			}
		}
    });
	
	$("select[name=donation]").change(function(){
		if($(this).val() == 0){$("input[name=donation_amount]").show().addClass('required');}else{$("input[name=donation_amount]").hide().removeClass('required');}
	});

    $("select[name=number_of_tee_signs]").trigger('change');
    $("select[name=number_of_players]").trigger('change');
	$("select[name=number_of_tickets_banquet]").trigger('change');
	$("select[name=donation]").trigger('change');


    $("input[name$='[pay_option]']").change(function(){
		if($("input[name$='[pay_option]']:checked").val() == 'pay_now'){
			$("#ff_payment_fields").show();
			$("#ff_payment_fields input").addClass('required');
			$("#ff_payment_fields select").addClass('required');
		}else{
			$("#ff_payment_fields").hide();
			$("#ff_payment_fields input").removeClass('required');
			$("#ff_payment_fields select").removeClass('required');
		}
		
	});
	
	$("input[name$='[pay_option]']").trigger('change');
	
	$('select').addClass('form-control');
	$('input[type=submit]').addClass('btn btn-success');
	
	$("select#album_category").change(function(){
		location.href = "/gallery/category/" + $(this).val();
	});
});
</script>
