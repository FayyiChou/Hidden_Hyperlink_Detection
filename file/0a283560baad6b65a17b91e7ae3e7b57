<script>
    function showPjGZ() {
        $(".gl-item").hover(
//            alert(this);
            function () {
                var skuId = getSkuId(this);
                $(this).addClass("hover");
            },
            function () {
                $(this).removeClass("hover");
            }
        );
    }
    function getSkuId(obj) {
        var link = $($(obj).find("a").get(0)).attr("href");
        return link.substr(link.lastIndexOf("/")+1, link.indexOf(".")-2);
    }
    showPjGZ();
</script>
<script>
    jQuery(function(){
        jQuery.ajax({
            'url' : "//mall.jd.com/view/getPopShopHeaderHtml.html"+"?venderId=179659",
            'type':'get',
            'dataType' : 'jsonp',
            'jsonp':'callback',
            'jsonpCallback' : 'callback',
            'success' : function(data){

                if(data && data.result == true){
                    addCssByStyle(data.cssContent);
                    jQuery('#j_jshop_header').replaceWith(data.headHtmlContent);
                }else{
                    jQuery('#j_jshop_header').replaceWith('');
                }
            },
            'error' : function(){
                jQuery('#j_jshop_header').replaceWith('');
            }
        });

        function addCssByStyle(cssString){
            var doc=document;
            var style=doc.createElement("style");
            style.setAttribute("type", "text/css");

            if(style.styleSheet){// IE
                style.styleSheet.cssText = cssString;
            } else {// w3c
                var cssText = doc.createTextNode(cssString);
                style.appendChild(cssText);
            }

            var heads = doc.getElementsByTagName("head");
            if(heads.length){
                heads[0].appendChild(style);
            } else{
                doc.documentElement.appendChild(style);
            }
        }
    });
</script>
<script>
    /**
     * 生成分页信息
     * @param records
     */
    var totalPage = 0; //总页数
    var currPage = 1;
    function getSkus(page) {
        if(totalPage==0) {
            return ;        }
        $("#itag").show();
        currPage=page;
        var url = "//hui.jd.com/json/activity/getSkus.action?promoId=1061777255&venderId=179659";
        jQuery.ajax({
            url :url,
            data:"page=" + currPage,
            type:'post',
            cache:false,
            async:false,
            success:function(list) {
                var data = [];
                var skuIdArr=[];
                jQuery.each(list, function(index,product){
                    data.push(createSku(product));
                    skuIdArr.push(product.id);
                });
                jQuery(".gl-wrap li").remove();
                jQuery(".gl-wrap").append(data.join(""));
                getSkuPrice(skuIdArr)
                getSkuPjNum(skuIdArr);
                getSkuGzNum(skuIdArr);
                showPjGZ();
                $("#itag").hide();
                window.location.hash = "#activity";
//                window.showtouchurl = true;
            },
            error:function(request,errMsg,errorObj){
//                alert("网络异常，请稍后再试！");
                $("#itag").hide();
            }
        });
        createPage();
    }


    function getSkuPrice(skuIdArr) {

        queryPrice(skuIdArr.slice(0, 10));
        if(skuIdArr.length>10) {
            queryPrice(skuIdArr.slice(10));
        }

    }
    function queryPrice(skuIdArr) {
        jQuery.ajax({url:"//p.3.cn/prices/mgets?type=1&source=hui"+"&skuIds=J_"+skuIdArr.join(",J_"),
            dataType:"jsonp",success:function(list){
                jQuery.each(list, function(index,obj){
                    var skuId = obj.id.substring(2);
                    if(obj.p>0) {
                        jQuery("#price" +skuId ).html("<em>￥</em><i>" + obj.p + "</i>");
                        $("#cart"+skuId).attr("href","//cart.jd.com/gate.action?pid="+skuId+"&pcount=1&ptype=1&r="+(+new Date( )));
                        $("#cart"+skuId).parent().removeClass("p-btn-n");
                    } else{
                    }
                });
            }});

    }
    function createSku(product) {
        var sku=[];
        sku.push("<li class=\"gl-item\"><div class=\"gl-i-wrap\"><div class=\"p-img\">");
        sku.push(createLink(product.id));
//        sku.push("<img src=\"/common/skin/i/p-img.jpg\">")
        sku.push("<img src=\"" + product.imgUrl + "\">");
        sku.push("</a></div><div class=\"p-name\">");
        sku.push(createLink(product.id));
        sku.push(product.name);
        sku.push("</a></div>");
        sku.push("<div class=\"p-price\"><strong id='price"+product.id+"'>");
//            sku.push("<em>￥</em><i>168.00</i></strong><span class="ori-price">346.00</span></div><div class=\"p-btn\">");
//        sku.push("<i>暂无报价</i></strong><span class=\"ori-price\">"+product.price.toFixed(2)+"</span></div><div class=\"p-btn\">");
        sku.push("<i class=\"nopri\">暂无报价</i></strong><span class=\"ori-price\">"+product.jdPrice+"</span></div><div class=\"p-btn p-btn-n\">");
//        sku.push("<a href=\"https://cart.jd.com/gate.action?pid="+product.id+"&pcount=1&ptype=1&r="+(+new Date( ))+"\" class=\"btn-cart\" id=\"cart"+product.id+"\"><i class=\"icon-cart\"></i>加入购物车</a>");
        sku.push("<a href=\"javascript:;\" class=\"btn-cart\" id=\"cart"+product.id+"\"><i class=\"icon-cart\"></i>加入购物车</a>");
        sku.push("</div><div class=\"p-stock\"><span class=\"fl\" id=\"f1"+product.id+"\">评价 0</span><span class=\"fr\" id=\"fr"+product.id+"\">关注 123</span></div>");
        sku.push("</div></li>");
        return sku.join("");
    }
    function createLink(skuId){
        return "<a href=\"//item.jd.com/" + skuId + ".html\" target=\"_blank\">";
    }
    function getSkuPjNum(skuIdArr){
        jQuery.getJSON("//club.jd.com//clubservice/summary-m-"+skuIdArr.join(",")+".html?callback=?", {}, function (data) {
            try {
                var array = data.CommentsCount;
                for(var t in array){
                    var obj = array[t];
                    jQuery("#f1" + obj.SkuId).text("评价 "+obj.CommentCount);
                }
            } catch (e) {
            }
        });
    }
    function getSkuGzNum(skuIdArr) {
        jQuery.ajax({url:"//follow-soa.jd.com/rpc/product/queryForCountByPids?sysName=hui.jd.com&productIds="+skuIdArr.join(","),
            dataType:"jsonp",success:function(t){
                if(t.success){
                    var gzObj= t.data;
                    for(var i in gzObj){
                        jQuery("#fr" + i).text("关注 " + gzObj[i]);
                    }
                }
        }});
    }

    function getSkusCount() {
        var url = "//hui.jd.com/json/activity/getSkusCount.action?promoId=1061777255&venderId=179659";
        jQuery.ajax({
            url :url,
            type:'post',
            cache:false,
            async:false,
            success:function(data) {
                if(typeof(data)=="undefined" || data==0){
                    totalPage = 0;
                } else{
                    data = parseInt(data, 10);
                    totalPage = data % 20 == 0 ? parseInt(data / 20) : parseInt(data / 20 + 1);
                }
            },
            error:function(request,errMsg,errorObj){
//                alert("网络异常，请稍后再试！");
            }
        });

    }

    function createPage() {
        if(totalPage==0){
            return;
        }
        var prePage = currPage-1;   // 上页
        var nextPage = currPage+1;  // 下页
        var pagePos = 2; 
        var pageInfo = new Array("<span class=\"p-num\" id=\"pageSpan\">");
        if(currPage > 1){             pageInfo.push("<a class=\"pn-prev\" href=\"#activity\" onclick=\"getSkus(" + prePage + ");\"><i>&lt;</i><em>上一页</em></a>");
        } else{
            pageInfo.push("<a class=\"pn-prev disabled\" href=\"\"><i>&lt;</i><em>上一页</em></a>");
        }
        if(currPage > (pagePos + 2)) pageInfo.push("<b class=\"pn-break\"> …</b>");        var numTemp = pagePos;
        var pageIndex = 0
        while(numTemp > 0){            pageIndex = currPage - numTemp;
            if(pageIndex > 1){
                pageInfo.push("<a href=\"#activity\" class=\"\" onclick=\"getSkus(" + pageIndex + ");\" > " + pageIndex + "</a>");
                numTemp--;
            } else{
                numTemp = 0;
            }
        }
        pageInfo.push("<a class=\"curr\">" + currPage + "</a>");        numTemp = 1;
        while(numTemp <= pagePos){            pageIndex = parseInt(currPage, 10) + numTemp;
            if(totalPage > pageIndex){
                pageInfo.push("<a href=\"#activity\" class=\"\" onclick=\"getSkus(" + pageIndex + ");\" > " + pageIndex + "</a>");
                numTemp++;
            } else{
                numTemp = pagePos + 1;
            }
        }
        if((totalPage - currPage) > (pagePos + 2)){             pageInfo.push("<span class=\"text\">...</span>");
        }
        if(parseInt(currPage, 10) < parseInt(totalPage, 10)){            pageInfo.push("<a class=\"pn-next\" href=\"#activity\" onclick=\"getSkus(" + nextPage + ");\" >下一页");
        } else {
            pageInfo.push("<a class=\"pn-next disabled\">下一页");
        }
        pageInfo.push("<i>&gt;</i></a></span>");
//        pageInfo.push("<span class=\"p-skip\"><em>共<b>" + totalPage + "</b>页</em>");
        pageInfo.push("<span class=\"p-skip\"><em>共<b>" + totalPage + "</b>页&nbsp;&nbsp;到第</em>");
//        pageInfo.push("<input id=\"totalPage\" type=\"hidden\" value=\"" + totalPage +"\"/>")
        pageInfo.push("<input class='input-txt' type=\"text\"id=\"ppage\" " +
                "onkeyup=\"this.value=this.value.replace(/\D/g,'')\" onafterpaste=\"this.value=this.value.replace(/\D/g,'')\"/>" +
                "<em>页</em><a class=\"btn btn-default\" href=\"#activity\" onclick=\"goPage();\">确定</a></span>");

        jQuery(".p-wrap").html("");
        jQuery(".p-wrap").html(pageInfo.join(""));
//        jQuery("#ppage").mask("123456");
    }
    function goPage(){
        var page = jQuery("#ppage").val();
        page = parseInt(page, 10);
        if(isNaN(page)){
            page = 1;
        }

        if(page>totalPage){
            page = totalPage;
        }
        if(page ==0) {
            page = 1;
        }
        jQuery("#ppage").val(page);
        if(page == currPage) {
            return ;
        }
        getSkus(parseInt(page,10));
    }
    $(function(){
        getSkusCount();
        getSkus(1);
    });


</script>
