<script>
function init_screen(){
		var _top=0;
		
		$(".quanping").find("div").show().each(function(){
			var _width=$(this).text().length*57;
			var _left=$(window).width();
			var time=parseInt(18+_width*18/_left)*1000;
		/*	if(_width<_left){
				var time=18000;
				//$(this).css({width:_width});
			}else{
				var time=26000;
				//$(this).css({width:_left});
			}*/
			
			$(this).css({width:_width});
			var _height=$(window).height();
			
			_top=parseInt(Math.random()*$(window).height());
			if(_top>=_height-100){
				_top=0;
			}
			
			if($(this).offset().left=="-"+_width){
				$(this).remove();
			}
			if($(this).offset().left==0){
				$(this).css({left:_left,top:_top});
				$(this).animate({left:"-"+_width+"px"},time,'linear',function(){
				});
			}
		});
	}
</script>

