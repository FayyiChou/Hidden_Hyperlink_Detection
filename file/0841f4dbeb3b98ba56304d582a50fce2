<!--mp3插件-->
<script type="text/javascript" src="mp3/swfobject.js"></script>
<script type="text/javascript">
    var flashvars = {
    };
	var params = {
		wmode: "transparent"
	};
    var attributes = {};
	swfobject.embedSWF("/mp3/sound.swf", "sound", "1", "1", "9.0.0", "/mp3/expressInstall.swf", flashvars, params, attributes);

	function play(music) {
		var sound = swfobject.getObjectById("sound");
		if (sound) {
		    sound.SetVariable("f", music);
		    sound.GotoFrame(1);
		}
	}
</script>
<!--mp3插件/end-->
