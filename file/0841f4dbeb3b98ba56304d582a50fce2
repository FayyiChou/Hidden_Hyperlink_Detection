      <script>
             $(".caijing ul li").click(function(){

		var opid = $(this).attr("c");//选项ID
		var inum = $(this).find("i.b label");
		var ispan = $(this).find("span");
		var ititle = $("#Y_Position .Y_Position_Title label");
		//ajax
		/*if(ADMINID=='14' || ADMINID=='13'){
			notice("你不具有投票的权限，请联系客服或注册");

			return;
		}*/


		$.get("action.php",{type:"SetPosition",op:opid,fangjian:},function(msg){
			if(msg==false)
			{	notice("你已经投过票了，不能再投了");
			}
			else
			{
				var total=parseInt(msg.op1)+parseInt(msg.op2)+parseInt(msg.op3);

				$('.kanzhang a').html('看涨'+Math.round(msg.op1/total*100)+'%');
				$('.kanzheng a').html('盘整'+Math.round(msg.op2/total*100)+'%');
				$('.kandie a').html('看空'+Math.round(msg.op3/total*100)+'%');
				notice("投票成功");
			}
		},"json");
	});
	setInterval(getToupiao,20000);
	function getToupiao(){
		$.get("action.php",{type:"GetPosition",fangjian:},function(msg){
			//alert(msg);

			if(typeof msg=="object")
			{
				var total=parseInt(msg.op1)+parseInt(msg.op2)+parseInt(msg.op3);

				$('.kanzhang a').html('看涨'+Math.round(msg.op1/total*100)+'%');
				$('.kanzheng a').html('盘整'+Math.round(msg.op2/total*100)+'%');
				$('.kandie a').html('看空'+Math.round(msg.op3/total*100)+'%');

			}
		},"json");
	}
    </script>
