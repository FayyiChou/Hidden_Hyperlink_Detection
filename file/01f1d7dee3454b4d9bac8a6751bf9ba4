    $(function () {
      // utm_source detecting
      var t = document.location.search.substr(1).split('&');
      var _hot_utm = '';
      for (var i = 0; i < t.length; ++i) {
        var l = t[i].split('=');

        if (l[0] == "utm_source") {
          $.cookie(l[0], l[1], { path: '/' });
          _hot_utm = l[1];
        }
      }

      //Выбираем utm_source из гугл аналитики
      //для случая, когда не можем определить из строки запроса
      if(!_hot_utm.length) {
        var _utmz = $.cookie('__utmz');
        if (typeof _utmz != 'undefined' && _utmz != undefined)
          if(_utmz.length) {
            _utmz = _utmz.split("|");
            for(var j = 0; j < _utmz.length; j++) {

              var _utmz_el = _utmz[j].split('=');
              if (_utmz_el[0] == "utmcsr" && _utmz_el[1] != "(direct)") {
                $.cookie("utm_source", _utmz_el[1], { path: '/' });
              }
            }
          }
      }
    });
  </script>

