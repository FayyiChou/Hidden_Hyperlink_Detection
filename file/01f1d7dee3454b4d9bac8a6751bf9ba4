<script type="text/javascript">
    $(".city").click(function () {
        $(".city li").click(function () {
            if ($(this).attr("data-value") == 676) {
                $(".officeSpb").show();
                $(".officeSpb select").attr("name", "form_dropdown_SIMPLE_QUESTION_727");
                $(".officeMsk").hide();
                $(".officeMsk select").attr("name", "");
            } else if ($(this).attr("data-value") == 675) {
                $(".officeSpb").hide();
                $(".officeSpb select").attr("name", "");
                $(".officeMsk").show();
                $(".officeMsk select").attr("name", "form_dropdown_SIMPLE_QUESTION_727");
            } else {
                $(".officeSpb").hide();
                $(".officeSpb select").attr("name", "");
                $(".officeMsk").hide();
                $(".officeMsk select").attr("name", "");
            }
        })
    })

    $(document).ready(function () {
        $(".officeMsk select").attr("name", "form_dropdown_SIMPLE_QUESTION_727");
    });

</script></div>
