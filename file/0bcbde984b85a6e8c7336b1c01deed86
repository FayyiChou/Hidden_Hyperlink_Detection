	 function openCountryCodeDialog(countryCodeObjArr) {
	        var htmlStr = '<div class="mask"></div>';
	        htmlStr += '<div class="dialog">';
	        htmlStr += '<div class="dcent">';
	        $.each(countryCodeObjArr, function(index, value) {
	            var countryCodeObj = value;
	            htmlStr += '<div class="code-node line mkcl">';
	            htmlStr += '<b data-value="false" class="ccr radio roff r"></b>';
	            htmlStr += '<p class="item">';
	            var countryCodeDisplay = "+" + countryCodeObj.countryCode;
	            htmlStr += '<span class="split cc">' + countryCodeDisplay + '</span><span class="split">' + $("#login_userName").val() + '</span><input type="hidden" value="' + countryCodeObj.siteID + '">';
	            htmlStr += '</p>';
	            htmlStr += '</div>';

	        });
	        htmlStr += '</div>';
	        htmlStr += '</div>';
	        $("#selectCountryCodeDiv").html(htmlStr);

	        $(".radio").on("click", function(e) {
	            $(this).removeClass('roff');
	            $(this).addClass('ron');
	            var self = $(this);
	            setTimeout(function() {
	                var countrycode = self.parent().find(".cc").text().trim().replace("+", "00");
	                var newLoginName = countrycode + $("#login_userName").val();
	                $("#selectCountryCodeDiv").html("");
	                $("#login_userName").val(newLoginName);
	                localInfo.siteID = self.parent().find("input").val();
	            }, 200);

	        });

	    }
