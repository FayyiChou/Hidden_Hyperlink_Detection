	login_password[0].focus();
	userName[0].focus();
	/**set userName input focus End**/
};

function chgRandomCode(ImgObj, randomCodeImgSrc) {
	ImgObj.src = randomCodeImgSrc+"&_t=" + new Date().getTime();
};

function gotoresetpwd()
{
	var useraccount= convPlusOfPhoneAccount($("#login_userName").val());
	gotoUrl(localInfo.findPwd+localInfo.urlQurey+"&userAccount="+useraccount);
}

$(function () {
    $("#center").on({
        mouseover: function () {
        	$("#soft_center_nav").addClass("cur_img_up");
        	$("#soft_center_nav").removeClass("cur_img_down");
            $("#soft-center").show();
        },
        mouseout: function () {
        	$("#soft_center_nav").addClass("cur_img_down");
        	$("#soft_center_nav").removeClass("cur_img_up");
            $("#soft-center").hide();
        }
    });
    $("#languagetipId").on({
        mouseover: function () {
        	$("#language-imgID").addClass("language-img_up");
        	$("#language-imgID").removeClass("language-img_down");
            $("#language_list").show();
        },
        mouseout: function () {
        	$("#language-imgID").addClass("language-img_down");
        	$("#language-imgID").removeClass("language-img_up");
            $("#language_list").hide();
        }
    });
    if(localInfo.lang.toLowerCase() == "zh-cn") {
	    $("#loginHelpId").attr("href","https://cloud.huawei.com/help");
    } else {
    	$(".loginHelp").css("display","none");
    }
    
    
    if($(".third_login_tip span").length == 0)
    {
    	$(".third_login_tip .third_other_login").remove();
    }
    
    
});



function thirdAccountLogin(obj)
{
  var hrefObj=$("."+obj.className+" a");
  var a=localInfo.thirdLoginUrlAddrass;
  var b=hrefObj.attr("toUrl");
/*   var toUrl=a+b
       + "?loginUrl=" + encodeURIComponent(localInfo.loginUrl) 
       + "&service=" + encodeURIComponent(localInfo.service) 
       + "&loginChannel=" + localInfo.loginChannel 
       + "&reqClientType=" + localInfo.reqClientType
	   + "&quickAuth=" + localInfo.quickAuth
  	   + "&lang=" + localInfo.lang; */
   var toUrl= a + b + localInfo.queryString;
	hrefObj.attr("href",toUrl);
}



function getAccountInfo(userAcct,isGetAll,callback) {
	var dataParms = {
		"userAccount": convPlusOfPhoneAccount(userAcct),
		"reqClientType": localInfo.reqClientType,
		"isGetAll":isGetAll
		//"pageToken":localInfo.pageToken
	};
	ajaxHandler("getUserAccInfo", dataParms, function(data) {
		var isSuccess = data.isSuccess;
		if (isSuccess == '1') {
			callback(data);
		}
	}, function() {}, false, "json");
}

function isActiveAccount(userAccount)
{
	var isExist = false;
	getAccountInfo(userAccount,0,function(data){
		if (data && data.isSuccess==1) {
			var datas = data.userAcctInfoList;
			for (var i=0;i<datas.length;i++) {
				var account = datas[i];
				if ((account.accountType == 1 || account.accountType == 2) && account.accountState == 1) {
					isExist = true;
					break;
				}
			}
		}
   });
	return isExist;
}

/**
* 仅是调用getActivateEMailURL接口
*/
function getActivateEMailURL(userAccount,reqClientType, $btn) {
	var param = {
			accountType:1,
			userAccount:convPlusOfPhoneAccount(userAccount),
			reqClientType:reqClientType,
			email:convPlusOfPhoneAccount(userAccount),
			//pageToken:localInfo.pageToken,
			languageCode:localInfo.lang
	};
	ajaxHandler("getActivateEMailURL", param, function(data) {
			if (data && data.isSuccess == '1') {
				$btn.attr("disabled");
				$btn.addClass("auth_code_grey");
				jsInnerTimeout($btn, rss.getAuthCode);
			}else {
				$btn.removeAttr("disabled");
				if (data.errorCode == "10000001" || data.errorCode == "70001201")
				{
					showError($(".errortip"), rss.error_10000001);
				}
				if (data.errorCode == "10000002")
				{
					showError($(".errortip"), rss.error_10000002);
				}
				if (data.errorCode == "10000004")
				{
					showError($(".errortip"), rss.error_10000004);
				}
				if (data.errorCode == "70002001")
				{
					showError($(".errortip"), rss.error_70002001);
				}
				if (data.errorCode == "70001401")
				{
					showError($(".errortip"), rss.error_70001401);
				}
				if (data.errorCode == "70002008")
				{
					showError($(".errortip"), rss.error_70002008);
				}
				if (data.errorCode == "70001102")
				{
					showError($(".errortip"), rss.error_70001102_2);
				}
				if (data.errorCode == "70001104")
				{
					showError($(".errortip"), rss.error_70001104_3);
				}
				if (data.errorCode == "70002019")
				{
					showError($(".errortip"), rss.error_70002019);
				}
				if (data.errorCode == "70002009")
				{
					showError($(".errortip"), rss.error_70002009);
				}
				else
				{
					showError($(".errortip"), rss.error_70002018);
				}
			}
		}, function(){}, true, "json");
}



function queryMutiCountCode(userAccount, countryCodeObjArr) {
	// 账号为空，不需要访问后台
	if (!userAccount)
	{
		return;
	}
	
    var dataParms = {
        "userAccount" : convPlusOfPhoneAccount(userAccount),
        "reqClientType" : localInfo.reqClientType
        //"pageToken" : localInfo.pageToken
    };
    ajaxHandler("isExsitUser", dataParms, function(data) {
        var isSuccess = data.isSuccess;
        var errorCode = data.errorCode;
        if (isSuccess == 1) {
            if (data.existAccountFlag == 1) {
                var countryCodeObj = {};
                countryCodeObj.siteID = data.siteID;
                countryCodeObjArr.push(countryCodeObj);
            } else if (data.existAccountFlag == 2) {
                var siteInfoList = data.siteInfoList;
                if (siteInfoList) {
                    for (var i = 0; i < siteInfoList.length; i++) {
                        var siteInfo = siteInfoList[i];
                        var countryCodeObj = {};
                        countryCodeObj.siteID = siteInfo.siteID;
                        countryCodeObj.countryCode = siteInfo.cy;
                        countryCodeObjArr.push(countryCodeObj);
                    }
                }
            }

        }

    }, function() {
    }, false, "json");

}



