function chgRandomCodeForLogin()
{
	var randomCode= $("#randomCode");
	var randomCodeImg=$('#randomCodeImg');
	chgRandomCode(randomCodeImg[0],localInfo.webssoLoginSessionCode);
	hideErrorMsg();
	randomCode.val("");
}

