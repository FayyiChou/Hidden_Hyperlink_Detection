		max : 50,
		validOnChange : true,
		errorFunction : function(obj, options) {
			switch (options.type) {
				case "require":
					//showMsg(rss.login_js_inputaccount);
					showError($("#msg_username"), rss.login_js_inputaccount);
					break;
				case "emailormobile":
					//showMsg(rss.login_js_accountlimit);
					showError($("#msg_username"), rss.login_js_accountlimit);
					break;
				case "length":
					//showMsg(rss.login_js_accountlength);
					showError($("#msg_username"), rss.login_js_accountlength);
					break;
			};
			//showError(error_tip);
		},
		successFunction : s
	});

	// show password
	ec.form.validator.bind(login_password, {
		type : [ "require"],
		trim : false,
		validOnChange : true,
		min : 8,
