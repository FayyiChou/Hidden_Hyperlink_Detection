				if("1"==data.isSuccess)
				{
					hideErrorMsg(1);
					$(".error_msg_sanjiao").hide();
					$("#btnLogin").focus();
				}
				else
				{
					chgRandomCode(randomCodeImg[0],localInfo.webssoLoginSessionCode);
					//showMsg(rss.login_js_codeerror);
					showError($("#msg_randomCode"), rss.login_js_codeerror);
					hideErrorMsg(1000);
					randomCode.focus();
					randomCode.val("");
				}
				
			},function(data){
				
			},false,"JSON");
			
			
		}
		else if(typeof(randomCode[0]) !=  "undefined" && randomCode.val().length!=4)
		{
			//showMsg(rss.login_js_codeerror);
		}
		
	});
	
	ec.form.validator.register("emailormobile", function(b, a) {
		if (a.allowEmpty && ec.util.isEmpty(b)) {
			return true;
		}
		if (b.length < 11) {
			return false;
		}
		return /^(1[34578])[0-9]{9}$|^\s*([A-Za-z0-9_-]+(\.\w+)*@(\w+\.)+\w+)\s*$/.test(b);
	});

	ec.form.validator.register("chinaLang", function(b, a) {
		if (a.allowEmpty && ec.util.isEmpty(b)) {
			return true;
		}
		if (b.length != 4) {
			return false;
		}
		else
		{
			return true;
		}
	});
	

