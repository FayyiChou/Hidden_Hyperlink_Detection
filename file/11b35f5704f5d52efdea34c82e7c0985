<style>
.pageNav {border-left:1px solid #d8e1e6; border-right:1px solid #d8e1e6; clear:both; font-family:Tahoma, Helvetica, Arial, simsun, sans-serif; line-height:1.231; padding:5px 0; height:40px;}
.pageNav .pageCon {color:#333333; display:inline; float:left; height:36px; left:50%; position:relative;}
.pageNav .pageCon a,
.pageNav .pageCon span {background:#fff; border:1px solid #ccc; border-radius:5px; display:block; font-size:12px; height:16px; height:22px; float:left; line-height:1.231; margin:0 2px; overflow:hidden; padding:8px 10px 0; position:relative; right:50%;}
.pageNav .pageCon a {background:#fff; border:1px solid #ccc; color:#888; text-decoration:none;}
.pageNav .pageCon a:hover {background:#b31515; border:1px solid #b31515; color:#fff; text-decoration:none;}
.pageNav .pageCon .curPage {background:#b31515; border:1px solid #b31515; color:#fff; font-weight:bold;}
.pageNav .pageCon span.sumPage {border:1px solid #ccc; white-space:nowrap;}
