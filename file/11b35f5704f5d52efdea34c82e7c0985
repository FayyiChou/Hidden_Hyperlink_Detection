        <script>
	var mq_count=0;
	function writePage()
	{
		if(typeof(nowID) == "undefined" || typeof(Images) == "undefined")
		{
			return "";
		}
		if(mq_count==1)
		{
			clearInterval(mq_page_timer);
			return "";
		}
		clearInterval(mq_page_timer);
		var currentPage=nowID;
		var totalNum=Images.length;
		var picListFenye="";
		if(totalNum==0){
			return "";
		}
        picListFenye="<div class=\"pageCon\">";
        if(currentPage==1){//已到首页
			picListFenye+="<a href=\"javascript:void(0)\" class=\"firstPage\" title=\"到第一页\" target=\"_self\">首页</a>";
			picListFenye+="<a href=\"javascript:void(0)\" class=\"prevPage\" title=\"到上一页\" target=\"_self\">上页</a>";
		}else{
			picListFenye+="<a href=\""+Images[0].getAttribute("link")+"\" class=\"firstPage\" title=\"到第一页\" target=\"_self\">首页</a>";
			picListFenye+="<a href=\""+Images[currentPage-2].getAttribute("link")+"\" class=\"prevPage\" title=\"到上一页\" target=\"_self\">上页</a>";
		}
        
        var beginAndEndStr = getPageStartEnd(totalNum,currentPage,8);
        var beginPage = beginAndEndStr.split("&")[0];		//分页显示 开始页数
        var endPage = beginAndEndStr.split("&")[1];		//分页显示 结束页数
        var hiddenFlag = false;	//是否显示...
        for(var i=0;i<totalNum;i++){
        	if(i!=0 && (i+1<beginPage || i+1>endPage) && i+1 !=totalNum){
        		hiddenFlag = true;
        		continue;
        	}
        	if(hiddenFlag){
        		picListFenye += "<span>...</span>";
        		hiddenFlag = false;
        	}
			if(currentPage==(i+1)){
				picListFenye += "<span class=\"curPage\">"+eval(i+1)+"</span>" ;
			}else{
				picListFenye += "<a href=\""+Images[i].getAttribute("link")+"\" target=\"_self\">"+eval(i+1)+"</a>";
			}
        }
        if(currentPage==totalNum){//已到尾页
			picListFenye+="<a href=\"javascript:void(0)\" class=\"nextPage\" title=\"到下一页\" target=\"_self\">下页</a>";
			picListFenye+="<a href=\"javascript:void(0)\" class=\"lastPage\" title=\"到最后一页\" target=\"_self\">尾页</a>";
		}else{
			picListFenye+="<a href=\""+Images[currentPage].getAttribute("link")+"\" class=\"nextPage\" title=\"到下一页\" target=\"_self\">下页</a>";
			picListFenye+="<a href=\""+Images[totalNum-1].getAttribute("link")+"\" class=\"lastPage\" title=\"到最后一页\" target=\"_self\">尾页</a>";
		}

		picListFenye+="<span class=\"sumPage\">共<cite>"+Images.length+"</cite>页</span>";
		picListFenye+="</div>";
		document.getElementById("mypage").innerHTML=picListFenye;
		mq_count=1;
	}

	


	function getPageStartEnd(totalPage,currentPage,showPage)
	{
		var beginPage=1;
		var endPage=totalPage;
		var currentSite=-1;

		if(showPage%2==0)
			currentSite=showPage/2;
		else
			currentSite=showPage/2+1;
		if(totalPage<=1)
			return beginPage+"&"+endPage;

		if(totalPage<=showPage){
			beginPage=1;
			endPage=totalPage;
		}else{
			if(currentPage<=currentSite){
				beginPage=1;
				endPage=showPage;
			}else{
				beginPage=currentPage-(currentSite-1);
				if(showPage%2==0){
					endPage=currentPage+currentSite;
				}else{
					endPage=currentPage+currentSite-1;
				}				
			}
			if(endPage>totalPage){
				endPage=totalPage;
			}
			if(endPage-beginPage<showPage-1){
				beginPage=endPage-(showPage-1);
			}
		}
		
		return beginPage+"&"+endPage;
	}


	var mq_page_timer =  window.setInterval("writePage()",2000);
	
	//window.onload=writePage;
	</script>
<!-- #15404 end -->


