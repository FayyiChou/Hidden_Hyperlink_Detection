</div>		<script language="javascript" type="text/javascript">
			function show(){
				document.getElementById("two_code").style.display = "none";
			}
			
			function was_search(){
				var searchwordTxtT = document.getElementById("searchwordTxt");
				if(searchwordTxtT.value == "" || searchwordTxtT.value == "请输入搜索内容"){
					jAlert("请输入搜索内容！","山西省投资项目联合审批办事大厅");
					return;
				}
				var actionUrl = "http://www.hbfgw.gov.cn:8090/was5/web/search?channelid=239980&searchword=***&perpage=&templet=";
				var utfcode = encodeURI(searchwordTxtT.value);
			    var sw = "&searchword= "+utfcode;
				window.location.href = actionUrl+sw;
			}
			
			function tanchu(){
