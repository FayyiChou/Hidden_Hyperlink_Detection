			if(r){
				window.location.href="/logout.jspx?returnUrl=/index2.jspx";
			}
		});
	}
	$(function(){
		Cms.siteFlow("", location.href, document.referrer);
		var myDate=new Date();
	    var strDate = "今天是:"+myDate.getFullYear()+"年"+(myDate.getMonth()+1)+"月"+myDate.getDate()+"日\r";
		strDate += "星期"+('日一二三四五六'.charAt(myDate.getDay()));
		$("#localtime").html(strDate);
		
