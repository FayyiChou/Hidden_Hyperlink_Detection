function writeLocalAccount(){
	var sFntColor = "#25416b";
	//alert(sLocalAccount);
	var aAccount=sLocalAccount.split("\n");
	$("#write").html("");
	if(aAccount.length>4){
		$("#write").append("<span><a href=javascript:clearAccount() style=\"font-size:9pt\">清除</a>：</span>");
	}
	for(var i=0;i<aAccount.length&&(aAccount.length%2)==0;i+=2){
		$("#write").append("<span><a href=\"javascript:localAccountLogin("+i+")\">"+aAccount[i].replace(/\r/g,"")+"</a></span>");
	}
}

function localAccountLogin(nIndex){
	var aAccount=sLocalAccount.split("\n");
	document.getElementById("username").value=aAccount[nIndex];
	document.getElementById("password").value=aAccount[nIndex+1];
	doLogin();
}
function clearAccount() {
	w_hidden.location.href = "/lysm/common/clear_account.jsp";
}
$(function() {
	writeLocalAccount();
