<script type="text/javascript">
	$(function(){
		var schoolnews = $("#schoolnews");
		schoolnews.find(".title span").click(function(){
			if(!$(this).hasClass()){
				$(this).addClass("now");
				$(this).siblings("span").removeClass("now");
				a_now = $(this).siblings("a.now");
				a_now.removeClass("now");
				a_now.siblings("a").addClass("now");
				schoolnews.find(".schoolnews_content").toggle();
			}
		});
	});

      function searchArticle(){
	if($("#searchText").val()!=""){
		var searchText=$("#searchText").val();
		window.location.href="search/search_result_2.jsp?page_id=2&current_id=2&search_text="+searchText;
	}else{
		alert("请输入关键字！");
	}	
}
</script>


