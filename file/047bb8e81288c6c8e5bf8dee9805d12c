   function showbucdiv(num)
        {
            if(num=="3")
            {
              ymPrompt.win("iframeAddSlave.aspx?type=0&id="+ 6387940,450,150,'补充附件',null,null,null,{id:'a'});
               document.getElementById("buchong1").style.display="none";
              document.getElementById("buchong2").style.display="none";
              document.getElementById("buchong3").style.display="none";
               document.getElementById("buchong4").style.display="none";
            } else
            if(num=="2")
            {
              document.getElementById("buchong1").style.display="none";
              document.getElementById("buchong2").style.display="block";
               document.getElementById("buchong3").style.display="none";
                document.getElementById("buchong4").style.display="none";
            } else
             if(num=="4")
            {
              document.getElementById("buchong1").style.display="none";
              document.getElementById("buchong2").style.display="none";
               document.getElementById("buchong3").style.display="block";
                document.getElementById("buchong4").style.display="none";
            }else
             if(num=="5")
            {
              document.getElementById("buchong1").style.display="none";
              document.getElementById("buchong2").style.display="none";
              document.getElementById("buchong3").style.display="none";
              document.getElementById("buchong4").style.display="block";
            }
            
        }
            </script>

            
