<script language="javascript">
var b_Language;
b_Language = navigator.language;  
if(!b_Language)
{
b_Language = navigator.browserLanguage;
}

if (b_Language == "zh-CN" || b_Language == "zh-cn")
{
window.location.href="/cn";
}
else if (b_Language == "en-us")
{
window.location.href="/en";
}
else if (b_Language == "ja")
{
window.location.href="/jp";
}
else
{
window.location.href="/en";
}
