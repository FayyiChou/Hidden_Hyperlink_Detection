<!-- inquiry -->
<script language="JavaScript">
function inquirycheck() {
	with(document.inquiryform) {
		if (inquiryfullname.value == "" ){
			alert("Please fill in your Name." );inquiryfullname.focus();return false;
		}
		if (inquiryemail.value == "" || (inquiryemail.value.indexOf == -1 ||inquiryemail.value.indexOf == -1 )) {
			alert("Please fill in your Email Address.");inquiryemail.focus();return false;                                          
		} else if (inquiryemail.value == "" || (inquiryemail.value.indexOf('@',0) == -1 ||inquiryemail.value.indexOf('.',0) == -1 )) {
			alert("Please enter a valid Email Address.");inquiryemail.focus();return false;
		}
		if (inquirymessage.value == "" ){
			alert("Please fill in the Details of your inquiry.");inquirymessage.focus();return false;
		}
		if (validcode.value == "" ){
			alert("Please fill in the Verification Code.");validcode.focus();return false;
		}
		return true;
	}
}
</script>
<script type="text/javascript">
　function InquiryRefreshValidate(){
    document.inquiryform.inquiryImg.src="inc/validcode.asp?SafeCodes=" + Math.random();
　}
</script>

