			getObject('bg_'+m+'_'+i).style.background=strPic2;
			getObject('td_'+m+'_'+i).style.color='#ffffff';
			getObject('tab_'+m+'_'+i).style.display='none';
			}
	}
}
    if (m==2 | m==3 | m==4 | m==5 | m==6) {
	strPic1='url(images/maintit_bg_h.gif)';
	strPic2='url(images/maintit_bg.gif)';
