function showtab(m,n,count){
	var strPic1;
	var strPic2;
	var tdcolor;
    if (m==1) {
	strPic1='url(images/mainmenu_bg_hover.gif)';
	strPic2='url(images/mainmenu_bg.gif)';
		for(var i=1;i<=count;i++){
		if (i==n){			
			getObject('bg_'+m+'_'+i).style.background=strPic1;
			getObject('td_'+m+'_'+i).style.color='#1b79b7';
			getObject('tab_'+m+'_'+i).style.display='';
			}
