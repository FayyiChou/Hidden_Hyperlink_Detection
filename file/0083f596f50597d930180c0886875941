(function(){
var postAct = document.getElementById("rfixid");
var postActb = document.getElementById("rbside");
var postActTop = postActb.offsetTop;
document.onscroll = function() {
var docTop = document.body.scrollTop | document.documentElement.scrollTop;
if (postActTop < docTop) {
postAct.setAttribute("style", "width:380px;display:block;position:fixed;top:80px; z-index:200;");
