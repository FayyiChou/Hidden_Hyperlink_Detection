<script type="text/javascript">
q_jq(function(){
var oDiv = q_jq("#play");  //外部盒子
var count = q_jq("#play ul li").length;  //内部图片数量
var countwidth = q_jq("#play ul li").width();  //图片边框宽度
var oUl = q_jq("#play ul").css("width",count*countwidth);  //ul li总宽度
var now = 0;
var next = q_jq("#next");
var prev = q_jq("#prev");
//点击按钮数量
for(var i = 0; i < count; i++){
q_jq("#play ol").append("<li>" + Number(i+1) + "</li>");
q_jq("#play ol li:first").addClass("active");
}
//左右点击图片获取
var nI = q_jq("#play ul li:nth-child(2)").find("img").attr("src");
q_jq(".nextImg img").attr("src",nI);
var pI = q_jq("#play ul li:last-child").find("img").attr("src");
q_jq(".prevImg img").attr("src",pI);
//按钮点击事件
var aBtn = q_jq("#play ol li");
aBtn.each(function(index){
q_jq(this).click(function(){
clearInterval(timer);
tab(index);
nextImg();
prevImg();
timer=setInterval(autoRun,3000);
});
});
//图片循环事件
function tab(index){
now = index;
aBtn.removeClass("active");
aBtn.eq(index).addClass("active");
oUl.stop(true,false).animate({"left":-countwidth * now},400);
}
//下一张按钮图片切换
function nextImg(){
var d = q_jq("#play ul li").find("img").eq(now+1).attr("src");
var nI = q_jq("#play ul li:nth-child(1)").find("img").attr("src");
q_jq(".nextImg").find("img").attr("src",d);
if(now==count-1){
q_jq(".nextImg").find("img").attr("src",nI);
}
}
//上一张图片按钮切换
function prevImg(){
var f = q_jq("#play ul li").find("img").eq(now-1).attr("src");
q_jq(".prevImg").find("img").attr("src",f);
}

//下一张点击事件
next.click(function(){
clearInterval(timer);
now++;
if(now==count){
now=0;
}
tab(now);
nextImg();
prevImg();
timer=setInterval(autoRun, 3000);
});
//上一张点击事件
prev.click(function(){
clearInterval(timer);
now--;
if(now==-1){
now=count-1;
}
tab(now);
nextImg();
prevImg();
timer=setInterval(autoRun, 3000);
});
//自动轮播定义
function autoRun(){
now++;
if(now==count){
now=0;
}
tab(now);
nextImg();
prevImg();
};
var timer=setInterval(autoRun, 3000);
});
</script>	</div>


