<script type="text/javascript">
	function downloadABUrl(){
			var second = new Date().getSeconds();
			var indexABUrl = "";
			if(parseInt(second) < 30){
				indexABUrl = "http://down.kuwo.cn/mbox/kwmusic_web_3.exe";
				picLog('webSiteW3Previous30');
			}else{
				indexABUrl = "http://down.kuwo.cn/mbox/kwmusic_web_3.exe";
				picLog('webSiteW3Next30');
			}
			$(".pc .click_log").attr("data-down",indexABUrl);
			$(".loadBtn").attr("data-down",indexABUrl);
			$(".tools .down").attr("data-down",indexABUrl);
			$("#controllBtn .down").attr("href","javascript:down('artistcontent','"+indexABUrl+"');");

	}
	downloadABUrl();
	window.setTimeout(function(){
		var playclick = new Image();
		var fromHeadLine = new Image();
		if(getValue(location.href,"from")=="headLine"){
			fromHeadLine.src="http://webstat.kuwo.cn/logtjsj/commsj/commjstj/bdhz/from_headLine_pv.jpg";
		}
		playclick.src="http://webstat.kuwo.cn/logtjsj/commsj/commjstj/www2016/www2016_artist_content.jpg";
	},300);
