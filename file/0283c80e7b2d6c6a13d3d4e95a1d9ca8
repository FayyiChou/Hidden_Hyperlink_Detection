<script>
	var loginUrl = location.href;//第三方登陆成功后回跳地址
	if(loginUrl.indexOf("#")>-1){
		loginUrl = loginUrl.substring(0,loginUrl.indexOf("#"));
	}
	;(function($,window,document,undefined){
		var $login = {};
		//登陆初始化入口
		$.fn.loginInit = function(){
			var loginInfo = getData();
			var loginDomStr = createLoginDom();
			var cssStr = getCssStr();
			var isMy = location.href.indexOf("user/home")>-1;
			$("head").append(cssStr);
			if(!isMy){
				$.getScript("/static/js/web/homePage/ugc.js");
			}
			$(this).append(loginDomStr);
			if(loginInfo.userid!=""&&loginInfo.sid!=""){
				//存在uid和sid检测是否匹配
				checkUser(loginInfo,isMy);
			}else{
				$kw_login.setUserInfo("",false);
			}
			bindFn(isMy);
		}
		//检测当前账户是否有效
		function checkUser(loginInfo,isMy){
			var uid = loginInfo.userid;
			$.ajax({
				url:"http://www.kuwo.cn/api/user/checkLogin",
				type:"post",
				data:{
					'uid':uid,
					'sid':loginInfo.sid
				},
				dataType:"json",
				success:function(jsondata){
					if(jsondata.status==200){
//						$(".loginMenu").html('<a href="http://www.kuwo.cn/www/user/home" class="home"><i></i>我的主页</a><a href="javascript:;" class="logout"><i></i>退出</a>')
						$(".loginMenu").html('<a href="javascript:;" class="logout"><i></i>退出</a>');
                        var nickName = getCookie('uname3') || getCookie('username');
                        try{
                            nickName = unescape(nickName);
						}catch (e){}
                        $kw_login.setUserInfo(nickName,true);

						var uname = getCookie('username');
                        try{
                            uname = unescape(uname);
                        }catch (e){}
						$.ajax({
							url: "http://www.kuwo.cn/api/www/discuzLogin?uname=" + uname,
							type: "get",
							dataType: "jsonp",
							jsonp: "jpcallback",
							jsonpCallback: "dzLoginResult",
							success: function (jsondata) {
                                var dzScript = jsondata.data.dzResult;
                                console.info(dzScript)
							    $("body").append(dzScript);

							}
						});
					}else{
						//在个人主页出现账户问题跳到主页其他留在本页刷新
						if(isMy){
							location.href = "http://www.kuwo.cn";
						}else{
							$kw_login.setUserInfo("",false);
						}
					}
				}
			})
		}
		//事件绑定
		function bindFn(isMy){
			$("#loginBtn,.kwLogin").live("click",function(){
				$(".login").show().find(".command").attr("checked","checked");
				$(".loginShadow").show();
			});
			$(".qqLogin").click(function() {
				var qqAuth = "https://graph.qq.com/oauth/show?which=ConfirmPage&display=pc&client_id=100243533&response_type=code&scope=get_user_info,add_share&redirect_uri=http://i.kuwo.cn/US/platform/qq.jsp";
				openWindow(qqAuth, 'QQ账号', 'QQ账号登录', 502, 390);
			});
			$(".wbLogin").click(function() {
				var sinaAuth = 'https://api.weibo.com/oauth2/authorize?client_id=2972927130&response_type=code&redirect_uri=http://i.kuwo.cn/US/platform/weibo.jsp&display=popup';
				openWindow(sinaAuth, 'sina', '新浪微博登录', 620, 491);
			});
			$(".checkBoxLogin").click(function(){
				var $this = $(this);
				$this.hasClass("checked")?$this.removeClass("checked"):$this.addClass("checked");
			});
			$(".sureUp").click(function(){
				if($(".checkBoxLogin").hasClass("checked")){
					$("#keepLogin").val("1");
				}
				if(!$(".loginBox .command").attr("checked")){
					errorTips('请选同意《酷我音乐用户服务协议》《隐私政策》');
					return false;
				}
				$("#loginFrom").submit();

			});
			$(".login").keydown(function(e){
				var e = e||event;
				if(e.keyCode == 13){
					$(".sureUp").click();
					return false;
				}
			});
			$(".closeWindow").live("click",function(){
				$(this).parents(".ugcBox").hide();
				$(".loginShadow").hide();
				$(".loadShadow").hide();
			});
			$(".close").live("click",function(){
				var $this = $(this);
				$this.parents(".boxShadow").hide();
				$(".loginShadow").css("z-index","2");
			});
			$(".logout").live("click",function(){
//				try{
//					delAllCookie();
//				}catch(e){
//					console.log(e.message);
//				}
//				$.ajax({
//					url:"http://kuwo.cn/api/www/loginOut",
//					type:"get",
//					dataType:"json",
//					success:function(jsondata){
//						if(jsondata.status==200){
//							//在个人主页退出跳到主页其他留在本页刷新
//							if(isMy){
//								location.href = "http://www.kuwo.cn";
//							}else{
//								location.reload();
//							}
//						}
//					}
//				})
				var url = location.href;
				location.href="http://www.kuwo.cn/api/www/loginOut?callbackUrl="+encodeURIComponent(url);
			});
		}
		//登陆
		function login(postData){
			$.ajax({
				url:"http://www.kuwo.cn/api/www/kuwoLogin",
				type:"post",
				data:postData,
				success:function(data){
					var data = eval("("+data+")");
					var msg = data.msg;
					if(data.status==200&&msg=="成功"){
						location.reload();
					}else{
						errorTips(msg)
					}
				}
			});
		}
		//设置用户名显示
		$login.setUserInfo = function(name,flag){
			var $userName = $("#userName");
			var $loginBtn = $("#loginBtn");
			name = decodeURIComponent(name);
			$userName.html(name).attr("title",name);
			if(flag){
				$userName.css("display","block");
				$loginBtn.hide();
			}else{
				$userName.hide();
				$loginBtn.css("display","block");
			}
		}
		//获取uid username sid
		function getData(){
			var userid = getCookie("userid");
			var sid = getCookie("sid");
			sid = sid.substring(0,sid.indexOf("_"));
			//qq微博登陆sid取websid
			if(getCookie("t3")=="qq"||getCookie("t3")=="weibo"){
				sid = getCookie("websid");
			}
			return {
				'userid':userid,
				'sid':sid
			}
		}
		//获取cookie
		function getCookie(cname) {
			var name = cname + "=";
			var ca = document.cookie.split(';');
			for(var i=0; i<ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1);
				if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
			}
			return "";
		}
		//设置cookie
		function setCookie(cname, cvalue, exdays) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays*24*60*60*1000));
			var expires = "expires="+d.toUTCString();
			document.cookie = cname + "=" + cvalue + "; " + expires;
		}
		//删除cookie中所有定变量函数
		function delAllCookie(){
			var myDate=new Date();
			myDate.setTime(-1000);//设置时间
			var data=document.cookie;
			var dataArray=data.split("; ");
			for(var i=0;i<dataArray.length;i++){
				var varName=dataArray[i].split("=");
				document.cookie=varName[0]+"=''; expires="+myDate.toGMTString();
			}

		}
		//错误提示
		function errorTips(msg){
			var errorTips = '<div class="errorTips radius boxShadow"><span></span><a class="close" href="javascript:;"></a></div>';
			if(!$('.errorTips').html())$('body').append(errorTips);
			var $errorTips = $('.errorTips');
			var $errorTipsSpan = $('.errorTips span');
			$errorTipsSpan.html(msg);
			$(".loginShadow").css("z-index",4).show();
			$errorTips.show();
			clickFlag = false;
		}
		//QQ微博登陆弹窗
		function openWindow(k, type, name, width, height) {
			window.open(k, name, "width=" + width + ",height=" + height + ",menubar=0,scrollbars=1,status=1,titlebar=0,toobar=0,location=1,resizable=yes");
		}
		//创建dom
		function createLoginDom(){
			return '<div class="loginShadow" style="display: none"></div>'+
					'<div class="ugcBox login boxShadow" style="margin-top:-200px">'+
					'<div class="topBox">'+
					'<h1>登录</h1>'+
					'<a href="javascript:;" class="closeWindow"></a>'+
					'</div>'+
					'<form id="loginFrom" action="/api/www/kuwoLogin" method="post" >'+
					'<input type="hidden" name="retUrl" id="retUrl" value="'+location.href+'">'+
					'<input type="hidden" name="keepLogin" id="keepLogin" value="0">'+
					'<div class="left">'+
					'<span class="lable">酷我账号登录</span>'+
					'<input placeholder="酷我账号" type="text" name="uname" class="number intInput radius" />'+
					'<input placeholder="密码" type="password" name="password" class="pwd intInput radius" autocomplete="off" />'+
					'<div class="leftLine">'+
					'<span class="checkBoxLogin checked"></span>'+
					'<span class="autoLogin">自动登录</span>'+
					'<a href="http://www.kuwo.cn/www/user/register">免费注册</a>'+
//					'<a href="http://pc.i.kuwo.cn/US/mbox/login2015new/findPwd.jsp" target="_blank">忘记密码？</a>'+
					'</div>'+
					'<div class="zc_bot">'+
						'<input name="" class="command" type="checkbox" checked="checked" value="" />'+
						'<label>'+
							'同意'+
							'<a href="http://www.kuwo.cn/static/page/license/license.html" target="_blank">《酷我音乐用户服务协议》</a>'+
							'<a href="http://www.kuwo.cn/static/page/license/license_privacy.html" target="_blank">《隐私政策》</a>'+
					'</label>'+
					'</div>'+
					'<a href="javascript:;" class="sureUp radius20">登录</a>'+
					'<div class="noNum">'+
//					'<span>没有账号？</span>'+

					'</div></div>' +
					'</form>' +
				    '<div class="right">'+
					'<span class="lable">酷我账号登录</span>'+
					'<a class="wbLogin radius20" href="javascript:;"><i></i><span>新浪微博登录</span></a>'+
					'<a class="qqLogin radius20" href="javascript:;"><i></i><span>QQ登录</span></a>'+
					'</div></div>';
		}
		//相关css
		function getCssStr(){
			return '<link type="text/css" rel="stylesheet" href="/static/css/web/homePage/ugc.css" />'+
					'<!--[if lt IE 9]>'+
						'<style>'+
							'.login .left .intInput {background:url(http://www.kuwo.cn/static/image/web/homePage/ugc/loginInputBg.jpg);border:0;height:40px;width:232px;line-height:40px;}'+
							'.login .left .isFocus {background:url(http://www.kuwo.cn/static/image/web/homePage/ugc/loginInputFocusBg.jpg);}'+
				            '.sureUp {background:url(http://www.kuwo.cn/static/image/web/homePage/ugc/upSure.jpg);}'+
						 	'.login .right a {background:url(http://www.kuwo.cn/static/image/web/homePage/ugc/loginTypeBg.jpg);border:0}' +
							'.errorTips .down{background:url(http://www.kuwo.cn/static/image/web/homePage/downBtn.png)}'+
						'</style>'+
					'<![endif]-->';
		}
		window.$kw_login = $login;
	})(jQuery,window,document);
	$(".loginBox").loginInit();
	$(".userBox").hover(function(){
		$(".loginMenu").show();
	},function(){
		$(".loginMenu").hide();
	});
</script>

