		var otherShowQuestionTag=0;
			if(showDegree == '1' || showQuestionTag == '1' || showMessageCount == '1' ){
					FrontNews_list01.getAjaxValueByNewsId("423","false",showDegree,showMessageCount,showQuestionTag,"FrontNews_list01-1436413923421_VisitedCount_1","FrontNews_list01-1436413923421_CommentCount_1","FrontNews_list01-1436413923421_showQuestionTag_1");
					FrontNews_list01.getAjaxValueByNewsId("419","false",showDegree,showMessageCount,showQuestionTag,"FrontNews_list01-1436413923421_VisitedCount_2","FrontNews_list01-1436413923421_CommentCount_2","FrontNews_list01-1436413923421_showQuestionTag_2");
					FrontNews_list01.getAjaxValueByNewsId("393","false",showDegree,showMessageCount,showQuestionTag,"FrontNews_list01-1436413923421_VisitedCount_3","FrontNews_list01-1436413923421_CommentCount_3","FrontNews_list01-1436413923421_showQuestionTag_3");
					FrontNews_list01.getAjaxValueByNewsId("392","false",showDegree,showMessageCount,showQuestionTag,"FrontNews_list01-1436413923421_VisitedCount_4","FrontNews_list01-1436413923421_CommentCount_4","FrontNews_list01-1436413923421_showQuestionTag_4");
			}		
	}
	function setVisited(id,linkUrl,detailOpenStyle){	
		$.get("/FrontNews.do", "method=getVisitedCountByAjax&newsId="+id, function(data){
			var linkUrl1 = linkUrl;
			var OpenStyle = detailOpenStyle
			var url = '/FrontNews.do?method=setVisitedCountByAjax&newsId='+id+'&times='+data;
			$.post(url, function(data){
				window.open(linkUrl1,OpenStyle);
				});
		},'html');
	}
	$(FrontNews_list01['FrontNews_list01-1436413923421_init']);
	// ]]>
	Date.prototype.Format = function (fmt) { //author: meizz 
	    var o = {
	        "M+": this.getMonth() + 1, 
	        "d+": this.getDate(), 
	        "h+": this.getHours(), 
	        "m+": this.getMinutes(), 
	        "s+": this.getSeconds(), 
	        "q+": Math.floor((this.getMonth() + 3) / 3), 
	        "S": this.getMilliseconds() 
	    };
	    if (/(y+)/.test(fmt)) fmt = fmt.replace(RegExp.$1, (this.getFullYear() + "").substr(4 - RegExp.$1.length));
	    for (var k in o)
	    if (new RegExp("(" + k + ")").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : (("00" + o[k]).substr(("" + o[k]).length)));
	    return fmt;
	}
	window.onload =function (){
			var startDate = "";
			var endDate = "";
	 	 	 if(startDate != ""){
				if(endDate != ""){
					var newDate=  new Date().Format("yyyy/MM/dd");
					var newDateValue = Date.parse(newDate);
					var endDate1= endDate.replace(/-/g, "/");
					var endDateValue =  Date.parse(endDate1);
					if(endDateValue - newDateValue < 0){
						var infoMakerValue = document.getElementById("423_infoMaker");
						if(infoMakerValue !=null){
							infoMakerValue.style.display = "none";
						}
					}else{
						var infoMakerValue = document.getElementById("423_infoMaker");
						if(infoMakerValue !=null){
							infoMakerValue.style.display = "block";
						}
					}
				}else{
					var infoMakerValue = document.getElementById("423_infoMaker");
					if(infoMakerValue !=null){
						infoMakerValue.style.display = "block";
					}
				}
			}  
