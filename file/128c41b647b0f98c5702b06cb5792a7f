<script type="text/javascript">
lastScrollY=0;
function heartBeat(){
var diffY;
if (document.documentElement && document.documentElement.scrollTop)
diffY = document.documentElement.scrollTop;
else if (document.body)
diffY = document.body.scrollTop
else
{}

percent=.1*(diffY-lastScrollY);
if(percent>0)percent=Math.ceil(percent);
else percent=Math.floor(percent);
document.getElementById("LeftAd").style.top=parseInt(document.getElementById("LeftAd").style.top)+percent+"px";
document.getElementById("RightAd").style.top=parseInt(document.getElementById("LeftAd").style.top)+percent+"px";
lastScrollY=lastScrollY+percent;
}
suspendcode12=""
suspendcode14=""
document.write(suspendcode12);
document.write(suspendcode14);
window.setInterval("heartBeat()",1);
function ClosedivLeft1()
{
LeftAdOne.style.visibility="hidden";
} 
function ClosedivLeft2()
{
LeftAdTwo.style.visibility="hidden";
} 
function ClosedivRight1()
{
rightAdOne.style.visibility="hidden";
} 
function ClosedivRight2()
{
