function resizeImage(b, f, d) {
	if (b.width && b.height) {
		var c = parseInt(b.width);
		var a = parseInt(b.height);
		if ((f / c) <= (d / a)) {
			if (f < c) {
				b.width = f;
				b.height = (f / c) * a;
			} else {
				b.width = c;
				b.height = a;
			}
		} else {
			if (d < a) {
				b.height = d;
				b.width = (d / a) * c;
			} else {
				b.width = c;
				b.height = a;
			}
		}
	}
}
function thumbnailResizeImage(obj){
	resizeImage(obj,90,90);
	obj.style.top = (98-obj.height)/2+"px";
	if(navigator.appVersion.indexOf('MSIE 6.0')!=-1){
		obj.style.left = (98-obj.width)/2+"px";
	}else{
		obj.style.left = (108-obj.width)/2+"px";
	}
}
