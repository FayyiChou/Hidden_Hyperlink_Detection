 <script type="text/javascript"> 
$(document).ready(function(){
	$(".imageplus-close").click(function(){
		$("#imageplus1").css("display","none");
	});
	$("#picture").on("mouseenter", function(e){
		$(".imageplus-d").css("display","block");
		$(".imageplus-s-wrap").addClass("imageplus-s-current");
	});
	$("#picture").on("mouseleave", function(){
		$(".imageplus-d").css("display","none");
		$(".imageplus-s-wrap").removeClass("imageplus-s-current");
	});
	$(".imageplus-d").on("mouseenter", function(){
		$("#picture").unbind("mouseenter");
		$("#picture").unbind("mouseleave");
		$(".imageplus-d").css("display","block");
		$(".imageplus-d").addClass("imageplus-d-hover");
		$(".imageplus-s-wrap").removeClass("imageplus-s-current");
	});
	$(".imageplus-d").on("mouseleave", function(e){
		$("#picture").unbind("mouseenter");
		$("#picture").unbind("mouseleave");
		$(".imageplus-d").css("display","block");
		$(".imageplus-d").removeClass("imageplus-d-hover");
		$(".imageplus-s-wrap").addClass("imageplus-s-current");
		$("#picture").on("mouseleave", function(){
			 $(".imageplus-d").css("display","none");
			 $(".imageplus-s-wrap").removeClass("imageplus-s-current");
	    });
		$("#picture").on("mouseenter", function(e){
			$(".imageplus-d").css("display","block");
	        $(".imageplus-s-wrap").addClass("imageplus-s-current");
		});
	});
	$(".imageplus-s-wrap").on("mouseenter", function(){
		$("#picture").unbind("mouseenter");
		$("#picture").unbind("mouseleave");
		$(".imageplus-d").css("display","block");
		$(".imageplus-s-wrap").addClass("imageplus-s-current");
	});
	$(".imageplus-s-wrap").on("mouseleave", function(e){
		$("#picture").on("mouseleave", function(){
			 $(".imageplus-d").css("display","none");
			 $(".imageplus-s-wrap").removeClass("imageplus-s-current");
	   });
		$("#picture").on("mouseenter", function(e){
			$(".imageplus-d").css("display","block");
	       $(".imageplus-s-wrap").addClass("imageplus-s-current");
		});
	});	
}); 
</script>
