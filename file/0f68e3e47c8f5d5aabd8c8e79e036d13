		});
	});
	// footer widget minisite filtering
	jQuery('body').on('click', '#footer .minisite-tabs .sort-buttons a', function(e){
		jQuery("#footer .minisite-tabs .loading").show();
		jQuery("#footer .minisite-tabs .post-list").animate({opacity: "0.15"}, 0);	
