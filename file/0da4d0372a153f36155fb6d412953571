	<script>
var funs = (	function (){
var content =$("content"),side =$("side"), side_holder =$("side_holder"),side_inner = side_holder.$E(".side_inner"),s_top = $E(".s_top"),s_bottom = $E(".s_bottom"),documentViewer = $("documentViewer");
function resizeViewer(){
var size=window.getSize(),height = size.y;
var el=side_inner.$E(".cur"),mt=el.getStyle("margin-top").toInt(), mb=el.getStyle("margin-bottom").toInt();
var hh=height-side_holder.getPosition().y-s_bottom.getSize().y,maxH=el.getSize().y * 5+4*(Math.max(mt,mb))+mt+mb;
hh=hh>maxH?maxH:Math.min(hh,side_inner.getSize().y,side_inner.getScrollSize().y);
side_holder.setStyle("height",hh);
var left = content.getPosition().x+content.getSize().x+3,w = side.getSize().y;
if(left+w>size.x){left=size.x-w;}
side.setStyle("left",left);
documentViewer && documentViewer.setStyle("height",height-documentViewer.getPosition().y-5);
scollCallback();
}
	
function init(){
resizeViewer();
s_top.addEvent("click",function(){scroll(-1);});
s_bottom.addEvent("click",function(){scroll(1);});
var el =side_inner.$E(".cur"),index = el.get("index").toInt(),ch=el.getSize().y;
side_inner.scrollTop=index*ch-Math.floor((side_inner.getSize().y-ch)/2);
}
function scroll(s){new Fx.Scroll(side_inner).start(false,side_inner.scrollTop+side_inner.getSize().y*s).chain(scollCallback);}
function scollCallback(){
if(side_inner.scrollTop>0){	s_top.removeClass("s_top_dis");}else{	s_top.addClass("s_top_dis");}
var h = side_inner.getSize().y;
if(h+side_inner.scrollTop<side_inner.getScrollSize().y){s_bottom.removeClass("s_bottom_dis");}else{s_bottom.addClass("s_bottom_dis");}
}

return {
init: init,
resizeViewer:resizeViewer
};
})();
funs.resizeViewer();
funs.init();
$$$(function(){
funs.resizeViewer();
window.addEvent('resize', funs.resizeViewer);
$$("#side .side_inner a").addEvent("click",function(){$(this).set("href", "/file/"+this.get("fid")+"/preview?s=");});
});
</script>

