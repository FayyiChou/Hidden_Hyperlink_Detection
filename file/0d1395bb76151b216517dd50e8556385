  <script>
    $(".list-ul>li").on('mousemove',function(){
        if ($(this).children('ul').get(0)){
            $(this).find('ul').css("display","block");
            $(this).children('a').addClass("a-hover");
        }else {
            $(this).children('a').addClass('ahover');
        }
    }).on('mouseout',function(){
        $(this).find('ul').css("display","none");
        $(this).children('a').removeClass("a-hover ahover");
    });
</script> 
  <script>
    $(function(){
        $('.yuyue_vertop .click_btn').click(function(e){
            e.stopPropagation();
            $('.content_jumpDiv').click(function(e){
                e.stopPropagation();
            });
            $('.yuyue_vertop').css('z-index',99);
            $(this).parents('.yuyue_vertop').css('z-index',999);

            $('.content_jumpDiv').fadeOut('fast');
            $(this).parents('.yuyue_vertop').find('.content_jumpDiv').fadeIn("");
            var check = 0;
            var postid = $(this).attr("id");
            var phone = $(this).attr("data-phone");
            var obj1 = ".weixinqrcode_" + postid;
            $(obj1).each(function (){
                if ($(this).attr("src"))
                {
                    check = 1;
                }
            });
            if (check)
            {
                return false;
            }
            var url = "/ajax/weixinqrcode/";
            var cityname = "深圳";
            $.ajax({
                type: "post",
                url: url,
                data: {postid:postid, phone:phone},
                dataType: "json",
                success: function(data){
                    $(obj1).each(function (){
                        $(this).attr("src", data.url);
                    });
                    var obj2 = ".location_" + postid;
                    $(obj2).each(function (){
                        if (data.location)
                        {
                            $(this).text(data.location);
                        }
                        else
                        {
                            $(this).text(cityname);
                        }
                    });
                    if (data.location)
                    {
                        var obj3 = ".xiegang_" + postid;
                        $(obj3).each(function (){
                            $(this).show();
                        });
                    }
                }
            });
        });
        $('.yuyue_vertop .close_btn,body').click(function(){
            $('.content_jumpDiv').fadeOut('')
        })
    })
</script> 
