<script> 
$(function(){
	$(".gov_weixin_share .closeBtn").click(function(){
		$(".gov_weixin_share").hide();
	})
	
	//右侧浮窗
	function rigScroll(){
		var winWidth=$(window).width();
		if(winWidth<1024){
			$(".gov_weixin_share").hide();
		}else{		
			var winRig=(winWidth-1005)/2-105;
			$(".gov_weixin_share").css({"right":winRig})
		}
	}
	rigScroll();
	
	$(window).scroll(function() {
		rigScroll();
	});
 
	$(window).resize(function() {
		rigScroll();
	});	
})
</script> 

