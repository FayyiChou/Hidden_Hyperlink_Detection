    <script type="text/javascript">
				var $HideField_li = $('.HideField > li');
				var Scr_counts = Math.ceil($('.HideField > li').length/35);
				var $Crl_list = $('.carousel-onlineBooth-list');
				for(var i = 1;i <= Scr_counts;i++){
					for(var j = 1;j <= 35;j++){
						var index = (i-1) * 35 + (j-1);
						$HideField_li.eq(index).clone().appendTo($Crl_list);
					}
					$('.carousel-onlineBooth-list > li').wrapAll("<ul class='carousel-onlineBooth-li clearfix fl'></ul>");
				}
				
