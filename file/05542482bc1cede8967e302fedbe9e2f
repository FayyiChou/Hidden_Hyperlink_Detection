<script type="text/javascript">
(function($){
	/*============ scroll js =============*/   
	$.fn.extend({
		
		Scroll:function (opt,callback){
			if (!opt) var opt = {};
			var timerID;
			var _this = this.eq(0).find(".carousel-onlineBooth-list");
			var lineW = _this.find(".carousel-onlineBooth-li:first").outerWidth(true) //获取列宽 
			line = 1; /*opt.line ? parseInt(opt.line, 10) : parseInt(_this.width() / lineW, 10), //每次滚动的列数，默认为一屏，即父容器列宽 */
			speed = opt.speed ? parseInt(opt.speed, 10) : 500; //滚动速度，数值越大，速度越慢（毫秒） 
			timer = opt.timer ? parseInt(opt.timer, 10) : 3000; //滚动的时间间隔（毫秒） 

			if (line == 0) line = 1;
			var upWidth = 0 - line * lineW;
			//滚动函数 
			var scrollLeft = function () {
				if (!_this.is(":animated")) {
					_this.animate({
						left: upWidth
					}, speed, function () {
						for (i = 1; i <= line; i++) {
							_this.find(".carousel-onlineBooth-li:first").appendTo(_this);
						}
						_this.css({ left: 0 });
					});
				}
			}
			

			var autoPlay = function () {
				if (timer) timerID = window.setInterval(scrollLeft, timer);
			};
			var autoStop = function () {
				if (timer) window.clearInterval(timerID);
			}; //鼠标事件绑定 
			_this.hover(autoStop, autoPlay).mouseout();
			
		}
	})

})(jQuery);


/*============ 页面加载主函数 =============*/
$(function(){

	$('.exhLoca-Tab > span:last').hide();
	$('.exhLoca-Pane > img:eq(0)').show();
	$('.exhLoca-Tab > a:eq(0)').addClass('active');

	/* ==== 在线展台滚动调用 ==== */
    $(".carousel-onlineBooth-wrap").Scroll({ 
    	line: 3, 
    	speed: 500, 
    	timer: 3000
    });


    var index=0;
    var adTimer;
    var $img_li = $("#carousel-proPublish .carousel-list li");
    var $dot_li = $("#carousel-proPublish .carousel-dot-list li");
    var $txt_li = $('.colum-proPublish .colum-list li');
    var len  = $img_li.length;

    $txt_li.eq(0).addClass('on');
    $img_li.eq(0).show();
    $dot_li.eq(0).addClass('on');
    function banner_scroll(){ 
      index++;
      if(index==len){index=0;}
      banner_change();         
    }
    $txt_li.mouseover(function(){
      index = $(this).index();
      banner_change()
    })
    $dot_li.click(function(){
      index = $dot_li.index($(this));
      banner_change()
    })
    adTimer = setInterval(banner_scroll, 5000);
    //$(".banner-list").hover(function(){
//       clearInterval(adTimer);
//    },function(){
//       adTimer = setInterval(banner_scroll, 5000); 
//    });
    function banner_change(){
        $img_li.eq(index).fadeIn(300).siblings().fadeOut(300);
        $dot_li.eq(index).addClass('on').siblings().removeClass('on');
        $txt_li.eq(index).addClass('on').siblings().removeClass('on');
    }


    
})
</script>
</html>
