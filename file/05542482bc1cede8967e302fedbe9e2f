<script type="text/javascript">
	jQuery("#carousel-banner").slide({mainCell:".carousel-list ul",autoPlay:true});
	jQuery("#carousel-ad-1").slide({mainCell:".carousel-list ul",autoPlay:true});
	// jQuery("#carousel-proPublish").slide({mainCell:".carousel-list ul",autoPlay:true});
