  <script type="text/javascript">
function jump_dq(){
		area1 = $("#select_dq").val();
		href3 = "http://www.125edu.com/gaokao/dq_fsx/#"+area1;
		window.open(href3);
}
var xxfs_arr=null;

function jump_xx(){

		area1 = $("#select1").val();
		area2 = $("#select2").val();
		area3 = $("#select3").val();
		area4 = $("#select4").val();
		area5 = $("#select5").val();
		var href2 ='';
		$.each(xxfs_arr,function(k,v){
			if(area1==v.dq_id){
				href2 = "http://www.125edu.com/gaokao/xx_fenshu-"+v.catid+"-"+area2+"-"+area1+"-0-0-"+area5+"-"+area3+"-"+area4+"-0-1-1-2-0-1.html";
			}
		})
		if(href2==''){alert('请选择生源地');}else{
		//href2 = "http://www.125edu.com/gaokao/xx_fenshu-"+xxfs_arr_ob.area1.catid+"-"+area2+"-"+area1+"-0-0-"+area5+"-"+area3+"-"+area4+"-0-1-1-1-0-98.html";
		window.open(href2);
		}
}
var zyfs_arr=null;  

function jump_zy(){

		areas1 = $("#select_z1").val();//学校省份・
		areas2 = $("#select_z2").val();//生源所在地
		areas3 = $("#select_z3").val();//办学类型
		areas4 = $("#select_z4").val();//办学性质
		areas5 = $("#select_z5").val();//录取科类
		var href1 ='';
		$.each(zyfs_arr,function(k,v){
			if(areas2==v.dq_id){
				href1 = "http://www.125edu.com/gaokao/zy_fenshu-"+v.catid+"-"+areas3+"-"+areas2+"-"+areas1+"-0-0-"+areas4+"-"+areas5+"-0-1-1-1-0-2.html";
			}
		})
		if(href1 ==''){alert('请选择生源地');}else{
		//href1 = "http://www.125edu.com/gaokao/zy_fenshu-"+zyfs_arr.area1.catid+"-"+areas3+"-"+areas2+"-"+areas1+"-0-0-"+areas4+"-"+areas5+"-0-1-1-1-0-98.html";
		window.open(href1);
		}
}
function alarm(){
	alert("此模块数据正在更新，请明天再来访问，给您带来不便敬请谅解！");
}
function add_favorite(title) {
		$.getJSON('http://www.125edu.com/api.php?op=add_favorite&title='+encodeURIComponent(title)+'&url='+encodeURIComponent(location.href)+'&'+Math.random()+'&callback=?', function(data){
			if(data.status==1)	{
				$("#favorite").html('收藏成功');
			} else {
				alert('请登录');
			}
		});
	}
</script>  
