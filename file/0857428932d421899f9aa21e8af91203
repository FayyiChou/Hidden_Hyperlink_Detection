  <script>
// 右侧浮动咨询
$(document).ready(function(){
	$("ul.sidenav_con li").hover(function() {
		$(this).find("div").stop().animate({left: "-135", opacity:1}, "fast").css("display","block");
		$(this).find(">a").addClass("current");
	},function(){
		$(this).find("div").stop().animate({left: "100px", opacity: 0}, "fast");
		$(this).find(">a").removeClass("current");
	});
	$("#guan").click(function(){
		$(".add_mr").css("display","block");
		$(".demopage").css("display","none")
	});
	$("#add_mr").click(function(){
		$(this).css("display","none");
		$(".demopage").css("display","block")
	});
	$(function(){
	var aa=$(window).height()*0.2;
	var cc=$(window).height();
    $(".martop").css("margin-top",aa);
    $(".demopage").css("height",cc);
});
	
});
// 右侧浮动咨询结束束
