<script language="javascript" type="text/javascript">
$(function(){
	overShow();
});
function overShow(){
	var v = $.cookie("zcoolideaclose");
	if(v==null||v==""){
		$("#modal-gameover").modal("show");
	}
}
function overHide(){
	var date = new Date();
	date.setTime(date.getTime()+10*60*1000);//只能这么写，10表示10秒钟
	$.cookie("zcoolideaclose","close",{ expires: date });//3天
}


