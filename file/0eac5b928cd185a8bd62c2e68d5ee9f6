    <script language="javascript" type="text/javascript">
        function showCKWX() {
            var obj = Fid("div_ckwx");

            if (obj != null) {
                obj.style.display = 'block';
                FsetOffsetWindowPostionByRate(obj, 2, 3);
                FsetModal();
            }
        }
        function CloseCKWX() {
            var obj = Fid("div_ckwx");

            if (obj != null) {
                obj.style.display = 'none';
                FunsetModal();
            }
        }
        function showType(type) {
            var obj = Fid("div_choiceType");

            if (obj != null) {
                var hobj = Fid("Hidden_Type");

                if (hobj != null) {
                    hobj.value = type;
                }

                obj.style.display = 'block';
                FsetOffsetWindowPostionByRate(obj, 2, 3);
                FsetModal();

                var s_a = Fid("r_a").style.display = "none";
                var s_p = Fid("r_p").style.display = "none";

                if (type == "yes") {
                    s_a = Fid("r_a").style.display = "";
                }
                else {
                    s_p = Fid("r_p").style.display = "";
                }
            }
        }
        function CloseType() {
            var obj = Fid("div_choiceType");

            if (obj != null) {
                obj.style.display = 'none';
                FunsetModal();
            }
        }
    </script>

    <script language="javascript" type="text/javascript">
        function check(st) {
            var username = document.getElementById(st + "_UserName").value;
            var userpwd = document.getElementById(st + "_UserPwd").value;
            if (username.length == 0 || username == "") {
                alert("用户名不能为空，请输入用户名！");
                return false;
            }
            if (userpwd.length == 0 || userpwd == "") {
                alert("密码不能为空，请输入密码！");
                return false;
            }
            var checkcode = document.getElementById(st + "_TextBox_Check").value;
            if (checkcode.length == 0 || checkcode == "") {
                alert("验证码不能为空，请输入验证码！");
                return false;
            }
            return true;
        }
    </script>

    <script src="Script/jQuery.js" type="text/javascript"></script>

    <script type="text/javascript">
        function check_school() {
            if ($.trim($("#school_name").val()) == "") {
                alert("请填写学校名称！");
                return false;
            }
        }

        function wangjimima() {
            alert("如果您忘记密码，可以通过以下两种方法处理：\r\n1.已选择指导教师的，可以由指导教师重置密码。\r\n2.由教务处或者所在院系拥有管理账号的管理人员重置密码。");
            return false;
        }

    </script>

