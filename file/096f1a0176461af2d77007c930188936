<script>
var ShareId = "", shareTit, shareDesc, shareUrl, sharePic;

$('.module02 .share').mouseenter(function(){
	var o=$(this).parents('.item');
	shareTit=o.find('h4').text();
	shareDesc=o.find('.font p').text().replace(/浏览全文>>/g,'');
	shareUrl=o.find('a').eq(0).attr('href');
	sharePic=o.find('.pic img').attr('src');
});

window._bd_share_config = {
	common : {
		onBeforeClick : function(cmd, config){
			if(shareTit) config.bdText=shareTit;
			if(shareDesc) config.bdDesc=shareDesc;
			if(shareUrl) config.bdUrl=shareUrl;
			if(sharePic) config.bdPic=sharePic;
			return config;
		},
		bdStyle : '1'
	},
	share : {}
}

with(document)0[(getElementsByTagName('head')[0]||body).appendChild(createElement('script')).src='http://bdimg.share.baidu.com/static/api/js/share.js?cdnversion='+~(-new Date()/36e5)];
</script>


