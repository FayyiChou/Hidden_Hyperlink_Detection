<script type="text/javascript">
$(function(){
var imgWid = 0 ;
var imgHei = 0 ; //变量初始化
var big = 1.05;//放大倍数
$(".brand_img").hover(function(){
	$(this).find("img").stop(true,true);
	var imgWid2 = 0;var imgHei2 = 0;//局部变量
	imgWid = $(this).find("img").width();
	imgHei = $(this).find("img").height();
	imgWid2 = imgWid * big;
	imgHei2 = imgHei * big;
	$(this).find("img").css({"z-index":2});
	$(this).find("img").animate({"width":imgWid2,"height":imgHei2});
},function(){$(this).find("img").stop().animate({"width":imgWid,"height":imgHei});});
})
</script>
