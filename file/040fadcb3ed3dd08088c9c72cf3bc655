    }

    $('#wangzhi').val(window.location.href);
    function getUrl() {
        var wangZhiTxt = $('#wangzhi');
        wangZhiTxt.select();
        document.execCommand("Copy"); // 执行浏览器复制命令
        $('.wangzhiTan').show();
        setTimeout(function () {
            $('.wangzhiTan').fadeOut();
        }, 1000);
    }  

</script>
