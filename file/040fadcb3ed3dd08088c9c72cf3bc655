    function AskHelp() {
        $('#ask_topic').fadeIn(500).loadAskPanel(0); $('.layerBg').css('height', $(document).height()).show();
    }
    function WantGo() {
        $('#dream_place').fadeIn(500).loadDPPanel('', 0, true); $('.layerBg').css('height', $(document).height()).show();
    }

    var SEARCHLEFT = -44, box;
    if ($.cookie("CityID") == null || $.cookie("ChName") == null || $.cookie("IndexAreaName") == null) {
        $.getScript("http://ip.lotour.com", function () {
            $.cookie("CityID", result.regionid, { expires: 30, path: '/', domain: 'lotour.com' });
            $.cookie("ChName", result.Chname, { expires: 30, path: '/', domain: 'lotour.com' });
            $.cookie("IndexAreaName", result.regionname, { expires: 30, path: '/', domain: 'lotour.com' });
            $("#dujiaxianlu").attr("href", "http://d.lotour.com/dujiaxianlu/" + $.cookie("ChName") + "/");
        });
    }
    else
        $("#dujiaxianlu").attr("href", "http://d.lotour.com/dujiaxianlu/" + $.cookie("ChName") + "/");

    $('.lt-srh-new-layer li').click(function () {
        var index = $(this).index();
        $('.lt-srh-new-clk a').attr('class', 'l' + (index + 1));
        if (index > 1) {
            $("#htype").attr("value", index - 1);
        }
        else if (index == 1) {
            $("#htype").attr("value", 5);
        }
        else {
            $("#htype").attr("value", index);
        }
        $("keys").attr("value", "");
        switch (index) {
            case 0:
                box = $("#keyword").searchbox({ "Left": SEARCHLEFT, "Top": 40, "Left": SEARCHLEFT });
                break;
            case 1:
                box = $("#keyword").searchbox({ "Flay": "xue_Daren", "Js": "", "Url": "http://my.lotour.com/DarenSearch/handler/AssociateDaren.ashx", "i": 1, "j": 5, "Top": 40, "Left": SEARCHLEFT, "tips": "" });
                break;
            case 2:
                box = $("#keyword").searchbox({ "Flay": "xue_Hotel", "Js": "http://js.lotour.net/searchlayer/djmdd.js", "Url": "http://d.lotour.com/hotel/api/AssociateRegion", "i": 1, "j": 4, "Top": 40, "Left": SEARCHLEFT });
                break;
            case 3:
                box = $("#keyword").searchbox({ "Flay": "xue_Djxl_Mdd", "Js": "http://js.lotour.net/searchlayer/djmdd.js", "Url": "http://d.lotour.com/dangdigentuan/api-AssociateRegion", "i": 1, "j": 4, "Top": 40, "Left": SEARCHLEFT });
                break;
            case 4:
                box = $("#keyword").searchbox({ "Flay": "xue_OrderLine_Dest", "Js": "http://d.lotour.com/dujiaxianlu/api-AssociateCommonOsite?sSite=" + $.cookie("CityID") + "&randID=" + escape(new Date()) + "&callback=?", "Url": "http://beijing.lotour.com/dujiaxianlu/api-AssociateOSiteRegion", "i": 0, "j": 4, "Top": 40, "Left": SEARCHLEFT });
                break;
        }
        $('.lt-srh-new-layer').hide()
        return false;
    }).eq(0).trigger("click");

    var loginState = new lotourLoginState({});
    //loginState.init();

    //导航
    $('.lt-nav-item').hover(function () {
        $(this).find('.menu,.lt-srh-new').toggle()
