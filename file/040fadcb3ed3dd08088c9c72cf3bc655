    $('.nav-new-btn').hover(function () {
        $(this).find('.btn').toggleClass('btn-on');
        $(this).find('.nav-new-layer').toggle();
    });
    $('.nav-new-layer li,.ltCommentList li').hover(function () {
        $(this).toggleClass('hover');
    });
</script>
