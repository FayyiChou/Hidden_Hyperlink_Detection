<script type="text/javascript">
    $(function () {
        if (window.PIE) { $('.ia-writer img,.commnLstL img,.ltCommentBox,.imglist li,.imglist2 li .txt2 img,.ia-text img,.ia-code img').each(function () { PIE.attach(this); }); }
