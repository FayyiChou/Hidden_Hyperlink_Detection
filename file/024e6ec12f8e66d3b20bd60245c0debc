<script>
var uri = document.location.href.toLowerCase();
var na = ['/release/','/user/reg','/work/','/work'];
var na_ga = [];

function is_a(){
for(var i in na)
    if(uri.indexOf(na[i])>=0) return false;
return true;
}

function is_ga_a(){
var t = na.concat(na_ga);
for(var i in t)
    if(uri.indexOf(t[i])>=0) return false;
return true;
}
</script>





