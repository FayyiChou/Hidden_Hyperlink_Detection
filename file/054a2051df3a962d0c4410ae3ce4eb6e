  <script type="text/javascript">
  function IsPC()  {      
	  var userAgentInfo = navigator.userAgent;
	  //Safari
	  if (userAgentInfo.indexOf("Safari") > 0) {
      // $(".btn-zhuce1").addClass("btn_zhuce_s");
       
    }
  var Agents = new Array("Android", "iPhone", "SymbianOS", "Windows Phone", "iPad", "iPod");     
  var flag = true;       
   for (var v = 0; v < Agents.length; v++) {        
	  if (userAgentInfo.indexOf(Agents[v]) > 0) {
		  flag = false; break; }     
	  }         
   return flag; 
  }      
    $(function(){
    	$("#dz1").hide();
    	$("#dz2").hide();
    	  if(!IsPC()){
    		  $("#navbar").removeClass("navbar-fixed-top");
    		  $("#dz1").show();
    	  }else{
    		  
    		  $("#dz2").show();
    	  }
    	  
	  $(".btn-login1").click(function(){
			 document.denglu.action="/denglu";
			 document.denglu.method="POST";
			 document.denglu.submit();
	  });
	  
	    $(".btn-zhuce1").click(function(){
			   alert("建设中，请下载App注册!");
			  
		});
	
  });


  
  
  </script>
