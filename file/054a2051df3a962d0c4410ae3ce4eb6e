			   //document.frm.method="post";
			   //document.frm.action="http://zhushou.360.cn/detail/index/soft_id/3102082?recrefer=SE_D_";
			  // document.frm.submit();
			  
			   $("#btn-xiazhai").removeClass("btn-xiazhai_active").addClass("btn-xiazhai");
			   $(this).removeClass("btn-xiazhai").addClass("btn-xiazhai_active");
			   $("#xiazai_qr").attr("src",src+"qr2.jpg");
		  });
		  $("#btn-xiazhai").click(function(){
			  //  alert("APP正在建设中!");
			   
			   $("#btn-xiazhai2").removeClass("btn-xiazhai_active").addClass("btn-xiazhai");
			   $(this).removeClass("btn-xiazhai").addClass("btn-xiazhai_active");
			   $("#xiazai_qr").attr("src",src+"qr_yuanding.jpg");
