var speed=15 
shans12.innerHTML=shans11.innerHTML 
function Marquee15(){ 
if(shans12.offsetWidth-shans.scrollLeft<=0) 
shans.scrollLeft-=shans11.offsetWidth 
else{ 
shans.scrollLeft++ 
} 
} 
var MyMar5=setInterval(Marquee15,speed) 
shans.onmouseover=function() {clearInterval(MyMar5)} 
shans.onmouseout=function() {MyMar5=setInterval(Marquee15,speed)} 
</SCRIPT>
