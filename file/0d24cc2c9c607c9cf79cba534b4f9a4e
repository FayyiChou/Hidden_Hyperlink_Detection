<script>
	$('.standard-filter-submit').on('click', function (e) {
		e.preventDefault();
		var v = $('.construct.browse-standards').find('input:checkbox:checked').map(function () {
			return this.value;
		}).get();
		location.href = '/standards/search' + (v.length ? '?c=' + v.join(',') : '');
	});

	var c = $('section.construct.browse-standards');
	!c.hasClass('where-help') && !c.hasClass('supply-chains') && c.on('click', 'label img', function (e) {
		setTimeout(function() {
			$('.standard-filter-submit').trigger('click');
		}, 10);
	});

	c.on('click', '.box .expand', function (e) {
		if (!$(e.target).is('label') && !$(e.target).is('img'))
			$(this).parents('.box').find('.ei-input.ei-checkbox').trigger('click');
	});
</script>


