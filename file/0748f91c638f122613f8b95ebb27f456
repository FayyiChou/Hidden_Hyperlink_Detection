$(".js-history").click(function(){
    goBack();
})

function goBack() {
    if ((navigator.userAgent.indexOf('MSIE') >= 0) && (navigator.userAgent.indexOf('Opera') < 0)) { // IE  
        if (history.length > 0) {
            window.history.go(-1);
        } else {
            window.location.href = 'http://m.danqi.com/';
        }
    } else { //非IE浏览器  
        if (navigator.userAgent.indexOf('Firefox') >= 0 ||
            navigator.userAgent.indexOf('Opera') >= 0 ||
            navigator.userAgent.indexOf('Safari') >= 0 ||
            navigator.userAgent.indexOf('Chrome') >= 0 ||
            navigator.userAgent.indexOf('WebKit') >= 0) {
            if (window.history.length > 2) {
                window.history.go(-1);
            } else {
		window.location.href = 'http://m.danqi.com/';
            }
        } else {//未知的浏览器  
            if (history.length > 1) {
                window.history.go(-1);
            } else {
		window.location.href = 'http://m.danqi.com/';
            }
        }
    }
}

$(".go_home").fadeOut();
$(window).scroll(function(){
    if ($(window).scrollTop() > 200) {
        $(".go_home").fadeIn(500);
    } else {
        $(".go_home").fadeOut(500);
    }
});
$(function() {
    ykdCommon.init();
})
var ykdCommon = {
    init: function() {
        this.scrollTop.init();
    },
    scrollTop: {
        init: function() {
            var _this = this;

            $(document).on('click', '.go_home', function(e) {
                _this.scrollTo();
                return false;
            });
        },

        scrollTo: function() {
            $('html, body').animate({
                scrollTop: 0
            }, 300);
        }
    }
};
</script>


