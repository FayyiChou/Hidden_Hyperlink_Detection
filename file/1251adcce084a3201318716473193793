<script type="text/javascript">
window.onload = function() {
	//分页
	var pageInit = false;
	$("#search_result_page").pagination('0', {
		items_per_page: 12,
		current_page: '1' - 1,
		num_display_entries: 5,
		first_text: "首页",
		prev_text: '上一页',
		next_text: '下一页',
		end_text: "尾页",
		first_show_always: true,
		end_show_always: true,
		link_to: '#',
		callback: function(idx, o) {
			if (pageInit) {
				var locationURL = window.location.href;
				//替换url地址中p3-p4的值
				var p3 = locationURL.indexOf('p3-');
				var p4 = locationURL.indexOf('-p4');
				var p5 = locationURL.indexOf('-p5');
				var id = 1;
				if (p3 == -1 && p4 == -1) {
					$("#sort a").each(function(){
						if($(this).attr('class') == 'active'){
						   id = $(this).attr('value')
						}
					})
					locationURL = locationURL + "-0-p2-" + id + "-p3-1-p4-0-p5";
				}
				var p3 = locationURL.indexOf('p3-');
				var p4 = locationURL.indexOf('-p4');
				var newURL = locationURL.substring(0,p3+3) + (idx+1) + locationURL.substring(p4,p4+8);
				window.location.href = newURL;
			}
			pageInit = true;
		}
	});
	
	//排序
	$("#sort a").bind('click',function(){
		var locationURL = window.location.href;
		//替换url地址中p2-p3的值
		var p2 = locationURL.indexOf('p2-');
		var p3 = locationURL.indexOf('-p3');
		var p4 = locationURL.indexOf('-p4');
		var p5 = locationURL.indexOf('-p5');
		if (p2 == -1 && p3 == -1 && p4 == -1 && p5 == -1) {
			locationURL = locationURL + "-0-p2-1-p3-1-p4-0-p5";
		}
		var p2 = locationURL.indexOf('p2-');
		var p3 = locationURL.indexOf('-p3');
		var p4 = locationURL.indexOf('-p4');
		var p5 = locationURL.indexOf('-p5');
		var newURL = locationURL.substring(0,p2+3) + $(this).attr("value") + "-p3-1-p4-0-p5";
		var newURL = locationURL.substring(0,p2+3) + $(this).attr("value") + "-p3-1"+locationURL.substring(p4,p5+3);
		window.location.href = newURL;
	})

	//清空cookie
    $("#clearrecentview").click(function(){
        $.cookie('collegeview', null, {path: '/', expires: -1});
		$("#clearrecentviewlistDIV>div").remove();
    });
};
</script>
