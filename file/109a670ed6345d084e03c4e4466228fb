<script>
$(function(){
    var isWidescreen=screen.width>=1280;
    var f = jQuery("#topCurtain");
    if (f.length < 1) {
        return
    }
    var g = null;
    var c = jQuery("#site_header");
    var l = c.css("padding-top");
    if (l && l.indexOf("px") >= 0) {
        l = l.replace("px", "")
    }
    if (c.attr("data-hfix")) {
        g = c.attr("data-hfix")
    }
    var h = f.find(".index_topbanner_fold");
    var i = f.find(".big_topbanner");
    var a = jQuery("#smallTopBanner");
    var e = typeof isWidescreen != "undefined" ? isWidescreen: false;
    h.click(function() {
        var m = $(this);
        if (m.hasClass("index_topbanner_unfold")) {
            m.removeClass("index_topbanner_unfold");
            m.html("展开<s></s>");
            i.slideUp();
            a.slideDown();
            if (g) {
                c.animate({
                    "padding-top": l + "px"
                })
            }
        } else {
            m.addClass("index_topbanner_unfold");
            m.html("收起<s></s>");
            i.slideDown();
            a.slideUp();
            if (g) {
                c.animate({
                    "padding-top": (l - g) + "px"
                })
            }
        }
    });
    var k = function(m) {
        var n = setTimeout(function() {
            var o = m;
            if (o.hasClass("index_topbanner_unfold")) {
                o.removeClass("index_topbanner_unfold");
                o.html("展开<s></s>");
                i.slideUp();
                a.slideDown();
                if (g) {
                    c.animate({
                        "padding-top": l + "px"
                    })
                }
            }
        },
        5000);
        m.click(function() {
            clearInterval(n)
        })
    };
    h.addClass("index_topbanner_unfold");
    k(h);
});
</script><!-- 顶部横幅广告 E -->
