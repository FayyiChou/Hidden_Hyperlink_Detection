          <script language="javascript">
              function loginCheck(form) {
                  if (form.user_name.value == "") {
                      form.user_name.focus();
                      return false;
                  }
                  if (form.password.value == "") {
                      form.password.focus();
                      return false;
                  }
                  return true;
              }
          </script>

