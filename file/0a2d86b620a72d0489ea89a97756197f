$(document).ready(function(){
	$(".menu ul li").hover(
	  function(){
		$(".menu ul li").removeClass("curr");
		$(this).addClass("curr");
		$(".menu ul li .next_menu").css("display", "none");
		$(this).children(".next_menu").css("display", "block");
		$(".m-xilie").css("display", "none");
		var thisid = $(this).attr("data");
		$("#" + thisid).slideDown(300);
	  },
	  function(){
		var thisid = $(this).attr("data");
		$(".m-xilie").hover(
			function(){
				$(".menu ul li").removeClass("curr");
				$("li[data='" + thisid + "']").addClass("curr");
			},
			function(){
				$("#" + thisid).css("display", "none");
				
				$(".menu ul li").removeClass("curr");
				$(".menu ul li.this").addClass("curr");
				$(".menu ul li .next_menu").css("display", "none");
				$(".menu ul li.curr .next_menu").css("display", "block");	
			}
		);
		$(".menu ul li").removeClass("curr");
		$(".menu ul li.this").addClass("curr");
		$(".menu ul li .next_menu").css("display", "none");
		$(".menu ul li.curr .next_menu").css("display", "block");		
	  }
	);	
	
});
</script>  
