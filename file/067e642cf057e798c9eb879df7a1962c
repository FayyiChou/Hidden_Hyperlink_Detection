//小地图显示和隐藏
$('#showmap').on('click', function(){
    var smallMap = document.getElementById('map');
	createpoint();
});
//创建小地图js
function createpoint (){
	var mp = new BMap.Map("smallMap");
	var point = new BMap.Point(pointx, pointy);
	mp.centerAndZoom(point, 15);
	// 复杂的自定义覆盖物
	function ComplexCustomOverlay(point, text) {
		this._point = point;
		this._text = text;
	}
	ComplexCustomOverlay.prototype = new BMap.Overlay();
	ComplexCustomOverlay.prototype.initialize = function(map) {
		this._map = map;
		var div = this._div = document.createElement("div");
		div.style.position = "absolute";
		div.style.zIndex = BMap.Overlay.getZIndex(this._point.lat);
		div.style.background = "url(/themes/new2013/files/community/images/mouseout.png) no-repeat";
		div.style.color = "#fff";
		div.style.height = "40px";
		div.style.width = "30px";
		div.style.padding = "2px";
		div.style.lineHeight = "30px";
		div.style.whiteSpace = "nowrap";
		div.style.MozUserSelect = "none";
		div.style.fontSize = "12px";
		div.style.cursor = "-moz-grab";
		mp.getPanes().labelPane.appendChild(div);
		return div;
	};
	ComplexCustomOverlay.prototype.draw = function() {
		var map = this._map;
		var pixel = map.pointToOverlayPixel(this._point);
		this._div.style.left = pixel.x - 15 + "px";
		this._div.style.top = pixel.y - 20 + "px";
	};
	var myCompOverlay = new ComplexCustomOverlay(point);
	mp.addOverlay(myCompOverlay);
}


$(window).scroll(function(e){
	changefnav();		
})
function changefnav(){
	if(window.chrome){
		elements=document.body;
	}else{
		elements=document.documentElement;
	}
	var top=elements.scrollTop;
	var offset = $("#scroller-top").offset();
	var listtop = offset.top; 
	if(top > listtop){
		$('ul.f-nav').attr('id','f-nav');
        $('.detail-menu').css('margin-top', '0px');
        $('#f-nav').css('background', 'white');
	}else{
		$('ul.f-nav').attr('id','');
		//if (subView == ''){
		//	document.getElementById("first_navon").click();
		//}
	}
}
$('.f-nav li').on('click', function(){
	$(this).prevAll().removeClass("now");
	$(this).nextAll().removeClass("now");
	$(this).addClass("now");
});
$(document).ready( function() {
	//当访问链接有锚点时，更改锚点状态
	var intro = location.hash;
	if (intro != '') {
		$(intro.replace('#intro_','#menu')).click();
	}
});
//获取小区成交数据
function getDeals(communityId, page){
    $.ajax({
	type:"GET",
	url:"/exchange/getdeals",
	dataType:"html",
	data:{'communityId':communityId, 'page':page, 'communityname':communityAddress},
	success: function(data){
	    $('#dealinfo').html(data);
	}
    });
}
    
//获取小区评论数据
function getComments(communityId, page){
    $.ajax({
	type:"GET",
        url:"/nJ_Community/getcomments",
	dataType:"html",
	data:{'communityId':communityId, 'page':page, 'communityname':communityAddress},
	success: function(data){
	    $('#commentinfo').html(data);
	}
    });
}

//--------------------------------soso街景--------------------------------------
function showmap(index){
        //是否有街景 【1：有街景】 【2：无街景】
        var isHaveStreetscape = arguments[1] ? arguments[1] : 1; 
        if(isHaveStreetscape == 2){
                $(".showmap3, .showmap2").hide();
        }
	if(index==1){
                $(".showmap1").css('color','#ff6000');
                $(".showmap2").css('color','#000');
		$('#div_baidu').show();
		$('#div_soso').hide();
                initBigMap("bigmap", point, '公交');
	}else{
                $(".showmap2").css('color','#ff6000');
                $(".showmap1").css('color','#000');                
                $('#div_baidu').hide();
		$('#div_soso').show();
	}
	return false;
}
//先按小区名字找，找不到再按坐标找
var communityAddress = "阿尔卡迪亚（C区，E区，F区）";
if(communityAddress!=''){
	showPanoByCommunityName(communityAddress);
}else{
	showByPosition();
}
function showPanoByCommunityName(){
	var poi;
    //Step-2 街景吸附：得到POI半径200米内最近的街景场景
    var getPanoInfo=function(){
        //创建街景服务对象，用于吸附得到传入POI的街景
        //详细手册参见：http://api.map.soso.com/doc_v2/ApiGuide.html#PanoramaService
        var ps = new soso.maps.PanoramaService(); //创建实例
        //吸附：poi坐标为中心，半径150米内最近街景，完成后回调：setPanoramalabel
        ps.getPano(poi.latLng,150,setPanoramalabel); 
    }
    //Step-3 创建街景，视角转向POI，并在街景中标注POI名称
    var setPanoramalabel=function(Result){
        //判断吸附结果是否有街景
        if(Result!=null){
            //Step-3.1 创建街景
            var pano = new soso.maps.Panorama(document.getElementById('pano_holder'), {
                pano: Result.svid  // svid就是panoId，SOSOAPI取名不一致
            });
            //Step-3.2 在街景中标注
            ////Step-3.4 调整街景视角，面向标注的Label，
            //以下算法有兴趣的朋友可以研究一下，算出来的就是 街景场景坐标 面向 POI坐标 的偏航角(heading)
            var x1 = Result.latlng.lng , y1 = Result.latlng.lat; //街景场景所在位置坐标
            var x2 = poi.latLng.lng , y2 = poi.latLng.lat;      //搜索结果第一项的位置坐标
            var alpha = Math.acos((y2 - y1) / Math.sqrt(Math.pow(x2 - x1, 2) + Math.pow(y2 - y1, 2)));
            if (x2 - x1 < 0){
                alpha = Math.PI * 2 - alpha;
            }
            var heading=alpha/Math.PI*180;
            //设置视角面向POI
            pano.setPov({heading : heading, pitch : 0});
        }else{
        	showByPosition();
        	return;
        }
    }
    //Step-1 程序入口：
    //在SOSO地图中，搜索
    //使用SearchService(检索服务)，首先配置检索选项
    var SearchOptions={
        //搜索范围
        location : "南京",
        //搜索完成后回调， results为搜索结果
        //本节主讲街景，关于SearchService的具体细节可参看http://api.map.soso.com
        complete : function(results){
            //获取检索结果的POI数组
            var pois = results.detail.pois;
            if(pois==null){
            	showByPosition();
            	return;
            }
            if(pois.length>0){//判断是否有搜索结果
                //本示例取搜索结果第一项进行标注，仅为说明使用方法
                poi=pois[0];
                getPanoInfo(); //见Step-2：吸附获取搜索结果的街景
            }else{
            	showByPosition();
            	return;
            }
        },
        error:function(){showByPosition();}
    };
    //创建检索服务实例，并将搜索设置传入其构造方法
    var ss = new soso.maps.SearchService(SearchOptions);
    ss.search(communityAddress); //执行搜索
}
function showByPosition(){
	//转换百度坐标为soso坐标
    var baiduLat=0;
    var baiduLng=0;
    soso.maps.convertor.translate(new soso.maps.LatLng(baiduLat,baiduLng), 3, function(res){
    	var point = res[0];
    });
    var pano = new soso.maps.Panorama(document.getElementById('pano_holder'));
    var pano_service = new soso.maps.PanoramaService();
    var radius=200;
    pano_service.getPano(point, radius, function (result){
        if(result==null){
        	hidePano_holder();
                showmap(1,2);
        	return;
        }
        var x1 = result.latlng.lng;
        var y1 = result.latlng.lat;
        
        var x2 = point.lng;
        var y2 = point.lat;

        var alpha = Math.acos((y2 - y1) / Math.sqrt(Math.pow(x2 - x1, 2) + Math.pow(y2 - y1, 2)));
		if (x2 - x1 < 0) {
			alpha = Math.PI * 2 - alpha;
		}
	     // 最后显示所有的属性
        pano.setPano(result.svid);
		pano.setPov({heading : alpha/Math.PI*180, pitch : 0});
    });
}
function hidePano_holder(){
	$('#pano_holder').hide();
	$('#pano_noImg').show();
}
//--------------------------------soso街景--------------------------------------
    //选择城市弹出层
    $('#city').click(function (event) {
        event.stopPropagation();
        var offset = $(event.target).offset();
        $('#cityMore').toggle('slow');
    });
    $(document).click(function (event) { $('#cityMore').slideUp('slow') });
    $('#cityMore').click(function (event) { $(this).fadeOut(500) });
</script>

