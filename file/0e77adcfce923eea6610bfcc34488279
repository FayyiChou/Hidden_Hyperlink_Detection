				#hs-colophon #hs-top-footer .hs-footer5{margin-top: 51px;}
				#hs-colophon .menu>.menu-item .menu-image-title{font-size: 15.5px; line-height: 31px;}
				#hs-colophon .menu .sub-menu .menu-item .menu-image-title{line-height: 27.6px;}
				#hs-colophon #hs-bottom-footer{height: auto;}
				#hs-colophon #hs-bottom-footer .hs-copy-right{font-size: 11px; margin-bottom: 0px;}
				html[lang="en-US"] #hs-colophon #hs-top-footer{padding-right: 10%}
				html[lang="en-US"] #hs-colophon #hs-top-footer .hs-footer5{margin-top: 32px;}
			}
