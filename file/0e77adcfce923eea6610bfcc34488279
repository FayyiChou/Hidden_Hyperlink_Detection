    <script type="text/javascript">
        //设置banner图ul宽度
        var count = $(".banner .slide").length;
        $(".banner ul").css("width", count * 100 + '%').find("li").css("width", 100 / count + "%");

        //设置banner图的宽度
        changeBannerImg();
        setBannerHeight();
        function setBannerHeight() {
            if ($(window).width() <= 414) {
                var bHeight = $(".banner").width() * 0.61;
                $(".banner").height(bHeight);
            }else if($(window).width() > 414){
                var bHeight = $(".banner").width() * 0.3472;
                $(".banner").height(bHeight);
            }
        }
        window.onresize = function () {
            setBannerHeight();
            changeBannerImg();
        }
        //设置手机pcbanner图片
        function changeBannerImg() {
            if ($(window).width() <= 414) {
                $(".banner .slide img").each(function (index) {
                    $(this).prop("src", $(this).attr("data-phone-src"));
                })
            } else {
                $(".banner .slide img").each(function (index) {
                    $(this).prop("src", $(this).attr("data-pc-src"));
                })
            }
        }
        var index = 0;
        $('.nav .nav-right .nav-item').on("click", function () {
            index = $(this).index();
            move();
        })
        function move() {
            var w = $('.banner ul li').width();
            $(".banner ul").animate({"left": -(index - 1) * w}, 200);

            var _that = $(".nav .nav-right").children("div").eq(index);
            if (!_that.hasClass('active')) {
                _that.addClass('active').siblings().removeClass('active');
            }
        }

        //轮播自动播放
        var time = 3000;  //轮播时间
        var timer = null;
        play();
        function play() {
            timer = setInterval(function () {
                index++;
                if (index - 1 >= $(".banner .slide").length) {
                    index = 1;
                }
                move();
            }, time);
        }

        $(".banner").on("mouseover", function () {
            clearInterval(timer);
        })
        $(".banner").on("mouseout", function () {
            play();
        })
    </script>
    
	</div><!-- #content -->

