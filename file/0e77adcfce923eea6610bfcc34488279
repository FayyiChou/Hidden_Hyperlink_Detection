    <script type="text/javascript">
        $(document).ready(function () {
            checkFadeIn();
            $("input").on("keydown",function(e){
                var evt = e||event;
                if(evt.keyCode==13){
                    var temp = window.location.origin;
                    window.location.href = temp+$(this).attr("data-src")+$(this).val();
                }
            })
            $(window).on("scroll", function () {
                checkFadeIn();
            });
        });
        function checkFadeIn() {
            if (($(".custom-container .news").offset().top - $(window).height() > 0 && $(".custom-container .news").offset().top - $(window).height() < $(window).scrollTop()) || $(".custom-container .news").offset().top - $(window).height() < 0) {
                $('.custom-container .news .item').addClass("animated fadeInUp");
            }
            if (($(".custom-container .apply .content ").offset().top - $(window).height() > 0 && $(".custom-container .apply .content ").offset().top - $(window).height() < $(window).scrollTop()) || $(".custom-container .apply .content ").offset().top - $(window).height() < 0) {
                $('.custom-container .apply .content >div').addClass("animated fadeInUp");

            }
            if (($(".custom-container .apply .apply-list ul ").offset().top - $(window).height() > 0 && $(".custom-container .apply .apply-list ul ").offset().top - $(window).height() < $(window).scrollTop()) || $(".custom-container .apply .apply-list ul ").offset().top - $(window).height() < 0) {
                $('.custom-container .apply .apply-list ul li').addClass("animated fadeInUp");
            }
        }
    </script>
