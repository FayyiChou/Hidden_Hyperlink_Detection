<script type="text/javascript">

    function setVisibility(city) {
        $('.overlay-item').not('.' + city).hide();
        $('.overlay-item.' + city).show();
        $('.office-area').not('#' + city).attr('class', 'office-area');
        $('.office-area#' + city).attr('class', 'office-area office-area-active');
    }

    $(document).ready(function () {

        setVisibility('london');

        $('.office-area').on('mouseover', function (event) {
            setVisibility(event.currentTarget.id);
        });
    });

</script>

