function showHidePost(elem) {
var d_elem = jQuery(elem),
d_hidePost = d_elem.parent().parent().prev().children(),
d_navNo = d_elem.parent().siblings(".nav-no");
d_hidePost.slideDown();

//显示分页
if(d_navNo.length!=0){
d_navNo.show();
}
d_elem.parent().hide();
return false;
}

function getMoreComment(obj, tid, pid, total_page) {
if(typeof pages[pid] == 'undefined') {
pages[pid] = 2;
}
$.ajax({
type: "GET",
url: "forum.php",
data: "mod=misc&action=commentmore&tid="+tid+"&pid="+pid + "&page="+pages[pid]+"&inajax=1",
success: function(msg){
$("#comment_"+pid).append(msg);
if(pages[pid]>=total_page) {
$(obj).parent().siblings().hide();
$(obj).parent().html('已全部加载');
}else {
pages[pid]+=1;
$("#commentpage_"+pid).click(function() {
getMoreComment(obj, tid, pid, total_page)
});
}
}
});
}

$(document).ready(function() {
 $(".post_reply").live("click",function() {
var uid =  0;
if(!uid) {
udblogin();
return false;
} else {
var msg = $(this).parents('.pop-inner').find('.post_reply_msg').val();
if(msg.length < 5) {
alert("发帖内容不能少于5个字符");
return false;
} 
$(this).parents('.fastpostform').submit();
}
});
 	 $('#favbtn').on('click', function() {
var obj = $(this);
$.ajax({
type:'POST',
url:obj.attr('href') + '&handlekey=favbtn&inajax=1',
data:{'favoritesubmit':'true', 'formhash':'0121e9e6'},
dataType:'xml',
})
.success(function(s) {
popup.open(s.lastChild.firstChild.nodeValue);
evalscript(s.lastChild.firstChild.nodeValue);
})
.error(function() {
window.location.href = obj.attr('href');
popup.close();
});
return false;
});
})
