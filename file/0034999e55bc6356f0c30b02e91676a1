 <script type="text/javascript">
   //首页专家团队
	/*$('.itemPro li').mouseover(function(){
				$(this).children('.docl').stop(0,1).hide(0).parent('li').siblings('li').children('.docl').stop(0,1).show(0);
				$(this).stop(0,1).animate({'width':'328px'},0).siblings('li').stop(0,1).animate({'width':'208px'},0);
	});*/
    	$('.itemPro li').mouseover(function(){
		var obj = $(this);
		var index = $(this).index();
		//首先先移除所有节点的on事件
		$('.itemPro li').each(function(){
			var i = $(this).index();
			$(this).removeClass('on_' + i);
		});
		obj.addClass('on_' + index);
	});

    //安全保障
