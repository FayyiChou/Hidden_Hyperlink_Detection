    <script type="text/javascript">
        var entityId = 0;
        var entityType = 0;
        $(document).ready(function () {
            var $smartView=$("#"+'view_collect_10006_268760501' );
            $(".w-collect-tips").hide();
            $(".w-collect-bg").hide();
            $("body").append($(".collect-dialog").html());
            $("body").append($(".collect-background").html());
            $smartView.find(".collect-dialog").remove();
            $smartView.find(".collect-background").remove();
            $smartView.find(".w-collect-link").click(function () {
                if(entityType==1)
                {
                    AjaxLoginCheck.callback="InsertProductCollect";
                    AjaxLoginCheck.checkLogin();
                }
                else if(entityType==2)
                {
                    AjaxLoginCheck.callback="InsertNewsCollect";
                    AjaxLoginCheck.checkLogin();
                }
            });

            $(".w-collect-close").click(function () {
                $(".w-collect-tips").fadeOut();
                $("#success_tip").fadeOut();
                $("#error_tip").fadeOut();
                $(".w-collect-bg").fadeOut();
            });

            $(".w-collect-bg").click(function () {
                $(".w-collect-tips").fadeOut();
                $("#success_tip").fadeOut();
                $("#error_tip").fadeOut();
                $(".w-collect-bg").fadeOut();
            });

        });

        function showCollectMsg(error,errorMsg,redirect)
        {
            if(errorMsg=='exist'){
                $("#error_tip").fadeIn();
                $("#errorMsg").html('您已收藏');
            }
            else if (errorMsg == "unlogin") {
                location.href = redirect;
                return;
            }
            else if (error==0) {
                $("#success_tip").fadeIn();
            }
            else {
                $("#error_tip").fadeIn();
                $("#errorMsg").html(errorMsg);
            }
            $(".w-collect-bg").fadeIn();
        }
        function InsertNewsCollect()
        {
            $.ajax({
                url: "/Plugins/Collect/InsertCollect",
                dataType: "json",
                data: { EntityId: entityId, EntityType: entityType },
                type: 'post',
                success: function (data) {
                    if(data.success==true){
                        showCollectMsg(0,data.message);
                    }
                    else{
                        showCollectMsg(1,data.message,data.redirect);
                    }
                }
            });
        }
        function InsertProductCollect()
        {
            var numbername = $(".p_summary_amount .buy_num").attr("name");
            var productattributechecked = $("input[id^=product_attribute]:checked");
            var productattribute = $("input[id^=product_attribute]");
            var attributecount = 0;
            var attributename = ",";
            $.each(productattribute, function (i, o) {
                if (attributename.indexOf("," + $(this).attr("name") + ",") < 0) {
                    attributename += $(this).attr("name") + ",";
                    attributecount += 1;
                }
            })

            if (productattributechecked.length < attributecount) {
                showCollectMsg(1,"<p>收藏失败</p><p>"+'请选择产品属性'+"</p>");
                return false;
            } else {
                var data = eval("({" + numbername + ":" + $(".p_summary_amount .buy_num").val() + "})");
                $.each(productattributechecked, function (i, o) {
                    data = $.extend(data, eval("({" + $(this).attr("name") + ":" + $(this).val() + "})"));
                })

                $.ajax({
                    url:'/addproductvarianttocart//2',
                    type:"post",
                    dataType: "json",
                    data:data,
                    success:function(response){
                        if (response.success == true) {
                            if(response.message.indexOf("该产品已添加到收藏夹")>=0||
                               response.message.indexOf("The product has been added to your shopping cart")>=0)
                                showCollectMsg(0);
                            else showCollectMsg(1,response.message);
                        }
                        else showCollectMsg(1,response.message,response.redirect);
                    }
                });
            }
        }
    </script>
</div>
