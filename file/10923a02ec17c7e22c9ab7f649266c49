<script type="text/javascript" >

function changeItem(id) {
  if (id==1) {
   document.getElementById("y01").style.backgroundImage='url(images/tongzhi_4.gif)';
   document.getElementById("t01").style.display = "";
   document.getElementById("y02").style.backgroundImage='url(images/tongzhi_6.gif)';
   document.getElementById("t02").style.display = "none";
  }else{
   document.getElementById("y01").style.backgroundImage='url(images/tongzhi_6.gif)';
   document.getElementById("t01").style.display = "none";
   document.getElementById("y02").style.backgroundImage='url(images/tongzhi_4.gif)';
   document.getElementById("t02").style.display = "";
  }
}


function changeItem1(id) {
  tempArr = document.getElementById("itemNav1").getElementsByTagName("a"); 
  tId = id.substring(1,id.length);
  for (i=0;i<tempArr.length;i++) {
    tempId = tempArr[i].id.substring(1,tempArr[i].id.length);
    document.getElementById("y"+tempId).className = "";
    document.getElementById("t"+tempId).style.display = "none";
  }

    document.getElementById("y"+tId).className = "selItem";
    document.getElementById("t"+tId).style.display = "";
}

function changeItem2(id) {
  tempArr = document.getElementById("itemNav2").getElementsByTagName("a");
  
  tId = id.substring(2,id.length);
  for (i=0;i<tempArr.length;i++) {
    tempId = tempArr[i].id.substring(2,tempArr[i].id.length);
    document.getElementById("yy"+tempId).className = "";
    document.getElementById("tt"+tempId).style.display = "none";
  }

    document.getElementById("yy"+tId).className = "selItem";
    document.getElementById("tt"+tId).style.display = "";
}
function changeItem3(id) {
  tempArr = document.getElementById("itemNav3").getElementsByTagName("a");
  
  tId = id.substring(3,id.length);
  for (i=0;i<tempArr.length;i++) {
    tempId = tempArr[i].id.substring(3,tempArr[i].id.length);
    document.getElementById("yyy"+tempId).className = "";
    document.getElementById("ttt"+tempId).style.display = "none";
  }

    document.getElementById("yyy"+tId).className = "selItem";
    document.getElementById("ttt"+tId).style.display = "";
}

