				function jsPriceFormat(_number){
					BX.Currency.setCurrencyFormat("RUB", {'CURRENCY':'RUB','LID':'ru','FORMAT_STRING':'# руб.','FULL_NAME':'Рубль','DEC_POINT':'.','THOUSANDS_SEP':' ','DECIMALS':'2','THOUSANDS_VARIANT':'S','HIDE_ZERO':'N','CREATED_BY':'','DATE_CREATE':'','MODIFIED_BY':'1','TIMESTAMP_X':''});  
					return BX.Currency.currencyFormat(_number, "RUB", true); 
				}
