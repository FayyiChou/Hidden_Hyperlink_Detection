<script type="text/javascript">
    $(function () {
        // dialog box settings
        $(".cookieBanner").dialog({
            height: 450,
            width: 450,
            modal: true,
			dialogClass: "cookieModal",
            closeOnEscape: false,
            title: 'Use of Cookies',
            open: function (event, ui) { $(".ui-dialog-titlebar-close").hide(); }
        });

        // close dialog and set cookie if 'Close' is clicked on
        $('.closeCookieBanner').click(function (evt) {
            evt.preventDefault();
            CloseOverlayAndSetCookie();
        });

        // open Manage Hertz Cookies link in ColorBox and close this dialog and set a cookie
        $('.manageCookieLink').click(function (evt) {
            evt.preventDefault();
            CloseOverlayAndSetCookie();

            // call color box
            $(this).colorbox({
                width: '850px',
                height: '95%',
                iframe: 'true',
                onClosed: function () { location.reload(true); }
            });
        });

        // update the expiry date of cookie if found
        if ($.cookie('CookieLaw') != null) {
            $.cookie('CookieLaw', 'AcceptSiteCookies', { expires: 7 });
        }

        function CloseOverlayAndSetCookie() {
            // set cookkie
            $.cookie('CookieLaw', 'AcceptSiteCookies', { expires: 7 });

            // close dialog
            $(".ui-dialog-titlebar-close").trigger('click');
        }
    });
</script>

