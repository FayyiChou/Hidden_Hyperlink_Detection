<script> 
var speed=50 
demo02.innerHTML=demo01.innerHTML 
demo00.scrollLeft=demo00.scrollWidth 
function Marquee(){ 
if(demo00.scrollLeft<=0) 
demo00.scrollLeft+=demo02.offsetWidth 
else{ 
demo00.scrollLeft-- 
} 
} 
var MyMar=setInterval(Marquee,speed) 
demo00.onmouseover=function() {clearInterval(MyMar)} 
demo00.onmouseout=function() {MyMar=setInterval(Marquee,speed)} 
</script>
	
