function openAndclose(){ 
	if(document.getElementById("mapsele").style.marginRight == "-260px"){
	    document.getElementById("mapsele").style.marginRight = "0";		    
	    	
	    document.getElementById("roundmap_map").style.width = "696px";
	    
	    document.getElementById("msclose").style.backgroundPosition = "left";
	} else{
	    document.getElementById("mapsele").style.marginRight ="-260px";
	    
	    document.getElementById("roundmap_map").style.width = "956px";	
	    
	    document.getElementById("msclose").style.backgroundPosition = "right";
	}
}
function openurl(url){
    open(url);
}
function openMapLp(index){
    Search.hideDiv('mapTab');
    var lps = getElementsByClassName(document,"div","map_lp");
    lps[0].style.display = 'none';
    lps[1].style.display = 'block';
    Search.showDiv('bus_ipt','traffic_result');
    Search.removeOtherOverlays();
    if('1' == index){
        gid('traffic_title').innerHTML="公交";
        gid('traffic_type').innerHTML="1";
    }else{
        gid('traffic_title').innerHTML="驾车";
        gid('traffic_type').innerHTML="";
    }
}

function onbus(x,y,title,newCode,tag,type){
    Search.searchdrive['sname'] = title;
    Search.searchdrive['x'] = x;
    Search.searchdrive['y'] = y;
    if('e' == tag){
        gid('sstartname').value = gid('start'+newCode).value;
        gid('sendname').value = title;
    }else{
        gid('sstartname').value = title;
        gid('sendname').value = gid('end'+newCode).value;
    }
    openMapLp(type);
    gid('traffic_type').innerHTML = type;
    Search.searchway();
}
function slidebd(newcode,index,cls){
    var i = 1;
    while(gid(newcode+'hd'+i)){
        gid(newcode+'hd'+i).className=cls;
        gid(newcode+'bd'+i).style.display='none';
        i++;
    }
    gid(newcode+'hd'+index).className='on '+cls;
    gid(newcode+'bd'+index).style.display='block';
}
    
	   
</script>
  
  </div>
</div>
