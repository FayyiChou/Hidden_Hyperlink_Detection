	var _TUAN_BASEURL = ""+"/loupan/tuan/save/";
	function vald() {
		var n = jQuery.trim(jQuery("#name1").val());
		var t = jQuery.trim(jQuery("#tel1").val());
		if (n.replace(/^\s+|\s+$/g, "") == "") {
			alert("请输入您的姓名");
			jQuery("#name1").focus();
			return false
		} else if (n.length > 20) {
			var C = "姓名字数最多20个！";
			alert(C);
			jQuery("#name1").focus();
			return false
	} else if (t.replace(/^\s+|\s+$/g, "") == "") {
			alert("请输入您的电话");
			jQuery("#tel1").focus();
			return false;
		} else if (t.length > 20) {
			var C = "电话字数最多20个！";
			alert(C);
			jQuery("#tel1").focus();
			return false
	} 
	var build_id = jQuery("#bid1").val();
 var K = "tel=" + t+ "&name=" + encodeURIComponent(n)+"&rs=" + jQuery("#rs1").val() +"&build_id="+ build_id;
			
K = encodeURI(K);
jQuery.ajax({ 
    url:_TUAN_BASEURL, //后台处理程序
    type:'post',         //数据发送方式
    dataType:'html',     //接受数据格式
    data:K,         //要传递的数据
		 success: function(json){
		var B = json;
			 if(B > 0 ) {
					alert("恭喜你，报名成功！");
					jQuery("#name1").val("");
					jQuery("#tel1").val("");
				} else {
					alert("出现异常！");
				}								
		}
  });
}
	
	function f3() {
		var n = jQuery.trim(jQuery("#name3").val());
		var t = jQuery.trim(jQuery("#tel3").val());
		if (n.replace(/^\s+|\s+$/g, "") == "") {
			alert("请输入您的姓名");
			jQuery("#name3").focus();
			return false
		} else if (n.length > 20) {
			var C = "姓名字数最多20个！";
			alert(C);
			jQuery("#name3").focus();
			return false
	} else if (t.replace(/^\s+|\s+$/g, "") == "") {
			alert("请输入您的电话");
			jQuery("#tel3").focus();
			return false;
		} else if (t.length > 20) {
			var C = "电话字数最多20个！";
			alert(C);
			jQuery("#tel3").focus();
			return false
	} 
	var build_id = jQuery("#bid1").val();
 var K = "tel=" + t+ "&name=" + encodeURIComponent(n)+"&rs=" + 1 +"&build_id="+ build_id;
			
K = encodeURI(K);
jQuery.ajax({ 
    url:_TUAN_BASEURL, //后台处理程序
    type:'post',         //数据发送方式
    dataType:'html',     //接受数据格式
    data:K,         //要传递的数据
		 success: function(json){
		var B = json;
