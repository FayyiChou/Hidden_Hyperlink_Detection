var BoxMarginTop = jQuery('#Float').offset().top;
jQuery(window).scroll(function(){
	if( jQuery(window).scrollTop() > BoxMarginTop && !jQuery("#Float").hasClass("filter-stick")){
		jQuery('#Float').addClass('filter-stick');
		jQuery("#dianshang").removeClass("ds2"); 
    	jQuery("#dianshang").addClass("ds1");
	}else if( jQuery(window).scrollTop() <= BoxMarginTop && jQuery("#Float").hasClass("filter-stick")){
		jQuery('#Float').removeClass('filter-stick');
		jQuery('#Float').show();
		jQuery("#dianshang").removeClass("ds1"); 
    	jQuery("#dianshang").addClass("ds2");
		
	}
	});

var arg = 1;
var c1 = 0;
var c2 = 0;
// 控制条件
function hideorshowmore(){
    if(arg  == 0){
    	c1 = 1;
    	c2 = 0;
    	arg = 1;
       	 jQuery("#dianshang").fadeIn("slow"); 
    	 jQuery("#bbb").removeClass("b2"); 
        	jQuery("#bbb").addClass("b1"); 
        	jQuery("#FloatNav").removeClass("navdown"); 
        	jQuery("#FloatNav").addClass("navup");
    } else {
    	 c2 = 1;
    	 c1 = 0;
    	arg = 0;//
    	 jQuery("#dianshang").slideUp("slow"); 
       	jQuery("#bbb").removeClass("b1"); 
       	jQuery("#bbb").addClass("b2");
    	jQuery("#FloatNav").removeClass("navup"); 
    	jQuery("#FloatNav").addClass("navdown");
    }
} 


