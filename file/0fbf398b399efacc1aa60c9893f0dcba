jQuery(".btn2").hover(
  function () {
	  var id = jQuery(this).attr('id');
	  jQuery("#tslider").hide();
	  jQuery("#tslider1").hide();
	  jQuery("#tslider2").hide();
	  jQuery("#tslider3").hide();
	  jQuery("#t"+id).show();
	  jQuery("#t"+id).responsiveSlides({
		    auto: true,
		    pager: false,
		    nav: true,
		    speed: 1000,
		    // 对应外层div的class : slide_container
		    namespace: "slide"
		    });
  },
  function () {
	  jQuery(this).removeClass("hover");
  }
);
</script>
