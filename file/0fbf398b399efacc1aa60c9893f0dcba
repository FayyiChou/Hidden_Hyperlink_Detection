window.onscroll = function () {  
    var top = document.documentElement.scrollTop || document.body.scrollTop;  
    if(top >600){
    	jQuery("#jiantou").fadeIn("slow"); 
    }else{
    	jQuery("#jiantou").slideUp("slow"); 
    		
    	
    }
};


function top1(){
	window.scrollTo(0,0);
}
