	new function() {
	    var searchType = '1';
	    Element.observe(document, 'SearchBar:OptionList', function(event) {
	        searchType = event.memo;
	    });
	    new newhouse.global.search.SearchSuggestion('keyword_apf_id_6', '/loupan/search/', {
	        onParameters: function(entry) {
			
	           var params = entry.toQueryParams();
	           // params= encodeURIComponent(encodeURIComponent(params));
	           // alert(params.k);
	            if (searchType) {
	                params.c = '18';
	               // params.k = (encodeURI(params.k));
	                params.t = searchType;
	                params.vr = 1;
	            }
				//alert( Object.toQueryString(params));
	            return ("kw="+params.kw);
	        },
	        formid: 'apf_id_6_form'
	    });
	}
	

</script>



