<script type="text/javascript"><!--
	var loupanid= 1862;
	
	// close window
	function closeLeft(cid){
		jQuery("#pdetail_"+cid).hide();
	}
	
	// open window
	function openLeft(cid){
		jQuery(".maptck").hide();
		jQuery("#pdetail_"+cid).show();
	}
	
	
