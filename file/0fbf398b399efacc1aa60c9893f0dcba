	function showdong(){
	    var l = arguments.length;
	    if(l>0){
	        var showid = arguments[0];
	        for(i=1;i<=ds_layouttotal;i++){
	            if(i==showid){
	                if(document.getElementById("xklist"+showid).style.display=='none'){
	                    document.getElementById("xklist"+showid).style.display='';//显示
	                    document.getElementById("imgcur"+showid).src='/images/navupicon.gif';//显示
	                }else{
					    closeIt();			
	                    document.getElementById("xklist"+i).style.display='none';
	                    document.getElementById("imgcur"+i).src='/images/navovericon.gif';//显示
	                }
	            }else{
				    closeIt();
	                document.getElementById("xklist"+i).style.display='none';
	                document.getElementById("imgcur"+i).src='/images/navovericon.gif';//显示
	            }
	        }    
	    }
	}
	
