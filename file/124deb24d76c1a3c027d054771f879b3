	  <script>
var $jqplus=jQuery.noConflict();

function makeprovince(provar,formid){
	//document.getElementById('pro').innerHTML	=	provar;
	$jqplus("#"+formid+" select[name='province']").html(provar);
}
function makecity(cityvar,formid){
	//document.getElementById('city').innerHTML	=	cityvar;
	$jqplus("#"+formid+" select[name='city']").html(cityvar);
}
function maketown(townvar,formid){
	//document.getElementById('town').innerHTML	=	townvar;
	$jqplus("#"+formid+" select[name='town']").html(townvar);
}
//
function newinfo(formid){
	//
	modplus(formid);
	//
	var pro		=	$jqplus("#"+formid+" select[name='province']").val();
	var city	=	$jqplus("#"+formid+" select[name='city']").val();
	var town	=	$jqplus("#"+formid+" select[name='town']").val();
	var url = "http://xuexiao.eol.cn/ajax/liandongjsonpgb.php?"+"pro="+pro+"&city="+city+"&town="+town+"&formid="+formid;
	//alert(url);
	//var url = "http://eol.cn/ticker.php?"+"pro="+pro+"&city="+city+"&town="+town;
	$jqplus.getScript(url);
}


function checkplus(formid){
	if($jqplus("#"+formid+" input[name='schoolname']").val()	==	"请输入学校名称"){
		$jqplus("#"+formid+" input[name='schoolname']").val("");
	}
	$jqplus("#"+formid).submit();
}
function checkval(formid){
	if($jqplus("#"+formid+" input[name='schoolname']").val()	==	"请输入学校名称"){
		$jqplus("#"+formid+" input[name='schoolname']").val("");
	}
}
function modplus(formid){
	//
	if($jqplus("#"+formid+" select[name='schooltype']").val()	==	"44"){
		var plusstr	=	"<option value='0'>功能选择</option><option value='1'>寄宿</option><option value='2'>实验班</option><option value='3'>择校</option><option value='4'>示范校</option>";
	}else if($jqplus("#"+formid+" select[name='schooltype']").val()	==	"42" ||　$jqplus("#"+formid+" select[name='schooltype']").val()	==	"43"){
		var plusstr	=	"<option value='0'>功能选择</option><option value='1'>寄宿</option>";
	}else{
		var plusstr	=	"<option value='0'>功能选择</option>";
	}
	$jqplus("#"+formid+" select[name='sfunc']").html(plusstr);
}

//newinfo('mst');
//newinfo('mstxx');
//newinfo('mstxx');

function checkmail()
			{
				if (!document.ordermail.email.value.match(/^.*@([\w\-]+\.)+[a-z]{2,4}$/ig))
				{
					document.ordermail.email.focus();
					alert("请正确填写邮件地址.");
					return false;
				} else {
					return true;
				}
			}
			function orderweekly(o)
			{
				document.ordermail.mod.value = (o==1)?'weekly_order':'weekly_cancel';
			}
</script>
 

