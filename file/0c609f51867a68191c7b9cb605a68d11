    function report() {
        Behavioural.collectModel = { 'vwz.projectid': houseInfo.projcode, 'vwz.houseid': houseInfo.houseid, 'vwg.page': 'zfpageagent' }
        Behavioural.report(Behavioural.collectModel);
    }
    function sendToHouse(phone) {
        if (typeof (Behavioural) !== "undefined") {
            Behavioural.collectModel = { 'vwz.houseid': houseInfo.houseid, 'vwz.projectid': houseInfo.projcode, 'vwz.phone': phone, 'vwg.page': 'zfpageagent' }
            Behavioural.sendToHouse(Behavioural.collectModel);
        }
    }

    var imagecount = '13';
    var arr = [];
    var arr1 = [];
    for (var i = 0; i < imagecount; i++) {
        arr.push("xixi-" + (i + 1));
        if (i > 5) {
            arr1.push("xixi-" + (i - 5));
        }
    }
    if (arr.length > 0) {
        new slider({ offset: 5000, target: arr, displayCount: 4 });
