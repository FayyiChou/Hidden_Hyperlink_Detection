<link href="http://esf.js.soufunimg.com/esf/zu/css/pop.min.css?v=1334125553511" rel="stylesheet">
<script type="text/javascript">
    var luckybag = {
        path: "/RentDetails/PostService/IsGotLuckBag.aspx",
        time: 60,
        timer: {},
        cookieluckbagID: "",
        /*
      *设置Cookie 
      */
        SetCookie: function (cookieName, cookieValue) {
            var date = new Date();
            var year = date.getFullYear();
            var month = date.getMonth() + 1;
            var day = date.getDate();

            var newdate = year + "/" + month + "/" + day;

            var expiredate = new Date(new Date(newdate).getTime() + 24 * 60 * 60 * 1000 - 1);


            document.cookie = cookieName + "=" + cookieValue + ";expires=" + expiredate + ";path=/;domain=fang.com";
        },
        /*
        * 读取指定的Cookie值   
        */

        GetCookie: function (name) {
            var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
            if (arr = document.cookie.match(reg))
                return (arr[2]);
            else
                return null;
        },
        //数据异步提交
        PostData: function () {
            $.ajax({
                type: "POST",
                url: luckybag.path,
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                data: { type: "GetBag" },
                dataType: "json",
                success: function (datas) {
                    if (datas.result == "1") {

                        var LuckHtml = document.createElement("div");
                        LuckHtml.className = "fuddiv";
                        LuckHtml.innerHTML = "<p></p>";
                        var fuddivcon = document.createElement("div");
                        fuddivcon.className = "fuddivcon";
                        fuddivcon.innerHTML = "<a class=\"close\" href=\"javascript:;\" onclick='javascript:$(\".fuddiv\").hide();'><img src=\"http://esf.js.soufunimg.com/esf/zu/image/huodong/x_btn1.png\" alt=\"\"></a>";
                        fuddivcon.innerHTML += "<div class=\"contit\">房天下的福袋</div>";

                        if (datas.Clist && datas.Clist.LCoupon.length > 0) {
                            for (var i = 0; i < datas.Clist.LCoupon.length; i++) {
                                var item = datas.Clist.LCoupon[i];
                                if (item.type == "二手房") {
                                    var no3 = document.createElement("div");
                                    no3.className = "fdyhq no3";
                                    no3.innerHTML = "<h3><span style=\"display:\">" + item.type + "佣金抵用券</span></h3>";
                                    no3.innerHTML += "<div class=\"num\"><em>￥</em>" + item.money + "</div>";
                                    no3.innerHTML += "<div class=\"info\">已放入您的账户</div>";
                                    fuddivcon.appendChild(no3);
                                }
                                if (item.type == "租房") {
                                    var no2 = document.createElement("div");
                                    no2.className = "fdyhq no2";

                                    no2.innerHTML = "<h3><span style=\"display:\">" + item.type + "租金抵用券</span></h3>";
                                    no2.innerHTML += "<div class=\"num\"><em>￥</em>" + item.money + "</div>";
                                    no2.innerHTML += "<div class=\"info\">已放入您的账户</div>";
                                    fuddivcon.appendChild(no2);
                                }
                                if (item.type == "积分") {
                                    var fdyhq = document.createElement("div");
                                    fdyhq.className = "fdyhq";

                                    fdyhq.innerHTML = "<h3><span style=\"display:none\">" + item.type + "佣金抵用券</span></h3>";
                                    fdyhq.innerHTML += "<div class=\"num\"><i></i>" + item.money + "</div>";
                                    fdyhq.innerHTML += "<div class=\"info\">已放入您的账户</div>";
                                    fuddivcon.appendChild(fdyhq);
                                }

                            }
                            var rmore = document.createElement("div");
                            rmore.className = "rmore";
                            rmore.innerHTML += "<a  href=\"javascript:;\" onclick=\"luckybag.BagUrl()\">查看优惠券&gt;&gt;</a>";
                            fuddivcon.appendChild(rmore);
                        }
                        LuckHtml.appendChild(fuddivcon);
                        document.body.appendChild(LuckHtml);
                        luckybag.SetCookie("luckybag_got_" + userid, "true");
                    }
                    else {
                        luckybag.ReadBag();
                    }
                }
            })

        },
        //是否领取过
        IsGotLuckBagData: function () {
            $(".bottom_fu").hide();
            var isgot = luckybag.GetCookie("luckybag_got_" + userid);
            if (!isgot) {
                $.ajax({
                    type: "POST",
                    url: luckybag.path,
                    contentType: "application/x-www-form-urlencoded; charset=utf-8",
                    data: { type: "IsGot" },
                    dataType: "json",
                    success: function (datas) {
                        if (datas.result == "0") {
                            luckybag.PostData();
                        }
                        else if (datas.result == "1") {
                            luckybag.ReadBag();
                            luckybag.SetCookie("luckybag_got_" + userid, "true");
                        }
                        else {
                            luckybag.ReadBag();
                        }
                    }
                })
            }
            else {
                luckybag.ReadBag();
            }
        },
        ReadBag: function (str) {
            var LuckHtml = document.createElement("div");
            LuckHtml.className = "fuddiv";
            LuckHtml.innerHTML = "<p></p>";
            var fuddivcon = document.createElement("div");
            fuddivcon.className = "fuddivcon";
            fuddivcon.innerHTML += "<a onclick='javascript:$(\".fuddiv\").hide();' class=\"close\" href=\"javascript:;\"><img src=\"http://esf.js.soufunimg.com/esf/zu/image/huodong/x_btn1.png\" alt=\"\"></a>";
            fuddivcon.innerHTML += "<div class=\"alreadyre\">今天已经领完了，明天再来吧~</div>";
            LuckHtml.appendChild(fuddivcon);
            document.body.appendChild(LuckHtml);
        },
        BagUrl: function () {
            var url = "http://my.fang.com/MyCoupons/Index.html?city=" + houseInfo.shortDb;
            window.location.href = url;
        },
        //点击红包
        ClickLuckBag: function () {

            $(".bottom_fu").hide();
            if (!userid) {
                var isgot = luckybag.GetCookie("luckybag_got_" + userid);

                var LuckHtml = document.createElement("div");
                LuckHtml.className = "fuddiv";
                LuckHtml.innerHTML = "<p></p>";
                var fudindiv = document.createElement("div");
                fudindiv.className = "fudindiv";
                fudindiv.innerHTML += "<a onclick='javascript:$(\".fuddiv\").hide();' class=\"close05\" href=\"javascript:;\"><img src=\"http://esf.js.soufunimg.com/esf/zu/image/huodong/close_x.png\" width=\"20\" height=\"20\" alt=\"\"></a>";
                fudindiv.innerHTML += "<div class=\"intit\">房天下给您发来一个新春福袋</div>";
                fudindiv.innerHTML += "<div style=\"width:300px; overflow:hidden\"><input id=\"phone\" maxlength=\"11\" onblur='luckybag.TelB()' class=\"fd_input1\" type=\"text\" value=\"\"></div>";
                fudindiv.innerHTML += "<div style=\"width:300px; overflow:hidden\"><a onclick='luckybag.GetCode()' class=\"btn_hqyzm\" href=\"javascript:;\" id=\"getcode\">获取验证码</a><input id=\"codeid\" maxlength=\"4\" class=\"fd_input1 fd_input2\" type=\"text\" value=\"\"></div>";
                //fudindiv.innerHTML += "";
                //fudindiv.innerHTML += "";
                //fudindiv.innerHTML +="";
                fudindiv.innerHTML += "<p id=\"tis\" style=\"display:none\" class=\"tis\">您已领取过</p>";
                fudindiv.innerHTML += " <a onclick='luckybag.checkCode()' class=\"btn_dkfd\" href=\"javascript:;\">打开福袋</a>";
                LuckHtml.appendChild(fudindiv);
                document.body.appendChild(LuckHtml);
            }
            else {
                this.IsGotLuckBagData();
            }
        },
        CloseLuckBag: function () {
            $(".bottom_fu").hide();
            luckybag.SetCookie("luckybag_close", "true");
        },
        TelB: function () {
            var phone = document.getElementById("phone");
            var isgot = luckybag.GetCookie("luckybag_got_" + phone.value);
            if (isgot) {
                $("#tis").fadeIn("fast").html("您已经领取过");
                document.getElementById('getcode').disabled = true;
            }
            else {
                var reg = /^(13[0-9]|14[0-9]|15[0-9]|17[0-9]|18[0-9]|95013)\d{8}$/;

                if (!reg.test(phone.value)) {
                    $("#tis").fadeIn("fast").html("请输入正确的手机号");
                    document.getElementById('getcode').disabled = false;
                }
                else {
                    $.ajax({
                        type: "POST",
                        url: luckybag.path,
                        contentType: "application/x-www-form-urlencoded; charset=utf-8",
                        data: { type: "IsGotByPhone", phone: phone.value },
                        dataType: "json",
                        success: function (datas) {
                            if (datas.result == "1") {
                                $("#tis").fadeIn("fast").html("您已经领取过");
                                luckybag.cookieluckbagID = datas.useridByphone;
                                luckybag.SetCookie("luckybag_got_" + datas.useridByphone, "true");
                                luckybag.SetCookie("luckybag_got_" + phone.value, "true");
                                document.getElementById('getcode').disabled = true;
                            }
                            else {
                                document.getElementById('getcode').disabled = false;
                                $("#tis").fadeIn("fast").html("");
                            }
                        }
                    })
                }
            }
        },
        GetCode: function () {

            if (document.getElementById('getcode').disabled) {
                return false;
            }
            var reg = /^(13[0-9]|14[0-9]|15[0-9]|17[0-9]|18[0-9]|95013)\d{8}$/;
            var phone = document.getElementById("phone");
            if (!reg.test(phone.value)) {
                $("#tis").fadeIn("fast").html("请输入正确的手机号");
            }
            else {
                $("#tis").hide();
                smsLogin.send(phone.value, "", function (data) {
                    if (data.Message == "Success") {
                        luckybag.timer = setInterval('luckybag.autoCount()', 1000);
                    } else if (data.Message == "Error" && data.IsShowMathCode == "true") {

                    } else {
                        $("#tis").fadeIn("fast").html(data.Tip);
                    }
                });
            }
        },
        autoCount: function () {
            luckybag.time--;
            if (luckybag.time > -1) {
                $("#getcode").text(luckybag.time + "秒后再获取");
            } else {
                clearInterval(luckybag.timer);
                luckybag.time = 60;
                $("#getcode").text("获取验证码");
            }
        },
        checkCode: function () {

            var phone = document.getElementById("phone");
            if (phone.value != "") {
                var code = $("#codeid").val().replace(/^\s+|\s+$/g, "");
                if (code != "") {
                    smsLogin.verify(phone.value, code, function (data) {
                        if (data.Message == 'Success') {

                            $(".fuddiv").hide();
                            //异步访问获取userid
                            if (!userid) {
                                jQuery.ajax({
                                    type: 'get',
                                    url: '/RentDetails/PostService/DefaultUserLogin.aspx',
                                    contentType: 'application/json',
                                    dataType: 'json',
                                    cache: false,
                                    async: true
                                }).done(function (data) {
                                    window.userid = data.userid;
                                    window.username = data.username;
                                    changeHtml("login", data);
                                    setLoginTopBar(data);
                                    luckybag.IsGotLuckBagData();
                                })
                            }
                            else {
                                luckybag.IsGotLuckBagData();
                            }
                        } else {
                            $("#tis").fadeIn("fast").html(data.Tip);
                            //if (luckybag.time > -1) {
                            //    clearInterval(luckybag.timer);
                            //    luckybag.time = 60;
                            //    $("#getcode").text("获取验证码");
                            //}
                        }
                    })
                }
                else {
                    $("#tis").fadeIn("fast").html("请输入正确的验证码");
                }
            }
            else {
                $("#tis").fadeIn("fast").html("请输入正确的手机号");
            }

        },

        //显示福袋logo
        ShowBagHtml: function () {
            var LuckHtml = document.createElement("div");
            LuckHtml.className = "bottom_fu";
            LuckHtml.innerHTML = "<p></p>";
            var bf_inner = document.createElement("div");
            bf_inner.className = "bf_inner";
            bf_inner.innerHTML += "<a class=\"moving_fu\" href=\"javascript:;\" onclick='luckybag.ClickLuckBag()'><img src=\"http://esf.js.soufunimg.com/esf/zu/image/huodong/moving_fu.gif\" alt=\"\"></a>";
            bf_inner.innerHTML += "<a href=\"javascript:;\" onclick='luckybag.CloseLuckBag()'><img class=\"x_btn2\" src=\"http://esf.js.soufunimg.com/esf/zu/image/huodong/x_btn2.png\" alt=\"\"></a>";
            LuckHtml.appendChild(bf_inner);
            document.body.appendChild(LuckHtml);
        }

    }
    $(function () {

        //1.是否领取过 luckybag_got
        //2.当天是否关闭福袋弹框 luckybag_close
        //3.当天浏览页面的次数 luckybag_count
        //var isgot = luckybag.GetCookie("luckybag_got_"+userid);

        jQuery.post(luckybag.path, { type: "HuodongOn" }, function (data) {

            if (data && data.result == 100 && data.item.open.toUpperCase() == "TRUE") {
                var itemcitys = '上海';
                var allcitys = data.item.citys;
                var isclose = luckybag.GetCookie("luckybag_close");
                var bag_count = luckybag.GetCookie("luckybag_count");

                if (allcitys == "全国" || allcitys.indexOf(itemcitys) > -1) {
                    if (!isclose) {
                        if (parseInt(bag_count) == 3) {
                            //显示
                            if (userid) {
                                luckybag.cookieluckbagID = userid;
                                var isgot = luckybag.GetCookie("luckybag_got_" + luckybag.cookieluckbagID);
                                if (!isgot) {

                                    $.ajax({
                                        type: "POST",
                                        url: luckybag.path,
                                        contentType: "application/x-www-form-urlencoded; charset=utf-8",
                                        data: { type: "IsGot" },
                                        dataType: "json",
                                        success: function (datas) {
                                            if (datas.result == "1") {
                                                luckybag.SetCookie("luckybag_got_" + userid, "true");
                                            }
                                            else {
                                                luckybag.ShowBagHtml();
                                            }
                                        }
                                    })
                                }
                            }
                            else {
                                luckybag.ShowBagHtml();
                            }
                        }
                        else {
                            bag_count = parseInt(bag_count * 1) + 1;
                            if (bag_count > 4) {
                                bag_count = 4;
                            }
                            luckybag.SetCookie("luckybag_count", bag_count);
                        }
                    }
                }
            }
        }, "json");
    })

</script>





