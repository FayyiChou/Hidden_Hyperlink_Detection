<script type="text/javascript">
    (function (h) {
        var imageHits = new HouseAdHitsImage();
        imageHits.create(h.agentIds, h.houseid, h.shortDb, h.isNorth, 'acz');

        var mallInfo = imageHits.queryString("mall");
        var mallType = "";
        var mallNum = "";
        var mallSource = "";
        var url = "";
        var isNorth = h.isNorth;
        var shortCity = h.shortDb;
        var agentId = h.agentIds;
        var houseID = h.houseid;
        var mallHouse = (function () {
            try {
                if (mallInfo != null) {
                    mallType = mallInfo.split(',')[0];
                    mallNum = mallInfo.split(',')[1];
                    if (mallInfo.split(',').length == 3) {
                        mallSource = mallInfo.split(',')[2];
                    }
                    if (mallNum != null && mallType != null) {
                        if (isNorth == "Y") {
                            url = "http://clickstoren.esf.soufun.com/agent_";
                        }
                        else {
                            url = "http://clickstores.esf.soufun.com/agent_";
                        }
                        url += agentId + "_" + houseID + "_" + shortCity + "_cz_" + mallType + "_" + mallNum + (mallSource == "" ? "" : "_" + mallSource) + ".gif";
                        var img = document.createElement("img");
                        img.src = url;
                        document.getElementById("mallHouseDiv").appendChild(img);
                    }
                }
            }
            catch (err) { }
        }())
    }(houseInfo))
</script>
