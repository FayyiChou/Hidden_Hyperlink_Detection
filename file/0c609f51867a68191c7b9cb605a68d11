<script type="text/javascript" src="http://map.qq.com/api/js?v=2.exp&amp;key=d84d6d83e0e51e481e50454ccbe8986b"></script><script src="http://open.map.qq.com/apifiles/2/4/53/main.js" type="text/javascript"></script>
<script type="text/javascript">
    (function ($) {
        var liArr = $("#ul_map li");
        var divArr = $("#zf_Map_anchor  .map-wrap"); // 紧邻ul的两个div
        var mapsoso = document.getElementById("mapsoso");//soso地图
        liArr.each(function (index) {
            $(this).on({
                "click": function () {
                    toggle(index);
                }
            })

        })

        function toggle(index) {
            // 重置状态
            divArr.hide();
            liArr.removeClass();
            // 给相应的div 、li设置属性
            divArr[index].style.display = "";
            liArr[index].className = "on";

        }
        var loadStreeView = (function () {
            var isshowmap = 'True',
               param = '10021030140220114752800|11|0|1'.split('|');
            if (isshowmap === 'True') {
                if ($.trim(mapsoso.innerHTML) === "") {
                    if (param[0] !== "") {
                        //街景容器
                        var heading = 0;
                        var Interval;

                        var pano = new soso.maps.Panorama(mapsoso, {
                            pano: param[0], //场景ID
                            pov: { //视角
                                heading: param[1] === "" ? 0 : parseInt(param[1], 10), //偏航角
                                pitch: param[2] === "" ? 0 : parseInt(param[2], 10) //俯仰解
                            },
                            zoom: param[3] === "" ? 1 : parseInt(param[3], 10) //缩放
                        })

                        qq.maps.event.addListener(pano, 'pov_changed', function () {
                            var pov = pano.getPov();
                            if (Math.abs(pov.heading - heading) > 1) { clearInterval(Interval) }
                        })

                        qq.maps.event.addListener(pano, 'pano_changed', function () { clearInterval(Interval) })

                        Interval = setInterval(function () {
                            heading += 0.1; if (heading >= 360) heading = 0;
                            pano.setPov({ heading: heading });
                        }, 100);



                    }
                }
            }
        }())
    }(jQuery))
</script>




        
