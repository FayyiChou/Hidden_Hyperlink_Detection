<script type="text/javascript">
    (function (window, undefined) {
        var Im = {
            //初始化需要参数{username: "", //登录用户请传通行证用户名，非登录不传showname: "", //显示用 city: "北京"//城市必填}
            initarr: {},
            //初始化IM
            onload: function () {

                window.fangWebIM.init(Im.initarr);
            },
            click: function (contactId, showname, city, loupan, avatar, tel) {
                window.fangWebIM.openChat({
                    "type": 'agent', //新房为xf，二手房为agent，家居为home
                    "contactId": contactId,
                    "showname": showname,
                    "city": city,
                    "pname": 0,
                    "tip": "我正在浏览您的店铺",
                    "loupan": loupan,
                    "avatar": avatar,
                    "tel": tel
                });


            },
            Agentname: {}//经纪人用户名集合 多个用引文逗号隔开
        }
        window.Im = Im;
    }(window))


    window.onload = function () {

        jQuery.post("/RentDetails/PostService/GetUserInfo.aspx", {}, function (data) {
            if (data) {
                //data = eval(data);
                window.userid = data.userid;
                window.username = data.username;
                Im.initarr = {
                    type: "agent",
                    userid: data.userid, //登录用户请传通行证用户ID，非登录不传
                    username: data.username, //登录用户请传通行证用户名，非登录不传
                    showname: "", //显示用
                    city: "上海"//城市必填
                }
                Im.onload();
            }
        },"json")
        //脚本加载经纪人是否在线 开始

        jQuery.post("/rentsearch/PostService/ImIsOnline.aspx", { username: escape(agentlist) }, function (data) {

            if (data) {
                var dom = jQuery("a[id^=im_]"),
                    length = dom.length,
                    onlinearr = data.split(',');
                if (length > 0) {

                    if (imtype === "detail") { //详情
                        jQuery(dom).each(function () {
                            var online = jQuery(this).find("img[name$=online]");
                            var offline = jQuery(this).find("img[name$=offline]");
                            switch (onlinearr[0]) {
                                case "1":
                                    online.show();
                                    offline.hide();
                                    break;
                                case "0":
                                    online.hide();
                                    offline.show();
                                    break;
                                case "-1":
                                    online.hide();
                                    offline.hide();
                                    break;
                                default:
                                    break;

                            }
                        })

                    } else {

                        for (var i = 0; i < length; i++) {//列表
                            if (onlinearr[i]) {
                                var online = jQuery(dom[i]).find("img[name$=online]");
                                var offline = jQuery(dom[i]).find("img[name$=offline]");
                                switch (onlinearr[i]) {
                                    case "1":
                                        online.show();
                                        offline.hide();
                                        break;
                                    case "0":
                                        online.hide();
                                        offline.show();
                                        break;
                                    case "-1":
                                        online.hide();
                                        offline.hide();
                                        break;
                                    default: break;
                                }
                            }
                        }
                    }
                }
            }
        })
        //脚本加载经纪人是否在线 结束
    }
</script>
