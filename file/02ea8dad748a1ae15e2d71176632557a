			  function submitlive()
			  {

				  var myuid = '';
				  if(myuid == '')
				  {
					  alert('请先登录！');
					  return false;
				  }
				  else
				  {
					  //window.open("agline/index.php", "_blank");
					  window.open("/bbcc/new_ag/playgame.php","_blank")
					  return true;
				  }
			  }

