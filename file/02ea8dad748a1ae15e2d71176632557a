			  function load_total(type){
				  var station=$("#station").val();
				  var myuid = '';
				  if(myuid=="" && station==""){
				   station="a"
				  }
				  var vlty="ul[tit='"+type+"']";
				  $(vlty).html("");

				  //$.ajaxSetup({
				  //    async : false
				  //});


				  $.post("../interface/ranking.php?station="+station+"&limit=8&dateType="+type, {}, function(jsonStr)
				  {
					  var insertText="";

					  if(jsonStr !=null){
						  for(var i= 0;i<jsonStr.length;i++){
							  var uname = jsonStr[i]["username"];
							  j=i+1;
							  insertText+="<li>";
							  insertText+="<b>"+j+"</b>";
							  insertText+="<b>"+uname+"</b>";
							  insertText+="<b>"+jsonStr[i]["na"]+"</b>";
							  insertText+="</li>";
						  }

					  }else{
						  insertText+="<li><b style='width:50%;'>&yen;优惠正在统计中！</b></li>";
					  }
					  $(vlty).html(insertText);
				  }, "json");

			  }



			  $(function(){
				  load_total('day');
				  load_total('yester');
				  load_total('month');
				  $("#jin").css("display","");
				  setInterval(lunbopai,5000);
			  });

			  function lunbopai(){
				  if($("#jin").css("display")=='block'){
					  $("#jin").css("display","none");
					  $("#yue").css("display","none");
					  $("#zuo").css("display","block");
					  return;
				  }
				  if($("#zuo").css("display")=='block'){
					  $("#zuo").css("display","none");
					  $("#jin").css("display","none");
					  $("#yue").css("display","block");
					  return;
				  }
				  if($("#yue").css("display")=='block'){
					  $("#yue").css("display","none");
					  $("#zuo").css("display","none");
					  $("#jin").css("display","block");
					  return;
				  }
			  }


