  <script type="text/javascript">
    $(window).scroll(function () {
        if ($(this).scrollTop() > 100) {
            $(".c_meau").stop().show().animate({ bottom: '100px' }, 300);
        }
        else {
            $(".c_meau").stop().animate({ bottom: '-430px' }, 300);
        }
    });
    $('.c_meau .fr_t').click(function () {
        $('body,html').animate({ scrollTop: 0 }, 500);
        $(".c_meau").animate({ bottom: '-380px', opacity: '0' }, 500);
        return false;
    });

	 $(function(){
		//默认状态
		if($(window).width()>768){
			if ($(window).scrollTop()>0) {
				$(".navbar").addClass("add"); 
				$("#logo").attr("src","http://img.sxqswl.net/logo2.png");
			} else {
				$(".navbar").removeClass("add"); 
				$("#logo").attr("src","http://img.sxqswl.net/logo1.png");
			}
			//滚动的时候
			$(window).scroll(function(){
				 if ($(window).scrollTop()>0) {
				  $(".navbar").addClass("add"); 
				  $("#logo").attr("src","http://img.sxqswl.net/logo2.png");
					} else {
					 $(".navbar").removeClass("add"); 
					 $("#logo").attr("src","http://img.sxqswl.net/logo1.png");
				}
			 });	

			} else{
			 if ($(window).scrollTop()>0) {
				  $(".navbar").addClass("add"); 
				  $("#logo").attr("src","http://img.sxqswl.net/logo2.png");
					} else {
					 $(".navbar").addClass("add"); 
					 $("#logo").attr("src","http://img.sxqswl.net/logo1.png");
			}

 $(window).scroll(function(){
     if ($(window).scrollTop()>0) {
      $(".navbar").addClass("add"); 
	  $("#logo").attr("src","http://img.sxqswl.net/logo2.png");
        } else {
		 $(".navbar").addClass("add"); 
		 $("#logo").attr("src","http://img.sxqswl.net/logo1.png");
    }
 
 });	

}
 
 
//窗口改变大小 

$(window).on('resize',function(){
  
 if($(window).width()>768)
{

if ($(window).scrollTop()>0) {
      $(".navbar").addClass("add"); 
        } else {
		 $(".navbar").removeClass("add"); 
}
	
//滚动的时候
$(window).scroll(function(){
     if ($(window).scrollTop()>0) {
      $(".navbar").addClass("add"); 
        } else {
		 $(".navbar").removeClass("add"); 
    }
 
 });	

} else{
 if ($(window).scrollTop()>0) {
      $(".navbar").addClass("add"); 
        } else {
		 $(".navbar").addClass("add"); 
}

 $(window).scroll(function(){
     if ($(window).scrollTop()>0) {
      $(".navbar").addClass("add"); 
        } else {
		 $(".navbar").addClass("add"); 
    }
 
 });	

} 
});  
});
