<script type="text/html" id="social_friends_tpl">
    <% var totalFriends = friends.length;%>
    <% if (totalFriends) { %>
    <form method="post">
    <div class="static-container">
    <h3>Друзья</h3>
    <ul class="frieds-list-fb">
        <% for (var i = 0; i < friends.length; i++) { %>
        <% with (friends[i]) { %>
        <li <%if (false !== isSiteMember) {%>data-userid="<%=isSiteMember.id%>"<%}%>>
            <img src="<%=picture%>" alt="" class="float-left" style="margin-right: 8px;" />
            <a class="name-fb" href="<%=(link ? link : '#')%>"><strong><%=name%></strong></a>
            <%if (isFriend) {%>
                <a class="remove" href="#">Remove from friends</a>
            <%} else if (false !== isSiteMember) {%>
                <a class="invite" href="#">Add to friends</a>
            <%} else {%>
                <a data-socialid="<%=id%>" data-name="<%=name%>" class="fb-invite" href="#">Invite</a>
            <%}%>
        </li>
        <% } %>
        <% } %>
    </ul>
