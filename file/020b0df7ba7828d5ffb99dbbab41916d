        function goTracelList() {
            var arae = document.getElementById("txtDestination").value;
            //var start = document.getElementById("start").value;
            //var end = document.getElementById("end").value;
            if (arae == "目的地") {
                //window.location.href = "TravelList.aspx?StartDate=" + start + "&EndDate=" + end;
                window.location.href = "TravelList.aspx";
            } else {
                //window.location.href = "TravelList.aspx?LineName=" + arae + "&StartDate=" + start + "&EndDate=" + end;
                window.location.href = "TravelList.aspx?LineName=" + arae;
            }
        }
