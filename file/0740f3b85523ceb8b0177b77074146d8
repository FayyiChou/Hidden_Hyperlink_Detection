    function clearbgText(field){
        if (field.defaultValue == field.value)
            field.value = '';
        else if (field.value == '')
            field.value = field.defaultValue;
    }
    function onMail(){
        $j('#receive_radio').val("0");
    }
    function onPost(){
        $j('#receive_radio').val("1");
    }

</script>
    </div>
</div>
