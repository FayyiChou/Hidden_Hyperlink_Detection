<script type="text/javascript">
var oldactiveid = $(".pw a[class='on']").attr('id').replace('pbid_link','');
var oldactiveid1 = oldactiveid;
var nodenow;
var timesetid = 0;
function shownode(test){
	timesetid = 0;
	var thisid = $(nodenow).attr('id').replace('pbid_link','');
	$(".pw a[ableflag=true]").removeClass();
	$(nodenow).addClass('on');
	$("#pbid_ul"+oldactiveid1).hide();
	$("#pbid_ul"+thisid).show();
	oldactiveid1 = thisid;
	if(thisid==oldactiveid)
	{
		$("#ps_cofig").show();
	}
	else
	{
		$("#ps_cofig").hide();
	}
}

$(".pw a[ableflag=true]").bind("mouseenter",function(){
	nodenow = this;
	if(timesetid > 0){
		clearTimeout(timesetid);
		timesetid = 0;
	}
	timesetid = setTimeout(shownode, 300);
});
/* onmouseover时触发的事件暂时去去掉，author:huhw date:2012.5.21
$("#pb_ps_area").bind("mouseleave",function(){
	if(timesetid > 0){
		clearTimeout(timesetid);
		timesetid = 0;
	}
	var thisid = oldactiveid ? oldactiveid : '1';
	$(".pw a[ableflag=true]").removeClass();
	$("#pbid_ul"+oldactiveid1).hide();
	$("#pbid_link"+thisid).addClass('on');
	$("#pbid_ul"+thisid).show();
	oldactiveid1 = thisid;
	$("#ps_cofig").show();
});
*/
$("#price_submit").bind("click",function(){
	var price_1 = parseInt($("#min_p").val());
	var price_2 = parseInt($("#max_p").val());
	var min = 0;
	var max = 0;
	if(!price_1 && price_2)
	{
		min = 0;
		max = price_2;
	}
	else if(price_1 && !price_2)
	{
		min = price_1;
		max = 9999;
	}
	else if(price_1 && price_2)
	{
		if(price_1>price_2)
		{
			min = price_2;
			max = price_1;
		}
		else
		{
			min = price_1;
			max = price_2;
		}
	}
	if(!min && !max)
	{
		alert("请输入自定义价格区间。");
	}
	else
	{
		location.href = "/car/0-0-0-0-0-0-7-0-0-1-0-0/?min="+min+"&max="+max
	}
});

$("input[name='check_config']").bind("click",function(){

	var config = "";
	$("input[name='check_config']").each(function(){
		if($(this).attr("checked")){
			config += $(this).val();
		}
	})

	if(config=="")
	{
		config=0;
	}

	var href = "/car/29-0-0-0-0-0-7-0-"+config+"-1-0-0/#nav";
	location.href = href;
});
$(document).ready(function(){
	/*$(window).bind("scroll resize", function(){
		var scrollTop = $(document).scrollTop();
		var clientHeight = document.documentElement.clientHeight;
		var clientWidth = document.documentElement.clientWidth;
		var left = (clientWidth - (clientWidth - 953)/2);
		var top = scrollTop + Math.floor(clientHeight - 170);
		if(scrollTop > 0){
			$("div#returntop_btn").show();
			$("div#returntop_btn").css({"left":left, "top":top});
		}
		else{
			$("div#returntop_btn").hide();
		}

	});*/
	//58合作
    $.get(
        '/auto/index.php?r=service/heZuoApi/wuBaUsed&city_id='+__ipLocationInfo.city_id+'&pbid=&min=&max&type_id=7&range_id=29',
        {},
        function(html){
            if(html)
            {
                $('#tw_newcar').addClass('tw_newcar').html(html);
            }
        },
        'html'
    );
});
//已选条件 框固定效果yu.jie2015-5-5 更多和收起时高度不一样
function fixSelectCar() {
    var top=$(window).scrollTop();
    var fixcss = 'selectedcar_tit_fix';
    if($("#hid_condition1").is(":hidden")){
        if ($(window).scrollTop()>=656){
            $("#selectedcar_tit").addClass(fixcss);
        }else{
            $("#selectedcar_tit").removeClass(fixcss);
        }
    }else{
        if ($(window).scrollTop()>=480){
            $("#selectedcar_tit").addClass(fixcss);
        }else{

            $("#selectedcar_tit").removeClass(fixcss);
        }
    }
}
$(window).scroll(function(){
    fixSelectCar();
});

</script>
