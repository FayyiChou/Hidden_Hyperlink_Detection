	<script language="javascript">

	function search_car()

	{

		var keywords = document.getElementById('k');

		var keywords = $("#k").val();

		if(keywords == '快速找车')

		{

			$("#k").val("");

			$("#searchForm").action="http://search.xcar.com.cn";

		}

		else

		{

			$("#searchForm").action="http://search.xcar.com.cn/search.php";

		}

		$("#searchForm").submit();

	}

	</script>

