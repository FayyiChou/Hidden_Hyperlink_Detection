			function type_hd(){
				if($("#type_img").attr("src")=="http://icon.xcar.com.cn/2011/header/images/xsh_city.gif"){
					$("#type_img").attr("src","http://icon.xcar.com.cn/2011/header/images/xsh_city2.gif");
