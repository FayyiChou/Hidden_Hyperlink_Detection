$(document).ready(function() {
    App.init();
    $('#reportClick').click(function () {
        $('#fileList').html('');
        $("#reportApp").modal('show');
    });
    initScreenUploader();
    $('#form').submit(function() {
        if ($('#password').val() == '') {
            alert(appInstallPasswordAlert);
            return false;
        }

        saveData();
        return false;
    });

    initView();
});

