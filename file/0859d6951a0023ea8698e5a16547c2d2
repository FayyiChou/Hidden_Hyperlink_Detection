<script type="text/javascript">
function CheckSurveySearch() {
    var searchcategory = jq("#ctl00_search1_ddlsurveysearch").val();
    if (searchcategory=="0")
    {
        alert("请选择调查类别!");
        return false;
    }
    var searchkey = jq("#ctl00_search1_txtsurveysearch").val();
    searchkey = jq.trim(searchkey);
    if (searchkey == "" || searchkey == "请输入标题关键字"){
        alert("请输入标题关键字!");
        return false;
    }
    var sk = searchkey.replace(/:/g, "[xqzwp]");
    //document.location.href = 'http://survey.1diaocha.com/Survey/_SurveySearch_' + searchcategory + '_' + encodeURI(sk) + '.html';
    document.location.href = 'http://survey.1diaocha.com/Survey/_SurveyDefault_' + searchcategory + '_' + encodeURI(sk) + '.html';
    return false;
}

</script>


