var ivyImgAdList = [];
// keys: 关键字, types: ['bottom', 'side'], main:主广告内容, holder:占位图片地址, adLink：点击链接, adId：广告标识
function ivyImgAd(keys, type, main, holder, jsData, adLink, count, adId) {
this.keys = keys.split('|||');
this.main = main;
this.holder = holder == 'null' ? null : holder;
this.jsData = jsData;
this.type = type;
this.adLink = adLink;
this.count = count;
this.adId = adId;
}
