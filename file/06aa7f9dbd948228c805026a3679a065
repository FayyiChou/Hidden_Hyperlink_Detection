function fill_m_ask(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
html+='<li><a href="http://kuaiwen.pcbaby.com.cn/question/'+data[i].questionId+'.html" target="_blank" title="'+data[i].title+'"><p class="tit">'+data[i].title+'</p><p class="des">'+data[i].answerIntro+'...</p></a></li>';
}
document.getElementById("m_ask").innerHTML=html;
}
function fill_b_ask(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
html+='<li><a href="http://kuaiwen.pcbaby.com.cn/question/'+data[i].questionId+'.html" target="_blank" title="'+data[i].title+'"><p class="tit">'+data[i].title+'</p><p class="des">'+data[i].answerIntro+'...</p></a></li>';
}
document.getElementById("b_ask").innerHTML=html;
}
function fill_m_bbs(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
var link = data[i].topicUrl.indexOf("http://")>-1?data[i].topicUrl.replace("http://","https://"):data[i].topicUrl;
html+='<li><a href="'+link+'" target="_blank" title="'+data[i].title+'"><p class="pic"><img src="'+data[i].userFaceUrl+'"/></p><div class="fl"><div class="tit">'+data[i].title+'</div><div class="author">'+data[i].userName+'</div></div></a></li>';
}
document.getElementById("m_bbs").innerHTML=html;
hoverli();
}
function fill_b_bbs(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
var link = data[i].topicUrl.indexOf("http://")>-1?data[i].topicUrl.replace("http://","https://"):data[i].topicUrl;
html+='<li><a href="'+link+'" target="_blank" title="'+data[i].title+'"><p class="pic"><img src="'+data[i].userFaceUrl+'"/></p><div class="fl"><div class="tit">'+data[i].title+'</div><div class="author">'+data[i].userName+'</div></div></a></li>';
}
document.getElementById("b_bbs").innerHTML=html;
hoverli()
}
/*搜索*/
var s_input=pc.getElem("#q");
var form_div=pc.getElem("#s-form");
pc.addEvent(form_div,"click",function(e){
form_div.className='form form-active';
s_input.value='';
e.stopPropagation();
})
pc.addEvent(pc.getElem("body"),"click",function(){
form_div.className='form';          
})
function hoverli(){
/*排行榜li的鼠标经过*/
var rankLi = pc.getElems('.rank li');
pc.each(rankLi, function(li){
pc.addEvent(li,"mouseenter", function(){
this.className="hover";
})
pc.addEvent(li,"mouseleave", function(){
this.className="";
})
});
}
</script>
<script>
var h;
var bol = true;
window.onscroll = function(){
if (bol) {
h = document.body.scrollTop?document.body.scrollTop:document.documentElement.scrollTop;
if (h>document.documentElement.clientHeight) {
pc.getElem(".ad-left").style.display = "block";
}else
{
pc.getElem(".ad-left").style.display = "none";
}
};
}
pc.getElem('.close-ad').onclick = function(){ pc.getElem(".ad-left").style.display = "none"; bol = false}
</script>
