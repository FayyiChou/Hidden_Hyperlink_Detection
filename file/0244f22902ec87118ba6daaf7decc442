<script type="text/javascript">
  // -------------- 二维码 --------------
  $(function () {
	var pageUrl = location.href;
	var domainScope = {
	  'acs.vip.com' : 1,
	  'help.vip.com' : 1,
	  'comm.vip.com/message' : 1
	}
	for (var i in domainScope) {
	  if (pageUrl.indexOf(i) != -1) {
		$('body').append($('#J_comm_code').html());
	  }
	}
  });
</script>



