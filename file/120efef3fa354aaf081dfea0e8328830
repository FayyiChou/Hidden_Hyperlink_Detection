<script>

var new_window = null;

jQuery(function(){

	var url = "http://cas.study.yanxiu.jsyxsq.com/proj/proj/loginUser/get_user_2.json?jsoncallback=checkLogin";

	var parm="";

	var prifun=jQuery("[pricode]").map(function(){return jQuery(this).attr("pricode");}).get();

	if(prifun&&prifun.length>0){//有权限方面的验证请求	

		parm="?ptcode="+static_pri.ptcode+((prifun&&(prifun.length>0))?"&prifun="+(prifun.join("&prifun=")):"");

	}

	url = "http://cas.study.yanxiu.jsyxsq.com/proj/currUser"+parm;

	

	jQuery.getScript(url, function(){

			checkLogin(myprops);

	});

	

	if(getUrlParam('meg') == '0')

	{

		alert("用户名或密码错误！")

	}

});



function checkLogin(data){

	if(data && data!='null'){

		var member = eval(data);

		jQuery("#member_username").html(member.username);

		jQuery("#member_space_notenum").html(member.space_notenum);

		jQuery("#member_space_pmsnum").html(member.space_pmsnum);

		jQuery("#member_space_sum").html(member.space_notenum + member.space_pmsnum);		

		jQuery("#nologin_div").hide();

		jQuery("#login").hide();

		jQuery('.next').css('right',20);

		jQuery("#login_div").show();

		for(i=0;i<member.extInfo.userProjectList.length;i++){

			var up = member.extInfo.userProjectList[i];

			var roleid = member.extInfo.roleid;

			if(typeof(roleid) == "undefined"){

				jQuery("#link_list").append("<li><a target=\"_blank\" href=\"http://cas.study.yanxiu.jsyxsq.com/proj/setProject?pid="+up.project.id+"\">"+up.project.name+"</a></li>");

			}else{

				jQuery("#link_list").append("<li><a target=\"_blank\" href=\"http://cas.study.yanxiu.jsyxsq.com/proj/setProject?pid="+up.project.id+"&roleId="+roleid+"\">"+up.project.name+"</a></li>");

			}			

		}

	}else{

		jQuery("#login_div").hide();

		jQuery("#nologin_div").show();

		jQuery("#login").show();

		jQuery('.next').css('right',350);

	}

}



function getAccessCheckResult(urls) {

	url = "http://cas.study.yanxiu.jsyxsq.com/proj/currUser?access=1&projectId=1943";



	jQuery.getScript(url, function(){

			checkAccess(data,urls);

	});

}



function checkAccess(data,urls){



	if(data && data!='null'){

		var d = eval(data);

		if (d.result == "suc") {

			new_window.location.href = urls;

		} else if (d.result == "sucs") {

			new_window.close();

    		var d = dialog({

                  title: '消息提示',

                  content: '用户权限验证失败，您无法访问此页面！ 原因：您未加入此项目',

                  width: 260,

                  okValue: '确 定',

                  ok: function () {}

    	 	});

    		d.showModal();

		} else {

			new_window.close();

		 	var d = dialog({

                  title: '消息提示',

                  content: '用户权限验证失败，暂时无法访问此页面，请重试！',

                  okValue: '确 定',

                  ok: function () {}

		 	});

   			d.showModal();

		}		

	} else {

		new_window.close();

		var d = dialog({

              title: '消息提示',

              content: '用户权限验证失败，您无法访问此页面！ 原因：您未登录或登录已超时',

              width: 250,

              okValue: '确 定',

              ok: function () {}

	 	});

		d.showModal();

	}

}

</script>

	

