<script>
function repairIframeHeight1(){
	document.getElementById("iframe1").height = $(window.iframe1.document.body).height();
}
function repairIframeHeight2(){
	document.getElementById("iframe2").height = $(window.iframe2.document.body).height();
}
function repairIframeHeight3(){
	document.getElementById("iframe3").height = $(window.iframe3.document.body).height();
}
	
var hasLoad = false;
//滚动条到页面底部加载
$(window).scroll(function () {
    var scrollTop = $(this).scrollTop();
    var scrollHeight = $(document).height();
    var windowHeight = $(this).height();
    if (scrollTop + windowHeight == scrollHeight && !hasLoad) {//第一次滚动到页面底部加载统计表
      	//此处是滚动条到底部时候触发的事件，在这里写要加载的数据，或者是拉动滚动条的操作
		if('1'=='1'){
    		if('false'=='false'){//新项目
				$("#iframe1").show();
				$("#iframe2").show();
        		document.getElementById("iframe1").src="http://cas.study.yanxiu.jsyxsq.com/proj/proj/index/total_subject.htm?projectId=1943&usedTempletId=3";
                document.getElementById("iframe2").src="http://cas.study.yanxiu.jsyxsq.com/proj/proj/index/total_area.htm?projectId=1943&usedTempletId=3";
    		}
			$("#iframe3").show();
			document.getElementById("iframe3").src="http://cas.study.yanxiu.jsyxsq.com/proj/proj/index/total_project.htm?projectId=1943&usedTempletId=3";
		}
		hasLoad = true;//已经加载
    }
});
</script>
