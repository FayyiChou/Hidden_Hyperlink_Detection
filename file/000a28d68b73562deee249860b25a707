	var obj = eval('(' + jsonString + ')');
	if (obj){
		document.getElementById('inputSuccess_3').style.display = 'none';
		document.getElementById('inputFailure_3').style.display = 'none';
		if (obj.isSuccess == 'false'){
			document.getElementById('inputFailure_3').style.display = '';
			document.getElementById('inputFailure_3').innerHTML = obj.message;
		}else{
			document.getElementById('inputSuccess_3').style.display = '';
			document.getElementById('inputSuccess_3').innerHTML = obj.message;
			document.getElementById('inputContainer_3').style.display = 'none';
			setTimeout('window.location.reload(false)', 2000);
		}
	}
}
