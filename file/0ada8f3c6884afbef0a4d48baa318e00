<script type="text/javascript">
    $(function () {
        var gs = "";
        var us = "";
        $("[name='p']").each(function () {
            var id = $(this).attr('g');
            if (gs != '') gs += ',';
            gs += id;
            us = $(this).attr('u');
        });
        Shop.getStoreGoodsPrice(us, gs, '510104', function (data) {
            if (data.Status == 2) {
                $.each(data.PriceList, function (i, item) {
                    var $this = $('[g=' + this.id + ']');
                    $this.html("¥" + item.price);
                });
            }
        });
    });
</script>
