<!--纠错 start-->
<script>
    var tempdata = null;
    var closePop = function () {
        $(".popBox").hide();
        $(".errorMask").hide();
    }
    var openPop = function (id) {
        closePop();
        $("#" + id).show();
        $(".errorMask").show();
    }
    $(".checkError span").click(function () {
        openPop("checkErrorPop");
    });
    $(".popClose,.errorMask").click(function () {
        closePop();
    });
    $(".submitBtn").click(function () {
        $(".submitBtn").hide();
        var content = "";
        $("ul.errorList").children().each(function (index, item) {
            if ($(item).children(0).attr("checked")) {
                content += $(item).children(0).val() + ",";
            }
        });
        if (content.length > 0)content += "；";
        content += $(".errorTxt").children(0).val();
        if (content.length == 0) {
            closePop("checkErrorPop");
            $(".submitBtn").show();
            return;
        }
        $.ajax({
            type: "Post",
            url: "/api/price/PostError",
            data: { specbindid: 285406, content: content, rdm: Math.random() },
            dataType: "json",
            success: function (data) {
                if(data==1){
                    openPop("submitSuccessPop");
                    setTimeout(closePop, 2000);
                    $("ul.errorList").children().each(function (index, item) {
                        if ($(item).children(0).attr("checked")) {
                            $(item).children(0).attr("checked",false);
                        }
                    });
                    $(".errorTxt").children(0).val("");
                    $(".submitBtn").show();
                }
            }
        });
    });
</script>

