  <script>	function login(){
		move();
		$('.room').show();
		$('#verifyImgs').attr('src','http://www.52zjk.cn/Auth/verify/'+new Date().getTime());
		$(window).scroll(move).resize(move);
	}
	function move(){
		var winht = $(window).height();
		var docht = $(document).height();
		var wintop = $(document).scrollTop() + winht / 2 - 150;
		var winleft = $(document).width() / 2 - 250;
		$('.room').css({'height':docht});
		$('.log').css({'top':wintop,'left':winleft});
	}
	function closelog(){
		$('.room').hide();
	}
	$(function(){
		$('#loginform').submit(function(){
			var log_account = $('#log_account').val();
			var log_password = $('#log_password').val();
			var verify = $('#verify').val();
			if (log_account == '')
			{
				layer.msg('用户名不能为空');
				$('#log_account').focus();
				return false;
			}
			if (log_password == '')
			{
				layer.msg('密码不能为空');
				$('#log_password').focus();
				return false;
			}
			if (verify == '')
			{
				layer.msg('验证码不能为空');
				$('#verify').focus();
				return false;
			}
			$.post("/Auth/checklogin","log_account=" + log_account + "&log_password=" + log_password + "&verify=" + verify,function(data){
				if(data == 1 ){
					layer.msg('登陆成功！');
					window.open(document.URL,'_self');
				}else{
					layer.msg(data);
				}
			});
			return false;
		})
	})
	function go_oauths(){
		layer.open({
			type: 2,
			title: '使用QQ账号登陆张垣生活网',
			shadeClose: true,
			shade: 0.8,
			area: ['750px', '466px'],
			content: '/Auth/openlogin/?login=1' //iframe的url
		}); 
		//window.open('/Auth/openlogin/?login=1','_blank');
	}
	function go_oauthsina(){
		layer.open({
			type: 2,
			title: '使用新浪微博账号登陆张垣生活网',
			shadeClose: true,
			shade: 0.8,
			area: ['750px', '466px'],
			content: 'https://api.weibo.com/oauth2/authorize?client_id=3022717560&redirect_uri=http%3A%2F%2Fwww.52zjk.cn%2FAuth%2FcallBack&response_type=code' //iframe的url
		}); 
	}
	function logout(){
		$.post("/Auth/logout",function(data){
			if (data == 0)
			{
				layer.msg('退出成功！');
				window.open('/','_self');
			}else{
				layer.msg('退出失败！或者您还未登陆！')
			}
		});
	}
</script>
