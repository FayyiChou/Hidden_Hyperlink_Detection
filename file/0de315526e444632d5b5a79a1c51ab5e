    <script type="text/javascript">
        //焦点图
        $.fn.extend({
            Focus: function () {
                var _this = $(this);
                var ina_a = _this.find(".ina_focus_nr a");
                if (ina_a.length == 1) {
                    ina_a.eq(0).show();
                }
                else if (ina_a.length > 1) {
                    ina_a.eq(0).show();
                    var n = 0, time;
                    _this.append("<div class='ina_focus_bottom'></div>")
                    for (var i = 0; i < ina_a.length; i++) {
                        if (i == 0) {
                            _this.find(".ina_focus_bottom").append("<a href='javascript:' class='ina_dq'></a>")
                        }
                        else {
                            _this.find(".ina_focus_bottom").append("<a href='javascript:'></a>")
                        }
                    }
                    function ina_focus(m){
                        clearInterval(time);
                        for(var i=0;i<ina_a.length;i++){
                            if(ina_a.eq(i).is(":visible")){
                                if(m!=i){
                                    _this.find(".ina_focus_bottom a").removeClass("ina_dq").eq(m).addClass("ina_dq");
                                    ina_a.eq(m).fadeIn("slow");
                                    ina_a.eq(i).fadeOut("slow");
                                    time=window.setInterval(function(){m++;if(m>=ina_a.length){m=0}ina_focus(m)},5000);
                                    break;
                                }
                            }
                        }
                    }
                    _this.find(".ina_focus_bottom a").mouseover(function () {
                        ina_focus($(this).index());
                    });
                    time = window.setInterval(function () {
                        n++;
                        if (n >= ina_a.length) {
                            n = 0
                        }
                        ina_focus(n)
                    }, 5000)
                }
            }
        });
        $(".ina_focus").Focus();
        //申请车型
        $(".ina_sqcx_but").click(function () {
            $(".ina_sqcx_nr").hide();
            $(".ina_xzpp").css({display: "inline-block"});
        })
        function ina_xzpp(obj) {
            $(".ina_sqcx_nr.ina_xzpp .ina_xzpp_nr dl dd a").remove("ina_dq");
            $(".ina_sqcx_nr").hide();
            $(".ina_xzcx").css({display: "inline-block"});
            if (obj != 'undefined')
            {
                $.get('/ind/cars.php', {sign_id: obj}, function (data) {
                    $("#ina_xzpp_nr_cars").html(data);
                }, 'html');
            }
            next_step(obj);
        }
        function next_step(obj)
        {
            var nextstep = document.getElementById('next-step-value');
            var nextstepvalue = nextstep.value;
            nextstep.value = nextstepvalue + '_' + obj;
        }
        $(".ina_sqcx_nr.ina_xzcx .ina_pre").click(function () {
            document.getElementById('next-step-value').value = '0';
            $("#buy_data0").addClass('ina_dq');
            $(".ina_sqcx_nr").hide();
            clearInterval(0);
            $(".ina_xzpp").css({display: "inline-block"});
        });
        function ina_xzcx(object, obj) {
            $(".ina_sqcx_nr.ina_xzcx .ina_xzpp_nr dl dd a").remove("ina_dq");
            $(".ina_sqcx_nr").hide();
            $(".ina_dztx").css({display: "inline-block"});
            next_step(obj);
            $("#next_remember").html(object).attr("data", object);
            $("#apply_car_ok").html(object).attr("data", object);
            again();
            if($("#appleySendCaptchaBtn").attr('class') !='ina_fsyzm ina_fsyzm_blue'){
                $("#appleySendCaptchaBtn").addClass('ina_fsyzm_blue').html('发送验证码');
                $('#appleySendCaptchaBtn').bind('click',function () {
                    appleySendCaptchaBtnsend();
                });
            }
            $("#buy_data0").addClass('ina_dq');
            $("#next-buy-value").val("0");
            $("span[class='ina_ts']").each(function(i){
                $(this).hide();
            });
        }
        function check_buy_data(obj, data)
        {
            for (var i = 0; i < 5; i++)
            {
                if (data == i)
                {
                    obj.className = 'ina_gmsj ina_dq';
                } else {
                    document.getElementById('buy_data' + i).className = 'ina_gmsj';
                }
            }
            document.getElementById('next-buy-value').value = data;
        }
        $(".ina_sqcx_nr.ina_dztx .ina_pre").click(function () {
            $("#check_buy_li").find('span').each(function(i){
                if($(this).attr('class') == 'ina_gmsj ina_dq')
                {
                    $(this).removeClass('ina_dq');
                }
            });
            var nextstep = document.getElementById('next-step-value');
            var nextstepvalue = nextstep.value.split('_');
            nextstep.value = '0_' + (nextstepvalue[1]);
            $(".ina_sqcx_nr").hide();
            $(".ina_xzcx").css({display: "inline-block"});
        });
        $(".ina_sqcx_nr.ina_sqcg .ina_xzpp_nr ul li .ina_cxsq").click(function () {
            $("#next-step-value").val('0');
            $(".ina_sqcx_nr").hide();
            $(".ina_xzpp").css({display: "inline-block"});
            for (var i = 0; i < 5; i++)
            {
                $("#buy_data" + i).attr('class', 'ina_gmsj');
            }
            $('.ina_text_yzm').val("请填写验证码");
        });

        function loginSendcaptcha() {
            sendSms('#regSendCaptchaBtn', '#mobile', '/ind/send.php', '验证码已经发送到您的手机，请注意查收。');
        }
        function resetSendcaptcha() {
            sendSms('#reset_pwd_btn', '#reset_mobile', '/ind/send.php', '验证码已经发送到您的手机，请注意查收。');
        }
        $('#reset_pwd_btn').click(function () { // 发送验证码
            resetSendcaptcha();
        });
        $('#regSendCaptchaBtn').click(function () { // 发送验证码
            loginSendcaptcha();
        });
        //apply 发送验证码start;
        $('#appleySendCaptchaBtn').click(function () { // apply发送验证码
            appleySendCaptchaBtnsend();
        });
        //apply 发送验证码end;
        function appleySendCaptchaBtnsend()
        {
            sendSms('#appleySendCaptchaBtn', '#apply_mobile', '/ind/send.php', '验证码已经发送到您的手机，请注意查收。');
        }
        $("#appleySendCaptchaOk").click(function () {
            
            if ($("#apply_buier").val() == "" || "请填写真实姓名" == $("#apply_buier").val())
            {
                $("#apply_buier").focus().siblings('span["class=ina_ts"]').last().show().html('<i class="ina_icon ina_error" style="display: block;"></i>请填写购车人真实姓名！');
                return false;
            }
            if ($("#apply_mobile").val() == "" || "请填写手机号码" == $("#apply_mobile").val())
            {
                $("#apply_mobile").focus().siblings('span["class=ina_ts"]').last().show().html('<i class="ina_icon ina_error" style="display: block;"></i>请填写手机号码！');
                return false;
            }
            if ($("#next-buy-value").val() == "")
            {
                alert("请选择购买时间！");
                return false;
            }
            if ($("#next-step-value").val().split('_').length < 3)
            {
                alert("请重新选择申请的车型！");
                return false;
            }
            if($("#check_code_input").val() !=0)
            {
                if ($("#verifycode").val() == "" || "请填写验证码" == $("#verifycode").val())
                {
                    $("#verifycode").focus().siblings('span["class=ina_ts"]').last().show().html('<i class="ina_icon ina_error" style="display: block;"></i>请填写验证码！');;
                    return false;
                }
            }
            if (verifyForm(verifyRules, ['apply_buier', 'apply_mobile'])) {
                $.post('/ind/user.php', $('#apply_sign_form').serialize(), function (data) {
                    if ('undefined' == typeof data.errorCode) {
                        alert('操作失败，服务器错误。');
                    } else {
                        if (data.errorCode == '0') {
                            $(".ina_sqcx_nr").hide();
                            $(".ina_sqcg").css({display: "inline-block"});
                            $("#apply_car_ok").html($("next_remember").attr('data'));
                        } else {
                            if (data.errorCode == 1 && 'undefined' != data.inputErrors && ('手机号不正确' ==data.inputErrors||'不能重复申请！'==data.inputErrors))
                            {
                                $("#apply_mobile").focus().siblings('span["class=ina_ts"]').last().show().html('<i class="ina_icon ina_error" style="display: block;"></i>'+data.inputErrors+'！');
                            }
                            if (data.errorCode == 1 && 'undefined' != data.inputErrors && '验证码不正确，请重新获取！' ==data.inputErrors)
                            {
                                $("#appleySendCaptchaBtnError").show().html('<i class="ina_icon ina_error" style="display: block;"></i>'+data.inputErrors+'！');
                            }
                            if (data.errorCode == 1 && 'undefined' != data.inputErrors && '已经申请过了！请不要在申请了！' ==data.inputErrors)
                            {
                                $("#apply_buier").focus().siblings('span["class=ina_ts"]').last().show().html('<i class="ina_icon ina_error" style="display: block;"></i>此车型，您已经申请过了！请更换。');
                            }
                        }
                    }
                }, 'json');
            }
        });
        $('#regist_btn').click(function () {
            registerSubmit();
        });
        $('#reset_sure').click(function () {
            resetpwdSubmit();
        });
        function registerSubmit() {
            if (verifyForm(verifyRules, ['username', 'mobile', 'pass'])) {
                $.post('/ind/user.php', $('#register').serialize(), function (data) {
                    processData(data);
                }, 'json');
            }
        }
        function resetpwdSubmit() {
            if (verifyForm(verifyRules, ['reset_mobile', 'reset_pwd'])) {
                $.post('/ind/user.php', $('#reset_form').serialize(), function (data) {
                    processData(data);
                }, 'json');
            }
        }
        function processData(data) {
            if ('undefined' == typeof data.errorCode) {
                alert('操作失败，服务器错误。');
            } else {
                if (data.errorCode == '0') {
                    location.href = location.href;
                } else {
                    if ('undefined' != typeof data.inputErrors) {
                        $.each(data.inputErrors, function (inputName, errorMsg) {
                            $('span[class=ina_ts]').hide();
                            var errorSpan = $('#' + inputName).siblings('span[class=ina_ts]');
                            if (data.errorCode != 0)
                            {
                                if (data.errorCode == 3)
                                {
                                    errorSpan.html('<i class="ina_icon ina_error"></i>' + errorMsg + '<a class="ina_szmm" onclick="ina_setmm()" href="javascript:">设置密码</a>');
                                } else {
                                    errorSpan.html('<i class="ina_icon ina_error"></i>' + errorMsg);
                                }
                                errorSpan.show();
                            } else {
                                errorSpan.hide();
                            }
                        });
                    }
                    if ('undefined' != typeof data.error) {
                        alert(data.error);
                    }
                }
            }
        }
        function sendSms(btn, mobileInput, url, successMsg) {
            var sendCBtn = $(btn);
            var mobile = $.trim($(mobileInput).val());
            if ('请填写手机号码' == mobile) {
                $(mobileInput).focus().siblings('span["class=ina_ts"]').last().show().html('<i class="ina_icon ina_error" style="display: block;"></i>手机号码不能为空！');
                return false;
            }
            sendCBtn.addClass("waitC");
            sendCBtn.removeClass("ina_fsyzm_blue");
            waitC(btn);
            $.post(url, {mobile: mobile}, function (data) {
                if ('object' != typeof data) {
                    alert('服务器错误');
                }
                else {
                    if ($.trim(data.errorCode) == '0') {
                        //alert(successMsg);
                       if (btn = '#appleySendCaptchaBtn')
                        {
                            $(btn+'Error').hide();
                        }
                    }
                    if ($.trim(data.errorCode) == 1) {
                        //alert(data.error);
                        if (btn = '#appleySendCaptchaBtn')
                        {
                            $(btn+'Error').show().html('<i class="ina_icon ina_error"></i>'+data.inputErrors+'</span>');
                            //sendCBtn.removeClass("waitC");
                            sendCBtn.removeClass("ina_fsyzm_blue");
                        }
                    }
                }
            }, 'json');
        }
        function waitC(btn) {
            $(".waitC").unbind("click");
            $(".waitC").html("<i id=\"timeC\"></i>&nbsp;秒后重发");
            timeC(60, btn);
            nowC = setInterval("timeC(now, \'" + btn + "\')", 1000);
        }
        function timeC(t, btn) {
            $("#timeC").html(t);
            if (t == 0) {
                clearInterval(nowC);
                $(".waitC").html("发送验证码");
                $(".waitC").addClass("ina_fsyzm_blue");
                $(".waitC").removeClass("waitC");
                again();
                if (btn == '#loginSendPassBtn') {
                    $(btn).click(function () {
                        loginSendpass();
                    });
                }
                if (btn == '#regSendCaptchaBtn') {
                    $(btn).click(function () {
                        loginSendcaptcha();
                    });
                }
                if (btn = '#appleySendCaptchaBtn')
                {
                    $(btn).click(function () {
                        if($("#appleySendCaptchaBtnError").is(":hidden") == true)
                        {
                            $("#appleySendCaptchaBtn").removeClass('ina_fsyzm_blue').addClass('waitC');
                        }
                        appleySendCaptchaBtnsend();
                    });
                }
                if (btn = '#reset_pwd_btn')
                {
                    $(btn).click(function () {
                        resetSendcaptcha();
                    });
                }
                return;
            }
            t--;
            now = t;
        }
        function again() {
            $(".againC").on("click", function () {
                $(this).addClass("waitC");
                $(this).removeClass("againC");
            });
        }
        $(".ina_wjmm").click(function () {
            if ($("#login_user").val() != "")
            {
                $("#reset_mobile").val($("#login_user").val());
            }
            $(".ina_dltk").hide();
            $(".ina_xgzh").show();
            $("#reset_mobile").focus();
        });
        $(".ina_sqcx_nr.ina_sqcg .ina_xzpp_nr ul li .ina_cxsq").click(function () {
            $(".ina_sqcx_nr").hide();
            $(".ina_xzpp").css({display: "inline-block"});
            $(".waitC").html("发送验证码");
            $(".waitC").addClass("againC");
            $(".waitC").removeClass("waitC");
        });
        $("input[id=login_user]").focus(function(){
            $(this).css({color: "#333"});
            $(this).parent().addClass("ina_dq");
        }).blur(function(){
            if(''!= $(this).val())
            {
                $(this).css({color: "#999"});
                $(this).parent().removeClass("ina_dq");
                if($(this).val() != "" && !/^1[3|5|8]\d{9}$/.test($(this).val()))
                {
                    $.post('/ind/user.php',{'verify_step':'check_mobile','mobile':$(this).val(),'veritify_code':$("#reg_session_indelog").val(),'action':'login_check'},function(data){
                        if(data.errorCode == 4 && 'undefined' != typeof data.inputErrors)
                        {
                            $("#login_user_error").show().html('<i class="ina_icon ina_error"></i>已注册未设密码，去<a href="javascript:" onclick="ina_setmm()" class="ina_szmm">'+data.inputErrors.mobile+'</a>');
                            return false;
                        }else if(data.errorCode == 1 && 'undefined' != typeof data.inputErrors)
                        {
                            $("#login_user_error").show().html('<i class="ina_icon ina_error"></i>'+data.inputErrors.mobile);
                            return false;
                        }else{
                            $("form[id=login]").find("span[class=ina_ts]").hide();
                        }
                    },'json');
                }else{
                    $("form[id=login]").find("span[class=ina_ts]").hide();
                }
            }else{
                $(this).css({color: "#999"});
                $(this).parent().removeClass("ina_dq");
            }
        });
        $("input[id=login_pass]").focus(function(){
            $(this).css({color: "#333"});
            $(this).parent().addClass("ina_dq");
        }).blur(function(){
            $(this).css({color: "#999"});
            $(this).parent().removeClass("ina_dq");
        });
        $("input").each(function(i){
        $(this).focus(function(){
            if($(this).val() != '')
            {
                $(this).keydown(function(){
                   $(this).parent().parent().find('span[class=ina_ts]').hide(); 
                });
            }
        });
    });
    </script>

    <script>
        $("input[type=text]").each(function (i) {
            $(this).focus(function () {
                if ($(this).val() != '')
                {
                    $(this).keydown(function () {
                        $(this).parent().parent().find('span[class=ina_ts]').hide();
                    });
                }
            });
        });
    </script>

