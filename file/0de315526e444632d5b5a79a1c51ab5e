    <script>
        //剩余时间
        var time_tool = function (make_id)
        {
            var ina_ts = $(make_id).attr('data') - (new Date());//计算剩余的毫秒数  
            var ina_dd = parseInt(ina_ts / 1000 / 60 / 60 / 24, 10);
            var ina_hh = parseInt(ina_ts / 1000 / 60 / 60 % 24, 10);
            var ina_mm = parseInt(ina_ts / 1000 / 60 % 60, 10);
            if (ina_dd < 10 && ina_dd >= 0) {
                ina_dd = "0" + ina_dd
            }
            if (ina_hh < 10 && ina_hh >= 0) {
                ina_hh = "0" + ina_hh
            }
            if (ina_mm < 10 && ina_mm >= 0) {
                ina_mm = "0" + ina_mm
            }
            if (make_id == '#car_num')
            {
                if (ina_dd <= 0 && ina_hh <= 0 && ina_mm <= 0) {
                    var ina_date = "还有00天00时00分";
                }
                else {
                    var ina_date = "还有" + ina_dd + "天" + ina_hh + "时" + ina_mm + "分";
                }
                $(make_id).html("<i class='ina_icon'></i>" + ina_date);
            } else if (/^(#ian_cx_right_)(\d){1,}/.test(make_id))
            {
                if (ina_dd <= 0 && ina_hh <= 0 && ina_mm <= 0) {
                    var ina_date = "00天00时00分";
                }
                else {
                    var ina_date = ina_dd + "天" + ina_hh + "时" + ina_mm + "分";
                }
                $(make_id).children('strong').html(ina_date);
            } else if (/^(#ian_min_index_)(\d){1,}/.test(make_id))//次推
            {
                if (ina_dd <= 0 && ina_hh <= 0 && ina_mm <= 0) {
                    var ina_date = "00天00时00分";
                }
                else {
                    var ina_date = ina_dd + "天" + ina_hh + "时" + ina_mm + "分";
                }
                $(make_id).html('还有' + ina_date);
            } else if (/^(#ian_main_index_)(\d){1,}/.test(make_id))//主推
            {
                if (ina_dd <= 0 && ina_hh <= 0 && ina_mm <= 0) {
                    var ina_date = "00天00时00分";
                }
                else {
                    var ina_date = ina_dd + "天" + ina_hh + "时" + ina_mm + "分";
                }
                $(make_id).html('还有' + ina_date);
            }
        };
    </script>

