    <script type="text/javascript">
	
		$(function(){
			var myDate = new Date();
			var year_str = myDate.getFullYear();
			$('#year_str').html(year_str);
		})
	
		$(".ina_zhezhao").click(function () {
            $(this).hide();
            $(".ina_dl").hide();
			$('.ina_tk').hide();
        });
		
        //选择城市
        $(".ina_select span").click(function () {
            $(this).toggleClass("ina_dq");
            $(this).siblings(".ina_select_tk").toggle();
        });
        $(document).click(function (e) {
            e = e || window.event;
            var target = e.srcElement || e.target;
            if (($(target).closest(".ina_select_tk").length <= 0) && ($(target).closest(".ina_select span").length <= 0)) {
                $(".ina_select span").removeClass("ina_dq");
                $(".ina_select .ina_select_tk").hide();
            }
        });
        $('.user_del').click(function () {
            logoutSubmit();
        });
        function logoutSubmit()
        {
            //if (window.confirm('你确定要退出吗？')) {
            $.post('/ind/user.php', {verify_step: 'logout'}, function (data) {
                if (data.errorCode == '0') {
                    //alert("退出成功！");
                    location.href = "/";
                }
            }, 'json');
           // }
        }
        var ina_width = document.documentElement.clientWidth || document.body.clientWidth;
        var ina_height = document.documentElement.clientHeight || document.body.clientHeight;
        $(".ina_zhezhao").css({width: ina_width, height: ina_height});
        function ina_setpassword() {
            $(".ina_zhezhao,.ina_setpassword").show(function(){
                $("#pass").focus();
            });
        }
        //登录注册
        function ina_dl() {
            $("form[id=login]").find('span[class=ina_ts]').html('');
            $(".ina_zhezhao,.ina_dltk").show(function(){
                $("#login_user").focus();
            });
        }
        function ina_zhuce() {
            $(".ina_zhezhao,.ina_zhucetk").show();
        }
        $(".ina_dl .ina_bt i").click(function () {
            $(".ina_zhezhao").hide();
            $(this).parent().parent().hide();
            $("form[id=login]").find('span[class=ina_ts]').html('');
            $("form[id=moneies_change_fm]").find("span[class=ina_ts]").hide();
            $("form[id=register_modifypwd]").find("span[class=ina_ts]").html('');
            $("form[id=register_modifytel]").find("span[class=ina_ts]").html('');
            $("form[id=moneies_change_fm]").find("input[type=text]").each(function(i){
                $(this).val($(this).attr('error'));
            });
            $("#login_user").val('');
        });

        $('#login_btn').click(function () {
            loginSubmit();
        });
        function loginSubmit() {
            var verifyIds = ['login_user', 'login_pass'];
            var postData = $('#login').serialize();
            if (verifyForm(loginVerifyRules, verifyIds)) {
                $.post('/ind/user.php', postData, function (data) {
                    processData(data);
                }, 'json');
            }
        }
        function ina_setmm(){
            if($("#login_user").val() != ''){
                if($("#login_user").val() =='手机号')
                {
                    
                }else{
                    if(/^1\d{10}/.test($("#login_user").val()))
                    {
                       $("#reset_mobile").val($("#login_user").val());
                    }
                }
            }
            $(".ina_zhezhao,.ina_xgzh").show();
            $("#reset_mobile").focus();
        }
		
		function toMObilePage(){
			var url = window.location.href;
			var tmpArr = url.split('.');
			if(tmpArr[1] == 'news18a'){
				if(window.location.search){
					var newUrl = 'http://mhuiche.news18a.com' + window.location.pathname + window.location.search+'&mobile=yes';
				}else{
					var newUrl = 'http://mhuiche.news18a.com' + window.location.pathname + window.location.search+'?mobile=yes';
				}
				
			}else{
				if(window.location.search){
					var newUrl = 'http://m.huiche100.com' + window.location.pathname + window.location.search+'&mobile=yes';
				}else{
					var newUrl = 'http://m.huiche100.com' + window.location.pathname + window.location.search+'?mobile=yes';
				}
			}
			window.location.href = newUrl;
		}

		function toPcPage(){
			window.location.href = window.location.href;
		}
		
		


