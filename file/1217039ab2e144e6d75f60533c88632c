    <script type="text/javascript">
        var ThreadSet = {
            'siteid'   : 1,
            'fid'      : 1,
            'tid'      : 7316494,
            'atid'     : 7316494,
            'authorid' : 14194104,
            'url'      : 'http://q.mama.cn/',
            'ptid'     : 30        };

        var topicadminForm;
        var _ptid = 0;
        var _pid = 0;

        //举报
        function reportDialog(ptid, pid) {
            _ptid = ptid;
            _pid = pid;
            topicadminForm = art.dialog({
                content: '正在加载内容... ...',
                title: '举报',
                width: 250,
                top: 250,
                fixed: true,
                padding: 0,
                opacity: 0.4,
                lock: true
            });
            jQuery.get('http://q.mama.cn/load/report', {'ptid': ptid, 'pid': pid}, function (html) {
                jQuery(".aui_content").html(html);
            });
            return false;
        }


        function reasonselect(obj) {
            if (obj.value == 99) {
                jQuery("#reasondiv").show();
            } else {
                jQuery("#reasondiv").hide();
            }
        }
        function reportSubmit(obj) {
            if (obj.form.reasonselector.value == 99 && obj.form.reason.value == '') {
                alert('请输入原因');
                return false;
            }
            jQuery.post("http://q.mama.cn/report/create", jQuery(obj.form).serialize(), function (data) {
                if (data.status >= 0) {
                    //if(_ptid>=0 && _pid>0)
                    //jQuery('#report_' + _ptid + '_' + _pid).html('&nbsp;&nbsp;已举报&nbsp;&nbsp;');
                    alert(data.info);
                    topicadminForm.close();
                }
            });
        }

        /**
         * js楼层跳转，优化SEO
         * @param fid
         * @param tid
         */
        function findPost(fid, tid) {
            if (fid && tid) {
                window.location.href = 'http://q.mama.cn/topic/find/' + fid + '/' + tid + '/';
            }
            return false;
        }

        //复制提示
        jQuery.getScript('//q.mama.cn/web/js/ZeroClipboard.js');
        function setCopy(strUrl) {
            topicadminForm = art.dialog({
                //	content: jQuery('#copyDialog').html(),
                content: '<a id="copy-button" href="#" class="copy-button-txt" data-clipboard-text="' + strUrl + '">点此复制到剪贴板</a>',
                title: '提示信息',
                width: 250,
                top: 250,
                fixed: true,
                padding: 0,
                opacity: 0.4,
                lock: true
            });
            //复制
            var client = new ZeroClipboard(document.getElementById("copy-button"), {
                moviePath: "//q.mama.cn/web/js/ZeroClipboard.swf"
            });

            client.on("load", function (client) {
                client.on("complete", function (client, args) {
                    this.style.display = "none";
                    topicadminForm.close();

                });
            });

            return false;
        }

        jQuery(function () {
            jQuery('.floor').each(function () {
                var floorid = jQuery(this).attr('floorid');
                p = Math.ceil(floorid / MMQW.setting.PrePage);
                jQuery(this).attr('href', '#' + floorid);
            });
        });
    </script>

