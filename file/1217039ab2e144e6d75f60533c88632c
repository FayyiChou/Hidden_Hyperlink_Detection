	var jsonpUrl = "//passport.mama.cn/ssoapi/getCode/?key=mmq_pc&service=http%253A%252F%252Fq.mama.cn%252Ftopic%252F7316494%252F&t=1491090854&sign=4490f008118395a070678a0df6892886";
