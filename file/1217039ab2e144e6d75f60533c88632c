<script>
    jQuery(function(){
        var $ =jQuery;
        $.fn.backTop = function(b) {
            var d = $.extend({
                id: null
            }, b || {});
            function scroll() {
                var domId = "#" + d.id,
                        $dom = $(domId),
                        scrollTop = $(document).scrollTop(),
                        winTop = $(window).height(),
                        offsetTop = scrollTop - winTop;

                (scrollTop > 200) ? $dom.show() : $dom.hide();

                var h = scrollTop - offsetTop + 360;
                $dom.css("bottom", 110);
                if (!window.XMLHttpRequest) {
                    $dom.css("top", e + winTop - 320)
                }
            }
            $("#backTopBtn").click(function() {
                $("html, body").animate({
                    scrollTop: 0
                }, 120)
            });
            $(window).bind("scroll",scroll);
            scroll()
        }

        $("#returnTop").backTop({
            id: "returnTop"
        });
        $(".scan-hover").hover(function() {
            $(this).next().show();
        }, function() {
            $(this).next().hide()
        });
    });
</script>
