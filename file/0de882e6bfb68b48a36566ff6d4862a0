//alert("php");
    $(document.body).addEvent('click', function(event) {
        var evt = event.srcElement ? event.srcElement : event.target;
        if(evt.id == 'peisongdizhi' || $(evt).hasClass('togger_c') ) return;
        else {
            $$('#list1').hide();
        }
    });
	function shoplist()
	{
		//alert($$('#list1').css("display"));
		var aa=document.getElementById("list1").style.display;
		if(aa == "none")
		{
				$$('#list1').show();
		}else
		{
				$$('#list1').hide();
		}
	

	}
		function aa()
	{
		//alert("php");
				$$('#list1').hide();	

	}
	function Getdistrict(id){
		var date = new Date();
		var time = date.getTime();
		var product_id=document.getElementById("product_id").value;  
    var url='/index.php/product-Getcity.html';
		options = {
        url: url,
        method: 'post', 
        data: 'region_id='+id+'&product_id='+product_id,
        onSuccess:function(rs) { 
        	rs = JSON.decode(rs);
        	var html="";
        	for(var i=0;i<rs.length;i++)
        	{
        		html+="	<li class='togger_c'><a  data-value='"+rs[i]['region_id']+"' class='second togger_c' onclick='second(\""+rs[i]['region_id']+"\",\""+rs[i]['local_name']+"\");'>"+rs[i]['local_name']+"</a></li>";
        	}
 					document.getElementById("listcity").innerHTML=html;    	
        } 
	};
 	new Request(options).send("time="+time+"&region_id="+id+"&product_id="+product_id); 
    }
  function Getdqu(id){
		var date = new Date();
		var time = date.getTime();
			var product_id=document.getElementById("product_id").value;  	
    var url='/index.php/product-getArea.html';
		options = {
        url: url,
        method: 'post', 
        data: 'region_id='+id+'&product_id='+product_id,
        onSuccess:function(rs) { 
        	rs = JSON.decode(rs);
	        	var html="";
	        	for(var i=0;i<rs.length;i++)
	        	{
	        		html+="	<li class='togger_c'><a  data-value='"+rs[i]['region_id']+"' class='second togger_c' onclick='three(\""+rs[i]['region_id']+"\",\""+rs[i]['local_name']+"\");'>"+rs[i]['local_name']+"</a></li>";
	        	}
	 					document.getElementById("listqu").innerHTML=html; 
 					 if(rs.length == 0)
        	{
        		   	var sheng=document.getElementById("sheng").innerHTML;
					    	var city=document.getElementById("city").innerHTML;
					    	document.getElementById("peisongdizhi").value=sheng+city;
									$$('.ctab').each(function(e){
					    		if(e.get('data-index')==0 || e.get('data-index')==1){
					    			e.removeClass('curr');
					    		}
					    		if(e.get('data-index')==2){
					    			e.show();
					    			e.addClass('curr');
					    			$$('#stock_province_item').hide();
					      	  $$('#stock_city_item').hide();
					      	  $$('#stock_area_item').show();
					          $$('#stock_town_item').show();
					          $$('#list1').hide();
					    		}
					    			Getprice(id);
				    	})
				    
        	}  	
        } 
	};
 	new Request(options).send("time="+time+"&region_id="+id+"&product_id="+product_id); 
    }  
    function second(id1,name1){
    	Getdqu(id1);
    	var ceshi =$$('.second').get('data-value');

    	document.getElementById("city").innerHTML=name1;
    	$$('.ctab').each(function(e){
    		if(e.get('data-index')==0 || e.get('data-index')==1){
    			e.removeClass('curr');
    		}
    		if(e.get('data-index')==2){
    			e.show();
    			e.addClass('curr');
    			$$('#stock_province_item').hide();
      	  $$('#stock_city_item').hide();
      	  $$('#stock_area_item').show();
          $$('#stock_town_item').hide();
    		}
    	})
    }
