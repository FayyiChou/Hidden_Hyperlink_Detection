   	function Getprice(id){
		var date = new Date();
		var time = date.getTime();
		var product_id=document.getElementById("product_id").value;  

    var url='/index.php/product-Getprice.html';
		options = {
        url: url,
        method: 'post', 
        data: 'region_id='+id+"&product_id="+product_id,
        onSuccess:function(rs) { 
        	rs = JSON.decode(rs);
 				if(rs == null || rs == "")
 				{
 					rs=0;
 					document.getElementById("price").innerHTML="￥"+parseInt(rs).toFixed(2); 
 				//	Dialog.alert('确认收货地，以确保在商家销售区域。');
 					document.getElementById("wuhuo").style.display="block";  
 				}else
 				{
 					//rs=rs.parseInt(re)
 					document.getElementById("price").innerHTML="￥"+parseInt(rs).toFixed(2); 
 					document.getElementById("wuhuo").style.display="none";  
 					 
 				}  	
       } 
	};
 	new Request(options).send("time="+time+"&region_id="+id+"&product_id="+product_id); 
    } 
    
