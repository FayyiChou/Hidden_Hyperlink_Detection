    function ceshi(num)
    {
    	var _this=document.getElementById("ctab"+num);
    	if(num==0){
      	$$('.ctab').hide();
      	_this.show();
      	_this.addClass('curr');
      	$$('#stock_province_item').show();
      	$$('#stock_city_item').hide();
      	$$('#stock_area_item').hide();
      	$$('#stock_town_item').hide();
      }
         if(num==1){
         	document.getElementById("qu").innerHTML="请选择";
      	$$('.ctab').show();
      	_this.show();
      	$$('.ctab').removeClass('curr');
      	_this.addClass('curr');
      	$$('#stock_province_item').hide();
      	$$('#stock_city_item').show();
      	$$('#stock_area_item').hide();
      	$$('#stock_town_item').hide();
      }
    }
