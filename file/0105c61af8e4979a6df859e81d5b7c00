    <script type="text/javascript">
        function doAction(){
          var searchword =document.getElementById("s_key").value;
          var pattern=/[`~!@#$%^&*()_+<>?:"{},.\/;'[\]]/im;  
          if(searchword ==null||searchword ==''||searchword =='输入搜索内容'){
              alert('请输入检索词！');
              return ;
          }else if (pattern.test(searchword)){
              alert('请不要输入非法字符！');
              return false;
        }
         window.open(document.getElementById("znjs").value+'?searchword='+encodeURI(searchword));
          return false;
        }
        function doSearch(){
          if (event.keyCode==13)  //回车键的键值为13 
          doAction();
        }
        function reciSearch(){
          document.getElementById("rcs").style.display="";
        }
    </script>
