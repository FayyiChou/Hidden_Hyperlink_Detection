<script type="text/javascript">
document.domain = "naver.com";

function notiAlert(frmStr)
{
    open_window("", frmStr, 100, 100, "");
    var frm = eval("document." + frmStr);
    frm.target = frmStr;
    try{
        frm.submit();
    }catch(e){}
}









function visit() {


	try{
		var ajax = new Ajax("/MyCafeVisit.nhn", {
			suspend : "true",         
			method : "POST",
			params : { "clubid":14770839,"usingCounter":true,"ceremonyCheckNeeded":false },      
			onLoad : cafeVisitCB
		});
		ajax.request();
	}catch(e){
		JEagleEyeClient.raiseError(e, this, {
    		message : "MyCafeVisit.nhn - failed:" + 14770839
    	});
	}



}
function cafeVisitCB(res) { 
	if(res.status==200) {
		try{ 
	
			var counter = eval('(' + res.responseText + ')');
			
			if(counter == null || counter == 'undefined') {
				return false;
			}
			
		
				if(counter.totalVisitCount != null) { $("totalVisitCount").innerHTML = counter.totalVisitCount; }
				if(counter.todayVisitCount != null) { $("todayVisitCount").innerHTML = counter.todayVisitCount; }
		
		
		
				if(counter.visitEventResult != null && counter.visitEventResult.win == 'true') {
					open_window('/CafeEventPop.nhn?clubid=14770839&eventtype=2&eventresult='+counter.visitEventResult.condition+'&eventarticle='+counter.visitEventResult.eventArticleid, 'cafe_event_popup', 330, 250);
				}
				if(counter.ceremonyPassed == 'true') {
					open_window("/CafeCeremonyProcess.nhn?clubid=14770839&m=pass","cafe_ceremony_pass", 450, 200);
				}
		       
		
			if(counter.notiList != null) {
				for (var i = 0; i < counter.notiList.length; i++) {
					if (counter.notiList[i].exposeAll == 'true' || counter.notiList[i].exposeCafe == 'true') {			
						document.notifrm.action = counter.notiList[i].param2;
						document.notifrm.noticode.value = counter.notiList[i].noticode;
						document.notifrm.comment.innerText = counter.notiList[i].message;
						
						notiAlert('notifrm');
					}
				}
			}
		}catch(e){
			JEagleEyeClient.raiseError(e, this, {
        		message : "response:" + res.responseText}
        	);
		}  
	} 
} 

LH.add("visit()");

Function.prototype.bind = function(){
    var f = this, a = $A(arguments), object = a.shift();
    return function(){
        f.apply(object, a.concat($A(arguments)));
    }
};

function callAppView(componentid, appId) {
    var ajax = new Ajax("/AppCafeViewAjax.nhn?clubid=14770839&appId="+appId, {
        suspend : "true",
        onLoad : appViewCallBack.bind(window,componentid,appId)
    });
    ajax.request();
}


function appViewCallBack(componentid, appId, res) {
    $(componentid).innerHTML = '';
    $(componentid).innerHTML = res.responseText;

    try{
        parentResizeIframe('cafe_main');
    }catch(e){}
}

















 

	//agent
	var bSupportHTML5 = true;
	var oNavi = navigator.userAgent;
    var bIE = oNavi.indexOf('MSIE') > 0 ? true : false,
    	 oIEVersion = (bIE) ? parseFloat(oNavi.match(/(?:MSIE) ([0-9.]+)/)[1]) : 0;
   if(bIE && oIEVersion < 10){ bSupportHTML5 = false; }
   
   //setting
    var cafe_watch_area_Id = "widget-watch";
    var cafe_watch_thumbImg ="http://cafeimgs.naver.net/widget/watch/thumb_digital_121_05.jpg";	 
    var cafe_watch_css =  "digital_121_05|4|B2B2B2|000000";
    var cafe_watch_designId =  "46";
	var isUsingDesignId =true;	 
		 
	//resetting    
	if(cafe_watch_css.indexOf("undefined")>-1) cafe_watch_css = 'digital_064_05|4|7F7F7F|FFFFFF';
	var cafe_watch_name = cafe_watch_css.split('|')[0],
	 	cafe_watch_font = cafe_watch_css.split('|')[1],
	 	cafe_watch_color = cafe_watch_css.split('|')[2],
	 	cafe_watch_bgcolor = cafe_watch_css.split('|')[3];
	 	
		sDataUrl =  '/ContentWidget.nhn?m=watch&city=KR004&';
 	var oContentsTable = {
			"digital_121_06" : 10,
			"digital_064_05" : 7,
			"analog_121_04" : 1,
			"analog_121_07" : 2,
			"analog_121_08" : 3,
			"digital_038_01" : 6 
	};
		
	//oContentsTable 내에 없는 디자인으로 선택되어있는경우 default로 변경된다.
	if(!oContentsTable[cafe_watch_name]){
		cafe_watch_name = 'digital_064_05';
		cafe_watch_font = '4';
		cafe_watch_color = '7F7F7F';
		cafe_watch_bgcolor = 'FFFFFF';
	}
	
  	if( bSupportHTML5 ){
 	 	var optionWatchWidget = {
	        fontColor: "#"+cafe_watch_color,
	        bgColor: "#"+cafe_watch_bgcolor,
	        req_url: sDataUrl,  
	        img_url: 'http://static.naver.net/cafe/widget/watch/widget_watch_20161121.png',
	        fontData: cafe_watch_font,
	        contentsType: oContentsTable[cafe_watch_name],
	        containerName: cafe_watch_area_Id
	    };
	    
	    var oClockWidget = new naver.widget.Clock(optionWatchWidget);
	    
