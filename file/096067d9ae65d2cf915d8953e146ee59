	/**
	 * 채팅홈을 팝업으로 띄운다.
	 */
	 function openChatHomePopUp() {
		var oWnd = window.open('https://chat.cafe.naver.com/ChatHome.nhn?cafeId=14770839', 'CafeChatHome', 'width=570,height=520,toolbar=0,status=0,menubar=0,location=0,scrollbars=yes,resizable=1');
		if (!oWnd) {
			alert("선택하신 기능이 차단되었습니다.\n팝업 차단을 해지하신 후에 다시 시도해 주세요.");
		} else {
			enda = oWnd;
			oWnd.focus();
		}
	}

	function chatting() {
		
        	
	        
	        
	            joinCafe();
	        
       	
	}
	
	
	function accessChat() {
	    var oSslChecker = new Image();
	
		oSslChecker.onload = function() {
			openChatHomePopUp();
	    };
	
	    oSslChecker.onerror = function() {
	    	alert("채팅에 필요한 인증서를 지원하지 않는 OS입니다.");
	    };
	
	    oSslChecker.src = "https://secure.cafe.naver.com/secure-images/sslcheck/cafe.gif";
	}
	
	// 멤버정보 변경
	function cafeMemberInfoEdit(event){
		open_window("/CafeMemberInfo.nhn?clubid=14770839&memberid=", "popup", 500, 780, "toolbar=0,menubar=0,scrollbars=yes,resizable=yes");	    
	}

	// 별명 변경 
	function openProfileEdit(){
	    window.open("http://admin.blog.naver.com/AdminMain.nhn?blogId=&Redirect=Basicinfo", "profile");
	}

	// 퍼스나콘 변경 
	function openItemBag()
	{
	    window.open("http://admin.blog.naver.com/AdminMain.nhn?blogId=&Redirect=ItembagP", "profile");
	}
	
