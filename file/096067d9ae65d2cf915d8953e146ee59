<script type="text/javascript" src="/static/js/mycafe/javascript/lib/nanoajax/nanoajax.min-1476841572000-685.js" charset="UTF-8"></script>

<script type="text/javascript">
    
    setMinSize(773, -1);
    setMaxSize(773, -1);

    
    function moveIframePage(url) {
        $("cafe_main").src = url;
    }

    
    
    function suggestJoin() {
        

        var clubname = "타오바오 완전정복";
        if (confirm('카페멤버만 이용할 수 있는 기능입니다.\n' + clubname.replace(/&lt;(?=.+?>)/gi, "<") + ' 카페에 가입하시겠습니까?')) {
            joinCafe();
        }
    }
    

    
    
</script>





