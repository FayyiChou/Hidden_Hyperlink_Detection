		});
		
		oMenus.add(oMenu);
	
		var oMenu = new Cafe.menu.Menu({
			nMenuId : 100,
			bBadMenu : false,
			sMenuType : "M",
			sBoardType : " ",
			bHasArticleForm : false,
			bHasTemplate : false
		});
		
		oMenus.add(oMenu);
	
		var oMenu = new Cafe.menu.Menu({
			nMenuId : 103,
			bBadMenu : false,
			sMenuType : "L",
			sBoardType : " ",
			bHasArticleForm : false,
			bHasTemplate : false
		});
		
		oMenus.add(oMenu);
	
	
	// 메뉴로 이동
	// 사실상 아래 메소드는 특정 메뉴를 클릭했을 때, 1. 해당 메뉴를 Bold등의 처리를 하고, 2. 동일한 메뉴(스페셜메뉴, 기본메뉴 등에)가 있을 경우, 같이 Bold 처리함 
	function goMenu(nMenuId)	{
	   // 메뉴이동속도를 빠르게 하기 위해 실제 메뉴이동은 별개로 처리
	   setTimeout(function () {
		   oMenuMoveUpdater.update(nMenuId);
	   }, 1);
	}

	function toggleMenuGroup( gid ) {	
		if ($(gid).style.display == 'none') {
			Element.show(gid);
			$(gid+"btn").className = "up-btn"; 
		} else {
			Element.hide(gid);
			$(gid+"btn").className = "down-btn";
		}		
	}

</script>
