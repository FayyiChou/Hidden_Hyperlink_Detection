<script>
	function ll (){
		var wh = $('#banner .bzBanner').width();
		var bh = $('#banner .bzBanner .btn').width();
		var imgh = $('#banner .bzBanner').find('img').height();
		var bh = $('#banner .bzBanner .btn').width();
		
		var kk2 = (wh/2-bh)+bh/2 ;
		$('#banner .bzBanner .btn').css({'left':kk2,'top':imgh+10});
		$('#banner .bzBanner').height(imgh);
	}
	$(window).ready(ll);
	$(window).resize(ll);
</script>
    <script type="text/javascript">
    $('.tab-box ul li').each(function(i){
      $(this).click(function(){
        $(this).addClass('on').siblings().removeClass('on');
        $('.log-list').eq(i).addClass('on').siblings().removeClass('on');
        kk();
      });
    });
      function kk(){
  var win_h = $(window).height();
  var win_w = $(window).width();
      var box_h = $('#log-reg-box').height();
       $('#log-reg-box').animate({'top':((win_h-box_h)/2),'left':((win_w-360)/2)});
      }
      $(window).resize(kk);
      $(window).ready(kk);
		
      $('.btn-login-now').click(function(){
        $('#log-reg-box').fadeIn(200);
          $('#log-reg-box input[name="account"]').focus();
       
        $('.mask').fadeIn(200);
      });
      $('.btn-login-close').click(function(){
        $('#log-reg-box').fadeOut(200);
        $('.mask').fadeOut(200);
      });
    </script>
