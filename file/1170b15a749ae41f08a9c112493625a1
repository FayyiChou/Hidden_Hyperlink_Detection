<script type="text/javascript" charset="gbk" src="http://bbs.xmfish.com/wap/js/TouchSlide.1.1.js"></script>
<script>
$(function() {
	$('.plates_main').each(function(index, element) {
		var html = '', len = $(this).find('.plates_con li').length;
		if(len>1){
			html += "<ul class='plates_tab'>"
			for(var i = 1;i<=len;i++){html += '<li></li>';}
			html += '</ul>' 
			$(this).append(html);
		}
	});
});
setTimeout(function(){ document.getElementById('loading').style.display = 'none';
	TouchSlide({slideCell: "#hot_plate", titCell: "#hot_plate .plates_tab li", mainCell: "#hot_plate .hot_con", effect: "leftLoop", autoPlay: false});
	if(document.getElementById('collect_plate').getElementsByTagName('ul')){console.log(11);TouchSlide({slideCell: "#collect_plate", titCell: "#collect_plate .plates_tab li", mainCell: "#collect_plate .collect_con", effect: "leftLoop", autoPlay: false});}
 },500);
if(document.getElementById('top_img').length){
TouchSlide({slideCell: "#top_img", titCell: "#top_position li", mainCell: "#top_img .top_wrap", effect: "leftLoop", autoPlay: true});
}
</script>
