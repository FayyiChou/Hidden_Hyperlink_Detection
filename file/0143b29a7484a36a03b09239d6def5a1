				dataType:'JSONP',
				success:function (data) {
					 $(".addtab-main").html(data);
					addTab('.addtab-wrap', '.addtab-ul li', '.addtab-main-one');//详情新增
				}



			})


		})
	</script>
	<script>
		$(function(){
			var tab_hot = {
				id:$('.auto_reading'),
				init:function(){
					tab_hot.id.find('.dtxt>.box').eq(0).show().siblings().hide();
					tab_hot.id.find('.det-title>span').on('click',function(){
						var m = $(this).index();
						tab_hot.id.find('.det-title>span').removeClass();
						$(this).addClass('now');
						tab_hot.id.find('.dtxt>.box').eq(m).show().siblings().hide();
					})
				}
			}
			tab_hot.init();
		})
	</script>


</body></html>