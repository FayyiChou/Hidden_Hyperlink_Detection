<script type="text/javascript">
$(function(){
var max = $('.banner ul.img li').length,
li_width = $('.banner').width(),
stime = 7000,
x =0;
$(".banner li").width(li_width);
for(i=0; i<max;i++){$(".banner ul.list").append('<li></li>')};
$(".banner ul.list li:first").addClass('on');		
function lock(){
if(x == max) x = 0;
$('.banner ul.img').animate({'left':-x*li_width});
$('.banner ul.list li').removeClass('on').eq(x).addClass('on');
};
$(".banner ul.list li").live('click',function(){
x = $(this).index();lock();
});
var st = setInterval(function(){x++;lock()},stime);
$('.banner').hover(function(){clearInterval(st)},function(){st = setInterval(function(){x++;lock()},stime)});
})
</script>

