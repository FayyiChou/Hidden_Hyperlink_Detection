<script type="text/javascript">
	$('#menu .nav-menu > li').hover(function() {
		$(this).find('.child').animate({
			opacity : 'show',
			height : 'show'
		}, 200);
		$(this).find('.item').addClass('navhover');
	}, function() {
		$('.child').stop(true, true).hide();
		$('.item').removeClass('navhover');
	});
</script>
