	<div id="codefans_net" style="position:absolute;left:0; top:210px; z-index:9999;" onMouseOver="adStop();" onMouseOut="adMove();">
		<a href="http://www.sace.org.cn/Home/Index/bl"  target="_blank" />
			<img src="../../../Upload/float_bl.png" border="0" style="width: 170px;" usemap="#sacemap">
		</a>	 
	</div>
	<script language="javascript" type="text/javascript">
			var x = 0;
			var y = 0;
			var xin = true;
			var yin = true;
			var stepInThis = 1;
			//运动速度
			var delayInThis = 10;
			//广告对象
			var objInThis = document.getElementById("siweMn");
			//控制广告移动
			var itlInThis = "";
			
			function floatInThis() { 
				var L=0;
				var T=0;
				
				//屏幕的宽度
				var R = document.body.clientWidth - objInThis.offsetWidth;

				//屏幕的高度 var B = document.body.clientHeight-objInThis.offsetHeight
				var B = parseInt("750") - parseInt(objInThis.offsetHeight);

				objInThis.style.left = (x + document.body.scrollLeft) + "px";
				objInThis.style.top = (y + document.body.scrollTop + 210) + "px";
				
				//控制x轴位置
				x = x + stepInThis*(xin?1:-1);
				if (x < L) { 
					xin = true; x = L;
				} 
				if (x > R){ 
					xin = false; x = R;
				} 
				
				//控制y轴位置
				y = y + stepInThis*(yin?1:-1);
				if (y < T) { 
					yin = true; y = T;
				} 
				if (y > B) { 
					yin = false; y = B;
				} 
			}
			
			//鼠标悬停，广告停止移动
			function adStop(){ 
				clearInterval(itlInThis);
			} 
			//鼠标移开，广告继续移动
			function adMove(){
				itlInThis = setInterval("floatInThis()", delayInThis);
			} 
			
			function onLoadInThis(){
				objInThis = document.getElementById("codefans_net");
				itlInThis = setInterval("floatInThis()", delayInThis);
			}

			window.onload = function(){
				onLoadInThis();
			}
	</script>
