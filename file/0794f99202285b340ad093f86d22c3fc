<script type="text/javascript"> 
$(".select").hover(
  function () {
    $(this).find(".msgNav").show();
  },
  function () {
    $(this).find(".msgNav").hide();
  }
);
$(".title-btn").toggle(
  function () {
    $(this).addClass("on");
	$(this).parent(".box-content").removeClass("off");
  },
  function () {
    $(this).removeClass("on");
	$(this).parent(".box-content").addClass("off");
  }
);
$(function(){
	$(".picbig,.no-pic").each(function(i){
		var cur = $(this).find('.img-wrap').eq(0);
		var w = cur.width();
		var h = cur.height();
	   $(this).find('.img-wrap img').LoadImage(true, w, h,'http://www.phpcms.cn/statics/images/msg_img/loading.gif');
	});
	if($("#contentImg").length){
		$("#contentImg img").LoadImage(true, 950, 850,'http://www.phpcms.cn/statics/images/msg_img/loading.gif')
	};
	$("#topLogin").load("http://www.phpcms.cn/index.php?m=member&c=index&a=mini&forward="+encodeURIComponent(location.href)+"&siteid=1&random=1486440361");
})
</script>
