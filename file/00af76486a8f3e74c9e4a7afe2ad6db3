function ukFreeApply(data) {
	var oCount = getByClassName(document, "span", "freeTestCount")[0]; 
	var oFreeTestList = getByClassName(document, "ul", "freeTestList")[0];  
	var freeTestListResult = '';

	if(data.RESULT_COUNT != '0'){
		oCount.innerHTML = data.RESULT_COUNT;
		for(var i=0; i<data.RESULT_DATA.length; i++){
			freeTestListResult += '<li><a style="color:#333; text-decoration:none;" href="http://www.51offer.com/threestepsother/onestepother.html?uker=uker&src=000003" title="">' + format(data.RESULT_DATA[i].created) + '&nbsp;<span style="color:#4eb6ea">' + cutstr(data.RESULT_DATA[i].username, 14) + '</span>&nbsp;测试了&nbsp;<span style="color: #3564cd;">' + cutstr(data.RESULT_DATA[i].cname, 14) + '</span>&nbsp;成功率</a></li>';

/*
			freeTestListResult += '<li>' + format(data.RESULT_DATA[i].created) + '&nbsp;<span class="blue1">' + cutstr(data.RESULT_DATA[i].username, 14) + '</span>&nbsp;测试了&nbsp;<a href="http://www.51offer.com/threestepsother/onestepother.html?uker=uker&src=000003" target="_blank" title="' + data.RESULT_DATA[i].ename + '（' + data.RESULT_DATA[i].cname + '" class="blue uline">' + cutstr(data.RESULT_DATA[i].cname, 14) + '</a>&nbsp;成功率</li>';

			freeTestListResult += '<li style="list-style: none; height: 24px; overflow: hidden;"><a href="http://www.51offer.com/threestepsother/onestepother.html?uker=uker&src=000003" title="">' + format(data.RESULT_DATA[i].created) + '&nbsp;<span style="color:#4eb6ea">' + cutstr(data.RESULT_DATA[i].username, 14) + '</span>&nbsp;测试了&nbsp;<span style="color: #3564cd;">' + cutstr(data.RESULT_DATA[i].cname, 14) + '</span>&nbsp;成功率</a></li>';
*/

		};
		oFreeTestList.innerHTML = freeTestListResult;
	} else {
		alert("错误");	
	};
};
function add0(m){return m<10?'0'+m:m };
function format(timeStamp)
{
	var time = new Date(timeStamp);
	var m = time.getMonth()+1;
	var d = time.getDate();
	return add0(m)+'-'+add0(d);
};

function getByClassName(parent, tagName, className){
	var aElements = parent.getElementsByTagName(tagName);
	var arr = [];
	
	for(var i=0; i<aElements.length; i++){
		var aClassName = aElements[i].className.split(' ');
		for(var j=0; j<aClassName.length; j++){
			if(aClassName[j] == className){
				arr.push(aElements[i]);
				break;	
			};
		};
	};
	return arr;
};

function arrIndexOf(arr, v){
	for(var i=0; i<arr.length; i++){
		if(arr[i] == v){
			return i;	
		};	
	};	
	return -1;
};

function cutstr(str,len){  
	var str_length = 0;  
	var str_len = 0;  
	str_cut = new String();  
	str_len = str.length;  

	for(var i = 0;i<str_len;i++){  
		a = str.charAt(i);  
		str_length++;  
		if(escape(a).length > 4){  
			str_length++;  
		}  
		str_cut = str_cut.concat(a);  
		if(str_length>=len){  
//			str_cut = str_cut.concat("...");  
			return str_cut;  
		};  
	};

	if(str_length<len){  
		return  str;  
	} ; 
};  
</script> 
