<script>
  function addLove(uid,memberId){
	 	App.ajax("",{
	        type:'POST',
	        url:'/util/app/jsonresult',
	        data: {threadType:12,am:'addLove',uid:uid,memberId:memberId},
	        success:function(res){
	            if(res.success){
	              	App.tips({message: "暗恋成功！" , autoclose: 3});
	              	setInterval(function(){
                      window.location.reload();
                    },500);
	            } else {
	            	if (res.code == 100){
		                   App.tips({type: "error" ,message: "请先登录！" , autoclose: 3});
					} else if (res.code == 101){
						App.tips({type: "error" ,message: "不能自恋！" , autoclose: 3});
					} else if (res.code == 102){
						App.tips({type: "error" ,message: "已经暗恋了！" , autoclose: 3});
					} else if (res.code == 103){
						App.tips({type: "error" ,message: "你还没加入爱情幸运号，不能暗恋哦" , autoclose: 3});
					}else if (res.code == 104){
						App.tips({type: "error" ,message: "你已被撤销爱情幸运号会员资格，不能暗恋" , autoclose: 3});
					}else if (res.code == 105){
						setInterval(function(){
                           window.location.reload();
                        },500);
					}

	            }
	        },
	        error:function(){
	            App.tips({type: "error" ,message: "暗恋失败！" , autoclose: 3});
	        },
	        dataType: 'json'
	 	});
	}

	function cancelLove(uid,memberId){
        App.ajax("",{
            type:'POST',
            url:'/util/app/jsonresult',
            data: {threadType:12,am:'cancelLove',uid:uid,memberId:memberId},
            success:function(res){
                if(res.success){
                    App.tips({message: "取消暗恋成功！" , autoclose: 3});
                    setInterval(function(){
                      window.location.reload();
                    },500);
                } else {
                    if (res.code == 100)
					{
                        App.tips({type: "error" ,message: "请先登录！" , autoclose: 3});
                    }
					else if(res.code==101)
					{
                       App.tips({type:"error",message: "您已经取消了对该用户的暗恋" , autoclose:2});
                       setInterval(function(){
                          window.location.reload();
                       },500);        
                    }
                    else if(res.code==102)
					{
                       App.tips({type:"error",message: "您已经退出爱情幸运号或者已被撤销，无法进行该操作" , autoclose:2});
                       setInterval(function(){
                           window.location.reload();
                       },500);
                    }
					else 
					{
                        App.tips({type: "error" ,message: "取消暗恋失败！" , autoclose: 3});
                    }
                }
            },
            error:function(){
                App.tips({type: "error" ,message: "取消暗恋失败！" , autoclose: 3});
            },
            dataType: 'json'
        });
    }

	AM.ready("ajax",function(){
		var uids = "";
		$("#view-bd").find(".side").each(function(){
		    var uid =$(this).children("p:first").children("a:eq(1)").attr("userid");
			if(uid != undefined){
			   uids += uid+",";
			}
		})
		uids = uids.substring(0,uids.length-1);
		
	    App.ajax("",{
	        type:'POST',
	        url:'/util/app/jsonresult',
	        data: {threadType:12,am:'getViewSide',fid:I9.platform.fid,tid:I9.platform.tid,uids:uids},
	        success:function(res){
	            if(res.success){
				    
				    var j = 0;
	            	for(var i = 0;i < res.data.length;i++){
					   var uid =$(".side:eq("+j+")").children("p:first").children("a:eq(1)").attr("userid");
					   if(uid != undefined){
					        if(res.data[i] != ""){
							    $(".side:eq("+j+")").children("p:first").children("a:eq(1)").hide();
                                $(".side:eq("+j+")").children("p:first").append(res.data[i]);
                                j++;								
							}else{
							   j++;
							}
					   }else{
					     j++;
						 i--;
					   }
	            	}

                    var COOKIE_NAME = "19lou_love_pop_daohang";
                    if(getCookie(COOKIE_NAME)){
                    }else{
                       if($(".blind-learn").length > 0){
                         $(".blind-learn").css({"display":"block"});
                          addCookie(COOKIE_NAME,"19loucookie",24)
                       }
                    }
	            } 
	        },
	        error:function(){
	            App.tips({type: "error" ,message: "获取暗恋信息异常！" , autoclose: 3});
	        },
	        dataType: 'json'
	 	});

         function addCookie(objName,objValue,objHours){//添加cookie
              var date = new Date();
              var str = objName + "=" + escape(objValue);
                if(objHours > 0){
                    var ms = objHours*3600*1000;
                    date.setTime(date.getTime() + ms);
                    str += "; expires=" + date.toGMTString();
                }
                document.cookie = str;
         }
         function getCookie(name)//取cookies函数
         {
            var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
            if(arr != null) return unescape(arr[2]); return null;
         }

	 	
	 	$(".close").live("click",function(){
			$(this).closest(".popo-blind").remove();
		})
		
		
    
		
   });


</script>
