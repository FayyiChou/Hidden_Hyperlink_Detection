function zone_close(){
		$('#mylist').empty();
		$('#myzone').hide();
	}
	
function zone_show(p){
	$('#mylist').empty();
	var result = null;
		result = $.grep(dataSource,function(val,key){
			if(val.province == p){
				return true;
			}
			else{
				return false;
			}
		});
		


	if(result == null){
		zone_close();
	}
	
	$.each(result,function(key,val){
		$('#mylist').append('<tr><td>'+val.name+'</td></tr>');
	});
	
	$('#mytitle').text(p);
	
	$('#myzone').show();
	

   myzone.style.left=event.clientX-150;
   myzone.style.top=event.clientY+5; 
   myzone.style.position="absolute";


}

   
function setProvince(p){
	var $v = p;
	if($v!=''){
		var result = $.grep(dataSource,function(val,key){
			if(val.province == $v){
				return true;
			}
			else{
				return false;
			}
		});
		zone_show($v);
	}
}

