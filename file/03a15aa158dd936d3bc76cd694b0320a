var type = "slide";
if (type == 'click') {
	$(function(){
		$('#pro_type_GjxxNB li:has(ul)').click(function(event){
	    	if (this == event.target) {
	        	if ($(this).children().is(':hidden')) {
	                $(this).css({background:'url(template/jixie-110118-a16/images/minus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'})
	                .children().show();
	                
	              	$(this).siblings().each(function (){
	              		$(this).find("ul").hide();
	              		if ($(this).children().is("ul")){
	              			$(this).css({background:'url(template/jixie-110118-a16/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'});
	              	    }
	              	});
	            } else {
	                $(this).css({background:'url(template/jixie-110118-a16/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'})
	                .find("ul").hide();
	            }  
	        }
		}).css('cursor','pointer');
	          
	    $('#pro_type_GjxxNB li:has(ul)').css({background:'url(template/jixie-110118-a16/images/plus.gif) no-repeat left 13px','text-indent':'16px','padding-top':'3px'});
	        	
	    $('#pro_type_GjxxNB li:not(:has(ul))').css({cursor: 'pointer','list-style-image':'none'});
	});
} else if(type=='slide'){
	$("#pro_type_GjxxNB > ul").droppy();
	$("#pro_type_GjxxNB ul ul li:last-child").css("border","0px");
} 
