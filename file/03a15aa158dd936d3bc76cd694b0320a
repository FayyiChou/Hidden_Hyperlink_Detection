<script type="text/javascript">

var speed=20;
var marquee_product2NkLPTA = document.getElementById("marquee_product2NkLPTA");
var marquee_product1NkLPTA = document.getElementById("marquee_product1NkLPTA");
var marquee_demoNkLPTA = document.getElementById("marquee_demoNkLPTA");

var showwidthNkLPTA;
var loopwidthNkLPTA;
var iNkLPTA;
var mNkLPTA,nNkLPTA;

showwidthNkLPTA=marquee_demoNkLPTA.offsetWidth*1;
loopwidthNkLPTA=marquee_product1NkLPTA.offsetWidth*1;

if(loopwidthNkLPTA<=1){loopwidthNkLPTA=1;}

mNkLPTA=Math.ceil(showwidthNkLPTA/loopwidthNkLPTA);

for(nNkLPTA=0;nNkLPTA<mNkLPTA;nNkLPTA++){
marquee_product2NkLPTA.innerHTML+=marquee_product1NkLPTA.innerHTML;
}

marquee_demoNkLPTA.scrollLeft=loopwidthNkLPTA;
iNkLPTA=0;

function MarqueeNkLPTA(){

if(iNkLPTA>=loopwidthNkLPTA){
 iNkLPTA=0;
 }else{
 iNkLPTA++;
}
marquee_demoNkLPTA.scrollLeft=loopwidthNkLPTA-iNkLPTA;

}
var MyMarNkLPTA=setInterval(MarqueeNkLPTA,speed);
marquee_demoNkLPTA.onmouseover=function(){clearInterval(MyMarNkLPTA);}
marquee_demoNkLPTA.onmouseout=function(){MyMarNkLPTA=setInterval(MarqueeNkLPTA,speed);}

</script>

