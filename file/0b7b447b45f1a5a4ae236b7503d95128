<script>
jQuery(document).ready(function($){
    $('.menu-toggle').click(function () {
		console.log($(this).html());
		console.log(this.innerHTML);
		$('.site-header-menu').slideToggle('slow');
	});

    $(document).on('keydown', 'input[type="tel"], input.number', function(e){-1!==$.inArray(e.keyCode,[46,8,9,27,13,110,190])||/65|67|86|88/.test(e.keyCode)&&(!0===e.ctrlKey||!0===e.metaKey)||35<=e.keyCode&&40>=e.keyCode||(e.shiftKey||48>e.keyCode||57<e.keyCode)&&(96>e.keyCode||105<e.keyCode)&&e.preventDefault()});
	// Home Page Form Validation Code
	$("#homepage-booking-form").validate({
	  rules: {
		name: "required",
		when: "required",
		mobile: {
			required: true,
			number: true
		},
		email: {
			required: true,
			email: true
		},
		guest_count: {
			required: true,
			min: 1
		}
	  },
	  messages: {
		name: "Please specify your name",
		when: "Please specify your date",
		mobile: "Please enter your mobile number",
		email: {
		  required: "We need your email address to contact you",
		  email: "Your email address must be in the format of name@domain.com"
		},
		guest_count: "Please select guests"
	  },
	  submitHandler: function(form) {
			$.post("/wp-admin/admin-ajax.php",{action:"booking_form_data",data:$(form).serializeArray()},function(response){
				if(response){
					//console.log(response);
					window.location="/booking-thanks";
				}
			});	
		}
	});

	$("#subcriber_form").validate({
		  rules: {
			name: "required",
			email: {
			  required: true,
			  email: true
			}
		  },
		  messages: {
			name: "Please specify your name",
			email: {
			  required: "We need your email address to contact you",
			  email: "Your email address must be in the format of name@domain.com"
			}
		  },
		  submitHandler: function(form) {
			  $.post("/wp-admin/admin-ajax.php",{action:"subcriber_form_data",data:$(form).serializeArray()},function(response){
					if(response){
						window.location="/subscribe-thanks";
					}
			 });		
		  }
	});
	
 });
</script>
