<script>
jQuery(document).ready(function($){
	$( ".loading-div" ).attr( "style","display:none");
	$( document ).ajaxStart(function() {
		$( ".loading-div" ).attr( "style","display:block");
	});
	$( document ).ajaxComplete(function() {
		$( ".loading-div" ).attr( "style","display:none");
	});
});
</script>

