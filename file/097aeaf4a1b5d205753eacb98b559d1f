<style>
	
	#firework {
		width: 31px;
		height: 100px;
		background: url(/ch/src/spring/firework.png) no-repeat;
		position: absolute;
		left: 380px;
		top: 20px;
	}
	
  .flake {
    background-repeat: no-repeat;
    background-size: contain;
    _outline: 1px solid blue;
  }
  .flake0 { background-image: url(/ch/src/spring/red1.png); }
  .flake1 { background-image: url(/ch/src/spring/red2.png); }
  .flake2 { background-image: url(/ch/src/spring/red1.png); }
  .flake3 { background-image: url(/ch/src/spring/red2.png); }
  
  .flake4 { background-image: url(/ch/src/spring/coin1.png); }
  .flake5 { background-image: url(/ch/src/spring/coin2.png); }
  .flake6 { background-image: url(/ch/src/spring/coin3.png); }
  .flake7 { background-image: url(/ch/src/spring/coin4.png); }
  
  .flake8 { background-image: url(/ch/src/spring/ingot1.png); }
  .flake9 { background-image: url(/ch/src/spring/ingot2.png); }
  .flake10 { background-image: url(/ch/src/spring/ingot3.png); }
  .flake11 { background-image: url(/ch/src/spring/ingot4.png); }
  
</style>
<script>
(function($) {
	
	$('#hdTitle h1').after('<div id="firework"></div>');
	
	
	
    $.fn.snow = function(options) {
        var $flake = $('<div class="flake">').css({
          'position':'absolute',
          'top':'-50px'
        }),
        documentHeight = 130, //$(document).height(),
        documentWidth = $(document).width(),
        defaults = {minSize: 50,maxSize: 100,newOn: 600 },
        options = $.extend({}, defaults, options);
          
        var interval = setInterval(function() {
        	
        		var flakeClass = 'flake'+Math.floor(Math.random()*12); // 亂數 0~11
          
            var startPositionLeft = Math.random() * documentWidth - 100, startOpacity = 0.9 + Math.random(), 
            sizeFlake = options.minSize + Math.random() * options.maxSize,
            endPositionTop = documentHeight - 40,
            endPositionLeft = startPositionLeft - 100 + Math.random() * 200,
            durationFall = documentHeight * 10 + Math.random() * 5000;
            $flake.clone().appendTo('body').css({
              left: startPositionLeft,
              opacity: startOpacity,
              width: sizeFlake,
              height: parseInt(sizeFlake*0.6,10),
            }).animate({
              top: endPositionTop,
              left: endPositionLeft,
              opacity: 0.5
            }, durationFall, 'linear', function() {
                $(this).remove()
            }).addClass(flakeClass);
                       
        }, options.newOn);
    };
    
    
    
})(jQuery);

$.fn.snow();
</script>

