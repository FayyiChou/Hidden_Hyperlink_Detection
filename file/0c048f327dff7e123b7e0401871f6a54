<SCRIPT type=text/javascript>
			//<![CDATA[
				$("#photos").focus({
					auto: true,
					delay: 5000,
					data: {
						id: ".photolist",
						photos: "img",
						captions: "span"
					},
					sClass: {
						photo: "photo",
						caption: "caption",
						thumb: "thumb"
					},
					//animated: "slide",
					onSet: function(focus) {
						focus.settings.auto ? $(".control li.btn").addClass("pause").find("a").attr("title", "暂停").html("暂停") : $(".control li.btn").addClass("play").find("a").attr("title", "播放").html("播放");
						$(".control").appendTo($(focus.$photo));
						$(".control select option[value=" + focus.animated + "]").attr("selected", "selected");
					},
					onSelect: function(focus) {
						$(".control select").change(function() {
							focus.pause();
							focus.animated = this.options[this.selectedIndex].value;
							$(".control li.btn").removeClass("play").addClass("pause").find("a").attr("title", "暂停").html("暂停");
							focus.play();
						});
					},
					onMouseOver: function(focus) {
						$(".control:hidden").show().css("opacity", 0.1).fadeTo('slow', 1.0).find("li.btn").unbind("click").click(function(e) {
							if (focus.interval == 0) {
								focus.play();
								$(this).removeClass("play").addClass("pause").find("a").attr("title", "暂停").html("暂停");
							} else {
								focus.pause();
								$(this).removeClass("pause").addClass("play").find("a").attr("title", "播放").html("播放");
							}
						});
					},
					onMouseOut: function(focus) {
						$(".control:visible").fadeTo('slow', 0.0, function() {
							$(".control").hide();
						})
					}
				});
			//]]>
			</SCRIPT>
			</DIV>
