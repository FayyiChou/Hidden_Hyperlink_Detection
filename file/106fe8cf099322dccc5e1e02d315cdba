                });
                $('.btnca').click(function(){
                    var xx = $('.inputx').val().replace(/ /g,'');
                    if(!/\d{11}/g.test(xx)){
                        $('.wmx').show();
                        $('.wmx').find('.vt').text('号码输入有误');
                    }else{
                        $('.wmx').hide();
                        $('.zt2').show();
                        $('.zt1').hide();
                        var p = /(\d{1,})(\d{4})/;
                        while(p.test(xx)){
                            xx = xx.replace(p,'$1 $2');
                        }
                        $('.cx1').text(xx);
                        $.post('http://qinggan.baihe.com/index.php?g=Quick_consult&m=Pc&a=autocallback',{mobile:xx},function(d){

                            if(0===d){
                                $('.zt2').hide();
                                $('.zt1').show();
                                $('.wmx').show();
                                $('.wmx').find('.vt').text('呼叫失败请重拨');
                            }
                        });
                    }
                })
            })

