   <script>
   $('.videodlgbox').hover(function(){
                $(this).find('.videoboxtitle').animate({'top':0},200);
            },function(){
                $(this).find('.videoboxtitle').animate({'top':-60},200);
            });
            $('.videoclose').click(function(){
                $('#video_1').get(0).pause();
                $('.videodlg').hide();
                $('.page').removeClass('maobl');
            })
            var show = function(e){
                $('.page').addClass('maobl');
                $('.videodlg').show();
                $('#video_1').get(0).play();
            }
