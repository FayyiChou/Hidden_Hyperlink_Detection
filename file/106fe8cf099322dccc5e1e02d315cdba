                    var i = $(this).index();
                    $('.navlist').each(function(e){
                        $(this).find('li').eq(i).addClass('on').siblings('li').removeClass('on');
                    });

                });

                var d1 = new Date();
                var d2 = new Date(2016,1,19);
                var day = parseInt((d1-d2)/(1000*60*60*24));
                var st = 1000+(day-2)*24
                var st2 = 52013+(day-4)*24

                var num = new Number(1000+(day)*24+parseInt(1*d1.getHours())).toString();
                var num2 = new Number(52013+(day)*24+parseInt(1*d1.getHours()));

                var sz = $('#zss').empty();
                var  stx = new String(num);
                for(var i = 0;i<stx.length;i++){
                    sz.append('<span class="sz">0</span>');
                }
                addnum(sz,st,num,10,stx.length-1);
              var ii = 0;
                ii = window.setInterval(
                        function(){
                            if(st2==num2){
                                window.clearInterval(ii);
                            }else{
                                $('#ffx').text(++st2);
                            }
                        },150
                )
            });
            function addnum(tag,start,end,time,l){
                var ix,sz = tag.find('.sz');
                if(start>end){
                    return;
                }else{
                    ix = window.setInterval(function(){
                        if(start>end){
                            window.clearInterval(ix);
                            return;
                        }
                        var x = new String(start);
                        var j = l;
                        for(var i = x.length-1;i>=0;i--){
                            sz.eq(j--).text(x.charAt(i));
                        }
                        ++start
                    },time);

                }
            }
            function stopDefault(e){
//防止浏览器默认行为(W3C)
                if(e && e.preventDefault){
                    e.preventDefault();
                }
//IE中组织浏览器行为
                else{
                    window.event.returnValue=fale;
                }
                return false;
            }
        </script>

