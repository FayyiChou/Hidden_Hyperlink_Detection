                $('.closex').click(function(){
                    $('.zt2').hide();
                    $('.zt1').show();
                    $(this).closest('.callb').hide();
                    $('.page').removeClass('maobl');
                    $('.callmebox').css({
                        '-webkit-transform': 'translate(-50%, -200%)',
