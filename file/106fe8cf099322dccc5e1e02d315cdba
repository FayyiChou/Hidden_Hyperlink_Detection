                    });
                });
                $('.mfbd,.bgb2,.telphone').click(function(){

                    $('.page').addClass('maobl');
                    $('.callb').show();
                    window.setTimeout(function(){
                        $('.callmebox').css({
                            '-webkit-transform': 'translate(-50%, -50%)',
                            '-moz-transform': 'translate(-50%, -50%)',
