function postToBbs(sid,cid)
{
var oBao = new ActiveXObject("Microsoft.XMLHTTP");
var str="/soft/shareutf.asp?softid="+sid+"&cid="+cid+"&url="+location.href+"&u=ubb";
oBao.open("POST",str,false);
oBao.send();
var atext = decodeURIComponent(oBao.responseText);
window.clipboardData.setData('text', atext);
alert('ubb ok!');
}
</script>

