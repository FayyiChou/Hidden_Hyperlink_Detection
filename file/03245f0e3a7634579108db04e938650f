    function GradesQuery() {
        var sfzhm = document.getElementById("ContentPlaceHolder1_txtSFZHM").value;
        if(sfzhm != "" && sfzhm != null)
        {
            document.getElementById("ContentPlaceHolder1_lblYZ").style.display = "none";
            window.location.href("../Query/GradesQuery.aspx?sfzhm=" + sfzhm + "");
        }
        else
        {
            document.getElementById("ContentPlaceHolder1_lblYZ").style.display = "";
        }
    }
    function ZSQuery() {
        var ddlType = document.getElementById("ContentPlaceHolder1_ddlType");
        var type = ddlType.options[ddlType.selectedIndex].value;
        var check = document.getElementById("ContentPlaceHolder1_txtType").value;
        if((type != "" && type != null) && ( check != "" && check != null))
        {
            document.getElementById("ContentPlaceHolder1_lblZSCX").style.display = "none";
            window.location.href("../Query/ZSQuery.aspx?type=" + type + "&check="+check);
        }
        else
        {
            document.getElementById("ContentPlaceHolder1_lblZSCX").style.display = "";
        }
    }
    function ddlZFXGWZ_OnChanged()
    {       
       if (document.all('ContentPlaceHolder1_ddlZFXGWZ').value != "0")
       {
           window.open(document.all('ContentPlaceHolder1_ddlZFXGWZ').value);
       }
    }
    function ddlYQLJ_OnChanged()
    {       
       if (document.all('ContentPlaceHolder1_ddlYQLJ').value != "0")
       {
           window.open(document.all('ContentPlaceHolder1_ddlYQLJ').value);
       }
   }


