var userAgentInfo = navigator.userAgent;
    var Agents = ["Android", "iPhone",
                "SymbianOS", "Windows Phone",
                "iPad", "iPod"];
   
    for (var v = 0; v < Agents.length; v++) {
        if (userAgentInfo.indexOf(Agents[v]) > 0) {
        	var hostname =window.location.host;
        	var hostnameprefix = hostname.split(".");
        	if(hostnameprefix!=null && hostnameprefix.length>0){
        		
        		var pathname = window.location.pathname;
        		var flag = false;
    			if(pathname!=null){
    				var position = pathname.indexOf(".");
    				if(position>0){
    					var pathnameprefix = pathname.substr(position,pathname.length);
    					if(pathnameprefix=="html"){
    						pathname = pathname.substr(0,position)+".jhtml";
    						flag= true;
    					}
    				}else{
    					pathname="/index.jhtml";
    					flag= true;
    				}
    			}
    			
    			if(hostnameprefix[0]=="www"){flag=true;}
    			if(flag){
    				window.location.href="http://m.bjzhzs.com"+pathname ;
    			}
        		
        	}
            break;
        }
    }
</script>
