        'scrnwi':screen.width,
        'scrnhi':screen.height,
        "uigs_pbtag":"A",
        "uigs_cookie":"SUID,sct"
    };
    var SugPara = {
        "domain":"w.sugg.sogou.com",
        productId:"english",
        sugType:"english",
