    <script>
        var navFunc = function(o, p, key_name){
            var q = document.getElementById("query").value;
            p = p || "";
            key_name = key_name || "query"
            if(q){
                var h = o.getAttribute("href_bak");
                if(h.indexOf("?") != -1){
                    h = h + "&p=" + p + "&" + key_name + "=" + encodeURIComponent(q);
                }else{
                    h = h + "?p=" + p + "&" + key_name + "=" + encodeURIComponent(q);
                }
                o.href = h;
            }else{
                var h2 = o.href;
                if(h2.indexOf("?") != -1){
                    h2 = h2 + "&p=" + p;
                }else{
                    h2 = h2 + "?p=" + p;
                }
                o.href = h2;
            }
        };
        var navFuncMore = function(o, p){
            var q = document.getElementById("query").value;
            p = p || "";
            var key_name ="";
            if(q){
                var h = o.getAttribute("href");
                if(h.indexOf("?") != -1){
                    h = h + key_name + encodeURIComponent(q);
                }else{
                    h = h + key_name + encodeURIComponent(q);
                }
                o.href = h;
            }else{
                var h2 = o.href;
                if(h2.indexOf("?") != -1){
                    h2 = h2  + p;
                }else{
                    h2 = h2  + p;
                }
                o.href = h2;
            }
        };
    </script>
