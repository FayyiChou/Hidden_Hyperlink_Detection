<div class="backtop" style="display: block; z-index: 100; opacity: 0.14405480293801048; "><img src="http://img.yjbys.com/m/shixi/back.png"></div>
<script type="text/javascript" src="http://js.yjbys.com/common/jquery.js"></script>
<script>
    $(document).ready(function(){
        //控制隐藏
        var p = $(".artCont").html();
        var stringMaxLength = 760;
        if(p.length > stringMaxLength){
            $(".artCont").after('<div class="more"><em><code class="fx"></code></em><span>查看更多</span></div>');
            $(".artCont").html(p.substring(0,stringMaxLength)+ " ..........");
            $(document).on('click','.more',function(){
                if($(this).find("code").hasClass("fx")){
                    $(".artCont").html(p);
                    $(this).find("span").text("收起全部");
                    $(this).find("code").removeClass("fx").addClass("c");
                }else{
                    $(this).find("code").removeClass("c").addClass("fx");
                    $(this).find("span").text("查看更多");
                    $(".artCont").html(p.substring(0,stringMaxLength)+ " ..........");
                }
            });
        }

// 虚拟分页
        if($(".pages li").length){
            $('.hao_page').css('display', 'none');
        }
        $(".dow").click(function(){
            if($(this).find("i").hasClass("classify_i")){
                $(this).find("i").removeClass("classify_i").addClass("i");
                $(this).find("span").text("收起");
                $(".neilian").find("li:nth-child(n+19)").fadeIn(1000);
            }else{
                $(this).find("i").removeClass("i").addClass("classify_i");
                $(this).find("span").text("查看更多");
                $(".neilian").find("li:nth-child(n+19)").hide();
            }
        });

        // 分类展开隐藏
        var backtop=100;
        $(window).scroll(function(){
            if($(this).scrollTop()>backtop){
                $(".backtop").fadeIn(500);
            }else{
                $(".backtop").fadeOut(500);
            }
        })
        // 返回顶部按钮渐隐渐现
        $(".backtop").click(function(){
            $("body,html").animate({
                "scrollTop":"0"
            },400)
        });
        // 返回顶部
    });

</script>

