<script>
function getTime(){
  var aTime= new Date();
  var strMinute = aTime.getMinutes()<10?("0"+aTime.getMinutes()):aTime.getMinutes();
  var aTime_detail=aTime.getFullYear()+"年"+(aTime.getMonth()+1)+"月"+aTime.getDate()+"日  "+aTime.getHours()+":"+strMinute
  return ("北京时间："+aTime_detail);
}
</script>


