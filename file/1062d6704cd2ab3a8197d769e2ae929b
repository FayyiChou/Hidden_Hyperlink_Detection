function goto(url){
	var userName = $("#hiddenName").val();
	
	if(null != userName && userName != "" && "null" != userName)
	{
		window.location.href="/"+url;		
	}
	else
	{
		
		window.location.href="/login.do?act=forwardToLogin";
	}
	
}

function gotoDown(){
	var contextName='http://m.saclub.com.cn:80/';
	window.location.href=contextName+"app.do?origin=967";
}

function closeImg(){
	$.ajax({
		type: "POST", // 用POST方式传输
		dataType: "text", // 数据格式:JSON
		url: "feedback.do?act=closeImg", // 目标地址
		error: function (XMLHttpRequest, textStatus, errorThrown) { 
			
		},
		success: function (data){
			}
	})
}
</script>


