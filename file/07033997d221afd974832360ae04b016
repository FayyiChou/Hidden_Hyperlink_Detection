      <script type="text/javascript">
      $(window).bind("scroll", function(){
      if ($(window).scrollTop()>=300) {
      $("#gotop").fadeIn(600);
      } else {
      $("#gotop").fadeOut(600);
      }
      });
      
      $(function(){
      $("#gotop").click(function(){
      $("html,body").animate({scrollTop:0},1000);
      return false;
      });
      });
	  
	        $(function(){
      
      $(".member").hover(function(){
      $(".member ul").slideDown(200);
      },
      function(){
      $(".member ul").slideUp(200);
      }	
      
      );
      
      })
	  
	  $(function(){
	
	$("#ulID252").remove();

      })
      
      </script> 

