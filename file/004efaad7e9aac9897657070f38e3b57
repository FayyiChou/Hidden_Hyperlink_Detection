var t=setinterval(myfunc,1500)
var d=document.getelementbyid("div1")
function myfunc(){
var o=d.firstchild
d.removechild(o)
d.appendchild(o)
}
d.onmouseover=function(){clearinterval(t)}
d.onmouseout=function(){t=setinterval(myfunc,1500)}
