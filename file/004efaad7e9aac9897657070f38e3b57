var page=0;
var speed=3000;

function showpage(id){
	page=id;
	settransition();
	b2bcontent.innerhtml=b2bstr[id];
	b2bcontent.filters.revealtrans.play();
}
function turnpage(){
	showpage(page);
	thetimer=settimeout("turnpage()", speed);
	page++;
	if(page>=5) page=0;
}
function settransition(){
if (document.all){
	b2bcontent.filters.revealtrans.transition=4;//math.floor(math.random()*23)
	b2bcontent.filters.revealtrans.apply();
	}
}
turnpage();
</script></span></td>
  </tr>
</table>
