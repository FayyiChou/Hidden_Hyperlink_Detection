<script type="text/javascript">
    function extand() {
        //sel.style.visibility = 'visible';
        document.getElementById('worldwide').style.visibility = 'visible';
    }
    
    function pinch(obj) {
        document.getElementById('worldwide').style.visibility = 'hidden';
        //sel.style.visibility = 'hidden';
        bb    = 1;
        logic = true;
        clearTimeout(timeid)
    }
    
    var timeid;
    var bb    = 1;
    var logic = true;
    var sel;
    function sele(obj) {
        sel = obj;
    }
    
    function fade() {
        if (document.all);
        if (logic == true) {
            document.getElementById('worldwide').style.visibility = 'visible';
            //sel.style.visibility = 'visible';
            bb += 6;
            if (bb > 75) {
                logic = false;
            }
            if (bb < 75) {
                timeid = setTimeout('fade()', 1);
            }
        }
    }
</script>
        <script type="text/javascript">
            $(document).ready(function() {
                var p = $("#titleImg");
                var q = $("#worldwide");
                var wid=q.width() - p.width();
                var position = p.position();
                var bro=$.browser;
                if (bro.msie || bro.mozilla) {
                    $("#worldwide").css("left", position.left - wid - 0);
                    $("#worldwide").css("top", position.top + p.height() - 0);
                } else if (bro.opera) {
                    $("#worldwide").css("left", position.left - wid - 0);
                    $("#worldwide").css("top", position.top + p.height() - 0);
                } else {
                    $("#worldwide").css("left", position.left - wid - 0);
                    $("#worldwide").css("top", position.top + p.height() - 0);
                }
            })
        </script>
    
