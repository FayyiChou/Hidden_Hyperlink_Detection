<script type="text/javascript">
	//猜你喜欢点击换一换
	if($("#change_also_look").length>0) {
		var _ajax_lock = false;
		if(!_ajax_lock) {
			$("#change_also_look").click(function () {
				_ajax_lock = true;
				var _request_url = '/index.php?c=bookDetail&a=ajaxAlsoLook&cate=0';
				$.ajax({
					url: _request_url,
					type: "get",
					success: function (_data) {
						var book_list = eval('(' + _data + ')'),
							status = book_list.status;
						if (1 == status) {
							var _html = new Array(),_list=book_list.result;
							if (_list.length > 0) {
								for (var book_data in _list) {
									_html.push('<li><a href="/shuku/' + _list[book_data].url_id + '.html" onclick="cc(\'ck_m_cainixihuan\')"><img src="' + _list[book_data].image_link + '" onerror="this.src=\'https://img1.km.com/bookimg/public/pic/common/default.jpg\'"><span>' + _list[book_data].title + '</span></a></li>');
								}
								$("#also_look_list").html(_html.join(''));
							}
						}
						else {
							alert("网络异常，请稍后重试！");
						}
					}
				}).always(function () {
					_ajax_lock = false;
				});
			});
		}
	}
	if (navigator.userAgent.toLocaleLowerCase().indexOf("iphone") >= 0) {
		$(".app-download").attr("href", "https://at.umeng.com/y89Dye");
	}
</script>

