<script>
    $(function(){
            // 打点
            var q = location.search.replace(/^.*(\?|&)q=([\-\w]+).*$/, "$2");
            if (q !== ''){
                var search=window.location.search.match(/(\?|&)clk=([^#&]*)(&|$)/);
                var step=window.location.search.match(/(\?|&)step=([^#&]*)(&|$)/);
                var keyword = '';
                if (search&&search[2])
                {
                    var search=search[2];
                }
                if(search !== 'suggest')
                {
                    var result_num = '565';
                    var result_num = result_num == '' ? 0 : result_num ;
                    if (search == 'key')
                    {
                        var t_url='search_input';
                    }else if (search == 'click')
                    {
                        var t_url='search/word_search';
                    }
                    if(step && step[2]) //首页搜索跳过来
                    {
                        uxl_track('w_home/'+t_url+'/'+keyword+'/result/'+result_num);
                    }else
                    {
                        var type='buy';
                        if (type == 'halfprice')
                        {
                            var url='w_halfprice_carlist';
                        }else if (type == 'buy')
                        {
                            var url='w_carlist';
                        }
                        var is_car = 1;
                        uxl_track(url+'/'+t_url+'/'+keyword+'/result/'+result_num+'/iscar/'+is_car);
                    }
                }
            }
	            fix_channel_div();
        $('.list-con .con').on('hover',function(){
            $(this).children('.collect-star').toggle();
            $(this).children('.compare-box').toggle();
            //$(this).find('.tit').attr({ 'href':'javascript:void(0);', 'onclick':"return false;" });

        }).on('click', '.pad', function(ev){
            var conlink = $(this).parent().children('.aimg').attr('href')
            if (typeof(conlink) != 'undefined') {
                if(ev.target.className != 'tit'){
                    window.open(conlink);
                }
            }
        });

        _init_carfavorite();
        $("#cDload").click(function(){
            $(".pop_ext").hide();
        });

        $("img.lazy").lazyload();
        var sTitle = '';
        try{
           sTitle =  decodeURIComponent("");
        }catch(e){
           sTitle = "" 
        }
        $("#none-txt-q").attr("title",sTitle).html(sTitle.substring(0,40));
