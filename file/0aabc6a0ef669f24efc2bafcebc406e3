<script>
    var is_hf = 'buy'=='halfprice' ? 1 : 0;
    var search_q = '';
    var search_ot = 'p1-2'.indexOf('y1') != -1 ? '全国其他城市' : ('p1-2'.indexOf('y2') != -1 ? '周边城市' : '');
    var search_count = '565';
    var zg_page_flag = 0;
    $('.select-filter a').live('click', function(){
        if (!(typeof($(this).attr('data-for'))=='undefined')) {
            zhuge_ckflag(1,'列表页-筛选');
            var zg_filter_suffix = $(this).attr('data-for').substr(-1,1);
            switch (zg_filter_suffix) {
                case 'b': {  // 品牌
                    var data_is_hot_brand = $(this).attr('data-is-hot-brand') ? $(this).attr('data-is-hot-brand') : 0;
                    uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/bid/' + $(this).attr('data-valueid') + '/hot/' + data_is_hot_brand);
                    zhuge_track(is_hf ? 'C-付一半列表页-品牌' : 'C-车辆列表页-品牌', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '品牌': $.trim($(this).text()) });
                    zhuge_track('品牌筛选', { '品牌值': $.trim($(this).text()), '来源页面':(is_hf ? '付一半列表页' : '车辆列表页') });
                } break;
                case 's': {  // 车系
                    var data_is_hot_series = $(this).attr('data-is-hot-series') ? $(this).attr('data-is-hot-series') : 0;
                    uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/carseries/' + $(this).attr('data-valueid')  + '/hot/' + data_is_hot_series);
                    zhuge_track(is_hf ? 'C-付一半列表页-车系' : 'C-车辆列表页-车系', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '车系': $.trim($(this).text()) });
                    zhuge_track('车系筛选', { '车系值': ($(this).parent().parent().find('th em').text() ? $(this).parent().parent().find('th em').text() + ' —— ' : '') + $.trim($(this).text()), '来源页面':(is_hf ? '付一半列表页' : '车辆列表页') });
                } break;
                case 'j': {  // 排量
                    zhuge_track('车辆列表页-排量', { '排量值': $.trim($(this).text()), '来源页面':(is_hf ? '付一半列表页' : '车辆列表页') });
                } break;
                case 'p': {  // 价格
                    uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/price/' + $(this).attr('data-valueid') );
                    zhuge_track(is_hf ? 'C-付一半列表页-价格' : 'C-车辆列表页-价格', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '价格': $.trim($(this).text()) });
                    zhuge_track('价格筛选', { '价格区间': $.trim($(this).text()), '来源页面':(is_hf ? '付一半列表页' : '车辆列表页') });
                } break;
                default : { };
            }
        }
    });
    $('.select-box .menu a').live('click', function(){
        if ($(this).attr('data-for') && $(this).attr('data-valueid')) {
            var filter_k = $(this).attr('data-for').substr(7,1);
            var filter_v = $(this).attr('data-valueid');
            var filter_t = $.trim($(this).text());
            if ($.inArray(filter_k, ['l', 'r', 'k', 'j', 'c', 'g', 'q'])!==-1 && filter_v) {
                switch (filter_k) {
                    case 'l': {  // 级别
                        uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/classlevel/' + filter_v );
                        zhuge_track(is_hf ? 'C-付一半列表页-车型' : 'C-车辆列表页-车型', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '车型': filter_t });
                    } break;
                    case 'r': {  // 车龄
                        uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/carage/' + filter_v );
                        zhuge_track(is_hf ? 'C-付一半列表页-车龄' : 'C-车辆列表页-车龄', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '车龄': filter_t });
                    } break;
                    case 'k': {  // 里程
                        uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/carmile/' + filter_v );
                        zhuge_track(is_hf ? 'C-付一半列表页-里程' : 'C-车辆列表页-里程', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '里程': filter_t });
                    } break;
                    case 'j': {  // 排量
                        uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/displacement/' + filter_v );
                        zhuge_track(is_hf ? 'C-付一半列表页-排量' : 'C-车辆列表页-排量', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '排量': filter_t });
                    } break;
                    case 'c': {  // 颜色
                        uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/colour/' + filter_v );
                        zhuge_track(is_hf ? 'C-付一半列表页-颜色' : 'C-车辆列表页-颜色', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '颜色': filter_t });
                    } break;
                    case 'g': {  // 变速箱
                        uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/carengine/' + filter_v );
                        zhuge_track(is_hf ? 'C-付一半列表页-变速箱' : 'C-车辆列表页-变速箱', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '变速箱': filter_t });
                    } break;
                    case 'q': {  // 座位数
                        uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/seat/' + filter_v );
                        zhuge_track(is_hf ? 'C-付一半列表页-座位数' : 'C-车辆列表页-座位数', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--', '变速箱': filter_t });
                    } break;
                    default : { };
                }
            }
        }
    });

    function hmt_search(type){
        if(!$.cookie('hmtsearch_record')){
            var searchrecord=1;   //  filterorderrecordfor;
            $.cookie('hmtsearch_type',  type, { path: '/', expires: 24*60*60*365+1 });
            $.cookie('hmtsearch_record', searchrecord , { path: '/', expires: 24*60*60*365+1 });
            _hmt.push(['_trackEvent', 'Filter', 'FO_'+searchrecord+'_'+type]);
        }else{
            if($.cookie('hmtsearch_type')!=type){
                var searchrecord=parseInt($.cookie('hmtsearch_record'))+1;
                $.cookie('hmtsearch_type',  type, { path: '/', expires: 24*60*60*365+1 });
                $.cookie('hmtsearch_record', searchrecord , { path: '/', expires: 24*60*60*365+1 });
                _hmt.push(['_trackEvent', 'Filter', 'FO_'+searchrecord+'_'+type]);
            }
        }
    }

    $(function(){
        var flag = '';
        var series_flag = '';
        var curBrandid = '';
        var curseriesid = '';
        if (curBrandid && !flag) {
            var html = '<dd class="selected"><a data-placeholder data-for="hidden_b" data-valueid="" data-spell = "" class="spell"></a></dd>';
            $("#select1 .select-all").after(html);
        }
        if (curseriesid && !series_flag) {
            var html = '<dd class="selected"><a data-placeholder data-for="hidden_s" data-valueid="'+curseriesid+'" class="search_search_click " title="全部品牌二手车"></a></dd>';
            $("#select2 .select-all").after(html);
        }
    });
</script>
