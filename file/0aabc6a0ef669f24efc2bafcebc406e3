                        $('.php_saled_car_mark').eq(0).before(str);
                        fix_channel_div('cityTwoBtn','class');
                    }); */
                }else if(($('.php_saled_car_mark').length == 0)&&(select.length >  0)&&(select.length < PAGE_SIZE)){
                    /* 去除跳转周边和全国其他城市的链接
