                    });*/
                }else if (page == 1){
                    if ($('.php_saled_car_mark').length == select.length){   //都是已售车源，需要修改
//                        $.post(searstr+'redirect/',{ 'searchstr':searchstr, 'q':q, type:'ajax'  },function(data) {
//                            if (data != -1){
//                                var keyword = $('#search_search').val().trim() == '请输入品牌、车系搜索' ? '' : $('#search_search').val().trim();
//                                keyword = keyword ? '?q=' + keyword : '';
//                                var url = data + keyword; // 附加请求参数
//                                //channel合作渠道链接处理
//                                if (location.search.indexOf('channel=') !== -1) {
//                                    if (url.indexOf('channel=') === -1) {
//                                        url = url + (url.indexOf('?') === -1 ? '?' : '&') + 'channel=' + (location.search.replace(/.*?channel=([^\s&]+).*/, "$1"));
//                                    }
//                                }
//                                window.location.href = url
//                            }else{
//                                $('.seatch-none').show();
//                            }
//                        });
                    }
                }
            }else if(searchstr.indexOf('y2') != -1){
                if (page == last_page){
                    $.post(searstr+'get_num/',{ 'searchstr':searchstr, 'cityid' : cityid,'q' : q, type:'ajax' },function(data){
                        var data = JSON.parse(data);
                        select.eq(select.length-1).after(data.html);
                    });
                }
                $('.cityTwoBtn').css("border-top",0);
                fix_channel_div('cityTwoBtn','class');
            }else if(searchstr.indexOf('y1') != -1){
                if (total == 0){
                    $('.seatch-none').show();
                }
                fix_channel_div('cityTwoBtn','class');
            }
        }else{
            if (select.length == 0){
                $('.seatch-none').show();
            }
        }

        //增加车系搜索量
        var brandid = "";
        var seriesid_xin = "";
        $.post("/apis/apis_ajax/add_search_series",{ brandid:brandid,seriesid_xin:seriesid_xin });
        //zg-code-begin
        var jschannel = (location.search.indexOf('channel=')!==-1) ? location.search.replace(/.*?channel=([^\s&]+).*/, "$1") : '';
        var zhuge_loc_flag = $.cookie('XIN_ZG_FROM_PAGE');
        if (zhuge_loc_flag) {
            zhuge_loc_flag = $.parseJSON(zhuge_loc_flag);
            zhuge_loc_flag = zhuge_loc_flag.page;
        } else {
            zhuge_loc_flag = jschannel ? '外部推广' : '直接访问';
        }
        zhuge_loc_flag = zhuge_loc_flag ? zhuge_loc_flag : '未知';
        if (jschannel) {
            zhuge_track(is_hf ? '付一半列表页' : '车辆列表页', { '来源页面':zhuge_loc_flag, '来源渠道':jschannel,'城市':'呼和浩特' });
        } else {
            zhuge_track(is_hf ? '付一半列表页' : '车辆列表页', { '来源页面':zhuge_loc_flag,'城市':'呼和浩特' });
        }
        $('.car-box a').bind('click', function(){
            if (is_hf) {
                zhuge_dckflag(1,'付一半列表页');
            } else {
                zhuge_dckflag(1,'列表页');
            }
        });
        zhuge_track('搜索关键词', {
            '搜索关键词': search_q ? search_q : '',
            '搜索结果数': search_count ? search_count : 0,
