            '搜索所在城市=搜索结果城市': search_ot ? '否' : '是'
        });
        if (zg_page_flag==1) {
            zhuge_track('访问页面', { '页面URL':location.href });
        }
        if (!is_hf) {
            $('.opt .opt-tab a').bind('click', function(){
                var filter_k = $(this).attr('data-for').substr(7,1);
                var filter_v = $(this).attr('data-v');
                if (filter_k == 'v' && filter_v) {
                    switch (filter_v) {
                        case '1' : {
                            uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/allcars' );
                            zhuge_track(is_hf ? 'C-付一半列表页-全部车源' : 'C-车辆列表页-全部车源', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--' });
                        } break;
                        case '6' : {
                            uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/personcars' );
                            zhuge_track(is_hf ? 'C-付一半列表页-个人车源' : 'C-车辆列表页-个人车源', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--' });
                        } break;
                        case '9' : {
                            uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/uxincarws' );
                            zhuge_track(is_hf ? 'C-付一半列表页-优信认证' : 'C-车辆列表页-优信认证', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--' });
                        } break;
                        case '4' : {
                            uxl_track( 'w_' + (is_hf ? 'halfprice_carlist' : 'carlist')  + '/dealercars' );
                            zhuge_track(is_hf ? 'C-付一半列表页-商家质保' : 'C-车辆列表页-商家质保', { '城市':TOP_INFO.location.cityname ? TOP_INFO.location.cityname : '--' });
                        } break;
                    }
                }
            });
        }
