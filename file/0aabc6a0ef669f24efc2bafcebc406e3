<script language="javascript"> 
    function countChar(textarea,span) {
        document.getElementById(span).innerHTML = document.getElementById(textarea).value.length; 
    }
    $('#uAsk').delegate('.simTab a', 'click', function(){
        $('#uAsk .simTab a').removeClass('active');
        $(this).addClass('active');
    });
    $('#uAsk .titTab').delegate('a:not(.active)', 'click', function(){
        $('#uAsk .titTab a').removeClass('active');
        $(this).addClass('active');
        var thisIndex = $(this).index();
        $('#uAsk .simTab').removeClass('active').hide();
        $('#uAsk .simTab:eq('+thisIndex+')').addClass('active').show();
        $('#uAsk .simTab a').removeClass('active');
    });
    $(function(){
        $('#uAsk .submit-btn').on('click', function ajaxSubmit(){
            var idArr = [];
            var title = $('#suptare').val();
            var pid = $('#uAsk .titTab .active').attr('data_type');
            var idObj = $('#uAsk .simTab .active').each(function(){
                idArr.push($(this).attr('data_type'));
            });
            var id = idArr.toString();
            if (!title || !pid || !id) {
                $('#uAsk .tip span').html('信息填写不完整');
                return false;
            }
            var title_filter = title.match(/[a-zA-Z0-9\u4e00-\u9fa5]/g);
            if (!title_filter || title_filter.length > 240 || title_filter.length < 6) {
                $('#uAsk .tip span').html('标题长度应在6〜240个字');
                return false;
            }
            $.ajax({
                type: "POST",
                url: "/qa/ask/",
                data: { "title":title,"pid":pid,"id":id},
                dataType: 'json',
                beforeSend: function(){
                    $('#uAsk .submit-btn').html('正在提交...');
                    $('#uAsk .submit-btn').unbind('click');
                },
                success: function(data) {
                    if (data.code == 1) {
                        $('#uAsk,.shade').hide();
                        $('#suptare').val('');
                        $("#uAsk .titTab a").removeClass('active');
                        $("#uAsk .titTab a:eq(0)").addClass('active');
                        $('#uAsk .simTab').removeClass('active').hide();
                        $('#uAsk .simTab:eq(0)').show();
                        $('#uAsk .tip span').html('');
                    }else{
                        $('#uAsk .tip span').html(data.message);
                    }
                    $('#uAsk .submit-btn').html('提交问题').on('click', ajaxSubmit);
                },
                error: function() {
                    $('#uAsk .tip span').html('请求失败!');
                    $('#uAsk .submit-btn').html('提交问题').on('click', ajaxSubmit);
                }
            });
        });

        $('#question_button').one('click', function(){
            if (!TOP_INFO.user.mobile){
                return false; //未登录的情况，不加载我要提问中的标签数据
            }
            $.ajax({
                url: "/qa/get_category",
                dataType: "json",
                success: function(data) {
                    if(data && data.data && data.data[0]){
                        for (var i in data.data[0]) {
                            var type = data.data[0][i][0];
                            if(!type || !data.data[0][i][1] || !data.data[type]) {
                                continue;
                            }
                            //var index = type-1;
                            if (i == 0) {
                                var addclass = 'active';
                                var display = '';
                            } else {
                                var addclass = '';
                                var display = 'style="display:none;"';
                            }
                            
                            $("#uAsk .titTab").append('<a href="javascript:void(0)" class="'+addclass+'" data_type='+type+'>'+data.data[0][i][1]+'</a>');
                            var tag = '<p class="simTab ' + addclass + '"' + display +'>';
                            for (var j in data.data[type]) {
                                if(!data.data[type][j][0]) {
                                    continue;
                                }
                                tag += '<a href="javascript:void(0)" data_type='+data.data[type][j][0]+'>'+data.data[type][j][1]+'</a>';
                            }
                            $("#uAsk .tag").append(tag + '</p>');
                        }
                    }
                }
            });
        });
    });
</script>

