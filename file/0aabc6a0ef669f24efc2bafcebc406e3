                    var act_arr = window.location.pathname.split('/');
                    for (i = act_arr.length - 1; i >= 0; i--) {
                        if (!!act_arr[i]) {
                            window.location.href = mdomain + '/article/article_detail/' + act_arr[i] + param;
                            break;
                        }
                    }
                }else if (con_name == 'article' && act_name == 'listing') { //活动列表页面跳转
