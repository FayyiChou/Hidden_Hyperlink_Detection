    //----------- 诸葛 end -----------
    
    // 修复channel链接，针对div
    function fix_channel_div(e,t) {
        var sCase = getSearchParam('case');
        var sChannel = getSearchParam('channel');
        var sLpopt = getSearchParam('lpopt');
        var aParam = [];
        if(sChannel){
            sChannel = 'channel=' + sChannel;
            aParam.push(sChannel)
        }
        if(sCase){
            sCase = 'case=' + sCase;
            aParam.push(sCase)
        }
        if(sLpopt){
            sLpopt = 'lpopt=' + sLpopt;
            aParam.push(sLpopt)
            console.log();
        }
        if (aParam.length) {
            var ela = '';
            if (t=='id') {
                ela = ('#'+e + ' ');
            } else if (t=='class') {
                ela = ('.'+e + ' ');
            }
            $(ela + 'a').each(function () {
                var $this = $(this),sHref = $this.attr('href');
                if(sHref && filterUrl(sHref)){
                    if(sHref.indexOf('channel=') === -1 && sChannel){
                        $this.attr('href',sHref + (sHref.indexOf('?') === -1 ? '?' : '&') + sChannel )
                    }
                    if(sHref.indexOf('case=') === -1 && sCase){
                        $this.attr('href',sHref + (sHref.indexOf('?') === -1 ? '?' : '&') + sCase )
                    }
                    if(sHref.indexOf('lpopt=') === -1 && sLpopt){
                        $this.attr('href',sHref + (sHref.indexOf('?') === -1 ? '?' : '&') + sLpopt )
                    }
                }
            });
        }

        function filterUrl(url){
            if(url == '#' || url.indexOf('javascript') == 0 || url.indexOf("tel:") == 0){
                return false;
            }
            return true;
        }
    }
    // 修复channel链接，针对单独的url
    function fix_channel_url(url) {
        var channel = "{$channel}";
        channel = location.search.replace(/.*?channel=([^\s&?]+).*/, "$1");
        channel = channel == location.search ? '' : channel;
        if(channel && url && (url.indexOf('channel=') === -1)) {
            if(!((url=='#') || (url.indexOf('javascript')==0))){
                url += (url.indexOf('?') === -1 ? '?' : '&') + 'channel=' + channel;
            }
        }
        return url;
    }
    //----------- ab start -----------
    function uxl_track(event) {
        var ux_event = (event != 'undefined') ? $.trim(event) : '';
        if (ux_event.length > 0) {
            if(_abq) _abq.push(['_track'].concat(ux_event.split('/')));
        }
    }    
    //----------- ab end -----------
    function getSearchParam(name){
        var reg = new RegExp("(^|&)"+ name +"=([\\w\\d_]*)(&|$)");
        var res = window.location.search.substr(1).match(reg)
        return (res && decodeURIComponent(res[2])) || '';
    }
</script>
