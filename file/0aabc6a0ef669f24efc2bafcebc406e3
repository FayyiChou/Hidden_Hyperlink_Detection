    //Web端全站诸葛打点
    function zhuge_track(event, properties,href) {
        if (typeof(event)=='string' && event.length>0 && event.length<=32) {
            if (properties && typeof(properties)=='object') {
                if(href){
                    zhuge.track(event, properties,function(){
                        window.location.href = href
                    });
                    return false;
                }else{
                    zhuge.track(event,properties);
                }
            } else {
                zhuge.track(event);
            }
        }
        return true;
    }
     // 新打点:使用自定义属性,把数据保存在标签上,通过绑定事件去获取数据然后打点
    (function ($){
        if(!$) {
          console.warn('缺少zepto或者jquery库');return;
        }
        $(function(){
            if(!zhuge || !zhuge.track ) return;
            $(document).on('click','.xinStatistics',function(){
                var $this = $(this),data;
                if(data = $this.attr('data-statistics')){
                    try{
                        data = $.parseJSON(data);
                    }catch(e){
                        console.warn('JSON格式的字符串解析错误',e);
                        return;
                    }
                    var event = data.title || '';
                    var properties = data.data || '';
                    var href =  $this.attr('href');
                    if(event){
                        if(properties){
                            if(href){
                                zhuge.track(event, properties ,function(){
                                    window.location.href = href
                                });
                                return;
                            }
                            zhuge.track(event, properties);
                        }else{
                            zhuge.track(event);
                        }
                    }
                }
            });
        });
    })(window.jQuery || window.Zepto);
    //cookieflag，设置cookie，zhuge统计使用 (type:1 设置；type:0 清除; flag:来源页面名称设置)
    function zhuge_ckflag(type,flag) {
        var ret = 0;
        type = parseInt(type);
        flag = $.trim(flag);
        if (type==1 && flag) {
            ret = 1;
            $.cookie('XIN_ZG_FROM_PAGE', '{"page": "'+flag+'","url": "'+location.href+'"}', { path: '/', expires: 0 });
        } else if (type==0) {
            ret = 1;
            $.cookie('XIN_ZG_FROM_PAGE','', { path: '/', expires: 1 });
        }
        return ret;
    }
    
    //detail_cookieflag，设置cookie，zhuge统计使用 (type:1 设置；type:0 清除; flag:来源页面名称设置), For:车辆详情页
    function zhuge_dckflag(type,flag) {
        var ret = 0;
        type = parseInt(type);
        flag = $.trim(flag);
        if (type==1 && flag) {
            ret = 1;
            $.cookie('XIN_CD_ZG_FROM_PAGE_PC', '{"page": "'+flag+'","url": "'+location.href+'"}', { domain:'.xin.com', path: '/', expires: 0 });
        } else if (type==0) {
            ret = 1;
            $.cookie('XIN_CD_ZG_FROM_PAGE_PC','', { domain:'.xin.com', path: '/', expires: 1 });
        }
        return ret;
    }
