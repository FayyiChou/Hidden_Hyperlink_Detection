        var page = '2';
        var last_page = '0';
        var searchstr = 'p1-2';
        var q = '';
        var viewtype = '';
        var cityid = "1802";
        var cityname = "包头";
        var column = "" || '';
        var perimeter = "" || '';
        var total = '565';
        var PAGE_SIZE = '40' || 40;
        if ((currpage == 'buy')|| column){
            var searstr = '/search/';
        }else{
            var searstr = '/halfprice/';
        }

        /*
         *周边城市判断逻辑：
         *1. 本城市仅有已售车辆
         *      a. 本城市仅有已售车辆，则判断周边城市，如果周边城市有在售车辆，则直接跳转到周边城市；
         *      b. 如果周边城市无在售车辆，则判断全国其它城市，如果全国其它城市有在售车辆，则直接跳转到全国其它城市；
         *      c. 如果全国其它城市无在售车辆，则直接显示搜索结果为空的界面；
         *2.  本城市有在售和已售车辆
         *      a. 如果周边城市和全国其它城市都有在售车辆，则显示两个按钮；
         *      b. 如果仅全国其它城市有在售车辆，则显示一个跳转按钮；
         *3.  搜索字符串中，y1代表全国其它城市，y2代表周边城市
         */
        if (column == 17){
            var select = $('.car-ext');
        }else if (viewtype != 1){ //展示方式1横排2竖排
            var select = $('.con');
        }else{  //展示方式1横排
            var select = $('.car-vtc');
        }
        if (cityid != 0){
            //处理本城市数据
            if (!searchstr||(searchstr.indexOf('y') == -1)||(column == 17)){
                if (($('.php_saled_car_mark').length > 0)&&($('.php_saled_car_mark').length < select.length )){
                    /* $.post(searstr+'get_num/',{ 'searchstr':searchstr, 'cityid' : cityid,'q' : q, type:'ajax' },function(data){
