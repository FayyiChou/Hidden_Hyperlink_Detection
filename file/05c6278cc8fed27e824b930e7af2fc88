function rollText(n){
clearInterval(rollText_tt);
tn++;
if (tn>=tli.length){tn=0;}
rollText_tt=setInterval("rollText(tn)",4000); 
for (var j=0; j<tli.length; j++){
tli[j].style.display=j==n?"block":"none";
}
} 
