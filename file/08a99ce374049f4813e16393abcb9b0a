			);
	}
	function InitMap() {
		maplet = new Maplet("mapbarmap");
		if(marker) {
			maplet.addOverlay(marker);
		}
		maplet.showOverview(false);  
		maplet.centerAndZoom(point, 11);
	};
	InitMap();
