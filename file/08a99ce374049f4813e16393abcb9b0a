	var maplet, marker, pstr, point, lat, lng;
	pstr = '北京市';
	point = new MPoint(pstr);
	lat = +'114.929884';
	lng = +'24.451161';
	if(lat && lng) {	
		pstr = lat + ',' + lng;
		point = new MPoint(pstr);
		marker = new MMarker(
			point, 
