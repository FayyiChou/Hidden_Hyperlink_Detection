    var ssoLoginOutURL = "https://uniportal.huawei.com/uniportal/?redirect=";
    //跳转到 uniport, encodeURIComponent方式编码
    function forLogin(){
    	window.location.href = ssoLoginOutURL + encodeURIComponent(encodeURIComponent(location.href));
    }
    
    /**
     * 打开新窗口登陆 add by Michael Jr. Zhu 2012.11.17 p.m
     */
    function openNewWindowForLogin() {
    	var myLang = "zh";
    	var pageJSP = (myLang == "zh") ? "success.jsp" : "success_en.jsp";
    	
    	var redirectURL = getMyOriginURL() + "/cbb/independentRD/pages/sessionCheckReBuild/jsp/" + pageJSP;
    	var url = ssoLoginOutURL + redirectURL;/* encodeURIComponent(encodeURIComponent(location.href)); */
    	window.open(url, 'Login');
    }
    
    //跳转到 uniport, escape()方式编码（中文参数需要3次用escape编码）
    function forLoginEscapeParam() {
		var uri = location.href;
    	uri = encodeURI(encodeURI(encodeURI(uri))).replace("#","%23").replace(/&/g,"%26");
    	window.location.href = ssoLoginOutURL + uri;
    }
    
     getMyOriginURL = function() {  
	    //获取当前网址，如： http://localhost:8083/enterprise/share/meun.jsp  
	    var currURL=window.document.location.href;  
	    //获取主机地址之后的目录，如： enterprise/share/meun.jsp  
	    var pathName=window.document.location.pathname;  
	    var pos=currURL.indexOf(pathName);  
	    //获取主机地址，如： http://localhost:8083  
	    var hostPath=currURL.substring(0,pos);  
	    //获取带"/"的项目名，如：/enterprise  
	    var projectName=pathName.substring(0,pathName.substr(1).indexOf('/')+1);  
	    return (hostPath+projectName);  
	};
</script>
