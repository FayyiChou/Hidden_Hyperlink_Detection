	//语种切换,nls为希望切换到该语种的变量，中文：zh,英文：en
	function changeLang(nls) 
	{
		//上下文路径
		var contextPath = "/enterprise";
		var ajaxPid = "";
		var ajaxIdAbsPath = "";
		//模块
		var mid = $("#mid").val();
		//导航树的pbi节点
		if($.bbq != undefined){
			ajaxPid = $.bbq.getState("pid");
			ajaxIdAbsPath = $.bbq.getState("idAbsPath");
		}
		//语种切换前的url，不包括请求参数，即url的?号前面部分
		var _url = window.location.href.indexOf("?")>0?window.location.href.substr(0,window.location.href.indexOf("?")):window.location.href;
		_url = _url.indexOf("#")>0?_url.substr(0,_url.indexOf("#")):_url;
		var reUrl = _url;
		//语种切换前的url的请求参数，即url的?号后面部分
		var qryStr = "null";
		qryStr = qryStr.replace(/amp;/g,"");
		//调用语种切换的处理方法,在layout.js文件
		//因为在单独的js文件里面，无法用request对象，所以将必要的参数在jsp获取后传入
		processChangeLang(nls, contextPath, mid, ajaxPid, ajaxIdAbsPath, reUrl, qryStr);

	}
</script>


