<script type="text/javascript">
var url = 'e.huawei.com';
var surl = 'support.huawei.com';
var enterpriseHeadUrl = '/enterprise/Main!getStaticHeadHtml.action?language=zh';
//加载HTML
$.ajax({
  url: enterpriseHeadUrl,
  cache: true,
  async: false,
  dataType: "text",
  success: function(data){
	  if(null == url ||  "" == url){
		   url = "e.huawei.com";
	  }
      if(null == surl || "" == surl){
		  surl = "support.huawei.com";
	  }

	  var regSc= /<script(?:\s+[^>]*)?>([\s\S]*?)<\/script\s*>/igm;
	  var regCc= /<link(?:\s+[^>]*)?\/>/igm;
	  var shref = /support.huawei.com(?:\s+[^>]*)?/ig;
	  var zhref = /href=\"(\/.*\")/ig;
	  var zhref1 = /href=\'(\/.*\')/ig;
	  var containerId = /<div id=\"container\"(?:\s+[^>]*)?>/ig;
	  var mainId = /<div id=\"main\" class=\"bgf6f6f6\">/ig;
	  var hreflogin = /<ul class=\"head_link\"(?:\s+[^>]*)?>([\s\S]*?)<\/ul\s*>/igm;
	  var clearfix = /clearfix(?:\s+[^>]*)?/ig;
	  var shtml = data.replace(regSc, "").replace(regCc,"").replace(shref, surl);
	  if(enterpriseHeadUrl.indexOf("share-head") > 0 ){
	  	var imgSrc1 = /<img(?:\s+[^>]*)?src=\"\/(?:\s+[^>]*)?/ig;
	  	shtml.replace(imgSrc1,"<img src=\"http:\/\/"+url+"\/");
	  }
	  shtml = shtml.replace(zhref,"href=\"http:\/\/"+url+"$1")
	  	.replace(zhref1,"href=\'http:\/\/"+url+"$1")
	  	.replace(mainId,"<div class=\"bgf6f6f6\">")
	  	.replace(hreflogin,"")
	  	.replace(containerId,"<div style=\"background-color: #fff;\">");//.replace(clearfix,"clearfixEnt");
     $("#headerDIV").html(shtml);
	  //$("#searchSupportE").replaceAll("#headerDIV #search_1").show();
	   $("#headerlinks").replaceAll("#headerDIV #loginLinks").show();
	  $("#searchSupportE").replaceAll("#headerDIV .search").show();
	  $("#headerDIV .nav-cont-wrap").css("height","345px");
	  $("#logo img").attr("alt","Huawei");
	  //channel links
	  var channel_link = $(".e_tabs_li");							
	  channel_link.click(function(){
		window.location = "http://"+url+"/cn/partners/index.htm?tabs="+$(this).attr("name");	
	  });
	  var contactUs_link = $("#contactUs");							
	  contactUs_link.click(function(){
		window.location = "http://"+url+$(this).attr("name");	
	  });
	  var selectRegionLang_link = $("#selectRegionLang");							
	  selectRegionLang_link.click(function(){
		  var toUrl = "http://"+surl+$(this).attr("name");
			selectRegionLang_link.attr("href", toUrl);	
	  });
	  $('.navlist div a').css("font-size", "16px");
	  $('.navlist div a') .each(function(){
		   if($(this).attr("href") != undefined && $(this).attr("href") != null && $(this).attr("href").indexOf(surl+"/enterprise") > 0){
			   $(this).css('color', '#cc0000');
			   return;
		    }
		});
  }
});


</script>


