<script type="text/javascript">
$(function() {
	$('#currentPage').append('<div id="cp_submenu"><a id="" href="/parlons" title="一起说法语">说法语</a><a id="lpp" href="/lpp" title="小王子 Le Petit Prince">小王子</a><a href="/dict/photo" title="权威法语词典">法语图解词典</a><a href="/recette" title="法国美食">法国美食</a></div>');

	var radio = $(this).next('audio').get(0);
	$('#homeRadio li').each(function() {
		var audio = $('audio', this)['0'];
		var play_pause = $('.rplay_pause', this);

		play_pause.on('click', function() {
				pauseAllAudio(radio);
				audio.play();
		});

		audio.addEventListener("play", addSpinner, false);
		function addSpinner() {
			play_pause.on('click', function(){
				audio.pause();
			});
			play_pause.css('background', 'url(/static/img/radio/rplay.png) no-repeat');
		}
		audio.addEventListener("pause", removeSpinner, false);
		function removeSpinner() {
			play_pause.on('click', function(){
				pauseAllAudio(song);
				audio.play();
			});
			play_pause.css('background', 'url(/static/img/radio/rpause.png) no-repeat');
		}
	});
	var allAudioEls = $('audio');
	function pauseAllAudio(exclude) {
	    allAudioEls.each(function() {
	        var a = $(this).get(0);
	        if(a !== exclude) {
	            a.pause();
	        }
	    });
	}
});
</script>
