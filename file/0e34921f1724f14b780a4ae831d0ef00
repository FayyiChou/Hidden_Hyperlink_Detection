<SCRIPT language="JavaScript">
	jQuery.noConflict();
	jQuery(document).ready(function(){
        
		var z =jQuery("#s1").html();
		jQuery("#s14").html(z);
		
		function changeImg(){
		//jQuery(".zq").eq(2).css({"top":"0px","position":"relative"});
		jQuery("#s1").animate({top:"-=60px"},function(){ 
				
			
			var top = jQuery('#s1').position().top;
			 if(top==-240){ 
				  jQuery("#s1").css({"top":"240px","position":"relative"});
				  
			 }
			});
		jQuery("#s14").animate({top:"-=60px"},function(){ 
				
			
			var topz = jQuery('#s14').position().top;
			 if(topz==-240){ 
				  jQuery("#s14").css({"top":"0px","position":"relative"});
				  
			 }
			});
		}
		setInterval(changeImg,5000);		
	});
	</script>
<script type="text/javascript" language="javascript">

var curtab=2;
function turnTab(id)
{
	if(id!=curtab)
	{
	document.getElementById("mz"+curtab).className="";
	document.getElementById("tab"+curtab).style.display="none";
	document.getElementById("mz"+id).className="mzbg";
	document.getElementById("tab"+id).style.display="block";
	curtab=id;
	}
}
</script>
<script type="text/javascript" language="javascript">
var isIE = function(ver){
	    var b = document.createElement('b')
	    b.innerHTML = '<!--[if IE ' + ver + ']><i></i><![endif]-->'
	    return b.getElementsByTagName('i').length === 1
	}
	if(isIE()&&(isIE(6)||isIE(7)||isIE(8))){
	    jQuery("#hb1 ").hide();
	}         
</script>
