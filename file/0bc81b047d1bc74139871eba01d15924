    <script type="text/javascript">
	var hfcsSH = 0;
	jQuery('.homepage-featured-companies-section .single-column').css('min-height','inherit');
	jQuery('.homepage-featured-companies-section .single-column').each(
	function(){
		if(hfcsSH < jQuery(this).outerHeight()){
			hfcsSH = jQuery(this).outerHeight(); 
		}//end if
	});
	jQuery('.homepage-featured-companies-section .single-column').css('min-height', hfcsSH + 'px');
	
	/* RESIZE */
	jQuery(window).resize(
	function(){
		hfcsSH = 0;
		jQuery('.homepage-featured-companies-section .single-column').css('min-height','inherit');
		jQuery('.homepage-featured-companies-section .single-column').each(
		function(){
			if(hfcsSH < jQuery(this).outerHeight()){
				hfcsSH = jQuery(this).outerHeight(); 
			}//end if
		});
		jQuery('.homepage-featured-companies-section .single-column').css('min-height', hfcsSH + 'px');
	});
	</script>
