		jQuery('ul .has-child > a').click(
		function(e){
			e.preventDefault();
			jQuery(this).parent().find('.sub-menu').slideToggle();
			if(jQuery(this).parent().hasClass('open-sub-menu'))
				jQuery(this).parent().removeClass('open-sub-menu')
			else
				jQuery(this).parent().addClass('open-sub-menu')
		});
