<script type="text/javascript">
/* ADD COMPANY */

jQuery('.add-company-button').click(
function(e){
	e.preventDefault();
	jQuery.blockUI({ 
		message: jQuery('#add-company-wrapper') ,
		css: {
			backgroundColor: 'rgba(170, 170, 170, 0.7)',
			border: 'none',
			width: '80%',
			height: '600px',
			left: '10%',
			top: '5%',
			padding: '20px',
			cursor: 'default'
		}
	});		
});	

/* CANCEL AND CLOSE */

jQuery('#add-company-wrapper').on('click','#close,#cancel',
function(){
	jQuery('#add_company input[type="text"],#add_company input[type="email"],#add_company textarea').val(' ');
	jQuery.unblockUI(); 
});//end close click

/* SUBMIT FORM */

jQuery('#add_company_form').submit(
function(e){
	e.preventDefault();
	var form = jQuery(this);
	jQuery.ajax({
		url: ajaxurl,
		type: 'POST',
		data:{
			action: 'add_company_submit',
			formData: form.serializeArray()
		},
		dataType: 'json',
		beforeSend:function(){
			jQuery.blockUI({ 
				message: 'Please Wait ....',
				css: {
					backgroundColor: '#FFF',
					border: 'none',
					padding: '20px',
					cursor: 'default',
					border: '3px solid #CCC',
					fontSize: '20px',
					fontWeight: 'bold'
				}
			});
		},
		success:function(data){
			jQuery('#add_company_form')[0].reset();
			if(data.status == 'error'){
				jQuery.blockUI({ 
					message: 'Error',
					css: {
						backgroundColor: '#FFF',
						border: 'none',
						padding: '20px',
						cursor: 'default',
						border: '3px solid #CCC',
						fontSize: '20px',
						fontWeight: 'bold'
					}
				});	
				setTimeout(jQuery.unblockUI, 2000); 						
			}
			else{
				jQuery.blockUI({ 
					message: 'Success',
					css: {
						backgroundColor: '#FFF',
						border: 'none',
						padding: '20px',
						cursor: 'default',
						border: '3px solid #CCC',
						fontSize: '20px',
						fontWeight: 'bold'
					}
				});	
				
				setTimeout(jQuery.unblockUI, 2000); 
			}
		}
	});
});
</script></div><!--#main-->
