        <script>
	
	function sunmitphone(){
        var city = $("select[name=province] option:selected").val();
		var keshi = $("select[name=department] option:selected").val();
		var tel = $("input[name=phonenumber]").val();
		var xiangxi = $("textarea.xiangxi").val();
		var xingbie = $(".gender input[name=gender]:checked").val();
		if(xiangxi=='请您尽可能详细描述您的症状，以便得到医生更好的解答。'){
			xiangxi='';	
		}

		
		$.get('freephone/submitphoneloft/province-'+city+'__department-'+keshi+'__phonenumber-'+tel+'__xiangxi-'+xiangxi+'__xingbie-'+xingbie, function (result)
		{		
			alert(result)
		});
		
	}
	
	
	</script>
        
