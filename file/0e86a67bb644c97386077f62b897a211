<script>
$(function(){
	
	$(".banner-new ul li:first").show();	
	$(".banner-new dl dd").mouseover(function(){
		var num = $(this).index();
		$(this).addClass("current").siblings("dd").removeClass("current");
		$(".banner-new ul li").eq(num).show().siblings("li").hide();
	});
	
	
	$(".header-nav ul li.down-nav").hover(function(){
		$(".header-nav-down").show().hover(function(){
			$(this).show();
			$(".header-nav ul li.down-nav").addClass("active");
		},function(){
			$(this).hide();	
			$(".header-nav ul li.down-nav").removeClass("active");
		});
	},function(){
		$(".header-nav-down").hide();	
		$(".header-nav ul li.down-nav").removeClass("active");
	});
	
	
	var timer;
	var name;
	$(".sub-more-nav-down dl").hover(function(){
		clearTimeout(timer);
		name = $(this).attr("name");
		$(".sub-more-nav-down-more").show().find("."+name).show().siblings(".keshilist").hide();
		$(".sub-more-nav-down-more").hover(function(){
			clearTimeout(timer);
			$(".sub-more-nav-down dl[name="+name+"]").addClass("active");
		},function(){
			$(this).hide();
			$(".sub-more-nav-down dl").removeClass("active");	
		});
	},function(){
		timer = setTimeout(function(){
			$(".sub-more-nav-down-more").hide();
		},600);
		$(".sub-more-nav-down dl").removeClass("active");
	});
		
		
	$(".agents-or-iseed .agents-or-iseed-list .item:first").show().siblings(".item").hide();
	$(".agents-or-iseed .page-panel-header h3").click(function(){
		var num = $(this).index();
		$(this).addClass("active").siblings("h3").removeClass("active");
		$(this).parents(".agents-or-iseed").find(".agents-or-iseed-list .item").eq(num).show().siblings(".item").hide();
	});
	
	$(".intellectual-products ul li").hover(function(){
		var num = $(this).index();
		$(this).addClass("active").siblings("li").removeClass("active");
		$(this).parents(".intellectual-products").find("dl dd").eq(num).show().siblings("dd").hide();
	});
	
	var topImgSsrc;
	$(".header_new .topNav a.ico").hover(function(){
		topImgSsrc = $(this).find("img").attr("src");
		var thisHoSrc = $(this).find("img").attr("data-hover");
		$(this).find("img").attr("src",thisHoSrc);
	},function(){
		$(this).find("img").attr("src",topImgSsrc);	
	});
	
	$(".doctor-zixun-qa dl").hover(function(){
		$(this).find("dd").slideDown().parents("dl").siblings("dl").find("dd").slideUp();
	});
	
	$(".doctor-zixun-tab ul li:first").addClass("active");
	$(".doctor-zixun-item:first").show().siblings(".doctor-zixun-item").hide();
	$(".doctor-zixun-tab ul li").hover(function(){
		var name =$(this).attr("name");
		$(this).addClass("active").siblings("li").removeClass("active");
		$(".doctor-zixun-item[name="+name+"]").show().siblings(".doctor-zixun-item").hide();
	});
	
	$(".bottom-tips em").click(function(){
		$(".bottom-tips").slideUp();
	});
	

});
</script>

<script>
function select_region_area(){
	var id=$('#area_change').val();
	
	$.get(G_BASE_URL + '/remotemedical/arae/value-' + id, function (data)
	{
		$('#city_change').html(data);
	});
	
	
}

function select_hospital(){
	var id=$('#city_change').val();
	
	$.get(G_BASE_URL + '/remotemedical/hospital_select/value-' + id, function (data)
	{
		$('#hospital_list').html(data);
	});
	
	
}

function jump_url(){
	var id=$('#hospital_list').val();
	window.open(G_BASE_URL +"/remotehd"+ id +".html","_blank"); 
	
}
</script>
