var speed1=30
var de1 = document.getElementById('de1');
var de2 = document.getElementById('de2');
var de = document.getElementById('de');
de2.innerHTML=document.getElementById('de1').innerHTML
var n=0;
var t = 0;
function Marquee1a(){
	var t_p = t;
	t = parseInt(de2.offsetWidth)-parseInt(de.scrollLeft)
	//if(t==t_p){ alert(2);}
	if(t<=0 || t==t_p) {
		de.scrollLeft-=de1.offsetWidth;
		//alert(1);
	} else {
		de.scrollLeft++;
	}
	//document.getElementById('disp').innerHTML=demo.scrollLeft+'PX'+demo1.offsetWidth+'PX'+demo2.offsetWidth+"a"+(n++);
}
var MyMar1a=setInterval(Marquee1a,speed1)
de.onmouseover=function() {clearInterval(MyMar1a)}
de.onmouseout=function() {MyMar1a=setInterval(Marquee1a,speed1)}
