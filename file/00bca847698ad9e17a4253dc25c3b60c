    <script language="javascript" type="text/javascript">
        function SetCurNum(cn, tn) {
            curnum = cn;
            for (var i = 1; i < tn + 1; i++) {
                if (i != cn) {
                    $('#li_' + i).css("background-color", "").css("color", "");
                }
            }
        }
        function SettingClass(CurNum, TotalNum) {

            for (var i = 1; i < TotalNum + 1; i++) {
                if (i == CurNum) {
                    $('#li_' + i).css("background-color", "#096").css("color", "#fff");
                }
                else {
                    $('#li_' + i).css("background-color", "").css("color", "");
                }
            }

        }
        function getNum(totalnum) {
            if (curnum >= totalnum) {
                curnum = 1;
            }
            else {
                curnum += 1;
            }
            SettingClass(curnum, totalnum);
            return curnum;
        }
        setInterval(function () { subTab(this, 'brt', '', 'zzz_', getNum(4)) }, 5000);
    </script>
    
