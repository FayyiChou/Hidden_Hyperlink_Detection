<script type="text/javascript">
$(function(){
    
	$(".cn_icon_js").each(function(){
		var cnwidth = $(this).width();
        if(cnwidth==29){
			$(this).css("padding-left","52px");
		}
        if(cnwidth==58){
			$(this).css("padding-left","38px");
		}
        if(cnwidth==87){
			$(this).css("padding-left","23px");
		}
        if(cnwidth==116){
			$(this).css("padding-left","9px");
		}
    });
	
    $(".click").live('click', function(){
        $(this).parent().prev('ul').find('.more_room').show();
        $(this).attr('class', 'clicked').html('收起房型');
    });

    $(".clicked").live('click', function(){
        $(this).parent().prev('ul').find('.more_room').hide();
        $(this).attr('class', 'click').html('更多房型');
    });
    
    window.map_initialize = function() { 
        $.getScript("//static.qyer.com/js/map.js");
    }  

    var list = {"108":{"hotel_id":"108","name":"\u502b\u6566\u674e\u516c\u9928","lat":"51.465961","lng":"-0.021658","url":"http:\/\/www.qyer.com\/shop\/guesthouse\/108\/","address":"Undercliff Road London SE13 7TT\r\n","cover_pic":"\/\/static.qyer.com\/upload\/poi\/hotel\/cover_pic\/9d\/0a\/9d0ad2ef6872631bf36f88dbbf1a5aec.t.80_80.jpg","min_price":"19.00","currency_alias":null}};
    $("#show_all_hotel_map").bind("click", function(){
        show_hotel_Map(list);
    });

    $(".show_hotel_map").click(function(){
        var hotel_id = $(this).data('hotel_id');
        var t = [];
        t.push(list[hotel_id]);
        show_hotel_Map(t);
    });

});
</script>

