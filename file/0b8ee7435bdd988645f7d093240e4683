                <script>
            $(function(){
                var tabs = $("#cn_filter_tit");
                var cnts = $("#cn_filter_cnts");
                var index = -1;
                tabs.on("click","a",function(){
                    var order = $(this).attr("data-order");
                    if(index != order){
                        index = order;
                        tabs.find("li").eq(""+order+"").addClass("current").siblings("li").removeClass("current");
                        cnts.find("ul").eq(""+order+"").show().siblings("ul").hide();
                    }
                });
            });
        </script>
