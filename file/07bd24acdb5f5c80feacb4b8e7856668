<script type="text/javascript">

    var fn_after_loginSuccess = null;

    function closeLoginDialog() { $('.logindialog').hide();}

    function loginResponse2(resp, statusText, xhr, form) {
        hideStatus();
        var respUrl = '';
        if (xhr.responseXML != undefined) {
            respUrl = xhr.responseXML.URL;
        }
        var result = eval('(' + resp + ')');
        if (result.Success == 1) {
            closeLoginDialog();
            if (fn_after_loginSuccess != null) { fn_after_loginSuccess(); GetLoginStatus();}
            else hiAlert("登录成功！", "提示", function () { GetLoginStatus(); });
        } else {
            if (result.AddtionalData == "true") {
                $('.login_vcode').show();
                $('#formLogin2 input[name="VCode"]').addClass("required");
                cimg();
            }
            hiAlert(result.Message, "提示");
        }
    }

    $(function () {
        $('.popbox .close').click(function () { $('.popbox').hide(); });

        $('#formLogin2').validate({
            ignore: "",
            errorClass: "error2",
            errorPlacement: function (error, element) {
                switch (element.attr("name")) {
                    default:
                        error.insertAfter(element);
                        break;
                }
            },
            success: function (label) {
                label.html("&nbsp;").addClass("ok2");
                try {
                    label.parent().find(".prompt-info").hide();
                } catch (ex) { }
            },
            highlight: function (element, errorClass) {
                $(element).parent().find(".prompt-info").hide();
                $(element).parent().find("." + errorClass).removeClass("ok2");
            }
        });

        var formLogin2Opts = {
            type: 'post',
            url: '/users/login2.html',
            statusCode: errorHandler,
            success: loginResponse2
        };
        $("#formLogin2").ajaxForm(formLogin2Opts);

        $('#btnLogin2').click(function () {
            if ($('#formLogin2').valid()) {
                showStatus("正在登录，请稍候...");
                $('#formLogin2').submit();
            }
        });

        $('.login3rd').click(function () {
            closeLoginDialog();
            askIsLogin();
        });
    });
  
    function showLoginDialog(fn)
    {
        fn_after_loginSuccess = fn;
        $('.logindialog').show();
    }
