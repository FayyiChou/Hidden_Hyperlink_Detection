<script type="text/javascript">
    function InvitePersonSuccess(resp, statusText, xhr, form) {
        try {
            var result = GetJson(resp);
            if (result.AddtionalData == null)
                hiAlert(result.Message, "提示");
            else
                hiAlert(result.Message + '<br/>' + result.AddtionalData);
            if(result.AddtionalData2!=null)
            {
                var arySuccessPersonIds = result.AddtionalData2.split(",");
                for(var i=0;i<arySuccessPersonIds.length;i++)
                {
                    $('.inviteRelatedPerson li[data-value="' + arySuccessPersonIds[i] + '"]').remove();
                }
                if ($('.inviteRelatedPerson li.d').length == 0)
                    $('.inviteRelatedPerson').remove();
            }
        } catch (err) { }
    }

    function closeInviteP() { $('.invitePersonDialog2').hide(); }

    function showInviteP()
    {
        if($('.selJobDiv').find("ul").attr("data-value")!="loaded")
        {
            jQuery.ajax({
                type: "post",
                url: '/Users/Company/GetCompanyJob.html',
                error: showError,
                success: function (data)
                {
                    var result = GetJson(data);
                    if(result.Success==1)
                    {
                        var aryJob = eval('(' + result.Data + ')');
                        var strHtml = "";
                        for(var i=0;i<aryJob.length;i++)
                        {
                            strHtml += '<li data-value="' + aryJob[i].JobID + '">' + aryJob[i].JobName + '</li>';
                        }
                        $('.selJobDiv').find("ul").html(strHtml);
                        $('.selJobDiv').find("ul").attr("data-value", "loaded");
                        $('.selJobDiv li').click(function () {
                            $('#selJobName').val($(this).text());
                            $('#selJobId').val($(this).attr("data-value"));
                            $('#selJobId').valid();
                            $('.selJobDiv').find("ul").hide();
                        });

                        var aryQMsg = eval('(' + result.AddtionalData + ')');
                        strHtml = "";
                        for(var i=0;i<aryQMsg.length;i++)
                        {
                            strHtml +=  '<li>' + aryQMsg[i].MsgContent + '</li>';
                        }
                        $('#formInvitePerson2 .qMsgDiv').find("ul").html(strHtml);
                        $('#formInvitePerson2 .qMsgDiv').find("ul").attr("data-value", "loaded");
                        $('#formInvitePerson2 .qMsgDiv li').click(function () {
                            $('#formInvitePerson2 textarea[name="MsgContent"]').val($(this).text());
                            $('#formInvitePerson2 textarea[name="MsgContent"]').valid();
                            $('#formInvitePerson2 .qMsgDiv').find("ul").hide();
                        });
                    } else {
                        hiAlert(result.Message, "提示");
                    }
                }
            });
        }

        $('.invitePersonDialog2').show();
    }

    
    function VPContact(id)
    {
        if ($('.pcontact:visible').length == 2) return true;
        else {
            jQuery.ajax({
                type: "post",
                url: "/cms/RequestViewResumeContact.html",
                data:{PersonId:id},
                error: showError,
                success: function (data) {
                    var result = GetJson(data);
                    if(result.Success==1)
                    {
                        if (result.AddtionalData == "0")
                            _VPContact(id);
                        else _VApplyContact(result.AddtionalData, id);
                    }else
                    {
                        if (result.Data == "InvalidData")
                            hiAlert("请求的数据不正确", "提示");
                        else
                            hiAlert(result.Message, "提示", function () {
                                showLoginDialog(function () { VPContact(id); });
                            });
                    }
                }
            });
            return false;
        }
    }


    function VApplyContact(applyId, pId) {
        if ($('.pcontact:visible').length == 2) return true;
        else {
            var changetime = new Date().getTime();
            jQuery.ajax({
                type: "get",
                url: "/cms/checklogin.html?UserType=2&t=" + changetime,
                error: showError,
                success: function (data) {
                    var result = GetJson(data);
                    if (result.Success == 1) {
                        _VApplyContact(applyId, pId);
                    } else {
                        hiAlert(result.Message, "提示", function () {
                            showLoginDialog(function () { VApplyContact(applyId, pId); });
                        });
                    }
                }
            });
            return false;
        }
    }


    function _VApplyContact(applyId,pId) {
        jQuery.ajax({
            type: "post",
            url: "/Users/Company/ViewApplyContact.html",
            data: { ApplyId: applyId, PUserID: pId },
            error: showError,
            success: function (data) {
                try {
                    var result = GetJson(data);
                    if (result.Success == 1) {
                        var objContactInfo = eval('(' + result.Data + ')');
                        $("#person_phone").text(objContactInfo.Phone);
                        $("#person_email").text(objContactInfo.Email);
                        $("#person_qq").text(objContactInfo.QQNo);
                        $("#person_address").text(objContactInfo.Address);
                        $(".pcontact").show();
                        $('html, body').animate({
                            scrollTop: $('a[name="pcontact"]').offset().top
                        }, 2000);
                    } else {
                        hiAlert(result.Message, "提示");
                    }
                } catch (ex) { }
            }
        });
        return false;
    }



    function _VPContact(id) {
        hiConfirm("确定查看此简历的联系方式吗？<br>请慎重，查看一个简历的联系方式，将扣一个点。", "提示", function (r) {
            if (r == true) {
                jQuery.ajax({
                    type: "post",
                    url: "/Users/Company/ViewResumeContact.html",
                    data: { PUserID: id },
                    error: showError,
                    success: function (data) {
                        try {
                            var result = GetJson(data);
                            if (result.Success == 1) {
                                var objContactInfo = eval('(' + result.Data + ')');
                                $("#person_phone").text(objContactInfo.Phone);
                                $("#person_email").text(objContactInfo.Email);
                                $("#person_qq").text(objContactInfo.QQNo);
                                $("#person_address").text(objContactInfo.Address);
                                $(".pcontact").show();
                                $('html, body').animate({
                                    scrollTop: $('a[name="pcontact"]').offset().top
                                }, 2000);
                            } else {
                                hiAlert(result.Message, "提示");
                            }
                        } catch (ex){}
                    }
                });
            }
        });
        return false;
    }


    
    function InviteP(ids)
    {
        var changetime = new Date().getTime();
        jQuery.ajax({
            type: "get",
            url: "/cms/checklogin.html?UserType=2&t=" + changetime,
            error: showError,
            success: function (data) {
                var result = GetJson(data);
                if (result.Success == 1) {
                    _InviteP(ids);
                } else {
                    hiAlert(result.Message, "提示", function () {
                        showLoginDialog(function () { InviteP(ids); });
                    });
                }
            }
        });
    }

    function _InviteP(ids)
    {
        $('#PUserIDs').val(ids);
        showInviteP();
    }

    function AddPerson2Fav(ids)
    {
        var changetime = new Date().getTime();
        jQuery.ajax({
            type: "get",
            url: "/cms/checklogin.html?UserType=2&t=" + changetime,
            error: showError,
            success: function (data) {
                var result = GetJson(data);
                if (result.Success == 1) {
                    _AddPerson2Fav(ids);
                } else {
                    hiAlert(result.Message, "提示", function () {
                        showLoginDialog(function () { AddPerson2Fav(ids); });
                    });
                }
            }
        });
    }

    function _AddPerson2Fav(puserids) {
        jQuery.ajax({
            type: "post",
            url: '/Users/Company/AddFavorite.html',
            data: { PUserIDs: puserids },
            error: showError,
            success: function (data) {
                try {
                    var result = GetJson(data);
                    if (result.Success == 1) {
                        hiAlert("已成功将指定的个人加入收藏夹！", "提示");
                    } else {
                        hiAlert(result.Message, "提示");
                    }
                } catch (ex) { }
            }
        });
        return false;
    }

    function downResume(personId)
    {
        var changetime = new Date().getTime();
        jQuery.ajax({
            type: "get",
            url: "/cms/checklogin.html?UserType=2&t=" + changetime,
            error:showError,
            success: function (data) {
                var result = GetJson(data);
                if (result.Success == 1) {
                    _downResume(personId);
                } else {
                    hiAlert(result.Message, "提示", function () {
                        showLoginDialog(function () { downResume(personId); });
                    });
                }
            }
        });
    }

    function _downResume(resumeid) {
        hiConfirm("是否同时下载简历的联系方式？<br/>（选择包括联系方式时，将进行相应扣点。）", "提示", function (r) {
            var isdowncontact = 0;
            if (r == true) isdowncontact = 1;
            showStatus("正在处理数据，请稍候...");
            jQuery.ajax({
                type: "post",
                url: '/users/company/downresume.html',
                data: { downcontact: isdowncontact, selPersonId: resumeid },
                error: showError,
                success: function (data) {
                    hideStatus();
                    var result = GetJson(data);
                    if (result.Success == 1) {
                        $("#fileDownFrame").attr("src", result.Data);
                    } else {
                        hiAlert(result.Message, "提示");
                    }
                }
            });
        });
    }


    jQuery(function () {
        $('.selJobDiv').hover(function () { $(this).find("ul").show(); }, function () { $(this).find("ul").hide(); });
        $('#formInvitePerson2 .qMsgDiv').hover(function () { $(this).find("ul").show(); }, function () { $(this).find("ul").hide(); });

        $('#formInvitePerson2').validate({
            ignore: "",
            errorClass: "error2",
            errorPlacement: function (error, element) {
                switch (element.attr("name")) {
                    default:
                        error.insertAfter(element);
                        break;
                }
            },
            success: function (label) {
                label.html("&nbsp;").addClass("ok2");
                try {
                    label.parent().find(".prompt-info").hide();
                } catch (ex) { }
            },
            highlight: function (element, errorClass) {
                $(element).parent().find(".prompt-info").hide();
                $(element).parent().find("." + errorClass).removeClass("ok2");
            }
        });

        var formInviteOpts = {
            type: 'post',
            url: "/users/company/InvitePerson.html",
            statusCode: errorHandler,
            success: InvitePersonSuccess
        };

        $('#btnConfirmInvite').click(function () {
            if ($('#formInvitePerson2').valid()) {
                if ($('#formInvitePerson2').attr("data-value") == "ajax")
                    $("#formInvitePerson2").ajaxForm(formInviteOpts);
                $('#formInvitePerson2').submit();
                closeInviteP();
            }
        });
    });
