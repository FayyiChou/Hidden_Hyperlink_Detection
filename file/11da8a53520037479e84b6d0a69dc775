<script>
$('#suggest').click(function(){ //Отправляем форму поиска
		$('.search-form').submit();
});
$(document).ready(function(){
	var startTime = new Date().getTime();
	var endTime;
	var elapsedTime;
	 
	var url = "/wp-content/themes/entrastudio/ajax/search.php";
			$.ajax({ //Отправляем письмо
				url: url,
				type: "POST",
				data: {
					search: "true"
				},
				dataType: "text",
				cache: false,
				context: $(this),
				success: function(response){
					$.all_products = eval(response);
					startSuggest();
					endTime = new Date().getTime();
					elapsedTime = (endTime - startTime)/1000;
					console.log("Загрузка заняла "+elapsedTime+" сек");
				}
			});
});
function startSuggest() {
	
  new Suggest.Local(
        "search_input",    // input element id.
        "suggest", // suggestion area id.
        $.all_products,      // suggest candidates list
        {dispMax: 10, interval: 1000,ignoreCase: true, prefix: true, highlight: true, classMouseOver: 'over', classSelect: 'select',hookBeforeSearch:function(){
			
		}}); // options
}
