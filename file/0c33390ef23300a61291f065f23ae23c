function zoomIn() {
 newZoom= parseInt(ozoom.style.zoom)+10+'%'
 ozoom.style.zoom =newZoom;
 } 
function zoomOut() {
 newZoom= parseInt(ozoom.style.zoom)-10+'%'
 ozoom.style.zoom =newZoom;
 }
