<script type="text/javascript">
	function dialogSelect(isCover) {
		var	editor = $.editorObj,
			checked = $('.cover_list input:checked');

		if (checked.length == 0) {
			if (isCover) {
				$.tipCommon('请选择封面', 'warn', 1500000);
				return false;
			} else {
				$.tipCommon('请选择产品', 'warn', 1500);
				return false;
			}
		} else {
			if (isCover) {
				$('#cover').val(checked.val());
				editor.sync();
				is_submit=true;
				if($.editorOpts && $.editorOpts.onSubmit){
					$.editorOpts.onSubmit($.editorObj);
				}else{
					$('#release_form').submit();
				}
			} else {
				var maxwidth=68;
				$('#for_product').html('<span class="l">关于</span> <a href="/product_reviews_'+checked.val()+'.html"  title="'+checked.attr('title')+'">' + splitStr(checked.attr('title'),maxwidth) + ' </a>的口碑报告');
				$('#related_product').val(checked.val());
				var ratingList = checked.attr('ratingList'), rating_item = null, rating_str = "";
				if (ratingList !== "") {
					ratingList = ratingList.split("|");
					if (ratingList.length > 1) {
						ratingList.pop();
					}
					for (var p = 0; p < ratingList.length; p++) {
						rating_item = ratingList[p].split(',');
						rating_str += '<li><span class="l">' + rating_item[0] + '：</span>';
						rating_str += '<div class="service_rating white_scroes"><div class="rating editable"><div class="value" style="width: 80px;">';
						rating_str += '<input type="hidden" value="5" name="rating[' + rating_item[1] + ']">';
						rating_str += '</div></div></div></li>';
					}
					$('.edit_scroes').html(rating_str);
					Jumei.Koubei.enableRating();
					editor.extraFileUploadParams.product_id=checked.val();
				}
			}
			$.boxClose();
		}
	}

