function onImgError(img,imgW,headimg){
	var noImgSrc;
	if(headimg){
		noImgSrc = 'https://p0.jmstatic.com/product_report/default/avatar/avatar_small.png';
	}else{
		imgW = imgW || Math.max(img.offsetWidth, img.offsetWidth);
		noImgSrc = imgW > 160 ?　"/images/noimg350x350.png" : imgW > 100 ?　"/images/noimg160x160.png" : imgW > 60 ?　"/images/noimg100x100.png" : "/images/noimg60x60.png"
	}
	img.src = noImgSrc;
	img.onerror = null;
}
