                        $(function () {
                            var timer = 0;
                            $(".infor_public_select").hover(function () {
                                _this = $(this);
                                timer = window.setTimeout(function () {
                                    _this.find("ul").slideDown(300);
                                    _this.find(".myselect_show").addClass("open");
                                }, 200)
                            }, function () {
                                window.clearTimeout(timer);
                                _this.find("ul").slideUp(300);
                                _this.find(".myselect_show").removeClass("open");
                            })
                            $(".infor_public_select ul li").click(function () {
                                this_val = $(this).html();
                                $(this).parents(".infor_public_select").find(".myselect_show").html(this_val);
                            })
                        })
