<script>
    function alarmFun(close_button,alarm_obj,msg){
        $('#msgtitle').html(msg)
        var window_height = $(window).height();
        $(".black_blank").css("height",window_height);
        $(".black_blank").show();
        $(alarm_obj).show();
        $(close_button).click(function(){
            $(".black_blank").hide();
            $(alarm_obj).hide();
        })
    }
</script>
