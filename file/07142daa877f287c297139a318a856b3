$(function(){
	$(".dh_nav li.m").each(function(index){
		$(this).hover(function(){
			if($(this).find("dl").length == 0){
				$(this).removeClass("cur_box").siblings().removeClass("cur").removeClass("cur_box");
				$(this).siblings().find("dl").hide();
				$(this).addClass("cur");
				
				}else{
				$(this).addClass("cur_box").siblings().removeClass("cur").removeClass("cur_box");
				$(this).find("dl").show().parents("li").siblings().find("dl").hide();
				
					
					}
			
			},function(){
				$(this).removeClass("cur").removeClass("cur_box");
				$(this).find("dl").hide();
				})
		})
	})
</script>
