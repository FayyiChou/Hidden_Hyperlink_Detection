            var speed=15
            demonstration12.innerHTML=demonstration11.innerHTML
            function Marquee11(){
            if(demonstration12.offsetWidth-demonstration.scrollLeft<=0)
            demonstration.scrollLeft-=demonstration11.offsetWidth
            else{
            demonstration.scrollLeft++
            }
            }
            var MyMar1=setInterval(Marquee11,speed)
            demonstration.onmouseover=function() {clearInterval(MyMar1)}
            demonstration.onmouseout=function() {MyMar1=setInterval(Marquee11,speed)}
