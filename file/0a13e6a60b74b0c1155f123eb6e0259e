<script type="text/javascript">

    $(function(){


        $(window).scroll(function() {
                

            //滚动上去的像素
            var scroll_t=$(window).scrollTop();

            if(scroll_t>100)
            {

                //元素距离可视区域左边的距离
                var box=$('.nav').offset();
                //固定定位
                hd_fixed($('.nav'),{'left':box.left+'px','top':'0px'},'fixed');

            }
            else
            {
                //绝对定位
                //$('#main .box').css({'top':'200px'});
                $('.nav').css({'left':+'0','top':'0px','position':'static'});
            }



        });


    })

    </script>

