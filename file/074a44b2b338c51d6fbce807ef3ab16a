jQuery(document).ready(function(){
        jQuery('#home-sticky .sticky-tab').click(function(){
            var sticky = jQuery('#home-sticky');
            var stickytab = jQuery(this);
            var stickycontent = jQuery('#home-sticky .sticky-content');
            if (stickytab.hasClass('open')) {
                jQuery('#home-sticky').animate({width:0},300);
                stickycontent.animate({width:0},300,function(){
                    stickytab.removeClass('open');
                    stickycontent.css({width:0,display:'none'});
                });
            } else {
                jQuery('#home-sticky').animate({width:294},300);
                stickycontent.css({width:0,display:'block'}).animate({width:294},300,function(){
                    stickytab.addClass('open');
                });
            }
        });
        jQuery('#home-sticky .sticky-content').animate({width:294},300);
	jQuery("#slideshow").easySlider({
		loop: true,                           // Looping
		orientation: 'fade',                  // Fading
		autoplayDuration: 5000,               // Autoplay with 1 second intervals
		autogeneratePagination: true,         // Automatically generate pagination links
		restartDuration: 5000,                // In case of user interaction, restart the autoplay after 2.5 seconds
		pauseable: true 
	});
        jQuery('ul.default_series_list li a, ul.pro_series_list li a').hover(function(){
            jQuery('div.img',this).fadeOut('fast',function(){
                var _this = jQuery(this);
                if (_this.hasClass('hover')) {
                    _this.removeClass('hover');
                } else {
                    _this.addClass('hover');
                }
                _this.fadeIn('fast',function(){
                   jQuery(this).css({opacity:1}); 
                });
            });
        });

        //jQuery("#tech_info_filter_form").submit(function(){
        jQuery('.btnApply').click(function(){
            var _data = {
                cat: $('cat_filter').value,
                prod: $('prod_filter').value,
                doc: $('doc_filter').value,
                'ajax-html':1
            };
            //Object.toQueryString(_data)
            //_data = jQuery(this).serialize() + "&ajax-html=yes"
            var _url = jQuery('#tech_info_filter_form').attr('action');
            jQuery(".techInfo").html('<p>Loading... please wait.</p>');

            jQuery.ajax({
               type: "GET",
               url: _url,
               data: _data,
               success: function(html){
                    jQuery(".techInfo").fadeOut('slow',function(){
                        jQuery(this).html(html).fadeIn('slow');
                    });
               },
               error: function (data, status, e){
                   //jQuery(".techInfo").fadeIn();
               }
            });

            return false;
        });
		
	jQuery("#pro-alliance-form,#contact-us-form,#support_form").submit(
		function(){
			var fID = jQuery(this).attr("id");
			var _data = jQuery(this).serialize() + "&ajax=yes";
			var _url = jQuery(this).attr("action");
			var pform = jQuery(this);
			jQuery(".c_msg", pform)
			.css("display","none")
			.html("<p class='info'>Sending data. Please wait...</p>")
			.fadeIn("slow");
			
			jQuery.ajax({
			   type: "POST",
			   url: _url,
			   data: _data,
			   success: function(msg) {
					var reset = (msg.indexOf("<!--RESET-->") > -1);
					if (jQuery(".c_fields").length && reset) {
						jQuery(".c_fields", pform).fadeOut("slow", function() {
							jQuery(".c_msg", pform).hide().html(msg).addClass('success').fadeIn('slow');
						});
					} else {
						jQuery(".c_msg", pform).html(msg)
						.delay(3000)
						.fadeOut("slow");
					}
					
				  if(reset){
					   jQuery("input[type=text], textarea").each(
							function(index,item){
								jQuery(item).val(jQuery(item).attr("title"));
							}
					   );
				   }
			   }
			});
			return false;
		}
	);

        var show_tqna = function(slected){
            jQuery("ul.faq").css("display", "none");
            jQuery("ul.faq." + slected).css("display", "none");
            var quest = jQuery("ul.faq." + slected + ":first li:first").html();
            var ans =  jQuery("ul.answer." + slected + ":first li:first").html();

            var q_s = jQuery('ul.faq.' + slected + ' li');
            var a_s = jQuery('ul.answer.' + slected + ' li');

            var qna = jQuery("#q-n-a").html('');

            for(var i=0; i < q_s.length; i++) {
                var q_ = jQuery('<h1></h1>').html(jQuery(q_s[i]).html());
                var a_ = jQuery('<p></p>').html(jQuery(a_s[i]).html());
                qna.append(q_);
                qna.append(a_);
            }
        }

        show_tqna(jQuery("#faqsel").val());

	jQuery("#faqsel").change(
