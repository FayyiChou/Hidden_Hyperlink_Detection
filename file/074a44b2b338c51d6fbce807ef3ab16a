function slideSwitch() {
    var $active = jQuery('#slideshow li.active');
    if ( $active.length == 0 ) $active = jQuery('#slideshow li:last');
    // use this to pull the divs in the order they appear in the markup
    var $next =  $active.next().length ? $active.next()
        : jQuery('#slideshow li:first');

    // uncomment below to pull the divs randomly
    // var $sibs  = $active.siblings();
    // var rndNum = Math.floor(Math.random() * $sibs.length );
    // var $next  = $( $sibs[ rndNum ] );

    $active.addClass('last-active');
	$active.fadeOut('fast').removeClass('active last-active');
	$next.fadeIn('fast').addClass('active');
	/*
	$active.animate({opacity: 0}, 500, function() {
		$active.css({display: 'none'});
        $active.removeClass('active last-active');
		$next.css({opacity: 0.0})
			.addClass('active')
			.animate({opacity: 1.0}, 500, function() {
				$active.removeClass('active last-active');
			});
    });
	*/
	/*
    $next.css({opacity: 0.0})
        .addClass('active')
        .animate({opacity: 1.0}, 5000, function() {
            $active.removeClass('active last-active');
        });
	*/
}

jQuery(function() {
    //setInterval( "slideSwitch()", 5000 );
});

