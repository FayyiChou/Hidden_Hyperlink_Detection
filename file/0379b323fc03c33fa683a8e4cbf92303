<script type="text/javascript">
$(function(){
	$("#j-h_searchText").keydown(function(e){if(e.keyCode==13){subSearchInHead();}});
	//加载搜索热门关键词
	var wordUrl = "/help/b_hotSearch.do";
	 $.ajax({type:"post",
        	 url: wordUrl,
          cache: false,
        success: function(res){
        	var data = eval("("+res+")");
        	var sw = data;
            if(!sw){
                return ;
            }
            var html = "";
            $.each(data,function(i,ad){
           	 var con = "<a target='_blank' href="+ad.link+" title="+ad.link+"><span>"+ad.title+"</span></a>";
           	 html += con;
            });
            $(".h_hotKeyword").html(html);
        }
    });
});
function subSearchInHead(){
	var skw=$.trim($("#j-h_searchText").val());
	if(""==skw){return false;}else{window.location.href="/seller_search?question="+skw;}
}
</script>

