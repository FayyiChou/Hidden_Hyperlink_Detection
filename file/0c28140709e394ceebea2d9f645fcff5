	<script>
	    	;(function($){
	    		
	    		var lastRmenuStatus=false;
		    	$(window).scroll(function(){//bug
		    		var _top=$(window).scrollTop();
		    		var _remenu=$("#izl_rmenu");
		    		if(_top>50){
		    			_remenu.data("expanded",true);
		    		}else{
		    			_remenu.data("expanded",false);
		    		}
		    		if(_remenu.data("expanded")!=lastRmenuStatus){
		    			lastRmenuStatus=_remenu.data("expanded");
		    			if(lastRmenuStatus){
		    				_remenu.slideDown();
		    			}else{
		    				_remenu.slideUp();
		    			}
		    		}
		    	});
		    	
		    	
		    	$("#izl_rmenu").click(function(){
		    		
		    		$("html, body").animate({
		    			"scroll-top":0
		    		},"fast");
		    		
		    	})
	    		
	    	}(jQuery))
	    	
	    	
	</script>
