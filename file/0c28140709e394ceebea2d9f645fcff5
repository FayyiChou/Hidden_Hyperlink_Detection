   var webTwo=""
   var webRoot="http://www.ulin5.com"
   var corpId=""
   
   function isLogin(){
	   var flag=true;
	   var userLogin="";  
	   if($.trim(userLogin).length==0){//表示没有登录
			
			var returnUrl="http://"+webTwo;
			  
			var loginurl =webRoot+"/b2cWap/toLogin.html?finalValue=";

			window.location =loginurl+encodeURIComponent(returnUrl); 
			
			flag=false;
	   }else{
		   
		   flag=true;
		   
	   }
	   
	   return flag;
	   
   }
   
</script>

