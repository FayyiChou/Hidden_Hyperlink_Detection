            <script> 
var speed=30 
dream2.innerHTML=dream1.innerHTML 
function Marquee(){ 
if(dream2.offsetWidth-dream.scrollLeft<=0) 
dream.scrollLeft-=dream1.offsetWidth 
else{ 
dream.scrollLeft++ 
} 
} 
var MyMar=setInterval(Marquee,speed) 
dream.onmouseover=function() {clearInterval(MyMar)} 
dream.onmouseout=function() {MyMar=setInterval(Marquee,speed)} 
