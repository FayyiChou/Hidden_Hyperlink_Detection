            <script> 
var speedb=30 
dream2b.innerHTML=dream1b.innerHTML 
function Marqueeb(){ 
if(dream2b.offsetWidth-dreamb.scrollLeft<=0) 
dreamb.scrollLeft-=dream1b.offsetWidth 
else{ 
dreamb.scrollLeft++ 
} 
} 
var MyMarb=setInterval(Marqueeb,speedb) 
dreamb.onmouseover=function() {clearInterval(MyMarb)} 
dreamb.onmouseout=function() {MyMarb=setInterval(Marqueeb,speedb)} 
                                </script>
