			function inlinelink(url, type, subtype) {


				if (subtype == 'RemoveUser') {
					window.location = '/milka/servlet/login/closeAccount?siteid=milka-prd&locale=trtr1&PagecRef=' + url;
				} else if (type == 'page' && subtype == 'BrowserHistoryBack') {
					history.go(-1);
				} else if (type == 'page') {
					// PagecRef=url must be final element, to not break Mirabell European map links - otherwise, that can be implemented as a new type of link
					window.location = 'http://www.milka.com.tr/milka/page?siteid=milka-prd&locale=trtr1&Mid=1&PagecRef='+url;
				} else if (subtype == 'Disclaimer') {
					newurl = 'http://www.milka.com.tr/milka/page?siteid=milka-prd&locale=trtr1&Mid=1&PagecRef=19&url=' + escape(url) + '&cache=off';
					window.location = newurl;
				} else if (subtype == 'MediaPopup') {
					loadPopup('/milka/images/../media_popup.html?url=/milka/images/trtr1/movies/' + url);
				} else {
					window.open(url);
				}
			}
