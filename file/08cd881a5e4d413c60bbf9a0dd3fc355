    if ('' == "Y") {
        $.ajax({
            dataType: 'text',
            url: "/JsVersion.txt",
            type: 'get',
            cache: true,
            success: function (Jsv) {
                $.ajax({
                    url: "/public/vendor.js?" + Jsv, cache: true, dataType: "script",
                    success: function () {
                        $.ajax({
                            url: "/public/plugins.js?" + Jsv, cache: true, dataType: "script",
                            success: function () {
                                $.ajax({
                                    url: "/public/app.js?" + Jsv, dataType: "script", cache: true,
                                });
                            }
                        });
                    }
                });

            },
            error: function () {
                $.ajax({
                    url: "/public/vendor.js", cache: true, dataType: "script",
                    success: function () {
                        $.ajax({
                            url: "/public/plugins.js", cache: true, dataType: "script",
                            success: function () {
                                $.ajax({
                                    url: "/public/app.js", dataType: "script", cache: true
                                });
                            }
                        });
                    }
                });
            }
        });
    }


