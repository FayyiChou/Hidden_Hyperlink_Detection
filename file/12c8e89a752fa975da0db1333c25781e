        function countClick(cmd){
            $.ajax({
                url:'/ajax_new/ajax_action.php?action=count_click&id=51298&share_id='+cmd,
                type:'get',
                //dataType:'json',
                async:'false',
                success:function(res){
                    console.log(res);
                },
                error:function(){

                }
            });
        }

