			var $backToTopEle = $('.bt-to-top').click(function() {
				$("html, body").animate({
					scrollTop: 0
				}, 120);
			}),
				$backToTopFun = function() {
					var st = $(document).scrollTop(),
						winh = $(window).height();
					if(st > 0){
						$backToTopEle.css('display','block');
					}else{
						$backToTopEle.css('display','none');
					};
				};
			$(window).bind("scroll", $backToTopFun);
		});
	</script> 
