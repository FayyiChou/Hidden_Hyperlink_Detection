<script type="text/javascript">
$(document).ready(function(){
	var tab = $('.title[rel=tab]');
	//只显示第一个有active样式的内容
	tab.find('.active:first').each(function(){
		var $this = $(this);
		$this.parent().parent().next().children().hide().eq($this.index()).show();
	});
	// tab切换
	var timeout, tabindex;
	tab.find('li').mouseover(function(){
		var $this = $(this);
		//有延迟
		timeout = setInterval(function(){
			$this.addClass('active').siblings().removeClass('active');
			var t = $this.parent().parent().next().children().eq($this.index());
			t.fadeIn().siblings().hide();
		}, 250);
	}).mouseout(function(){clearInterval(timeout);});

	$("#hdlybox").kxbdSuperMarquee({ isMarquee:true, isEqual:true, direction:"left", scrollDelay:35});
	$("#note").kxbdSuperMarquee({ isMarquee:true, isEqual:false, direction:"up", scrollDelay:60});
        //$("#note").kxbdSuperMarquee({ isEqual:false, direction:"up", distance:200, time:3,});

	//图片新闻flash调用
	var pics='/uploadfiles/editor/201605/4641049609484343701_.jpg|/uploadfiles/editor/201605/4687927337991897147_.jpg|/uploadfiles/editor/201605/5695642704202940041_.jpg|/uploadfiles/editor/201605/5431190643718090778_.jpg|/uploadfiles/editor/201603/5439566538608256832_.jpg';var links=escape('detail/1149|detail/1148|detail/1147|detail/1146|detail/1096');var texts='自治区党校党史党建教研部教授、主任刘文长授课为我局…|我局院士工作站2016年会议胜利召开，柔性引进张永民、…|党委副书记、工会主席曹生贤一行到综合勘探队井队现场…|自治区国土厅专家组莅临一院中卫深井项目部检查指导工…|局长刘自俭一行赴新疆煤田地质局调研';

	var _width = 400, _height = 260, _txtheight = 20;
	var _flashvars = 'pics=' + pics + '&links=' + links + '&texts=' + texts + '&borderwidth=' + _width + '&borderheight=' + _height + '&textheight=' + _txtheight;

	var sf1 = new SWFObject('focus1.swf','mp1', _width, _height+_txtheight,'7');
	sf1.addParam('allowfullscreen','true');
	sf1.addParam('wmode','opaque');
	sf1.addParam('bgcolor', '#ffffff');
	sf1.addParam('FlashVars', _flashvars);
	sf1.write('newspic');



});
</script>
