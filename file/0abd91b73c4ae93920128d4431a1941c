<!-- 加载漂浮广告模版 -->
<div id="fly" style="position:absolute; left:0; top:0;"></div>

<script type="text/javascript">
var x = 50,y = 60; 
var xin = true, yin = true; 
var step = 1; 
var delay = 20 ;
var obj=$("#fly");
function floatDiv() { 
	var L=T=0; 
	var R= $(document).width()-obj.width();
	var B = $(document).height()-obj.height();
	obj.css({left:x+document.body.scrollLeft, top:y+document.body.scrollTop}); 
	
	//x坐标
	x = x + step*(xin?1:-1);
	if (x < L) 
	{ xin = true; x = L} 
	if (x > R)
	{ xin = false; x = R} 
	//y坐标
	y = y + step*(yin?1:-1);
	if (y < T) 
	{ yin = true; y = T } 
	if (y > B)
	{ yin = false; y = B } 
} 

var itl= setInterval("floatDiv()", delay); 
obj.onmouseover=function(){
	clearInterval(itl);
} 
obj.onmouseout=function(){
	itl=setInterval("floatDiv()", delay);
} 
</script> 

