<script src="js/lib/jquery-1.10.2.min.js"></script>
<script src="js/lib/jquery.mousewheel.js"></script>
<script src="js/lib/hammer.min.js"></script>
<!--[if lt IE 9]>
<style style="css/test">
.middle{
    top:0!important;
}
</style>
<![endif]-->
<script>
$(function() {
    var lock = false,
        _index = $('.slide0').index(),
        _lastIndex = 0,
        _length = $('.swiper-slide').length;
    $(window).mousewheel(function(e, delta, deltaX, deltaY) {
        e.preventDefault();
        e.stopPropagation();
        takePage(delta);
    })

    function takePage(delta) {
        if (lock) {
            return false;
        };
        lock = true;
        _lastIndex = _index;
        //向下
        if (delta < 0) {
            _index++;
            if (_index >= _length) {
                _index = 0;
            }
        } else if (delta > 0) {
            _index--;
            if (_index < 0) {
                _index = _length - 1;
            }
        }
        doResetCurrentIndex();
    }
    function doResetCurrentIndex() {
        var last = $('.swiper-slide').eq(_lastIndex);
        last.addClass('status-out')
        $('.swiper-slide').eq(_index).css("z-index",last.css("z-index")-1).css('display', "block");
        var height=document.body.offsetHeight;
        $('.swiper-slide').eq(_index).addClass('status-in')
        $('.cls-swiper-pager span').removeClass('active');
        $('.cls-swiper-pager span').eq(_index).addClass('active');
        if (_index > 2) {
            $('.cls-nav-header').addClass('themeBlack');
        } else {
            $('.cls-nav-header').removeClass('themeBlack');
        }
        (function(index,_last){
           setTimeout(function() {
                $('.swiper-slide').eq(index).css('z-index',6);
                _last.removeClass('status-out status-in').css('display', "none");
                lock = false;
            }, 2000)            
        })(_index,last)


    }
    $('.swiper-slide').eq(_index).css('display', "block")
    setTimeout(function() {
        $('.swiper-slide').eq(_index + 1).css("z-index", 5);
        $('.swiper-slide').eq(_index).addClass('status-in');
    }, 30)
    var timer;
    $('.cls-nav-header .btn-icon').on('click', function() {
        clearTimeout(timer);
        if (!$('.cls-nav-header .list-nav').hasClass('active')) {
            $('.cls-nav-header .list-nav').addClass("block");
            setTimeout(function() {
                $('.cls-nav-header .list-nav').addClass('active')
            }, 30)
        } else {
            $('.cls-nav-header .list-nav').removeClass('active');
            timer = setTimeout(function() {
                $('.cls-nav-header .list-nav').removeClass("block");
            }, 700)
        }
    })
    if ("ontouchstart" in window) {
        $('.swiper-slide').each(function(index, el) {
            var cm = new Hammer(el);
            cm.get('swipe').set({
                direction: Hammer.DIRECTION_VERTICAL
            });
            cm.on('swipe', function(ev) {
                var direction = ev.direction;
                if (direction == Hammer.DIRECTION_UP) {
                    takePage(-1);
                } else if (direction == Hammer.DIRECTION_DOWN) {
                    takePage(1);
                }
            })
        })
    }
})
</script>

</html>
