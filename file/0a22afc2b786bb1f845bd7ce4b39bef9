<script>

function rmss(){

	var length=document.getElementsByName("jsc").length;
	var jsc;
	for(var i=0;i<length;i++){
		 jsc=document.getElementsByName("jsc")[i].innerHTML;
		 document.getElementsByName("jsc")[i].href="/was5/web/search?channelid=220740&searchword="+encodeURI(jsc);
		}
}
</script>
		
