<script type="text/javascript">
    var gwdshare = gwdshare || { version: "1.0", udata: "type=tools&amp;uid=GWD-800820", pageFilter: "", jsLoaded: false };
    (function () {
        function async_load() {
            var s = document.createElement('script');
            s.type = 'text/javascript';
            s.async = true;
            s.src = './images/gwdshareasync.min.js';
            var x = document.getElementsByTagName('script'), y = x[x.length - 1];
            y.parentNode.appendChild(s);
        }
        if (!gwdshare.jsLoaded) {
            if (window.attachEvent) window.attachEvent('onload', async_load);
            else window.addEventListener('load', async_load, false);
            gwdshare.jsLoaded = true;
        }
    })();
</script>
<script>

    // ****判断是否移动设备****
    var isMobile = {  
    Android: function() {  
        return navigator.userAgent.match(/Android/i) ? true : false;  
    },  
    BlackBerry: function() {  
        return navigator.userAgent.match(/BlackBerry/i) ? true : false;  
    },  
    iOS: function() {  
        return navigator.userAgent.match(/iPhone|iPod/i) ? true : false;  
    },  
    Windows: function() {  
        return navigator.userAgent.match(/IEMobile/i) ? true : false;  
    },
    UC:function(){
        return navigator.userAgent.match(/Linux/i) ? true : false;
    },  
    any: function() {  
        return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Windows() || isMobile.UC());  
    }  
};  
	function GetQueryString(name) {
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)","i");
		var r = window.location.search.substr(1).match(reg);
		if (r!=null) return unescape(r[2]); return null;
	}
	
    if( isMobile.any() )   
    {
		if(GetQueryString("location")!="mobile"){
			if(confirm("您需要访问触屏版吗？"))
				window.location.href="/mobile/";
		}
    }  
</script>
