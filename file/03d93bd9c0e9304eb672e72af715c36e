<script type="text/javascript" id="">
$(".margin010").on("click","ul[class=tonglei_ul] li",function(){
         var href=$(this).find("a").attr("href");
         $("#fenlei_title").find("a").each(function(){
                   var classz=$(this).attr("class");
                            if(classz=="cur"){
                                     var index=$(this).index();
                                     name=returnName(index)+"";
                            }
         });
                   window.open(href+name,"_blank");
                   return false;
});

         function returnName(index){
                   var name="";
                   if(index==0){
                            name="?recmd=box09";
                   }else if(index==1){
                            name="?recmd=box10";
                   }else if(index==2){
                            name="?recmd=box08";
                   }
                   return name;
         }

$("#mboxDynamicLeft li").click(function(){
  
         var href=$(this).find("a").attr("href");
         window.open(href+"?recmd=box13","_blank");
         return false;
});

$("#mboxDynamic li").click(function(){
  
         var href=$(this).find("a").attr("href");
         window.open(href+"?recmd=box12","_blank");
         return false;
});

</script>

