//置业顾问电话

(function ($) {
    $('.footer-plates').css({'border':'1px solid #e0e0e0','margin':'10px 0 0','padding':'7px 20px','text-align':'left'});
    $('.footer-plates').hide();
    $('.footer-select').click(function(){
        $('.footer-plates').hide();
        $('.'+$(this).attr('plates')+'-plates').show();
    });
    $('.footer-infos').mouseleave(function(){
        setTimeout(function(){$('.footer-plates').hide();},500);
    });
})(jQuery)

//统一登录退出功能添加history
;(function($) {
    var base64CurrentUrl = "aHR0cDovL3d3dy5hbmp1a2UuY29tL2p6aC1scDM5ODU1MC8=";
   $(document).ready(function() {
        var href;
        if(base64CurrentUrl) {
            $("body").on("click.logout", "a", function(e) {
                var elm = $(this);
                if(elm.html() == "退出" && (href = elm.attr("href")) ) {
                    e.preventDefault();
                    window.location.href = href + (href.indexOf("?") >= 0 ? "&" : "?") + "history=" + base64CurrentUrl;
                }
            });
        }
    }); 
})(jQuery);
</script>

