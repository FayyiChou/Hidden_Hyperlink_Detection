var zoneseconds=7*3600
var beat=((swatchseconds-zoneseconds)*10-(swatchseconds-zoneseconds)*10%864)/864
if (beat < 0) beat = 1000+beat;
if (beat < 100) beat = "0"+beat;
if (beat < 10) beat = "0"+beat;
