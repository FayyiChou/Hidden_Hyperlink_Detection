<script language = "JavaScript">
function CheckForm()
{
      if (form1.m_username.value=="")
   {
	  alert("用户名不能为空！");
	  form1.m_username.focus();
	  return false;
	 }
	  if (form1.m_password.value=="")
   {
	  alert("密码不能为空！");
	  form1.m_password.focus();
	  return false;
	 }
	 
}
</script>
