<script type="text/javascript">
    $(function () {
      var language = $.getCookie('PreferredCulture');
      var isSimple = $.isCn();
      var btn = $('#multiLanguageSwitch').click(function() {
          var cookieValue = "PreferredCulture=" + (isSimple?'zh-TW':'zh-CN');
          cookieValue += ";expires=" + new Date(9999,11,31).toGMTString();
          cookieValue += ";path=/;domain=.wanfangdata.com.cn";
          cookieValue += ";true";
          document.cookie = cookieValue;
          window.location.reload();
          return false;
      });
      if(!isSimple){
        btn.text('简体中文');
      }
      var container = $('#userStatus');
      container.find('.headerlogout').hide();
      var retUrl = document.location.href.toLocaleLowerCase().indexOf("returnurl") != -1 ? "" : '?ReturnUrl=' + encodeURIComponent(document.location.href);
      var login = container.find(".headerLogin"),register = container.find(".headerRegister").show();
      login.attr("href", login.attr("href") + retUrl);
      register.find('a').attr("href", register.find('a').attr("href") + retUrl);
      var authContext = $.authContext();
      if (authContext == null || authContext.AccountIds.length == 0) {
        return;
      }
      var accounts = $.getAccountId(['EduPerson','EduGroup'],authContext)
      
      if(accounts.EduGroup!=null){
        var display = $.getDisplay(accounts.EduGroup,authContext);
        container.find('.loginuser').prepend('<a href="http://my.edu.wanfangdata.com.cn/Organ/Subscribe">'+ display + '</a> | ');
      }
      if(accounts.EduPerson!=null){
        var display = $.getDisplay(accounts.EduPerson,authContext);
        container.find('.loginuser').prepend('<a href="/Library"><img src="/Content/Images/user.jpg" />'+display+'</a> | ');
        register.hide();
        container.find('.headerlogout').show();
      }
    });
</script>

