<script>
  $('.headerLogo img').attr('src',$.isCn()?'http://edu.wanfangdata.com.cn/Content/Images/logo.gif':'http://edu.wanfangdata.com.cn/Content/Images/logo_tw.gif');
	$(function () {
		function search(val) {
			var query = $.trim(val);
			if (!val || typeof (val)!="string") {
				query = $.trim($("#searchbox").val());
			}
			if (query == "") {
				$.alert("请输入检索词！");
				return false;
			}
			addSearchLogLazy(query, "Library.Home");

			var searurl = "/Search/Index?query=";
			query = encodeURIComponent(query);
			location.href = searurl + query;
		}
		$("#searchbox").enterpress(search)
		.autocomplete({
				source: function (request, response) {
					$.ajax({
						url: 'http://trend.wanfangdata.com.cn/AutoComplete',
						data: { q: request.term },
						dataType: "jsonp",
						success: function (data) {
							response(data);
						}
					});
				},
				select: function (event, ui) {
					search(ui.item.value);
				}
			})
		.watermark("在海量资料中查找");
		$("#btnsearch").click(search);
	});	
	$(function(){
	$('.headupdown .headupdowntitle').click(function () {
                $(this).toggleClass('on').find('.icon').toggleClass('icon-close').end().next('.headupdownlist').toggle();
            });
            $('.headupdown').mouseleave(function () {
                $(this).find('.headupdowntitle').removeClass('on').find('.icon').removeClass('icon-close').end().end().find('.headupdownlist').hide();
            });
	})
	$(function(){
		var libraryType="";
		var domain=document.domain;
		var domainPrefix=domain.split('.')[0];
		if(domainPrefix=="xx"){
			libraryType="XXG";
		}
		else if(domainPrefix=="cz"){
			libraryType="CZG";
		}
		else if(domainPrefix=="gz"){
			libraryType="GZG";
		}
		else{
			libraryType="ZH";
		}		
		var currentLibrayName=$("#"+libraryType).text();
		$(".headupdowntitle>span").text(currentLibrayName);
		$("#"+libraryType).parent().remove();
	})	
</script>

