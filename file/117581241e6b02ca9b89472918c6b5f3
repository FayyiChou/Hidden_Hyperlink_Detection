      <script>
    $(function(){
        $('.i_gammer_goodshow_top').first().addClass('active');
        $('.i_gammer_goodshow_top').hover(function(){
            $('.i_gammer_goodshow_top').first().removeClass('active')
            $(this).addClass('active');
        },function(){
            $(this).removeClass('active');
            $('.i_gammer_goodshow_top').first().addClass('active');
        });
        $('.i_gammer_goodshow_top').last().hover(function(){
            $('.i_gammer_goodshow_top').first().removeClass('active')
            $(this).addClass('active');
            $(".gamer-video-left-lists").css('background-size','1px 75%');
        },function(){
            $(this).removeClass('active');
            $('.i_gammer_goodshow_top').first().addClass('active');
            $(".gamer-video-left-lists").css('background-size','1px 82%');
        });
    })
</script> 
