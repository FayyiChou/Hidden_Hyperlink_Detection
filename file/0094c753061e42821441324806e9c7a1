<script type="text/javascript">
//变化样式
function switch_right_zixun(idname,name,id,zongs) {
	for(var i = 1; i <= zongs; i++) {
			document.getElementById(idname+i).className = 'zixun_li01_20130822';
			document.getElementById(name+i).style.display = 'none';
	}
	document.getElementById(idname+id).className = 'zixun_li01s_20130822';
	document.getElementById(name+id).style.display = 'block';
}
</script>

