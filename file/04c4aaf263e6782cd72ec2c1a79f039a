<!-- Scripts will go here. -->

<script type="text/javascript" src="http//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="http://www.giantnewtonscradle.com/scripts/jquery-1.7.2.min.js"><\/script>')</script><!--Load locally if CDN is down-->
<script type="text/javascript" src="http://www.giantnewtonscradle.com/scripts/waypoints.min.js"></script>
<script type="text/javascript" src="http://www.giantnewtonscradle.com/scripts/jquery.scrollTo-1.4.2-min.js"></script>
<script type="text/javascript" src="http://www.giantnewtonscradle.com/scripts/jquery.colorbox-min.js"></script>

<script type="text/javascript">
$(function() {

// Do our DOM lookups beforehand
var nav_container = $(".menuWrapper");
var nav = $(".menu");

var top_spacing = 15;
var waypoint_offset = 50;

nav_container.waypoint({
handler: function(event, direction) {

if (direction == "down")
    nav_container.css({"height":nav.outerHeight()});
                
    else
    nav_container.css({"height":"auto"});

nav.toggleClass("sticky", direction=="down");
}
});

var sections = $(".panel");
var navigation_links = $(".menu li a");

sections.waypoint({
handler: function(event, direction) {

    var active_section;
    active_section = $(this);
    if (direction === "up") active_section = active_section.prev();

    var active_link = $('.menu li a[href="#' + active_section.attr("id") + '"]');
    navigation_links.removeClass("selected");
    active_link.addClass("selected");

},
offset: '30%'
})


navigation_links.click( function(event) {

$.scrollTo(
    $(this).attr("href"),
    {
        duration: 1000,
        offset: { 'left':0, 'top':0*$(window).height() }
    }
);

event.preventDefault();
});



var navigation_button = $(".orderNowInner");

navigation_button.click( function(event) {

$.scrollTo(
    $(this).attr("href"),
    {
        duration: 1000,
        offset: { 'left':0, 'top':0*$(window).height() }
    }
);
});

//add selected to home button when scroll back to top
$(window).scroll(function(){
    var y = $(window).scrollTop();
    if(y < 50) {$('#homebutton').addClass("selected");} else {$('#homebutton').removeClass("selected");}
});

$(".colorbox1").colorbox({transition:"elastic", slideshow:"true"});

$(".colorbox2").colorbox({iframe:true, innerWidth:640, innerHeight:360});


});
</script>

</body>



</html>