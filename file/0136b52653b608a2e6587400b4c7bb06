	<script type='text/javascript'>
	//首先取得變數資料，轉換成陣列
	var STR_AIR_PT = '桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場$$--$$桃園國際機場';
	var STR_TVL_SAREA_WD = '新疆.北疆.南疆$$--$$吉普車特種團$$--$$絲路‧陜西‧甘肅$$--$$西藏‧川藏‧青藏鐵路‧阿里$$--$$雲南‧羅平‧元陽‧紅土地‧元謀‧滇西北$$--$$貴州‧黔東南‧黔西南$$--$$青海‧年保玉則神山‧門源‧玉樹‧牛頭碑$$--$$山西‧壺口$$--$$安徽‧黃山$$--$$湖北‧武漢‧神農架‧武當山$$--$$江西‧南昌$$--$$印度、不丹$$--$$中亞';
	var STR_TVL_DAREA_WD = '中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$中國大陸$$--$$亞洲$$--$$中亞';
	var STR_TVL_SAREA_VALUE = 'SAREA00001$$--$$SAREA00002$$--$$SAREA00003$$--$$SAREA00004$$--$$SAREA00007$$--$$SAREA00008$$--$$SAREA00009$$--$$SAREA00010$$--$$SAREA00011$$--$$SAREA00018$$--$$SAREA00021$$--$$SAREA00032$$--$$SAREA00035';
	var STR_TVL_DAREA_VALUE = 'DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00001$$--$$DAREA00006$$--$$DAREA00007';
	//定義陣列長度，資料來源為上述取得的小分類的資料變數
	var ary_AIR_PT = STR_AIR_PT.split("$$--$$");
	var ary_TVL_SAREA_WD = STR_TVL_SAREA_WD.split("$$--$$");
	var ary_TVL_DAREA_WD = STR_TVL_DAREA_WD.split("$$--$$");
	var ary_TVL_SAREA_VALUE = STR_TVL_SAREA_VALUE.split("$$--$$");
	var ary_TVL_DAREA_VALUE = STR_TVL_DAREA_VALUE.split("$$--$$");
	
	var tmp_TVL_DAREA = '';
	var tmp_TVL_SAREA = '';
	
	//先執行一次，否則大分類下拉選單會有重複的
	DecideSelectChecked('AIR_PT','',1)					//決定機場預設值
	ChangeAIRPort();									//決定大分類 
	DecideSelectChecked('TVL_DAREA',tmp_TVL_DAREA,2)	//決定大分類預設值
	ChangeDAreaOPT();									//決定小分類
	DecideSelectChecked('TVL_SAREA',tmp_TVL_SAREA,3)	//決定小分類預設值
	WriteTvlAreaValue();
	
	function ChangeAIRPort(){
		//取得使用者選取的機場UserSelectAIRP
		var UserSelectAIRP = GetSelectValue('AIR_PT');
		//清空下層連動選單分類
		CleanSelectOBJ('TVL_DAREA');
		CleanSelectOBJ('TVL_SAREA');
		//一律添加一個不限的選項
		AddSelectItem('TVL_DAREA','不限地區','');
		AddSelectItem('TVL_SAREA','不限地區','');
		//根據大分類代碼，添加小分類的選單
		for (var i=0 ; i<ary_AIR_PT.length ; i++) {
			
			if (UserSelectAIRP==ary_AIR_PT[i]) {
				NeedAddItem('TVL_DAREA',ary_TVL_DAREA_WD[i],ary_TVL_DAREA_VALUE[i]);
			} else {
				if (UserSelectAIRP=='') {
					//檢查是否已存在該選項
					NeedAddItem('TVL_DAREA',ary_TVL_DAREA_WD[i],ary_TVL_DAREA_VALUE[i]);
				}
			}
		}
		
	}
	function ChangeDAreaOPT(){
		//取得使用者選取的大分類代碼UserSelectDArea
		var UserSelectDArea = GetSelectValue('TVL_DAREA');
		var UserSelectAIRPT = GetSelectValue('AIR_PT');
		//清空小分類選單內容
		CleanSelectOBJ('TVL_SAREA');
		//一律添加一個不限的選項
		AddSelectItem('TVL_SAREA','不限地區','');
		//根據大分類代碼，添加小分類的選單
		//2012-09-11 [art] 添加的時候，必須要加入判斷機場，否則會以全部的機場下去增加子分類
		for (var i=0 ; i<ary_TVL_SAREA_WD.length ; i++) {
			if (UserSelectDArea==ary_TVL_DAREA_VALUE[i]) {
				if (UserSelectAIRPT==''){
					//不限機場的話，子分類應該全部加上
					NeedAddItem('TVL_SAREA',ary_TVL_SAREA_WD[i],ary_TVL_SAREA_VALUE[i]);
				}else{
					//有限制機場，則必須判斷機場條件才加入子分類
					if (UserSelectAIRPT==ary_AIR_PT[i]){
						NeedAddItem('TVL_SAREA',ary_TVL_SAREA_WD[i],ary_TVL_SAREA_VALUE[i]);
					}
				}
			} else {
				//如果使用者選取不限地區，那麼每一樣都加上選單
				if (UserSelectDArea=='') {
					NeedAddItem('TVL_SAREA',ary_TVL_SAREA_WD[i],ary_TVL_SAREA_VALUE[i]);
				}
			}
		}
	}
	//判斷是否已存在該選項，並增加新選項
	function NeedAddItem(OBJNM,ItemWD,ItemValue){
		var NeedAdd = jsSelectIsExitItem(OBJNM,ItemValue)
		if (!NeedAdd) {
			AddSelectItem(OBJNM,ItemWD,ItemValue);
		}
	}
	//增加下拉選單
	function AddSelectItem(IDName,OptTXT,OptValue){
		if (IDName=='' || OptTXT=='') { return; };
		var SELOBJ = document.getElementById(IDName);
		if (SELOBJ) {
			var opt=new Option(OptTXT,OptValue);
			SELOBJ.options[SELOBJ.length]=opt;
		}
	}
	//取得使用者選取項目
	function GetSelectValue(IDName){
		if (IDName=='') { return; };
		var OBJ = document.getElementById(IDName);
		if (OBJ) {
			for (var i=0;i<OBJ.length;i++){
				if (OBJ.options[i].selected==true) {
					return OBJ.value;
				}
			}
		}
	}
	//將大小分類資料寫入參數供傳遞使用
	function WriteTvlAreaValue(){
		var OBJ = document.getElementById("TVL_AREA");
		var VarA = GetSelectValue('TVL_DAREA');
		var VarB = GetSelectValue('TVL_SAREA');
		if (VarB!='') {
			VarC = VarA+'-'+VarB;
		} else {
			VarC = VarA
		}
		OBJ.value = VarC;
	}
	//判斷是否選單中已存在某分類值
	//2013-02-26 [art] 先檢查有物件才執行
	function  jsSelectIsExitItem(objNM, objItemValue) {
	     var isExit = false ;
	     var objSelect = document.getElementById(objNM);
	     if (objSelect) {
		     for (var i = 0 ; i < objSelect.options.length; i ++ ) {
		         if (objSelect.options[i].value == objItemValue)  {
		            isExit = true ;
		             break ;
		        }
		    }
		}
	     return  isExit;
	} 
	//清空下拉選單
	function CleanSelectOBJ(IDName){
		if (IDName=='') { return; };
		var SELOBJ = document.getElementById(IDName);
		if (SELOBJ) {
			SELOBJ.options.length=0;
		}
	}
	//取得搜尋的表單值
	function GetHiddenValue(IDName){
		var RTNValue = '';
		if (parent) {
			var GOBJ = parent.document.getElementById(IDName);
			if (GOBJ) {
				RTNValue = GOBJ.value;
			}
		}
		return RTNValue;
	}
	//將取得的資料設定為預設選項
	function DecideSelectChecked(IDName,RcvDEF,Mode){
		switch(Mode){
			case 1:
				var DefValue = GetHiddenValue(IDName);
				if (DefValue!='') { document.getElementById(IDName).value = DefValue; };
				break;
			case 2:
			case 3:
				if (RcvDEF=='') {
					var DefValue = GetHiddenValue(IDName);
					if (DefValue!='') { document.getElementById(IDName).value = DefValue; };
				} else {
					document.getElementById(IDName).value = RcvDEF;
				}
				break;
			default:
		}
	}
	</script>
	

