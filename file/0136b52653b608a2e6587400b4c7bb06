<script language="JavaScript" type="text/JavaScript">
// 2010-12-15 [Ham] 連動帶出 "旅遊地區" 選項的方式，改為用 iFrame
function garea_sw_new(ival)
{
	var frmitem = eval("self.iframeGAREA");
	frmitem.location.href = "/include/get_garea_option.asp?AREA_CD=" + ival;
}
function set_default_form()
{
	var form1 = document.FORM1;
	for (i=0 ; i<3 ; i++) {
		form1.elements[i].value = "不限";
	}
	for (i=3 ; i<13 ; i++) {
		form1.elements[i].selectedIndex = 0;
	}
	for (i=13 ; i<16 ; i++) {
		form1.elements[i].value = "";
	}
}
function garea_sw(ival)
{
	var form = document.FORM1;
	form.iGAREA.options.length = 0;
	var strGarea = showModalDialog("/eWeb/Module/inc_M_S_GO_garea.asp?area_cd="+ival+"","","dialogWidth=0pt;dialogHeight=0pt");
	var ary_Area = strGarea.split("<br>");
	if (ival == "") {
		form.iGAREA.options.length = 0;
		form.iGAREA.options[0]=new Option("不限","", 0);
		for (var i=1 ; i<ary_Area.length ; i++) {
			if (ary_Area[i].value != " ") {
				var ary_Area_dtl = ary_Area[i].split("#");
				if (ary_Area_dtl[0] != "") {
					if (ary_Area_dtl[1] != "") {
						form.iGAREA.options[i+1] = new Option(ary_Area_dtl[1], ary_Area_dtl[0], i);
					}
				}
			}
		}
		// 2009-12-29 Poison [EW.v806.098.B] 加入 selectedindex 計算，排除 Google 瀏覽器會將下拉選單指到最後一筆的問題
		form.iGAREA.selectedIndex = 0;
	} else {
		form.iGAREA.options.length=0;
		form.iGAREA.options[0]=new Option("不限","", 0);
		for (var i=1 ; i<ary_Area.length ; i++) {
			if (ary_Area[i].value != " ") {
				var ary_Area_dtl = ary_Area[i].split("#");
				if (ary_Area_dtl[0] != ""){
					if (ary_Area_dtl[1] != "") {
						form.iGAREA.options[i] = new Option(ary_Area_dtl[1], ary_Area_dtl[0], i);
					}
				}
			}
		}
		// 2009-12-29 Poison [EW.v806.098.B] 加入 selectedindex 計算，排除 Google 瀏覽器會將下拉選單指到最後一筆的問題
		form.iGAREA.selectedIndex = 0;
	}
}
function chkform()
{
/*
2013-10-18 [Aron] 修改搜尋引擎規則
規則1：州別國家城市的值不為空值才通過檢查。
規則2：關鍵字若不為空值，則必須大於兩個字才通過檢查。
修改檢查函式chkform()驗證規則如下
1.	州別國家城市的模式，套用規則1。
2.	州別國家城市+關鍵字，若關鍵字為空值套用規則1；否則套用規則2。
3.	旅遊區域的模式，不做任何檢查直接允許submit。
4.	旅遊區域+關鍵字，套用規則2。
*/

	if (document.getElementById("ikeyword").value.length == 1 ) {
		alert("關鍵字須大於 2 個字！");
		return false;
	}

	if (document.getElementsByName("TVL_DAREA").value) {
		var VarA = GetSelectValue("TVL_DAREA");
		var VarB = GetSelectValue("TVL_SAREA");
	}

	document.getElementById("FORM1").submit();
}
</script>

