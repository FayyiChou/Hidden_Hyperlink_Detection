function common_screen_fit() {
    var doc = document;
    var docElemProp = doc.documentElement["clientWidth"];
    var clientwidth = parseInt(doc.compatMode === "CSS1Compat" && docElemProp || doc.body["clientWidth"] || docElemProp);
    if (clientwidth < 1400) {
        doc.body.className = 'responsive960';
    } else{
        doc.body.className = 'responsive1200';
    }
}
common_screen_fit();
window.onresize = function() {
    common_screen_fit();
}
