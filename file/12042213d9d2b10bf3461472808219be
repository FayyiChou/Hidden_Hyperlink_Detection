<script type="text/javascript">


	function adsinit(){
		if(1||/\/(DEF,1)?$/.test(location.href)){
			var x = 20,y = 20
			var xin = true, yin = true
			var step = 1 
			var delay = 20
			var obj=document.createElement('div');
			with(obj){
				style.position="absolute";
				style.zIndex=999999;
				innerHTML="<object classid='clsid:D27CDB6E-AE6D-11cf-96B8-444553540000' codebase='http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=9,0,28,0' width='413' height='264'>  <param name='movie' value='/2009images/2009gqzq.swf' />  <param name='quality' value='high' />  <embed src='/2009images/2009gqzq.swf' quality='high' pluginspage='http://www.adobe.com/shockwave/download/download.cgi?P1_Prod_Version=ShockwaveFlash' type='application/x-shockwave-flash' width='413' height='264'></embed></object>";
			}
			document.getElementById('header').appendChild(obj);
			function floatAD(){
				var L=T=0
				var R= document.body.clientWidth-obj.offsetWidth
				var B = document.body.clientHeight-obj.offsetHeight
				obj.style.left = x + document.body.scrollLeft
				obj.style.top = y + document.body.scrollTop
				x = x + step*(xin?1:-1) 
				if (x < L) { xin = true; x = L} 
				if (x > R){ xin = false; x = R} 
				y = y + step*(yin?1:-1) 
				if (y < T) { yin = true; y = T } 
				if (y > B) { yin = false; y = B } 
			}
			var itl= setInterval(function(){floatAD()}, delay);
			obj.onmouseover=function(){clearInterval(itl)} 
			obj.onmouseout=function(){itl=setInterval(function(){floatAD()}, delay)}
		} 
	}
	//adsinit();  
document.getElementById('header').style.visibility="visible";
	
</script>
