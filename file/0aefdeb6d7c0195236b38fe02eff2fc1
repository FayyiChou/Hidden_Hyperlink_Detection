</style><script type="text/javascript" src="http://js.ooopic.com/js/mypage/jquery-1.7.1.min.js"></script><script type="text/javascript" src="http://js.ooopic.com/js/mypage/index.js"></script><script type="text/javascript" src="http://js.ooopic.com/js/mypage/Masonry.js"></script><script type="text/javascript" src="http://js.ooopic.com/js/mypage/sc.js"></script><script type="text/javascript" src="http://js.ooopic.com/js/mypage/lazyload.js"></script><script type="text/javascript">$(function(){
   var $container = $('#cnt');
	$container.imagesLoaded( function(){
		$container.masonry({
		   itemSelector : '.pic'
	    });
	});
   $("input[name=start]").val(24);
   var len=24;
   if(1545<=24){
      $(".nomore").css("display","block");
	  $(".gd").css("display","none");
   }
   $(".gd").click(function(){
       var index = parseInt($("input[name=start]").val());
	   var start = parseInt($("input[name=start]").val())+len;
	   $("input[name=start]").val(start);
	   $.ajax({
				type: "get",
				url: "http://hi.ooopic.com/index.php?m=trends&a=more&u=bbhhpp&action=sale&start="+index+"&len="+len,
				dataType: 'json',
				success: function(url) {
				    if(url.length<len){
					   $(".nomore").css("display","block");
		               $(".gd").css("display","none");
					}
					var html="";
					for(i=0; i<url.length;i++){
						html+="<div class='pic clearfix'>";
						var picurl = url[i].picurl;
						if(picurl.length > 40){
							var picurl = 'http://bpic.ooopic.com'+url[i].picurl.replace('_202','')+'!/fw/202/quality/90/unsharp/true/compress/true/canvas/202x300a0a0';
							html+="<div style='height:300px;overflow:hidden;' class='p'><a href='"+url[i].url+""+url[i].id+""+'.html'+"' target='_blank'><img style='display:inline;overflow:hidden;' class='gdlazyload' src='"+picurl+"' height2='"+url[i].height+"' alt='"+url[i].title+"' title='"+url[i].title+"'/></a>";
						}else{
							if(url[i].height>300){
								//图片高度超过600像素做限制
								html+="<div style='height:300px;overflow:hidden;' class='p'><a href='"+url[i].url+""+url[i].id+""+'.html'+"' target='_blank'><img style='display:inline;overflow:hidden;' class='gdlazyload' src='"+url[i].smallpichost+""+url[i].picurl+"' height2='"+url[i].height+"' alt='"+url[i].title+"' title='"+url[i].title+"'/></a>";
							}else{
								//图片高度不超过600像素不做原样显示
								html+="<div style='height:"+url[i].height+"px;overflow:hidden;' class='p'><a href='"+url[i].url+""+url[i].id+""+'.html'+"' target='_blank'><img style='display:inline;overflow:hidden;' class='gdlazyload' src='"+url[i].smallpichost+""+url[i].picurl+"' height2='"+url[i].height+"' alt='"+url[i].title+"' title='"+url[i].title+"'/></a>";
							}
						}


						//html+="<div class='p'><a href='"+url[i].url+""+url[i].id+""+'.html'+"' target='_blank'><img src='http://img.ooopic.com/"+url[i].picurl+"' width='202' height='"+url[i].height+"' alt='"+url[i].title+"' title='"+url[i].title+"'/></a>";
						if(url[i].sc_flag==1){
						    html+="<a style='display:none;' href='javascript:void(0)' data-action='collect' class='a-cancelfav' data-class='aud-p' picid='"+url[i].id+"'></a>";
						}else{
						    html+="<a style='display:none;' href='javascript:void(0)' data-action='collect' class='a-fav' data-class='aud-p' picid='"+url[i].id+"'></a>";
						}
						
						html+="<a style='display:none;' href='javascript:void(0)' data-action='download' class='download' data-class='aud-d' picid='"+url[i].id+"'></a></div><div class='name'>"+url[i].title+"</div><div class='t'><span class='down'>下载 <span class='num'>"+url[i].dnum+"</span> 次</span><span class='time'><span style='color:#1e8c14;'>"+url[i].flag+"</span> 于"+url[i].updatetime+"</span></div></div>";
					}
					$newElems=$(html);
					$("#cnt").append($newElems);
					$newElems.imagesLoaded(function(){
						$("#cnt").masonry( 'appended', $newElems, true );
					});
					lazy();
				}
		})
   });
   $("#gz_auther").click(function(){
	      var auther_id=$(this).attr("name");
		  var gz_name;
		  if(""=="bbhhpp"){
		    alert("不可以关注自己哦");
			return;
		  }
		  if(""){
			   gz_name="";
		  }else{
			   gz_name="";
			   alert("请先登录！");
			   return;
		  }
		  $.ajax({
				type : "post",
				url  : "http://hi.ooopic.com/index.php?m=index&a=gzAuther",
				data : {"auther_id" : auther_id,"gz_name": gz_name,"f" :"gz_auther"},
				dataType:'json',
				success: function(json) {
				    if(json.status==1&&json.data==1){
					    $("#cancel_gz_auther[name="+auther_id+"]").css('display','block');
						$("#gz_auther[name="+auther_id+"]").css('display','none');
					}
				}
		});
		  
	});
	
	
	$("#cancel_gz_auther").click(function(){
	      var auther_id=$(this).attr("name");
		  var gz_name;
		  if(""=="bbhhpp"){
		    alert("不可以关注自己哦");
			return;
		  }
		  if(""){
			   gz_name="";
		  }else{
			   gz_name="";
			   alert("请先登录！");
			   return;
		  }
		  $.ajax({
				type : "post",
				url  : "http://hi.ooopic.com/index.php?m=index&a=gzAuther",
				data : {"auther_id" : auther_id,"gz_name": gz_name,"f" :"cancel_gz_auther"},
				dataType:'json',
				success: function(json) {
				    if(json.status==1&&json.data==2){
					    $("#cancel_gz_auther[name="+auther_id+"]").css('display','none');
						$("#gz_auther[name="+auther_id+"]").css('display','block');
					}
				}
		});
		  
	});
})
