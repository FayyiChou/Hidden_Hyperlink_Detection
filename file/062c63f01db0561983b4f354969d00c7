        var cn = $('.text').children();
        var suc = false;
        var jump = 'img, div, embed, object, table';
        for (var i = 1; i < cn.length - 2; i++) {
            var c = cn.eq(i);
            var p = 0;
            if (!c.is(jump) && c.find(jump).length == 0) {
                while (p < bdf.outerHeight(true)) {
                    if (c.next().length == 0 || c.is(jump) || c.find(jump).length > 0) {
                        suc = false;
                        break;
                    }

                    var h = c.outerHeight(true) - parseInt(c.css('marginTop'));
                    var n = c.next();
                    if (c.position().top + h >= n.position().top) {
                        p += h;
                        c = n;
                        i++;
                        suc = true;
                    }
                    else {
                        suc = false;
                        break;
                    }
                }

                if (suc) break;
            }

            c.after(bdf);
        }

        if (suc) {
