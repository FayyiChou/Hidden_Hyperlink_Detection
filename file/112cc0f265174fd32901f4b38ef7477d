 function vNav(){
	if($('.nav_set').length <1){return false}
 	var up = 210;
 	var down = 111;
 	var nav = $('.nav_set');
 	var nav_up = nav.children('li');
 	var nav_down = $('.nav-down');
 	//var bar = $('nav_set li').find("a");
 	var t = nav.position().left;
	
	
 	nav.bind({'mouseenter':function(){
 		nav_down.stop().animate({top:down+'px'});
 		//bar.addClass("hover");
 	},'mouseleave':function(e){
 		var evt = $(e.relatedTarget);
 		if(evt.parents('.nav-down').length <1 && !evt.hasClass('nav-down')){
 			nav_down.stop().animate({top:'-'+up+'px'});
 			//bar.removeClass("hover");
 			nav_up.removeClass('on');
 		}
 	}})
 	nav_down.bind('mouseleave',function(e){
 		var evt = $(e.relatedTarget);
 		if(evt.parents('.nav').length <1 && !evt.hasClass('nav')){
 			nav_down.stop().animate({top:'-'+up+'px'});
 			//bar.removeClass("hover");
 			nav_up.removeClass('on');
 		}
 	})
 	nav_up.bind({'mouseenter':function(){
 		var offX = $(this).position().left;
 		var index = $(this).index();
 		$(this).addClass('on').siblings().removeClass('on');
 		nav_down.find('ul').eq(index).addClass('on').siblings().removeClass('on');
 		//bar.find('a').eq(index).addClass('hover').siblings().removeClass('on');
 	}});
 	nav_down.find('ul').bind('mouseenter',function(){
 		var index = $(this).index();
 		$(this).addClass('on').siblings().removeClass('on');
 		nav_up.eq(index).addClass('on').siblings().removeClass('on');
 		//bar.eq(index).addClass('hover').siblings().removeClass('hover');
 	})
 }
	
});


</script>
