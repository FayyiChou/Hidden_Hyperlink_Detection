    $('#search_string').autocomplete({

         source: function(request, response) {
           $.ajax({
             url: '/require/rep_add_select.php',
             dataType: 'json',
             data: {
               term : $('#search_string').val(),
               type : $('#search_type option:selected').val(), },
             success: function(data) { response(data); }
           });
         },

         minLength: 4,

         focus: function( event, ui ) {
           $( '#test' ).val( ui.item.label );
           return false;
         },
         
         select: function( event, ui ) {
           $( '#search_string' ).val( ui.item.string );
           return false;
         }

    });

