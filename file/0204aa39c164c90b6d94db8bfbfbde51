  <script type="text/javascript">
$(function(){
	$('div.today_release').tabs(".column_tab","on",".column_con");
	$('.size a').click(function(){
		$(this).addClass("on").siblings().removeClass("on");
		var size = $(this).attr("size");
		$('#detail_header_list img').attr('style','width:'+size+'px;height:'+size+'px');
		$('#detail_header_list').attr('class','imgList headerList_'+size);
	});

	$('div.mem_des textarea').bind({
		mouseenter:function(){$(this).parent().css("border-color","#fe7171")},
		mouseleave:function(){$(this).parent().css("border-color","#e1e1e1")},
		focus:function(){$(this).attr("readonly","")},
		blur:function(){$(this).attr("readonly","readonly")}
	});
	$('#textContent').txtCount($('#textTips'), 140);
	$('a.ico_emotion').click(function(){
		Face.load();
		return false;
	});
	$(document).click(function(){$('#js_emotion').hide();});
});
</script> 
