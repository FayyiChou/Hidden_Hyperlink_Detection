<script language="javascript" type="text/javascript">
var CPName = '超级大乐透';
chart.tbox = 0;

// 预测区单行清空选号（行号，是否清除工具箱）
function yc_clear($tds,n,clear){
	var $ball = $tds.filter('.q1,.q2');
	if($ball.length > 0){
		$ball.each(function(){
			var i = $tds.index(this);
			var cName = 'y1_1';
			if ((i >= 15 && i <= 17) || (i >= 21 && i <= 23) || i >= 29)
				cName = 'y2_1';
			this.className = cName;
		});
		delete yc.ball[n]
		delete yc.hm_str[n];
		if(clear || false) tbox.clear(n);
	}
}

// 折线样式
function reset_cLine() {
	var $z2 = $('#z2');
	var $z1 = $('#z1');
	return {
	'hq': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	},
	'dx': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	},
	'jo': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	},
	'zh': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	},
	'012': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	},
	'spj': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	}
	}
}

$(function(){
	var line = reset_cLine();
	init('cjdlt', '/cjdlt/php/dwzs2.php', ['hq'],line);
});
</script>

