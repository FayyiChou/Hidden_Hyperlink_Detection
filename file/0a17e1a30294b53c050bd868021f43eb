    //返回顶部
    $(window).scroll(function () {
        if ($(window).scrollTop() > 50) {
            $(".gotop").fadeIn(100);
        } else {
            $(".gotop").fadeOut(100);
        }
    });
    var cateId = 6314;
    //当点击跳转链接后，回到页面顶部位置
    $(".gotop").click(function () {
        $('body,html').animate({
            scrollTop: 0
        }, 0);
        return false;
    });
    initNavigator();
    function initNavigator() {
        var display = false;
        var nav = [];
        nav.push("<h3>内容导航</h3>");
        $('.content_mod_tit').each(function (index, dom) {
//            if (dom.innerText != '') {
//                nav.push('<p><a href="#content_mod_tit' + index + '"><em></em><b></b>' + dom.innerText + '</a></p>');
//                dom.setAttribute('id', "content_mod_tit" + index);
//                display = true;
//            }
            if ($(this).text() != '') {
                nav.push('<p><a href="#content_mod_tit' + index + '"><em></em><b></b>' + $(this).text() + '</a></p>');
                dom.setAttribute('id', "content_mod_tit" + index);
                display = true;
            }

        });
        if (display) {
            $("#navDom").empty();
            $("#navDom").append(nav.join(""));
        } else {
            $("#navDom").hide();
        }

        //快捷导航埋点
        var cateid3 = $("#infoCate3Id").val();
        $("#navDom a").each(function (index, dom) {
            $(this).click(function () {
                var num = index + 1;
                clickLog('from=bm_pc_detail_nrdaohang_' + num + '_' + cateid3);
            })
        })
    }
    initDownload();
    function initDownload() {
        var display = false;
        var nav = [];
        nav.push("<h3>材料下载</h3>");
        $('.content_mod_cont a').each(function (index, dom) {
            index = index + 1;
            if (dom.getAttribute("href").indexOf("58cdn.com.cn") > -1) {
                var infoCate3Id = $("#infoCate3Id").val();
                dom.onclick = function () {
                    clickLog('from=bm_pc_detail_caioliao_left_' + index + "_" + infoCate3Id);
                };
                display = true;
                nav.push('<p><a onclick="clickLog(\'from=bm_pc_detail_caioliao_right_' + index + "_" + infoCate3Id + '\')" href = "' + dom.getAttribute("href") + '"><span>' + dom.innerText + '</span><i></i></a></p>');
            } else {
                return;
            }
        });
        if (display) {
            $("#downloadDom").empty();
            $("#downloadDom").append(nav.join(""));
        } else {
            $("#downloadDom").hide();
        }
    }
</script>


