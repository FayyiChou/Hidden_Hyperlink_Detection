		_ub.collect(0,{"vwg.page":maima_page,"vwn.picturetype":encodeURIComponent(picturetype)});
	}else{
		_ub.collect(0,{"vwg.page":maima_page,"vwn.picturetype":encodeURIComponent('交通图')});
	}
</script>
		 <!--装修服务期限制-->
				</div>

