				function show_rate()
				{
					var c  = document.getElementsByName('manager');
					var id = '';
					for( var i=0;i<c.length;i++)
					{
						if(c[i].checked)
						{
							id += c[i].value + ',';
						}
					}
					id = id.substr(0, id.length-1);
					showWindow('rate', '/plugin.php?id=zzinc_member:rate&action=rate&page=1&fid=734&tid=11104582&pid='+id, 'get', -1);
				}
				function checkall_manage()
				{
					var c  = document.getElementsByName('manager');
					for( var i=0;i<c.length;i++)
					{
						if(c[i].checked == false)
						{
							c[i].checked = true;
						}
					}
