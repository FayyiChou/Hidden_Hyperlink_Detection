            <script type="text/javascript" language="javascript">
            var tx = document.getElementById("tx"), pwd = document.getElementById("ls_password");
            tx.onfocus = function()
            {
            if(this.value != "密码") return;
            this.style.display = "none";
            pwd.style.display = "";
            pwd.value = "";
            pwd.focus();
            }
            pwd.onblur = function()
            {
            if(this.value != "") return;
            this.style.display = "none";
            tx.style.display = "";
            tx.value = "密码";
            }
           function hidden_account(e)
           {
        	   e.value = '';
        	   e.style.cssText = 'color:black';
           }
         // 定义登录对象
         var login = {
             // 初始化
             init : function() {
                 
                 // api登录
                 $('#sind-login').bind('click', function(){
                     login.api_login('sina');
                 });
                 $('#qq-login').bind('click', function(){
                     login.api_login('qq');
                 });
                 
             },
             
             api_login : function(type){
            	
                 var login_url = 'http://account.178.com/';
                 var width = 0;
                 var height = 0;
                 if (type == 'qq') {
                     login_url += 'tencent.php';
                     width = 820;
                     height = 650;
                 } else {
                     login_url += 'sina.php';
                     width = 620;
                     height = 540;
                 }

                 // 使用了代理登录，因为绑定要弹出新窗口
                 login_url += '?p=register&to=http://bbs.178.com/static/loginproxy.html?v=107';

                 // 注册登录回调函数
                 window.__login_call_back__ = function(){
                     // 登录完成，这里可以做登录以后的一些事情
                     window.location.reload();
                 };
                 document.domain = '178.com';
                 // 打开新窗口，登录，回调     
                 var loginPopWindow = window.open(login_url, 't178_apilogin',["toolbar=0,scrollbars=1,status=0,resizable=1,width=", width, ",height=", height,",left=", (screen.width - width) / 2, ",top=", (screen.height - height) / 2].join(""));
                 loginPopWindow.focus();
             }
         };
       
         
            if (window.addEventListener) {
    	     	document.getElementById('sina-login').addEventListener('click', function () 
    	     	{
    	     		 login.api_login('sina');
    	     	}, false);
    	     	 document.getElementById('qq-login').addEventListener('click', function () {
