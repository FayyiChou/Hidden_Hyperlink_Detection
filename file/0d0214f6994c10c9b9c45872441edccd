                            <td valign="top"><script>
    function swap_mtyw(tt){
			switch(tt){
				case"mtyw1":
				document.getElementById("mtyw1").style.display = "block";
				document.getElementById("mtyw2").style.display = "none";
				document.getElementById("mtyw3").style.display = "none";
				document.getElementById("mtyw4").style.display = "none";
					break;
				case"mtyw2":
				document.getElementById("mtyw1").style.display = "none";
				document.getElementById("mtyw2").style.display = "block";
				document.getElementById("mtyw3").style.display = "none";
				document.getElementById("mtyw4").style.display = "none";
				     break;
				case"mtyw3":
				document.getElementById("mtyw1").style.display = "none";
				document.getElementById("mtyw2").style.display = "none";
				document.getElementById("mtyw3").style.display = "block";
				document.getElementById("mtyw4").style.display = "none";
				     break;
				case"mtyw4":
				document.getElementById("mtyw1").style.display = "none";
				document.getElementById("mtyw2").style.display = "none";
				document.getElementById("mtyw3").style.display = "none";
				document.getElementById("mtyw4").style.display = "block";
				     break;
			}
		}
		                    </script>
