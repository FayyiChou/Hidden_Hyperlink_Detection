            MaxValue: getId("idContent").scrollWidth - getId("idContent").clientWidth,
            onMin: function(){ getId("idSliderLeft").style.backgroundPosition = "bottom left"; },
            onMax: function(){ getId("idSliderRight").style.backgroundPosition = "bottom right"; },
            onMid: function(){ getId("idSliderLeft").style.backgroundPosition = "top left"; getId("idSliderRight").style.backgroundPosition = "top right"; },
            onMove: function(){ getId("idContent").scrollLeft = this.GetValue(); }
        });
        getId("idContent").scrollLeft =  sld.GetValue();
        //sld.SetPercent(.5);
        sld.Ease = true;
         
