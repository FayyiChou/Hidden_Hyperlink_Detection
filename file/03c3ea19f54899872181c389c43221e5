		$(document).ready(function(){

			var slist = '';

			var flist = '';
			
			var cpage = npage-1;

			for(var key in simages){
				if(key > 0)

				slist += ' <a href="'+images[key]+'" class="cboxElement" target="_blank" style="text-decoration:none;" rel="preview"><img src="../../images/auto/images/but1.gif" width="65" height="25" style="display:none" /></a>';
				else
				slist += '<a href="'+images[key]+'" class="cboxElement" target="_blank" style="text-decoration:none;" rel="preview"><img src="../../images/auto/images/but1.gif" width="65" height="25" /></a>';

				if(location.href.indexOf('_') != -1)
				{
					urls = location.href.split('_');
					url = location.href.replace('_'+urls[1] ,'_'+(parseInt(key)+1)+'.html');
				}
				else
				{
					url = location.href.replace('.html','_'+(parseInt(key)+1)+'.html');
				}
				
				var str1 = '';
				var str2 = '';
				if (key == totalpage - 1)
				{
					str1 = 'style="margin-right:0;"';
				}
				str1 = str1 + 'sum="'+totalpage+'"' + 'max="div"';
				if (key == npage - 1)
				{
					str2 = 'now';
				}

				flist += '<td><div '+ str1 +' class="'+ str2 +'"><a href="'+url+'"><img src="'+simages[key]+'" width="111" height="82" pic="img" px="'+ (parseInt(key)+1) +'" /></a></div></td>';

			}

			$("#full").html(slist);
			//$("#flist").html(flist);
			
			if ($.browser.msie) {
				$(".big_arr_right").attr("style","height:480px; background:#fff;filter:alpha(opacity=0);-moz-opacity: 0;opacity: 0;cursor:pointer;");
				$(".big_arr_left").attr("style","height:480px; background:#fff;filter:alpha(opacity=0);-moz-opacity: 0;opacity: 0;cursor:pointer;");
				
			}else {
				$(".big_arr_right").height(480);
				$(".big_arr_left").height(480);
			}
			$("a[rel='preview']").colorbox();
			
			
			//向右按钮
			$(".right_arr").click(function(){
				if(npage==totalpage)
				{
					if(nextpageurl == "javascript:void(0)")
					{
						alert("没有了哦");
					}
					else
					{
						location.href = nextpageurl;
					}
				}
				else
				{
						if(location.href.indexOf('_') != -1)
						{
							urls = location.href.split('_');
							location.href = location.href.replace('_'+urls[1] ,'_'+(npage+1)+'.html');
						}
						else
						{
							location.href = location.href.replace('.html','_2.html');
						}
				}

			});
			//向左按钮
			$(".left_arr").click(function(){
				if(npage<2)
				{
					if(prepageurl == "javascript:void(0)")
					{
						alert("这是第一页");
					}
					else
					{
						location.href = prepageurl;
					}
				}
				else
				{
					if(npage==2) 
					{
					   location.href = location.href.replace('_2.html','.html');
					} 
					else
					{
						if(location.href.indexOf('_') != -1)
						{
							urls = location.href.split('_');
							location.href = location.href.replace('_'+urls[1] ,'_'+(npage-1)+'.html');
						}
					}
				}

			});
			//大图向左按钮
			$(".big_arr_left").click(function(){
				if(npage<2)
				{
					if(prepageurl == "javascript:void(0)")
					{
						alert("这是第一页");
					}
					else
					{
						location.href = prepageurl;
					}
				}
				else
				{
					if(npage==2) 
					{
					   location.href = location.href.replace('_2.html','.html');
					} 
					else
					{
						if(location.href.indexOf('_') != -1)
						{
							urls = location.href.split('_');
							location.href = location.href.replace('_'+urls[1] ,'_'+(npage-1)+'.html');
						}
					}
				}

			}).mouseover(function(){

				$("#leftpic").show();
			}).mouseout(function(){
				$("#leftpic").hide();
			})
			//大图向右按钮
			$(".big_arr_right").click(function(){
				
				if(npage==totalpage)
