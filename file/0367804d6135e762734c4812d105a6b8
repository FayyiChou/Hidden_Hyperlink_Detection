$(document).ready(function () {
    $('a#toggler1').click(function (e) { // ëè÷íîå ìåíþ
      $(this).toggleClass('active');
      $('.t2').toggle();
         
      e.stopPropagation();
    });
 
    $('.t2').click(function (e) {
      e.stopPropagation();
    });
 
    $('body').click(function () {
      var link = $('a#toggler1');
      if (link.hasClass('active')) {
        link.click();
      }
    });
  });
</script>
