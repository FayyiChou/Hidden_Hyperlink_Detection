<script type="text/javascript">
function clos(){
var v = $('#sy_text').val();
if(v==''){
	$('#sy_text').val('汾酒典藏原浆');
	$('#sy_text').css('color','#999999');
}
shutList();
}
/*搜索下拉框键盘事件*/
function movesel(event){
	event = event || window.event;
	var keyCode = event.keyCode;
	if(keyCode=='40'){			
		var objall = $('#keyslist li');
		var all = objall.size()-2;
		var yy = $('#keyslist li:eq('+all+') a');	
		if(yy.css("background-color")=="rgb(255, 223, 198)" || yy.css("background-color")=="#ffdfc6"){		
			var x = $('#keyslist li:eq(0) a span:first');	
			var y = $('#keyslist li:eq(0) a');					
			$('#sy_text').val(x.text());
			$('#keyslist li a').css("background-color","#ffffff");
			y.css("background-color","#ffdfc6");
			return;	
		}
	    for(var i=0;i<9;i++){							
		    var objy = $('#keyslist li:eq('+i+')').find('a');
		    if(objy.css("background-color") == "rgb(255, 223, 198)" || objy.css("background-color") == "#ffdfc6"){
		    	$('#keyslist li a').css("background-color","#ffffff");
				var ox = $('#keyslist li:eq('+(i+1)+') a span:first');					
				var oy = $('#keyslist li:eq('+(i+1)+')').find('a');					
				$('#sy_text').val(ox.text());
				oy.css("background-color","#ffdfc6");
				return;
			}
	    }
		var x = $('#keyslist li:eq(0) a span:first');					
		var y = $('#keyslist li:eq(0)').find('a');					
		$('#sy_text').val(x.text());
		y.css("background-color","#ffdfc6");	
}else if(keyCode=='38'){
	var objall = $('#keyslist li');
	var all = objall.size()-2;
		var yy = $('#keyslist li:eq(0) a');	
		if(yy.css("background-color")=="rgb(255, 223, 198)" || yy.css("background-color")=="#ffdfc6"){			
			var x = $('#keyslist li:eq('+all+') a span:first');
			var y = $('#keyslist li:eq('+all+') a');					
			$('#sy_text').val(x.text());
			$('#keyslist li a').css("background-color","#ffffff");
			y.css("background-color","#ffdfc6");
			return;	
		}
	    for(var i=9;i>0;i--){							
		    var objy = $('#keyslist li:eq('+i+') a');
		    if(objy.css("background-color") == "rgb(255, 223, 198)" || objy.css("background-color") == "#ffdfc6"){
		    	$('#keyslist li a').css("background-color","#ffffff");
				var ox = $('#keyslist li:eq('+(i-1)+') a span:first');
				var oy = $('#keyslist li:eq('+(i-1)+') a');					
				$('#sy_text').val(ox.text());
				oy.css("background-color","#ffdfc6");
				return;
			}
	    }
		var x = $('#keyslist li:eq('+all+') a span:first');
		var y = $('#keyslist li:eq('+all+') a');					
		$('#sy_text').val(x.text());
		y.css("background-color","#ffdfc6");	
	}else if(keyCode=='13'){
		 for(var i=0;i<10;i++){							
			    var objy = $('#keyslist li:eq('+i+')').find('a');
			    if(objy.css("background-color") == "rgb(255, 223, 198)" || objy.css("background-color") == "#ffdfc6"){
					var url = $('#keyslist li:eq('+i+') a').attr('href');
					window.location.href=url;
					return;
				}
		    }
	}
}
/////*搜索下拉框键盘事件*/
</script>
<script>
function _over(obj){
	$("#keyslist ul li a").css('background-color','#ffffff');
	$(obj).css('background-color','#ffdfc6');
}
function _out(obj){
	$(obj).css('background-color','#ffffff');
}
</script>
