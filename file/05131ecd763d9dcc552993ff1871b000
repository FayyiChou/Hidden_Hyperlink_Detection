<script type="text/javascript">
	function setFoucs(){
		$('#sy_text').attr("onblur","javascript:if(this.value=='')this.value='汾酒典藏原浆';this.style.color='#999999';shutList();");
	}
	function clearFoucs(){
		$('#sy_text').attr("onblur","javascript:if(this.value=='')this.value='汾酒典藏原浆';this.style.color='#999999';");
	}
	function shutList(){
		$('#keyslist').hide();
	}
	function hideList(){
		$('#keyslist').hide();
	}
	function searchRelatedkeys(event){
		event = event || window.event;
		var keyCode = event.keyCode;
		if(keyCode=='40' || keyCode=='38'){
			return;
		}
		var value = $("#sy_text").val();
		var keywords=$.trim(value);
		if(keywords==''){
			$('#keyslist').hide();
			return false;
		}
	     $.ajax({
	         type: "POST",
	         url:"/web.php?c=search&m=search_related_ajax",
	         data: 'keywords='+keywords,
	         dataType:"json",
	         success:function(data){
	             if(data!=''){
	            	 $('#keyslist').show();
	            	 var keyslistHtml = "<ul id='snp'>";
		        	 var jsonList=eval(data);
		        	 for(var i=0;i<jsonList.length;i++){ 
		        		 keyslistHtml += '<li><a  onmouseover="_over(this)"  onmouseout="_out(this)"  href="'+jsonList[i].url_guanwang+'" class="relatedkeys"><span class="tt024left" id="r_keys'+i+'">'+jsonList[i].name+'</span><span class="tt024sp">约<font>'+jsonList[i].count+'</font>件商品</span></a></li>';
		        	 }
		        	 keyslistHtml += '<li><a href="javascript:shutList();" class="relatedkeys"><span style="text-align:right; display:block; padding-right:10px; color:#999999;">关闭</span></a></li>';
		        	 keyslistHtml += '</ul>';
		        	 $('#keyslist').html(keyslistHtml);
		        	 l = $("#keyslist").offset().left;
		        	 ll = $(".ntb2").offset().left;
		    		 if(l-ll != 0){
		    			 $("#keyslist").css('left',ll);
			         }
	             }else{
	            	 $('#keyslist').hide();
	 	        }
	         }
	     });  
	}
</script>
