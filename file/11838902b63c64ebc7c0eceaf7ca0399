<script language="JavaScript">
function CheckAll(form){
	for (var i=0;i<form.elements.length-0;i++){
		var e = form.elements[i];
		e.checked == true ? e.checked = false : e.checked = true;
	}
}


function operation(obj){
	 var bscId = 4037723
 	 document.form1.action = "my_book_panelover.php";				
 	 document.form1.submit();
}

function ctrlLayerView()
{
	var obj = document.getElementsByTagName('input');
	var j = obj.length;
	for (var i = 0; i < j; i++) {
		if (obj[i].type == "checkbox" && obj[i].name != "selectAll") {
			if (obj[i].checked == true) {
				document.getElementById('mdLayer').style.display = "block";
				return true;
			}
		}
	}
    
	document.getElementById('mdLayer').style.display = "none";
	return true;
}
</script>

