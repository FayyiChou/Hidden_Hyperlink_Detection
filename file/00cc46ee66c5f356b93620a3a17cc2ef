          <script type="text/javascript">
          	function search() {
          		if($.trim($("#keyword").val()) == "") {
          			$("#keyword").val("请输入您的问题");
          			$("#keyword").css("color","red");
          			return ;
          		} else {
          			if($.trim($("#keyword").val()) == "请输入您的问题") {
          				$("#keyword").css("color","red");
          				return ;
          			} else {
          				$("#keyword").val($.trim($("#keyword").val()));
          				$("#area").val($("#areaSelect").children('option:selected').val());
          				$("#time").val($("#timeSelect").children('option:selected').val());
          				$("#sform").submit();
          			}
          		}
          	}
          	
          	function ask(obj) {
          		var kw = $.trim($("#keyword").val());
          		if(kw == "" || kw == "请输入您的问题") {
          			obj.href = basePath + "/question/new";
          		} else {
          			obj.href = basePath + "/question/new?keyword=" + encodeURI(kw);
          		}
          	}
          </script>
          <div class="left a_666"><a href="javascript:void(0)" onclick="ask(this);" class="btn_ask">我要提问</a></div>
        </form>
