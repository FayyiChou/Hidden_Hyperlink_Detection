	<script type="text/javascript">
	
	function getUrlqzStr(strUrl){  
	    var q=strUrl;  
	    var qs=q.split('.'); 
	    var hw=qs[qs.length - 2];

	    if(hw == 'com' || hw == 'net' || hw == 'org')
	    {
	    	hw=qs[qs.length - 3];
	    }
	    return hw;  
	} 
	var qz = getUrlqzStr(window.location.hostname);
	document.getElementById("hh").innerText = qz;
	document.title = qz;
	
	</script>