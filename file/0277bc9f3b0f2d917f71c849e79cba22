 	// 加入收藏  
 	function addCookie(){  
		if (document.all){    
			window.external.addFavorite('http://localhost:8080/index.jsp','内蒙古电子商务协会');    
		}else if(window.sidebar){  
			window.sidebar.addPanel('内蒙古电子商务协会', 'http://localhost:8080/index.jsp', ""); 
		}  else{
			alert( "亲爱的用户你好：\n你使用的不是IE浏览器，此操作被浏览器阻挡了，你可以选择手动设置！\n给你带来的不便，本站深表歉意。"); 
		}
		}
 
  
  
function setHomepage(){ 
	if (document.all){ 
		document.body.style.behavior='url(#default#homepage)';  
		document.body.setHomePage('http://localhost:8080/index.jsp');
	 }else if(window.sidebar){
			if(window.netscape){
			try{
			netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect"); 
			}catch (e){ 
					alert( "亲爱的用户你好：\n你使用的不是IE浏览器，此操作被浏览器阻挡了，你可以选择手动设置为首页！\n给你带来的不便，本站深表歉意。"); 
		}
		}	 
		} 
	}
 </script>
