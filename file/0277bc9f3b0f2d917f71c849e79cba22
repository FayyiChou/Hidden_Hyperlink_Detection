<script language="javascript">
var bodyfrm = ( document.compatMode.toLowerCase()=="css1compat" ) ? document.documentElement : document.body;
var adst = document.getElementById("ShowAD").style;
adst.top = ( bodyfrm.clientHeight -250-22 ) + "px";
adst.left = ( bodyfrm.clientWidth -85 ) + "px";
function moveR() {
adst.top = ( bodyfrm.scrollTop + bodyfrm.clientHeight - 250-22) + "px";
adst.left = ( bodyfrm.scrollLeft + bodyfrm.clientWidth - 85 ) + "px";
}
setInterval("moveR();", 80);
function closead()
{
adst.display='none';
}
