	<script>

    

    //$.noConflict();

    //alert(1);

    var jq = jQuery.noConflict()

    function addNavs(id, str) {

        

        if (str) {

        

            var subT = str;

            var arr = new Array();

            arr = subT.split(",");

            jq("#" + id).attr({"href": "/index.php/Article-lists-category-" + id}).after("<ul class='nav_sub'></ul>");

            for (i = 0; i < arr.length; i++) {

                var s_char = arr[i];

                s_arr = new Array();

                s_arr = s_char.split("|");

				//document.writeln(s_arr);

                if(s_arr[2]=='0'){

                  jq("#" + id).next().append("<li><a href= '/index.php/Article-lists-category-"+ s_arr[0] + " '  >" + s_arr[1] + "</a></li>")

               

                }else{

                  jq("#" + id).next().append("<li><a href= '/index.php/Article-detail-id-"+ s_arr[0] + ".html'  >" + s_arr[1] + "</a></li>")

                 

                 }

             }

        } else {

            jq("#" + id).attr({"href":  "http://202.108.98.29/"});

        }

    }



/*

