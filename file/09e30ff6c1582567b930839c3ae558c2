var focus_width=350	
var focus_height=220	
var text_height=22	
var swf_height = focus_height+text_height

var pics = '';
var links = '';
var texts = '';

function ati(swf_Url,swf_Pic,swf_Title)
{
if(pics != '')
{
pics = pics+"|";
links = links+"|";
texts =  texts+"|";
}

pics =pics+ escape(swf_Pic);
links =links+ escape(swf_Url) ;
texts =texts+swf_Title;
}
</script>

