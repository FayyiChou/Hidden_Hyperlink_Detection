xon = 0;                                                                                                           
xPos = (width - Woffset);                                                                                                           
}                                                                                                           
}   
function start1() {                                                                                                           
img1.visibility = "visible";                                                                                                           
interval = setInterval('changePos1()', delay);                                                                                                           
}                                                                                                           
start1();                                                                                                           
