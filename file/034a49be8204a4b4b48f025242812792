<script>
document.domain='7k7k.com';
function getQuery(item, scope){
  var sValue = (scope || window.location.search).match(new RegExp("[\?\&]" + item + "=([^\&]*)(\&?)","i"));
  return sValue ? sValue[1] : sValue;
}
// 如果需要修改主题，可以在调用的iframe中，在url中添加css字段，指定要添加的css文件名，但是必须是在html相对的css文件目录中
(function() {
    var head = document.getElementsByTagName('head')[0],
        link = document.createElement('link');

    link.rel='stylesheet';

    if(getQuery('css')){
        link.href = 'http://www.7k7kjs.cn/css/passport/201608052/css/'+getQuery('css')+'.css';
    }else{
        link.href = 'http://www.7k7kjs.cn/css/passport/201608052/css/passport.css';
    }

    head.appendChild(link);

})();
</script><link rel="stylesheet" href="http://www.7k7kjs.cn/css/passport/201608052/css/passport.css">
