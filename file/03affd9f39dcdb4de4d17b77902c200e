        var aNav = $('.nTabNav li');
        var aList = $('.ntablist');
        var iNow = 0;
        var len = aNav.length;
        function tab(iNow){
            for(var i=0; i<len; i++){
                aNav[i].className = '';
                aNav[iNow].className = 'active';
                //aList[i].style.display = 'none';
                if(i<iNow){
                    aList[i].style.display = 'none';
                }else{
                    aList[i].style.display = 'block';
                }
            }
        }
        for(var i=0; i<len; i++){
            aNav[i].index = i;
            aNav[i].onclick = function(){
                var n = this.index;
                tab(n);
            };
        }
    </script>
