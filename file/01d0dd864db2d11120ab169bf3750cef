        $(document).ready(function () {
            $(".fast_menu a").hover(function (e) {
                var _tx_w = $(this).find("span");
                if (!($(".fast_menu a span").is(":animated"))) {
                    _tx_w.stop().animate({ lineHeight: "48px", opacity: "0" }, 100).animate({ lineHeight: "220px", opacity: "0" }, 100, function () {
                        if (_tx_w.css("line-height") == "220px") {
                            _tx_w.animate({ lineHeight: "136px", opacity: "1", }, 200);
                            _tx_w.css("color", "rgb(213, 146, 57)");
                        }
                    });
                }
            }, function () {
                $(this).find("span").stop(true).animate({ lineHeight: "136px", opacity: "1", }, 0);
                $(this).find("span").css("color", "#444");
            });
        });
