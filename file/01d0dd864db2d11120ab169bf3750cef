        function GetPrice() {
            // 美元
            $.ajax({
                url: "http://api.markets.wallstreetcn.com/v1/price?symbol=USDollarIndex&timestamp=&price=&_=1461202927983",
                type: "GET",
                dataType: 'jsonp',
                jsonp: "callback",
                success: function (data) {
                    if (data.results[0].change < 0) {
                        $("#dollar_price").html(data.results[0].price);
                        $("#dollar_ud").html("<img src='/./content/site/images/down1.jpg'>");
                    }
                    else if (data.results[0].change > 0) {
                        $("#dollar_price").html(data.results[0].price);
                        $("#dollar_ud").html("<img src='/./content/site/images/up1.jpg'>");
                    }
                    else {
                        $("#dollar_price").html(data.results[0].price);
                    }
                    $("#dollar_change").html(data.results[0].change.toFixed(2));
                    $("#dollar_changeper").html(data.results[0].changePercent + "%");
                    if (data.results[0].change < 0) {
                        $("#dollar_cc").attr("class", "go_l");
                    }
                    else if (data.results[0].change > 0) {
                        $("#dollar_cc").attr("class", "go_h");
                    }
                },
                error: function (e) {
                }
            });



            //新华龙油
            $.ajax({
                url: 'http://hq.rongdingkeji.com/3rd/xh',
                dataType: "jsonp",
                success: function (data) {


                    if (data.soilList[0].chg < 0) {
                        $("#o100_price").html(data.soilList[0].mpnowpic);
                        $("#o100_ud").html("<img src='/./content/site/images/down1.jpg'>");
                        $("#o100_cc").attr("class", "go_l");
                    }
                    else if (data.soilList[0].chg > 0) {
                        $("#o100_price").html(data.soilList[0].mpnowpic);
                        $("#o100_ud").html("<img src='/./content/site/images/up1.jpg'>");
                        $("#o100_cc").attr("class", "go_h");
                    }
                    else {
                        $("#o100_price").html(data.soilList[0].mpnowpic);
                    }
                    $("#o100_change").html(data.soilList[0].chg);

                    $("#o100_changeper").html(data.soilList[0].chg + "%");


                }
            });


            //长江银油铜
            $.get("/Home/getChangJiangData", function (res) {
                if (res.data != null && res.Date != "") {
                    var arr = res.data.split(",");

                    $.each(arr, function (Index, Item) {
                        //最新价
                        var strCurPrice = "";
                        //涨跌
                        var strChange = "";
                        switch (Item.toString()) {
                            case "长江银100克":
                                strCurPrice = arr[Index + 5];
                                strChange = arr[Index + 8];
                                if (parseFloat(strChange) < 0) {
                                    $("#changjiangyin_price").html(strCurPrice);
                                    $("#changjiangyin_ud").html("<img src='/./content/site/images/down1.jpg'>");
                                    $("#changjiangyin").attr("class", "go_l");
                                }
                                else if (parseFloat(strChange) > 0) {
                                    $("#changjiangyin_price").html(strCurPrice);
                                    $("#changjiangyin_ud").html("<img src='/./content/site/images/up1.jpg'>");
                                    $("#changjiangyin").attr("class", "go_h");
                                }
                                else {
                                    $("#changjiangyin_price").html(strCurPrice);
                                }
                                $("#changjiangyin_change").html(strChange);

                                $("#changjiangyin_changeper").html((parseFloat(strChange) / parseFloat(strCurPrice) * 100).toFixed(2) + "%");

                                break;
                            case "长江铜1吨":
                                strCurPrice = arr[Index + 5];
                                strChange = arr[Index + 8];

                                if (parseFloat(strChange) < 0) {
                                    $("#changjiangtong_price").html(strCurPrice);
                                    $("#changjiangtong_ud").html("<img src='/./content/site/images/down1.jpg'>");
                                    $("#changjiangtong").attr("class", "go_l");
                                }
                                else if (parseFloat(strChange) > 0) {
                                    $("#changjiangtong_price").html(strCurPrice);
                                    $("#changjiangtong_ud").html("<img src='/./content/site/images/up1.jpg'>");
                                    $("#changjiangtong").attr("class", "go_h");
                                }
                                else {
                                    $("#changjiangtong_price").html(strCurPrice);
                                }
                                $("#changjiangtong_change").html(strChange);
                                $("#changjiangtong_changeper").html((parseFloat(strChange) / parseFloat(strCurPrice) * 100).toFixed(2) + "%");
                                break;
                            case "长江油[燃料油]100桶":
                                strCurPrice = arr[Index + 5];
                                strChange = arr[Index + 8];
                                if (parseFloat(strChange) < 0) {
                                    $("#changjiangyou_price").html(strCurPrice);
                                    $("#changjiangyou_ud").html("<img src='/./content/site/images/down1.jpg'>");
                                    $("#changjiangyou").attr("class", "go_l");
                                }
                                else if (parseFloat(strChange) > 0) {
                                    $("#changjiangyou_price").html(strCurPrice);
                                    $("#changjiangyou_ud").html("<img src='/./content/site/images/up1.jpg'>");
                                    $("#changjiangyou").attr("class", "go_h");
                                }
                                else {
                                    $("#changjiangyou_price").html(strCurPrice);
                                }
                                $("#changjiangyou_change").html(strChange);
                                $("#changjiangyou_changeper").html((parseFloat(strChange) / parseFloat(strCurPrice) * 100).toFixed(2) + "%");
                                break;
                        }
                    });
                }

            });
        }

    </script>




