    <script type="text/javascript">

        $(function () {
            GetPrice();
            //
            $('#demo-01').jCarouselLite({
                btnPrev: '#prev-01',
                btnNext: '#next-01',
                visible: 1
            });
            $('#demo-02').jCarouselLite({
                btnPrev: '#prev-02',
                btnNext: '#next-02',
                visible: 1
            });
