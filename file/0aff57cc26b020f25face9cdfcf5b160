                        <script type="text/javascript" language="javascript">
                            var MarqueeDiv2Control = new Marquee(
                            {
                                MSClass: ["ISL_Cont_1", "ContInner"],
                                Direction: 2,
                                Step: 1,
                                Width: 678,
                                Height: 150,
                                Timer: 20,
                                ScrollStep: 1,
                                SwitchType: 0,
                                AutoStart: true
                            });
                            MarqueeDiv2Control.BakStep = MarqueeDiv2Control.Step;
                            document.getElementById("LeftArr").onmouseover = function () { MarqueeDiv2Control.Direction = 2 };
                            document.getElementById("LeftArr").onmouseout = document.getElementById("LeftArr").onmouseup = function () { MarqueeDiv2Control.Step = MarqueeDiv2Control.BakStep };
                            document.getElementById("LeftArr").onmousedown = document.getElementById("RightArr").onmousedown = function () { MarqueeDiv2Control.Step = MarqueeDiv2Control.BakStep + 3 };
                            document.getElementById("RightArr").onmouseover = function () { MarqueeDiv2Control.Direction = 3 };
                            document.getElementById("RightArr").onmouseout = document.getElementById("RightArr").onmouseup = function () { MarqueeDiv2Control.Step = MarqueeDiv2Control.BakStep };

                        </script>
