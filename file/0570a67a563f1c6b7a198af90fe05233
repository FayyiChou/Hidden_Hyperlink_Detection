<script type="text/javascript" src='http://www.gongdakaoyan.com/templets/default/js/jquery-3.0.0.min.js'></script>
<script type="text/javascript" src='http://www.gongdakaoyan.com/templets/default/js/marquee.js'></script>
<script type="text/javascript">
	$(function () {
		var $marqueeDiv1 = $('#marqueeDiv1');
		var $marqueeDiv2 = $('#marqueeDiv2');

		var marqueeBoxLen = $("#marqueeDiv1").css('length').split('p')[0];

		var $marqueeUl1 = $('#marqueeUl1');
		var $marqueeUl2 = $('#marqueeUl2');

		var $navLI1 = $('#navLI1');
		var $navLI2 = $('#navLI2');

		var intervalId1 = '';
		var intervalId2 = '';

		$marqueeUl1.empty();
		$marqueeUl2.empty();

		// 生成数据
		var createHtml =  function () {

			var commentHtml = '';
			var orderHtml = '';
			for (var i = 0, len1 = comment.length; i < len1; i++) {
				commentHtml += "<li><span>"+comment[i].name+"：</span><span>"+comment[i].comment+"</span></li>";
			}

			for (var j = 0, len2 = order.length; j < len2; j++) {
				orderHtml += "<li><span>"+order[j].order_name+"</span><span>"+order[j].order_id+"</span><span>"+order[j].order_satus+"</span></li>";
			}

			$marqueeUl1.append(orderHtml);
			$marqueeUl2.append(commentHtml);
		};
		
		createHtml();

		var height1 = parseInt($marqueeUl1.css('height').split('p')[0], 10) - parseInt($(".marquee-div1").css('height').split('p')[0], 10);
		var height2 = parseInt($marqueeUl2.css('height').split('p')[0], 10) - parseInt($(".marquee-div2").css('height').split('p')[0], 10);

		var intervalFun1 = function () {
			intervalId1 = setInterval(function () {
				var curTop1 = $marqueeUl1.css('top').split('p')[0];
				$marqueeUl1.css('top', (curTop1 - 1) + 'px');

				if (curTop1 <= (-100 + parseInt(height1, 10))) {
					$marqueeUl1.css('top', '0px');
				}
			}, 100);
		};
		
		var intervalFun2 = function () {

			intervalId2 = setInterval(function () {
				var curTop2 = $marqueeUl2.css('top').split('p')[0];
				$marqueeUl2.css('top', (curTop2 - 1) + 'px');
				if (curTop2 <= -(parseInt(height2, 10))) {
					$marqueeUl2.css('top', '0px');
				}
			}, 100);
		};

		$("#marqueeDiv1").mouseover(function () {
			clearInterval(intervalId1);
		});

		$("#marqueeDiv1").mouseleave(function () {
			intervalFun1();
		});

		$("#marqueeDiv2").mouseover(function () {
			clearInterval(intervalId2);
		});

		$("#marqueeDiv2").mouseleave(function () {
			intervalFun2();
		});

		$navLI1.click(function () {
			clearInterval(intervalId1);
			clearInterval(intervalId2);
			intervalFun2();
			$marqueeDiv1.hide();
			$marqueeDiv2.show();
		
			if ($(this).attr('class') === 'mouseover') {
				$(this).attr('class', 'mouseleave');
				$(this).siblings('li').attr('class', 'mouseover');
			}
		});
		$navLI2.click(function () {

			clearInterval(intervalId1);
			clearInterval(intervalId2);
			intervalFun1();
			$marqueeDiv2.hide();
			$marqueeDiv1.show();

			if ($(this).attr('class') === 'mouseover') {
				$(this).attr('class', 'mouseleave');
				$(this).siblings('li').attr('class', 'mouseover');
			}

		});
		// 控制显示隐藏
		
		intervalFun2();
	});
</script>
