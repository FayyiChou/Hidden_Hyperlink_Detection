                // Baidu
                var dataUrl = '/so/inc/inc_search_tib.aspx';
                var sug = new S.Suggest('#top_txt_word', dataUrl, {
                    resultFormat: '',
                    containerCls: 'bd-sug',
                    charset: 'utf-8',
                    queryName: 'wd',
                    callbackFn: 'bdsug.sug'
                });
                sug.on('dataReturn', function() {
                    this.returnedData = this.returnedData.s || [];
                });
            });
