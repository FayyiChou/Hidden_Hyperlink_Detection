<script type="text/javascript">
function search_all_top()
{
	var Url;
	var t_key=document.getElementById("top_txt_word").value;
	//var t_type=document.getElementById("hid_search_type").value;
	//过滤空格
	t_key=t_key.replace(/[ ]/g,""); 
	window.location.href="/so/?t=0&wd="+escape(t_key)+"";
	
}
function TopsetTab(n)
{
for(m=1;m<8;m++) 
    { 
        document.getElementById("top_v"+m).style.display="none"; 
    } 
    document.getElementById("top_v"+n).style.display="block"; 
}
</script>

