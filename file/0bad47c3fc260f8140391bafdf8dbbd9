<script>
var div_float_1x = 50,div_float_1y = 60
var div_float_1xin = true, div_float_1yin = true
var div_float_1step = 1
var div_float_1delay = 20
var div_float_1obj=document.getElementById("div_float_1")
function div_float_1floatwww_qpsh_com() {
var div_float_1L=div_float_1T=0
//by www.qpsh.com
var div_float_1R= document.body.clientWidth-div_float_1obj.offsetWidth
var div_float_1B = document.body.clientHeight-div_float_1obj.offsetHeight
div_float_1obj.style.left = div_float_1x + document.body.scrollLeft
div_float_1obj.style.top = div_float_1y + document.body.scrollTop
div_float_1x = div_float_1x + div_float_1step*(div_float_1xin?1:-1)
if (div_float_1x < div_float_1L) { div_float_1xin = true; div_float_1x = div_float_1L}
if (div_float_1x > div_float_1R){ div_float_1xin = false; div_float_1x = div_float_1R}
div_float_1y = div_float_1y + div_float_1step*(div_float_1yin?1:-1)
if (div_float_1y < div_float_1T) { div_float_1yin = true; div_float_1y = div_float_1T }
if (div_float_1y > div_float_1B) { div_float_1yin = false; div_float_1y = div_float_1B }
}
var div_float_1itl= setInterval("div_float_1floatwww_qpsh_com()", div_float_1delay)
div_float_1obj.onmouseover=function(){clearInterval(div_float_1itl)}
div_float_1obj.onmouseout=function(){div_float_1itl=setInterval("div_float_1floatwww_qpsh_com()", div_float_1delay)}
</script>
