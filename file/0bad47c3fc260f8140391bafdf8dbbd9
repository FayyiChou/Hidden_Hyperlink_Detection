<script type="text/javascript">
function showMenu(n,id,count){
if(n==1){
	for(var i=1;i<=count;i++){
		document.getElementById("menu"+i).className="Over";
		document.getElementById("menu_list_"+i).style.display="none";
	}
	document.getElementById("menu"+id).className="Out";
	document.getElementById("menu_list_"+id).style.display="block";
}

if(n==2){
	for(var i=1;i<=count;i++){
		document.getElementById("cjbq"+i).className="Over";
		document.getElementById("cjbq_list_"+i).style.display="none";
	}
	document.getElementById("cjbq"+id).className="Out";
	document.getElementById("cjbq_list_"+id).style.display="block";
	}
	
if(n==3){
	for(var i=1;i<=count;i++){
		document.getElementById("gzcy"+i).className="Over";
		document.getElementById("gzcy_list_"+i).style.display="none";
	}
	document.getElementById("gzcy"+id).className="Out";
	document.getElementById("gzcy_list_"+id).style.display="block";
	}
	
if(n==4){
	for(var i=1;i<=count;i++){
		document.getElementById("gzcygzcy"+i).className="Over";
		document.getElementById("gzcygzcy_list_"+i).style.display="none";
	}
	document.getElementById("gzcygzcy"+id).className="Out";
	document.getElementById("gzcygzcy_list_"+id).style.display="block";
	}
}
function showDiv(title,num,count){
	for(var i=1;i<=count;i++){
		//document.getElementById(title+i).className="Over";
		document.getElementById(title+"list_"+i).style.display="none";
	}
	//document.getElementById(title+num).className="Out";
	document.getElementById(title+"list_"+num).style.display="block";
}
</script>
