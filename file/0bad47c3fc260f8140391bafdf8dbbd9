<script> 
function onLuceneSearch(){
    //var select = document.getElementById("select").value;
	if(document.all.skey.value==''){
		alert('请输入关键字');
		return false;
	} else {
		var encodeURI_keyword = encodeURI(encodeURI(document.all.skey.value));
		window.open('/cms/cmsWebapp/jsp/lucene/qwjs_jg.jsp?siteunid=FE77B0421AE8204BC3AF147482FCE6BE&keyword=' + encodeURI_keyword);
	}
}
</script><!--[顶部页面]end-->
