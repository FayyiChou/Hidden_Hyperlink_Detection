    <script language="javascript">
function search2()
{
	if(document.all.RandomName.value=="请输入受理编号")
	{
		window.alert("请输入受理编号");
		return false;  
	}
	if(document.all.RandomPassword.value=="请输入查询密码")
	{
		window.alert("请输入查询密码");
		return false;
	}
	dakai1();
	document.all.RandomName.value="请输入受理编号";
	document.all.RandomPassword.value="请输入查询密码";
}  
function dakai1()
{	
	window.open("http://xzfw.ningde.gov.cn/login/BusinessView.aspx?close=true&ShouLiBianHao="+document.all.RandomName.value+"&ShouLiMiMa="+document.all.RandomPassword.value, "", "height=400,width=500,toolbar=no, menubar=no,scrollbars=yes, resizable=yes,location=no, status=no");
} 
</script>
