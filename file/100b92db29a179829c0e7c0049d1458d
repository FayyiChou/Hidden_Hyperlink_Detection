<script type="text/javascript">
$(document).ready(function () {
    $("#back_top").hide();
    // fade in #back-top
    $(function () {
        $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
                $('#back_top').fadeIn();
            } else {
                $('#back_top').fadeOut();
            }
        });
        // scroll body to 0px on click
        $('#back_top a.top').click(function () {
            $('body,html').animate({scrollTop: 0
            }, 500);
            return false;
        });
    });
});
</script>
