  <script type="text/javascript">
var tID = 1;
function show_menu(ID){
  if(ID!=tID){
    $('sub_menu').innerHTML = '&nbsp;'+$('menu_cont_'+ID).innerHTML;
    $('menu_'+ID).className = 'nav-on';
    $('menu_'+tID).className = 'nav-off';
    tID=ID;
  }else{
    $('menu_'+ID).className = 'nav-on';
    $('sub_menu').innerHTML = '&nbsp;'+$('menu_cont_'+ID).innerHTML;    
  }
}
show_menu(1);
</script> 
