<script language="JavaScript" type="text/javascript">
//<![CDATA[
function startBanner(){
    //计算元素水平位置使其居中
    var screen_width = window.screen.width;

    var rate = screen_width/1920;

    jq('.parallax_classic_texts .parallax_classic_text_line').each(function(){
        elm = jq(this);
        var final_left = elm.attr('data-final-left');
        var initial_left = elm.attr('data-initial-left');

        var elmw = elm.width();
        var maxleft = (screen_width-elmw)/2-30; //左边元素的最大位置
        var minleft = screen_width/2+30; //右边元素的最小位置
        
        final_left *= rate;
        initial_left *= rate;

        if(elm.hasClass('left-elm')){
            if(initial_left > maxleft) initial_left = maxleft;
            if(final_left > maxleft) final_left = maxleft;
        }
        if(elm.hasClass('right-elm')){
            if(final_left < minleft) final_left = minleft;
            if(initial_left < minleft) initial_left = minleft;
        }

        elm.attr({'data-final-left':final_left,'data-initial-left':initial_left});
    });

    jQuery('#parallax_classic_bullets').parallax_classic({
	    skin: 'bullets',
        width: window.screen.width,
                height: 365,
                backgroundKeepCebnter:1,
        minWidth:1200, //important
        minHeight:365, //important
	    width100Proc:0, //保持窗口宽度,important
	    responsive:1, //等比自适应窗口宽度,important
	    autoPlay:5,
	    defaultExitOFF:true,
	    autoHideBottomNav:false,
	    autoHideNavArrows:true,
	    showPreviewThumbs:false,
	    showCircleTimer:true,
	    myloaderTime:2,
	    scrollSlideDuration:1.0,
	    scrollSlideEasing:'easeInQuint',				
	    thumbsWrapperMarginBottom:20,
        absUrl:'images_demo/parallax/',
        showCircleTimer:false
                
    });
}
//]]>
