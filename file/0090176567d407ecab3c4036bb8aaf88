    <script type="text/javascript">
        $(document).ready(function () {
            $.each($("#i_news li"), function (i, item) {
                $(item).mouseover(function () {
                    $("#i_news li").attr("class", "one_li1");
                    $(this).attr("class", "one_li");
                    $("div[lang='1div']").hide();
                    $("div[lang='1div']").eq(i).show();
                });
            })
            $.each($("#hx li"), function (i, item) {
                $(item).mouseover(function () {
                    $("#hx li").attr("class", "on_li1");
                    $(this).attr("class", "on_li");
                    $("div[lang='3div']").hide();
                    $("div[lang='3div']").eq(i).show();
                });
            })
            $.each($("#tux li"), function (i, item) {
                $(item).mouseover(function () {
                    $("#tux li").attr("class", "login_title_li1");
                    $(this).attr("class", "login_title_li");
                    $("div[lang='2div']").hide();
                    $("div[lang='2div']").eq(i).show();
                });
            })
            $.each($("#sp li"), function (i, item) {
                $(item).mouseover(function () {
                    $("#sp li").attr("class", "on_li1");
                    $(this).attr("class", "on_li");
                    $("div[lang='4div']").hide();
                    $("div[lang='4div']").eq(i).show();
                });
            })
        })
