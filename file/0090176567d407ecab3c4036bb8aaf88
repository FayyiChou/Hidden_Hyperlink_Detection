    <script src="/Admin/Js/highcharts.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            LineAjax();
            ColumnAjax();
            PieAjax();
        });
        function LineAjax() {
            var strXT = $("#ctl00_ContentPlaceHolder1_HiddenField1").val();
            $.ajax({ url: "/Admin/service/Handler1.ashx?Method=BindLine",
                type: "GET",
                dataType: "json",
                error: function () { },
                data: { strXt: strXT },
                success: function (result) {

                    chart = new Highcharts.Chart({
                        chart: {
                            renderTo: 'divChart',
                            type: 'line',
                            //                            marginRight: 130,
                            //                            marginBottom: 25,
                            width: 356,
                            height: 234
                        },
                        title: {
                            text: '内蒙古风电场统计 - ' + result.xName,
                            x: -20 //center
                        },
                        subtitle: {
                            text: '数据来源: nmgfdw.com',
                            x: -20
                        },
                        xAxis: {
                            categories: result.xAxis
                        },
                        yAxis: {
                            title: {
                                text: '总和'
                            },
                            plotLines: [{
                                value: 0,
                                width: 1,
                                color: '#000000'
                            }]
                        },
                        tooltip: {
                            formatter: function () {
                                return '<b>' + this.series.name + '</b><br/>' +
                        this.x + ': ' + this.y + '';
                            }
                        },
                        legend: {
                            layout: 'vertical',
                            align: 'right',
                            verticalAlign: 'top',
                            x: -10,
                            y: 100,
                            borderWidth: 0
                        },
                        series: result.series
                    });
                }
            });
        }
        function ColumnAjax() {
            var strXT = $("#ctl00_ContentPlaceHolder1_HiddenField2").val();

            $.ajax({ url: "/Admin/service/Handler1.ashx?Method=BindLine",
                type: "GET",
                dataType: "json",
                error: function () { },
                data: { strXt: strXT },
                success: function (result) {

                    chart = new Highcharts.Chart({
                        chart: {
                            renderTo: 'divZT',
                            type: 'column',
                            //                            marginRight: 130,
                            //                            marginBottom: 25,
                            width: 356,
                            height: 234
                        },
                        title: {
                            text: '内蒙古风电场统计 - ' + result.xName,
                            x: -20 //center
                        },
                        subtitle: {
                            text: '数据来源: nmgfdw.com',
                            x: -20
                        },
                        xAxis: {
                            categories: result.xAxis
                        },
                        yAxis: {
                            title: {
                                text: '总和'
                            },
                            plotLines: [{
                                value: 0,
                                width: 1,
                                color: '#000000'
                            }]
                        },
                        tooltip: {
                            formatter: function () {
                                return '<b>' + this.series.name + '</b><br/>' +
                        this.x + ': ' + this.y + '';
                            }
                        },
                        legend: {
                            layout: 'vertical',
                            align: 'right',
                            verticalAlign: 'top',
                            x: -10,
                            y: 100,
                            borderWidth: 0
                        },
                        series: result.series
                    });
                }
            });
        }
        function PieAjax() {
            var strXT = $("#ctl00_ContentPlaceHolder1_HiddenField3").val();


            $.ajax({ url: "/Admin/service/Handler1.ashx?Method=BindPie",
                type: "GET",
                dataType: "json",
                error: function () { },
                data: { strXt: strXT },
                success: function (result) {

                    chart = new Highcharts.Chart({
                        chart: {
                            renderTo: 'divBT',
                            plotBackgroundColor: null,
                            plotBorderWidth: null,
                            plotShadow: false,
                            //                            marginRight: 130,
                            //                            marginBottom: 25,
                            width: 356,
                            height: 234

                        },
                        title: {
                            text: '内蒙古风电场统计 - 饼图'
                        },
                        tooltip: {
                            'formatter': function () {

                                return '<b>' + this.point.name + '</b>: ' + Math.round(this.percentage * 100) / 100 + ' %';
                            }
                        },
                        plotOptions: {
                            pie: {
                                allowPointSelect: true,
                                cursor: 'pointer',
                                dataLabels: {
                                    enabled: true,
                                    color: '#000000',
                                    connectorColor: '#000000',
                                    formatter: function () {
                                        return '<b>' + this.point.name + '</b>: ' + Math.round(this.percentage * 100) / 100 + ' %';
                                    }
                                }
                            }
                        },
                        series: result.series
                    });
                }
            });
        }
    </script>
