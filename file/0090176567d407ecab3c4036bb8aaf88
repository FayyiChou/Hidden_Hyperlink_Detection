    <script type="text/javascript">
        function sOrn(id) {
            if ($.trim($("div[id='ShowOrNone" + id + "']").attr("class")) == "menu_content hide") {
                for (var i = 0; i < 8; i++) {
                    if (i + 1 == id) {
                        $("div[id='ShowOrNone" + id + "']").attr("class", "menu_content");
                    }
                    else {
                        $("div[id='ShowOrNone" + (i + 1) + "']").attr("class", "menu_content hide");
                    }
                }
            }
        }
        function sOrn1(id) {
            if ($.trim($("div[id='ShowOrNone1" + id + "']").attr("class")) == "menu_content hide") {
                for (var i = 0; i < 8; i++) {
                    if (i + 1 == id) {
                        $("div[id='ShowOrNone1" + id + "']").attr("class", "menu_content");
                    }
                    else {
                        $("div[id='ShowOrNone1" + (i + 1) + "']").attr("class", "menu_content hide");
                    }
                }
            }
        }
    </script>
