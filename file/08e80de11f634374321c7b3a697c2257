                $(function () {
                    setInterval(function () {
                        var s_yh_b = $('.s_yh_a li').last().hide().css('margin-top', '-94px').clone();
                        $('.s_yh_a li').last().remove();
                        $('.s_yh_a ul').prepend(s_yh_b);
