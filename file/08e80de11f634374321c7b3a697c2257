						},function(){
							$(this).css('background','transparent');
						})
                    }, 5000);
					$('.s_yh_a .overflow li').hover(function(){
						$(this).css('background','#d3f0f8');
					},function(){
						$(this).css('background','transparent');
					})
                });
</script>

