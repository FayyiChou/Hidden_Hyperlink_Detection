	//关注
	function followuser(flag,fid){
	var login_user_uid = "";
	if(login_user_uid <=0){
		//alert('请您先登录');
		loginPop("http://www.soyoung.com/p1412240");
		return false;
	}
	var followcontent = $('#userid_'+fid).html();
	$('#userid_'+fid).html("<span id=userid_"+fid+"><a>等待...</a></span>");
	$.ajax({
        type:"post",
        url:'/home/followuser',
        data:{"uid":login_user_uid,'fid':fid,'flag':flag},
        dataType:"json",
        success:function(result){
            if(result.code == 200){
            	if(flag ==1){
                	$('#userid_'+fid).html("<span id=userid_"+fid+"><a class='btn_3' onclick='followuser("+2+","+fid+")'>"+"已关注"+"</a></span>");
    	            return false;
                }else if(flag ==2){
                	$('#userid_'+fid).html("<span id=userid_"+fid+"><a onclick='followuser("+1+","+fid+")'>"+"加关注"+"</a></span>");
    	            return false;
                }
                /*
				alert('关注、取消失败');
				$('#userid_'+fid).html(followcontent);
				return false;
				*/
            }
            if(result.code == 100){
                if(flag ==1){
                	$('#userid_'+fid).html("<span id=userid_"+fid+"><a class='btn_3' onclick='followuser("+2+","+fid+")'>"+"已关注"+"</a></span>");
    	            return false;
                }else if(flag ==2){
                	$('#userid_'+fid).html("<span id=userid_"+fid+"><a onclick='followuser("+1+","+fid+")'>"+"加关注"+"</a></span>");
    	            return false;
                }

            }
	    },
	    error:function(){
	    	$('#userid_'+fid).html(followcontent);
		}
	});
}

$(function(){
	$('.head_pic_pop_show').hover(function(){
		var uid = $(this).attr('uid');
		var uid_s = $(this).find('.data_pop').attr('uid_s');
		var t = $.trim($('#data_pop_'+uid+ '_'+ uid_s).html());
		if(t==''){
            $.ajax({
            	type:"get",
            	url:'/home/showuserinfo',
            	data:{"uid":uid},
            	dataType:"html",
            	success:function(obj){
            	   $('#data_pop_'+uid + '_'+ uid_s).html(obj);
            	   $('#data_pop_'+uid + '_'+ uid_s).show();
            	},
            });
    	}else{
			  $('#data_pop_'+uid+ '_'+ uid_s).show();
		}
	},function(){
		$('.data_pop').empty().hide();
	});
	//doctor reply
	$('.head_pic_pop_show_doctor').hover(function(){
		var uid = $(this).attr('uid');
		var uid_s = $(this).find('.data_pop').attr('uid_s');
		var t = $.trim($('#data_pop_'+uid+ '_'+ uid_s).html());
		if(t==''){
    		$.ajax({
				type:"get",
				url:'/home/showuserinfo/flag/3',
				data:{"uid":uid},
				dataType:"html",
				success:function(obj){
				   $('#data_pop_'+uid + '_'+ uid_s).html(obj);
				   $('#data_pop_'+uid + '_'+ uid_s).show();
				},
			});
		}else{
			  $('#data_pop_'+uid+ '_'+ uid_s).show();
		}

	},function(){
		$(this).find('.data_pop').empty().hide();
	});
});
function goto_url(group_id){
	window.location.href="/dpg"+group_id;
}

function delpost(post_id){
	if(confirm("删除帖子会扣除50氧分，确定要删除吗？")){
		var user_money = "0";

		var require_money = "50";

		if(parseInt(user_money) < parseInt(require_money)){
		    alert("氧分达到"+"50"+"才可以删帖，您的氧分不足！");
		    return false;
		}
		var marrow = "0";
		if(marrow == 1){
		    alert("精华帖暂时不提供删除功能，如有疑问可私信新氧");
		    return false;
		}
		var calendar = "2";
		if(calendar ==5){
		    alert("日记暂不提供删除功能");
		    return false;
		}
	}else{
	    return false;
	}

}

function writeCookie(name, value, hours) {
	var expire = "";
	if (hours != null) {
		expire = new Date((new Date()).getTime() + hours * 360000);
		expire = "; expires=" + expire.toGMTString();
	}
	document.cookie = name + "=" + escape(value) + expire + ";path=/";
}

function readCookie(name) {
	var cookieValue = "";
	var search = name + "=";
	if (document.cookie.length > 0) {
		offset = document.cookie.indexOf(search);
		if (offset != -1) {
			offset += search.length;
			end = document.cookie.indexOf(";", offset);
			if (end == -1) end = document.cookie.length;
			cookieValue = unescape(document.cookie.substring(offset, end))
		}
	}
	return cookieValue;
}
