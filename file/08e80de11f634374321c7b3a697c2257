    function delReply(post_id, reply_id){
        if(confirm('确定删除吗?')){
            $.ajax({
                type:"post",
                url:'/post/delreply',
                data:{"post_id":post_id,"reply_id":reply_id},
                dataType:"json",
                success:function(obj){
                    if(obj.code == 200){
                        $('#reply-'+reply_id).slideUp('normal'
                                ,function(){
                                	$(this).remove();
                                }
                        );

                    }else{
                        alert('删除失败');
                    }
                },
                error:function(){
                    alert('系统忙，请稍后再试');
                }
            });
        }
    }
    function updatePostStatus(post_id, type, value) {
        var _text = '';
        /**edited by fy start**/
        if(type=="top_hd"){
        	HiddenPost.init(post_id, type, value).show();
        	return ;
        }
        /**edited by fy end**/
        if (1 == value) {
            _text += '添加';
        } else {
            _text += '取消';
        }
        if ('top' == type) {
            _text += '置顶';
        } else if ('marrow' == type) {
            _text += '加精';
        }
        if('update_date' == type){
            _text = '沉贴';
        }
        if('reply_locked_yn' == type){

            if(1 == value){
                _text = '锁帖';
            }else{
                _text = '取消锁帖';
            }
        }
        if ('topic_yn' == type) {
        	_text = '置为话题';
        }
        var tag_id = ''
        if (confirm('确定'+_text+'吗？')) {
	        $.post('/post/update', {post_id:post_id, type:type, value:value, tag_id:tag_id}, function(){
	            window.location.reload();
	        });
        }
    }
    function delComment(item_id, comment_id, class_id) {
        var post_id = '1412240';
        if(confirm('确定删除吗?')){
        $.post('/comment/del', {item_id:item_id, comment_id:comment_id, class_id:class_id,primary_id:post_id}, function(){
            class_id = parseInt(class_id);
            if (6 == class_id) {
                var type = 'post';
                var is_beauty = 'Y';
            } else if (7 == class_id) {
                var type = 'post';
                var is_beauty = 'N';
            } else if (8 == class_id) {
                var type = 'reply';
                var is_beauty = 'Ns';
            }
            getComment(1, item_id, type, is_beauty);
        });
        }
    }




    $(function () {
   //      $('.beauty_content_list .content img').each(function(){
   //          var img=new Image();
			// img.src=$(this).attr('src');
			// var width=img.width,height=img.height;

			// var new_H = Math.floor((height/width)*625);
   //          if(width >= 625){
   //              $(this).not($('.beauty_content_list .content .reply_box .list .pic img').css({'width':'36px','height':'36px'})).css({'width':'625px','height':new_H});
   //          }
   //      });
        $('.brow > span').click(function () {
            $(this).parent().find('.brow_pop').fadeIn();
        });
        $('.brow_pop span').click(function () {
            $(this).parent().fadeOut();
        });
        $('.brow_pop span').hover(function () {
            $(this).addClass('hover');
        }, function () {
            $(this).removeClass('hover');
        });
        $('.associate_project .price_content li').hover(function () {
            $(this).find('.list_d').show();
            $(this).css('z-index', '10');
        }, function () {
            $(this).find('.list_d').hide();
            $(this).css('z-index', '0');
        });
        // addViewCnt();



	var availHeight = window.screen.availHeight - 100;
	var navHeight = $('#hd .nav').height();

	$(window).scroll(function () {
		var hdHeight = $('#hd').height() + $('#hd').offset().top;
		var sidebarH = $('.sidebar').height();
		var columnH = $('.column').height();
		var top = document.documentElement.scrollTop || document.body.scrollTop;
		var top_n = top + navHeight + 30;

		if (top > (hdHeight) && columnH > sidebarH && sidebarH < availHeight) {
			if (typeof document.body.style.maxHeight === "undefined") {
				$('.sidebar').css({ 'position': 'absolute', 'top': top_n, 'left': '50%', 'margin-left': '258px' });
			} else {
				$('.sidebar').css({ 'position': 'fixed', 'top': navHeight + 30, 'left': '50%', 'margin-left': '258px' });
			}
		} if (sidebarH >= availHeight && (top + availHeight) > sidebarH && columnH > sidebarH) {
			if (typeof document.body.style.maxHeight === "undefined") {
				$('.sidebar').css({ 'position': 'absolute', 'top': top - (sidebarH - availHeight + 30), 'left': '50%', 'margin-left': '258px' });
			} else {
				$('.sidebar').css({ 'position': 'fixed', 'top': 'auto', 'left': '50%', 'margin-left': '258px', 'bottom': '0' });
			}
		} if ((top + availHeight) >= (columnH + hdHeight) && columnH > sidebarH && columnH > availHeight) {
			$('.sidebar').css({ 'position': 'absolute', 'top': columnH - sidebarH, 'bottom': 'auto','margin-left':'258px','left':'50%' });
		} if (top <= hdHeight) {
			$('.sidebar').css({ 'position': 'relative', 'margin-left': '0' ,'left':'auto','top':'auto'});
		}

		var bd_top = $('#bd').offset().top + 45;
		var top_ie6 = top + 45;
		if (top > bd_top) {
			if (typeof document.body.style.maxHeight === "undefined") {
				$('.beauty_tool').css({ 'position': 'absolute', 'top': top_ie6, 'left': '50%', 'margin-left': '-498px', 'width': '996px', 'z-index': '98' });
			} else {
				$('.beauty_tool').css({ 'position': 'fixed', 'top': '45px', 'left': '50%', 'margin-left': '-498px', 'width': '996px', 'z-index': '98' });
			}
		} else {
			$('.beauty_tool').css({ 'position': 'relative', 'top': '0', 'left': '0%', 'margin-left': '0', 'width': '996px', 'z-index': '98' });
		}

        	});
});


</script>
