<script type="text/javascript">



var currentindex=1;



    $("#flashBg").css("background-color",$("#flash1").attr("name"));



function changeflash(i) {	



    currentindex=i;



    for (j=1;j<=5;j++){



        if (j==i) 



        {$("#flash"+j).fadeIn("normal");



        $("#flash"+j).css("display","block");



        $("#f"+j).removeClass();



        $("#f"+j).addClass("dq");



        $("#flashBg").css("background-color",$("#flash"+j).attr("name"));



        }



        else



        {$("#flash"+j).css("display","none");



        $("#f"+j).removeClass();



        $("#f"+j).addClass("no");}



    }}



function startAm(){



    timerID = setInterval("timer_tick()",5000);



}



function stopAm(){



    clearInterval(timerID);



}



function timer_tick() {



    currentindex=currentindex>4?1:currentindex+1;



	changeflash(currentindex);



}



var showDialog = function(){



        createTv();



        $('#swf_tv').dialog({width:630, height: 'auto', modal: true, close: function(event, ui) {



                $('#swf_tv').html('');



        }});



    }



    var createTv = function(){



        $('#swf_tv').flash({



            swf:'http://player.youku.com/player.php/sid/XMzY2MDIyOA/v.swf',



            width: 610,



            height: 498,



            allowfullscreen:true,



            flashvars: {



                ShowId:0,



                Cp:0,



                Tid:0,



                VideoIDS:'XMzY2MDIyOA',



                isAutoPlay:true,



                Version:'/v1.0.0643',



                winType:'interior',



                iku_num:1



            }



        });



    };



$(document).ready(function(){



    $(".flash_bar div").click(function(){stopAm();startAm();});



    startAm();



});







</script>



