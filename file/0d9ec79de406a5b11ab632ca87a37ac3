<script type="text/javascript">

 var myDate = new Date();
var week="";
switch(myDate.getDay())
{
	case 0:week = "星期日";break;
	case 1:week = "星期一";break;
	case 2:week = "星期二";break;
	case 3:week = "星期三";break;
	case 4:week = "星期四";break;
	case 5:week = "星期五";break;
	case 6:week = "星期六";break;
}
var hour = myDate.getHours();
var hourPormpt = "";
if(hour<9) { hourPormpt = "早上好"; }
else if(hour<12) { hourPormpt = "上午好"; }
else if(hour<14) { hourPormpt = "中午好"; }
else if(hour<17) { hourPormpt = "下午好"; }
else if(hour<22) { hourPormpt = "晚上好"; }
hourPormpt = hourPormpt + "！";

var myYears = ( myDate.getYear() < 1900 ) ? ( 1900 + myDate.getYear() ) : myDate.getYear();
document.getElementById("currenttime").innerHTML = myYears + "年" + (myDate.getMonth() + 1) + "月" + myDate.getDate() + "日 " + week +" "+ hourPormpt;
                      

</script>
</div>

