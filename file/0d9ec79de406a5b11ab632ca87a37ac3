<script type="text/javascript"> 
   var MarginRight = 0;    //浮动层离浏览器右侧的距离
   var MarginTop =parseInt(parseInt(document.documentElement.clientHeight)*(1/2));
  // var MarginTop = 500;    //浮动层离浏览器顶部的距离
   var Width = 50;        //浮动层宽度
   var Heigth= 70;      //浮动层高度

  $(function (){
			$(".ewmbox002").mouseover(function(){
			  $(".erweibox").stop().animate({left:"-90px"},1000);
		  			 
			})
			$(".ewmbox002").mouseout(function(){
			 	 $(".erweibox").stop().animate({left:"50px"},1000);
			})
	 });
	 

  
   for( var i =0; i<$(".erweibox>ul li").length;i++)
   
   	var browser = (function(){
	var isIE6 = /msie 6/i.test(navigator.userAgent);
	var isIE = /msie/i.test(navigator.userAgent);
		return {
		msie:isIE,
			version:function(){
				switch(true){
				case isIE6:return 6;
				}
			}()
		};
	})();

  function getScrollTop() {  
        var scrollPos;  
        if (window.pageYOffset) {  
        scrollPos = window.pageYOffset; }  
        else if (document.compatMode && document.compatMode != 'BackCompat')  
        { scrollPos = document.documentElement.scrollTop; }  
        else if (document.body) { scrollPos = document.body.scrollTop; }   
        return scrollPos;   
	} 
    //实时设置浮动层的位置
   function Move()
   {
       document.getElementById("FloatDIV").style.top = MarginTop + 'px';
       document.getElementById("FloatDIV").style.right= MarginRight + 'px';
   } 
   function ie6Move()
   {
	   var scrooltop=getScrollTop();//document.body.scrollTop||scrooltopdocument.documentElement.scrollTop; 
	   //alert(scrooltop);
	   document.getElementById("FloatDIV").style.position='absolute';
       document.getElementById("FloatDIV").style.top =scrooltop+ MarginTop + 'px';
       document.getElementById("FloatDIV").style.right =MarginRight  + 'px';
   }
   if(browser.msie && browser.version==6)
	{
			ie6Move();
	}
	else{
			Move();
	}
   window.onscroll=function(){
		if(browser.msie && browser.version==6)
		{
			ie6Move();
		}
		else{
			Move();
		}
	}
   
 
</script>
