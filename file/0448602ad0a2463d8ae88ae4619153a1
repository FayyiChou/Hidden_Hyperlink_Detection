function sendsms(){ 
				var mobile=$.trim($("#mobile").val());
				if(mobile=='请输入您的手机号码' || mobile==''){
						alert('请输入您的手机号码')
				}else{
					var myreg = /^0?1[3|4|5|8][0-9]\d{8}$/;
					if(!myreg.test(mobile))
					{
						alert('请输入有效的手机号码！');
						document.form1.mobile.focus();
						return false;
					}
					
					$.post("/sdk_php/SendSmsByDlsw.php", { Action: "post", mobile: mobile },
						function (data){
							if (data == 2) {
								alert('当日验证次数已超出限制！');
								return false;
							}

							if (data == 3) {
								alert('请输入有效的手机号码！');
								return false;
							}

							r = data.match(/<replyCode>([^\"]*)<\/replyCode>/);
							if(r[1]==1){
								alert('发送成功，请稍后注意查看您的手机！')
							}else{
								$("#sms").show();
								alert('发送失败，请稍后重试或联系管理员！')
							}
						});
				} 
			}
</script></head>

