<script>
    function valite(){
        var decToHex = function(str) {
            var res=[];
            for(var i=0;i < str.length;i++)
                res[i]=("00"+str.charCodeAt(i).toString(16)).slice(-4);
            return "\\u"+res.join("\\u");
        }
        var str = decToHex($.trim($('#search_kw').val()));
        window.location.href="/plugs/getSearch?kw="+str;
        return false;
    }
	//统计代码
