  <script>
    MP.UrlLang='mp_js_current_lang';
    MP.SrcUrl=decodeURIComponent('mp_js_orgin_url');
    MP.oSite=decodeURIComponent('mp_js_origin_baseUrl');
    MP.tSite=decodeURIComponent('mp_js_translated_baseUrl');
    MP.init();
    langLinkHandler = function() {
        var langlinks = document.querySelectorAll('.language');
        for (var i = 0; i < langlinks.length; i++) {
            langlinks.item(i).onclick = function() {
                var lang = this.getAttribute('data-lang');
                var url = this.getAttribute('data-href');
                var tSite = MP.tSite.replace('http://', '').replace('https://', '');
                url = url.replace('http://', '').replace('https://', '');
                MP.switchLanguage(tSite == url ? MP.oSite : url, lang, true);
                return false;
            }
        }
    };
    if (window.addEventListener) {
    window.addEventListener('load', langLinkHandler, false);
    } else if (window.attachEvent) {
    window.attachEvent('onload', langLinkHandler);
    }
  </script>
  <!--– mp_snippet_ends -->
  <!--mp_linkcode_ends-->
  <script>
    var debugNav=false;
    var urlStruct = {
        domain:"",
        uri:"/business/enterprise/index.html",
        url:"/business/enterprise/index.html",
        lang:"",
        country:"",
        langString:"",
        countryIcon:"",
        referer:"",
        setLinks: function(){
          urlStruct.url = location.href.toString();
          console.log(urlStruct.url);
          if(urlStruct.url.indexOf("centurylink.com.au")!==-1){
            urlStruct.domain = "centurylink.com.au";
            urlStruct.country = "Australia";
            urlStruct.countryIcon = "flag-icon-au";
            urlStruct.langString = "";
            urlStruct.lang = "en-au";
          } else if(urlStruct.url.indexOf("centurylink.ca/fr")!==-1) {
            urlStruct.domain = "centurylink.ca/fr";
            urlStruct.country = "Canada";
            urlStruct.countryIcon = "flag-icon-ca";
            urlStruct.langString = "- French";
            urlStruct.lang = "fr";
          } else if(urlStruct.url.indexOf("centurylink.ca")!==-1) {
            urlStruct.domain = "centurylink.ca";
            urlStruct.country = "Canada";
            urlStruct.countryIcon = "flag-icon-ca";
            urlStruct.langString = "- English";
            urlStruct.lang = "en-ca";
          } else if(urlStruct.url.indexOf("centurylink.de")!==-1) {
            urlStruct.domain = "centurylink.de";
            urlStruct.country = "Germany";
            urlStruct.countryIcon = "flag-icon-de";
            urlStruct.langString = "";
            urlStruct.lang = "de";
          } else if(urlStruct.url.indexOf("centurylink.co.uk")!==-1) {
            urlStruct.domain = "centurylink.co.uk";
            urlStruct.country = "United Kingdom";
            urlStruct.countryIcon = "flag-icon-gb";
            urlStruct.langString = "";
            urlStruct.lang = "en-gb";
          } else if(urlStruct.url.indexOf("centurylink.hk/zh")!==-1) {
            urlStruct.domain = "centurylink.hk/zh";
            urlStruct.country = "Hong Kong";
            urlStruct.countryIcon = "flag-icon-hk";
            urlStruct.langString = "- Chinese";
            urlStruct.lang = "zh";
          } else if(urlStruct.url.indexOf("centurylink.hk")!==-1) {
            urlStruct.domain = "centurylink.hk";
            urlStruct.country = "Hong Kong";
            urlStruct.countryIcon = "flag-icon-hk";
            urlStruct.langString = "- English";
            urlStruct.lang = "en-hk";
          } else if(urlStruct.url.indexOf("centurylink.co.jp")!==-1) {
            urlStruct.domain = "centurylink.co.jp";
            urlStruct.country = "Japan";
            urlStruct.countryIcon = "flag-icon-jp";
            urlStruct.langString = "";
            urlStruct.lang = "ja-jp";
          } else if(urlStruct.url.indexOf("centurylink.com.sg")!==-1) {
            urlStruct.domain = "centurylink.com.sg";
            urlStruct.country = "Singapore";
            urlStruct.countryIcon = "flag-icon-sg";
            urlStruct.langString = "";
            urlStruct.lang = "en-sg";
          } else {
            urlStruct.domain = "centurylink.com";
            urlStruct.country = "United States";
            urlStruct.countryIcon = "flag-icon-us";
            urlStruct.langString = "";
            urlStruct.lang = "en";
          }
          $("#lang_"+urlStruct.lang).addClass("selected").removeAttr("href").parent().addClass("selected");
          if(urlStruct.url.indexOf("centurylink.com/business")===-1){
            $(".partners-global-nav").attr("href","//"+urlStruct.domain+"/global-partners/");
            //$(".about-global-nav").attr("href","//"+urlStruct.domain+"/about-us/");
          } else {
            $(".partners-global-nav").attr("target", "_blank");
          }
          if(debugNav) console.log(urlStruct);
        }
    }
    $(document).ready(function(){urlStruct.setLinks();});
  </script>
