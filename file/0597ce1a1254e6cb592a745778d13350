<script type="text/javascript">
var cityObj = $('#cityname');
    $('#province').change(function(){
		var id = $(this).val();
		if(id == ''){
			$('#cityname option').remove();
			var option = "<option value=''>全部城市</option>";
			cityObj.append(option);
		}else{
			$.post("/Zh/Search/showCityAjax.shtml",{provinceid : id},function(data){
                if(typeof JSON == 'undefined'){
                    $('head').append($("<script type='text/javascript' src='/Public/js/json2.js'>"));                          }
                obj=JSON.parse(data);
				$('#cityname option').remove();
				var option1 ="<option value=''>全部城市</option>";
				 cityObj.append(option1);
				$.each(obj,function(index,value){
					var option = "<option value="+value['id']+">"+value['0']+"</option>";		
					cityObj.append(option);
				});
			})
                  
		}
	});
</script>    <!--头部搜索文件结束 -->
</div>
