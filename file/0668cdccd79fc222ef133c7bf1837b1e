	function nav_active(){
		$(".nav li").children(".active").nextAll("ul").show();
	};
	nav_active();

  	$(".nav > li").mouseover(function(){
  		$(".nav li").children("a").removeClass("active").eq($('.nav li').index(this)).addClass("active");
  		$(".nav li").children("ul").hide();
  		$(this).children("ul").show();	
	});
})

