var COOKIE_PRE = '4A17_';var _CHARSET = 'utf-8';var LOGIN_SITE_URL = 'http://www.gnb360.com/member';var MEMBER_SITE_URL = 'http://www.gnb360.com/member';var SITEURL = 'http://www.gnb360.com/shop';var SHOP_SITE_URL = 'http://www.gnb360.com/shop';var RESOURCE_SITE_URL = 'http://www.gnb360.com/data/resource';var RESOURCE_SITE_URL = 'http://www.gnb360.com/data/resource';var SHOP_TEMPLATES_URL = 'http://www.gnb360.com/shop/templates/default';
