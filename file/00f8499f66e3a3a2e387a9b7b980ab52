<script type=text/javascript>
function $get(id) {
	return document.getElementById(id);
}
$get("demo2").innerHTML = $get("demo1").innerHTML
function Marquee() {
    if ($get("demo2").offsetWidth - $get("demo").scrollLeft <= 0)
        $get("demo").scrollLeft -= $get("demo1").offsetWidth
    else {
        $get("demo").scrollLeft++
    }
}
var MyMar = setInterval(Marquee, 25)
$get("demo").onmouseover = function () { clearInterval(MyMar) }
$get("demo").onmouseout = function () { MyMar = setInterval(Marquee, 25) }
</script>




