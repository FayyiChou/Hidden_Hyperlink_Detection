        function render(data, containerDom, juicerDom, id, again) {
            $.ajax({
                url: "http://js." + ZBJInfo.baseURI + "/vas/GetDeliveryContent",
                type: "get",
                dataType: 'jsonp',
                jsonp: 'jsonpcallback',
                cache: false,
                data: data
            }).done(function (res) {
                if (res.success) {
                    function func1(index) {
                        return parseInt(index) + id;
                    }
                    function func2(index) {
                        return parseInt(index) + id + 5;
                    }
                    juicer.register('linkid', func1);
                    juicer.register('linkid2', func2);
                    containerDom.html(juicer(juicerDom.html(), res)).find('img').lazyload({
                        effect: "fadeIn"
                    });
                    juicer.unregister('linkid', func1);
                    juicer.unregister('linkid2', func2);
                }
                else{
                    if(!again){
                        arrTryAgainTime[arrTryAgainTime.length] = 5;
                        again = arrTryAgainTime.length - 1;
                    }
                    tryAgain(render,[data, containerDom, juicerDom, id], again);
                }
            }).fail(function(){
                if(!again){
                    arrTryAgainTime[arrTryAgainTime.length] = 5;
                    again = arrTryAgainTime.length - 1;
                }
                tryAgain(render,[data, containerDom, juicerDom, id], again);
            })
        }

        function tryAgain(func, arrArg, again) {
            if(!arrTryAgainTime[again]--){
                return false;
            }
            setTimeout(function () {
                arrArg.push(again);
                func.apply(this,arrArg);
            }, 1000);
        }
    })();

    (function (ZDK) {
        var $anchor = $('.j-anchor');
        var $nav = $('.j-nav').children();

        $nav.click(function () {
            $("html,body").stop().animate({scrollTop:$anchor.eq($(this).index()).offset().top},500);
        });

        $('.j-side-btn-gotop,.j-btn-gopub').click(function () {
            $(".pub-box-filter").show();
            $(".pub-box").show().addClass('fixed');
            return false;
        });


        //   点击收费
        var clickStats = seajs.use("common:widget/va-components/click-stats/click-stats");
        var stats = new clickStats({});
        var clickStatsData={}, clickStatsQuery = "";

        $('body').on('click','a',function () {
            var _$this = $(this);
            var expressId = _$this.data("expressid");
            var appId = _$this.data("appid");
            if (expressId) {
                var partnerData = stats.get('defaultData');
                clickStatsData = {
                    appId : appId,
                    data : expressId,
                    addition : partnerData
                };
                clickStatsQuery = stats.build_query(clickStatsData);
                stats.request(ajaxUrl,clickStatsQuery)
            }
        });

        ZDK.define(function (require) {
            require('module.slider');
            ZDK.module.slide({
                target: $(".slide-box-img"),
                item: 'img',
                size: 1,
                step: 1,
                time: 400,
                timeout: 8000,
                path: false,
                oninited: function (slide) {
                    var $elem = $(".slide-box-control");
                    slide.cur = $elem.find('div').click(function () {
                        var $this = $(this);
                        var index = $this.index();
                        slide.play(index);
                    });
                }
            }).on("play", function (data) {
                var index = data.index;
                data.target.cur.eq(index).addClass('curr').siblings('.curr').removeClass('curr');
            });
        });

        var $window = $(window);
        var tempT = 0;
        var $menuBlock = $('.side-block');
        $window.scroll(function(){
            tempT = $window.scrollTop();
            tempT > 800 ? $menuBlock.fadeIn(500) : $menuBlock.stop(true,true).hide();
        });
    })(ZDK);
</script>

<script>
    seajs.use(["common:components/poptip/poptip"],function (ref) {
        window.Poptip=ref;
    })
    var host=window.location.host;
    if(host.indexOf(".zbjdev.")>-1){
        var url="http://cs.t17.zbjdev.com";
    }else {
        var url="http://cs.zbj.com";
    }
    $(document).ready(function () {
        $("#dialog-name").find(".service-type-item").click(function (e) {
            e.stopPropagation();
            $(this).addClass("service-item-active").siblings().removeClass("service-item-active");
        });
        $(".cs-chance").find(".cs-service-item").click(function (e) {
            e.stopPropagation();
            dialog.init();
            var index=$(this).index();
            $("#dialog-name").find(".service-type-item").eq(index).click();
        });
        $("#dialog-name").find(".dialog-close").click(function (e) {
            e.stopPropagation();
            dialog.close();
        });
        $("#dialog-name").find("#dialog-submit").click(function (e) {
            e.stopPropagation();
            dialog.submit();
        });
        $.ajax({
            url: url+'/api/TotalAndToday',
            type: 'get',
            dataType: 'jsonp',
            jsonp: 'jsonpcallback',
            success: function (res) {
                $("#dialog-name").find(".today-submit").text(res.today||1255);
                $("#dialog-name").find(".total-submit").text(res.total||111556);
            },
            error: function (res) {
                $("#dialog-name").find(".today-submit").text(1255);
                $("#dialog-name").find(".total-submit").text(111556);
            }
        });
    })
    var dialog={
        init:function () {
            $("#dialog-name").find(".service-type-item").removeClass('service-item-active').first().addClass("service-item-active");
            $("#dialog-name").find("#name-phone").val('');
            $("#dialog-name").find("#name-uname").val('');
            $("#dialog-name").show();
        },
        close:function () {
            $("#dialog-name").hide();
        },
        submit:function () {
            var pageId=$("#dialog-name").find(".service-item-active").index()==0?20:21;
            var telphone=$("#dialog-name").find("#name-phone").val();
            var name=$("#dialog-name").find("#name-uname").val();
            var reg = /^1[3|4|5|8|7]\d{9}$/;
            if(reg.test(telphone)){
                seajs.use(['common:components/refer-statistics/refer-statistics'], function (ref) {
                    var _objRefer = ref.getStatisticsData('object');
                    var data={
                        pageId: pageId,//页面ID
                        contact_name: name,//用户输入的称呼
                        contact_info: telphone,//用户输入的电话
                        remark: $("#dialog-name").find(".service-item-active").text(),
                        from: "猪站营销",
                        way_type:1,
                        way_2nd:0,
                        way_3nd:0,
                        first_page:_objRefer['first_page'] || '',
                        pub_page:window.location.href,
                        pmcode:_objRefer['pmcode'] || '',
                        referer:_objRefer['referer'] || ''
                    }
                    $.ajax({
                        url: url+'/chance/AddChanceNew',
                        data:data,
                        type: 'post',
                        dataType: 'jsonp',
                        jsonp: 'jsonpcallback',
                        success:function (res) {
                            if(res.success){
                                dialog.close();
                                alert("您的申请我们已收到，财务专员会在30分钟内联系您！");
                            }else {
                                Poptip.tip(res.data,"#dialog-submit")
                            }

                        },
                        error:function (res) {
                            Poptip.tip(res.data,"#dialog-submit")
                        }
                    })
                });
            }else {
                Poptip.tip("请输入正确的手机号码","#name-phone");
            }
        }
    }
    $.ajax({
        url: "/market/api/banner",
        type: "get",
        dataType: 'json',
        data: {
            from: 'zbj',
            cate: SemInfo.dir
        }
    }).done(function (res) {
        if(res.success){
            var _b1 = res.data.list[0];
            var _b2 = res.data.list[1];
            var _img = document.createElement('img');
            var _img2 = document.createElement('img');
            if(_b1 && _b1.image_src){
                console.log(_b1);
                _img.src = _b1.image_src;
                _img.alt = _b1.title;
                $('.j-link-block1').attr('href',_b1.href_url).attr('data-linkid',_b1.linkid).append($(_img));
            }
            if(_b2 && _b2.image_src){
                _img2.src = _b2.image_src;
                _img2.alt = _b2.title;
                $('.j-link-block2').attr('href',_b2.href_url).attr('data-linkid',_b2.linkid).append($(_img2).clone());
            }
        }
    });
</script>

