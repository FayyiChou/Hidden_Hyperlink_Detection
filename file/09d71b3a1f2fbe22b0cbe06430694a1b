function AllCoun(){
	tmp = os.con[0].checked;
	for(i=0;i<os.con.length;i++){
		os.con[i].checked = !(tmp);
		if(!tmp){$('#ms').show();$('#ms2').show();
		}else{$('#ms').hide();$('#ms2').hide();}
	}
	os.con[0].checked = tmp;
}
function ChkCoun(){
	if(os.con[1].checked){
		$('#ms').show();
		os.con[0].checked = false;
	}
	if(os.con.length==3){
		if(os.con[2].checked)
			$('#ms2').show();
			os.con[0].checked = false;
		if(os.con[1].checked==false&&os.con[2].checked==false){
			$('#ms').hide();
			$('#ms2').hide();
			os.con[0].checked = true;
		}
		if(os.con[1].checked==true&&os.con[2].checked==false){
			$('#ms2').hide();
			os.con[0].checked = false;
		}
		if(os.con[1].checked==false&&os.con[2].checked==true){
			$('#ms').hide();
			$('#ms2').show();
			os.con[0].checked = false;
		}
	}
	
}
