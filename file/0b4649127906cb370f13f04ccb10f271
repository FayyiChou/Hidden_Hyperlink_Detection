<script>
var newcodearr = Array();
</script>

<script type="text/javascript">
function getOffsetTop(obj) {
	if ($(obj).attr('top') != undefined) {
		return parseInt($(obj).attr('top'));
	}

	var b=0;
	var a=obj;
	while(a!=null&&a!=document.body) {
		b += a.offsetTop;
		a=a.offsetParent;
	}
	return b;
}
var showqrcode = function(citysuo, miaoid, guid, unid){
	var erweima = 'http://m.soufun.com/xf.d?ref=erweima&m=getmiaoinfo&city='+citysuo+'&activityid='+miaoid+'&guid='+guid;
	erweima = 'http://u.soufun.cn/qrcode.php?type=newhouse&resize=94&url='+encodeURIComponent(erweima);
	return erweima;
}

$(function() {
   var nlength = newcodearr.length;
   if(nlength > 0)
   {
	 for(var j = 1; j < nlength; j++)
	 {
	    daojishi(newcodearr[j].etime,newcodearr[j].newcode)
	 }
   }
	
	$(window).scroll(function(){
		var scroll_top;  
		if (document.documentElement && document.documentElement.scrollTop)
			scroll_top = document.documentElement.scrollTop;   
		else if (document.body) 
			scroll_top = document.body.scrollTop;
		if (scroll_top > 200) {
			$('.right_fl').show();
		} else {
			$('.right_fl').hide();
		}
		
		$('.content').each(function(index){
			var top = getOffsetTop(this);
			var height = $(this).height();
			var dom = $('.right_fl li').eq(index);
			if ((scroll_top+480) > top && scroll_top < (top + height-450)) {
				dom.addClass('on');
				$('.right_fl li a:eq(0):hidden').show();
				$('.right_fl li a:eq(1):visible').hide();
				dom.find('a').eq(0).hide();
				dom.find('a').eq(1).show();
			} else {
				dom.removeClass('on');
				dom.find('a').eq(0).show();
				dom.find('a').eq(1).hide();
			}
		});
	});

	$('.totop').click(function(e){
		e.preventDefault();
		$('body, html').animate({scrollTop:0}, 500);
	});

   $('.right_fl li').mouseover(function(){
		if (!$(this).hasClass('on')) {
			$(this).find('a').eq(0).hide();
			$(this).find('a').eq(1).show();
		}
   });
   $('.right_fl li').mouseout(function(){
		if (!$(this).hasClass('on')) {
			$(this).find('a').eq(0).show();
			$(this).find('a').eq(1).hide();
		}
   });
   $('.tehuims').mouseover(function(){
		if ($(this).find('.ms_ewm').length > 0) {
			var img = $(this).find('.ms_ewm img').eq(0);
			if (!$(img).attr('src')) {
				var data_id = $(img).attr('data-id');
				data_id = data_id.split('_')
				$(img).attr('src', showqrcode(data_id[0], data_id[1], data_id[2], data_id[3]));
			}
			$(this).find('.ms_ewm').show();
		}
   });
   $('.tehuims').mouseout(function(){
		if ($(this).find('.ms_ewm').length > 0) {
			$(this).find('.ms_ewm').hide();
		}
   });
});

function daojishi(etime,newcode)
{
   var timerun = false;
   var tempts = etime - 3600;
   if( etime > 0)
   {
	   var minsec = etime;
	   var tmpHours = parseInt(minsec / 60 / 60) ;
	   var hours = tmpHours%24;
	   var tmpDays = parseInt(tmpHours/24);
	   var days = tmpDays%30;
	   var month = parseInt(tmpDays/30);
	   var dom = $("."+newcode+"_time");
	    if (dom.is('p')) {
			dom.html('<strong class="cl_c00">'+month+'</strong>月<strong class="cl_c00">'+days+'</strong>天<strong class="cl_c00">'+hours+'</strong>小时');
		} else {
			dom.html('<strong class="cl_c00 fn">'+month+'</strong>月<strong class="cl_c00 fn">'+days+'</strong>天<strong class="cl_c00 fn">'+hours+'</strong>小时');
		}
   }
   else
   {
		etime = 0;  
        hours = 0;  
        days = 0;  
        month = 0;  
		$("#"+newcode+"_time").html('');
   }
   if(hours >0 || days > 0 || month > 0 ) 
	{
		timerun = true;
	} 
	else
	{
		timerun = false;
	}
	if (timerun) 
	{
        setTimeout('daojishi( ' + tempts +',"'+newcode+ '") ',1000*60*60);  
    }
}
</script>
