<script id="sfsearchjs" type="text/javascript" src="http://newhouse.js.soufunimg.com/newhouse/js/SouFunSearch.js?v=20160120" data="searchIndex"></script>

<script type="text/javascript">
$(function(){
	SouFunSearch.city = "淄博";

	$(".n_li").each(function() {
		var _self  = $(this);
		var key = 0;
		//mouseenter
		$(this).children(".dq").mouseenter(function(){
			if(_self.children(".content_layer").css("display") == "block")
			{
				_self.children(".content_layer").css("display","none");
				//_self.css("borderBottom","none");
			}
			else
			{
				_self.children(".content_layer").css("display","block");
				//_self.css("borderBottom","none");
			}
		});
		//mouseleave
		$(this).children(".dq").mouseleave(function(){
			setTimeout(function(){
				if(key == 0)
				{
					_self.children(".content_layer").css("display","none");
					//_self.css("borderBottom","");
				}
			},50);
			
		});
		//mouseenter
		$(this).children(".content_layer").mouseenter(function(){
			key=1;
		});
		//mouseleave
		$(this).children(".content_layer").mouseleave(function(){
			key = 0;
			_self.children(".content_layer").css("display","none");
			//_self.css("borderBottom","");
		});
		//三级菜单弹出控制
	});
});
function oversh(id){
	if(id=="show"){
		$("#ewm").show();
	}else{
		$("#ewm").hide();
	}
}
</script>

