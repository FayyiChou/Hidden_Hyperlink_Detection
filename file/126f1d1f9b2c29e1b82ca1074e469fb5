<script type="text/javascript">
    function sendComment(rid) {
        var comment = $('#commentContent').val();
        if (comment == '') {
            alert('请输入评论内容');
            return;
        }

        $.post('http://www.colg.cn/comment/add', {
            aid: 8947,
            rid: rid,
            content: comment
        }, function(data) {
            var jsonDATA = $.parseJSON(data);
            if (jsonDATA['REV'] == false) {
                alert(jsonDATA['MSG']);
            } else {
                $('#commentContent').val('');
                alert('评论成功！');
                location.reload();
            }

        });
    }

    function replyComment(cid) {
        var anh = $('#commentContent').offset().top - 80;
        $('html, body').stop().animate({scrollTop: anh}, 500);

        $('#subComment').unbind().click(function() {
            sendComment(cid);
        });
        $('#commentContent').focus();
    }

    $(document).ready(function() {
        $('#subComment').click(function() {
            sendComment(0);
        });
        $('.emoji-btn').hover(function() {
            $('.emoji-area').show();
            $('.name-area > .item').eq(0).click();
        }, function() {
            $('.emoji-area').hide();
        });

        $('.name-area > .item').click(function() {
            var typeid = $(this).attr('typeid');
            $('.emojis').hide();
            $('.emojis[typeid=' + typeid + ']').show();
            $('.name-area > .item').removeClass('on');
            $(this).addClass('on');
        });

        $('.emoji').click(function() {
            var code = $(this).attr('code');
            var content = $('#commentContent').val();
            $('#commentContent').val(content + code);
            $('.emoji-area').hide();
        });

            });
</script>

</body></html>