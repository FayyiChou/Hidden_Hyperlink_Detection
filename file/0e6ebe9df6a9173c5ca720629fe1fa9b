  <script type="text/javascript">
  var _bili_space_mid = 122879;
  var urlRE = /https?:\/\/space.bilibili.com\/\d+\//g; // //space.bilibili.com/1/
  var hashRE = /^#!\/.+?/;
  var url = window.location;
  var path = url.pathname.replace(/\//g, '');
  var origin = url.protocol + '//' + url.hostname;
  var hash = url.hash;
  var search = url.search;

  var oldUrlReg = {};
  var params;
  var newSearch = '';
  var newUrl = '';

  // 如果路由规则对不上，先改路由
  if (!urlRE.test(url.href) || path != _bili_space_mid) {
    newUrl = origin + '/' + _bili_space_mid + '/' + (hashRE.test(hash) ? hash : '#!/');
    window.location.href = newUrl;
  } else {
    // 对老版本做hash值的兼容
    oldUrlReg.video = /^#!\/video\/(\d+)\/(.*)\/(\d+)/; // tid/keyword/page
    oldUrlReg.fans = /^#!\/fans\/(.+?)\/(\d+)/; // list/page
    oldUrlReg.fav = /^#!\/fav\/(\d+)\/(\d+)\/(.*)\/(\d+)/; // fid/tid/keyword/page
    oldUrlReg.qz = /^#!\/qz-(.+)\/(\d+)/; // tab/page
    oldUrlReg.channel = /^#!\/channel-detail\/(\d+)\/(\d+)\/(\d+)/; // cid/page/order

    if (oldUrlReg.video.test(hash)) {
      params = oldUrlReg.video.exec(hash);
      newSearch = '?tid=' + params[1] + '&keyword=' + params[2] + '&page=' + params[3];
      newUrl = origin + '/' + _bili_space_mid + '/#!/video' + newSearch;
      window.location.href = newUrl;
    } else if (oldUrlReg.fans.test(hash)) {
      params = oldUrlReg.fans.exec(hash);
      newSearch = '?page=' + params[2];
      newUrl = origin + '/' + _bili_space_mid + '/#!/fans/' + params[1] + newSearch;
      window.location.href = newUrl;
    } else if (oldUrlReg.fav.test(hash)) {
      params = oldUrlReg.fav.exec(hash);
      newSearch = '?fid=' + params[1] + '&tid=' + params[2] + '&keyword=' + params[3] + '&page=' + params[4];
      newUrl = origin + '/' + _bili_space_mid + '/#!/fav' + newSearch;
      window.location.href = newUrl;
    } else if (oldUrlReg.qz.test(hash)) {
      params = oldUrlReg.qz.exec(hash);
      newSearch = '?page=' + params[2];
      newUrl = origin + '/' + _bili_space_mid + '/#!/qz-' + params[1] + newSearch;
      window.location.href = newUrl;
    } else if (oldUrlReg.channel.test(hash)) {
      params = oldUrlReg.channel.exec(hash);
      newSearch = '?cid=' + params[1] + '&page=' + params[2] + '&order=' + params[3];
      newUrl = origin + '/' + _bili_space_mid + '/#!/channel/detail' + newSearch;
      window.location.href = newUrl;
    }
  }
  </script>
