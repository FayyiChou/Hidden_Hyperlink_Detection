$(".Header .header_menu > li").css({ width:LIST_W});

})	

$(function(){
	
	$(".Header .header_menu > li > a > span.en").css({
			
			opacity: 0
			
			});

	$(".Header .header_menu > li").hover(function(){
	
		$(this).find(".cn").css({
			
			zIndex:1,
			opacity: 0
			
			});
		$(this).find(".en").css({
			
			zIndex:2,
			opacity: 1
			
			});
	
	},function(){
	
	
		$(this).find(".cn").css({
			
			zIndex:2,
			opacity: 1
			
			});
		$(this).find(".en").css({
			
			zIndex:1,
			opacity: 0
			
			});
	
	});
  	

});

