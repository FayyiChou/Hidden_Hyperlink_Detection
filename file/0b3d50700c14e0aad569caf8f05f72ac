<script type="text/javascript">
//抓網址get變數
function getQueryParams(qs) {
    qs = qs.split("+").join(" ");
    var params = [],
        tokens,
        re = /[?&]?([^=]+)=([^&]*)/g;
    while (tokens = re.exec(qs)) {
        params[decodeURIComponent(tokens[1])] = decodeURIComponent(tokens[2]);
    }
    return params;
}
var _GET = getQueryParams(document.location.search);//網址get變數

//版頭購物車 
$(function(){
	if (_GET['action'] != "shopping_checkout_2" && _GET['action'] != "shopping_checkout_3"){
		$(".Kg_MemberMenu04").hover(
			function(){
				$(".MemMenu_Cart").stop(true,true).slideDown(300);
			},function(){
				$(".MemMenu_Cart").stop().slideUp(300);
			}
		);
	}
});
$(function(){
	$("#button_search").click(function(){
		prod_search();
	});
});
function prod_search(Kw){
	var keyword ;
	if (Kw){
		keyword = Kw ;
	}else{
		keyword = $("#keyword").val();
	}

	// URLDECODE 網址，避免IE會出錯的問題
    var encode_keyword = encodeURIComponent(keyword);
	window.location.href = '../asolife/index.php?action=product_search&prod_keyword='+encode_keyword;
}
function headtop_removeScar( uid ){
	$.ajax({
		url:"/asolife/api/api.shopping_cart.php",
		type: "POST",
		dataType: "json",
		data: {myAct:"removeScarByUid",
			   uid: uid
		},
		success: function(Jdata){
			if(Jdata.error=='C00'){
				top_refresh();
			}
		}
	});
}
function top_refresh(_scar_no)
{
	if(_scar_no==null)
		var _scar_no = "";

	$.ajax({
		url: "/asolife/api/api.shopping_cart.php",
		type: "POST",
		dataType: "json",
		data: {myAct:"getCartNumber",
			   scar_no: _scar_no
		},
		success: function(Jdata){
			if( Jdata.error == 'C00' ){
				$('#top_content').empty();
				var html='';
				var count = 0;
					$.each( Jdata.content, function( key, value ) {
						var top_str = "";
						if(value['scar_type']==1)
							top_str = "(加贈)";
						if(value['scar_type']==2)
							top_str = "(加購)";
						html +='<ul class="MemMenu_Product clearfix" id="'+value['uid']+'">';
						html +='<li class="Cart_Pimg"><a href="../asolife/index.php?action=product_detail&prod_no='+value['prod_no']+'" class="mycart-img"><img border="0" alt="'+value["prod_name"]+'" title="'+value["prod_name"]+'" src="'+value['prod_file']+'"></a></li>';
						html +='<li class="Cart_Pname">';
						html +='<h2><a href="../asolife/index.php?action=product_detail&prod_no='+value['prod_no']+'" title="'+value["prod_name"]+'">'+value["prod_name"]+top_str+'</a></h2>';
						html +="<p>$ "+value['scar_price']+'<span>x'+value['scar_count']+'</span></p>';
						html +='</li>';
						html +='<li class="Cart_Pdelete">';
						if(value['scar_type'] != 1){
							html +='<a href="javascript:void(0);" title="刪除" onClick="javascript:headtop_removeScar( '+value['uid']+' );">刪除</a>';
						}
						html +='</li>';
						html +="</ul>";
						count = count+parseInt(value['scar_count']);
					});
					html +='<div class="MemMenu_Product_total">';
					html +='<p>總計:<span id="Cart_Total">$'+Jdata.total+'</span></p>';
					html +='<a href="../asolife/index.php?action=shopping_cart" title="結帳">結帳</a></div>';
				$('#top_content').append(html);
				$('#Car_Count').html(count);
				$('#total_cart').html(count+"件商品");
				$('#Cart_Count').html(count);
			}
		}
	})
}
top_refresh();

