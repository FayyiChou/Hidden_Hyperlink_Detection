   <script type="text/jscript" language="javascript">
         function classChange(classparent){
         
           var drp2 = document.getElementById("Productsearch1_product_smallclass");
           var drplength=drp2.length;
           var i;
           for(i=0;i<drplength;i++){
                drp2.options.remove(i);
            }
            drp2.length=0;
            
           
           if (classparent==""){
             var newOption = document.createElement("OPTION"); 
             newOption.text ="全部"; 
             newOption.value = "";           
             
             drp2.options.add(newOption); 
           }else{
            
            var newOption1 = document.createElement("OPTION"); 
             newOption1.text ="请选择小类"; 
             newOption1.value = "0";           
             
             drp2.options.add(newOption1); 
           var oHttpReq = new ActiveXObject("MSXML2.XMLHTTP");
           var oDoc = new ActiveXObject("MSXML2.DOMDocument");
           oHttpReq.open("POST", "product/productclassXML.aspx?classparent="+classparent, false);
           oHttpReq.send("");
            result = oHttpReq.responseText;
           var res=oDoc.loadXML(result);
          
            var nameItems = oDoc.selectNodes("//sew_productclass/Table/class_name"); 
            var idItems = oDoc.selectNodes("//sew_productclass/Table/class_id"); 
            var item;
            var id;
            for (item = nameItems.nextNode(),id=idItems.nextNode(); item&&id; item = nameItems.nextNode(),id=idItems.nextNode()){ 
             var namestr = item.nodeTypedValue; 
             var idstr = id.nodeTypedValue;
             var newOption2 = document.createElement("OPTION"); 
             newOption2.text =namestr; 
             newOption2.value = idstr; 
             
             
             drp2.options.add(newOption2); 
           
           
          }
        
           }
          
          }
    </script>
