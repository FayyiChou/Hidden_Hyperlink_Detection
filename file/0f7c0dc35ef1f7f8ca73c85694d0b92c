     <script>
$(function(){


  $(".TAB .toc").mousemove(function(){
    var $vv=$(this).parent(".TAB").attr("id");
	//alert($vv);
	  $($vv).hide();
	  $(this).parent(".TAB").find(".toc").removeClass("selected");
	  var xx=$(this).parent(".TAB").find(".toc").index(this);
	  //alert($xx);
	  $($vv).eq(xx).show();
	   //alert($($vv).eq(xx));
	  $(this).addClass("selected");
	  //$(".tel").text(nn.left+"/"+nv.left);
  
  });

});
</script> 
     <script>			





		 function select2() {


            $("#S2").html("");


            $.ajax(


            {


                type: "post",


                url: "/indexs.php?app=dogneed&act=region",


                data: {"type": "city","province":$('#S1').attr("value")},


				dataType: "json",


                success: function (msg) {


					$("#S2").show();


					$("#S2").append("<option value='0'>不限</option>");


                    for (var i = 0; i < msg.length; i++) {


                        $("#S2").append("<option value=" + msg[i].region_id + ">" + msg[i].region_name + "</option>");


                    }





                }


            })


        };


		 $(function () {





			$('#S1').bind("change", select2);


        });		


				 


				


	</script> 
