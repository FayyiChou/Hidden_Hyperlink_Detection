<script>
    function changeImg() {
        $(".xcpic ul").stop().animate({ left: -180 }, 3000, function () {
            /*滚动框归位*/
            $(".xcpic ul").css({ left: 0 });
            /*第一张插到第三张的后面*/
            $(".xcpic ul li:first").insertAfter(".xcpic ul li:last");
        });
    }
    setInterval(changeImg, 3000)
</script>

