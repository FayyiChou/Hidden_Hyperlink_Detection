        <script>        
var x=0;
$(".imgbox").hide();
$(".imgbox:first").show();
<!--当鼠标经过a的时候-->
$(".qhdian li").mouseover(function(){
	x=$(".qhdian li").index(this);
	/*当前a添加box,其他移除box*/
	$(".qhdian li").removeClass("fx");
	$(this).addClass("fx");
	/*让对应的图片显示*/
	$(".imgbox").hide();
	$(".imgbox:eq("+x+")").show();
})

/*定时器*/

function changeImg()
{	if(x<$(".imgbox img").length-1){x=x+1;}
	else{x=0;}
	$(".imgbox").hide();
	$(".imgbox:eq("+x+")").show();
	$(".qhdian li").removeClass("fx");
	$(".qhdian li:eq("+x+")").addClass("fx");
	}
var clock=setInterval(changeImg,3000)
$(".foxpic").mouseover(function(){
	clearInterval(clock);
	})
		   .mouseout(function(){
	clock=setInterval(changeImg,3000)
	})
        </script>
