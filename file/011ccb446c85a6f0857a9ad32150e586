<script>
// 关注微信banner广告悬浮效果
(function($){
    // 详情页面微信关注banner
    if ($('#fixed_ads').length > 0) {
      
        var  fixTop=$('#fixed_ads').offset().top;    
        $(window).scroll(function(){
            if($(".detail-rigad").height()<$("#postlist").height()){
              
                if ($(window).scrollTop() > fixTop ) {
                    $('#fixed_ads').css({'position':'fixed','top':'42px'});
                } else {
                    $('#fixed_ads').css({'position':'static'});
                }
            }
        });      
    }
})(jQuery);
</script>
<!--<script>
//没有登录用户，显示底部浮层  2017年01月06日 by lc
;(function($){
/*!
