		async:false,
		cache:true,
		url:_memberUrl+'/Ajax/updateViewList/uid/'+uid+ '/vid/' +play_id+'/time/'+ times,					
	});
}
</script>

<script>
function Play_vie(){
	var uid = $.cookie('_uid');
	if( uid ){
		if(document.getElementById("wsplayer")){
			getPlayerDom(playerName).getvideotime();		
		}
		var times = document.getElementById("aaa").value;
		var times=parseInt(times);
		
		var  play_id = document.getElementById("vod_id").value;
		$.ajax({
		type:'GET',
		dataType:"jsonp",
		jsonp:"jsonpCallback",	
		async:false,
		cache:true,
		url:_memberUrl+'/Ajax/updateViewList/uid/'+uid+ '/vid/' +play_id+'/time/'+ times,			
		});
	};
}

$(function(){
	setInterval("Play_vie()",300000);
});
</script>

