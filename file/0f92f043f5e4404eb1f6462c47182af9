	<script src="http://s.wasu.cn/portal/js/mainplayer.js?t=201703290001" type="text/javascript"></script>
	<!--用户观看视频获取积分系统-->
	<script>
	$(function(){
		if($.cookie('_uid')){
			var specitalVideoMark = '';	// 区分是否为指定视频的字段

			// 如果是 则在观看30分钟后调用增加积分的方法
			if( specitalVideoMark == 'db' ) {
				setTimeout("storage2()",600000);
			}else{
				setTimeout("update_integral()",30000);
			}
		}
	});
	var name='%E5%B8%83%E4%B8%81%E6%80%8E%E4%B9%88%E5%81%9A%E8%A7%86%E9%A2%91+%E5%B7%A7%E5%85%8B%E5%8A%9B%E5%B8%83%E4%B8%81%E8%9B%8B%E7%B3%95';
	function update_integral(){
		$.ajax({
	        type:'get',  
	 	    url: _memberUrl+'/Credit/update_integral/vid/'+7998415+'/name/'+name,
	        dataType : 'jsonp',  
	        jsonp:"jsoncallback", 
	        success: function(data) {
	            if(data == 'yes'){
					return false;
				}
	        }
    	});
    }
	
	// 指定视频积分 +10
    function storage2(){
		$.ajax({
	        type:'post',  
	 	    url: _memberUrl+'/Credit/storage2/vid/'+7998415+'/name/'+name,
	        dataType : 'jsonp',  
	        jsonp:"jsoncallback", 
	        success: function(data) {
	            if(data == 'yes'){
					return false;
				}
	        }
    	});
    }
