<script language=javascript><!--

function memlink(p_href,p_desc) {

	this.href = p_href;

	this.desc = p_desc;

	return this;

}

function showLinks(Links) {

	if(!Links.length) return ;

	document.write("<tr><td colspan=2>&nbsp; </td></tr>");

	document.write("<tr>");

	for(var i=1;i<Links.length;i++) {

		document.write("<td align=center><a href=\""+Links[i].href+"\" class='memlogin-link'>"+Links[i].desc+"</a></td>");

		if(i%2==0) {

			document.write("</tr>");

			if(i<Links.length) document.write("<tr>");

		}

	}

	if(i%2==0) document.write("</tr>");

}

--></script>

