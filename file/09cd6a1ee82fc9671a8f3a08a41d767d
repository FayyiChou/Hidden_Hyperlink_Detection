<script type="text/javascript">

function setfczlcityid(obj,id)
{
	$(obj).addClass("icon_bg");
	$(obj).siblings().removeClass("icon_bg");
	$("#id_index_limousine").html('<div style="text-align: center;clear: both;padding: 50px;color: #F80;font-size: 16px;line-height: 21px;"><img src="view/image/loading.gif" style="vertical-align: middle;"></div>');
	
	$.ajax({   
		url:'/c-index-a-fgetlimousinebycityid',
		type:'post',   
		data:'city_id='+id,   
		async : true, 
		error:function(){   
			alert('error');   
		},   
		success:function(data){ 
			$("#id_index_limousine").html(data);
		}
	});
}

