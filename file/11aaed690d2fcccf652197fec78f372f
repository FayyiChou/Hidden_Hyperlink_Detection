    $(function(){
        /*搜索*/
              $("#o_username,#o_email").focus(function(){
                    if($(this).val() == this.defaultValue)
                    {
                        $(this).val("")
                    }
                  }).blur(function(){
                        if($(this).val() == "")
                        {
                            $(this).val(this.defaultValue).css({"color":"#898989"})
                        }
                      }).keydown(function(){
                           $(this).css({"color":"#333333"})
                       })
                
                $(".opinion-show").click(function(){
                      //TweenMax.to($(this),1,{opacity:0, bottom:-27});
					  $(this).animate({opacity:0,bottom:"-27px"},350);
                      $(".opinion").animate({height:"show"},650);
                    })
                $(".slide_down").click(function(){
                      $(".opinion").animate({height:"hide"},650,function(){
                           //TweenMax.to($(".opinion-show"),1,{opacity:1, bottom:0});
						   $(".opinion-show").animate({opacity:1,bottom:0},1000)
                          })
                    })	   
                
              $(".slide_down").hover(function(){
                  $(this).animate({opacity:0.8},200)
                  },function(){
                       $(this).animate({opacity:1},200)
                      })	  
        })
    </script>
   <!--End-->
   

