				$("#hobby_tab_div a:first").addClass("on");
			$(".related_list_area:first").show();
			function changeHobbyType(type){
				$(".related_list_area").hide();
				$("#" + type + "_div").show();
				
				$("#hobby_tab_div a").removeClass("on");
				$("#" + type + "_tab").addClass("on");
