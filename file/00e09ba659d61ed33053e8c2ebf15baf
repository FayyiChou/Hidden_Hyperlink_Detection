	function getStyle(width, height, cut) {
		
        return '!' + width + 'x' + height + (cut == true ? 'cut' : '');
	}

	var html5_upload = false;
	var _createObjectURL;
	try {
	    _createObjectURL = window.createObjectURL || window.URL.createObjectURL || window.webkitURL.createObjectURL;    
	} catch (e) {

	}

	if (typeof FormData == 'function' && typeof _createObjectURL == 'function') {
	    html5_upload = true;
	}

	var needSign = html5_upload && (ATTACH_MODE != 'local') && IS_DIRECT;

    function dataURLtoBlob(dataurl) {
        var arr = dataurl.split(','), mime = arr[0].match(/:(.*?);/)[1],
            bstr = atob(arr[1]), n = bstr.length, u8arr = new Uint8Array(n);
        while(n--){
            u8arr[n] = bstr.charCodeAt(n);
        }
        return new Blob([u8arr], {type:mime});
    }
