<script type="text/javascript">
$(document).ready(function(e) {
	var sWidth = $("#focuses").width(); 
	var len = $("#focuses ul li").length, sHeight=sWidth/3; 
	var index = 0;
	var picTimer;
	$("#focuses ul li").css("width",sWidth);
	$("#focuses").css("height",sHeight+"px");
	var btn = "<div class='btnBg'><div class='btn'>";
	for(var i=0; i < len; i++) {
		btn += "<span></span>";
	}
	btn += "</div></div>";
	$("#focuses").append(btn);
    $("#focuses ul li").each(function() {
       var focuText=$(this).find("a").attr("title"),
	       focuindex=$(this).index();
	  $(".btn .span").eq(focuindex).find(".work").text(focuText);
	   
    });
	
	$("#focuses .btn span").css("opacity",0.5).mouseenter(function() {
		index = $("#focuses .btn span").index(this);
		showPics(index);
	}).eq(0).trigger("mouseenter");
	$("#focuses ul").css("width",sWidth * (len));
	
	$("#focuses").hover(function() {
		clearInterval(picTimer);
	},function() {
		picTimer = setInterval(function() {
			showPics(index);
			index++;
			if(index == len) {index = 0;}
		},12000); 
	}).trigger("mouseleave");
	
	function showPics(index) { 
		var nowLeft = -index*sWidth; 
		$("#focuses ul").stop(true,false).animate({"left":nowLeft},300); 
		$("#focuses .btn span").stop(true,false).removeClass("bg-red").css("opacity",0.5).eq(index).stop(true,false).addClass("bg-red").css("opacity",0.99); 
	}
});
</script>
