<script type="text/jscript">
$(function() {
	$("#mod-search").click(function(event){
		if($("#search-hot").is(":hidden")){
			$("#search-hot").css("display","block");
		}else{
			$("#search-hot").css("display","none");
		}
		event.stopPropagation();
	});
	$(".search_ico_bg").click(function(event){
		$(this).css("display","none");
		$("#mod-search").css("display","block");
		$("#search_input").focus();
		$("#search-hot").css("display","block");
		event.stopPropagation();
	});
	$(document).click(function(event){
			 $("#group_container,#message_containers,#search-hot,#mod-search").css("display","none"); 
			 $(".search_ico_bg").css("display","block");
    });
});
//键盘监听事件
$(function(){
  var seaType=-1;
  $(this).bind('focus',function(event){
        $(this).select();
        
    });
    $(this).each(function(){
        $(this).bind('keydown',function(e){
      e = (e) ? e : ((window.event) ? window.event : "") 
      var key = e.keyCode?e.keyCode:e.which; 
      var skeyType =0;
      if(seaType==-1){
      	skeyType = 4;
      }else if(seaType==0){
      	skeyType = 6;	
      }else if(seaType==1){
      	skeyType = 7;	
      }else if(seaType==2){
      	skeyType = 1;	
      }
      var search_key = $("#search_input").val();
            switch(key) {
            case 38:   
				if(seaType>-1){
				   seaType--;
				   $("#search-b li:eq("+seaType+")").children("a").focus();
				   $("#search-b li:eq("+seaType+")").children("a").attr('href',"http://www.nyato.com/index.php?app=public&mod=Search&act=index&a=public&t="+skeyType+"&k="+search_key+"");		   
				}else if(seaType==-1){
				   seaType=4;
				   seaType--;
				   $("#search-b li:last").children("a").focus();
				   $("#search-b li:eq("+seaType+")").children("a").attr('href',"http://www.nyato.com/index.php?app=public&mod=Search&act=index&a=public&t="+skeyType+"&k="+search_key+"");		   

				}
				return false;
                break;
            case 40:
				if(seaType<4){   
				   seaType++;
				   $("#search-b li:eq("+seaType+")").children("a").focus();		
				   $("#search-b li:eq("+seaType+")").children("a").attr('href',"http://www.nyato.com/index.php?app=public&mod=Search&act=index&a=public&t="+skeyType+"&k="+search_key+"");		   
				}else if(seaType=3){
					seaType=-1;
					seaType++;
					$("#search-b li:first").children("a").focus();
					$("#search-b li:eq("+seaType+")").children("a").attr('href',"http://www.nyato.com/index.php?app=public&mod=Search&act=index&a=public&t="+skeyType+"&k="+search_key+"");		   
				}
				return false;
                break;
            }
        });
  
});
});
function search_event(skey){
	   var searchkey = $("#search_input").val();
	   window.location.href= "http://www.nyato.com/index.php?app=public&mod=Search&act=index&a=public&t="+skey+"&k="+searchkey+"";		   
}
</script>
<script type="text/javascript">
    $(document).ready(function(){
        $("#mod-product dd").hover(function() {
            $(this).addClass("hover");
        }, function() {
            $(this).removeClass("hover");
        });
        core.plugInit('search');
    });

    core.plugFunc('dropnotify',function(){
        setTimeout(function(){
            core.dropnotify.init('message_list_container','message_container');
        },320);   
    });
	
	function searchFeed(){
		if (($('#search_input').val() == '') || $('#search_input').val()=='搜展会/ 活动/ 作品'){
			window.location.href='http://www.nyato.com/index.php?app=public&mod=Search&act=defaultSearch';
		}else{
			$('#search_feed').submit();
			return false;
		}
	}
    </script>
