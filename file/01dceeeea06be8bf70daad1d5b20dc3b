function Button1_onclick() {
	var value= document .getElementById("Text1").value;
	if ((value=='')||(value==' ')){
		alert("请输入检索词");
		return;
	}
	seachword=value;
	sern0="http://www.sdsqw.cn/ftr/ftr.htm";
	sern1="?seachword="+escape(seachword);
	ss=sern0+sern1;
	window.open(ss);
	return;
}
</script>
