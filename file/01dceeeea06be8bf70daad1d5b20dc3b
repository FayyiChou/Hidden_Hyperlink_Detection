<script type="text/javascript">
jQuery.noConflict();
jQuery(function () {
jQuery.ajax({
//请求的地址
url :　"index.php?option=com_chronoforms&chronoform=getweather",
//请求成功后执行的函数
success : function (data) {
//用eval()解析返回来的数据，将字符串转成JSON格式
var oD = eval('('+data+')');
//用jquery-1.8.2获取元素
var place = jQuery(".place"),
temp = jQuery(".temp"),
wind = jQuery(".wind"),
windPower = jQuery(".windPower");
//将返回来的数据放到相应的位置
place.html(oD["weatherinfo"]["city"]);
temp.html("&nbsp;&nbsp;&nbsp;"+oD["weatherinfo"]["temp1"] +"~"+oD["weatherinfo"]["temp2"]+ "°");
wind.html(oD["weatherinfo"]["weather"]);
//windPower.html(oD["weatherinfo"]["WS"]);
}
});
})
function Button2_onclick() {
var value= document.getElementById("search").value
	
		if ((value=='')||(value==' '))
		{
			alert("请输入检索词");
			return;
		}
		seachword=value;
		sern0="index.php/search";
//sern0="/dongyingsqw/index.php?option=com_chronoforms&chronoform=searchlist";
		sern1="?seachword="+escape(seachword);
		ss=sern0+sern1;
		window.open(ss);
		return;
	}
</script>
