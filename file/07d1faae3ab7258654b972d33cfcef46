    var ht_=document.getElementById("height"),  lis= ht_.getElementsByTagName('li'),l=lis.length-1;
    function shijian(){
        lis=ht_.getElementsByTagName('li');
        ht_.insertBefore(lis[lis.length-1],lis[0]);
    }
    var date1= 0,shi=180,downl=document.getElementById("download"),Tu5=document.getElementById("tu5");
    function can(){
        addClass(Tu5, "hide")
        downl.innerHTML="正在下载（免流量）";
        ht_.style.color="#fff";
        ht_.innerHTML="手机发现有8个问题<br/>请及时处理";
        removeClass(c, "hide");
        lod(1);
    }
    var tm=0;
    var ti= function (jian) {
        var tim=setInterval(function () {
            if (tm>jian){
                clearInterval(tim);
                can();
                
            }else{
                tm+=1;
                shijian();
                downl.innerHTML="<div style='font-size:18px;'>手机发现有8个问题</div><div style='font-size:16px;'>请及时处理</div>";
                addClass(c, "hide")
