    var c=document.getElementById("bg");
    var ctx=c.getContext("2d");
    c.width = 300;
    c.height = 150;
    var drow= function (curr) {
        ctx.clearRect(0,0, c.width, c.height);
        ctx.beginPath();
        ctx.strokeStyle='#9ec8fd';
        ctx.lineWidth=8;
        ctx.arc(151,80,60,-0.5*Math.PI,1.5*Math.PI);
        ctx.moveTo(151,80);
        ctx.closePath();
        ctx.stroke();
        ctx.beginPath();
        ctx.strokeStyle = '#fff';
        ctx.arc(151,80,60,-0.5*Math.PI,curr*2*Math.PI-0.5*Math.PI);
        ctx.moveTo(151,80);
        ctx.closePath();
        ctx.stroke();
        ctx.fillStyle="#fff";
        ctx.font="30px abc";
        text = Math.floor(curr*100)+"%";
        text_w= ctx.measureText(text).width;
        ctx.fillText(text,(150-(text_w/2)),90);
    };
    var t=0;
    var lod= function (gos) {
        var te=setInterval(function () {
            if (t>gos){
                clearInterval(te);
            }else{
                t+=0.01;
                drow(t);
            }
        }, alltime)
    };
    lod(0)
    var par=c.parentNode;
    var conver1=document.getElementById("conver1"),conver3=document.getElementById("conver3");
