  <script type="text/javascript">

function submitsearch() {
    if ($("#textfield").val() == "请输入关键字" || $("#textfield").val() == "") {
        alert("请输入查询关键字!");
        return;
    }
    else {
        $("#search").attr("action", $("#znjs").val() + "index.html?searchword=" + encodeURI($("#textfield").val()));
        $("#search").submit();
    }
}
</script>
                
