						<script type="text/javascript">
                        $(function(){
                            // 幻灯片
                            var fl_lis = $('#flashpics li').hide()
                                , fl_l = fl_lis.length
                                , fl_i = fl_l - 1
                                , fl_cur_li = $(fl_lis[i])
                                , flashpics_timer
                                , fl_arr = [];
                            
                            for(var j = 1; j <= fl_l; j ++){
                                fl_arr.push('<i>' + j + '</i>');
                            }
                            $('#flashpics span').html(fl_arr.join(''));
                            
                            var dNums = $('#flashpics i')
                                , curNum = $(dNums[fl_i]);
                            
                            function fn_flashpics(){
                                fl_i ++;
                                if(fl_i == fl_l){
                                    fl_i = 0;
                                }
                                fl_cur_li.hide();
                                fl_cur_li = $(fl_lis[fl_i]);
                                fl_cur_li.show();
                                curNum.removeClass('cur');
                                curNum = $(dNums[fl_i]);
                                curNum.addClass('cur');
                                $('#flashpics p').html(fl_cur_li.find('img').attr('alt'));
                                clearTimeout(flashpics_timer);
                                flashpics_timer = setTimeout(fn_flashpics, 3000);
                            }
                            fn_flashpics();
                            
                            $('#flashpics i').hover(function(){
                                clearTimeout(flashpics_timer);
                                fl_i = $(this).index();
                                fl_cur_li.hide();
                                fl_cur_li = $(fl_lis[fl_i]);
                                fl_cur_li.show();
                                curNum.removeClass('cur');
                                curNum = $(this);
                                curNum.addClass('cur');
                                $('#flashpics p').html(fl_cur_li.find('img').attr('alt'));
                            }, function(){
                                flashpics_timer = setTimeout(fn_flashpics, 1000);
                            });
                        });
                        </script>
                        <script type="text/javascript">
                            var tabs = document.getElementById('tabs').getElementsByTagName('i')
                                , cur_item = tabs[0];
                            for(var i = 0, item; item = tabs[i]; i ++){
                                item.onmouseover = (function(i){
                                        return function(){
                                                cur_item.className = '';
                                                tabs[i].className = 'active'
                                                cur_item = tabs[i];
                                                document.getElementById('acticle_list_1').style.display = 'none';
                                                document.getElementById('acticle_list_2').style.display = 'none';
                                                document.getElementById('acticle_list_3').style.display = 'none';
                                                document.getElementById('acticle_list_' + (i+1)).style.display = '';
                                            };
                                    })(i);
                            }
                        </script>                      
