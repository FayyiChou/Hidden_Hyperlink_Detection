			var nav = $('.nav ul li');
			var position = 0;
			switch(10001){
				case 10001:
				case 42:
					position = 64;
					break;
				case 1:
					position = 224;
					break;
				case 5:
					position = 384;
					break;
				case 2:
					position = 544;
					break;
				case 7:
					position = 704;
					break;
				case 40:
					position = 864;
					break;
			}
			$('.test').css({'left':position + 'px'});
			for(var i = 0;i < nav.length;i++){
				nav.eq(i).data('order',i);
			}
			nav.mouseenter(function(){
				$('.test').css({'left':64 + $(this).data('order') * 160 + 'px'});
			});
			nav.mouseleave(function() {
				$('.test').css({'left':position + 'px'});
			});
		})
