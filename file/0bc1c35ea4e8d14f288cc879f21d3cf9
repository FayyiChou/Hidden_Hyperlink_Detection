<script type="text/javascript">
    function ser_list(){
        var html = brands = '';
        //获取所有品牌
        $.ajax({
            type:'GET',
            url:'http://www.0931city.com/index.php?m=auto&c=auto&a=getBrand',
            data:'',
            dataType:'jsonp',
            jsonp: "callback",
            success: function(data){
                var op="";
                for(var i=0;i<data.length;i++ ){
                    op+="<option value='"+data[i].id+"'>"+data[i].title+"</option>";
                }
                html += op;
                $("#brand_ids").html("<option value='0'>选品牌</option>"+html);
            }
        });
    }
    function getSubrands(id,obj){
        $.ajax({
            type:'GET',
            url:'http://www.0931city.com/index.php?m=auto&c=auto&a=getSerial',
            data:'brand_id='+id,
            dataType:'jsonp',
            jsonp: "callback",
            success: function(data){
                len = data.length;
                obj.serial_ids.options.length=len;
                for (var j=0;j<len;j++){
                    obj.serial_ids.options[j].text=data[j].title;
                    if(j == 0){
                        obj.serial_ids.options[j].value='0';
                    }else{
                        obj.serial_ids.options[j].value=data[j].id;
                    }
                }
            }
        });
    }
    ser_list();
    var auto_status = 0;
    $("#auto-submit").click(function(){
        var brand_id = $("#brand_ids").val();
        var serial_id = $("#serial_ids").val();
        var name = $("#auto_name").val();
        var sex = $("input[name='auto-sex']:checked").val();
        var mobile = $("#auto_phone").val();
        var email = " ";
        var content = " ";
        var errorname, errorphone  = 1;
        if(auto_status == 1){
            alert("请不要重复提交");
            return false;
        }
        if(name == "") {
            alert("请正确填写真实姓名~");
            errorname = 0;
            return false;
        }
        if(mobile == "" || !(/^1[3-8][0-9]{9}|15[0-9]{9}jQuery/).test(mobile)) {
            alert("请填写正确的手机号码");
            errorphone = 0;
            return false;
        }
        if(errorname == 0 || errorphone == 0) {
            return false;
        }else{
            $.ajax({
                type:'GET',
                url:'http://www.0931city.com/index.php?m=auto&c=signup&a=response1',
                data:'serial_id='+serial_id+'&brand_id='+brand_id+'&name='+name+'&mobile='+mobile+'&email='+email+'&sex='+sex+'&content='+content,
                dataType:'jsonp',
                jsonp: "callback",
                success: function(data){
                    if(data.status == 1) {
                        auto_status = 1;
                        alert("提交团购成功");
                        $("#serial_ids").html("<option value='0'>选车系</option>");
                        $("#content").val("");
                        return false;
                    } else {
                        alert("提交失败");
                        return false;
                    }
                }
            });
        }
    })
</script>
