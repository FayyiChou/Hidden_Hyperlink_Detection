                <script type="text/javascript">
                       var id = 128080;
                       $.ajax({
                            url: "http://www.0931city.com/index.php?m=content&c=vote&a=getVote",
                            type: "post",
                            dataType: "json",
                            data: {id:id},
                            success: function(data) {
                                if(data){
                                    $("#add_1").html(data['zantong']);
                                                                   $("#add_2").html(data['fandui']);
                                    $("#add_3").html(data['wusuowei']);
                                }else{
                                    $("#add_1").html(0);
                                    $("#add_2").html(0);
                                    $("#add_3").html(0);
                                }
                            }
                        });
                   



                    function add_vote(type){
                        
                        $("#add-btn-"+type).click(function(){
                           if($(this).attr('data-type') == 'true'){
                               $(this).attr('data-type','false');
                               $.ajax({
                                   url: "http://www.0931city.com/index.php?m=content&c=vote&a=clickVote",
                                   type: "post",
                                   data: {id:id,type:type},
                                   success: function(data) {
                                       if(data){
                                           $("#add_"+type).html(parseInt($("#add_"+type).html())+1);
                                       }
                                   }
                               });
                           }
                        });
                    };
                </script>
