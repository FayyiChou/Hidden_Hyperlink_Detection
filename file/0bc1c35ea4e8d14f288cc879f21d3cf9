<script type="text/javascript">
    $(function(){
        var submitnum = 0;
        $("#house_bm_submit").click(function(){
            var house_sex = $("input[name='house-sex']:checked").val();
            var house_name = $("#house_name").val();
            var house_phone = $("#house_phone").val();
            var house_sele = $("#house_sele").val();
            var house_status = $("#house_status").val();
            var strText = $("#house_sele").find("option:selected").text();

            if(submitnum > 0){
                if(house_status == house_sele){
                    alert('请勿重复提交!');
                    return false;
                }
            }
            if(house_name==''){
                alert('姓名不得为空');
                return false;
            }
            if(house_phone==''){
                alert('电话不得为空');
                return false;
            }
            if (!/(^(0\d{2,3})?-?([2-9]\d{5,7})(-\d{1,5})?$)|(^((\d{3})|(\d{0}))?(13|14|15|18|17)\d{9}$)/g.test(house_phone)) {
                alert("请输入正确的电话号码");
                return false;
            }

            $.post("http://www.0931city.com/index.php?m=house&c=signup",{houseid:house_sele,house_name:strText,name:house_name,sex:house_sex,mobile:house_phone,email:" ",content:" "},function(data){
                if(data=='1'){
                    $("#house_status").val(house_sele);
                    alert("团购申请成功");
                    $("#house_name").val('');
                    $("#house_phone").val('');
                    submitnum++;
                    return false;
                }else{
                    alert("失败，请重新提交");
                    return false;
                }
            });
        });
    });
</script>


