        <script type="text/javascript">
            function sqsubmit(){
                var sqphone = $("#sqphone").val();
                if(sqphone == ''){
                    alert('电话号码不得为空');
                    return false;
                }
                if(!(/^1[3-8][0-9]{9}|15[0-9]{9}jQuery/).test(sqphone)){
                    alert('请输入正确的手机格式');
                    return false;
                }
                var toubiaoUrl = "http://www.0931city.com/anli.html";
                var boor = true;
                $.post(
                        toubiaoUrl,
                        {mobile:sqphone},
                        function(e){
                            if(e.status ==1){
                                if(boor==true){
                                    alert('申请成功');
                                    setTimeout(function(){
                                        $(".close").click();
                                    },2000);
                                    boor = false;
                                    if(boor == false){
                                        setTimeout(function(){
                                            window.location.href="http://www.0931city.com/anli.html";
                                        },2000);
                                    }

                                }



                            }else{
                                alert('申请失败');
                                return false;
                            }
                        },
                        "json"
                )
            }
        </script>
    </div>
</div>
