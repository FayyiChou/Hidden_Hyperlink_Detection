<script>

	function topay(vip_id,price) {

		if(!parseInt('0')){
			layer.msg('*请先登录！');
			document.cookie="userlurl="+window.location.href;
			$('#btn_login_reg').click();
			return;
		}
		$('#vip_id').val(vip_id);
		$('#money').val(price);

		if ($('#money').val() == '') {
			$('#err_tips').html('*请选择需要购买的VIP套餐');
			return false;
		}
		if ($('#task').val() == '') {
			$('#err_tips').html('*请选择支付方式');
			return false;
		}
		$('.pay_btn').html('支付处理中，请在新窗口查看结果...');
		$('#err_tips').html('');
		$('#vip_frm').submit();
	}

	function down_process2(file_id,event){
		var e=event||window.event;var ms=e.clientX+"*"+e.clientY;
		$.ajax({
			type : 'post',
			url : 'ajax.php',
			data : 'action=pc_2017040122&file_id='+file_id+'&ms='+ms+'&sc='+screen.width+'*'+screen.height,
			dataType : 'text',
			success:function(msg){
				if(msg == 'true'){

				}else{
					alert(msg);
				}
			},
			error:function(){
			}

		});
	}

	function ckust() {
		if('0'=='0'){
			cterror("此功能需要登录后才能使用。");
			return false;
		}
		if(''==''){
			cterror("本功能仅对VIP用户开放");
			window.location.href="/vip.php";
			return false;
		}
		return true;
	}
	function vip_down(file_id,verycode,event){
		if(!ckust()){
			return false;
		}
		var c1=layer.load();
		$.ajax({
			type : 'post',
			url : 'yythems_ajax.php',
			data : 'action=load_down_addr_vip&file_id='+file_id+'&verycode='+verycode,
			dataType : 'json',
			success:function(msg){
				layer.close(c1);
				if(msg.status){
					down_process2(file_id,event);
					layer.closeAll();
					window.location.href=msg.str;
					layer.msg('跳转下载链接中，请稍等...');
				}else{
					layer.msg(msg.str,function(){});
				}
			},
			error:function(){
			}
		});
	}
	function svip_down(file_id,verycode,event){
		if('0'=='0'){
			cterror("此功能需要登录后才能使用。");
			return false;
		}
		var c1=layer.load();
		$.ajax({
			type : 'post',
			url : 'yythems_ajax.php',
			data : 'action=load_down_addr_svip&file_id='+file_id+'&verycode='+verycode,
			dataType : 'json',
			success:function(msg){
				layer.close(c1);
				if(msg.status){
					down_process2(file_id,event);
					layer.closeAll();
					window.location.href=msg.str;
					layer.msg('跳转下载链接中，请稍等...');
				}else{
					layer.msg(msg.str,function(){});
				}
			},
			error:function(){
			}
		});
	}

	function com_down(file_id,verycode,event){

		var c1=layer.load();
		$.ajax({
			type : 'post',
			url : 'ajax.php',
			data : 'action=load_down_addr_com&file_id='+file_id+'&verycode='+verycode,
			dataType : 'text',
			success:function(msg){
				layer.close(c1);
				var arr = msg.split('|');
				if(arr[0] == 'true'){
					down_process2(file_id,event);
					window.location.href=arr[1];
					layer.closeAll();
					layer.msg('跳转下载链接中，请稍等...');
				}else{
					layer.msg(msg,function(){});
				}
			},
			error:function(){
			}
		});
	}
	function ckver(tag,file_id,event) {
		var vecd=$(tag).prev().val();//验证码
		var ftag=$(tag).prev().attr('ftag');//来自哪里

		if(ftag=='vip'){
			vip_down(file_id,vecd,event);
			return false;
		}else if(ftag=='svip'){
			svip_down(file_id,vecd,event);
			return false;
		}else if(ftag=='com'){
			com_down(file_id,vecd,event);
			return false;
		}
	}
	function vip_downvip_down(tab,file_id) {
		var het='230px';
		if('0'=='0'){
			het='480px';
		}
		var tabg=$('#very_btn');
		tabg.val('');
		tabg.prev().attr('src','imagecode.php?t='+Date.parse(new Date()));
		tabg.attr('ftag',tab);
		$('#vecysmt').attr('fid',file_id);

		layer.open({
			type: 1,
			shadeClose:true,
			title:'下载验证',
			area: ['400px', het],
			content:$('#vecytable')
		});
	}
	function save_as(file_id){

		if(!ckust()){
			return false;
		}
		$.ajax({
			type : 'get',
			url : 'ajax.php',
			data : 'action=save_as&file_id='+file_id+'&t='+Math.random(),
			dataType : 'text',
			success:function(msg){
				if(msg == 'true'){
					ctsuccess("另存文件操作成功。");
				}else if(msg =='ufile'){
					cterror("此文件已在您的网盘中，不需另存。");
				}else{
					cterror("另存文件操作失败，请重试。");
				}
			},
			error:function(){
			}

		});
	}
	function favorite_as(user_id){
		$.ajax({
			type : 'post',
			url : 'yythems_ajax.php',
			data : 'action=favorite&uid='+user_id+'&task=to_favorite&t='+Math.random(),
			dataType : 'text',
			success:function(msg){
				if(msg == 'ok'){
					ctsuccess("关注成功");
				}else{
					cterror(msg);
				}
			},
			error:function(){

			}

		});
	}

