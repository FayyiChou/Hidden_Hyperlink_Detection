		function sendfile(name,link){
			layer.open({
				title:'发布文件',
				type: 2,
				area: ['700px', '680px'], //宽高
				content: 'ajax.php?action=to_file&md=showsend&js='+name+'&link='+link,
			});
		}
		$('#to_file').click(function(){
			if('0'=='0'){
				layer.msg('请登录后再操作！',function(){});
				setTimeout(function(){$('.navbar-right li:eq(1)').addClass('open')},300);
				return false;
			}
			sendfile('','');
		});
		$('#pay_file').click(function () {
			if('0'=='0'){
				layer.msg('请登录后再操作！',function(){});
				setTimeout(function(){$('.navbar-right li:eq(1)').addClass('open')},300);
				return false;
			}
			var fid=$(this).attr('fid');
			layer.confirm('您确定要购买该文件吗？',function(){
				var c1=layer.load();
				$.post('ajax.php?action=pay_file',{fid:fid},function(data){
					layer.close(c1);
					var res=eval('('+data+')');
					if(res.success){
						layer.msg(res.msg);
						setTimeout('window.location.reload()',1000);
					}
				})
			});
		});

		$('.pageclass button').click(function(e){
			e.stopPropagation();
			var that=$(this);
			if(that.hasClass('active')){
				return false;
			}
			var c1=layer.load();
			var pid=that.parent().attr('pid');
			var page=that.text();
			$.post('ajax.php',{action:'getacom',pid:pid,page:page},function(data){
				layer.close(c1);
				var res=eval('('+data+')');
				var shtml='';
				if(res){
					$.each(res,function(k,v){
						shtml+='<div class="m-l-lg">';
						shtml+='<a class="pull-left thumb-sm avatar">';
						shtml+='<img onerror="loaderrimg(this)" style="width: 40px;height: 40px;" src="http://cdn.feemoo.com/'+v.logo+'">';
						shtml+='</a>';
						shtml+='<div class="m-l-xxl panel c_bd" ';
						if(v.rid==v.id){
							shtml+='style="background-color:#FFE3CB"';
						}
						shtml+='>';
						shtml+='<div class="panel-heading pos-rlt">';
						shtml+='<span class="arrow left pull-up c_lbd';
						if(v.rid==v.id){
							shtml+=' cc_bd" style="border-right-color:#FFF"';
						}else{
							shtml+='"';
						}
						shtml+='></span>';
						shtml+='<span class="text-muted m-l-sm pull-right">';
						shtml+='<i class="fa fa-clock-o"></i>';
						shtml+=v.time;
						if(((0==1||0==v.pid)&&v.is_offer=='1'&&!v.offer_status)||(0==1&&v.is_offer=='1'&&v.offer_status&&!v.offer_id&&v.offer_id!=0)){
							shtml+='<a mid="'+pid+'" rid="'+v.id+'" onclick="usecomc(this,'+v.uid+')" href="javascript:;" class="btn btn-default btn-xs"><i class="icon-bubble"></i>采纳</a>';
						}
						shtml+='<a mid="'+pid+'" onclick="tocomc(this,'+v.uid+')" href="javascript:;" class="btn btn-default btn-xs"><i class="icon-bubble"></i>回复</a>';
						if(''=='1'){
							shtml+='<a mid="'+v.id+'" onclick="delcomment(this)" href="javascript:;" class="btn btn-default btn-xs"><i class="icon-close"></i>删除</a>';
						}
						shtml+='</span>';
						shtml+='<a target="_blank" class="text-info" href="space.php?username='+v.username+'">'+v.nickname+'</a>'+(v.tname?' 回复 <a target="_blank" class="text-info" href="space.php?username='+v.tuname+'">'+v.tname+'</a>':'')+'：';
						shtml+=v.content;
						shtml+='</div></div></div>';
					});
					that.parent().parent().prev().html(shtml);
					$('.child_com a').click(function (e) {
						e.stopPropagation();
					});
				}
			});
			that.addClass('active');
			that.siblings().removeClass('active');
		})
	})

