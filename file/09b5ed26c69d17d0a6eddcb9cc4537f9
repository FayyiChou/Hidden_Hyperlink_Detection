<script>
	function favorite_as(user_id){
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			$('.navbar-right li:eq(1)').addClass('open');
			return false;
		}
		var c1=layer.load();
		$.ajax({
			type : 'post',
			url : 'yythems_ajax.php',
			data : 'action=favorite&uid='+user_id+'&task=to_favorite&t='+Math.random(),
			dataType : 'text',
			success:function(msg){
				layer.close(c1);
				if(msg == 'ok'){
					layer.msg("关注成功");
				}else{
					layer.msg(msg,function(){});
				}
				setTimeout(function(){window.location.reload();},2000);
			},
			error:function(){

			}

		});
	}
	function favorite_aso(user_id){
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			$('.navbar-right li:eq(1)').addClass('open');
			return false;
		}
		var c1=layer.load();
		$.ajax({
			type : 'post',
			url : 'yythems_ajax.php',
			data : 'action=favorite&uid='+user_id+'&task=un_favorite&t='+Math.random(),
			dataType : 'text',
			success:function(msg){
				layer.close(c1);
				if(msg == 'ok'){
					layer.msg("取消关注成功");
				}else{
					layer.msg(msg,function(){});
				}
				setTimeout(function(){window.location.reload();},2000);
			},
			error:function(){

			}

		});
	}
	function delmsg(tag){
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			$('.navbar-right li:eq(1)').addClass('open');
			return false;
		}
		layer.confirm('确定要删除吗?', function(index){
			var c1=layer.load();
			$(tag).addClass('disabled');
			var id=$(tag).attr('mid');
			$.post('ajax.php',{action:'delmsg',id:id},function(data){
				layer.close(c1);
				var res=eval('('+data+')');
				if(res.success){
					layer.msg('删除成功！');
					window.location.reload();
				}
			});
			layer.close(index);
		});
	}
