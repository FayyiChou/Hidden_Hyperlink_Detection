	function tocol(tag,tab){ //收藏
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			setTimeout(function(){$('.navbar-right li:eq(1)').addClass('open')},300);
			return false;
		}
		if(tab=='9'||tab=='10'){
			layer.msg('SVIP专区和官方动态不能顶！',function(){});
			return false;
		}
		var c1=layer.load();
		var that=$(tag);
		$(that).addClass('disabled');
		$.post('ajax.php',{action:'collect',mid:that.attr('mid')},function(res){
			layer.close(c1);
			var data=eval('('+res+')');
			if(data.success){
				var cnum=that.find('span').text();
				that.find('span').text((cnum?parseInt(cnum):0)+1);
			}else{
				layer.msg(data.msg,function(){});
			}
		})
	}
	function DrawImgw(tag,w,h){//做成定宽的
		var boxWidth=w; //w 这里设置需要的宽度
		var boxHeight=h;//h 这里设置需要的高度
		$(tag).css('width','');
		$(tag).css('height','');
		var imgWidth=$(tag).width();
		var imgHeight=$(tag).height();//比较imgBox的长宽比与img的长宽比大小

		if((boxWidth/boxHeight)<(imgWidth/imgHeight)){ //重新设置img的width和height
			$(tag).width((boxHeight*imgWidth)/imgHeight);
			$(tag).height(boxHeight);//让图片居中显示
			var marginLeft=(boxWidth-$(tag).width())/2;
			var marginTop=(boxHeight-$(tag).height())/2;

			$(tag).css("margin-left",marginLeft);
			$(tag).css("margin-top",marginTop);
		}else{ //重新设置img的width和height
			$(tag).width(boxWidth);
			$(tag).height((boxWidth*imgHeight)/imgWidth);//让图片居中显示
			var margin=(boxHeight-$(tag).height())/2;
			var wm = ($(tag).width())/2;

			$(tag).css("margin-top",margin);
			$(tag).css("margin-bottom",margin);
		}
	}

