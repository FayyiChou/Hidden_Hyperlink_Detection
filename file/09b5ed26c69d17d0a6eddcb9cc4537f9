<script>
	function totk(name, value) {
		var exp = new Date();
		exp.setTime(exp.getTime() + 180000);
		document.cookie = name + "=" + escape(value) + ";expires=" + exp.toGMTString();
	}

	function topay(vip_id,price) {

		if(!parseInt('0')){
			layer.msg('*请先登录！');
			document.cookie="userlurl="+window.location.href;
			$('#btn_login_reg').click();
			return;
		}
		$('#vip_id').val(vip_id);
		$('#money').val(price);
		if(vip_id<500){
			$('#mtk').val('203csEDpPRgrNGQkJhyaQcJdzi1+/Klq9Ipx73Su2NQZ7KD7Zi7PXni7tTUKTN8U4gZ8XbXbWqwucorzc5IUOvT+Vpsuc6ma5g2jVw');
			totk('vip_param','fc495bMwZdbMmskcLRB3DLGQ9lNnl+90EPs4IiMm9Hey1sncVZwNm8xL5O53ag');
		}else{
			$('#mtk').val('');
			totk('vip_param','');
		}
		if ($('#money').val() == '') {
			$('#err_tips').html('*请选择需要购买的VIP套餐');
			return false;
		}
		if ($('#task').val() == '') {
			$('#err_tips').html('*请选择支付方式');
			return false;
		}
		$('.pay_btn').html('支付处理中，请在新窗口查看结果...');
		$('#err_tips').html('');
		$('#vip_frm').submit();
	}
	function msgdetal_s(mid){
		window.open("http://www.feemoo.com/mydisk.php?item=profile&menu=cp&action=social&ntab=detail&id="+mid);

	}
