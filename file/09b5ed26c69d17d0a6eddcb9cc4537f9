	function tosave(tag){
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			setTimeout(function(){$('.navbar-right li:eq(1)').addClass('open')},300);
			return false;
		}
		var c1=layer.load();
		$(tag).addClass('disabled');
		var name=$(tag).parent().prev().prev().prev().find('input').val();
		var pic=$(tag).parent().prev().prev().find('input').val();
		var sort=$(tag).parent().prev().find('input').val();
		$.post('ajax.php',{action:'addmtab',name:name,pic:pic,sort:sort},function(data){
			layer.close(c1);
			var res=eval('('+data+')');
			if(res.success){
				layer.msg('添加成功！');
				window.location.reload();
			}
		})
	}
	function tomdtab(tag){
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			setTimeout(function(){$('.navbar-right li:eq(1)').addClass('open')},300);
			return false;
		}
		var c1=layer.load();
		$(tag).addClass('disabled');
		var id=$(tag).attr('tid');
		var name=$(tag).parent().prev().prev().prev().find('input').val();
		var pic=$(tag).parent().prev().prev().find('input').val();
		var sort=$(tag).parent().prev().find('input').val();
		$.post('ajax.php',{action:'mdtab',id:id,name:name,pic:pic,sort:sort},function(data){
			layer.close(c1);
			var res=eval('('+data+')');
			if(res.success){
				layer.msg(res.msg);
				window.location.reload();
			}
		})
	}
	function todeltab(tag){
		if('0'=='0'){
			layer.msg('请登录后再操作！',function(){});
			return false;
		}
		layer.confirm('确定要删除吗?', function(index){
			var c1=layer.load();
			$(tag).addClass('disabled');
			var id=$(tag).attr('tid');
			$.post('ajax.php',{action:'todeltab',id:id},function(data){
				layer.close(c1);
				var res=eval('('+data+')');
				if(res.success){
					layer.msg('删除成功！');
					window.location.reload();
				}
			})
			layer.close(index);
		});
	}
	function tocom(tag,tab){ //评论
