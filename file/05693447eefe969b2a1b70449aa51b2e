                $(function () {
                    $(".cent_Tox .cent_R .Notice .Noprt>a").each(function (n) {
                        $(this).hover(function () {
                            $(".cent_Tox .cent_R .Notice  .Noprt>a").removeClass("to");
                            $(this).addClass("to");
                            $(".after_box").hide();
                            $(".after_box:eq(" + n + ")").show();
                            $(".after_a").hide();
                            $(".after_a:eq(" + n + ")").show();
                        });
                    });

                })
