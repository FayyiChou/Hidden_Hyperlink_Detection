                        <script type="text/javascript">
                            jQuery(document).ready(function () {
                                var zcDefault = jQuery("#zCHidden");
                                if (zcDefault.val() != "") {

                                    var zcLi = zcDefault.parent().children(".select_box").children(".select_ul").children("ul").children("li");
                                    for (var i = 0; i < zcLi.length; i++) {
                                        if (zcLi.eq(i).attr("rl") == zcDefault.val()) {
                                            zcLi.eq(i).addClass("selected");
                                            zcDefault.parent().children(".select_box").children(".select_show").html(zcLi.eq(i).html());
                                        }
                                    }
                                }
                            })
                            function zhyClickZcShow(obj) {
                                switch ($(obj).attr("sw")) {
                                    case "0":
                                        $(obj).attr("sw", "1").removeClass("select_showon").addClass("select_showun");
                                        $(obj).parent().children(".select_ul").css("display", "block");
                                        break;
                                    case "1":
                                        $(obj).attr("sw", "0").removeClass("select_showun").addClass("select_showon");
                                        $(obj).parent().children(".select_ul").css("display", "none");
                                        break;
                                }
                            }
                            function zhyClickZcShowCu(obj) {

                                $(obj).parent().parent().parent().children(".select_show").attr("rl", $(obj).attr("rl")).html($(obj).html());
                                $(obj).parent().parent().parent().parent().children("#zCHidden").val($(obj).attr("rl"));

                                $(obj).parent().parent().parent().children(".select_show").attr("sw", "0").removeClass("select_showun").addClass("select_showon");
                                $(obj).parent().parent().css("display", "none");
                            }
                        </script>
                        <div class="kes3" style=" width:30px;">
