    <script type="text/javascript">
        function icomouseover(_obj, _src) {
            $(_obj).css("background", "url(" + _src + ") no-repeat");
            $(_obj).css("background-position", "center 10px");
            $(_obj).css("display", "inline-block");
            $(_obj).css("color", "#fff");
        }
        function icomouseout(_obj, _src) {
            $(_obj).css("background", "url(" + _src + ") no-repeat");
            $(_obj).css("background-position", "center 10px");
            $(_obj).css("display", "inline-block");
            $(_obj).css("color", "#044e8d");
        }
    </script>
