showModalDialog(url,'example04','dialogWidth:825px;dialogHeight:600px;dialogLeft:50px;dialogTop:50px;center:yes;help:yes;resizable:yes;status:yes') 
} 
function openwin2(url) 
{ 
window.open(url); 

} 
