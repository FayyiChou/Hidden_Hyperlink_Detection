<script type="text/javascript">
(function ($) {
    $(function () {
        var ival;
        var cycle = (function() {
            if (ival) {
                clearInterval(ival);
            }
            ival = setInterval(function () {
                var next = $('.categoryslider .nav-tabs .active').next();
                if (next.length === 0) {
                    next = $('.categoryslider .nav-tabs li').first();
                }
                next.find('a').tab('show');
            }, 5000);
        });
        $('.categoryslider').hover(function() {
            if (ival) {
                clearInterval(ival);
                ival = null;
            }
        }, cycle);

        cycle();
    });
})(jQuery);
</script>
