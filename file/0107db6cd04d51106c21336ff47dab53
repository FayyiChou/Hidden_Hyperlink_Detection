    <script>
        Getlist();
        function Getlist() {
            var vsl = '';
            $.ajax({
                type: "GET",
                url: "/JsonHandler/ProCorrelation/GetProjectHandler.aspx",
                data: { kname: vsl, ktype: 2 },
                dataType: "json",
                beforeSend: function () {
                    var lod = "<img src='http://img.caizhaowang.com/www/zhaobiao/images/rili/lodingforpro.gif' style='text-align: center;padding-left: 43%;padding-top: 3%;'>";
                    $(".neirong_list ul").html(lod);
                },
                success: function (data) {
                    var ht = [];
                    if (data.length > 0) {
                        for (var i = 0; i < data.length; i++) {
                            ht.push("  <li><a href=\"" + data[i].url + "\" title=\"" + data[i].proname + "-中国采招网\" target=\"_blank\">" + data[i].proname.substr(0, 30) + "</a>&nbsp;&nbsp;<span>" + data[i].protime + "</span>  </li>");
                        }
                        ht.push("<div class=\"clear\"></div>");
                        //ht.push("<div class=\"more_btn\"><a href=\"http://search.bidcenter.com.cn/search?keywords=" + encodeURI(vsl) + "\" title=\"" + vsl + "-招标采购\" rel=\"nofollow\" style='color: #FFF;'>查看更多</a></div>");
                        //var alis = $(".part-four_a");
                        //for (var i = 0; i < alis.length; i++) {
                        //    $(alis[i]).css("background-color", "");
                        //}

                        $(".neirong_list ul").html(ht.join(" "));
                    } else {
                        $(".neirong_list").hide();
                    }
                }
            });
        }
    </script>
    <script>
        //是否收藏项目
        isshoucang($("#shoucangpro"));
    </script>
