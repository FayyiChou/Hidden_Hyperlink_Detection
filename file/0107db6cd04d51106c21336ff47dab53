<script type="text/javascript">
    //'#jq_quice_r_register'
    var showRegisterSelector = '#jq_quick_r_register';
    var jq_showRegisterPage = null;
    var islinkusercenter = false;
    function layerShow(selector) {
        jq_showRegisterPage = $.layer({
            type: 1,
            area: ['auto', 'auto'],
            title: false,
            border: [0],
            closeBtn: [0, false], //去掉默认关闭按钮
            page: { dom: selector }
        });
    }
    //显示注册页
    $(".jq_lijichakan").live("click", function () {
        layerShow(showRegisterSelector);
    });
    //关闭弹出层
    $(".close_img").live("click", function () {
        layer.close(jq_showRegisterPage);
    });
    //显示注册页
    function jq_quick_r_register() {
        layer.close(jq_showRegisterPage);
        showRegisterSelector = "#jq_quick_r_register";
        layerShow(showRegisterSelector);
    }
    //显示登录页
    function jq_quick_r_login() {
        layer.close(jq_showRegisterPage);
        showRegisterSelector = "#jq_quick_r_login";
        layerShow(showRegisterSelector);
    }
    //显示完善页
    function jq_quick_r_perfect() {
        layer.close(jq_showRegisterPage);
        showRegisterSelector = "#jq_quick_r_perfect";
        layerShow(showRegisterSelector);
    }
    //检查手机号
    function checkMobile() {
        var obj = $("#jq_quick_mobile");
        var s = $.trim(obj.val());
        var tid = obj.attr("id");
        var stel = /(^0{0,1}1[3-8][0-9]{9}$)/;
        if (s == "" || s == "请填写真实联系方式")
            layer.alert('请输入您的手机号');
        else if (!stel.exec(s))
            layer.alert('如：13611280101');
        else if (s == "13611280101")
            layer.alert('请输入真实手机号码');
        else
            return true;
        return false;
    }
