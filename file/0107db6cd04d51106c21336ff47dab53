            var  a = getcookie("name");
            if (a != "") {
                var dome = getcookie("name").split('&')[0].toString();
                var dome2 = (dome.length > 8 ? dome.substring(0, 6) + ".." : dome);
                
                $("#login").html("您好,<a href=\"http://www.bidcenter.com.cn/busercenter/index.aspx\" style=\"    margin-right: 10px;\" title=\"" + dome + "\" class=\"cheng\">" + dome2 + "</a><a href=\"javascript:void(0);\" onclick=\"javascript:pub.logoutMember();return false;\">退出</a>");
                $("#login").show();
                $("#nologin").hide();
            } else {
                $("#login").hide();
                $("#nologin").show();
            }
            function getcookie(c) {
                var a = document.cookie.indexOf(c), b = document.cookie.indexOf(";", a);
                return (a == -1 ? "" : unescape(document.cookie.substring(a + c.length + 1, b > a ? b : document.cookie.length))).replace(";", "")
            }
