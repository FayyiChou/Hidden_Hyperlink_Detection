    //快速注册
    var isRegisterIng = false;
    function QuickRegister() {
        var jq_company = $.trim($("#usercompany").val());
        var jq_pattern_company = /[\u4E00-\u9FA5]/g;

        if (isRegisterIng) {
            layer.alert('提交中，请勿重复提交');
            return;
        } else if (!checkMobile()) {
            layer.alert('请正确输入手机号', 0, function (index) {
                layer.close(index);
                $("#jq_quick_mobile").focus();
            });
            return;
        } else if (jq_company == "" || jq_company == "填写在工商局注册的公司全称") {
            layer.alert('请输入公司工商注册全称', 0, function (index) {
                layer.close(index);
                $("#usercompany").focus();
            });
            return;
        } else if (jq_company.length < 5 || jq_company.length > 30) {
            layer.alert('公司名称必须5字以上', 0, function (index) {
                layer.close(index);
                $("#usercompany").focus();
            });
            return;
        } else if (GetStringByteLength(jq_company) - jq_company.length < 5) {
            layer.alert('公司名称必须5字以上汉字', 0, function (index) {
                layer.close(index);
                $("#usercompany").focus();
            });
            return;
        } else if (!jq_pattern_company.test(jq_company)) {
            layer.alert('公司名称必须包含汉字', 0, function (index) {
                layer.close(index);
                $("#usercompany").focus();
            });
            return;
        } else if (!companyName(jq_company)) {
            layer.alert('请输入公司工商注册全称', 0, function (index) {
                layer.close(index);
                $("#usercompany").focus();
            });
            return;
        } else if ($.trim($("#userlianxiren").val()) == "" || $.trim($("#userlianxiren").val()) == "联系人") {
            layer.alert('请输入联系人', 0, function (index) {
                layer.close(index);
                $("#userlianxiren").focus();
            });
            return;
        } else if ($.trim($("#userzhiwei").val()) == "") {
            layer.alert('请选择联系人职位', 0, function (index) {
                layer.close(index);
                $("#userzhiwei").focus();
            });
            return;
        }
            /* else if ($("#jq_quick_validatecode").attr("isgetvcode") != "true") {
            layer.alert('请先获取验证码', 0, function (index) {
            layer.close(index);
            $("#jq_quick_validatecode").focus();
            });
            return;
            }
            else if ($.trim($("#jq_quick_validatecode").val()).length != 6) {
            layer.alert('请确认您输入的验证码', 0, function (index) {
            layer.close(index);
            });
            return;
            }*/
        else if (!$("#jq_quick_isAgree").is(":checked")) {
            layer.alert('您还没有同意采招网服务条款', 0);
            return;
        } else {
            isRegisterIng = true;
            var qData = {
                mobiles: $.trim($("#jq_quick_mobile").val()),
                //vcode: $.trim($("#jq_quick_validatecode").val()),
                newsid: $("#newID").val(), newstype: $("#newType").val(),
                company: $.trim($("#usercompany").val()),
                lianxiren: $.trim($("#userlianxiren").val()),
                zhiwei: $.trim($("#userzhiwei").val())
            };
            $("#jq_lijizhuce").html("正在提交，请稍侯..");
            $.ajax({
                type: "GET", //用POST方式传输
                contentType: "application/json", //数据格式:JSON
                url: "http://www.bidcenter.com.cn/JsonHandler/QuickRegister.aspx?v=" + new Date().getTime() + ($.fn.FastRegPosition && $.fn.FastRegPosition > 0 ? "&position=" + $.fn.FastRegPosition : ""),
                data: qData,
                dataType: "jsonp",
                jsonp: "jsoncallback",
                jsonpCallback: "jsoncallback1",
                success: function (data) {
                    if (data.Ret) {
                        $("#news_contet_detail").css({ height: "", overflow: '' })
                        jq_quick_r_perfect();
                    } else {
                        if (data.Retbs == 1) {
                            layer.alert(data.Msg, 0, function (index) {
                                layer.close(index);
                                jq_quick_r_login();
                                $("#uname").val($.trim($("#jq_quick_mobile").val()))
                            });
                        } else {
                            layer.alert(data.Msg);
                        }
                    }
                    isRegisterIng = false;
                },
                error: function (e) {
                    //alert(e);
                }
            });
            //$.getJSON("http://www.bidcenter.com.cn/JsonHandler/QuickRegister.aspx?v=" + new Date().getTime() + ($.fn.FastRegPosition && $.fn.FastRegPosition > 0 ? "&position=" + $.fn.FastRegPosition : ""), qData, function (data) {
            //    if (data.Ret) {
            //        $("#news_contet_detail").css({ height: "", overflow: '' })
            //        jq_quick_r_perfect();
            //    } else {
            //        if (data.Retbs == 1) {
            //            layer.alert(data.Msg, 0, function (index) {
            //                layer.close(index);
            //                jq_quick_r_login();
            //                $("#uname").val($.trim($("#jq_quick_mobile").val()))
            //            });
            //        } else {
            //            layer.alert(data.Msg);
            //        }
            //    }
            //    isRegisterIng = false;
            //});
        }
    }
    // 返回当前字符串字节长度
    function GetStringByteLength(val) {
        var Zhlength = 0; // 全角
        var Enlength = 0; // 半角
        for (var i = 0; i < val.length; i++) {
            if (val.substring(i, i + 1).match(/[^\x00-\xff]/ig) != null)
                Zhlength += 1;
            else
                Enlength += 1;
        }
        return (Zhlength * 2) + Enlength;
    }
    //判断公司名称是否是全名
    function companyName(s) {
        var str_key = "办事处,公司,小学,中学,学校,中心,局,银行,分行,社,总队,处,电站,院,酒店,矿,政府,所,部,协会,厂,场,集团,馆,行,会,署,网";
        var key = str_key.split(",");
        var r = 0;
        for (var i = 0; i < str_key.length; i++) {
            if (s.indexOf(key[i]) > -1) {
                return true;
            }
        }

        return false;

    }
    var isRegisterPerfectIng = false;
    function QuickRegisterPerfect() {
        var sEmail = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
        if (isRegisterPerfectIng) {
            layer.alert('提交中，请勿重复提交');
            return;
        } else if ($.trim($("#useremail").val()) == "" || $.trim($("#useremail").val()) == "电子邮箱") {
            layer.alert('请输入电子邮箱', 0, function (index) {
                layer.close(index);
                $("#useremail").focus();
            });
            return;
        } else if (!sEmail.exec($("#useremail").val())) {
            layer.alert('请正确输入电子邮箱', 0, function () {
                $("#useremail").focus();
            });
            return;
        } else if ($.trim($("#userkeywords1").val().replace('关键词一', '') + $("#userkeywords2").val().replace('关键词二', '') + $("#userkeywords3").val().replace('关键词三', '')) == "") {
            layer.alert('请填写关键词', 0, function (index) {
                layer.close(index);
                $("#userkeywords1").focus();
            });
            return;
        } else {
            isRegisterPerfectIng = true;
            var qData = {
                email: $.trim($("#useremail").val()),
                keyword1: $.trim($("#userkeywords1").val()),
                keyword2: $.trim($("#userkeywords2").val()),
                keyword3: $.trim($("#userkeywords3").val())
            }
            $("#btnQuickRegisterPerfect").html("正在提交，请稍侯..");
            $.ajax({
                type: "GET", //用POST方式传输
                contentType: "application/json", //数据格式:JSON
                url: "http://www.bidcenter.com.cn/JsonHandler/QuickRegisterPerfect.aspx?v=" + new Date().getTime(),
                data: qData,
                dataType: "jsonp",
                jsonp: "jsoncallback",
                jsonpCallback: "jsoncallback1",
                success: function (data) {
                    if (data.Ret) {
                        $("#btnQuickRegisterPerfect").html("正在跳转，请稍侯..");
                        if (islinkusercenter)
                            window.location.href = "http://www.bidcenter.com.cn/busercenter/index.aspx";
                        else
                            window.location.reload();
                    } else {
                        layer.alert(data.Msg);
                        isRegisterPerfectIng = false;
                    }
                },
                error: function (e) {
                    //alert(e);
                }
            });
            //$.getJSON("http://www.bidcenter.com.cn/JsonHandler/QuickRegisterPerfect.aspx?v=" + new Date().getTime(), qData, function (data) {
            //    if (data.Ret) {
            //        $("#btnQuickRegisterPerfect").html("正在跳转，请稍侯..");
            //        if (islinkusercenter)
            //            window.location.href = "http://www.bidcenter.com.cn/busercenter/index.aspx";
            //        else
            //            window.location.reload();
            //    } else {
            //        layer.alert(data.Msg);
            //        isRegisterPerfectIng = false;
            //    }
            //});
        }
    }
    function QuickRegisterLogin() {
        var name = $("#uname");
        var pwd = $("#upassword");
        var n = $.trim(name.val());
        var p = $.trim(pwd.val());
        pub2.loginMember(n, p, name, pwd);
    }
    var pub2 = {
        loginMember: function (n, p, name, pwd) {
            var flag_jzmm = 0;
            var flag_jzyhm = 1;
            if ($.trim(n) == "" || n == "请输入用户名或手机号") {
                layer.alert("请输入用户名", 0, function (index) {
                    layer.close(index);
                    name.focus();
                });
                return false;
            }

            if ($.trim(p) == "" || p == "请输入密码") {
                layer.alert("请输入密码", 0, function (index) {
                    layer.close(index);
                    pwd.focus();
                });
                return false;
            }

            var o = [];
            var dt = new Date();
            var srctime = dt.getTime();
            o.push("http://sso.bidcenter.com.cn/Inc/member_login_crs_domain.ashx");
            o.push("?name=");
            o.push(n);
            o.push("&d=");
            o.push(new Date().getTime());
            o.push("&pwd=");
            o.push(p);
            o.push("&auto=");
            o.push(flag_jzmm);
            o.push("&run=");
            o.push(flag_jzyhm);
            o.push("&callback=");
            o.push(srctime);
            o.push("&jsoncallback=?");
            $("#btnQuickRegisterLogin").html("正在登录，请稍候..");
            $.ajax({
                type: "GET", //用POST方式传输
                contentType: "application/json", //数据格式:JSON
                url: o.join(""),
                dataType: "jsonp",
                jsonp: "jsoncallback",
                jsonpCallback: "jsoncallback1",
                success: function (r) {
                    if (r.status == "true") {
                        setTimeout(function () {
                            window.location.reload()
                        }, 200);
                    } else {
                        $("#btnQuickRegisterLogin").html("立即登录");
                        layer.alert(decodeURIComponent(r.brief));
                        pwd.val("");
                        pwd.focus();
                    }
                },
                error: function (e) {
                    //alert(e);
                }
            });
            //$.getJSON(o.join(""), function (r) {
            //    if (r.status == "true") {
            //        setTimeout(function () {
            //            window.location.reload()
            //        }, 200);
            //    } else {
            //        $("#btnQuickRegisterLogin").html("立即登录");
            //        layer.alert(decodeURIComponent(r.brief));
            //        pwd.val("");
            //        pwd.focus();
            //    }
            //});
        },
        logoutMember: function () {
            var y = "http://sso.bidcenter.com.cn/logout";
            location.href = y + "?redirectURL=" + encodeURI(location.href.replace(/&/g, '|'));
        },
        login: function (APIKey) {
            var y = "http://sso.bidcenter.com.cn/";
            location.href = y + "validate?apk=" + APIKey + "&u=" + encodeURI(location.href.replace(/&/g, '|'));
            return false;
        }
    };
    $("a[href=#]").attr("href", "javascript:void(0);");
</script>

        <script type="text/javascript">
            $.fn.FastRegPosition = 668;
        </script>
        
    </div>
