<script>
    var lf = null;
    function modelerror() {
        var html = $('#div_error_tpl').html();
        lf = layer.open({type: 1, title:false, area: ['650px', '430px'], content: html, 'skin':'layui-custom'});
    }

    var lf1 = null;
    function showfavorite() {

        $.ajax({
            url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
            type: 'get',
            dataType:'jsonp',
            data:{action:'home.get.islogin'},
        })
        .done(function(data) {
             if (data.status == -201) {
                layer.msg('请登录后收藏,正在为您跳转至登录页面', {time: 2000}, function () {
                    location ="http://reg.justeasy.cn/login.html?backurl=http://www.justeasy.cn/3d/id-182322.html";
                });
                return false; 
            }else{
                var html = $('#div_favorite_tpl').html();
                lf1 = layer.open({type: 1, title:false, area: ['650px', '310px'], content: html, 'skin':'layui-custom'});
                $('#collectionid').chosen({
                    width: '320'
                });
                getUserCollection();
            }
        })
    }
    function addmodelerror(id) {
        if (submiting == 0) {
            var content = $.trim($("#error_content").val());
            var repeatid = $.trim($("#repeatid").val());
            var errers = $('input[name="errors"]:checked ').val();
            if (!content) {
                layer.msg('报错描述不能为空', {time: 2000});
                return;
            }
            submiting = 1;
            var ll = layer.load(0, {shade: [0.5,'#fff']});
            ajax.action = "home.add.modelerror";
            ajax.params = [{'k':'modelid', 'v':id},{'k':'repeatid', 'v':repeatid},{'k':'errers', 'v':errers},{'k':'content', 'v':content}];

            ajax.execute(function () {
                submiting = 0;
                layer.close(ll);
                if (ajax.executeinfo.code == -1001) {
                    layer.msg('请登录后报错,正在为您跳转至登录页面', {time: 2000}, function () {
                        location = sy_const_domain.user_domain + "/login.html";
                    });
                }else if (ajax.executeinfo.code == 3001) {
                    layer.msg(ajax.executeinfo.msg, {time: 2000}, function () {
                        layer.close(lf);
                    });
                }else{
                    layer.msg(ajax.executeinfo.msg, {time: 2000});
                }
            });
        }
    }

    // 收藏功能 2016/07/27 gm
    function getUserCollection() {

        // 收藏弹窗的图片
        //var img = $("#m_title").attr("src");
        //$('#fa_img').attr("src",img);
        var img_id = 182322;
        $.ajax({
            url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
            type: 'get',
            dataType:'jsonp',
            data:{action:'home.get.collect',id:img_id},

        })
                .done(function(data) {
                    if (data && data.status == 200) {
                        var option = "<option value='0'>默认图集</option>";
                        $.each(data.list, function(index, val) {
                            option += "<option value='" + val.id + "'>" + val.name + "</option>"
                        });
                        $('#collectionid').html(option);
                        $('#collectionid').trigger('chosen:updated')
                        //if ($(".newGetInput").length == 0) {
                        //var createHTML = '<div class="newGetInput"><input type="text" class="getIptNames" id="collectionname" placeholder="图集名称"><span class="CkaddTj" id="btn_create_coll">创建</span></div>';
                        //};
                        //$('.chosen-results').after(createHTML);
                        // 样式的修改
                        $('.layui-layer-content').css({'height':'310px','overflow':'hidden'});
                        $('#fa_img').attr("src",'http://small.justeasy.cn/'+data.small_pic);
                        //$('.chosen-drop').css({'height':'170px','overflow-y':'scroll'});
                    }else if(data.code == -1001){
                        layer.msg('请登录后收藏,正在为您跳转至登录页面', {time: 2000}, function () {
                            location = sy_const_domain.user_domain + "/login.html?backurl="+sy_const_domain.home_domain+'/3d/id-'+182322+'.html';
                        });
                    }else{

                    }
                })
    }

    //添加图集
    $(document).on("click", "#btn_create_coll", function() {

        var name = $("#collectionname").val();
        var tags = $("#tags").val();
        if (name == "") {
            layer.msg("请填写图集名称！");
            return;
        }
        /*if (tags == "") {
         layer.msg("请填写标签名称！");
         return;
         }
         var tagsArr = tags.split("#");
         if (tagsArr.length > '6') {
         layer.msg("标签数不能超过6个!");
         return;
         }
         for (var i = 0; i < tagsArr.length; i++) {
         if (tagsArr[i].length > '6') {
         layer.msg("单个标签不能超过6个字!");
         return;
         }
         if (tagsArr[i] == "") {
         layer.msg("标签只能用一个#隔开哦！");
         return;
         }
         }*/

        $.ajax({
            url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
            type: 'get',
            dataType:'jsonp',
            data: {
                name: name,
                action :'home.get.addFavorite'
            },
        })
                .done(function(data) {
                    if (data && data.status == 200) {
                        $("#collectionid").append("<option value='" + data.list + "'>" + name + "</option>");
                        $("#collectionid").val(data.list);
                        $('#collectionid').trigger("chosen:updated");
                        $("#collectionname").val('');
                    }else if (data.status == -201) {
                        layer.msg('请登录后收藏,正在为您跳转至登录页面', {time: 2000}, function () {
                            location = sy_const_domain.user_domain + "/login.html?backurl="+sy_const_domain.home_domain+'/3d/id-'+182322+'.html';
                        });
                    }else {
                        layer.msg(data.msg);
                    }
                })
                .fail(function() {
                    layer.msg("网络异常");
                })
    });


    //收藏
    $(document).on('click', "#btn_save", function(event) {
        event.preventDefault();
        var workid = $("#workid").val();
        if (workid == "") {
            layer.msg("请选择图片！");
            return;
        }
        var img = $("#small_work_img").val();
        var collectionid = $("#collectionid").val();
        if (collectionid == "") {
            layer.msg("请选择图集！");
            return;
        }
        $.ajax({
            url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
            type: 'get',
            dataType:'jsonp',
            data: {
                collectionid: collectionid,
                id: workid,
                img: img,
                title:$('#tilte_p_name').val(),
                action:'home.get.addFavorite_e'
            },
        })
                .done(function(data) {
                    if (data && data.status == 200) {
                        layer.msg("收藏成功！");
                        layer.closeAll('page');
                        $("#save_" + workid).text(parseInt($("#save_" + workid).text()) + 1);
                        $('#fa_num').text(parseInt($('#fa_num').text())+1);
                        $('#fa_active').addClass('icon-stars').removeClass('icon-stars-h');
                        $('.no-border').addClass('active');
                    } else {
                        layer.msg(data.msg);
                    }
                })
                .fail(function() {
                    layer.msg("网络异常");
                })

    });
    /* gm 2017-3-31 会员关注功能 */
    $(document).on('click', '.follow', function(event) {
        event.preventDefault();
        var fid = $(this).attr("data");
        follow(fid);
    });
    $(document).on('click', '.unfollow', function(event) {
        event.preventDefault();
        var fid = $(this).attr("data");
        unfollow(fid);
    });

    /* 关注 */
    function follow(fid) {
        var num = parseInt($("#icon-fs-y").text());
        $.ajax({
            url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
            type: 'get',
            dataType:'jsonp',
            data:{action:'user.get.follow',fid:fid},
        }).done(function(data) {
            if (data && data.status == 200) {
                layer.msg(data.msg, {
                    icon: 1,
                    time: 2000 //2秒关闭（如果不配置，默认是3秒）
                }, function(){
                    $(".follow[data='"+fid+"']").removeClass('follow').addClass('unfollow').text("取消关注");
                });
            } else if (data.status == 500) {
                layer.confirm(data.msg, {icon: 0,btn:['确定']},function(index){
                    layer.close(index);
                    location.href="http://reg.justeasy.cn/user/login/index.php?backurl=http://www.justeasy.cn/3d/id-182322.html";
                });
            } else {
                layer.msg(data.msg);
            }
        }).fail(function(){
            layer.msg('网络异常！');
        });
    }

    /* 取消关注 */
    function unfollow(fid) {
        $.ajax({
            url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
            type: 'get',
            dataType:'jsonp',
            data:{action:'user.get.unfollow',fid:fid},
        }).done(function(data) {
            if (data && data.status == 200) {
                layer.msg(data.msg, {
                    icon: 1,
                    time: 2000 //2秒关闭（如果不配置，默认是3秒）
                }, function(){
                    $(".unfollow[data='"+fid+"']").removeClass('unfollow').addClass('follow').html("关注");
                });
            } else if (data.status == 500) {
                layer.confirm(data.msg, {icon: 0,btn:['确定']},function(index){
                    layer.close(index);
                    location.href="http://reg.justeasy.cn/user/login/index.php?backurl=http://www.justeasy.cn/3d/id-182322.html";
                });
            } else {
                layer.msg(data.msg);
            }
        }).fail(function(){
            layer.msg('网络异常！');
        });
    }



</script>
