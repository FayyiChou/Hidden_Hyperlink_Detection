    function confirmdownload(id){
        $.ajax({
            url: '/home/model/confirmdown.php',
            type: 'post',
            data: {id: id},
        })
        .done(function(data) {
            if(data.status==-200){
                layerLogin('downloadmodel',2);
            }else if(data.status==-201){
                var html = $('#div_confirm_tpl0').html();
                lf5 = layer.open({type: 1, title:'温馨提示', area: ['400px', '200px'], content: html, 'skin':'layui-custom'});
            }else if(data.status==-202){
                var html = $('#div_confirm_tpl2').html();
                lf5 = layer.open({type: 1, title:'温馨提示', area: ['400px', '200px'], content: html, 'skin':'layui-custom'});
            }else if(data.status==-203){
                var html = $('#div_confirm_tpl1').html();
                lf5 = layer.open({type: 1, title:'温馨提示', area: ['450px', '200px'], content: html, 'skin':'layui-custom'});
            }else if(data.status==200){
                var html = $('#div_confirm_tpl3').html();
                lf5 = layer.open({type: 1, title:'温馨提示', area: ['400px', '200px'], content: html, 'skin':'layui-custom'});
            }else if(data.status==201){
                var html = $('#div_confirm_tpl4').html();
                lf5 = layer.open({type: 1, title:'温馨提示', area: ['400px', '200px'], content: html, 'skin':'layui-custom'});
            }else if(data.status==202){
                var html = $('#div_confirm_tpl5').html();
                lf5 = layer.open({type: 1, title:'温馨提示', area: ['400px', '200px'], content: html, 'skin':'layui-custom'});
            }else if(data.status==203){
                var html = $('#div_confirm_tpl6').html();
                lf5 = layer.open({type: 1, title:'温馨提示', area: ['400px', '200px'], content: html, 'skin':'layui-custom'});
            }
        })
        $.getJSON('http://data.justeasy.cn/home/index/downloadrecord.cgi?callback=?', {id: '182322',name:'现代书架组合',token:'b37533c7e3773ade122f64e775cafd7a'}, function(json, textStatus) {
        });
    }

