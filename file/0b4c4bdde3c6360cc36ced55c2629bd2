<script type="text/javascript">
    $(function(){

        // 右侧广告
    var advert_url = 'http://small.justeasy.cn/';
    $.ajax({
        url:'http://data.justeasy.cn/service/cget.cgi?callback=?',
        type:'type',
        dataType:'jsonp',
        data:{action:'home.get.advert',position:681,num:10},
        success:function(json){
            if(json.status == 200){
                var html = '';
                $.each(json.data,function(i,items){
                    html +='<div class="img360"><a href="'+items['url']+'" target="_blank"><img src="'+advert_url+items['pic']+'"></div>'
                });
                $("#ad").append(html);
            }
        }
    });

        // 输入图集时 按钮变亮
        $('.getIptNames').on('focus', function(){
            $('.CkaddTj').css("background-color","#ee7700");
        });

        $('.getIptNames').on('blur', function(){
            var text = $('.getIptNames').val();
            if(text == ''){
                $('.CkaddTj').css("background-color","#e0e0e0");
            }
        });
         var token ="B37533C7E3773ADE122F64E775CAFD7A";
         $.getJSON('http://data.justeasy.cn/home/index/getmodelviewnum.cgi?callback=?', {id: "182322",name:"现代书架组合",token:token}, function(json, textStatus) {
             $('#model_view').text(json.num);
         });

        //getcomment(182322,1,1);
        /*
        $("#comment_butt").click(function(){
            var objid="<%=id%>";
            var comment=$('.textareabox').val();
            if(!comment){
                layer.msg('评论内容不能为空', {time: 2000});
                return false;
            }
            addcomment(objid,comment,1,1);
        });*/

        // 获取收藏的数量
        $.ajax({
            url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
            type: 'get',
            dataType:'jsonp',
            data:{action:'home.get.fa_ex_num',id:182322,type:1},
                success:function(data){
            $('#fa_num').text(data);
            }
        });
    // 是否收藏
    $.ajax({
        url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
        type: 'get',
        dataType:'jsonp',
        data:{action:'home.get.ifsc',id: 182322,},
        success:function(data){
            if(data.status == 200){
                $('#fa_active').addClass('active');
            }
        }
    });

    $.ajax({
        url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
        type: 'get',
        dataType:'jsonp',
        data:{action:'home.get.modellike',id:182322},
        success:function(data){
            if(data.tag == 1){
                $("#like_active").addClass('active');
            }
        }
    });

    $.ajax({
        url: 'http://data.justeasy.cn/service/cget.cgi?callback=?',
        type: 'get',
        dataType:'jsonp',
        data:{action:'home.get.likenum',id:182322,type:'1'},
        success:function(data){
            if(data.code == '1000'){
                if(data.like_num == 'no'){
                    $("#like_num").text('0');
                }else{
                    $("#like_num").text(data.like_num);
                }
            }
        }
    });
    $(".album-container ul li").on('mouseover',function(){

        var index =$(".album-container ul li").index(this);
        $(".album-container ul li").removeClass('active');
        $(".album-container ul li").eq(index).addClass('active');
        $("#container ul li").hide();
        $("#container ul li").eq(index).show();
        $("#container ul li").removeClass('big');
        $("#container ul li").eq(index).addClass('big');
    })
    
    $(".flex-prev").on('click',function(){
        var index=$(".big a").attr('rel');
        if(index == '0'){
            return false;
        }
        index= index -1
        $(".album-container ul li").removeClass('active');
        $(".album-container ul li").eq(index).addClass('active');
        $("#container ul li").hide();
        $("#container ul li").eq(index).show();
        $("#container ul li").removeClass('big');
        $("#container ul li").eq(index).addClass('big');
        
    })
    $(".flex-next").on('click',function(){
        var index=$(".big a").attr('rel');
        index = Number(index) + 1;
        if(!$("#container ul li").eq(index).find('a').attr('rel')){
            return false;
        }
        $(".album-container ul li").removeClass('active');
        $(".album-container ul li").eq(index).addClass('active');
        $("#container ul li").hide();
        $("#container ul li").eq(index).show();
        $("#container ul li").removeClass('big');
        $("#container ul li").eq(index).addClass('big');
    })
    
    $('.bigpic').fullsizable({
        detach_id: 'container'
    });
    $(document).on('fullsizable:opened', function(){
        $("#jquery-fullsizable").swipe({
          swipeLeft: function(){
            $(document).trigger('fullsizable:next')
          },
          swipeRight: function(){
            $(document).trigger('fullsizable:prev')
          },
          swipeUp: function(){
            $(document).trigger('fullsizable:close')
          }
        });
    });

    $('.icon-search-plus').click(function(){
        $('.bigpic').click();
    })

    })
