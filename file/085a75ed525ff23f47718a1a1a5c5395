<script language="javascript"> 
    function getCookie(name)
    {
    var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");
    if(arr=document.cookie.match(reg))
    return unescape(arr[2]);
    else
    return null;
    }

    function check(id)
    {


        var reg = /^((13[0-9])|(15[0-9])|(18[0-9])|(14[0-9]))+\d{8}$/ ;
        formobj=eval("document.form"+id+".mobile");
        if(formobj.value=="" || formobj.value=="请您输入手机号" || formobj.value=="手机号：" ||  formobj.value=="手机号")
        {
            alert("手机号不能为空！");
            formobj.focus();
            return false;
        }else if (!reg.test(formobj.value)){
            alert("手机号不正确！");
            formobj.focus();
            return false;
        }

        var data = $('#form'+id).serialize();

        data=data+"&commencookie="+getCookie("commencookie")+"&lastProductId="+pagerank;
data = decodeURIComponent(data, true);
data = encodeURI(encodeURI(data));
$.ajax({
    url: "http://www.edu24ol.com/subject/sendinfo_m.asp",
    data: data,
    dataType: "jsonp",
    jsonp:'callback',
    success: function (message) {
        if (message == "1") {

            alert("您的信息已提交成功！");
            $("txtname").val("");
            $("#mobile").val("");
        } else if(message == "2") {
            alert("您的信息未提交成功！");
        }else{
			alert("信息提交失败！");
		}
    }

})
}
</script>


