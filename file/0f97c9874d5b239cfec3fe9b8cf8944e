<script type="text/javascript">
$(document).ready(function(){
  var currentPosition_jx = 0;
  var slideWidth_jx = 130;
  var slides_jx = $('.slide_jx');
  var numberOfSlides_jx = slides_jx.length;


  // Remove scrollbar in JS
  $('#slidesContainer_jx').css('overflow', 'hidden');

  // Wrap all .slides_jx with #slideInner div
  slides_jx
    .wrapAll('<div id="slideInner"></div>')
    // Float left to display horizontally, readjust .slides_jx width
	.css({
      'float' : 'left',
      'width' : slideWidth_jx
    });

  // Set #slideInner width equal to total width of all slides_jx
  $('#slideInner').css('width', slideWidth_jx * numberOfSlides_jx);

  // Insert control_jxs in the DOM

  // Hide left arrow control_jx on first load
  manageControls(currentPosition_jx);

  // Create event listeners for .control_jxs clicks
  $('.control_jx')


    .bind('click', function(){
    // Determine new position
	if($(this).attr('id')=='rightControl_jx' && currentPosition_jx!=numberOfSlides_jx-4){
			currentPosition_jx = currentPosition_jx+1;
	}
	if($(this).attr('id')=='leftControl_jx' && currentPosition_jx!=0){

			currentPosition_jx = currentPosition_jx-1;
	}	


		
	//currentPosition_jx = ($(this).attr('id')=='rightControl_jx') ? currentPosition_jx+1 : currentPosition_jx-1;
	//{if(currentPosition_jx>=0)currentPosition_jx+1} : {if(currentPosition_jx<=numberOfSlides_jx-4)currentPosition_jx-1;}   
	// Hide / show control_jxs
    manageControls(currentPosition_jx);
    // Move slideInner using margin-left
    $('#slideInner').animate({
      'marginLeft' : slideWidth_jx*(-currentPosition_jx)
    });
  });

  // manageControls: Hides and Shows control_jxs depending on currentPosition_jx
  function manageControls(position){
     //Hide left arrow if position is first slide
	//if(position==0){ $('#leftControl_jx').hide() } else{ $('#leftControl_jx').show() }
	 //Hide right arrow if position is last slide
    //if(position==numberOfSlides_jx-4){ $('#rightControl_jx').hide() } else{ $('#rightControl_jx').show() }
  }	
});
</script>





