<script type="text/javascript">


        $().ready(function () {
var date=new Date();
var year = date.getFullYear();
var month = 1+date.getMonth();
if(parseInt(month)<10)month="0"+month;
var day = date.getDate();
var day_1=parseInt(date.getDate())-1;
if(parseInt(day )<10)day ="0"+day ;
if(parseInt(day_1)<10)day_1="0"+day_1;
var today = year+"-"+month+"-"+day;
var yesterday = year+"-"+month+"-"+day_1;
for(i=0;i<4;i++){
	var a=$("#new_"+i);
	if(a.attr("date")==today||a.attr("date")==yesterday){
		var html=a.html();
		html+='  <img src="/images_new/iconnew.png" width="22" height="14" />';
		a.html(html);
	}
	var b=$("#new_2_"+i);
	if(b.attr("date")==today||a.attr("date")==yesterday){
		var html=b.html();
		html+='  <img src="/images_new/iconnew.png" width="22" height="14" />';
		b.html(html);
	}
}
});
</script>
