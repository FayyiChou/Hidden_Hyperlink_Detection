<script type="text/javascript">
$(document).ready(function(){
  var currentPosition_jx1 = 0;
  var slideWidth_jx1 = 130;
  var slides_jx1 = $('.slide_jx1');
  var numberOfSlides_jx1 = slides_jx1.length;


  // Remove scrollbar in JS
  $('#slidesContainer_jx1').css('overflow', 'hidden');

  // Wrap all .slides_jx1 with #slideInner1 div
  slides_jx1
    .wrapAll('<div id="slideInner1"></div>')
    // Float left to display horizontally, readjust .slides_jx1 width
	.css({
      'float' : 'left',
      'width' : slideWidth_jx1
    });

  // Set #slideInner1 width equal to total width of all slides_jx1
  $('#slideInner1').css('width', slideWidth_jx1 * numberOfSlides_jx1);

  // Insert control_jx1s in the DOM

  // Hide left arrow control_jx1 on first load
  manageControls(currentPosition_jx1);

  // Create event listeners for .control_jx1s clicks
  $('.control_jx1')
    .bind('click', function(){
    // Determine new position
	if($(this).attr('id')=='rightControl_jx1' && currentPosition_jx1!=numberOfSlides_jx1-4){
			currentPosition_jx1 = currentPosition_jx1+1;
	}
	if($(this).attr('id')=='leftControl_jx1' && currentPosition_jx1!=0){

			currentPosition_jx1 = currentPosition_jx1-1;
	}	
		
	//currentPosition_jx1 = ($(this).attr('id')=='rightControl_jx1') ? currentPosition_jx1+1 : currentPosition_jx1-1;
	//{if(currentPosition_jx1>=0)currentPosition_jx1+1} : {if(currentPosition_jx1<=numberOfSlides_jx1-4)currentPosition_jx1-1;}   
	// Hide / show control_jx1s
    manageControls(currentPosition_jx1);
    // Move slideInner1 using margin-left
    $('#slideInner1').animate({
      'marginLeft' : slideWidth_jx1*(-currentPosition_jx1)
    });
  });

  // manageControls: Hides and Shows control_jx1s depending on currentPosition_jx1
  function manageControls(position){
     //Hide left arrow if position is first slide
	//if(position==0){ $('#leftControl_jx1').hide() } else{ $('#leftControl_jx1').show() }
	 //Hide right arrow if position is last slide
    //if(position==numberOfSlides_jx1-4){ $('#rightControl_jx1').hide() } else{ $('#rightControl_jx1').show() }
  }	
});
</script>





