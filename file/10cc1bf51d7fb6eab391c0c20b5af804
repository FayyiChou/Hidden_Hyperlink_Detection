<script type="text/javascript">
    $(function(){
        $(".Free_classes").live("click",function(){
            var $cypedd = $(this).attr("_i");
            $.post("/index.php/Home/Index/kcurl",{cypedd:$cypedd},function(data){
                $("body").append('<div class="TC_denglu_warp"></div><div class="ZjMyVideoPlay"><dl><a href="#" id="CloasMyPlay">关闭</a></dl><iframe src="'+data+'" id="video" width="700" height="400" frameborder="0" scrolling="no"></iframe></div>');
                $(".ZjMyVideoPlay").css("top",$("body").scrollTop() + $("html").scrollTop() + 100);//窗口定位
                $(".TC_denglu_warp").height($(document).height());
            });
            return false;
        });

        $("#CloasMyPlay").live("click",function(){
            $(".TC_denglu_warp").remove();
            $(".ZjMyVideoPlay").remove();
            return false;
        });
    });
</script>
