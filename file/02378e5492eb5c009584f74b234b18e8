    $(".n_next").click(function(){  
	     var parent = $(this).parent(".pf_list");
		 var n_show = parent.find("ul"); 
		 var n_width = parent.width() ;
		 var len = n_show.find("li").length;
		 var page_count = Math.ceil(len / i) ;  
		 if( !n_show.is(":animated") ){ 
			  if( page == page_count ){ 
				n_show.animate({ left : '0px'}, "slow"); 
				page = 1;
				}else{
				n_show.animate({ left : '-='+n_width }, "slow");  
				page++;
			 }
		 };
		 return false;
   });
    //往前 按钮
    $(".n_last").click(function(){
	     var parent = $(this).parents(".pf_list");
		 var n_show = parent.find("ul"); 
		 var n_width = parent.width();
		 var len = n_show.find("li").length;
		 var page_count = Math.ceil(len / i) ;
		 if( !n_show.is(":animated") ){   
		 	 if( page == 1 ){ 
				n_show.animate({ left : '-='+n_width*(page_count-1) }, "slow");
				page = page_count;
			}else{
				n_show.animate({ left : '+='+n_width }, "slow");
				page--;
			}
		}
		return false;
    });
});
