<script type="text/javascript">
function show(weinxin_div){
	var bWidth=parseInt(document.documentElement.scrollWidth); //当前窗口的宽度
	var bHeight=parseInt(document.documentElement.scrollHeight);
	var black = document.getElementById("full_div");
	black.style.width=bWidth+"px";
	black.style.height=bHeight+"px";
	document.getElementById(weinxin_div).style.display = "block";
}

function closed(weinxin_div){
	document.getElementById(weinxin_div).style.display = "none";
	var black = document.getElementById("full_div");
	black.style.width="0px";
	black.style.height="0px";
}
</script>
