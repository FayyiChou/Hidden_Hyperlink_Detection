<script type="text/javascript">
    $(document).ready(function ($) {
        //总首页左侧类目鼠标交互
        $('.j-navIn').mouseenter(function () {
            $(this).find('.j-navList').show().css({ "z-index": "100" });
        });
        $('.j-navIn').mouseleave(function () {
            $(this).find('.j-navList').hide();
        });

        $('.j-listImgOut').mouseenter(function () {
            $(this).find('.j-listImg').show();
        });
        $('.j-listImgOut').mouseleave(function () {
            $(this).find('li').removeClass("hover");
            $(this).find('.j-listImg').hide();
        });
        //鼠标浮到图片效果
        $(".j-serviceImgDiv").mouseenter(function () {
            $(this).find("i").show();
        });
        $(".j-serviceImgDiv").mouseleave(function () {
            $(this).find("i").hide();
        });
        $('#cat1').click(function () {
            $('html,body').animate({ scrollTop: $('#gdlcat1').offset().top }, 900);
        });
        $('#cat2').click(function () {
            $('html,body').animate({ scrollTop: $('#gdlcat2').offset().top - 15 }, 900);
        });
    });
</script>

