<script language="javascript" type="text/javascript">
	function browse(){
		var Sys = {};
		var ua = navigator.userAgent.toLowerCase();
        
		var s;
		(s = ua.match(/msie ([\d.]+)/)) ? Sys.ie = s[1] :
		(s = ua.match(/firefox\/([\d.]+)/)) ? Sys.firefox = s[1] :
		(s = ua.match(/chrome\/([\d.]+)/)) ? Sys.chrome = s[1] :
		(s = ua.match(/opera.([\d.]+)/)) ? Sys.opera = s[1] :
		(s = ua.match(/version\/([\d.]+).*safari/)) ? Sys.safari = s[1] : 0;
		if (Sys.ie) jumpUrl();
        
		//if (Sys.firefox) jumpUrl();
		//if (Sys.chrome) document.write('Chrome: ' + Sys.chrome);
		//if (Sys.opera) document.write('Opera: ' + Sys.opera);
		//if (Sys.safari) document.write('Safari: ' + Sys.safari);
        //jumpUrl();

	}
    function jumpUrl(){
        
        DocURL=document.URL;
        fileName = DocURL.split("//")[1].split("/");
        file = fileName[fileName.length-1].split(".")[0];
        if (file == "") file="index";
        if (file == "index")
        {
            location.href="/ie.html";
        }
        if (file == "hygl")
        {
            location.href="/hygl_ie.html";
        }
        
    }
    
   
    function checkForm(){
        var form=document.getElementsByName("mailname")[0];
        if(document.getElementsByName("imapuser")[0].value=="")
        {
            alert("请填入用户名！");
            document.getElementsByName("imapuser")[0].focus();
            return;
        }
        if( document.getElementsByName("pass")[0].value=="")
        {
            alert("请填入用户密码！");
            document.getElementsByName("pass")[0].focus();
            return;
        }
        form.submit();
    }
</script> 
