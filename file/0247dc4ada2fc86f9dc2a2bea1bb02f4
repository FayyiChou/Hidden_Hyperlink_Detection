<script type="text/javascript">
$(function() {
	var piaocWidth = $("#automv_2").css('width').replace('px', '');
	// 初始化飘窗
	$('#automv_1').autoMove({startL:0, startT:0, angle:Math.PI/4, speed:20});
	$('#automv_2').autoMove({startL:1366-piaocWidth, startT:0, angle:-4*Math.PI/3, speed:20});
	
});

//下载手册
function handbook(fileName) {
	var iframe = $("iframe#download");
	
	if(iframe.size() == 0) {
		iframe = elem('<iframe src="javascript:" class="hide" id="download"></iframe>').appendTo("body");
	}
	
	iframe.attr("src", "http://he.gsxt.gov.cn/notice/download/handbook?name=" + fileName);
}

function hrefUrl(url) {
	window.location.href = url;
}
</script>


<script type="text/javascript">
//修复 ie 9 以下 input 不支持 placeholder, 需要放在 input 标签之后才会生效
jQuery('[placeholder]').focus(function() {
    var input = jQuery(this);
    if (input.val() == input.attr('placeholder')) {
        input.val('');
        input.removeClass('placeholder');
    }
}).blur(function() {
    var input = jQuery(this);
    if (input.val() == '' || input.val() == input.attr('placeholder')) {
        input.addClass('placeholder');
        input.val(input.attr('placeholder'));
    }
}).blur().parents('form').submit(function() {
    jQuery(this).find('[placeholder]').each(function() {
        var input = jQuery(this);
        if (input.val() == input.attr('placeholder')) {
        	input.val('');
        }
    })
});

$(function() {
	// 初始化飘窗
	//$('.floatWinYzym').autoMove({angle:-Math.PI/4, speed:20});
	
	// 查询类别的选择
	$('#navSearch li').click(function() {
		var isCurrent = $(this).hasClass("current");
		if (!isCurrent) {
			$('.current').removeClass("current");
			$(this).addClass('current');
		}
	});
	
	$('#formInfo').on('keydown', function(e) {
		if (e.keyCode == 13) {
			queryFormInfo();
			return false;
		}
	});
	
	$('#buttonSearch').click(function() {
		queryFormInfo();
		//国家局记录访问日志 当用户搜索企业的时候，在用户点击搜索按钮时，页面需要执行该方法，并传入参数为搜索的关键词。该方法会将搜索关键词发送到访问情况收集接口。
		 if(constants.yes == "1"){
			 Collect.searchKeywordCollect_gsxt($("#keyword").val());
		 } 
	});
	
	// 地方频道自定义
	if(constants.locale_this == constants.locale_heb){
		$("#HB_DF").show();$("#ALL_DF").hide();
	}
	
	if('false' == 'true'){
		  $("#keyword").val('');
		}
});

function queryFormInfo() {
	// 不同类型的查询
	// 搜索类型：1-企业信用信息、2-经营异常名录、3-严重违法企业名单、4-抽查检查信息、5-信息公告
	// 确定查询的类型
	var type = $.trim($('#navSearch .current').text());
	if(constants.yes == constants.capthca_gettype){
		if ("企业信用信息" == type) {
			$("#searchType").val('1');
			qry.search('#keyword', '1', constants.yes, constants.yes,captchaObjHtml,constants.no);
			//qry.search('#keyword', '1', constants.yes, constants.yes,'',constants.yes,'buttonSearch');
		} else if ("经营异常名录" == type) {
			$("#searchType").val('2');
			qry.search('#keyword', '2', constants.yes, constants.yes,captchaObjHtml,constants.no);
			//qry.search('#keyword', '2', constants.yes,constants.yes,'',constants.yes,'buttonSearch');
		} else if ("严重违法失信企业名单" == type) {
			$("#searchType").val('3');
			qry.search('#keyword', '3', constants.yes, constants.yes,captchaObjHtml,constants.no);
			//qry.search('#keyword', '3', constants.yes,constants.yes,'',constants.yes,'buttonSearch');
		}
	}else if(constants.no == constants.capthca_gettype){
		if ("企业信用信息" == type) {
			$("#searchType").val('1');
			//qry.search('#keyword', '1', constants.yes, constants.yes,captchaObjHtml,constants.no);
			qry.search('#keyword', '1', constants.yes, constants.yes,'',constants.yes,'buttonSearch');
		} else if ("经营异常名录" == type) {
			$("#searchType").val('2');
			//qry.search('#keyword', '2', constants.yes, constants.yes,captchaObjHtml,constants.no);
			qry.search('#keyword', '2', constants.yes,constants.yes,'',constants.yes,'buttonSearch');
		} else if ("严重违法失信企业名单" == type) {
			$("#searchType").val('3');
			//qry.search('#keyword', '3', constants.yes, constants.yes,captchaObjHtml,constants.no);
			qry.search('#keyword', '3', constants.yes,constants.yes,'',constants.yes,'buttonSearch');
		}
	} else {
		if ("企业信用信息" == type) {
			$("#searchType").val('1');
			qry.search('#keyword', '1', constants.yes, '0');
		} else if ("经营异常名录" == type) {
			$("#searchType").val('2');
			qry.search('#keyword', '2', constants.yes, '0');
		} else if ("严重违法失信企业名单" == type) {
			$("#searchType").val('3');
			qry.search('#keyword', '3', constants.yes, '0');
		}
	}
}
</script>
