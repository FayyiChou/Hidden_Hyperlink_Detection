        <script>
            $('.p_item[data-href]').each(function(){
                var href = $(this).data('href');
                if(href && href.indexOf('tel:')!==-1){
                    $(this).attr('gather',JSON.stringify({
                        event:'touch_topic_call',
                        event_name:'topic专题电话拨打',
                        city:city,
                        level1_page:level1_page,
                        level2_page:level2_page,
                        param1:custom_id,
                        param2:news_source,
                        param3:href.replace('tel:','')
                    }));
                }
            });
            if(typeof gather != "undefined"){gather.event_item=["div"];gather.event();}
        </script>
