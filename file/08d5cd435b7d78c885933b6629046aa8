    <script type="text/javascript">
        $(function () {
            /* 等级 */
            $(".bar div").animate({width: "3%"}, 800);
            var lv_name = ["文坛新秀", "小有武艺", "出类拔萃", "技冠群雄", "超凡入圣", "登峰造极"];//等级名称
            var lv;//作者等级
            lv = "1";
            $("div.rank").css("background", "url(images_2/lv/LV" + lv + "_2.png) no-repeat right");
            $("div.rank .lv").find("img").attr("src", "images_2/lv/LV" + lv + "_1.png");
            $("div.rank .bar").find("div").css("background", "url(images_2/lv/LV" + lv + "_3.png) no-repeat right");
            $("div.rank ").attr("title", lv_name[lv - 1]);

            /* 评论表情 */
            $(".expression").qqFace({
                id: 'facebox',
                assign: 'saytext',
                path: 'images_2/qqFace/'//表情存放的路径
            });

            /* 搜索图书 */
            $("div.search .option").hover(function () {
                $(this).find(".option_list").slideDown(300);
            }, function () {
                $(this).find(".option_list").hide();
            });
            $(".option .option_list ul li").click(function () {
                var text = $(this).html();
                $("div.search .option").find("span").html(text);
                $("div.search .option").find("span").attr("val", $(this).attr("val"));
                $(".option_list").hide();
            });

            $("input[name='keyWords']").keydown(function (e) {
                if (e.keyCode == 13) {
                    findBook();
                }
            });

            var inputword = "输入您要找的书名或者作者";
            $("input[name='keyWords']").val(inputword).bind("focus", function () {
                if ($(this).val() == "输入您要找的书名或者作者") {
                    $(this).attr("class", "search_input");
                    $(this).val("");
                }
            }).bind("blur", function () {
                if ($(this).val() == "") {
                    $(this).attr("class", "search_input blur");
                    $(this).val("输入您要找的书名或者作者");
                }
            });
            $("#search").click(function () {
                findBook();
            });
            /* 搜索图书结束 */

            /* 新增书评 */
            $("#submitButton").click(function () {
                var baoqings = $("#saytext").val().match(/\[em_([0-9]*)\]/g);//表情
                if (baoqings != null) {
                    var baoqing_num = baoqings.length;//表情个数
                    if (baoqing_num > 20) {
                        Alert("表情个数超过20个!!!");
                        return false;
                    }
                }
                var userid = "";
                if (!userid) {
                    Alert("请登录后在评论!!!");
                    return false;
                }
                var bookid = $("div.form input[name='bookid']").val();
                var comtTitle = $("div.form div input[name='comtTitle']").val().replace(/\s/g, "");
                var comtText = $("div.form div textarea").val().replace(/\s/g, "");

                if (comtTitle == "请输入标题" || comtTitle.length < 1) {
                    Alert("请输入标题！");
                } else if (comtText == "请输入内容" || comtText.length < 1) {
                    Alert("请输入内容！");
                } else if (replace_em_2(comtText).length > 500) {
                    Alert("输入内容不能超过500个字！");
                } else {
                    //将图片替换成路径
                    comtText = replace_em(comtText);
                    $.post("muntual/add.htm", {
                        bookId: bookid,
                        comtTitle: comtTitle,
                        comtText: comtText,
                        csrf_token: "0cb2a7d4-51d6-449a-a3cf-842f6863c618"
                    }, function (data) {
                        $("div.Review h2 span.FR").html("共" + data.totalNum + "条评论");
                        var html = "";
                        for (var i = 0; i < data.result.length; i++) {
                            html += "<div class='Review_list'><div class='portrait'>" +
                                    "<a href='#'><img src= '" + data.result[i].namePath + "' alt='' width='50' height='50'></a></div>" +
                                    "<div class='con'> <div class='title_con'>" + data.result[i].comtTitle + "</div>" +
                                    "<p class='uname'><a>" + data.result[i].nickName + "</a>说：</p>" +
                                    "<div class='text'> " + data.result[i].comtText + " </div>" +
                                    "<p class='time'>" + data.result[i].strComtTime + "</p></div>" +
                                    "<div style='clear: both'></div></div>";
                        }
                        $("div.Review_con").html("");
                        $("div.Review_con").html(html);
                        Alert("评论成功！");
                        $("div.form div input[name='comtTitle']").attr("class", "title blur").val("请输入标题");
                        $("div.form div textarea").attr("class", "title blur").val("请输入内容");
                        $("#words_num").html(500);
                    }, "json");
                }
            });
            /* 新增书评结束 */
            $(".btn_list>ul>li").last().css("border-right", "none");
            $("div.search .option").hover(function () {
                $(this).find(".option_list").slideDown(300);
            }, function () {
                $(this).find(".option_list").hide();
            });
            $(".option .option_list ul li").click(function () {
                var text = $(this).html();
                $("div.search .option").find("span").html(text);
                $(".option_list").hide();
            });
            $(".ranking ul li div.book span.FR").click(function () {
                var $medal = $(this).parent().siblings("div.medal");
                if ($medal.is(":hidden")) {
                    $medal.slideDown(300);
                } else {
                    $medal.slideUp(300);
                }
            });

            var authorId = "14774649597902";
            var bookId = "14774649968266";
            //作者的其他作品
            $.post("book/otherBooksAjax.htm", {authorId: authorId}, function (data) {
                var html = "";
                if (data.result && data.result.length > 1) {
                    for (var i = 0; i < data.result.length; i++) {
                        if (bookId != data.result[i].bookId) {
                            html += "<li>" +
                                    "<a href='book/search.htm?booktypeId=" + data.result[i].booktypeId + "'>[" + data.result[i].nameType + "]</a>" +
                                    "<a href ='book/" + data.result[i].bookId + "/index.htm' title='" +
                                    data.result[i].bookName + "'><span>" + data.result[i].shortBookName10 +
                                    "</span></a></li>";
                        }
                    }
                } else {
                    $("div.other_book").html("");
                }
                $("div.other_book ul").html("");
                $("div.other_book ul").html(html);
            }, "json");
            //获取图书的打赏记录
            $.post("monthly/rewardLogAjax.htm", {bookId: bookId}, function (data) {
                var html = "";
                if (data.result) {
                    for (var i = 0; i < data.result.length; i++) {
                        if (data.result[i].rewardType == "m") {
                            html += "<li><a>" + data.result[i].nickname + "</a>打赏了" + data.result[i].reward_monthly + "张月票</li>";
                        } else if (data.result[i].rewardType == "j") {
                            html += "<li><a>" + data.result[i].nickname + "</a>打赏了" + data.result[i].reward_monthly + "金币</li>";
                        }
                    }
                }
                $("div.btn_list div ul").html(html);
                /* 滚动打赏滚动 */
                scroll_News();
            }, "json");
        });

        // 搜索
        function findBook() {
            var keyWords = $("input[name='keyWords']").val();
            if (!keyWords || keyWords == "输入您要找的书名或者作者") {
                Alert("请输入搜索词");
                return false;
            }
            var type = $("div.search .option").find("span").attr("val");
            $("input[name='type']").val(type);
            $("#form").submit();
        }

        // 滚动打赏滚动 
        function scroll_News() {
            var $this = $(".btn_list div.FL");
            var index = $(".btn_list div.FL ul li").index() + 1;
            var scrollTimer;
            $this.hover(function () {
                clearInterval(scrollTimer);
            }, function () {
                if (index > 2) {
                    scrollTimer = setInterval(function () {
                        scrollNews($this);
                    }, 2000);
                }
            }).trigger("mouseleave");
        }

        // 滚动
        function scrollNews(obj) {
            var $self = obj.find("ul:first");
            var lineHeight = $self.find("li:first").height(); //获取行高
            $self.animate({"marginTop": -lineHeight + "px"}, 600, function () {
                $self.css({marginTop: 0}).find("li:first").appendTo($self); //appendTo能直接移动元素
            });
        }

        // 打赏作者
        function goldshow() {
            Modal({
                name: 'gold',
                sureCb: function() {
                    var userid = '';
                    if (userid) {
                        var $modal = $('.modal.gold');
                        var rewardMsg = $modal.find('textarea[name="message"]').val();
                        var rewardMoney = $modal.find('input[name="rewardMoney"]:checked').val();
                        var bookid = $('div.form input[name="bookid"]').val();
                        if (rewardMsg) {
                            $.post('useraa/awardMoneyAjax.htm', {
                                bookid: bookid,
                                rewardMoney: rewardMoney,
                                rewardMsg: rewardMsg
                            }, function (data) {
                                if (data == '恭喜您,打赏成功') {
                                    // 获取图书的打赏记录
                                    $.post("monthly/rewardLogAjax.htm", {
                                        bookId: bookid
                                    }, function (data) {
                                        var html = '';
                                        if (data.result) {
                                            for (var i = 0; i < data.result.length; i++) {
                                                if (data.result[i].rewardType == 'm') {
                                                    html += '<li><a>' + data.result[i].nickname + '</a>打赏了' + data.result[i].reward_monthly + '张月票</li>';
                                                } else if (data.result[i].rewardType == "j") {
                                                    html += '<li><a>' + data.result[i].nickname + '</a>打赏了' + data.result[i].reward_monthly + '金币</li>';
                                                }
                                            }
                                        }
                                        $('div.btn_list div ul').html(html);

                                        // 滚动打赏滚动
                                        scroll_News();
                                    }, 'json');
                                    return true;
                                }
                                Alert(data);
                                return false;
                            });
                        } else {
                            Alert('说点鼓励作者的话吧!!!');
                            return false;
                        }
                    } else {
                        Alert('您还未登录,请登录后在进行打赏金币...');
                        return false;
                    }
                }
            });

            var $modal = $('.modal.gold');
            $modal.find('input[name="rewardMoney"]').eq(0).prop('checked', 'checked');
            $modal.find('textarea[name="message"]').val('这本书实在是太好了,我决定打赏作品支持下!!!');

            // 获取金币
            $.post('account/userBalanceAjax.htm', function (data) {
                $('#gold_money').html('您还剩余' + data[0].money + '金币');
            }, 'json');
        }

        // 打赏月票
        function monthlyshow() {
            Modal({
                name: 'monthly',
                sureCb: function() {
                    var userid = '';
                    if (userid) {
                        var $modal = $('.modal.monthly');
                        var rewardMsg = $modal.find('textarea[name="message"]').val();
                        var rewardMonthly = $modal.find('input[name="rewardMonthly"]:checked').val();
                        var bookid = $('div.form input[name="bookid"]').val();
                        if (rewardMsg) {
                            $.post('useraa/awardMonthlyAjax.htm', {
                                bookid: bookid,
                                rewardMonthly: rewardMonthly,
                                rewardMsg: rewardMsg
                            }, function (data) {
                                if (data == '恭喜您,打赏成功') {
                                    // 获取图书的打赏记录
                                    $.post('monthly/rewardLogAjax.htm', {
                                        bookId: bookid
                                    }, function (data) {
                                        var html = '';
                                        if (data.result) {
                                            for (var i = 0; i < data.result.length; i++) {
                                                if (data.result[i].rewardType == "m") {
                                                    html += "<li><a>" + data.result[i].nickname + "</a>打赏了" + data.result[i].reward_monthly + "张月票</li>";
                                                } else if (data.result[i].rewardType == "j") {
                                                    html += "<li><a>" + data.result[i].nickname + "</a>打赏了" + data.result[i].reward_monthly + "金币</li>";
                                                }
                                            }
                                        }
                                        $('div.btn_list div ul').html(html);

                                        // 滚动打赏滚动
                                        scroll_News();
                                    }, 'json');
                                    return true;
                                }
                                Alert(data);
                                return false;
                            });
                        } else {
                            Alert('说点鼓励作者的话吧!!!');
                            return false;
                        }
                    } else {
                        Alert('您还未登录,请登录后在进行打赏月票...');
                        return false;
                    }
                }
            });

            var $modal = $('.modal.monthly');
            $modal.find('input[name="rewardMonthly"]').eq(0).prop('checked', 'checked');
            $modal.find('textarea[name="message"]').val('这本书实在是太好了,我决定投票作品支持下!!!');

            // 获取月票
            $.post('account/userBalanceAjax.htm', function (data) {
                $('#gold_nummonth').html('您还剩余月票' + data[0].monthly + '张');
            }, 'json');
        }

        // 加入书架
        function toRack() {
            $.get('/wenxue/rack/14774649968266.htm?dd=' + new Date(), function (data) {
                if (data == '您还没登录') {
                    Alert(data);
                } else if (data == '您书架已有此书，请到书架查看') {
                    Confirm({
                        content: '您书架已有此书，确定前往书架查看？',
                        sureCb: function() {
                            window.location.href = '/wenxue/user/list.htm';
                        }
                    });
                } else {
                    Alert(data);
                }
            }, 'json');
        }

        // 将图片替换成路径
        function replace_em(str) {
            str = str.replace(/\</g, '&lt;');
            str = str.replace(/\>/g, '&gt;');
            str = str.replace(/\n/g, '<br/>');
            str = str.replace(/\[em_([0-9]*)\]/g, '<img src="images_2/qqFace/$1.gif" border="0" />');
            return str;
        }

        // 内容字数判断
        function textdown() {
            var text = $("#saytext");
            var text_val = replace_em_2(text.val());
            if (text_val.length > 500) {
                $("#words_num").html("0(超出" + (text_val.length - 500) + "个字）");
            } else {
                $("#words_num").html(500 - text_val.length);
            }
        }

        // 过滤表情
        function replace_em_2(str) {
            str = str.replace(/\</g, '&lt;');
            str = str.replace(/\>/g, '&gt;');
            str = str.replace(/\n/g, '<br/>');
            str = str.replace(/\s*/g, "");
            str = str.replace(/\[em_([0-9]*)\]/g, '');
            return str;
        }

        
    </script>
