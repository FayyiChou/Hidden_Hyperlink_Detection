<script type="text/javascript">

    var viewport;
    var breakPointWidth = 980;

    $(document).ready(function () {

        var previousWidth = $.getSpikesViewPort().width;
        var checkWidth = function () {
            viewport = $.getSpikesViewPort();
            if ((viewport.width >= breakPointWidth && previousWidth <= breakPointWidth) || (viewport.width <= breakPointWidth && previousWidth >= breakPointWidth) && (viewport.width !== previousWidth)) {
                previousWidth = viewport.width;

                prepareTopMenu();
            }
        };

        checkWidth();
        prepareTopMenu();

        $.addSpikesWindowEvent("resize", checkWidth);
        $.addSpikesWindowEvent("orientationchange", checkWidth);
        setInterval(checkWidth, 5000);
    });

    function prepareTopMenu() {

        if (viewport.width > breakPointWidth) {
            $(".plus-button").off("click");
            $(".sublist.active").removeClass("active");
            $(".plus-button.close").removeClass("close");

            $('.top-menu li').on('mouseenter', function () {
                $('a', $(this)).first().addClass('hover');
                $('.sublist', $(this)).first().addClass('active');
            }).on('mouseleave', function () {
                $('a', $(this)).first().removeClass('hover');
                $('.sublist', $(this)).first().removeClass('active');
            });
        }
        else {
            $('.top-menu li').off('mouseleave mouseenter');
            $(".sublist.active").removeClass("active");
            $(".plus-button.close").removeClass("close");

            $(".plus-button").on("click", function () {
                if ($(this).hasClass("close")) {
                    $(this).removeClass("close");
                }
                else {
                    $(this).addClass("close");
                }
                var sublist = $(this).siblings(".sublist");
                if ($(sublist).hasClass("active")) {
                    $(sublist).removeClass("active");
                }
                else {
                    $(sublist).addClass("active");
                }
            });
        }
    }
</script>
