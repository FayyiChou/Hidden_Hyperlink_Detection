//返回顶部
(function($) {
  $(function() {
    $(".backtotop").hide();
  });

  $(window).scroll(function() {
    if ($(window).scrollTop() > 100) {
      $(".backtotop").fadeIn('normal');
    } else {
      $(".backtotop").fadeOut('normal');
    }
  });

  $(".backtotop").click(function() {
    $('body, html').animate({scrollTop: 0});
    return false;
  });

  $(".search_box .user").hover(function() {
    $('.search_box .user_list').fadeIn(500);
    return false;
  },function(){
    $('.search_box .user_list').fadeOut(500);
    return false;
  });

})($);
</script> 
