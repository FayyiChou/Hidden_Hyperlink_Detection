<script language="javascript">
function checksearch() {
if ($('#formsearch input[name="keyword"]').val() == '' || $('#formsearch input[name="keyword"]').val() == '请输入搜索关键字') {
alert('请填写搜索关键字');
$('#formsearch input[name="keyword"]').focus();
return false;
}
return true;
}
$(function () {
var dir = '';
switch (dir) {
case 'buy': { $('.searchtag li.current').removeClass('current'); $('.searchtag li:eq(1)').addClass('current'); $('#formsearch').attr('action', $('.searchtag li:eq(1) a').attr('rel')); break; }
case 'company': { $('.searchtag li.current').removeClass('current'); $('.searchtag li:eq(3)').addClass('current'); $('#formsearch').attr('action', $('.searchtag li:eq(3) a').attr('rel')); break; }
case 'invite': { $('.searchtag li.current').removeClass('current'); $('.searchtag li:eq(4)').addClass('current'); $('#formsearch').attr('action', $('.searchtag li:eq(4) a').attr('rel')); break; }
}
$('.searchtag li.current').next().css({ 'background-position': '-1px -40px' });
$('.searchtag li').click(function () {
if ($(this).attr('class') == 'current') {
return;
}
$('#formsearch').attr('action', $(this).find('a').attr('rel'));
var nt = $('.searchtag li.current').next();
if (nt) {
nt.css({ 'background-position': '0px -40px' });
}
$('.searchtag li.current').removeClass('current').css({ 'background-position': '0px -40px' });
$(this).addClass('current').css({ 'background-position': '0px -90px' });
var next = $(this).next();
if (next) { next.css({ 'background-position': '-1px -40px' }); }
});
$('#formlogin').ajaxForm({
dataType: 'json',
success: function (json, statusText, xhr, $form) {
if (json.error) {
alert(json.msg);
$('#formlogin :text:first').focus();
} else {
$.get('/login.aspx?html=1', function (rs) {
$('.toplogin').html(rs);
});
}
}
});
});
function adclick(obj, ad) { }
</script>
