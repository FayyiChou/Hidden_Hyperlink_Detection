var sps;
var htmlCache = {};
function builderHtml(letter) {
var html;
if (html = htmlCache[letter]) {
return html;
}
html = '';
var cArray = cs[letter];
if (cArray && cArray.length > 0) {
for (var i = 0; i < cArray.length; i++) {
var c = cArray[i];
if(c.length>0){
html += '<a target="_blank" href="/sell/' + c[0]+'/" target="_blank">' + c[1] + '</a> ';
}
}
} else {
html = '没有以字母<b>' + letter + '</b> 开头的类目!';
}
htmlCache[letter] = html;
return html;
}
function initLetterFilter() {
var lp = document.getElementById('LetterPanel');
sps = lp.getElementsByTagName('span');
for (var i = 0; i < sps.length; i++) {
var sp = sps[i];
sp.onmouseover = letterOver;
sp.onmouseout = letterOut;
}
}
var contentPanel = document.getElementById('SpellCategoryList');
function letterOver() {
var src = this;
src.className = 'LetterSelected';
contentPanel.innerHTML = builderHtml(src.innerHTML.toLowerCase());
contentPanel.style.display = 'block';
contentPanel.onmouseover = function () {
src.className = 'LetterSelected';
contentPanel.style.display = 'block';
}
contentPanel.onmouseout = function () {
src.className = 'LetterNormal';
contentPanel.style.display = 'none';
}
}
function letterOut() {
this.className = 'LetterNormal';
contentPanel.style.display = 'none';
}
initLetterFilter()
</script> 
