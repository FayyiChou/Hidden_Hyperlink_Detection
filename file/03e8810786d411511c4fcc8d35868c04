        <script type="text/javascript">
						var photoPrev = $$.get("photoPrev"),
								photoNext = $$.get("photoNext"),
								popTxt=$$.get("poptxt"),
								close=$$.get("close"),
								pop=$$.get("pop"),
								popClose=$$.get("popclose"),
								shortname = ".html",
								npage = 1,
								totalpage = 7,
								displaytype = 'st',
								namehand = '755926';
						//弹框
						function popShow(status,msg){
							switch(status){
								case "show":
									pop.className="";
									popTxt.innerHTML=msg;
									break;
								case "hide":
									pop.className="hidden";
									popTxt.innerHTML=msg;
									break;
							}
						}
						//上一张
						$$.addEvent(photoPrev, function(){
							popShow("hide","");
							if(npage<2)
							{
								popShow("show","已经是第一张了！");
								return;
							}
							else
							{
								if(npage==2) {
									if(namehand!='') location.href = namehand+shortname;
									else location.href = "view.php?aid={dede:field name='id'/}";
								} else if(displaytype=='st' && namehand!='') {
									location.href = namehand+"_"+(npage-1)+shortname;
								} else {
									location.href = "view.php?aid={dede:field name='id'/}&pageno="+(npage-1);
								}
							}
						},"click");
						//下一张
						$$.addEvent(photoNext, function(){
							popShow("hide","");
							if(npage==totalpage) {
								popShow("show","已经是最后一张了！");
								return;
							}
							else
							{
								if(displaytype=='st' && namehand!=''){
									location.href = namehand+"_"+(npage+1)+shortname;
								}
								else {
									location.href = "view.php?aid={dede:field name='id'/}&pageno="+(npage+1);
								}
							}
						},"click");
						//重新预览
						$$.addEvent(popClose,function(){
							popShow("hide","");
							location.href = namehand+shortname;
						},"click");
						//关闭
						$$.addEvent(close, function(){
							$$.get("pop").className="hidden";
						},"click");
					</script>
      </div>
    </div>
