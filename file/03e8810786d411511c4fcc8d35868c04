<script type="text/javascript">
//选项卡
function tabShow(t,d,name){
	var oTab=$(t+'>li');
	var oItem=$(d+'>div');
	oTab.bind('mouseover',function(){
		$(this).addClass(name).siblings().removeClass(name);
		oItem.eq(oTab.index(this)).removeClass('hidden').siblings().addClass('hidden');
	});
}
tabShow('#tabs1','#conts1','current');
//轮回展示
function showTurn(id){
	var oItem=$(id+'>li');
	oItem.bind('mouseover',function(){
		$(this).children('.himg').removeClass('hidden').siblings().addClass('hidden').end().end().siblings().children('.himg').addClass('hidden').siblings().removeClass('hidden');
	});
}
showTurn('#sort_a');
</script>
