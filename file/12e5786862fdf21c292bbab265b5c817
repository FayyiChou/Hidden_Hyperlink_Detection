function showMen1(n){	
	for(var i=1;i<=3;i++){
		var curCon=document.getElementById("Image"+(i+3));//按钮
		var curBon=document.getElementById("picqh"+(i+3));//盒子内容
		if(n==i){
			curBon.style.display="block";
			curCon.src="../../images/qzlx_bar"+i+"1.jpg";
			
		}else{
			curBon.style.display="none";
			curCon.src="../../images/qzlx_bar"+i+".jpg";	
				
		}
	}
}
$('.con3').each(function(index, element) {
    $(this).html($(this).html().replace(/<\/?[^>]*>/img,''));//过滤换行
});
