<script language="javascript">
nag1003XCDJIndex12691274 = window.navigator.appName;
var flog1003XCDJIndex12691274 = 0;
var tn1003XCDJIndex12691274 = 1;
var tims1003XCDJIndex12691274;
function ClickTab1003XCDJIndex12691274(n)
{
	flog1003XCDJIndex12691274 = 1;
	window.clearInterval(tims1003XCDJIndex12691274);
	ShowTab1003XCDJIndex12691274(n);
}

function ShowTab1003XCDJIndex12691274(n)
{
 if(nag1003XCDJIndex12691274=="Netscape")
 {
   n = parseInt(n);
   for(i=1;i < 4;i++)
   {
	//window['1003XCDJIndex12691274'].filters.revealTrans.apply();
	 if(n==i)
	 {
			MM_showHideLayers('1003XCDJIndex12691274'+i,'','show');
	 }
	 else
	 {
			MM_showHideLayers('1003XCDJIndex12691274'+i,'','hide');
	 }
	 //window['1003XCDJIndex12691274'].filters.revealTrans.play()
   }
 }
 else
 {
  
	window['1003XCDJIndex12691274'].filters.revealTrans.apply();
   for(i=1;i < 4;i++)
    {
     if (i==n)
 	 {
			MM_showHideLayers('1003XCDJIndex12691274'+i,'','show');
	 }
	 else
	 {
			MM_showHideLayers('1003XCDJIndex12691274'+i,'','hide');
	 }
    }
	 window['1003XCDJIndex12691274'].filters.revealTrans.play()

 }
 
}
function run1003XCDJIndex12691274()
{
	if(flog1003XCDJIndex12691274==0)
	{
		ShowTab1003XCDJIndex12691274(tn1003XCDJIndex12691274);
	}
	tn1003XCDJIndex12691274 ++;
	if(tn1003XCDJIndex12691274 > 4 ) 
	{
		tn1003XCDJIndex12691274 = 1;
		ShowTab1003XCDJIndex12691274(tn1003XCDJIndex12691274)
		//window.clearInterval(tims);
		//window.setTimeout("run()",2000);
	}
}
