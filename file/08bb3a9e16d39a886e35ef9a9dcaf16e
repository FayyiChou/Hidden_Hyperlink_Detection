{
	flog1003XCDJIndex1317 = 1;
	window.clearInterval(tims1003XCDJIndex1317);
	ShowTab1003XCDJIndex1317(n);
}

function ShowTab1003XCDJIndex1317(n)
{
 if(nag1003XCDJIndex1317=="Netscape")
 {
   n = parseInt(n);
   for(i=1;i < 6;i++)
   {
	//window['1003XCDJIndex1317'].filters.revealTrans.apply();
