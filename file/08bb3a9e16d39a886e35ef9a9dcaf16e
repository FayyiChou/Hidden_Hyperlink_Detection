	 {
			MM_showHideLayers('1003XCDJIndex1317'+i,'','show');
	 }
	 else
	 {
			MM_showHideLayers('1003XCDJIndex1317'+i,'','hide');
	 }
	 //window['1003XCDJIndex1317'].filters.revealTrans.play()
   }
 }
