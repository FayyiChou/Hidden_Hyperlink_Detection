<script language="javascript">
nag1003XCDJIndex1308 = window.navigator.appName;
var flog1003XCDJIndex1308 = 0;
var tn1003XCDJIndex1308 = 1;
var tims1003XCDJIndex1308;
function ClickTab1003XCDJIndex1308(n)
{
	flog1003XCDJIndex1308 = 1;
	window.clearInterval(tims1003XCDJIndex1308);
	ShowTab1003XCDJIndex1308(n);
}

function ShowTab1003XCDJIndex1308(n)
{
 if(nag1003XCDJIndex1308=="Netscape")
 {
   n = parseInt(n);
   for(i=1;i < 3;i++)
   {
	//window['1003XCDJIndex1308'].filters.revealTrans.apply();
	 if(n==i)
	 {
			MM_showHideLayers('1003XCDJIndex1308'+i,'','show');
	 }
	 else
	 {
			MM_showHideLayers('1003XCDJIndex1308'+i,'','hide');
	 }
	 //window['1003XCDJIndex1308'].filters.revealTrans.play()
   }
 }
 else
 {
  
	window['1003XCDJIndex1308'].filters.revealTrans.apply();
   for(i=1;i < 3;i++)
    {
     if (i==n)
 	 {
			MM_showHideLayers('1003XCDJIndex1308'+i,'','show');
	 }
	 else
	 {
			MM_showHideLayers('1003XCDJIndex1308'+i,'','hide');
	 }
    }
	 window['1003XCDJIndex1308'].filters.revealTrans.play()

 }
 
}
function run1003XCDJIndex1308()
{
	if(flog1003XCDJIndex1308==0)
	{
		ShowTab1003XCDJIndex1308(tn1003XCDJIndex1308);
	}
	tn1003XCDJIndex1308 ++;
	if(tn1003XCDJIndex1308 > 3 ) 
	{
		tn1003XCDJIndex1308 = 1;
		ShowTab1003XCDJIndex1308(tn1003XCDJIndex1308)
		//window.clearInterval(tims);
		//window.setTimeout("run()",2000);
	}
}
function startrun1003XCDJIndex1308()
{
	tims = window.setInterval("run1003XCDJIndex1308()",4000);
}
