if(!showad_er){return;}
if (window.screen.width<MinScreenW_er){
   alert("临时提示：\n\n显示器分辨率宽度小于"+MinScreenW_er+",不显示广告");
   showad_er = false;
   document.getElementById("Javascript.LeftDiv_er").style.display="none";
