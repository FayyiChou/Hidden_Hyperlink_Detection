<script type="text/javascript">
$(document).ready(function(){
	$("#img1").mouseover(
		function(){
			$("#quweizhiquanmulu").hide();
			$("#bumenzhiquanmulu").hide();
			$("#quweizhiquanmulu").slideDown(3000);
			
		}
	);
	$("#img2").mouseover(
		function(){
			$("#bumenzhiquanmulu").hide();
			$("#quweizhiquanmulu").hide();
			$("#bumenzhiquanmulu").slideDown(3000);
			
		}
	);
	$("#laywerall").mouseleave(function(){
		$("#bumenzhiquanmulu").hide();
		$("#quweizhiquanmulu").hide();
	});
});

	function showImg1(){
		document.getElementById('img1').src = '/sys/mdl/tanqi/image/anniu.jpg';
		hideImg2();
	}
	
	function hideImg1(){
		document.getElementById('img1').src = '/sys/mdl/tanqi/image/anniu1.jpg';
	}
	
	function showImg2(){
		document.getElementById('img2').src = '/sys/mdl/tanqi/image/anniu4.jpg';
		hideImg1();
	}
	
	function hideImg2(){
		document.getElementById('img2').src = '/sys/mdl/tanqi/image/anniu3.jpg';
	}	
