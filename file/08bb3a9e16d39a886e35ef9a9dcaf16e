<SCRIPT LANGUAGE="JavaScript">
	document.write('<div id="jobads" style="height:149px;left:178px;position:absolute;top:1237px;width:170px; z-index:1000">');
	document.write('<a href="http://1.93.2.174:8001/" target="_blank" onmouseover=stopme("jobads"); onmouseout=movechip("jobads");>');
	document.write('<img src="/file/105N/105O/52802314.jpg" border="0"></a>');
	
