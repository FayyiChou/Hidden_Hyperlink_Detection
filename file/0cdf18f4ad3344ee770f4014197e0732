  $(document).ready(function () {
function changeDisplay(ele){  
                                     var parent=ele.parent();
                                     if(parent.length == 0 || !parent.get(0)||parent.get(0).tagName=='TD'){ return; }                                        
                                     if(parent.length>0 && parent.get(0).tagName && parent.get(0).tagName=='DIV' && parent.css('display')=='table'){                                       
                                            parent.css('display','block');
                                     }
                                     changeDisplay(parent);
                                 }  var ele = $('#mynetcnmarquee_d2-1479-2a268');  
  changeDisplay(ele); 
