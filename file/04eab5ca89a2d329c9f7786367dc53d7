<a name="top"></a>
<script>
    if ($(window).width() > 1000 + 96) {
        $(document.body).prepend('<a id="returnTop" href="#top">回到顶部</a> ');
        $('#returnTop').css({ 'right': (($(window).width()-1000)/2-96) + 'px' })

        $(window).scroll(function () {
            if ($(document).scrollTop() > 300)
                $('#returnTop').fadeIn();
            else
                $('#returnTop').fadeOut();
            var bottom = $(document).height() - $(document).scrollTop() - $(window).height();
            if (bottom < 210){
                //$('#returnTop').css({ 'bottom': (210 - bottom) + 'px' });
				$('#returnTop').css({ 'position': 'absolute','top':($(document).height()-210-23)+'px' });
			}
            else{
                $('#returnTop').css({ 'top':'auto','bottom': '0px','position': 'fixed','_position': 'absolute' });
			}
        })
    }
</script>
