<script>
var isup=true;
$('.zuixin .down').click(function(){
	clearInterval(marqueeInterval[0])
	if(isup)
		marqueeId--;
	isup=false;
	startMarquee_1();
	return false;
})
$('.zuixin .up').click(function(){
	clearInterval(marqueeInterval[0])
	if(!isup)
		marqueeId++;
	isup=true;
	startMarquee();
	return false;
})
</script>
