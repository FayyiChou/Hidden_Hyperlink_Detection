	function uploadPic(){
		if($("#pic").val()){
			$("#content").click();
			$("#uploadImage").submit();
		}
	}
	
	//图片上传回调
	function appSendMsg(msg){
		if(msg.indexOf("[pscrn]")>-1 || msg.indexOf("[audio]")>-1){
			$("#content").val(msg);
			$("#sendbtn").click();
		}else{
			if(msg){
				alert(msg);
			}
		}
	}
	
	var cont = document.getElementById('content');
	cont.addEventListener('input', function(event) {
		var val = cont.value;
		if($.trim(val)){
			$("#uploadImage").hide();
			$("#sendbtn").show();
		}else{
			$("#uploadImage").show();
			$("#sendbtn").hide();
		}
	});
	
	$("#sendbtn").click(function(){
		$("#uploadImage").show();
		$("#sendbtn").hide();
	});
