        var _as = doc.createElement('script');
        _as.type = 'text/javascript';
        _as.async = true;
        _as.src = 'http://x.autoimg.cn/bi/dealer/ahas_body.20160808.min.js?d=' + (new Date()).toLocaleDateString().replace(/\//g, "");
