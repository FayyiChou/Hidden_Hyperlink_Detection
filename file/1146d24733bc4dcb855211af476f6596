        <script type="text/javascript">
            var completeOrder={
                recomendCarList:function(subdata){
                    jQuery.ajax({url:"/Ajax/GetAttentionSpec?seriesId="+subdata.SeriesId+"&dealerId="+subdata.DealerId+"&specId="+subdata.SpecId ,
                        cache:false,
                        success:function(data){
                            if (data && data.length > 0) {
                                var seriesName=jQuery("#SeriesName").val()||jQuery("#loadSeriesTittle"+subdata.Index).text();

                                var result='<p>关注<span class="font-bold">&nbsp;'+seriesName+'&nbsp;</span>的用户还关注：</p>';
                                result+='<ul class="recom-ul fn-clear">';
                                var template=' <li>'+
                                                '<p class="pic"><a href="#specUrl" target="_blank"><img src="#imageUrl" height="90"></a></p>'+
                                                '<p class="text"><a href="#specUrl" target="_blank">#specName</a></p>'+
                                              '</li>';
                                for(var i=0,n=data.length;i<n;i++){
                                    result+=template.replace(/#imageUrl/ig,data[i].ImageUrl+"?pvareaid=105544")
                                                    .replace("#specName",data[i].SeriesName+" "+data[i].SpecName)
                                                    .replace(/#specUrl/ig,"/"+ subdata.DealerId +"/spec_"+data[i].SpecId+".html?pvareaid=105544");
                                }
                                result+="</ul>";
                                jQuery("#attentionSpec").html(result);　
                            }
                        }
                    }
                    );
                },
                callBack:function (data){
                    $$("#orderInfo li").click(function(){
                        jQuery("#divResultSuccess").hide();
                        $$(jQuery(this).find("a").attr("data-target")).addClass("current");
                    });

                    this.recomendCarList(data);
                    jQuery("#divResultSuccess").show();
                }
            }
        </script>

