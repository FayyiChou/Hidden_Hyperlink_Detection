                                validate.refushImageDealer();
                            }
                        },
                        success: function (data1) {
                            if(validate && validate.setCookie){
                                validate.setCookie("autoBindUsername",data.UserName,null,"autohome.com.cn");
                                validate.setCookie("autoBindPhone",data.Phone,null,"autohome.com.cn");
                            }
                            (function(){
                                jQuery("#btnOrder" + index).show();
                                jQuery("#btnDoingOrder" + index).hide();
                            })();
                            if (data1.success == 1) {
                                (function(){
                                    jQuery('#tab-'+ index).removeClass('current');
                                    jQuery("#divResultSuccess p:lt(3)").each(function(i,p){
                                        if(data.OrderType==1){
                                            if(i==0){
                                                jQuery(this).html("您的询价信息已经成功提交，我们将及时联系您进行报价！");
                                            }else if(i==1){
                                                jQuery(this).html("询价信息已提交给内江市金达顺斯柯达4S店 ，如果该商家未及时与您联系，我们将安排其他经销商为您报价。");
                                            }else{
                                                jQuery(this).find("a").html("预约试驾").click(function(){
                                                    jQuery("#orderInfo").show();
                                                    jQuery("#divResultSuccess" + (data.OrderType == 3 ? "3" : "")).hide();
                                                    $$("#targetTab2").click();
                                                    jQuery("#successTipes").hide();
                                                });
                                            }
                                        }else if(data.OrderType==2){
                                            if(i==0){
                                                jQuery(this).html("您的试驾申请已成功提交，我们将及时与您联系！");
                                            }else if(i==1){
                                                jQuery(this).html("试驾申请已提交给内江市金达顺斯柯达4S店 ，如果该商家未及时与您联系，我们将安排其他经销商为您安排试驾。");
                                            }else{
                                                jQuery(this).find("a").html("询问底价").click(function(){
                                                    jQuery("#orderInfo").show();
                                                    jQuery("#divResultSuccess" + (data.OrderType == 3 ? "3" : "")).hide();
                                                    $$("#targetTab1").click();
                                                    jQuery("#successTipes").hide();
                                                });
                                            }
                                        }
                                    });
                                })();
                                completeOrder.callBack(data);
                                if(isShowCode || isShowCodeImg)
                                {
                                    oatmo.codesucc();
                                }

                            } else if(data1.success=="0") {

                            }else  if (data1.success == "-6") {
                                if (isShowCodeImg) {
                                    jQuery("#imgValidCodeDealerError").text("验证码不存在或已过期，请重新获取！").removeClass("fn-hide");
                                } else {
                                    jQuery("#confimCode1").html("验证码不存在或已过期，请重新获取！");
                                    jQuery("#confimCodeRow1").show();
                                }
                                oatmo.codefail();
                                //jQuery("#saveError" + index).text("验证码不存在或已过期，请重新获取！").removeClass("fn-hide");
                            } else if (data1.success == "-7" || data1.success == "-11") {
                                if (isShowCodeImg) {
                                    jQuery("#imgValidCodeDealerError").text("验证码输入错误！").removeClass("fn-hide");
                                } else {
