                                    jQuery("#confimCodeRow1").show();
                                }
                                oatmo.codefail();

                                validate.init_code();
                                //jQuery("#saveError" + index).text("验证码输入错误！").removeClass("fn-hide");
                            }
                        }
                    });
                });
                jQuery("#chkZH").click(function(){
                    this.checked ? jQuery('#trZHContent').show():jQuery('#trZHContent').hide();
                });
