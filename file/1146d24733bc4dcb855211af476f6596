        <script type="text/javascript">

            jQuery(function(){
                var loadSeriesSpec=function(option){
                    this.options=option;
                };
                loadSeriesSpec.prototype={
                    init:function(){
                        this.loadSpec();
                    },
                    loadSpec:function(){
                        var arr=this.options[0].ids;
                        var callback=this.options[0].onchange;
                        jQuery("#" +arr[2] + " a").unbind("click").bind("click", function () {
                            var tittle = jQuery("#" + arr[1]);

                            var oldpId = tittle.attr("data-spid");
                            var newpId = jQuery(this).attr("data-spid");
                            tittle.text(jQuery(this).text()).attr("data-spid", newpId);
                            jQuery("#" + arr[2]).parent().hide();

                            if (oldpId != newpId && callback) {
                                callback(tittle.attr("id"),newpId);
                            }
                        });
                    }
                }
                for (var i = 1; i < 4; i++) {
                    new loadSeriesSpec([{ ids: ["loadSpec" + i, "loadSpecTittle" + i, "loadSpecList" + i],onchange:function(id,specid){
                        var index=id.replace("loadSpecTittle","");
                        var seric=jQuery("#"+id);
                        for (var n = 1; n < 4; n++) {
                            if(index!=n){
                                jQuery("#loadSpecTittle"+n).text(seric.text()).attr("data-spid",seric.attr("data-spid"));
                            }
                        }
                        if(specid>0){
                            jQuery("#ordeSpecMsg"+index).hide();
                        }else {
                            jQuery("#ordeSpecMsg"+index).show();
                        }
                    }
                    }] ).init();
                }
            });
        </script>
        <input type="hidden" id="SeriesId" value="519">
        <input type="hidden" id="SeriesName" value="明锐">

        <script type="text/javascript">

            function changeArea(id) {

                var index=1;
                jQuery("#orderInfo .tabcont-title ul li").each(function(i,ele){
                    if(jQuery(ele).hasClass("current")){
                        index=i+1;
                        return true;
                    }
                });
                var selectProvinceValue =parseInt(jQuery("#provinceTittle" + index).attr("data-pid") || 0),
                            selectProvince = jQuery("#provinceTittle" + index).text(),
                            selectCityValue = parseInt(jQuery("#cityTittle" + index).attr("data-cid") || 0),
                            selectCity = jQuery("#cityTittle" + index).text();
                if(selectProvinceValue<1 && selectCityValue<1) return;

                var dealerCityId = 511000;
                var dealerProvinceId = parseInt(dealerCityId / 10000) * 10000;
                var dealerSetProvinceIds ='';
                var dealerSetCityIds = '';

                switch(1){//0=全国，1=本省，2=本市，3=自定义
                    case 1:
                        areaFlag=selectProvinceValue > 0 && selectProvinceValue == dealerProvinceId;
                        break;
                    case 2:
                        areaFlag=selectProvinceValue > 0 && selectProvinceValue == dealerProvinceId;
                        var provides=[110000,120000,310000,500000];
                        areaFlag=(areaFlag &&jQuery.inArray(selectProvinceValue, provides)>-1)|| (areaFlag&&selectCityValue<1) || selectCityValue == dealerCityId;
                        break;
                    case 3:
                        //正则匹配市是否在经销商范围内时才用到provinceRegex
                        var provinceRegex = '';
                        if (selectProvinceValue != 0) {
                            provinceRegex = (selectProvinceValue+"").substring(0, 2) + '\\d{2}00';
                        }
                        //第一步，判断省选框的值是否在经销商设定范围内
                        if (selectProvinceValue> 0 && dealerSetProvinceIds.match(selectProvinceValue+'') == null) {
                            areaFlag = false;
                        }
                            //第二步，省选择在处理范围内,判断市选框的值是否在经销商设定范围内
                        else if (selectProvinceValue >0 && dealerSetProvinceIds.match(selectProvinceValue+'') != null
                                    && selectCityValue >0
                                    && dealerSetCityIds.match(provinceRegex) != null
                                    && dealerSetCityIds.match(selectCityValue) == null) {
                            areaFlag = false;
                        }
                        else {
                            areaFlag = true;
                        }
                        break;
                    default:
                        areaFlag=true;
                        break;
                }

                if(areaFlag){
                    if(id){
                        jQuery("#areaTips1,#areaTips2,#areaTips3").hide();
                    }else{
                        jQuery("#areaTips1,#areaTips2,#areaTips3").hide();
                    }
                    var low= jQuery("#lowpricespan"+index);
                    if(low.length>0){
                        jQuery("#orderPhoneMsg"+index).hide();
                        jQuery("#lowpricespan"+index).show();
                    }
                }else{
                    if(id){
                        if (selectCityValue != "0") {
                            selectCity=selectCity=="选择城市"?"":selectCity;
                            jQuery("#areaDealer"+index).html(selectProvince == selectCity ? selectProvince : selectProvince + selectCity);
                        }
                        else {
                            jQuery("#areaDealer"+index).html(selectProvince);
                        }
                        jQuery("#lowpricespan"+index).hide();
                        jQuery("#areaTips1,#areaTips2,#areaTips3").show();
                    }else{
                        jQuery("#areaTips1,#areaTips2,#areaTips3").show();
                        if (selectCityValue != "0") {
                            selectCity=selectCity=="选择城市"?"":selectCity;
                            jQuery("#areaDealer1,#areaDealer2,#areaDealer3").html(selectProvince == selectCity ? selectProvince : selectProvince + selectCity);
                        }
                        else {
                            jQuery("#areaDealer1,#areaDealer2,#areaDealer3").html(selectProvince);
                        }
                    }
                }
                validate.init_code();
            }
            setTimeout(function(){ changeArea(); },500);
        </script>
            <script type="text/javascript">
                jQuery(function(){
                    //for (var i = 1; i < 4; i++) {
                    jQuery("#btnSendCodeok").click(function(){
                        validate.triggerCode({
                            isClear:true,
                            callback_success:function(){
                                $$("#liimgValidCodeSMS").layer("hide");
                                jQuery("#verifySMSTip").addClass("fn-hide");
                                validate.sms_djs();
                                $("#verifySMSTip").appendTo($("#SMSCode1").parent())
                            }
                        });
                    });
                    jQuery("#btnSendCodecancel").click(function(){
                        $$("#liimgValidCodeSMS").layer("hide");
                        jQuery("#verifySMSTip").addClass("fn-hide");
                        validate.clearBtnSend();
                        $("#verifySMSTip").appendTo($("#SMSCode1").parent())
                    });

                        //获取短信验证码
                        jQuery("#btnSendCode1").click(function () {
                            var self=this;
                            var index = $("#ulOrderArea li.current").attr("data-index");
                            var phone = jQuery.trim(jQuery("#orderPhone"+ index).val());
                            
                            var orderType = index == 1 ? (jQuery("#chkZH")[0].checked ? 3 : 1) : (index == 2 ? 2 : 4);
                            var specid=jQuery("#SpecId").length>0 ? jQuery("#SpecId").val():jQuery("#loadSpecTittle"+index).attr("data-spid");

                            if(specid<0) {
                                jQuery("#ordeSpecMsg"+index).show();
                                return;
                            }
                            if(!(/^(1[0-9]{10})$/.test(phone))) {
                                jQuery("#orderPhoneMsg"+index).show();
                                return;
                            }
                            jQuery(self).attr("disabled", "none");
                            if(sendCodet[1] != null)
                                return;

                            validate.applyCode({
                                isClear:false,
                                callback_success:function(){
                                    validate.triggerCode({
                                        isClear:true,
                                        callback_success:function(){
                                            validate.sms_djs();
                                        }
                                    });
                                }
                            });
                           
                            
                        });
                        $("#imgValidCodeSMSchangeCode").click(function() {
                            $("#imgValidCodeSMS").removeAttr("notchange");
                            validate.applyCode({isClear:false,refreshimg:true});
                        });
                    //}
                });
            </script>

    <script type="text/javascript">

        var areaFlag = true;

        jQuery(function(){
            var userSid =parseInt(readCookie("cookieCityId")) || parseInt(readCookie("area"));
            var userCityId = parseInt(userSid / 100) * 100;
            var userProvinceId = parseInt(userSid / 10000) * 10000;
            var prov = ["province", "provinceTittle", "provinceList"];
            var city = ["city", "cityTittle", "cityList"];
            var county = ["county", "countyTittle", "countyList"];

            for (var i = 1; i < 4; i++) {
                new  citySelector   ([
                     { ids: [prov[0] + i, prov[1] + i, prov[2] + i], onchange: function (pid) {
                         var id="";
                         jQuery("#orderInfo div ul li").each(function(){
                                 if(jQuery(this).hasClass("current"))
                                 {
                                     id=  jQuery(this)[0].childNodes[1].id.replace("targetTab","")
                                 }
                             })
                         var index=(prov[1] + id).replace("provinceTittle","");
                         var title=jQuery("#"+prov[1] + id);
                         for (var n = 1; n < 4; n++) {
                             if(index!=n){
                                 jQuery("#provinceTittle"+n).text(title.text()).attr("data-pid",title.attr("data-pid"));
                                 jQuery("#provinceList"+n +" dd a").each(function (){
                                     if(jQuery(this).attr("data-pid")==pid){
                                         jQuery(this).click();
                                         return;
                                     };
                                 })
                             }
                         }
                         changeArea&&changeArea(pid);
                     } },
                     { ids: [city[0] + i, city[1] + i, city[2] + i], onchange: function (cid) {
                         var id="";
                         jQuery("#orderInfo div ul li").each(function(){
                             if(jQuery(this).hasClass("current"))
                             {
                                 id=  jQuery(this)[0].childNodes[1].id.replace("targetTab","")
                             }
                         }
                            )
                         var index=(city[1] + id).replace("cityTittle","");
                         var seric=jQuery("#"+city[1]+id);
                         for (var n = 1; n < 4; n++) {
                             if(index!=n){
                                 jQuery("#cityTittle"+n).text(seric.text()).attr("data-cid",seric.attr("data-cid"));
                                 jQuery("#cityList"+n +" dd a").each(function (){
                                     if(jQuery(this).attr("data-cid")==cid){
                                         jQuery(this).click();
                                         return;
                                     };
                                 })
                             }
                         }
                         changeArea&&changeArea(cid);
                         if(cid>0){

                             var id="";
                             jQuery("#orderInfo div ul li").each(function(){
                                 if(jQuery(this).hasClass("current"))
                                 {
                                     id=  jQuery(this)[0].childNodes[1].id.replace("targetTab","")
                                 }
                             }
                                )
                             var index=(city[1] + id).replace("cityTittle","");
                             jQuery("#orderInfo .tabcont-title ul li").each(function(i,ele){
                                 if(jQuery(ele).hasClass("current")){
                                     index=i+1;
                                     return true;
                                 }
                             });
                             jQuery("#orderCityMsg"+index).hide();
                         }else {
                             jQuery("#orderCityMsg"+index).show();
                         }
                     } },
                     { ids: [county[0] + i, county[1] + i, county[2] + i], onchange: function () {
                         var id="";
                         jQuery("#orderInfo div ul li").each(function(){
                             if(jQuery(this).hasClass("current"))
                             {
                                 id=  jQuery(this)[0].childNodes[1].id.replace("targetTab","")
                             }
                         }
                            )
                         var index=county[1].replace("countyTittle","");
                         var seric=jQuery("#"+county[1]+id);
                         for (var n = 1; n < 4; n++) {
                             if(index!=n){
                                 jQuery("#countyTittle"+n).text(seric.text()).attr("data-sid",seric.attr("data-sid"));
                                 jQuery("#countyList"+n +" dd a").each(function (){
                                     if(jQuery(this).attr("data-sid")==seric.text()){
                                         jQuery(this).click();
                                         return;
                                     };
                                 })
                             }
                         }
                     } }
                ]).init(userProvinceId,userCityId);
            }
            new carSelector([{ ids: ["Brand1", "BrandTittle1", "BrandList1"] },
                              { ids: ["Series1", "SeriesTittle1", "SeriesList1"] },
                              { ids: ["Spec1", "SpecTittle1", "SpecList1"],onchange:function(specid){
                                  if(specid>0){
                                      jQuery("#sCar_Select_Tip").hide();
                                  }else {
                                      jQuery("#sCar_Select_Tip").show();
                                  }
                              } }]
                ).init();
            new yearSelector([{ ids: ["year1", "yearTittle1", "yearList1"], currdate: 2017 },
                              { ids: ["month1", "monthTittle1", "monthList1"], currdate: 4,onchange:function(month){
                                  if(month>0){
                                      jQuery("#sCar_DatePic_Tip").hide();
                                  }else {
                                      jQuery("#sCar_DatePic_Tip").show();
                                  }
                              }}]).init();
        });
    </script>


    <script src="//x.autoimg.cn//dealer/common/js/oatmo.20170214.js?3"></script>

<script>
    var oatmo = new OrderAtmoLog({
        baseUrl: "dealer.autohome.com.cn",//当前站点域名
        host: window.location.host,//当前页面host
        txtPhone:jQuery("#orderPhone1,#orderPhone2"),
        btnSubmit:jQuery("#btnOrder1,#btnOrder2"),
        getData:function(){
            var index = $("#ulOrderArea li.current").attr("data-index");
            var eid = "";
            var orderType = index == 1 ? (jQuery("#chkZH")[0].checked ? 3 : 1) : (index == 2 ? 2 : 4);

            if(sourceType==2){
                if(orderType==1||orderType==3){  
                    eid="1|4|71|1211|200083|300000";
                }
                else {
                    eid = "1|4|71|1211|200084|300074"; 
                }
            }

            return {
                CurrentUrl:window.location.origin + window.location.pathname,
                ReferUrl:document.referrer,
                UserPhone:escape(jQuery("#orderPhone" + index).val()),
                DealerIds:jQuery("#dealerId").val(),
                SpecId : jQuery("#SpecId").length>0 ? jQuery("#SpecId").val():jQuery("#loadSpecTittle"+index).attr("data-spid"),
                SeriesId:jQuery("#SeriesId").length>0 ? jQuery("#SeriesId").val():jQuery("#loadSeriesTittle"+index).attr("data-seid"),
                IsPublic:0,
                OrderType:index == 1 ? (jQuery("#chkZH")[0].checked ? 3 : 1) : (index == 2 ? 2 : 4),
                SiteId:_tools.get_order_siteid(jQuery("#SiteId").val()),
                EID:eid,
                LoadId:typeof(window.pageLoadId) != "undefined"?window.pageLoadId:"",
                IsOpenVerifyCode: (isShowCode || isShowCodeImg) ?1:0,
                ts:(new Date())
            };
        }
    });

    $(function(){
        setTimeout(function(){ oatmo.init();},500);
    });
</script>

    
    <script type="text/javascript">
        var sourceType = 2;
        var abtestk = "150000";
        var eid = '';
        var sites = { PvSite: 4, PvCategory: 71, PvSubCategory: 1211 };

        var sendCodet = new Array(2), isShowCode = false;isShowCodeImg = false;//

        var validate = {
            bindMilesEvent: function() { //行驶里程
                jQuery("#txt_Scar_Miles").on({
                    blur:function(){
                        var inputMiles = jQuery.trim(jQuery("#txt_Scar_Miles").val());
                        if (inputMiles == "" || isNaN(inputMiles) || inputMiles < 0.01 || inputMiles > 99.99){
                            jQuery("#sCar_Miles_Tip").show();
                        }else {
                            jQuery("#sCar_Miles_Tip").hide();
                            if(!isNaN(this.value)){
                                this.value=Number(this.value);
                            }
                        }
                    },
                    keyup:function(){
                        if(this.value.length>5){
                            this.value=this.value.substring(0,5);
                            jQuery("#sCar_Miles_Tip").show();
                        }else{
                            jQuery("#sCar_Miles_Tip").hide();
                        }
                    }
                });
            },
            validateReplace: function() {
                var ScarBrandId=jQuery("#BrandTittle1").attr("data-bid")||0,
                      ScarSeriesId=jQuery("#SeriesTittle1").attr("data-seid")||0,
                      ScarSpecId=jQuery("#SpecTittle1").attr("data-spid")||0,
                      ScarRegistYear=jQuery("#yearTittle1").attr("data-year")||0,
                      ScarRegistMonth=jQuery("#monthTittle1").attr("data-month")||0,
                      inputMiles=jQuery.trim(jQuery("#txt_Scar_Miles").val())

                var flag = true;
                if (ScarBrandId<=0|| ScarSeriesId <=0 || ScarSeriesId <=0 ||ScarSpecId<1 ) {
                    jQuery("#sCar_Select_Tip").show();
                    flag = false;
                } else {
                    jQuery("#sCar_Select_Tip").hide();
                }

                if (inputMiles == "" || isNaN(inputMiles) || inputMiles < 0.01 || inputMiles > 99.99) {
                    jQuery("#sCar_Miles_Tip").show();
                    flag = false;
                } else {
                    jQuery("#sCar_Miles_Tip").hide();
                }

                if (ScarRegistYear<=0 || ScarRegistMonth<=0) {
                    jQuery("#sCar_DatePic_Tip").show();
                    flag = false;
                } else {
                    jQuery("#sCar_DatePic_Tip").hide();
                }
                return flag;
            },

            bindUserNameEvent:function (controlName, errMsg, index) {
                jQuery(controlName + index).on({
                    blur:function(){
                        if (jQuery.trim(jQuery(controlName + index).val()) == ""){
                            jQuery(errMsg + index).text("请填写您的姓名");
                            jQuery(errMsg + index).show();
                        }else if(jQuery.trim(jQuery(controlName + index).val()).length>6){
                            jQuery(errMsg + index).text("名字不能超过6个字符");
                            jQuery(errMsg + index).show();
                        }else {
                            jQuery(errMsg + index).hide();
                        }
                    }
                });
            },
            bindPhoneEvent:function () {
                jQuery("[name='orderphoneinput']").on({
                    blur:function(){
                        //for(var i=1;i<4;i++)
                        //{
                        var index = $("#ulOrderArea li.current").attr("data-index");
                        var low = false;
                        var phone = jQuery.trim(jQuery("#orderPhone" + index).val());
                        if (phone == "" && phone.length != 11) {
                            jQuery("#orderPhoneMsg" + index).html("请输入正确的手机号码").show();
                            jQuery("#btnSendCode1").attr("disabled", "none");
                        } else if (!(/^(1(([34578][0-9])|(47)))\d{8}$/.test(phone))) {
                            jQuery("#orderPhoneMsg" + index).html("请输入正确的手机号码").show();
                            jQuery("#btnSendCode1").attr("disabled", "none");
                        } else {
                            jQuery("#orderPhoneMsg" + index).hide();
                            low = true;
                            if (sendCodet[index] == null) {
                                jQuery("#btnSendCode1").removeAttr("disabled");
                            }
                        }
                        low&&areaFlag?jQuery("#lowpricespan" + index).show():jQuery("#lowpricespan" + index).hide();
                        // }
                    },
                    keyup:function(){
                        //var val = this.value.replace(/\D+/g, '');
                        //if (val != this.value) {
                        //    this.value = val;
                        //}
                        // if(this.value.length>11){
                        //    this.value=this.value.substring(0,11);
                        //}
                    },
                    change:function() {
                        if (this.value.length == 11) {
                            $("#btnSendCode1").removeAttr("sms_guid").removeAttr("sms_captchaid");
                            validate.init_code();
                        }
                    }
                });
            },
            validateSubmit:function (specid,seriesId,specMsg, userName, userNameMsg, phone, phoneMsg, provinceid, cityid, cityMsg, SMSCode, confimCode, confimCodeRow, lowpricespan) {
                var flag = true;

                if (specid <1||seriesId <1) {
                    jQuery(specMsg).show();
                    flag = false;
                }
                else
                    jQuery(specMsg).hide();

                if (jQuery.trim(jQuery(userName).val()) == "") {
                    jQuery(userNameMsg).text("请填写您的姓名");
                    jQuery(userNameMsg).show();
                    flag = false;
                }else if(jQuery.trim(jQuery(userName).val()).length>10){
                    jQuery(userNameMsg).text("名字不能超过10个字符");
                    jQuery(userNameMsg).show();
                }
                else
                    jQuery(userNameMsg).hide();

                var phone = jQuery.trim(jQuery(phone).val());
                if (phone == ""&&phone.length !=11) {
                    jQuery(phoneMsg).html("请输入正确的手机号码").show();
                    jQuery(lowpricespan).hide();
                    flag = false;
                }
                else if (!(/^(1(([34578][0-9])|(47)))\d{8}$/.test(phone))) {
                    jQuery(phoneMsg).html("请输入正确的手机号码").show();
                    jQuery(lowpricespan).hide();
                    flag = false;
                }
                else {
                    jQuery(phoneMsg).hide();
                    areaFlag?jQuery(lowpricespan).show():jQuery(lowpricespan).hide();
                }

                if (provinceid < 1 || cityid < 1 ) {
                    jQuery(cityMsg).show();
                    flag = false;
                }
                else {
                    jQuery(cityMsg).hide();
                }
                if (isShowCode && jQuery.trim(jQuery(SMSCode).val()) == "") {
                    jQuery(confimCode).text("请输入验证码").show();
                    jQuery(confimCodeRow).show();
                    flag = false;
                } else {
                    jQuery(confimCodeRow).hide();
                }
                if (isShowCodeImg && jQuery.trim(jQuery("#imgCode").val()) == "") {
                    jQuery("#imgValidCodeDealerError").text("请输入验证码").removeClass("fn-hide");
                    flag = false;
                } else {
                    jQuery("#imgValidCodeDealerError").addClass("fn-hide");
                }
                return flag;
            },
            init_code:function() {
                var dealerId = jQuery("#dealerId").val();
                var that = this;
                jQuery.ajax({
                    type: "get",
                    url: "/handler/order/VerificationCode",
                    data: {dealerId:dealerId,_ts:(new Date()).getTime()},
                    dataType: "text",
                    success: function(data) {
                        isShowCode = (data === "true");
                        if (isShowCode) {
                            $("tr[name='trValidateCode']").show();
                            return;
                        }
                        else
                        {
                            $("tr[name='trValidateCode']").hide();
                        }
                    },
                    complete:function() {
                        var index = $("#ulOrderArea li.current").attr("data-index");
                        that.init_imgcode(index);
                    }
                });
            },
            init_imgcode:function(index) {
                if (isShowCode == true) {
                    //sms
                    this.initSendBtn({index:index});
                    this.showValidate(false);
                    return;
                }

                var inertBefore = "skipcont" + index;

                var ishas = $("#"+inertBefore).parent().find("#divimgValidCodeDealer").length;
                if (ishas == 0) {
                    $("#divimgValidCodeDealer").insertBefore($("#"+inertBefore));
                }


                var dealerId = jQuery("#dealerId").val();
                var cid = jQuery("#cityTittle" + index).attr("data-cid") || 0;
                var seriesId=jQuery("#SeriesId").length>0 ? jQuery("#SeriesId").val():jQuery("#loadSeriesTittle"+index).attr("data-seid");

                var that = this;

                jQuery.ajax({
                    type: "get",
                    url: "/handler/order/IsNeedVerificationImgCode",
                    data: {cityid:cid,dealerIds:dealerId,seriesId:seriesId,_ts:(new Date()).getTime()},
                    dataType: "text",
                    success: function(data) {
                        isShowCodeImg = (data === "true");
                        that.showValidate(isShowCodeImg);
                    }
                });
            },
            sms_djs:function(){
                var s = 60;
                jQuery("#btnSendCode1").attr("disabled", "none");
                jQuery("#btnSendCode1").val("(" + s + ")秒后重新获取验证码");
                sendCodet[1] = setInterval(function () {
                    s--;
                    if (s <= 0) {
                        validate.clearBtnSend();
                    } else {
                        jQuery("#btnSendCode1").val("(" + s + ")秒后重新获取验证码");
                        jQuery("#btnSendCode1").attr("disabled", "none");
                    }
                }, 1000);
            },
            triggerCode:function(opts) {
                var defaults = {
                    isClear: false,
                    refreshimg:false,
                    callback_success:function(){},
                    callback: function(){}
                };
                var options = $.extend(defaults, opts);


                var index = $("#ulOrderArea li.current").attr("data-index");
                var phone = $.trim($("#orderPhone"+index).val());
                var guid = $("#btnSendCode1").attr("sms_guid");
                if (!guid) {
                    guid = "";
                    return;
                }
                var imgcode =  $.trim($("#SMSCode11").val());
                var sms_captchaid = $("#btnSendCode1").attr("sms_captchaid");
                if (options.isClear && sms_captchaid && sms_captchaid != "" && imgcode == "") {
                    jQuery("#verifySMSTip").html("请先输入图片中的验证码来获取短信").removeClass("fn-hide");
                    $$("#liimgValidCodeSMS").layer("show");
                    validate.clearBtnSend();
                    return;
                }
                jQuery("#verifySMSTip").addClass("fn-hide");
                var ordertype = index == 1 ? (jQuery("#chkZH")[0].checked ? 3 : 1) : (index == 2 ? 2 : 4);
                var specId=jQuery("#SpecId").length>0 ? jQuery("#SpecId").val():jQuery("#loadSpecTittle1").attr("data-spid");
                jQuery.ajax({
                    type: "get",
                    url: "/handler/order/PostCode",
                    data: {phone:phone,guid:guid,code:imgcode,ordertype:ordertype,specId:specId,_ts:(new Date()).getTime()},
                    dataType:"json",
                    success: function (json) {
                        if (json) {
                            if (json.returncode == 0) {
                                $("#btnSendCode1").removeAttr("sms_captchaId");
                                if (json.HasSuccess) {
                                    //成功
                                    options.callback_success();
                                }
                            } else if (json.message && json.message != "") {
                                if (json.returncode == 2) {
                                    //验证码错误
                                    json.message = "请输入正确的图形验证码";
                                    $("#imgValidCodeSMSchangeCode").click();
                                } else if (json.returncode == 43) {
                                    //不存在这个guid
                                    if ($("#liimgValidCodeSMS").hasClass("fn-hide"))
                                        json.message = "网络繁忙，请重试";
                                    else
                                        json.message = "请输入正确的图形验证码";
                                }
                                //validate.applyCode(true);
                                jQuery("#verifySMSTip").html(json.message).removeClass("fn-hide");
                                //清除倒数
                                if (options.isClear) {
                                    validate.clearBtnSend();
                                }
                            }
                        } //end if
                        else {

                        }
                    },
                    error:function() {
                        //验证码获取错误，请稍候再试
                        //validate.applyCode(true);
                        jQuery("#verifySMSTip").html("验证码获取错误，请稍候再试").removeClass("fn-hide");

                        //清除倒数
                        if(options.isClear){
                            validate.clearBtnSend();
                        }
                    },
                    complete:function() {

                    }
                });//end ajax
            },
            clearBtnSend:function() {
                $("#btnSendCode1").removeAttr("disabled");
                jQuery("#btnSendCode1").val("免费获取验证码");
                clearInterval(sendCodet[1]);
                sendCodet[1] = null;
            },
            initSendBtn:function(opts) {
                var defaults = {
                    index: $("#ulOrderArea li.current").attr("data-index")
                };
                var options = $.extend(defaults, opts);

                if (isShowCode != true) {
                    return;
                }
                var notchange = $("#imgValidCodeSMS").attr("notchange");
                var guid = $("#btnSendCode1").attr("sms_guid");
                if (notchange && typeof(guid) != "undefined") {
                    return;
                }
                //this.applyCode(false,function() {
                //var index = $("#ulOrderArea li.current").attr("data-index");
                var index = options.index;
                var ishas = $("[data-smsinert="+index+"]").parent().find("[data-smsele='true']").length;
                if (ishas == 0) {
                    $("[data-smsele='true']").insertBefore($("[data-smsinert="+index+"]"));
                }

                var phone = jQuery("#orderPhone" + index).val();
                if(phone != ""){
                    jQuery("#orderPhone" + index).trigger("blur");
                }


                //});
            },
            applyCode:function(opts) {
                var defaults = {
                    isClear: false,
                    refreshimg:false,
                    callback_success:function(){},
                    callback: function(){}
                };
                var options = $.extend(defaults, opts);

                if(!options.refreshimg)
                {
                    $$("#liimgValidCodeSMS").layer("hide");
                    $("#verifySMSTip").appendTo($("#SMSCode1").parent()).addClass("fn-hide");
                }


                var index = $("#ulOrderArea li.current").attr("data-index");
                var phone = $.trim($("#orderPhone"+index).val());
                if (!/^(1[0-9]{10})$/.test(phone))
                    return;
                var guid = $("#btnSendCode1").attr("sms_guid");
                if (!guid)
                    guid = "";

                var that = this;

                jQuery.ajax({
                    type: "get",
                    url: "/handler/order/CodeCreate",
                    data: {phone: phone,guid:guid,_ts:(new Date()).getTime()},
                    dataType:"json",
                    success: function(json) {
                        if (!json) {
                            //jQuery("#verifySMSTip").html("网络繁忙，请稍后再试").show();
                            //清除倒数
                            if (options.isClear) {
                                that.clearBtnSend();
                            }
                            return;
                        }
                        if (json.returncode == 0) {
                            $("#btnSendCode1").attr("sms_guid", json.result.guid);
                            if (json.result.block) {
                                //img
                                //jQuery("#verifySMSTip").html("请输入图形验证码").show();
                                $("#SMSCode11").val("");
                                $("#btnSendCode1").attr("sms_captchaId", json.result.captchaId);
                                $$("#liimgValidCodeSMS").layer("show");
                                $("#verifySMSTip").appendTo($("#SMSCode11").parent())
                                $("#imgValidCodeSMS").attr("src", "/handler/order/GetImgCode?guid=" + json.result.captchaId + "&_t=" + (new Date()).getTime());
                                //清除倒数
                                if (options.isClear) {
                                    that.clearBtnSend();
                                }
                            } else {
                                $$("#liimgValidCodeSMS").layer("hide");
                                options.callback_success();
                            }
                        }
                        else if (json.message && json.message != "") {
                            jQuery("#verifySMSTip").html(json.message).removeClass("fn-hide");
                        }

                    },error:function() {
                        //验证码获取错误，请稍候再试
                        jQuery("#verifySMSTip").html("验证码获取错误，请稍候再试").removeClass("fn-hide");

                        //清除倒数
                        that.clearBtnSend();
                    },complete:function() {
                        if(options.callback)
                            options.callback();

                    },
                });
            },
            showValidate:function(show) {
                var o = $("#divimgValidCodeDealer");
                if (show) {
                    if ($(o).hasClass("fn-hide")) {
                        $(o).removeClass("fn-hide");
                        validate.refushImageDealer();
                    }
                } else {
                    $(o).addClass("fn-hide");
                }
            },
            refushImageDealer:function() {
                //var hosturl = "/handler/GetValidateCode?time=" + (new Date()).getTime();
                //$("#imgValidCodeDealer").attr("src", hosturl);
                var guid =$("#imgValidCodeDealer").attr("img_guid");
                if (!guid)
                {
                    guid = "";
                }
                var data = { consumerId: "yhz.captcha", _appId: "yhz", guid: guid };
                $.getJSON("//dealer.autohome.com.cn/handler/GetCodeCreate?callback=?".replace("dealer.autohome.com.cn",window.location.host),data,
                  function (json) {
                      if (json) {
                          if (json.HasSuccess) {
                              guid = json.result.guid;
                              $("#imgValidCodeDealer").attr("img_guid", guid);
                              var url = "//dealer.autohome.com.cn/handler/GetCodeImage".replace("dealer.autohome.com.cn", window.location.host) + "?consumerId=yhz.captcha&_appid=yhz&guid=" + guid + "&_t=" + (new Date()).getTime();
                              $("#imgValidCodeDealer").attr("src", url);
                              var o = $("#divimgValidCodeDealer");
                              if ($(o).hasClass("fn-hide")) {
                                  $(o).removeClass("fn-hide");
                              }
                          }
                          else {
                              //that.showErr(json.message, "imgcode");
                          }
                      }
                      else
                      {
                          //that.showErr("验证码加载失败", "imgcode");
                      }
                  });
            },
            initUserInfo:function() {
                var phone=readCookie("autoBindPhone");
                if(phone && phone.length === 11)
                {
                    jQuery("#orderPhone1,#orderPhone2").val(unescape(phone));
                }
                else
                {
                    $.getJSON("/Ajax/GetReportPhone?callback=?",
                    function(result){
                        if(result!=null &&result.MobilePhone )
                        {
                            jQuery("#orderPhone1,#orderPhone2").val(result.MobilePhone);
                        }
                    });
                }
                var username=readCookie("autoBindUsername");
                if(username && username !== "")
                {
                    jQuery("#orderUserName1,#orderUserName2").val(unescape(username));
                }
            },
            setCookie:function(name, value, expires, domain) {
                var sCookie = name + '=' + encodeURIComponent(value);
                if (expires) {
                    var exp = new Date(new Date().getTime() + expires);
                    sCookie += '; expires=' + exp.toGMTString();
                }
                if (domain) {
                    sCookie += '; domain=.' + domain;
                }
                sCookie += '; path=/';
                document.cookie = sCookie;
            },
            GetQueryString:function(name,def) {
                var result = location.search.match(new RegExp("[\?\&]" + name + "=([^\&]+)", "i"));
                if (result == null || result.length < 1) {
                    return def;
                }
                return result[1];
            },
            GetPvareaid:function() {
                var name = "pvareaid";
                var query = this.GetQueryString(name, "");
                if(query != "")
                    return query;
                var hh = location.hash.match(new RegExp("[\?\&\#]" + name + "=([^\&]+)", "i"));
                if (hh == null || hh.length < 1) {
                    return "";
                }
                return hh[1];
            }
        }

        // 初始化
        jQuery(function () {
            var dealerId = jQuery("#dealerId").val();
            validate.initUserInfo();
            //手机验证
            validate.bindPhoneEvent();
            //validate.init_code();
            for (var i = 1; i < 4; i++) {
                //姓名验证
                validate.bindUserNameEvent("#orderUserName", "#orderUserNameMsg", i);
                //公里数验证
                validate.bindMilesEvent();
                //同意绑定事件
                jQuery("#chkFLSM" + i).click(function () {
                    var index = parseInt(this.id.replace("chkFLSM", ""));
                    if (this.checked == true) {
                        jQuery("#btnOrder" + index).show();
                        jQuery("#btnUnable" + index).hide();
                    }
                    else {
                        jQuery("#btnOrder" + index).hide();
                        jQuery("#btnUnable" + index).show();
                    }
                });
                // 询价
                jQuery("#btnOrder" + i).bind("click", function () {
                    var index = parseInt(this.id.replace("btnOrder", ""));
                    var pid = jQuery("#provinceTittle" + index).attr("data-pid") || 0,
                        cid = jQuery("#cityTittle" + index).attr("data-cid") || 0
                    sid = jQuery("#countyTittle" + index).attr("data-sid") || 0;

                    var specid=jQuery("#SpecId").length>0 ? jQuery("#SpecId").val():jQuery("#loadSpecTittle"+index).attr("data-spid");
                    var specName=jQuery("#SpecName").length>0 ? jQuery("#SpecName").val():jQuery("#loadSpecTittle"+index).text();
                    var seriesId=jQuery("#SeriesId").length>0 ? jQuery("#SeriesId").val():jQuery("#loadSeriesTittle"+index).attr("data-seid");

                    trackDealerEvent('dlr_order_page_form_submit_click', {'business':'a','series':seriesId,'city':cid});

                    var flag = validate.validateSubmit(specid,seriesId,"#ordeSpecMsg"+index,
                                              "#orderUserName" + index, "#orderUserNameMsg" + index,
                                              "#orderPhone" + index, "#orderPhoneMsg" + index,
                                               pid, cid, "#orderCityMsg" + index,
                                              "#SMSCode1", "#confimCode1", "#confimCodeRow1",
                                              "#lowpricespan" + index);

                    
                    var orderType = index == 1 ? (jQuery("#chkZH")[0].checked ? 3 : 1) : (index == 2 ? 2 : 4);

                    var flagScar = true;
                    if (orderType == 3) { //试驾
                        flagScar = validate.validateReplace();
                    }
                    if (!flag || !flagScar) return;
                    if(sourceType==2){
                        if(orderType==1||orderType==3){
                            eid="1|4|71|1211|200083|300000";
                        }
                        else {
                            eid = "1|4|71|1211|200084|300074";
                        }
                    }
                    var userSid =parseInt(readCookie("cookieCityId")) || parseInt(readCookie("area"));
                    var userCityId = parseInt(userSid / 100) * 100;
                    var userProvinceId = parseInt(userSid / 10000) * 10000;
                    var username=jQuery("#orderUserName" + index).val(),phone= jQuery("#orderPhone" + index).val();

                    var scarMiles =jQuery.trim(jQuery("#txt_Scar_Miles").val()),  sCarYear=jQuery("#yearTittle1").attr("data-year"),
                                     sCarMonth=jQuery("#monthTittle1").attr("data-month"),sCarSpecId=jQuery("#SpecTittle1").attr("data-spid");
                    //屏蔽按钮
                    jQuery("#btnOrder" + index).hide();
                    jQuery("#btnDoingOrder" + index).show();
                    jQuery("#saveError1,#saveError2,#imgValidCodeDealerError").addClass("fn-hide");

                    var img_guid =  $("#imgValidCodeDealer").attr("img_guid");
                    var data =(function(){
                        return {
                            "Pid": pid,
                            "Cid": cid,
                            "Sid": sid,
                            "CityId": userCityId,
                            "ProvinceId": userProvinceId,
                            "DealerId": dealerId,
                            "OrderType": orderType,
                            "SpecId": specid,
                            "SpecName": specName,
                            "SeriesId": seriesId,
                            "Phone": escape(jQuery("#orderPhone" + index).val()),
                            "Usersex": jQuery("input[name='radUserSex" + index + "']:checked").val(),
                            "UserName": username,
                            //"vcode":'False'+"|"+(jQuery("#SMSCode1").val()||""),
                            "vcode":jQuery("#SMSCode1").val(),
                            "vcodeid":$.trim(jQuery("#btnSendCode1").attr("sms_guid")),
                            "img_guid":img_guid,
                            "ValidateCode":jQuery("#imgCode").val(),
                            "ScarBrandId": jQuery("#BrandTittle1").attr("data-bid"),
                            "ScarSeriesId": jQuery("#SeriesTittle1").attr("data-seid"),
                            "ScarSpecId": sCarSpecId,
                            "ScarRegistYear": sCarYear,
                            "ScarRegistMonth": sCarMonth,
                            "ScarMiles": scarMiles,
                            "ScarExpectPrice": 0,
                            "SiteId": _tools.get_order_siteid(jQuery("#SiteId").val()),
                            "PvSite": sites.PvSite,
                            "PvCategory": sites.PvCategory,
                            "PvSubCategory": sites.PvSubCategory,
