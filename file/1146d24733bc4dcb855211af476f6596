                    for (var o = i[n];
                    " " == o.charAt(0) ;) o = o.substring(1, o.length);
                    if (0 == o.indexOf(a)) return decodeURIComponent(o.substring(a.length, o.length))
                }
                return t === void 0 ? null : t
            },
            set: function (e, t, a) {
                var i = e + "=" + encodeURIComponent(t);
                if (a) {
