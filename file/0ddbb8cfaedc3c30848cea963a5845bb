<script type="text/javascript">
		function  goChange(IndexName){
			var cityName = '西安';
			window.location.href = "http://www.box1007.com/Index_changeSub.do?cityName="+cityName;
		} 
		
		
	function favoriteAtte(title){
		 var Sys = {};
		 var url = location.href;
		 var ua = navigator.userAgent.toLowerCase();
         if (window.ActiveXObject){
            Sys.ie = ua.match(/msie ([\d.]+)/)[1];
         }  
         if(Sys.ie){ 
	 		title= encodeURIComponent(title);
	  	 }
		 jQuery.ajax({  
	    	type: "GET",   
	    	url:"http://www.box1007.com/CustInfo_favoriteAtte.do", 
	    	data:"title="+title+"&url="+url+"&timeStamp="+new Date().getTime(),   
	   		success: function (result) {
	   			var condition = result;
				if(condition==1){//未登录
					alert("请您先登录！");
				}else if(condition==2){//关注成功
					alert("关注成功！");
				}
	        }
	    }); 
	   
	} 
</script>
