<script language="javascript">
var leadList = document.getElementById("lead") ;
var BNameList = document.getElementById("brand_id") ;

var URL = "http://iwatchome.tom.com/getBName.php" ;
var json ;

leadList.onchange = function(){
	sURL = addURLParam(URL,"lead",this.options[this.selectedIndex].value);
	var oRequest = new XMLHttpRequest();
	oRequest.open("get",sURL,true) ;
	

oRequest.onreadystatechange = function() {
if (oRequest.readyState == 4 && oRequest.status == 200){
	json = "json=" + oRequest.responseText;
	json = eval(json) ;
	//alert(json);
	BNameList.options.length = 0;
	BNameList.options[0] = new Option("------------------------------","");
	for (i=0;i<json.length;i++){
		 BNameList.options[i+1] = new Option(json[i].text,json[i].value);
	}
	BNameList.options[1].selected = "selected" ;

}

}
	
	//leadList.disabled = true ;
	oRequest.send(null);
}

function addURLParam(sURL, sParamName, sParamValue) { 
sURL += (sURL.indexOf("?") == -1 ? "?" : "&"); 
sURL += encodeURIComponent(sParamName) + "=" + encodeURIComponent(sParamValue); 
//alert(sURL); 
return sURL; 
}

</script>
