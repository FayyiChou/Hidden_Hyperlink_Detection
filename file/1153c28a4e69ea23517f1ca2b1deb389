<script type="text/javascript" src="/js/jquery.SuperSlide.2.1.1.js"></script>
<script type="text/javascript" src="/js/ZoomPic.js"></script>
<script  type="text/javascript" src="/js/jqnav.js"></script>
  <script type="text/javascript" src="/js2/ind.js"></script>
 <script  type="text/javascript"src="/js/Index.js"></script>

    



<script type="text/javascript">
    $(function () {
        //<input name="" id="seachText" type="text" class="schtxt1" style="height: 28px;">
    

        var $fixedMenu = $("#subNav li");
        $(".con").attr("id", function () { return "pointB" + $(".con").index(this) });
        $fixedMenu.click(function () {
            $(this).addClass("hover").siblings().removeClass("hover");
            var clickNum = "pointB" + $fixedMenu.index(this);
            $("html,body").animate({ scrollTop: $("#" + clickNum).offset().top }, 1000);
        });
        $(window).scroll(function () {
            var scroll = $(this).scrollTop() + 500;
            var scrollTop = $(this).scrollTop();
            var scrollHeight = $(document).height() + 2000;
            var windowHeight = $(this).height();
            if (scrollTop >= scrollHeight - windowHeight - 350) {
                $("#float_ewm").hide();
            }
            else {
                $("#float_ewm").show();
            }

            $(".con").each(function (k, v) {
                var oo = $(this).position().top + 200;//当前高度
                //+100像素
                if (oo <= scroll) {
                    $('#subNav li').removeClass('adv_active');
                    $('#subNav li').eq(k).addClass('adv_active');
                }
            });
        });
    })

</script>
  