        $("#seachText").keypress(function (event) {
            if (key != "") {
              
            }
            var key = event.which;//e.which是按键的值
            if (key == 13) {
                $("#seachButtn").click();
            }
        })
       
        $("#seachButtn").click(function () {
            var str = ($("#set1").text());
            var name = $("#seachText").val();
         
            if (str == "品牌") {
                window.location.href = "/brand/SearchBrand.aspx?nameTxt=" + name + "";
            } else {
                window.location.href = "/product/searchIndex.aspx?nameTxt=" + name + "";
            }
        });


