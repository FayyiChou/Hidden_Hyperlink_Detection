<div id="ShowAD" style="position:absolute; z-index: 100;">
<div style="width:144;height:18px;font-size:14px;font-weight:bold;text-align:left;CURSOR: hand;" onClick="closead();"><font color=ff0000>关闭</font></div>
<a href="#" target="_blank"><img src="ad/wei2.jpg" width="144" height="348" border="0"></a>
</div>
<script>
var bodyfrm = ( document.compatMode.toLowerCase()=="css1compat" ) ? document.documentElement : document.body;
var adst = document.getElementById("ShowAD").style;
	adst.top = ( bodyfrm.clientHeight -348-12 ) + "px";
	adst.left = ( bodyfrm.clientWidth -144 ) + "px";
function moveR() {
	adst.top = ( bodyfrm.scrollTop + bodyfrm.clientHeight - 348-12) + "px";
	adst.left = ( bodyfrm.scrollLeft + bodyfrm.clientWidth - 144 ) + "px";
}
setInterval("moveR();", 80);

function closead()
{
	adst.display='none';
}
</script>
