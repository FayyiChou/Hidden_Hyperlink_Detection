<script type="text/javascript">
      window.onload = function () {
    var oDiv = document.getElementById("div1");
    var oClose = document.getElementById("close");
    var topDistance = 0;

    function pageVal() {
        topDistance = document.body.scrollTop || (document.documentElement && document.documentElement.scrollTop);
        return topDistance;
    }
    addEvent(document, "mousewheel", pageVal);
    addEvent(document, "DOMMouseScroll", pageVal);

    Move(oDiv);
    function Move(obj) {
        var x = 1;
        var y = 2;

        function float() {
            var L = obj.offsetLeft + x;
            var T = obj.offsetTop + y;
            topDistance = pageVal();

            if (T >= document.documentElement.clientHeight - obj.offsetHeight + topDistance) {
                y *= -1;
                T = document.documentElement.clientHeight - obj.offsetHeight + topDistance;
            } else if (T <= topDistance) {
                y *= -1;
                T = topDistance;
            }
            if (L >= document.documentElement.clientWidth - obj.offsetWidth - 10) {
                x *= -1;
                L = document.documentElement.clientWidth - obj.offsetWidth - 10;
            } else if (L <= 0) {
                x *= -1;
                L = 0;
            }
            obj.style.left = L + "px";
            obj.style.top = T + "px";
        }

        var t = null;
        t = setInterval(function () {
            float();
        }, 20);

        obj.onmouseover = function () {
            clearInterval(t)
        };
        obj.onmouseout = function () {
            t = setInterval(function () {
                float();
            }, 20);
        };
        oClose.onclick = function () {
            obj.style.display = "none";
            obj.style.left = 0 + "px";
            obj.style.top = 0 + "px";
            oClose.style.display = "none";
            clearInterval(t);
        };
    }

    function addEvent(obj, event, fn) {
        if (obj.attachEvent) {
            return obj.attachEvent("on" + event, fn);
        } else {
            return obj.addEventListener(event, fn, false);
        }
    };
}
    </script>


