<script type="text/javascript" >
jQuery(document).ready(function(){
	jQuery(".nav ul li a").attr("id","aaa")
	jQuery(".nav ul li").hover(
		function(){
			jQuery(this).children(".drop_dwon").slideDown(200);
			jQuery(this).children(".nav ul li a").attr("id","sss")
		},
		function(){
			jQuery(this).children(".drop_dwon").slideUp(100);
			jQuery(this).children(".nav ul li a").attr("id","aaa")
		}
	);
});
setInterval("changeImg()",5000);
var i=0;		
function changeImg(){
	i=i+1;
	var num=i%3+1;	
	$("#img").attr("src","/themes/10516/default_10516_12339/images/banner_img0"+num+".jpg");			
}

</script>
