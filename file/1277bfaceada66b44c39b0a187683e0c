<script type="text/javascript"> 
$(function(){
	var sWidth=$("#focus").width();
	var sHeight = $("#focus").height();
	var len=$("#focus ul  a").length;
	var sWidth=$("#focus").width();
	var sm=$('<div cmsid="19634389"  class="sm"></div>');
	var index=0;
	var picTimer;
	var bttn='<div class="bttnBg"></div><div cmsid="13012245"  class="bttn">';
	for(var i=0;i<len; i++){
		var ii=i+1;
		bttn +="<span>"+ii+"</span>";
		}
		$("#focus").append(bttn);
		$("#focus").append(sm);
		$("#focus .bttnBg").css("opacity",0.3);
		$("#focus .bttn span").css("opacity",0.4).mouseenter(function(){
		index = $("#focus .bttn span").index(this);
		sh(index);//向上滚动则改为showTop,向左滚动则改为showLeft,渐隐渐现则为sh
		}).eq(0).trigger("mouseenter");
		$("#focus").hover(function(){	   
				clearInterval(picTimer);
				},function(){
				picTimer=setInterval(function(){
						sh(index);//向上滚动则改为showTop,向左滚动则改为showLeft,渐隐渐现则为sh
						index++;
						if(index==len){index=0;}
						},5000)	//滚动间隔，数越大间隔越长
					}).trigger("mouseleave");
		
		
		function sh(index){
			$("#focus ul").css({"width":sWidth,"height":sHeight});
			var titlea=$("#focus img").eq(index).attr("titlea")
			$(".sm").empty();
			$(".sm").append(titlea)
			$("#focus ul a img").css({position:"absolute",top:0,left:0}).parent().eq(index).stop(true,true).animate({"opacity":1},2000).css({display:"block"}).siblings().stop(true,true).animate({"opacity":0.2},1000).css({display:"none"});
			$("#focus .bttn span").stop(true,false).animate({"opacity":0.4},300).removeClass("red").eq(index).stop(true,false).animate({"opacity":0.8},300).addClass("red");
		
			}
		function showLeft(index){
			$("#focus ul").css("width",sWidth*len);
			var nowLeft=-index*sWidth;
			var title=$("#focus img").eq(index).attr("titlea")
			$("#focus .sm").empty();
			$("#focus .sm").append(titlea)
			$("#focus ul").stop(true,false).animate({"left":nowLeft},300);
			$("#focus .bttn span").stop(true,false).animate({"opacity":0.4},300).removeClass("red").eq(index).stop(true,false).animate({"opacity":0.8},300).addClass("red");
			}
		function showTop(index) { 
			$("#focus ul").css("height",sHeight*len);
			var nowTop = -index*sHeight;
			var title=$("img").eq(index).attr("titlea")
			$(".sm").empty();
			$(".sm").append(titlea)
			$("#focus ul").stop(true,false).animate({"top":nowTop},300);
			$("#focus .bttn span").stop(true,false).animate({"opacity":"0.4"},300).removeClass("red").eq(index).stop(true,false).animate({"opacity":"1"},300).addClass("red"); 
	}
             
});
   function formSubmit(){
                   var form = $(document).find("#myForm");
                   var keyword = $("#keyword").val();
                   form.action = '/html/search.html?keyword='+ keyword;
                   $("#keyword").val(keyword);
                   form.submit();
}
</script> <!--   href="暂无" style="font-size:16px;color:#ffffff;"   -->
