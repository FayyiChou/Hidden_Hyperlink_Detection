<script>  
function aaa(divId,obj){
	var moreLink = $(obj).attr("name");
	$(obj).parent().next("a").attr("href",moreLink);
  	$(obj).parent().parent().next("div").children("div").hide();
	$("#"+divId).show();
	$(obj).parent("ul").children("li").removeClass("jy_tab_on");
	$(obj).attr("class","jy_tab_on");
}
</script>













