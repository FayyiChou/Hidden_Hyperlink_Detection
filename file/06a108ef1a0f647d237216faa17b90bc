	   $(function(){
			if(isMD()){				
				var $padVideo='<video id="example_video_1" class="video-js vjs-default-skin" controls preload="none" width="854" height="396" poster='+g_videos_url+'/public/img/gti/downloadcenter/ad_d.jpg  data-setup="{}"><source src="';
				$padVideo+=g_videos_url+'gti/mp4/gti.mp4?ASDFAS" type="video/mp4" /></video>';
				$("#videocon").html($padVideo);
			}else{
				moviefirst();
				
			}
			$("#close").click(function(){
				$("#outercon").animate({"opacity":0},function(){$("#outercon").remove();});
				$("#cover").animate({"opacity":0},function(){$("#cover").remove();});
				$(this).remove();
			});
			
			$("#cover").height($(document.body).height()).width($(document.body).width())
		});
		
		var moviefirst=function(){
		$("#videocon").html('<div id="flashContent">&nbsp;</div>');
			<!-- For version detection, set to min. required Flash Player version, or 0 (or 0.0.0), for no version detection. --> 
		var swfVersionStr = "10.0.0";
		<!-- To use express install, set to playerProductInstall.swf, otherwise the empty string. -->
		var xiSwfUrlStr = g_videos_url+"old/babylon/playerProductInstall.swf";
		var flashvars = {};
		flashvars.src = g_videos_url + "downloadcenter/newjetta_30.mp4";                 //视频地址；
		flashvars.pic = "http://v.faw-vw-dns.com/downloadcenter/newjetta2016_800.png";                          //播放前的预览图，留空表示不使用；
		flashvars.onComplete = "myComplete";         //视频播放完毕后调用的js；
		flashvars.onError = "myError";               //视频错误调用的js；
		flashvars.onBuff = "myBuff";                 //视频缓冲调用的js；
		flashvars.onPlay = "myPlay";                 //视频开始播放调用的js；
		flashvars.autoPlay = "0";                    //是否自动播放 0 1；
		flashvars.autoReplay = "0";                  //是否循环播放 0 1；
		flashvars.allowFull="1";                     //是否有全屏按钮 0 1；
		flashvars.bufferTime="0";                    //视频缓冲时间，单位秒；
		flashvars.keepProportions = "1";             //是否保持缩放比例 0 1；
		flashvars.playBtnType = g_videos_url + "old/babylon/skin/play.swf";       //播放按钮等UI路径；
		flashvars.loadingType = g_videos_url + "old/babylon/skin/loading.swf";    //loading的UI路径；
		flashvars.videoList="";                      //视频列表，使用";"隔开，留空表示不使用；	
		flashvars.useDefinition = "0";               //是否启用清晰度选择	
		var params = {};
		params.quality = "high";
		params.bgcolor = "#000000";
		params.allowscriptaccess = "sameDomain";
		params.allowfullscreen = "true";
		params.scale = "noscale";
		params.wmode = "Opaque";
