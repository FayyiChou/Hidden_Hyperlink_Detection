        $(function () {
		$("#gzstr").val("");
            $("#nav-1").attr("class", "nav-1hover");
            //Tab切换
            $(function () {
                TabControl(".js_tab1", "site-starb", ".tab_2_body", "click");
                TabControl(".js_tab2", "site-starb", ".tab_3_body", "click");
                TabControl(".js_tab3", "site-starb", ".tab_4_body", "click");
                TabControl(".tab_1_nav", "star-gz-taba", ".tab_1_body", "click");
            })
            

            //图片滚动
            var speed = 20;
            var tab = document.getElementById("demo");
            var tab1 = document.getElementById("demo1");
            var tab2 = document.getElementById("demo2");
            tab2.innerHTML = tab1.innerHTML;
            function Marquee() {
                if (tab2.offsetWidth - tab.scrollLeft <= 0)
                    tab.scrollLeft -= tab1.offsetWidth
                else {
                    tab.scrollLeft++;
                }
            }
            var MyMar = setInterval(Marquee, speed);
            tab.onmouseover = function () { clearInterval(MyMar) };
            tab.onmouseout = function () { MyMar = setInterval(Marquee, speed) };
			
			var tab3 = document.getElementById("demo3");
            var tab4 = document.getElementById("demo4");
            var tab5 = document.getElementById("demo5");
            tab5.innerHTML = tab4.innerHTML;
            
			
			function Marqueet(){
flag=1;
if(tab3.scrollLeft<=0)
tab3.scrollLeft+=tab5.offsetWidth;
else{
tab3.scrollLeft--;
}
}

            var MyMart = setInterval(Marqueet, speed);
            tab3.onmouseover = function () { clearInterval(MyMart) };
            tab3.onmouseout = function () { MyMart = setInterval(Marqueet, speed) };
        })
        //预约工长量房弹窗
        function ShowBox(parm) {
            $("#gzid").val(parm);
            $(".get_box").show();
        }

        function CloseYY() {
            $(".get_box").hide();
            $("#gzid").val("");
            $(".get_box2").hide();
            $("#gdid").val("");
            $(".get_box_fuwu").hide();
            $(".StarDetail").hide();
            $("#overlay").hide();
            $(".Gzlist").hide();
        }

        //预约工地量房弹窗
        function ShowBox2(parm) {
            $("#gdid").val(parm);

            $(".get_box2").show();
        }
        function ShowBox3(type) {
            if (type == "fwzbj") {
                $("#fwzbj").show();
                $("#fwjl").hide();
                $("#fwjg").hide();
            }
            if (type == "fwjl") {
                $("#fwjl").show();
                $("#fwzbj").hide();
                $("#fwjg").hide();
            }
            if (type == "fwjg") {
                $("#fwjg").show();
                $("#fwzbj").hide();
                $("#fwjl").hide();
            }
        }
		function ShowStar() {
            $(".StarDetail").show();
        }
