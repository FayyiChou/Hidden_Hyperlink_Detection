        $("#smssend").click(function () {
            var mobile = $("#mobile").val();
            $.ajax({
                url: "/Ajax/Userhandle.ashx",
                type: "post",
                contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                data: { "type": "sms-l", "mobile": mobile },
                success: function (re) {
                    if (re == "1") {
                        alert("发送成功！");
                        fun_timedown(60);
                    } else if (re == "-1") {
                        alert("发送失败！");
                    } else if (re == "-2") {
                        alert("注册短信发送已经达到上限！");
                    }
                    else if (re == "-3") {
                        alert("抱歉，您尚未预约工长量房！稍后客服会联系您！");
                    }
                    else if (re == "-4") {
                        alert("手机号码格式不正确！");
                    }

                }

            });

        });
        function fun_timedown(time) {
            if (time == 'undefined')
                time = 60;
            $("#smssend").val("重新发送" + time + "秒");
            $("#smssend").attr("disabled", "disabled");
            time = time - 1;
            if (time >= 0) {
                setTimeout("fun_timedown(" + time + ")", 1000);
            } else {
                $("#smssend").removeAttr("disabled");
                $("#smssend").val("获取验证码");
            }
        }

        $(".star-gz-d img").hover(function () {
            var gzid = $(this).attr("id");
            var index = Number($(this).attr("index"));
            var hstr = "";
            var wstr = "";
            var X = $(this).offset().left;
            var Y = $(this).offset().top;
            hstr = "top:" + (Y+110 ) + "px";
            wstr = "left:" + (X - 190) + "px";
            

            var res = "";
            $.ajax({
                async: false,
                url: "/Ajax/Userhandle.ashx",
                type: "post",
                contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                data: { "type": "GetGzInfo", "gzid": gzid },
                success: function (re) {
                    $("#gzinfo").attr("style", "display:none;position: absolute;" + wstr + ";" + hstr);
                    $("#gzinfo").html(re);
                    $("#gzinfo").stop().fadeIn();

                }

            });
            return res;
        }, function () {
		$("#gzinfo").stop().fadeOut();

        });
		$("#gzinfo").hover(function () {
            $("#gzinfo").stop().fadeIn();
            
        },function() {
            $("#gzinfo").stop().fadeOut();
        });
