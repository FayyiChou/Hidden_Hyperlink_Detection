        function addOrder() {
            var oname = $("#OName").val();
            var omobile = $("#Tel").val();
            var community = $("#Community").val();
            var otype = 4;
            var squera = $("#Squera").val();
            var district = $("#District").val();
            if (oname.length == 0||oname=="您的姓名") {
                alert("您的称呼不能为空！");
                $("#OName").focus();
            }
            else if (omobile.length == 0||omobile=="您的手机号码") {
                alert("您的手机不能为空！");
                $("#Tel").focus();
            }
            else if (community.length == 0||community=="您的小区 如：保利心语") {
                alert("您的小区名不能为空！");
                $("#Community").focus();
            }
            else if (district == "0") {
                alert("请选择您的区域！");
            } else {
                $.ajax({
                    async: false,
                    url: "/Ajax/Userhandle.ashx",
                    type: "post",
                    contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                    data: { "type": "addorder-parm", "OName": oname, "OMobile": omobile, "Community": community, "OType": otype, "District": district },
                    success: function (re) {
                        var res = eval(re)[0];
                        if (res.status == "1") {
                            GetGZList(res.id, district);
                            $("#OName").val(""); $("#Tel").val(""); $("#Community").val(""); $("#Squera").val("");
                        } else if (res.status == "0") {
                            alert("预约失败");
                        }
                        else if (res.status == "-1") {
                            alert("预约数已达到上限");
                        }

                        else if (res.status == "-4") {
                            alert("您的手机号码格式不正确");
                            $("#Tel").focus();
                        }

                    }

                });
            }

        }
        function addGzOrder() {
            var oname1 = $("#OName1").val();
            var omobile1 = $("#OMobile1").val();
            var community1 = $("#OCommunity1").val();
            var gzid = $("#gzid").val();
            if (oname1.length == 0) {
                alert("您的称呼不能为空！");
                $("#OName1").focus();
            }
            else if (omobile1.length == 0) {
                alert("您的手机不能为空！");
                $("#OMobile1").focus();
            }
            else if (community1.length == 0) {
                alert("您的小区名不能为空！");
                $("#OCommunity1").focus();
            } else {
                $.ajax({
                    async: false,
                    url: "/Ajax/Userhandle.ashx",
                    type: "post",
                    contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                    data: { "type": "addorder", "OName": oname1, "OMobile": omobile1, "Community": community1, "OType": 1, "OID": gzid },
                    success: function (re) {
                        if (re == "1") {
                            alert("预约成功");
                            $(".get_box").fadeOut();
                            $("#OName1").val("");
                            $("#OMobile1").val("");
                            $("#OCommunity1").val("");

                        } else if (re == "0") {
                            alert("预约失败");
                        }
                        else if (re == "-1") {
                            alert("预约数已达到上限");
                        }

                        else if (re == "-4") {
                            alert("您的手机号码格式不正确");
                            $("#OMobile1").focus();
                        }

                    }

                });
            }

        }
