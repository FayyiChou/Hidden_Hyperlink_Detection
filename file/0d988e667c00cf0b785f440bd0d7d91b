        function UserOblur() {
            var mobile = $("#mobile").val();
            var res = "";
            $.ajax({
                async: false,
                url: "/Ajax/Userhandle.ashx",
                type: "post",
                contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                data: { "type": "adduser", "mobile": mobile },
                success: function (re) {
                    res = re;
                }

            });
            return res;

        }

        function MsgLogin() {
            var res = true;
            var mobile = $("#mobile").val();
            var yzm = $("#checkcode").val();
            if (mobile.length == 0) {
                alert("手机不能为空");
                res = false;
            }
            else
                if (yzm.length == 0) {
                    alert("验证码不能为空");
                    res = false;
                }
            return res;

        }

        function AddGzToOrder(owid) {
            var gzstr = $("#gzstr").val();
            if (gzstr.length == 0) {
                alert("您暂未选择任何工长！");
            } else {
                $.ajax({
                    async: false,
                    url: "/Ajax/Userhandle.ashx",
                    type: "post",
                    contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                    data: { "type": "AddGzToOrder", "owid": owid, "SelectGZ": gzstr },
                    success: function (re) {
                        if (re == "1") {
                            alert("预约成功！");
                            $(".Gzlist").hide();
                            $("#overlay").hide();
                            $("#District").val("");
                        } else {
                            alert("预约失败！");
                        }
                    }

                });
            }
            
        }

        function GetGZList(owid, district) {
            var res = "";
            
            $.ajax({
                async: false,
                url: "/Ajax/Userhandle.ashx",
                type: "post",
                contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                data: { "type": "GetGzList", "owid": owid, "district": district },
                success: function(re) {
                    res = eval(re);
                }

            });
            var str = "<div class=\"choose-title\"><a class=\"loginclose\" onclick=\"CloseYY()\" style=\"cursor: pointer;\"></a><div class=\"t-left\">请选择2位该区域工长为您服务:</div><div class=\"t-right\"><a href=\"javascript:AddGzToOrder('" + owid + "')\">请选择工长！</a></div></div><div class=\"choose-gz-c\"><ul>";
            if (res.length == 0) {
                str += "该区域暂无工长";
            } else {
                for (var i = 0; i < res.length; i++) {
                    if (i%8 == 7 ) {
                        str += "<li style=\"width:110px\">";
                    } else {
                        if (res.length > 23) {
                            str += "<li style=\"width:121px\">";
                        } else {
                            str += "<li>";
                        }
                    }
                    str += "<a id=\"gz-" + res[i].ID + "\" onclick=\"ChooseGZ('" + res[i].ID + "','" + res[i].Name + "')\"><div class=\"choose-gz-d\">";
                    str += "<img onerror=\"this.onerror=null;this.src=\'/Member/UpControl/upload/avatars/noavatar_medium.gif';\" src=\"/UpLoad/Logo/avatars/" + res[i].ID + "/medium.jpg\" alt=\"" + res[i].Name + "\" >";
                    str += "</div>";
                    str += "<span>" + res[i].Name + "</span> <span><em>曾任职：" + (res[i].Company != null ? res[i].Company.replace("，", " ").replace("/", " ").replace(";", " ") : "") + "</em></span>";
                    str += "</li></a>";
                }
            }
            str += "</ul></div>";
			$("#overlay").show();
            $(".Gzlist").html(str);
            $(".Gzlist").show();
        }

        function ChooseGZ(gzid, gzname) {
            var str = $("#gzstr").val();
            var list = str.split(',');
            if (str.indexOf("," + gzid + ",") > -1) {
                $("#gz-" + gzid).removeAttr("style");
                $("#gz-" + gzid).removeAttr("title");
                $("#gz-" + gzid).find("span").removeAttr("style");
                $("#gz-" + gzid).find("em").removeAttr("style");
                str = str.replace("," + gzid + ",", ",");
                if (str == ",") {
                    str = "";
                }
            } else {
                if (list.length < 4) {
                    $("#gz-" + gzid).attr("style", "background:#89CBAA;");
                    $("#gz-" + gzid).find("span").attr("style", "color:#fff");
                    $("#gz-" + gzid).find("em").attr("style", "color:#fff");
                    $("#gz-" + gzid).attr("title", "左键单击取消选择");
                    if (str.length > 0) {
                        str = str + gzid + ",";
                    } else {
                        str = str + "," + gzid + ",";
                    }
                } else {
                    alert("您已选满，请提交！");
                }
                
            }
            $("#gzstr").val(str);

            if (str.length==0) {
                $(".t-right a").attr("style", "background:#bbb");
                $(".t-right a").html("请选择工长！");
            } else {
                $(".t-right a").attr("style", "background:#F78418");
                $(".t-right a").html("已选好！");
            }

        }
