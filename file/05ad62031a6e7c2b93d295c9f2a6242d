function myAjaxResultFrame_onResult(){
	var myFrame = document.getElementsByName('myAjaxResultFrame')[0];
	//var strReturn;
	var text = myFrame.contentWindow.document.body.innerText;
	if (text=="")
	{
	
	}
	else
	{
		
		document.getElementById("INFO_DT").innerHTML=text.substr(0,text.indexOf("|"));
		document.getElementById("INFO_LXR1").innerHTML=text.substr(text.indexOf("|")+1,text.indexOf("--")-text.indexOf("|")-1);
		//document.getElementById("INFO_LXR2").innerHTML=text.substr(text.indexOf("|")+1,text.indexOf("--")-text.indexOf("|")-1);
		//document.getElementById("INFO_ZW").innerHTML=text.substr(text.indexOf("--")+2,text.length);
		
	}
}
function myAjaxResultFrame2_onResult(){
	var myFrame = document.getElementsByName('myAjaxResultFrame2')[0];
	//var strReturn;
	var text = myFrame.contentWindow.document.body.innerText;
	if (text=="")
	{
	
	}
	else
	{
		//strReturn=text
		document.getElementById("INFO_TEL").innerHTML=text.substr(0,text.indexOf("#1|#"));
		document.getElementById("INFO_FAX").innerHTML=text.substr(text.indexOf("#1|#")+4,text.indexOf("#2|#")-text.indexOf("#1|#")-4);
		document.getElementById("INFO_ADDS").innerHTML=text.substr(text.indexOf("#2|#")+4,text.indexOf("#3|#")-text.indexOf("#2|#")-4);
		document.getElementById("INFO_POST").innerHTML=text.substr(text.indexOf("#3|#")+4,text.indexOf("#4|#")-text.indexOf("#3|#")-4);
		document.getElementById("INFO_HOMEPAGE").innerHTML=text.substr(text.indexOf("#4|#")+4,text.length);
	}
}
