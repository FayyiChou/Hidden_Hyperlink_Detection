<script>

var time_flag=1;
var locationInterval;
	
	$(function(){
				setting_location();	
		
		locationInterval = window.setInterval(function(){
		check_location_status();
			time_flag++;
		},3000);
	})


function check_location_status(){
	$.getJSON("http://www.yun0001.com/index.php?c=wxlocation&a=check_status",{qrcode_id:'400095076'},function(res){
		//alert(res.errmsg);
		if(res.errmsg == 'scan_ok'){
			$('.tankuang_txt_left').html('您已经扫码二维码，请发送您的<br/>位置！');
		} else if(res.errmsg == 'to_mobile_bind') {
			$('.tankuang_txt_left').html('请前往微信端绑定用户信息哦！');
		}else if(res.errmsg == 'location_ok'){
			$('.tankuang_txt_left').html('获取位置并登录成功,<br/>页面正在跳转！');
			clearInterval(locationInterval);
			location.reload();
		} else if(res.errmsg == 'no_power_to_login') {
			$('.tankuang_txt_left').html('您已被账户禁止登陆！！');
			clearInterval(locationInterval);
		}else if(res.errmsg == 'false'){
			clearInterval(locationInterval);
		}
	});
}

function setting_location() {

	$(".tankuang_content").hide();
	//$(".chenpage").show();
	//$(".tankuang4").show();
}
</script>

