		<script type="text/javascript" charset="utf-8">
			mui.init({
				swipeBack : true
			//启用右滑关闭功能
			});
			loadpoint();
			var slider = mui("#slider");
			slider.slider({
				interval : 3000
			});
			
			function loadpoint() {
					//在这里加载数据
					$.ajax({
						type : "POST",//使用get方法访问后台
						dataType : "json",//返回json格式的数据
						url : "/TopMallWeb/mobCustInfoAction!queryAccountBonus.action?ramid="+ Math.random(),//要访问的后台地址
						data:{
							
						},
						success : function(data) {//msg为返回的数据，在这里做数据绑定
								var cardpoints = data.sumbonus;
								
								var html='';
								$("#totalpoint").html(cardpoints);
						},
					});
				}
			
			function searchpage(){
				var activeType=$('#activeType').val();
				window.location.href='/TopMallWeb/mobileGoodsAction!searchGoods.action?activeType='+activeType;
			}
		</script>
	

