						})();
						
						function fillPage(index, url, img){
							var ul = $(".logo-links");
							if(index % 5 == 0){
								ul.append("<li><a href='" + url + "' target=\_blank><img src='" + img + "''></a></li>");
							}else{
								ul.find("li:last").append("<a href='" + url + "' target=\_blank><img src='" + img + "''></a>");
							}
						}
					</script>
					<ul id="news-indicator"></ul>
					<script type="text/javascript">
						(function(){
							var auto = true, hoverStop = true;
							var interval = 5000;
							var count = 1, curr = 0;
							var timer;
							var ul = $("#logo-links");
							var list = ul.find(">li");
							count = Math.min(count, list.length);
							var height = list.find(">a").height()+30;
							var pages = Math.ceil(list.length / count);
							var wrap = $("<div>").css({
								position: "relative",
								overflow: "hidden",
								height: height * count + "px"
							});
							ul.wrap(wrap).css({
								position: "absolute"
							});
							var go = function(page) {
								stop();
								var page = page || 0;
								curr = page;
								ul.stop(true, true).animate({top: - page * count * height}, 300);
								if(indicators) {
									indicators.removeClass("focus");
									indicators.eq(page).addClass("focus");
								}
								start();
							}
							var next = function(){
								curr += 1;
								if(curr == pages) curr = 0;
								go(curr);
							}
							var start = function(){
								if(auto)
									timer = setTimeout(next, interval);
							}
							var stop = function(){
								if(timer) {
									clearTimeout(timer);
									timer = null;
								}
							}
							if(hoverStop)
								ul.mouseenter(stop).mouseleave(start);
							if(pages > 1) {
								var indicatorWrap = $("#news-indicator");
								for(var i=0;i<pages;i++) {
									indicatorWrap.append($("<li>"));
								}
								var indicators = indicatorWrap.find(">li");
								indicators.click(function(){
									go($(this).index());
								});
								go();
							}
						})();
					</script>
