<script type="text/javascript">
	var window_top = document.getElementById("float_window").offsetTop;
	var locateWindow = function(){
		if(document.documentElement.scrollTop+document.body.scrollTop > 400){
	        if(!(/AppleWebKit.*Mobile/i.test(navigator.userAgent) || (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent)))){
	            document.getElementById("float_window").style.top = window_top + (document.documentElement.scrollTop+document.body.scrollTop - 400) + "px";
	        }
	    }else{
	    	 document.getElementById("float_window").style.top = window_top + "px";
	    }
	};
	
	$(window).scroll(function(){
		locateWindow();
    });
</script>
