<script type="text/javascript" src="js/jquery-1.4.2.min.js"></script>
<script type="text/javascript">
 var $d = jQuery.noConflict();
$d(document).ready(function(){

	$d("#floatShow").bind("click",function(){
	
		$d("#onlineService").animate({width:"show", opacity:"show"}, "normal" ,function(){
			$d("#onlineService").show();
		});
		
		$d("#floatShow").attr("style","display:none");
		$d("#floatHide").attr("style","display:block");
		
		return false;
	});
	
	$d("#floatHide").bind("click",function(){
	
		$d("#onlineService").animate({width:"hide", opacity:"hide"}, "normal" ,function(){
			$d("#onlineService").hide();
		});
		
		$d("#floatShow").attr("style","display:block");
		$d("#floatHide").attr("style","display:none");
		
		return false;
	});
  
});
</script>


