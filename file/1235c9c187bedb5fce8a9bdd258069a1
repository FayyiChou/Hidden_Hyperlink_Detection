    (function(){
        // 如果base.js 或者module.js 或者功能module 没加载的话       
        if(!MOGU){

            var nowDate = new Date(),
                imgloader = new Image();

            //启动一个加载图片的进程，来判断当前用户的网速
            // 图片大小 22KB
            imgloader.onload = function(){

                var endTime = new Date(),
                    loadTime = endTime.getTime() - nowDate.getTime();

                //拼装地址
                var post_url = ['http://www.mogujie.com/mbs.htm?sform=',window.location.href,'&imgloadTime=',loadTime,'&serivetime=',1491055055].join('');
                //发送请求
                new Image().src = post_url;

            }

