<script type="text/javascript">
jQuery(function(){
/*瀑布流开始*/
var container = jQuery('.waterfull ul');
var loading=jQuery('#imloading');
// 初始化loading状态
loading.data("on",true);
/*判断瀑布流最大布局宽度，最大为1280*/
function tores(){
var tmpWid=jQuery(window).width();
if(tmpWid>1200){
tmpWid=1200;
}else{
var column=Math.floor(tmpWid/400);
tmpWid=column*400;
}
jQuery('.waterfull').width(tmpWid);
}
tores();
jQuery(window).resize(function(){
tores();
});
container.imagesLoaded(function(){
  container.masonry({
  	columnWidth: 400,
    itemSelector : '.item',
    isFitWidth: true,//是否根据浏览器窗口大小自动适应默认false
    isAnimated: true,//是否采用jquery动画进行重拍版
    isRTL:false,//设置布局的排列方式，即：定位砖块时，是从左向右排列还是从右向左排列。默认值为false，即从左向右
    isResizable: true,//是否自动布局默认true
    animationOptions: {
duration: 800,
easing: 'easeInOutBack',//如果你引用了jQeasing这里就可以添加对应的动态动画效果，如果没引用删除这行，默认是匀速变化
queue: false//是否队列，从一点填充瀑布流
}
  });
});

jQuery(window).scroll(function(){
if(!loading.data("on")) return;
// 计算所有瀑布流块中距离顶部最大，进而在滚动条滚动时，来进行ajax请求，方法很多这里只列举最简单一种，最易理解一种
var itemNum=jQuery('#waterfull').find('.item').length;
var itemArr=[];
itemArr[0]=jQuery('#waterfull').find('.item').eq(itemNum-1).offset().top+jQuery('#waterfull').find('.item').eq(itemNum-1)[0].offsetHeight;
itemArr[1]=jQuery('#waterfull').find('.item').eq(itemNum-2).offset().top+jQuery('#waterfull').find('.item').eq(itemNum-1)[0].offsetHeight;
itemArr[2]=jQuery('#waterfull').find('.item').eq(itemNum-3).offset().top+jQuery('#waterfull').find('.item').eq(itemNum-1)[0].offsetHeight;
var maxTop=Math.max.apply(null,itemArr);
if(maxTop<jQuery(window).height()+jQuery(document).scrollTop()){
//加载更多数据
loading.data("on",false).fadeIn(800);
(function(sqlJson){
/*这里会根据后台返回的数据来判断是否你进行分页或者数据加载完毕这里假设大于30就不在加载数据*/
if(itemNum>30){
loading.text('就有这么多了！');
}else{
var html="";
for(var i in sqlJson){
html+="<li class='item'><a href='http://www.datouwang.com/' class='a-img'><img src='"+sqlJson[i].src+"'></a>";
html+="<h2 class='li-title'>"+sqlJson[i].title+"</h2>";
html+="<p class='description'>"+sqlJson[i].intro+"</p><div class='qianm clearfloat'>";
html+="<span class='sp1'><b>"+sqlJson[i].looked+"</b>浏览</span>";
html+="<span class='sp2'>"+sqlJson[i].writer+"</span><span class='sp3'>"+sqlJson[i].date+"&nbsp;By</span></div></li>";		
}
/*模拟ajax请求数据时延时800毫秒*/
var time=setTimeout(function(){
jQuery(html).find('img').each(function(index){
loadImage(jQuery(this).attr('src'));
})
var jQuerynewElems = jQuery(html).css({ opacity: 0}).appendTo(container);
jQuerynewElems.imagesLoaded(function(){
jQuerynewElems.animate({ opacity: 1},800);
container.masonry( 'appended', jQuerynewElems,true);
loading.data("on",true).fadeOut();
clearTimeout(time);
        });
},800)
}
})(sqlJson);
}
});
function loadImage(url) {
     var img = new Image(); 
     //创建一个Image对象，实现图片的预下载
      img.src = url;
      if (img.complete) {
         return img.src;
      }
      img.onload = function () {
       	return img.src;
      };
 };
 loadImage('images/one.jpeg');
/*item hover效果*/
var rbgB=['#71D3F5','#F0C179','#F28386','#8BD38B'];
jQuery('#waterfull').on('mouseover','.item',function(){
var random=Math.floor(Math.random() * 4);
jQuery(this).stop(true).animate({'backgroundColor':rbgB[random]},1000);
});
jQuery('#waterfull').on('mouseout','.item',function(){
jQuery(this).stop(true).animate({'backgroundColor':'#fff'},1000);
});
})

jQuery(function() {
//焦点轮播
  jQuery(".slide").slide({
  mainCell:"#slideImg",
  titCell:"#slideBtn a",
  effect:"fold", //切换效果
  autoPlay:true,
  interTime:1500
  });
  
jQuery(".picScroll").slide({ mainCell:".picList", effect:"left",vis:3, pnLoop:false, scroll:3, autoPage:true});
   //高清视觉轮播
  jQuery(".visual_lunbo").slide({
  mainCell:"#Visual",
  effect:"left" //切换效果
  //autoPlay:true,
  //interTime:8000
  });
  

initBackTop()//初始化返回顶部按钮事件
        
        jQuery(".hd_picture li").mouseenter(function(){
jQuery(this).find('div').show();
jQuery(this).find('span').hide();
});

jQuery(".hd_picture li").mouseleave(function(){
jQuery(this).find('div') .hide();
jQuery(this).find('span').show();
});

                                                 
})

//初始化返回顶部按钮事件
function initBackTop(){
window.onscroll=function(){
if(document.body.scrollTop==0?document.documentElement.scrollTop:document.body.scrollTop > 50){
jQuery("#backTop").show();
}else{
jQuery("#backTop").hide();
}
}

jQuery("#backTop").unbind("click").bind("click",function(){
    pageScroll();
});
}
//鼠标滚动事件  
function pageScroll(){
    window.scrollBy(0,-50);
    scrolldelay = setTimeout('pageScroll()',1);
    var sTop=document.documentElement.scrollTop+document.body.scrollTop;
    if(sTop==0){
clearTimeout(scrolldelay);
}
}
</script>
