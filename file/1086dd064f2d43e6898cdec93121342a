        url:'/nx/iframe/prices/',
        type: "GET",
        dataType: 'json',
        timeout: 5000,
        success:function(json){
          if(json.code != '0') {
          //alert(json.errorDescription);
          return;
          }
          $.each(json.dataObject,function(i,n){
           if(n.className) {
            $("."+n.className).text(n.price);
        }
            });
          },
        error:function(xhr){
           //alert("请求出错(请检查相关度网络状况.)");
           }
        });



