		var url = "http://www.greenyark.com/include/search_menu_autocomplate.php";
	
			$( "#skey3" ).autocomplete({
				source: url,
				autoFocus: true,
				focus: function(event, ui) {
					//$(this).val(ui.item.label);
					return false;
				},
				select: function( event, ui ) {
					var value=ui.item.value;
					var label=ui.item.label;
					
					$(this).val(ui.item.label);
					if( value!=label ){
						window.location="http://www.greenyark.com/product/detail"+value;
					}
					else{
						document.getElementById("searchbanner").submit();
					}
					return false;
				}
			});
</script>




