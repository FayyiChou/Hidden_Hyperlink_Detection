<script type="text/javascript">
  // Get the modal
var modal = document.getElementById('myModal');
var modal1 = document.getElementById('myModal1');
var modalphoto_id = document.getElementById('myModalphoto_id');
// Get the button that opens the modal
var btn = document.getElementById("myBtn");
var btn1 = document.getElementById("name_badge");
var btnphoto_id = document.getElementById("photo_id")
// var btn2 = document.getElementById("photo_id");
// Get the <span> element that closes the modal
var span = document.getElementsByClassName("close")[0];
var span1 = document.getElementsByClassName("close1")[0];
var span2 = document.getElementsByClassName("close2")[0];

// When the user clicks on the button, open the modal 
btn.onclick = function() {
    modal.style.display = "block";
}

btn1.onclick = function() {
    modal1.style.display = "block";
    modal.style.display = "none";
}
btnphoto_id.onclick = function(){
	modalphoto_id.style.display = "block";
    modal.style.display = "none";
}
span.onclick = function() {
    modal.style.display = "none";
}
span1.onclick = function() {
    modal1.style.display = "none";

}
span2.onclick = function() {
    modalphoto_id.style.display = "none";
}

// btn2.onclick = function() {
//     modal1.style.display = "block";
//     modal.style.display = "none";
// }

// When the user clicks on <span> (x), close the modal
// span.onclick = function() {
//     modal.style.display = "none";
// }

// When the user clicks anywhere outside of the modal, close it

// window.onclick = function(event) {
//     if (event.target == modal1) {
//         modal1.style.display = "none";
//     }
// }
window.onclick = function(event) {
	
	if(window.location.href.search("index.php?route=common/home") != -1 || window.location.href == "https://www.namebadgenow.com/") {
    if (event.target == modal) {
        modal.style.display = "none";
    }
    else if (event.target == modalphoto_id) {
        modalphoto_id.style.display = "none";
    } 
    else if (event.target == modal1) {
        modal1.style.display = "none";
    } 
    else if (event.target == modal2) {
        modal1.style.display = "none";
    } 
	}
}
$(document).ready(function(){
	
    $(".name_badge").click(function(){
      $(".name_badge_check").css("display", "inline-block");
    });

    $(".photo_id").click(function(){
      $(".photo_id_check").css("display", "inline-block");
    });
    
});
function checkQty()
{
  /*if(document.getElementById('quantity').value == '')
  {
    document.getElementById('qtymsg').innerHtml ='Please enter quantity.';
  }
  else if(document.getElementById('quantity').value == '')
  {
    document.getElementById('qtymsg').innerHTML ='Please enter quantity.';
  }*/
  var qty = $('#quantity').val();
  if(qty == ''){
    $('#qtymsg').html('Please enter a quantity.');
  }
  else if(isNaN(qty)){
    $('#qtymsg').html('Please enter numeric value.');
  }
  else if (qty <= 0)
  {
    $('#qtymsg').html('Please enter a quantity greater than zero.');
  }
  else
  {
    window.location = 'https://www.namebadgenow.com/index.php?route=design/design&quantity='+qty;
  }
}
function isNumber(n) {
  return !isNaN(parseFloat(n)) && isFinite(n);
}
</script>

   
