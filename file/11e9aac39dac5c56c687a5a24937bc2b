<script type="text/javascript">
function CScol4Wx(){
	var $target = $wn('#csCol4wx');
	if(csCol4wxConfig.wxLinks.length > 0){
		var toAdd = "";
		for(var aa=0;aa<csCol4wxConfig.wxLinks.length;aa++){
			toAdd += "<dd><a href='" + csCol4wxConfig.wxLinks[aa][1] + "' target='" + csCol4wxConfig.wxLinks[aa][2] + "'>" + csCol4wxConfig.wxLinks[aa][0] + "</a></dd>";
		}
		$target.find('dl').append(toAdd);	
	}

	CSWxManager.setOption('iconSize', 40);
	// CSWxManager configuration is in custom.js
	CSWxManager.ready(function(wx) {
    	if (wx.length === 0) {
    		console.log('returns nothing');
      		return;
    	}
		
		function rotate() {
			// Specific to the MyWeather XML format
    		var hi = wx.fetch('high');
    		var lo = wx.fetch('low');
    		$target = $wn('#csCol4wx .wxInfo');
			$target.find('.temp').html(wx.fetch('temperature') + '&deg;');
			$target.find('.wxIcon').css('background-image', 'url("' + wx.fetch('iconurl') + '")');
			$target.find('.loc').html('<strong>' + wx.fetch('name') + '</strong>');
			$target.find('.condition').html(wx.fetch('sky'));
      		$target.find('.lo').html('Lo&nbsp;' + lo + '&deg;');
			$target.find('.high').html('Hi&nbsp;' + hi + '&deg;');
      		wx.next();
    	}

		rotate();
    	setInterval(rotate, 5000);
  	});  	
}
CScol4Wx();
</script>	</div><script>Worldnow.Helpers.debug('UB Redesign 2012 - Col. 4 Weather block - KCTV(241314)', 'UB_241314');</script><script>Worldnow.Helpers.debug('UB Col. 4A height buster(219028)', 'UB_219028');</script><div class="wnDVUtilityBlock wnEven"><style type="text/css">
