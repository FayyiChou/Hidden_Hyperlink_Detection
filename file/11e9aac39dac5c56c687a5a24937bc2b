function CScol4Atreatment(displaySizeId){
	$source = $wn("#DisplaySizeId" + displaySizeId);
	$source.find(".feature:gt(2)").hide();
	$source.find(".header .more").show();
	if($wn("#WNAd43").length > 0){
		$source.insertBefore($wn("#WNAd43"));
	} else {
		return;
	}
}

