   function Show_YXCCC(id) {
        if (id == 0) {
            $("#b_yx_sign0").addClass("rt activeon");
            $("#yx_sign0").show();

            $("#b_yx_sign1").removeClass("activeon");
            $("#b_yx_sign2").removeClass("activeon");
            $("#yx_sign1").hide();
            $("#yx_sign2").hide();
        }
        if (id == 1) {
            $("#b_yx_sign1").addClass("activeon");
            $("#yx_sign1").show();

            $("#b_yx_sign0").removeClass("activeon");
            $("#b_yx_sign2").removeClass("activeon");
            $("#yx_sign0").hide();
            $("#yx_sign2").hide();
        }
        if (id == 2) {
            $("#b_yx_sign2").addClass("activeon");
            $("#yx_sign2").show();

            $("#b_yx_sign0").removeClass("activeon");
            $("#b_yx_sign1").removeClass("activeon");
            $("#yx_sign0").hide();
            $("#yx_sign1").hide();
        }
    }
</script>

