	function FP_jumpMenu(el,frm,sel) {
      var href=el.options[el.selectedIndex].value; 
	  if(href=='')
		return false;
	  if(sel) 
		el.selectedIndex=0;
      if('_new'==frm) 
		open(href); 
	  else 
		eval(frm+".location='"+hrefb+"'");
    }
