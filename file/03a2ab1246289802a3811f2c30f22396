var TOGGLENAV = TOGGLENAV || [];
TOGGLENAV.$ = function(el){
return document.getElementById(el);
};
TOGGLENAV.getNextSibling = function(el){
if(null == el || null == el.nextSibling){ return false; }
if(1 == el.nextSibling.nodeType){
return el.nextSibling;
} else {
return TOGGLENAV.getNextSibling(el.nextSibling);
}
};
TOGGLENAV.toggleList = function(el){
var dd;
if('fold_tree' == el.className){
el.className = 'expand';
dd = TOGGLENAV.getNextSibling(el);
while('DD' == dd.tagName){
dd.style.display = 'block';
dd = TOGGLENAV.getNextSibling(dd);
}
} else if('expand' == el.className){
el.className = 'fold_tree';
dd = TOGGLENAV.getNextSibling(el);
while('DD' == dd.tagName){
dd.style.display = 'none';
dd = TOGGLENAV.getNextSibling(dd);
}
}
};
TOGGLENAV.adjustFooter = function() {
var footer = TOGGLENAV.$('ft'),
footerHeight = footer.offsetHeight,
_x = TOGGLENAV.tempEl1.offsetHeight - (document.body.offsetHeight - 40),
_tmp;
if ( _x > 0 ) {
footer.style.height = footerHeight + _x + 'px';
} else if (footer.style.height != 'auto') {
footer.style.height = 'auto';
_tmp = footer.offsetHeight;
TOGGLENAV.adjustFooter();
}
};

//模拟onmouseenter和onmouseleave
TOGGLENAV.addEvent = function(el, type, handler, capture){
if (typeof el.addEventListener != 'undefined') {
if (type === 'mouseenter') {
el.addEventListener('mouseover', TOGGLENAV.withinElement(handler), capture);
} else if (type === 'mouseleave') {
el.addEventListener('mouseout', TOGGLENAV.withinElement(handler), capture);
} else {
el.addEventListener(type, handler, capture);
}
} else if (typeof el.attachEvent != 'undefined') {
el.attachEvent('on' + type, handler);
} else {
el['on' + type] = handler;
}
};
TOGGLENAV.withinElement = function(handler){
return function(e){
var parent = e.relatedTarget;
while ( parent && parent != this ) {
try { parent = parent.parentNode; }
catch(e) { break; }
}
if ( parent != this )
handler.call(this, e);
};
};

