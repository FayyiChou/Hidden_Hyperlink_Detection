        <script>
            // 初始化皮肤样式
            (function () {
                var skin_stylesheet = document.getElementById('skin-stylesheet');
                var head = document.getElementsByTagName('head')[0];
                var skin_id = (function () {
                    var cookies = document.cookie.split(';');
                    var cookie;
                    var cookie_items;

                    for (var i = 0; i < cookies.length; i++) {
                        cookie = (function (s) {
                            return s.replace(/^\s+/, '').replace(/\s+$/, '');
                        })(cookies[i]);

                        cookie_items = cookie.split('=');

                        if (cookie_items.length === 2 && cookie_items[0] === 'skin') {
                            return cookie_items[1];
                        }
                    }

                    return 'default';
                })();

                skin_stylesheet = document.createElement('link');
                skin_stylesheet.id = 'skin-stylesheet';
                skin_stylesheet.rel = 'stylesheet';
                skin_stylesheet.type = 'text/css';
                skin_stylesheet.setAttribute('skin', skin_id);
                skin_stylesheet.href = COMBO_DOMAIN + '/combo/' + TIMESTAMP + '?public/layout/css/skins/' + skin_id + '.css';

                head.appendChild(skin_stylesheet);
            })();
        </script>
