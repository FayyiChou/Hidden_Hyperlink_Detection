			var str = url.substr(1);
			webid = str.split('=')[1];
			if (webid != 'web' ){
				if(navigator.userAgent.match(/Android|iPhone|iPad/i)) {
					window.location.href = 'http://m.51uban.com/';
				}
			}
		}
