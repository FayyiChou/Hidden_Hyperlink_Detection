<script>

var d="795";
var r="";
var v1="";
var v2="";
var v3="";
var v4="";
var v5="";
var v6="";
var v7="";
var v8="";
var v9="";
var v10="";
var v11="";
var v12="";
var t3="shop_id=86815&";
var tmp="";
var newurl="v1=&v2=&v3=&v4=&v5=&v6=&v7=&v8=&v9=&v10=&v11=&v12=&s0=&t2=0&t4=0&t5=2&t6=&t7=0&t8=0&t9=&t10=&z=0&p=0&v=0&u=1&y=2&o=o";
var v_value="";
var color="";
var url="";
var isSel=0;
var texts="";
for(var i1=0;i1<m.length;i1++){
	tmp="<div class=\"Pinfo\" onmouseover=\"this.style.background='#F7F7F7'\" onmouseout=\"this.style.background=''\">"
	tmp+="<div class=\"Pname\"><b><font color=\"#006666\">"+m[i1][2]+"</font></b>："
	var sorder=m[i1][0];
	if(sorder<=12){
		v_value=eval("v"+sorder);
	}else{
		v_value="";
	}
	
	url=newurl.replace("v"+sorder+"="+v_value,"v"+sorder+"=");
	if (v_value!=""){
		color=" style=\"color:blue;\"";
	}else{
		color=" style=\"color:white;\"";
	}	
	
	tmp+=" <a href=\"?"+t3+"d="+d+"&mz=&r="+r+"&"+url+"\" "+color+">全部</a>";
	try{
		if(m[i1].length>0)
		{
			isSel=0;
			tmp+=getArr2(i1,v_value,sorder);
		}
	}catch(err){}	
	
	
	texts+=tmp;
}


document.getElementById("property").innerHTML=out_table(texts);

function getArr2(index1,v_value,sorder){
	var valueID=0;
	var tmp="";
	var color="";
	var url="";
	var next_str="";
	
	for(var i2=3;i2<m[index1].length;i2++){
		valueID=m[index1][i2][0];	
		url=newurl.replace("v"+sorder+"="+v_value,"v"+sorder+"="+valueID);		
		
		if (v_value==valueID){
			color=" style=\"color:red;\"";
			isSel=1;
			try{	
			if(m[index1][i2][2]==1){
				next_str=getArr3(index1,i2,v_value,sorder,isSel);	
				}
			}catch(err){}			
		}else{
			color="";
		}	
		
		if(isSel==0){
			try{	
			if(m[index1][i2][2]==1){			
				next_str=getArr3(index1,i2,v_value,sorder,isSel);
				if(next_str!="")isSel=1;	
				}
			}catch(err){}
		}

		tmp+=" <a href=\"?"+t3+"d="+d+"&mz=&r="+r+"&"+url+"\" "+color+">"+m[index1][i2][1]+"</a>";
	}	
	return tmp+"</div></div>"+next_str;	
}

function getArr3(index1,index2,v_value,sorder,isSel){
	var valueID=0;
	var tmp="";
	var color="";
	var url="";
	var next_str="";	

	for(var i3=4;i3<m[index1][index2].length;i3++){		
		valueID=m[index1][index2][i3][0];
		url=newurl.replace("v"+sorder+"="+v_value,"v"+sorder+"="+valueID);
		if (v_value==valueID){
			color=" style=\"color:red;\"";
			isSel=2;
		}else{
			color="";
		}
	
		tmp+=" <a href=\"?"+t3+"d="+d+"&mz=&r="+r+"&"+url+"\" "+color+">"+m[index1][index2][i3][1]+"</a>";
	}

	if(isSel>0&&tmp!=""){
		return "<div class=\"Pvalue\">"+tmp+"</div>";
	}else{
		return "";
	}

}

function out_table(str){
	var tmp="";
	tmp+="<table width=\"100%\" align=\"center\"  border=\"0\" cellpadding=\"0\" cellspacing=\"0\" style=\"clear:both;\">";
	tmp+="  <tr style=\"background:#FFF;\">";
	tmp+="    <td height=\"20\" align=\"left\" id=\"category\"> ";	
	tmp+=str;	
	tmp+="</td> ";
	tmp+="  </tr> ";
	tmp+="</table> ";
	tmp+="	<div style=\"margin:auto;width:100%;clear:both;height:6px;overflow:hidden;\"></div> ";
	return tmp;
}


