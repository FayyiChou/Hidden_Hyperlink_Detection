<script>
dplus.define('page', function (page) {
    		page.setType('pc_play');
    		page.setId(PlayVideoInfo.vid)
    		page.setTitle(PlayVideoInfo.title);
    		page.setCategory(PlayVideoInfo.category);
    		page.setAuthor(PlayVideoInfo.authorid);
    		
    		var date = new Date();
			date.setHours(0,0,0,0);
			var inToday=date.getTime();
			var thisDay=date.getDay();
			date.setDate(date.getDate()-thisDay+1);
			var inWeekday=date.getTime();
			date.setDate(date.getDate()+thisDay-1);
			date.setDate(1);
			var inMonth=date.getTime();
    		var editor="";//发布时间占比
    		if(PlayVideoInfo.pubTime > inToday){
    			editor="当天";
    		}else if(PlayVideoInfo.pubTime > inWeekday){
    			editor="本周";
    		}else if(PlayVideoInfo.pubTime > inMonth){
    			editor="本月";
    		}else{
    			var pubDate=new Date(PlayVideoInfo.pubTime);
    			var m=pubDate.getMonth()+1;
    			editor=pubDate.getFullYear()+"-"+(m<10?"0"+m:m);
    		}
    		page.setEditor(editor);
    		page.view();
});
</script>
