      <!--资讯标题--> 
      <div class="title"> 
       <h1>2015山东济宁各级机关及直属单位考录公务员公告</h1> 
      </div> 
      <!--资讯标题--> 
      <!--相关信息--> 
      <div class="info"> 
       <div class="baidu_div"> 
        <div id="bdshare" class="bdshare_b" style="line-height: 12px;">
