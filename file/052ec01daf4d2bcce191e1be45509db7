        var pcount = $($(".popup .pop_box")).lenght;


        $(".popup").each(function (i) {
            $(this).mouseover(function () {
                $(".popup .pop_box").hide();
                $($(".popup .pop_box")[i]).toggle();
            });

            $(".max_popup").mouseout(function () {
                $(".popup .pop_box").hide();
            });

            $(this).hover(function () {
                clearTimeout(s);

            }, function () {

                s = setTimeout(function () { $($(".popup .pop_box")[i]).hide(); }, 500)
            });
        });
    }); 
