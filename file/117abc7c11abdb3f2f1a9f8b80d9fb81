function senddata(inputobj){
var obj;
obj=document.getElementById("inputnews");
var url = htmldecode("search.php?mod=portal&amp;searchid=&amp;searchsubmit=yes&amp;kw="+inputobj.value+"&srchtxt="+inputobj.value);
