	var is_ie = window.navigator.userAgent.indexOf("MSIE ") > 0;
	if(is_ie){
		var strVar="";
		strVar += "<div class=\"browser_upgrade_notice\" style=\"display:none;\">";
		strVar += "	<div class=\"container_12\">";
		strVar += "		<div class=\"grid_full\">";
		strVar += "			<div class=\"grid_8\">";
		strVar += "				<i class=\"fa fa-exclamation-circle fa-2x\" aria-hidden=\"true\"><\/i>";
		strVar += "				<div class=\"browser_upgrade_notice_text\">Дорогие пользователи, благодарим Вас за посещение нашего интернет-магазина.<br\/>Для вашего удобства, просим Вас использовать следующие браузеры.<br\/>Желаем Вам приятных покупок!<\/div>";
		strVar += "			<\/div>";
		strVar += "			<div class=\"grid_4 browser_list\">";
		strVar += "				<a target=\"_blank\" href=\"https:\/\/www.mozilla.org\/\"><img alt=\"\" src=\"\/supported_browsers\/rsz_firefox.png\"\/><\/a>";
		strVar += "				<a target=\"_blank\" href=\"https:\/\/www.google.com\/chrome\/browser\/desktop\/\"><img alt=\"\" src=\"\/supported_browsers\/rsz_chrome.png\"\/><\/a>";
		strVar += "				<a target=\"_blank\" href=\"https:\/\/support.apple.com\/downloads\/safari\"><img alt=\"\" src=\"\/supported_browsers\/rsz_safari.png\"\/><\/a>";
		strVar += "				<a target=\"_blank\" href=\"http:\/\/www.opera.com\/ru\/download\"><img alt=\"\" src=\"\/supported_browsers\/rsz_opera.png\"\/>";
		strVar += "				<a class=\"notice_close\" onclick=\"closeNotice();\"><i class=\"fa fa-times fa-2x\" aria-hidden=\"true\"><\/i><\/a>";
		strVar += "			<\/div>";
		strVar += "		<\/div>";
		strVar += "	<\/div>";
		strVar += "	<style type=\"text\/css\">";
		strVar += "		.browser_upgrade_notice{";
		strVar += "			display:inline-block;";
		strVar += "			text-align:left;";
		strVar += "			width:100%;";
		strVar += "			padding:10px 0;";
		strVar += "			background:#242735;";
		strVar += "			color:#fff;";
		strVar += "			font-size:14px;";
		strVar += "		}";
		strVar += "		.browser_upgrade_notice .browser_upgrade_notice_text{padding-left:40px;}";
		strVar += "		.browser_upgrade_notice .fa-exclamation-circle{";
		strVar += "			float:left;";
		strVar += "		}";
		strVar += "		.browser_list{text-align:right;}";
		strVar += "		.browser_list > a{display:inline-block;margin-left:10px;}";
		strVar += "		.browser_list > a img{vertical-align:middle;}";
		strVar += "		.browser_list .notice_close{color:#fff;cursor:pointer;}";
		strVar += "		@media only screen and (max-width: 1024px){";
		strVar += "			.browser_upgrade_notice .grid_8{width:100%;text-align:center;}";
		strVar += "			.browser_upgrade_notice .browser_list{width:100%;text-align:center;margin-top:5px;}";
		strVar += "		}";
		strVar += "	<\/style>";
		strVar += "<\/div>";
		strVar += "";
		function closeNotice(){
			jQuery('.browser_upgrade_notice').remove();
		} 	
		jQuery(strVar).prependTo(jQuery('.page')).slideDown(1000);
	}
</script>

        

	<script type="text/javascript">
	jQuery(function(j$) {
			var myHeader = j$('.navmain-container');
			myHeader.data( 'position', myHeader.position() );
			j$(window).scroll(function(){
			    var hPos = myHeader.data('position'), scroll = getScroll();
			    if ( hPos.top < scroll.top ){
				myHeader.addClass('fixed');
			    }
			    else {
				myHeader.removeClass('fixed');
			    }
			});
			
			function getScroll () {
			    var b = document.body;
			    var e = document.documentElement;
			    return {
				left: parseFloat( window.pageXOffset || b.scrollLeft || e.scrollLeft ),
				top: parseFloat( window.pageYOffset || b.scrollTop || e.scrollTop )
			    };
			}
	});
	</script>

<script type="text/javascript">
	
	function successMessage(message,message1,message2,message3)
		{
		    jQuery('body').append('<div class="alert"><div class="alert-inner"></div></div>');
		    var $alert = jQuery('.alert');
		    var $alertInner = jQuery('.alert-inner');
		    $alert.fadeIn(400);
                    $alertInner.html(message).append('<button class="close"><i class="fa fa-times"></i></button><div class="alertcontent"><a class="close cart" href="http://www.ym.ru/checkout/cart/">Перейти в корзину</a><a class="close continue">Вернуться к выбору товаров</a></div>');
		    jQuery('.close').click(function () {
			$alert.fadeOut(400);
		    });
		    $alert.fadeIn('400', function () {
			setTimeout(function () {
			    $alert.fadeOut('400', function () {
				jQuery(this).fadeOut(400, function(){ jQuery(this).detach(); })
			    });
			}, 10000)
		    });
		}
</script>


