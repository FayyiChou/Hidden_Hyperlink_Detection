<div class="widget widget-static-block"><script type="text/javascript" charset="utf-8">
(function ($) {
        var webSite = 'https://www.ym.ru/';
	// 菜单标题
	var title = 'Центр поддержки';
	// 菜单选项
	var menuLists = [
                {listText:'Время доставки',listHref:webSite+'spac-helpcenteradd'},
                {listText:'Как оформить заказ',listHref:webSite+'spac-placeorder'},
		{listText:'Как оплатить',listHref:webSite+'spac-helpcenter'},
		{listText:'Как доставить',listHref:webSite+'spac-expressnew'},
	];

/*****------------------------------------------------下面是代码部分不要改动---------------------------------------------------------*****/	
	var menuListHtml='',
		menuContent='',
		_href='',
		_clazz='';
	for (var i=0;i<menuLists.length; i++){
		_href = menuLists[i].listHref?menuLists[i].listHref:'javascript:;';
		_clazz = menuLists[i].listHref?'':'class="no-hover"';
		menuListHtml += '<li><a '+_clazz+' href="'+_href+'" title="'+menuLists[i].listText+'">'+menuLists[i].listText+'</a></li>'
	}
	content = "<div class=\"toplinks custommenu dropdown\"><div class=\"dropdown-toggle cover\"><div class=\"toplinks-inner\"><div class=\"value\">"
	+title
	+"</div></div></div><div class=\"dropdown-menu right-hand\">"
	+"<ul>"+menuListHtml+"</ul></div></div>";
	$('.toplinks').before(content);
})(jQuery)
</script>

<style>
.no-hover{color: inherit!important;cursor: text!important;}
.custommenu .dropdown-menu{border-bottom: none;}
</style></div>
