<script type="text/javascript">
jQuery(function(j$) {	
	//Randomize slide order  jiafu@Yema
	function shuffleArray(array) {
		for (var i = array.length - 1; i > 0; i--) {
			var j = Math.floor(Math.random() * (i + 1));
			var temp = array[i];
			array[i] = array[j];
			array[j] = temp;
		}
		return array;
	}
	var arr = jQuery('#yema_custom_carousel_owl >.item').toArray();
	shuffleArray(arr);
	jQuery("#yema_custom_carousel_owl").html();
	for(i=0;i<arr.length;i++){
		jQuery('#yema_custom_carousel_owl').append(arr[i]);	
	}
	
	j$("#yema_custom_carousel_owl").owlCarousel({
		singleItem:true,
		navigation:true,
		navigationText : false,
		lazyLoad:true,
		lazyEffect : "fade",
		autoHeight : true,
		autoPlay: true
	});	
});
</script>

