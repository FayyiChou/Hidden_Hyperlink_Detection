<script type="text/javascript">
var slideshow118=new TINY.slider.slide('slideshow118',{
	id:'it118_slider',
	auto:false,
	resume:true,
	vertical:false,
	navid:'pagination118',
	activeclass:'current',
	position:0
});

$(function(){
	changeElementsHeight118();
});
function changeElementsHeight118(){
	var $current_id = $("ul#pagination118").find("li.current").attr("id"), 
		$img_height = $(".img118"+$current_id).height(), 
		$div_height = $img_height;
	
	$(".it118_BOXin").css("height", $img_height);
	$(".it118_BOX").css("height", $div_height);
}
