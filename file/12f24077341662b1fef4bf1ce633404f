<script type="text/javascript">
    //遮罩
    $(function(){
        if('1'==1){
            var X = setTimeout(HIDE,5000);
            var sec = 5;//設定首次進入網站的廣告於幾秒後關閉
            $("#counting").show().html('此視窗將於'+sec+'秒後自動關閉');
            if(sec >0 ){
                var T = setInterval(SEC,1000);
            }
            function SEC(){
                sec--;
                $("#counting").html('此視窗將於'+sec+'秒後自動關閉');
                if(sec == 0){
                    clearInterval(T);
                    $("#counting").html("");
                }
            }
            function HIDE(){$(".BIGAD").hide();$("#countingDIV").hide();}
        }
        var mask_h = $(document).height();
        $(".body_mask").css({minHeight:mask_h-193});

       // 版尾嵌入
        if ($('#edm-box').length > 0) {
            if ($('#footer_edm').length > 0) {
                $("#footer_edm").appendTo("#edm-box").show();
            }
        }
    });
    //右側浮動購物車
    function top_refresh(_scar_no) {
        if (_scar_no == null)
            var _scar_no = "";

        $.ajax({
            url: "api/api.shopping_cart.php",
            type: "POST",
            dataType: "json",
            data: {myAct:"check"},
            success: function(Jdata){
                if( _scar_no == '' )
                {
                    _scar_no = getCookie('scar_no_isong');
                }

                $.ajax({
                    url: "/isong/api/api.shopping_cart.php",
                    type: "POST",
                    dataType: "json",
                    data: {
                        myAct: "getCartNumber",
                        scar_no: _scar_no
                    },
                    success: function (Jdata) {
                        if (Jdata.error == 'C00') {
                            $('#right_content').empty();
                            var sys_byeurl = 'isong';
                            var html = '';
                            var count = 0;
                            $.each(Jdata.content, function (key, value) {
                                if (key <= 3) {
                                    html += '<li class="Cart_Pimg"><a href="/' + sys_byeurl + '/index.php?action=product_detail&prod_no=' + value['prod_no'] + '" class="mycart-img"><img border="0" alt="' + value["prod_name"] + '" title="' + value["prod_name"] + '" src="' + value['prod_file'] + '"></a></li>';
                                    html += '</li>';
                                    html += "</ul>";
                                }
                                count += parseInt(value['scar_count']);
                            });

                            $('#right_content').append(html);

                            if(count == 0) {
                                $('#shoppingCartNumber').html('');
                            } else {
                                $('#shoppingCartNumber').html('<span class="count-number">' + count + '</span>');
                            }
                        }

                        //版頭購物車
                        $('#top_content01').empty();
                        var html='';
                        var count = 0;
                        $.each( Jdata.content, function( key, value ) {
                            var top_str = "";
                            if(value['scar_type']==1)
                                top_str = "(加贈)";
                            if(value['scar_type']==2)
                                top_str = "(加購)";
                            html +='<ul class="MemMenu_Product clearfix" id="'+value['uid']+'">';
                            html +='<li class="Cart_Pimg"><a href="../isong/index.php?action=product_detail&prod_no='+value['prod_no']+'" class="mycart-img"><img border="0" alt="'+value["prod_name"]+'" title="'+value["prod_name"]+'" src="'+value['prod_file']+'"></a></li>';
                            html +='<li class="Cart_Pname">';
                            html +='<h2><a href="../isong/index.php?action=product_detail&prod_no='+value['prod_no']+'" title="'+value["prod_name"]+'">'+value["prod_name"]+top_str+'</a></h2>';
                            html +="<p>$ "+value['scar_price']+'<span>x'+value['scar_count']+'</span></p>';
                            html +='</li>';
                            html +='<li class="Cart_Pdelete">';
                            if(value['scar_type'] != 1){
                                html +='<a href="javascript:void(0);" title="刪除" onClick="javascript:headtop_removeScar( '+value['uid']+' );">刪除</a>';
                            }
                            html +='</li>';
                            html +="</ul>";
                            count = count+parseInt(value['scar_count']);
                        });
                        html +='<div class="MemMenu_Product_total">';
                        html +='<p>總計:<span id="Cart_Total">$'+Jdata.total+'</span></p>';
                        html +='<a href="../isong/index.php?action=shopping_cart" title="結帳">結帳</a></div>';
                        $('#top_content01').append(html);
                        $('#total_cart').html(count+"件商品");

                        if(count == 0) {
                            $('.shopping_cart0').html('<span class="car_count" id="Car_Count">' + '</span>').hide();
                        } else {
                            $('.shopping_cart0').html('<span class="car_count" id="Car_Count">' + count + '</span>').show();

                        }

                    }
                })
            }
        })
    }
    //抓網址get變數
    function getQueryParams(qs) {
        qs = qs.split("+").join(" ");
        var params = [],
                tokens,
                re = /[?&]?([^=]+)=([^&]*)/g;
        while (tokens = re.exec(qs)) {
            params[decodeURIComponent(tokens[1])] = decodeURIComponent(tokens[2]);
        }
        return params;
    }
    var _GET = getQueryParams(document.location.search);//網址get變數





    function prod_search(Kw){
        var keyword ;
        var prod_sort_uid = $('select[name="prod_sort_uid"]').val();
        if (Kw){
            keyword = Kw ;
        }else{
            keyword = $("#keyword").val();
        }
        window.location.href = '/isong/index.php?action=product_search&prod_keyword=' + keyword + '&prod_sort_uid='  + prod_sort_uid;
    }
    function headtop_removeScar( uid ){
        $.ajax({
            url:"/isong/api/api.shopping_cart.php",
            type: "POST",
            dataType: "json",
            data: {myAct:"removeScarByUid",
                uid: uid
            },
            success: function(Jdata){
                if(Jdata.error=='C00'){
                    top_refresh();
                }
            }
        });
    }

    function headtop_removeScar( uid ){
        $.ajax({
            url:"/isong/api/api.shopping_cart.php",
            type: "POST",
            dataType: "json",
            data: {myAct:"removeScarByUid",
                uid: uid
            },
            success: function(Jdata){
                if(Jdata.error=='C00'){
                    top_refresh();
                }
            }
        });
    }

    $(function(){

        //購物車
        if (_GET['action'] != "shopping_checkout_2" && _GET['action'] != "shopping_checkout_3") {
            $(".SB_TopBox01_sell01").hover(
                    function () {
                        $(".MemMenu_Cart").stop(true, true).slideDown(300);
                        $(this).find(".shopping-cart-a").addClass("shopping-cart-active");
                    }, function () {
                        $(".MemMenu_Cart").stop().slideUp(300);
                        $(this).find(".shopping-cart-a").removeClass("shopping-cart-active");
                    }
            );
        }
        // 購物車重整
        top_refresh();
//浮動購物車
        $(window).scroll(function(e){

            var $W_h = $(document).scrollTop();
            
            if( $W_h > 193 ){
                $(".HEADER-leftfix").css({

                    top:10

                });


            }else{
                $(".HEADER-leftfix").css({

                    top:204

                });

            }
        });

        //版頭購物車
        if (_GET['action'] != "shopping_checkout_2" && _GET['action'] != "shopping_checkout_3"){
            $(".Kg_MemberMenu07").hover(
                    function(){
                        $(".MemMenu_Cart").stop(true,true).slideDown(300);
                    },function(){
                        $(".MemMenu_Cart").stop().slideUp(300);
                    }
            );
        }

        $("#button_search").click(function(){
            prod_search();
        });
        
        $('#keyword').keypress(function(event){
		if(event.keyCode == 13){
                    
			prod_search($(this).val());
		}
	});

        $(".searchshow").hover(
                function(){
                    $(".search_box").stop(true,true).slideDown(300);
                },function(){
                    $(".search_box").stop().slideUp(300);
                }
        );

        $(".Header .header_menu01 > li").hover(
                function(){
                    $(".body_mask").show();
                    $(this).find(".submenu_box").stop(true,true).show();
                    $(this).addClass("active_now");
                },function(){
                    $(".body_mask").hide();
                    $(this).find(".submenu_box").stop().hide();
                    $(this).removeClass("active_now");
                }
        );

        var MENU_W = 100;
        var LIST_N = $(".Header .header_menu01 > li").length;
        var LIST_W = MENU_W / LIST_N + "%";
        $(".Header .header_menu01 > li").css({ width:LIST_W});

        //版頭廣告展開
        //$(".gh_TopAD0"+num).show(500).delay(5000).hide(500); 
        $(".gh_Top_ADopen").on('click', function(){
            var num = $(this).attr("value");
            
            $(".gh_TopAD0"+num).show(500);
        });
        $(".gh_Top_ADclose").on('click', function(){
            var num = $(this).attr("value");
            $(".gh_TopAD0"+num).hide(500);
            $("#counting").hide();
        });

        $('#keyword').click(function() {
            //$(this).val("");
            $('input[name="keyword"]').val("");
        });

        $('.df_Back_top').click(function(){
            var $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
            $body.animate({
                scrollTop: 0
            }, 600);
        });


        top_refresh();

    });


    $(function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/zh_TW/sdk.js#xfbml=1&version=v2.4";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
