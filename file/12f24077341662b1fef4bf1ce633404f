<script>
    function validateEmail(email) {
        var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }
    function ajax_epmem(post_data){
        if (!validateEmail($('input[name="epmem_email"]').val())) {
            alert('請輸入正確E-MAIL!');
            return false;
        }
        var request_link = window.location.href;
        if (window.location.protocol == "https:") {
            request_link = "https:" + window.location.href.substring(window.location.protocol.length);
        }

        $.ajax({
            method: "POST",
            dataType: "text",
            url: request_link,
            data: post_data
        })
        .done(function(html) {
            alert(html);
        });
    }
    $('#epmem_add').click(function(){
        $('input[name="epmem_register_act"]').val('COMMAND_ADD');
        var post_data = $('#epmem_register_form').serialize();
        ajax_epmem(post_data);
        return false;
    });
    $('#epmem_cancel').click(function(){
        $('input[name="epmem_register_act"]').val('COMMAND_CANCEL');
        var post_data = $('#epmem_register_form').serialize();
        ajax_epmem(post_data);
        return false;
    });
</script>	
