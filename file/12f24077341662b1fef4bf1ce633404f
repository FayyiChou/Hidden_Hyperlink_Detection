<!-- 元件載入 -->
<script>
$(function(){
	
	//商品說明切換
    // 預設顯示第一個 Tab
    var _showTab = 0;
    var $defaultLi = $('ul.item354tabs li').eq(_showTab).addClass('active');
    $($defaultLi.find('a').attr('href')).siblings().hide();

    // 當 li 頁籤被點擊時...
    // 若要改成滑鼠移到 li 頁籤就切換時, 把 click 改成 mouseover
	$('ul.item354tabs li').click(function () {
        return false;
    });
    $('ul.item354tabs li').hover(function () {
        // 找出 li 中的超連結 href(#id)
        var $this = $(this),
            _clickTab = $this.find('a').attr('href');
        // 把目前點擊到的 li 頁籤加上 .active
        // 並把兄弟元素中有 .active 的都移除 class
        $this.addClass('active').siblings('.active').removeClass('active');
        // 淡入相對應的內容並隱藏兄弟元素
        $(_clickTab).stop(false, true).fadeIn(0).siblings().hide();
        return false;
    }).find('a').focus(function () {
        this.blur();
    });
    	//商品說明切換
    // 當 li 頁籤被點擊時...
    // 若要改成滑鼠移到 li 頁籤就切換時, 把 click 改成 mouseover
    $('ul.item354tabs01 li').hover(function () {
        $(".banner_show1").hide();
        // 找出 li 中的超連結 href(#id)
        var $this = $(this),
            _clickTab = $this.find('a').attr('tag');
        // 把目前點擊到的 li 頁籤加上 .active
        // 並把兄弟元素中有 .active 的都移除 class
        $this.addClass('active').siblings('.active').removeClass('active');
        // 淡入相對應的內容並隱藏兄弟元素
        $(_clickTab).stop(false, true).fadeIn(0).siblings(".pro_showbox").hide();
        return false;
    }).find('a').focus(function () {
        this.blur();
    });
    	//商品說明切換
    // 當 li 頁籤被點擊時...
    // 若要改成滑鼠移到 li 頁籤就切換時, 把 click 改成 mouseover
    $('ul.item354tabs02 li').hover(function () {
        $(".banner_show2").hide();
        // 找出 li 中的超連結 href(#id)
        var $this = $(this),
            _clickTab = $this.find('a').attr('tag');
        // 把目前點擊到的 li 頁籤加上 .active
        // 並把兄弟元素中有 .active 的都移除 class
        $this.addClass('active').siblings('.active').removeClass('active');
        // 淡入相對應的內容並隱藏兄弟元素
        $(_clickTab).stop(false, true).fadeIn(0).siblings(".pro_showbox").hide();
        return false;
    }).find('a').focus(function () {
        this.blur();
    });
    	//商品說明切換
    // 當 li 頁籤被點擊時...
    // 若要改成滑鼠移到 li 頁籤就切換時, 把 click 改成 mouseover
    $('ul.item354tabs03 li').hover(function () {
        $(".banner_show3").hide();
        // 找出 li 中的超連結 href(#id)
        var $this = $(this),
            _clickTab = $this.find('a').attr('tag');
        // 把目前點擊到的 li 頁籤加上 .active
        // 並把兄弟元素中有 .active 的都移除 class
        $this.addClass('active').siblings('.active').removeClass('active');
        // 淡入相對應的內容並隱藏兄弟元素
        $(_clickTab).stop(false, true).fadeIn(0).siblings(".pro_showbox").hide();
        return false;
    }).find('a').focus(function () {
        this.blur();
    });
    

    $(".logo_hover").hover(function(){

        $(this).find(".now_img").hide();
        $(this).find(".hover_img").show();

    },function(){

        $(this).find(".hover_img").hide();
        $(this).find(".now_img").show();

        });

    });
</script>
