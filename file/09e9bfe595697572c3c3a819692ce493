var suggest_about_search = function(suggestWords) {
    var abs = "<div class=\"search_tips\"><div class=\"search_tips_main relate_keywords clearfix\"><strong class=\"value\">相关搜索：</strong><div class=\"key\"><table><tbody><tr>";
    var akws = '';
    var i = 0;
    for (i ; i < suggestWords.length; i++) {
        if(i < 9) {
            var w  = suggestWords[i];
            if (i!=0 && i%3==0) akws +='</tr><tr>';
            var w_title = VeryCD.htmlspecialchars(w);
            akws += '<td><a title="' + w_title + '" onclick="VeryCD.Track(\'/stat/searchSimilar/\'+this.href);" href="/search/folders/' + encodeURIComponent(w) + '">' + w_title + '</a></td>';
        }
    }
    if(i < 9) {
        for(var j=0; j < 3-(i%3); j++) {
            akws += "<td>&nbsp;</td>";
        }
    }
    abs = abs + akws;
    abs += "</tr></tbody></table></div></div><div class=\"search_tips_shadow\"></div></div>";
    if(Sizzle('#about_search')[0]) {
    	Sizzle('#about_search')[0].style.display='';
    	Sizzle('#about_search')[0].innerHTML = abs;
     }
}
VeryCD.suggest_related_search(suggest_about_search,Sizzle('#search_keyword')[0].value,"folders");
