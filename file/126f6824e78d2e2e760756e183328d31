<script language="javascript" type="text/javascript">
var CPName = '排列三';
var tKey = {
    'zx': {                      // 选号位key值
        filter: ':gt(4):lt(10)', // 筛选<td/>对象的jQuery表达式
        ball: 'q1',               // 选号球样式
        num:2
    }
}

// 选号验证
function balls_match(balls){
    var checked = false;
	if(balls[0].length > 1){
	   checked = true;
	}
    return checked;
}

// 预测区单行清空选号（行号，是否清除工具箱）
function yc_clear($tds,n,clear){
	var $ball = $tds.filter('.q1,.q2,.q4,.q5');
	if($ball.length > 0){
		$ball.each(function(){
			var i = $tds.index(this);
			var cName = 'y1_1';
			if (i > 22 && i < 29) cName = 'y2_1';
			else if(i > 19 && i < 23) cName = 'y4_1';
			else if (i < 5 || (i > 14 && i < 20)) cName = 'y5_1';
			this.className = cName;
		});
		delete yc.ball[n]
		delete yc.hm_str[n];
		if(clear || false) tbox.clear(n);
	}
}

// 计算号码注数（机选号码数组）
function compute_zhu(code_str){
	var zhu = 0;
	var zu3 = new Array(0, 0, 2, 6, 12, 20, 30, 42, 56, 72, 90);
	var zu6 = new Array(0, 0, 0, 1, 4, 10, 20, 35, 56, 84, 120);

	var len = code_str[0].split(',').length;
	if(len > 1)	zhu = zu3[len] + zu6[len];
	return zhu;
}

// 折线样式
function reset_cLine() {
	var $z2 = $('#z2');
	var $z1 = $('#z1');
	return {
	'sx': {
		size: $z2.css('size'),
		color: $z2.css('color'),
		indent: $z2.css('zindex')
	},
	'qd': {
		size: $z1.css('size'),
		color: $z1.css('color'),
		indent: $z1.css('zindex')
	}
	}
}

$(function(){
	var line = reset_cLine();
	init('pl3', '/pl3/php/zxfx.php', ['0-9','sx','qd'],line);
});
</script>

