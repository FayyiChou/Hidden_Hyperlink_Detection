<!-- form submission when no keyword is entered. -->
<script type="text/javascript">
	function  hasKeyword(form){
		
		var inp   = $j(form).children('.elm04Input');
		var keyw  = $j(inp).val();
		keyw      = keyw.replace(/^\s+|\s+$/g,'');

		if(keyw == ''){
			alert("No keyword has been entered");
		}else{
			form.submit();
		}

	}
</script>
