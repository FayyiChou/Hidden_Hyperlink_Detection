		(function($){
			$.fn.extend({
				kqTab : function(o){
					var o=$.extend({
						delay:180,
						eventHandler:"mouseover"
					},o);
					return this.each(function(){
						var self=this,menu=$(self).find(".ttab-header-item"),content=$(self).find(".ttab-content-item");
						content.eq(0).show().siblings().hide();
						var tabIn=function(elem){
									var index=elem.index();elem.addClass("ttab-header-item-current").siblings().removeClass("ttab-header-item-current");
									content.eq(index).show().siblings().hide();
								},
								timeOut=function(elem,t){t?setTimeout(function(){tabIn(elem)},t):tabIn(elem);
								};
						menu.bind(o.eventHandler,function(){timeOut($(this),o.delay);});
					});
				}
			});
		})(jQuery);

