	<script type="text/javascript">
		// 指定箭头到中间位置
		var imgHeight = $(".fullSlide").height();
		var arrowHeight = $(".fullSlide .prev").height();
		var arrowTop = (imgHeight - arrowHeight)/2 + 'px';
		$(".fullSlide .prev").css("top",arrowTop);
		$(".fullSlide .next").css("top",arrowTop);

		// 只有一张图片，不显示底部圆点和左右箭头
		var imgTotal = $(".fullSlide .bd ul li").length;
		if(imgTotal == 1){
			$(".fullSlide .hd").css("display","none");
			$(".fullSlide").hover(function(){
				$(this).find(".prev,.next").css("display","none")
			});
		}else{
			// 鼠标在图片上时才显示左右箭头
			$(".fullSlide").hover(function(){
				$(this).find(".prev,.next").stop(true, true).fadeTo("show", 0.8)
			},function(){
				$(this).find(".prev,.next").fadeOut()
			});
		}

		$(".fullSlide").slide({
			titCell: ".hd ul",
			mainCell: ".bd ul",
			effect: "fold",
			autoPlay: true,
			autoPage: true,
			trigger: "click"
		});
	</script>
