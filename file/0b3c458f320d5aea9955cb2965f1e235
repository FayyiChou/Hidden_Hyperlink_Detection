<script type="text/javascript">
    returnTop.init();
    var w_h = getW().h;
    var fixedTop = {
        init:function(){
            this.fixedTop = $(".fixedBox-area-box");
            this.fixObj = $(".right-list-box");
            var that = this,
                length = this.fixObj.length,
                offsetTop = this.fixedTop.offset().top;
                function getOffset(){
                    setTimeout(function(){
                        offsetTop = that.fixedTop.offset().top;
                       if($("#shihuo-gpt-ad-02").height() <= 0 || $("#shihuo-gpt-ad-01").height() <= 0 || $(".home-area-top").height() > 60){
                          getOffset();
                       }
                    },100);
                }
                getOffset();
            $(window).bind("scroll",function(){
                if($(window).scrollTop() > offsetTop && $(window).scrollTop() < ($("#youhui-ajax-list").offset().top*1+$("#youhui-ajax-list").height()*1)-650){
                     $("#fixedBox").addClass("right-list-fixed");
                 }else{
                     $("#fixedBox").removeClass("right-list-fixed");
                 }
                 /*
                 if($("#moreYouhui").offset().top < $(window).scrollTop()+800 && window.scrollStopList){
                    that.fixedTop.removeClass("right-list-fixed");
                 }
                 */
            });
        },
        praise:function(id,ts){
            $.get('http://www.shihuo.cn/message_support_agaist?id='+id+'&type=1&source=1',{},function(msg){
                if(msg.status == 200){
                    if(msg.msg == '支持成功'){
                        $(ts).addClass('red');
                    }else if(msg.msg == '取消支持成功'){
                        $(ts).removeClass('red');
                    }

                    $(ts).html(msg.data.snum);
                }
            },'json');
        }
    };
   
    if(w_h<660){
        if($("#fixedBox .list1 li").length >= 5 ){
            $("#fixedBox .list1 li").eq(3).css("border","none");
            $("#fixedBox .list1 li").eq(4).hide();
            $("#fixedBox .list1 li").eq(5).hide();
        }
    }

    fixedTop.init();
    
/******用户任务*****/
    var usertask = {
        init:function(){
            $.get('/shihuo/userTask?rank='+Math.random(),{},function(data){
                $('#user-task').html(data);
            },'html');
        },
        sign:function(){//签到
            var that = this;
            $('#user_sign').live('click',function(){
                var _this = $(this);
                if($(this).data('close') === false) return false;
                $(this).attr('data-close', false);
                $(this).data('close',false);
                $.post('/ucenter/signin',{},function(data){
                    if($.trim(data.status) == 0){
                        $('<div id="integration" style="position: absolute; color:#e24c52; font-weight:bold;">+'+data.integral+'</div>').appendTo('body');
                        $("#integration").css({
                              left:_this.offset().left + 40,
                              top:_this.offset().top
                        });
                        $("#integration").animate({
                               top:_this.offset().top - 15
                        },500);
                        //$("#user-task").find(".jf").html("积分："+($("#user-task").find(".jf").html().replace("积分：","")*1+(data.integral*1)));
                        $("#user-task").find(".jb").html("金币："+($("#user-task").find(".jb").html().replace("金币：","")*1+(data.integral*1)));
                        setTimeout(function(){
                            $("#integration").remove();
                            that.init();
                        },1500);
                    }
                },'json')
            })
        },
        haitaoSign:function(){//海淘打卡
            var that = this;
            $('#haitao_sign').live('click',function(){
                if($(this).data('close') === false) return false;
                $(this).attr('data-close', false);
                $(this).data('close',false);

               $.post('/ucenter/haitaoSign',{},function(data){
                    if($.trim(data.status) == 0){
                        that.init();
                    }

                },'json')
            })
        }
    };
/******用户任务end*****/
$(function(){
    usertask.sign();
    usertask.haitaoSign();
    tryCourse.init();
})

