	<script type="text/javascript">
		$(function(){
			var headerSwiper = new Swiper ('.header-swiper-c', {
			   direction: 'vertical',
			   loop: false,
			   effect : 'cube',
			   autoplayDisableOnInteraction: false,
			   cube: {
			     slideShadows: false,
			     shadow: false,
			   },
			   autoplay: 3000,
			 })
			$(".header-banner .fa").click(function(e){
				$(".header-banner").hide();
				headerSwiper.startAutoplay&&headerSwiper.stopAutoplay();//停止
				e.stopPropagation();
			})
		})
	</script>
