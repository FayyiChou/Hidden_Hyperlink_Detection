<script>
/*浮动工具栏*/
$('.toolposit').find('i.close').click(function(){
  	if($(this).html()=='收起'){
  		$(this).parents('#rightToolbar').css({width:'46px'});
  		$('.kefubig').hide(),$('.kefusamll').show();
  		$(this).html('展开').addClass('on');
  	}else{
  	 $(this).parents('#rightToolbar').css({width:''});
     $('.kefubig').show(),$('.kefusamll').hide();
  	 $(this).html('收起').removeClass('on');
  	}
});

var hdstate = 0;
$(window).scroll(function() {
	var hdtoTop = parseInt($(".header-top").height() + $(".header").height());
	if ($(window).scrollTop() > hdtoTop && hdstate == 0){
		$('.totop').fadeIn("fast");//.show();
		hdstate = 1;
	} else if ($(window).scrollTop() < hdtoTop && hdstate == 1){
		$('.totop').hide();
		hdstate = 0;
	}
});

$(function(){
	$(".rt-close").click(function(){
		if($(this).hasClass("open")){
			$(this).removeClass("open").html('展开');//<i class="fa fa-caret-left"></i>
			$(".cont-min").css("display","block");
			$(".content").css("display","none");
		}else{
			$(this).addClass("open").html('收起');//<i class="fa fa-caret-right"></i>
			$(".cont-min").css("display","none");
			$(".content").css("display","block");
		}
	});

	$(".qr-code, .appdown, .app-down").hover(function(){
			$(this).children(".qrc-img").css("display","block");
	},function(){
			$(this).children(".qrc-img").css("display","none");
	});

	$(".kefu-qq, .tencent").hover(function(){
			$(this).addClass("kefu-qqtxt");
	},function(){
			$(this).removeClass("kefu-qqtxt");
	});
	//$('.totop').live('click', function() {
	$(".totop").click(function(){
		//window.scrollTo(0,0);
		$("html,body").animate({scrollTop:0},300);
	});

});
</script>
