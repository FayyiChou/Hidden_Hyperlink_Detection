       $('#jcarousel_bid').jcarousel({
           wrap: 'circular',
           animation: 500,
           vertical: true
       }).jcarouselAutoscroll({
           scroll: '+=1',
           interval: 5000
       }).mouseenter(function(){
           $(this).jcarouselAutoscroll('stop');
       }).mouseleave(function(){
           $(this).jcarouselAutoscroll('start');
       });

       $('#jcarousel_stats').jcarousel({
           wrap: 'circular',
           vertical: true
       }).jcarouselAutoscroll({
           scroll: '+=1'
       }).mouseenter(function(){
           $(this).jcarouselAutoscroll('stop');
       }).mouseleave(function(){
           $(this).jcarouselAutoscroll('start');
       });

		$(".box_header span").click(function(){
			var idx = $(this).index();
			if($(this).hasClass("select")){
				return;
			}else{
				$(this).addClass("select").siblings().removeClass("select");
				$("#boxDetail"+idx).show().siblings(".box_detail").hide();
			}
		});

jQuery("#boxDetail0").slide({mainCell:"#slide_gg",autoPlay:true,effect:"topMarquee",vis:7,interTime:50,trigger:"click"});
jQuery("#boxDetail1").slide({mainCell:"#slide_ht",autoPlay:true,effect:"topMarquee",vis:7,interTime:50,trigger:"click"});

   });


   var tabNavs=document.querySelectorAll(".category-title");
   for (var i = 0; i < tabNavs.length; i++) {
       tabNavs[i].id=i;
   }
   $(".category-title").bind("click", function() {
	    $(this).addClass("active");
	    $(this).siblings().removeClass("active");

	    var n = ($(this).attr("id"));
	    $(".tab-div-child").eq(n).show().siblings().hide();
	    $(".home-task-header>a").eq(n).show().siblings("a").hide();
	})


	//视频
	function videoModalOpen(url) {
		if (!url) {
			return alert("url不能为空！")
		}
		var htmlCodes = [
				'<div class="video-modal">',
				'  <div class="video-modal-dialog">',
				'    <div class="video-modal-content">',
				'      <div class="video-modal-body">',
				'        <button type="button" class="close" ><span >&times;</span></button>',
				'         <video src="'+url+'" controls="controls" class="video" autoplay="autoplay">',
				'           您的浏览器不支持video！', '         </video>',
				'         <div class="spin-w"></div>', '      </div>',
				'    </div>', '  </div>', '</div>' ].join("");

		var videoModal = $(htmlCodes).appendTo($("#video-container"));
		$(".video-modal .close").on("click", function() {
			$(".video-modal").remove();
		});
		var opts = {
			lines : 9,
			length : 10,
			width : 14,
			radius : 42,
			scale : 0.5,
			corners : 1,
			color : 'rgba(255,255,255,1)',
			opacity : 0.25,
			rotate : 0,
			direction : 1,
			speed : 1,
			trail : 60,
			fps : 20,
			zIndex : 2e9,
			className : 'spinner',
			top : '50%',
			left : '50%',
			shadow : false,
			hwaccel : false,
			position : 'absolute'
		};
		var spinner = new Spinner(opts).spin($(".spin-w")[0]);
		$(".video")[0].onplaying = function() {
			spinner.spin();
		};
	}
</script>
</body>
</html>
