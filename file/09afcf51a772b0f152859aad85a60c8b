<script type="text/javascript">
	window.onerror=closeErrors;
	function closeErrors()   {
		return   true;
	}
	
	function setHomepage(sURL) {
		if((navigator.userAgent.indexOf('MSIE') >= 0) && (navigator.userAgent.indexOf('Opera') < 0)){
			document.body.setHomePage(sURL);
		} else {
			alert('非 IE 浏览器请手动将本站设为首页');
			doane();
		}
	}
	
	function addFavorite_inner(url, title) {
		try {
			window.external.addFavorite(url, title);
		} catch (e){
			try {
				window.sidebar.addPanel(title, url, '');
	        } catch (e) {
	        	alert('请按 Ctrl+D 键添加到收藏夹');
			}
		}
	}
	
	function doane(event, preventDefault, stopPropagation) {
		var preventDefault = isUndefined(preventDefault) ? 1 : preventDefault;
		var stopPropagation = isUndefined(stopPropagation) ? 1 : stopPropagation;
		e = event ? event : window.event;
		if(!e) {
			e = getEvent();
		}
		
		if(!e) {
			return null;
		}
		
		if(preventDefault) {
			if(e.preventDefault) {
				e.preventDefault();
			} else {
				e.returnValue = false;
			}
		}
		
		if(stopPropagation) {
			if(e.stopPropagation) {
				e.stopPropagation();
			} else {
				e.cancelBubble = true;
			}
		}
		return e;
	}
	
	$(function(){
		$(".slide-headnav").click(function(){
			if($(this).attr("data-id")=="0"){
				$(this).attr("data-id","1");
				$("#sortType").html("功能分类");
				$("#sortNavlist1").css("display","block").siblings(".sort-navlist").css("display","none");
				$(".toggle-headnav").children(".fa-chevron-down").css("display","").next(".fa-chevron-up").css("display","none");
			} else {
				$(this).attr("data-id","0");
				$("#sortType").html("行业分类");
				$("#sortNavlist0").css("display","block").siblings(".sort-navlist").css("display","none");
				$(".toggle-headnav").children(".fa-chevron-down").css("display","").next(".fa-chevron-up").css("display","none");
			}
		});
	
		
		
		$(".sort-navlist li").hover(function(){
			$(this).addClass("on").find(".sub-box").removeClass('hide').addClass('show');
			var type = $(this).index();
			var maxLength=$(".sort-navlist").children().length;
			var TOP = $('.sort-navlist>li').css('height').split('px');
			if(type == maxLength-1){
				$(this).find('.sub-box').css('top','0'+(type-1)*TOP[0]+"px");
			}else{
				$(this).find('.sub-box').css('top','0'+type*TOP[0]+"px");
			}
		},function(){
			$(this).removeClass("on").find(".sub-box").removeClass('show').addClass('hide');
			$(this).find('.sub-box').css('top','0');
		});
		
		if(uid != "" && uid != undefined && '' == 1 && '' == 0){
			var content="",data={};
			var taskType = "<label class='check-box'><input type='radio' value='1' name='catogryId'>机械设计</label><label class='check-box'><input type='radio' value='3' name='catogryId'>电气设计</label><label class='check-box'><input type='radio' value='4' name='catogryId'>软件开发</label><label class='check-box'><input type='radio' value='2' name='catogryId'>安装运维</label><label class='check-box'><input type='radio' value='5' name='catogryId'>模拟仿真</label>";
			content="<form action='/user/setWitkeyPro.msp' method='post' id='professionalForm'><div id='skill-select'>"+
			        "<p class='title' style='margin-bottom:15px;'>请您勾选技能</p>"+
			            "<div class='checkbox-container' >"+ taskType +
			                "<input class='submitBtn' type='button' value='保存' onclick='javascript:saveProfessional()'/>"+
			            "</div>"+
			    	"</div>"
			        "</form>";
			 data={
					keyboard: false,
					content:content
			 }
			var skillModal = $.scojs_modal(data);
			skillModal.show();
			var top=$(window).height()/2-$(".modal-dialog").height()/2;
			$(".modal-header").hide();
			$(".modal-dialog").append($("<div class='bg-image'></div>"));
			$(".modal-dialog").css({"width":"400px","margin-top":top+"px"});
		}
	});
	
    function saveProfessional(){
		if($("input[name='catogryId']:checked").length==0){
			var modalfail = $.scojs_modal({
				  keyboard: true,
				  title:"提示",
				  content:"<p style='margin-top:30px;text-align:center'>至少选择一项！</p>"
				});
			modalfail.show();
			return;
		}
		$("#professionalForm").ajaxSubmit(function(json){
			tipsOp(json.msg, json.status);
			if(json.status == "success"){
				setTimeout(function(){
					window.location.reload();
				}, 2000);
			}
		});
	}
