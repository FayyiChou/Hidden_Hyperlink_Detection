        var newsSwiper = new Swiper('.news-swipe-c', {
            autoplay: 3000,
            // loop: true,
            pagination: ".news-swipe-c .swiper-pagination",
            paginationClickable: true,
            autoplayDisableOnInteraction: false,
            onSlideChangeStart: function (swiper) {
                var n = swiper.activeIndex;
                $(".news-summary-c .summary-item").eq(n).show().siblings().hide();
            }
        });

        var newsTitleSwiper = new Swiper('.news-list-c', {
            autoplay: 1,
            slidesPerView: (function(){
                var len = $(".news-list-c .swiper-slide").length;
                var min = 3;
                console.log(len);
                if(len < min){
                    return len;
                }
                return min;
            })(),
            paginationClickable: true,
            autoplayDisableOnInteraction: false,
            spaceBetween: 20,
            freeMode: true,
            speed: 10000,
            loop: true
        });

        $(".news-summary-c .summary-item .item-s").each(function (ele, index) {
            var text = $(this).text();
            var limit = 300;
            if (text.length > limit) {
                $(this).text(text.substr(0, limit) + "...")
            }
        });

    })
