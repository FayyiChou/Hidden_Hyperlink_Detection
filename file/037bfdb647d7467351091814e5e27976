                            function AutoScroll(obj) {
                                $(obj).each(function () {
                                    if ($(this).find("dl").size() > 1) {
                                        var liheight = 50;
                                        $(this).find("ul:first").animate({
                                            marginTop: "-" + liheight + "px"
                                        }, 600, function () {
                                            $(this).css({
                                                marginTop: "0px"
                                            }).find("dl:first").appendTo(this)
                                        })
                                    }
                                })
                            }
