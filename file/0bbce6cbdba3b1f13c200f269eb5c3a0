 <script src="./source/plugin/hdsoso_kefu/template/jquery-1.8.3.min.js" type="text/javascript"></script>
 <script src="./source/plugin/hdsoso_kefu/template/jquery-ui.js" type="text/javascript"></script>
 <script src="./source/plugin/hdsoso_kefu/template/jquery.cookie.js" type="text/javascript"></script>
 <script>
 	 
   jQuery.noConflict(); 
   jQuery(document).ready(function($) {
   	   var _top = $.cookie('_top');
   	   var _left = $.cookie('_left');
   	   var _min = $.cookie('_min');
   	   var _direct = 0;
   	   if(_top!=null){
   	      $(".hdsoso_kefu").css("top",_top+"px");
   	      $(".hdsoso_kefu").css("left",_left+"px");
   	   }
   	   if(_min==1){
   	     	$(".hdsoso_kefu").css("height","20px");
   	     	$(".hdsoso_min").text("最大化");
   	   }
   	   
   	   $(".hdsoso_kefu").draggable(
   	      {
   	      	handle: '.mov',
   	        stop: function( event, ui ) {
   	          //alert(ui.position.left );	
   	          $.cookie('_top',ui.position.top);
   	          $.cookie('_left',ui.position.left);
   	        }
   	      }
   	   );
       $('.hdsoso_close').click(function(){
       $('.hdsoso_kefu').hide();
     });
     $(".hdsoso_min").click(function(){
     	  if($.cookie('_min')==1){
     	  	var oriheight = $.cookie('_oriheight');
     	  $(".hdsoso_kefu").animate({height:oriheight},1000);
    	  $(this).text("最小化");
    	  $.cookie('_min',0);
     	  }else{
     	  	$.cookie('_oriheight',$(".hdsoso_kefu").css("height"));
     	    $(".hdsoso_kefu").animate({height:'20px'},1000);
     	    $(this).text("最大化");
     	    $.cookie('_min',1);
     	  }
      });
      
      $(".hdsoso_kefu").resizable({
        stop: function( event, ui ) {
   	          //alert(ui.position.left );	
   	          $.cookie('_width',$(this).css("width"));
   	          $.cookie('_heigth',$(this).css("height"));
   	      }	
      });
      if($.cookie('_width')!=null&&_min!=1){
      	$(".hdsoso_kefu").css("width",$.cookie('_width'));
      	$(".hdsoso_kefu").css("height",$.cookie('_heigth'));
      }

      
      
      $('.hdsoso_dock').click(function(){
         $(".hdsoso_kefu").hide();
         $('.hdsoso_kefu_mini').show();
         $.cookie('_dock',1);
      });
      $('.hdsoso_kefu_mini').click(function(){
         $(".hdsoso_kefu_mini").hide();
         $('.hdsoso_kefu').show();
         $.cookie('_dock',0);
      });
     
      if($.cookie('_dock')==1){
$(".hdsoso_kefu").hide();
$('.hdsoso_kefu_mini').show();
      }else if($.cookie('_dock')==0){
$(".hdsoso_kefu").show();
        $('.hdsoso_kefu_mini').hide();
      }else if(!$.cookie('_dock')&&0){
      	$(".hdsoso_kefu").hide();
        $('.hdsoso_kefu_mini').show();
      }else{
        $(".hdsoso_kefu").show();
        $('.hdsoso_kefu_mini').hide();
      }
     });
 </script>
