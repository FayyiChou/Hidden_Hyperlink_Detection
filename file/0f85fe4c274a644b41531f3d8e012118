<script type="text/javascript">
    //图片横向竖向居中显示
    function imgZoomInit_view_atlas_43_1265() {
        $('#collections_view_atlas_43_1265 .w-imglist-item').append(function () {
            ht = $(this).find('.w-imglist-in').html();
            return "<div class='w-imglist-bigimg' style='z-index: 78'>" + ht + "</div>";
        });

        $("#collections_view_atlas_43_1265 .w-imglist-item .w-imglist-in img").each(function (i) {
            var img = $(this);
            var realWidth;//原始宽度
            var realHeight;//原始高度
            var vs;//图片宽高比

            realWidth = img[0].naturalWidth;
            halfIntWidth = parseInt(350) / 2;
            realHeight = img[0].naturalHeight;
            halfIntHeight = parseInt(240) / 2;
            vs = realWidth / realHeight;

            //缩略图比例240:160(等于1.5)
            if (vs >= 1.5) {//横图则固定高度
                $(img).css("width", "auto").css("height", 240 + "px").css("marginLeft", (halfIntWidth - (halfIntHeight * parseInt(realWidth) / parseInt(realHeight))) + "px");
            }
            else {//
                $(img).css("width", 350 + "px").css("height", "auto").css("marginTop", (halfIntHeight - (halfIntWidth * parseInt(realHeight) / parseInt(realWidth))) + "px");
            }

        });
    }
    $(function () {
        if ("False" === "False") { $(".w-imglist-collections").parent().removeClass("overflow_hidden"); }
        imgZoomInit_view_atlas_43_1265();
    });

</script>
</div>
