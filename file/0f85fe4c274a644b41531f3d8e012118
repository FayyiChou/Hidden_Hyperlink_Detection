<script type="text/javascript">
    $(function () {

        $('#view_category_87_1265 .w_category_first .w_category_first_item:last').find("a").eq(0).css("border-bottom", "none");
        //处理边框导致的二级菜单位置错误变化
        var borderWith =0;
        if($("#view_category_87_1265_w_category_first_item").parent().length>0){
            borderWith  = ($("#view_category_87_1265_w_category_first_item").parent().css("borderRightWidth")).toString().split("p")[0];
        }

        $("#view_category_87_1265 .w_category_seconds").css("top", 0 - borderWith);
        $("#view_category_87_1265 .w_category_third").css("top", 0 - borderWith);

        //处理超出设计边框位置的二级菜单不能显示的问题
        $("#" + 'view_category_87_1265').children("div").eq(0).css("overflow", "visible");

        //处理第二层分类列表停靠位置
        $("#view_category_87_1265 .w_category_seconds").css("right", $("#view_category_87_1265 .w_category_seconds").width() - borderWith * 2 - $("#view_category_87_1265 .w_category_seconds").width() * 2);

        $('#view_category_87_1265 .w_category_first_item').hover(function () {
            $(this).children(".w_category_first_title").toggleClass("first_hover");

        });

        $('#view_category_87_1265 .w_category_second_item').hover(function () {
            $(this).children(".w_category_second_title").toggleClass("second_hover");

        });
        $('#view_category_87_1265 .w_category_third_item').hover(function () {
            $(this).children(".w_category_third_title").toggleClass("third_hover");

        });
        if ("False" == "False") {
            jQuery("#" + 'view_category_87_1265').css("z-index", "99999");
        }

        SetCategorySelectedStyle('view_category_87_1265');
    });


    view_category_87_1265.ShowResultPageList =  function (entityName,categoryId,categoryName,firstCategoryId,firstCategoryName,secondCategoryId,secondCategoryName) {
        if ("False".toLocaleLowerCase() != "true") {
            if ("True".toLocaleLowerCase() == "true") {
                if("False".toLocaleLowerCase()=="true"){
                    window.location.href = "/NewCategoryResultlist?&EntityTypeName=" + entityName
                        +"&categoryId=" + categoryId+"&categoryName=" + categoryName
                        +"&firstCategoryName=" + firstCategoryName+"&firstCategoryId="+firstCategoryId
                        +"&secondCategoryName=" + secondCategoryName+"&secondCategoryId="+secondCategoryId;
                }else{
                    window.location.href = "/CategoryResultlist?&EntityTypeName=" + entityName +"&categoryId=" + categoryId+"&categoryName=" + categoryName;
                }
            } else {
                if ("False".toLocaleLowerCase() != "true") {
                    //老版本当前页显示
                    TurnPageCategoryListSmartView("1265", "", "False", "1", "", "", entityName, categoryId);
                }
                else {
                    //新版本（精简）
                    TurnNewPageSmartView("1265", "", "False", "","false",categoryId,categoryName);
                }
                SetCategorySelectedById('view_category_87_1265', categoryId);
            }
        }
    }

</script>
