    $(function () {
        $smartview = $("#view_linkbutton_14_1265");
        var borderWidth = $smartview.find("div").first().outerWidth(true) - $smartview.find("div").width();
        var borderHeight = $smartview.find("div").first().outerHeight(true) - $smartview.find("div").height();
        var centerWidth = $smartview.width() - borderWidth;
        var centerHeight = $smartview.height() - borderHeight;
