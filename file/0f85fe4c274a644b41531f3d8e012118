<div  id="view_map_39_1265" class="yibuSmartViewMargin absPos"   >
<div class='yibuFrameContent overflow_hidden map_Style1_Item0' style='height:379px;width:456px;'>    <div id="view_map_39_1265_allmap" style="height: 379px; width: 456px;"></div>
<script type="text/javascript">
    var view_map_39_1265_ShowMapType="False";
    var view_map_39_1265_ShowNavigation="True";
    var view_map_39_1265_ShowMarker="True";
    var view_map_39_1265_toolBar;
    var view_map_39_1265_mapObj ;//= new AMap.Map("view_map_39_1265_allmap");
    var view_map_39_1265_marker = [{"AddressItemId":"1","Title":"青岛美华庆典用品制造厂","AddressName":"胶州市兰州西路1186号","ProvinceName":"山东省","CityName":"青岛市","Content":"电话：053287298217 / 87298269 / 87298268","DisplayOrder":1}];
    function view_map_39_1265_init(){
        view_map_39_1265_mapObj = new AMap.Map("view_map_39_1265_allmap",{resizeEnable: false,zoom:parseInt("14")});

        if(view_map_39_1265_ShowNavigation=="True"){
            view_map_39_1265_mapObj.plugin(["AMap.ToolBar"], function () {
                view_map_39_1265_toolBar = new AMap.ToolBar();
                view_map_39_1265_mapObj.addControl(view_map_39_1265_toolBar);
                view_map_39_1265_toolBar.show();
            });
        }
        if(view_map_39_1265_ShowMapType=="True"){
            view_map_39_1265_mapObj.plugin(["AMap.MapType"],function(){
                //地图类型切换
                var mapType= new AMap.MapType({
                    defaultType:0,//默认显示卫星图
                    showRoad:true
                });
                view_map_39_1265_mapObj.addControl(mapType);
            });
        }

        if(view_map_39_1265_marker!=null)
        {
            if(view_map_39_1265_marker.length>0){
                for (var i = 0; i < view_map_39_1265_marker.length; i++) {
                    view_map_39_1265_search(view_map_39_1265_marker[i].AddressName, view_map_39_1265_marker[i].CityName,
                        view_map_39_1265_marker[i].AddressItemId);
                }
            }
        }
    }

    function view_map_39_1265_search(addressname, cityname,addressItemId) {
        var MSearch;
        //加载地理编码插件
        view_map_39_1265_mapObj.plugin(["AMap.PlaceSearch"], function () {
            MSearch = new AMap.PlaceSearch({ //构造地点查询类
                pageSize:10,
                pageIndex:1,
                city:cityname //城市
            });
            //关键字查询
            MSearch.search(addressname, function(status, result){
                if (status === 'complete' && result.info === 'OK') {
                    view_map_39_1265_keywordSearch_CallBack(result,addressItemId);
                } else {
                    view_map_39_1265_geocoder(addressname, cityname,addressItemId);
                }
                view_map_39_1265_mapObj.setZoom(parseInt("14"));
            });
        });
    }

    function view_map_39_1265_geocoder(addressname, cityname,addressItemId ) {
        var MGeocoder;
        //加载地理编码插件
        view_map_39_1265_mapObj.plugin(["AMap.Geocoder"], function () {
            MGeocoder = new AMap.Geocoder({
                city: cityname, //城市，默认：“全国”
                radius: 3000
            });
            //返回地理编码结果
            //AMap.event.addListener(MGeocoder, "complete", geocoder_CallBack);
            //地理编码
            MGeocoder.getLocation(addressname, function(status, result){
                if(status === 'complete' && result.info === 'OK'){
                    view_map_39_1265_geocoder_CallBack(result,addressItemId);
                }
            });
        });
    }
    function view_map_39_1265_addmarker(i, d,itemId) {
        var lngX = d.location.getLng();
        var latY = d.location.getLat();
        var markerOption = {
            map: view_map_39_1265_mapObj,
            position: new AMap.LngLat(lngX, latY)
        };
        var mar = new AMap.Marker(markerOption);
        //marker.push(new AMap.LngLat(lngX, latY));
        //构建信息窗体中显示的内容
        var info = [];
        for (var i = 0; i < view_map_39_1265_marker.length; i++) {
            if(view_map_39_1265_marker[i].AddressItemId==itemId){
                info.push("<div style=\"padding:0px 0px 0px 4px;\"><b style=\"font-size:14px;\">" + view_map_39_1265_marker[i].Title + "</b>");
                info.push("" + view_map_39_1265_marker[i].Content + "</div></div>");
                continue;
            }
        }
        var infoWindow = new AMap.InfoWindow({
            content: info.join("<br/>")  //使用默认信息窗体框样式，显示信息内容
        });
        var fa=function(e){
            if(view_map_39_1265_ShowMarker=="True"){
                infoWindow.open(view_map_39_1265_mapObj, mar.getPosition());
            }
            view_map_39_1265_mapObj.setCenter(mar.getPosition());
        };
        AMap.event.addListener(mar, "click", fa) ;
    }

    //地理编码返回结果展示
    function view_map_39_1265_geocoder_CallBack(data,addressItemId) {
        var resultStr = "";
        //地理编码结果数组
        var geocode = new Array();
        geocode = data.geocodes;
        if (geocode.length > 0) {
            view_map_39_1265_addmarker(0, geocode[0],addressItemId);
        }
        view_map_39_1265_mapObj.setFitView();
    }
    function view_map_39_1265_keywordSearch_CallBack(data,addressItemId) {
        var poiArr = data.poiList.pois;
        var resultCount = poiArr.length;
        if (resultCount > 0) {
            for (var i = 0; i < resultCount; i++) {
                view_map_39_1265_addmarker(i, poiArr[i],addressItemId);
                break; // 只取第一条查询到的记录显示
            }
            view_map_39_1265_mapObj.setFitView();
        }
    }
    function view_map_39_1265_loadScript() {
        var script = document.createElement("script");
        script.type = "text/javascript";
        script.setAttribute("id","ampId_gaode");
        script.src = "http://webapi.amap.com/maps?v=1.3&key=ff5fc22da04b9f6cccd6784d347dd7d7";
        if (script.readyState) { //IE
            script.onreadystatechange = function () {
                if (script.readyState == "loaded" ||script.readyState == "complete") {
                    script.onreadystatechange = null;
                    setTimeout(function(){
                        view_map_39_1265_init();
                    },2000);
                }
            };
        } else { //Others
            script.onload = function () {
                setTimeout(function(){
                    view_map_39_1265_init();
                },2000);
            };
        }
        var mapScript=document.getElementById("ampId_gaode");
        if(mapScript==null){
            document.body.appendChild(script);
        }else{
            setTimeout(function(){
                view_map_39_1265_init();
            },2000);
        }
    }
    $(function() {
        view_map_39_1265_loadScript();
    });

</script></div>
</div>
