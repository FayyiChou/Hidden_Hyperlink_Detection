  <script type="text/javascript">

$(document).on("click",'.favorite',function(){
	var userid=0;
	if(!userid){
		$('#noty_center_layout_container').show();
		$('#noty_center_layout_container').find('iframe').attr('src','/user/login/index.html');
	}
	else{
		var _this=$(this);
		var title=_this.data('title');
		var object_id=_this.data('id');
		var cid=_this.data('cid');
		var url="/article/index/id/"+object_id+"/cid/"+cid+".html";
		var fa_num=_this.children('.art-btn-text').children('span').html();
		var fa_num1=_this.children('span').html();
		
		$.post("/user/favorite/do_favorite/id/39651.html",{title:title,object_id:object_id,url:url},function(e){
			if(e.status==1){
				setTimeout(function(){
					_this.parent().parent().next('div').fadeOut(1000);
				},1000);
					_this.parent().parent().next('div').css('background-color','greenyellow')
					_this.parent().parent().next('div').css('color','#000000');
					_this.parent().parent().next('div').html(e.info);
					_this.parent().parent().next('div').fadeIn(100);
					_this.children('.art-btn-text').children('span').html(parseInt(fa_num)+1);
					_this.children('span').html(parseInt(fa_num1)+1);
				
			}
			else{
				setTimeout(function(){
					_this.parent().parent().next('div').fadeOut(1000);
				},1000);
					//alert(2);
					_this.parent().parent().next('div').html(e.info);
					_this.parent().parent().next('div').fadeIn(100);
			}
		});
	}
});

$(document).ready(function(){
	$('#left-float-btn').animate({left:'-39px'});
});
$('.body').on('mouseover',function(){
	if($('#left-float-btn').css('left')=='0px')$('#left-float-btn').animate({left:'-39px'});
})
$(window).scroll(function() {
	//alert($('body').height());return;
	   		if (($(window).height() + $(window).scrollTop()) >= $('body').height()) {
                            $(".right-float-btn").show();
                            //setTimeout(insertcode, 1000);/*IE 不支持*/
                            //insertcode();
        }
		else $(".right-float-btn").hide();
		//$('#left-float-btn').animate({left:'-39px'});
  });

	/*控制左右两侧浮动位置*/
	var winWidth = $(window).width();
	if(winWidth > 1300){
		$('#left-float-btn').css({'left':(winWidth - 1300)/4 +'px'});
		$('#right-float-btn').css({'right':(winWidth - 1300)/4 +'px'});
	}else{
		$('#left-float-btn').css({'left':'10px'});
		$('#right-float-btn').css({'right':'10px'});
	}

	$('#t-s-leftbtn').click(function(){
		lunbo(top_slide_index-1);
		clearInterval(invental);
	});
	$('#t-s-rightbtn').click(function(){
		lunbo(top_slide_index+1);
		clearInterval(invental);
	});
	$('.top-slide-btn li').each(function(index,e){
		$(this).click(function(){
			lunbo(index);
			clearInterval(invental);
		});
	});

	$('#add-channel-btn').click(function(){
		var _this = $(this).parent();
		if( _this.hasClass('active') ){
			_this.removeClass('active');
			_this.find('.add-c-btn-center span').html('+');
		}else{
			_this.addClass('active');
			_this.find('.add-c-btn-center span').html('-');
		}
	});


	$('.r-ch-t-tab').each(function(index,e){
		var _this = $(this);
		_this.click(function(){
			$('.r-ch-t-tab').removeClass('active');
			_this.addClass('active');
			var _list = $('.re-ch-con-list');
			_list.removeClass('active');
			_list.eq(index).addClass('active');

		});
	});


	


/*展开顶部添加频道*/
function showMoreChannel(obj){
	var _this = $(obj),add_channel=$('.add-channel').eq(0);
	//$('.add-channel').eq(1).removeClass('active');
	if(_this.hasClass('active')){
		_this.removeClass('active');
		add_channel.removeClass('active');
	}else{
		_this.addClass('active');
		add_channel.addClass('active');
	}
}
/*顶部轮播*/
function lunbo(ind){
	if(ind > 3)ind = 0;
	if(ind < 0)ind = 3;
	var marginLeft = ind * 100;
	topslidewrap.animate({'marginLeft':'-' + marginLeft + '%'},200);
	var t_s_title = topslidewrap.find('.t-s-title');
	t_s_title.removeClass('active');
	t_s_title.eq(ind).addClass('active');
	var top_slide_btn = $('#top-slide-btn li');
	top_slide_btn.removeClass('active');
	top_slide_btn.eq(ind).addClass('active');
	top_slide_index = ind;
}

/*关闭删除新闻nid为news_id*/
function closeNews(obj,nid){
	var _this = $(obj);
	_this.parents('.news-detail').remove();
	_this.parents('.news-imglist').remove();
}
/*搜索*/
$('.h-w-c-s-button').click(function(){
	var keywords=$('#searchInput').val();
	if(!keywords){
		alert("请先输入搜索内容！");
		$('#searchInput').focus();
	}
	else{
		keywords=stripscript(keywords);
		//var url="/portal/search/index/keywords/"+keywords;
		window.location.href="/portal/search/index/keywords/"+keywords+".html";
		//$.post("/portal/search/index.html",{'keywords':keywords},function(data){
		//	alert(data);
		//});
	}
});
function stripscript(s) {
    var pattern = new RegExp("[`~!@#$^&*()=%|{}':;',\\[\\].<>/?~！@#￥……&*（）&mdash;―|{}【】‘；：”“'。，、？]")
        var rs = "";
    for (var i = 0; i < s.length; i++) {
        rs = rs + s.substr(i, 1).replace(pattern, '');
    }
    return rs;
}


/*加载更多*/
$('.load-more-news').click(function(){
	
	var _this=$(this);
	_this.find('img').addClass('fa-spin');
	_this.find('span').html('加载中...');
	var news_content =_this.prev('div').children('.news-list');
	var term_id=_this.attr('term_id');
	var keywords='';
	
	setTimeout(function(){
		var n=news_content.children('li:last-child').attr('data');//alert(n);
		$.post("/portal/list/get_more.html",{'start':n,'term_id':term_id,keywords:keywords},function(data){
			if(!data){
				alert("已经到了最后一条");return;
			}
			//alert(data[0]);return false;
			
			var dataObj=eval("("+data+")");//转换为json对象 
			//alert(dataObj.length);return;
			//alert(dataObj[0]['post_title']);//return;
			if(dataObj){
				//alert(dataObj.length);
				if(dataObj.length==0){
					alert("已经到了最后一条");return;
				}
					for(i=0;i<dataObj.length;i++)
					{
					//alert(i);
						
						n=parseInt(n) + parseInt(1);
						//news_content.attr('data',n);
						//alert(dataObj[i]['post_title']);
							var contentstr=dataObj[i]['post_content'];//if(i>0){alert(contentstr);return false;}
							
							
								arr=contentstr.split('src=\"');
												
								if(arr.length<=3){
									if(arr.length==1){
										pic0="/themes/xcweb/Public/images/index/default/"+dataObj[i]['id'] % 10+".jpg";
									}
									else{
										pic_arr=arr[1].split("\"");
										pic0=pic_arr[0];//alert(pic0);
									}
									
									if(dataObj[i]['smeta']=='{"thumb":""}' || !dataObj[i]['smeta']){
										var str ="<li class=\"news-detail\" data=\""+dataObj[i]['id']+"\"><div class=\"news-img\"><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+".html\" target=\"_blank\">";
										str=str+"<img src="+pic0+">";
										//if(pic0) str=str+"<img src="+pic0+">";
										//else str=str+"<img src='/themes/xcweb/Public/images/index/default/"+dataObj[i]['id'] % 10+".jpg'>";
										
									}
									else{
									var str ="<li class=\"news-detail\" data=\""+dataObj[i]['id']+"\"><div class=\"news-img\"><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+".html\" target=\"_blank\">";
										smpic=dataObj[i]['smeta'].split("\"");
										str=str+"<img src=/data/upload/"+smpic[3].replace("\\","")+">";
										
									}
									str=str+"</a></div><div class=\"news-details\"><h2><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+".html\" target=\"_blank\">"+dataObj[i]['post_title']+"</a></h2><p>"+dataObj[i]['post_excerpt'].substr(0,66)+"</p><div class=\"news-d-bottom\"><div class=\"n-d-left\">"+dataObj[i]['post_source']+" &nbsp;&nbsp;&nbsp;"+dataObj[i]['post_modified'].substr(0,10)+"</div>";
								str=str+"<ul class=\"share-btn clearfix\"><li title=\"关闭\" onClick=\"closeNews(this,"+dataObj[i]['id']+")\"><i class=\"icon-error\"></i></li><li title=\"评论数\"><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+"/#comment\"><i class=\"icon-comment\"></i>"+dataObj[i]['comments_num']+"</a></li><li title=\"收藏\" class=\"favorite\" data-id=\""+dataObj[i]['id']+"\" data-title=\""+dataObj[i]['post_title']+"\" data-cid=\""+dataObj[i]['term_id']+"\" ><i class=\"icon-heart\"></i><span>"+dataObj[i]['favorite_num']+"</span></li></ul></div><div class=\"msg\">亲，您已收藏过了！</div></div></li>";
								}
								if(arr.length>3){
									
										pic_arr=arr[1].split("\"");
										pic1=pic_arr[0];
										pic_arr=arr[2].split("\"");
										pic2=pic_arr[0];
										pic_arr=arr[3].split("\"");
										pic3=pic_arr[0];
									
									var str ="<li data=\""+dataObj[i]['id']+"\"><div class=\"news-imglist\"><div class=\"news-img-list-title\"><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+".html\" target=\"_blank\">"+dataObj[i]['post_title']+"</a></div><ul class=\"news-img-list-con\"><li class=\"nilc-img\"><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+".html\" target=\"_blank\"><img src=\""+pic1+"\" width=\"250\"height=\"160\"/></a></li><li class=\"nilc-img\"><a href=\"/article/index/id/"+dataObj[i]['id']+".html\"><img src=\""+pic2+"\"width=\"250\"height=\"160\"/></a></li><li class=\"nilc-img\"><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+".html\" target=\"_blank\"><img src=\""+pic3+"\"width=\"250\"height=\"160\"/></a></li></ul><div class=\"news-d-bottom\"><div class=\"n-d-left\">"+dataObj[i]['post_source']+" &nbsp;&nbsp;&nbsp;"+dataObj[i]['post_modified'].substr(0,10)+"</div>";
								str=str+"<ul class=\"share-btn clearfix\"><li title=\"关闭\" onClick=\"closeNews(this,"+dataObj[i]['id']+")\"><i class=\"icon-error\"></i></li><li title=\"评论数\"><a href=\"/article/index/id/"+dataObj[i]['id']+"/cid/"+dataObj[i]['term_id']+"/#comment\"><i class=\"icon-comment\"></i>"+dataObj[i]['comments_num']+"</a></li><li title=\"收藏\" class=\"favorite\" data-id=\""+dataObj[i]['id']+"\" data-title=\""+dataObj[i]['post_title']+"\" data-cid=\""+dataObj[i]['term_id']+"\" ><i class=\"icon-heart\"></i><span>"+dataObj[i]['favorite_num']+"</span></li></ul></div><div class=\"msg\">亲，您已收藏过了！</div></div></li>";
								
								}
								//alert(str);
							news_content.append(str);	
					}
				return;
				
			}
			
		});
	
	
		_this.find('.loadmore-news').html('<img src="/themes/xcweb/Public/images/loading.png"/>更多');
	},300)

	
});
