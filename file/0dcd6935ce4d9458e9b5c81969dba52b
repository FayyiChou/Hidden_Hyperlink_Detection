  <script>
/*去除频道*/
function channel_remove(obj,cid){
	$.post("/index/nav_del",{id:cid},function(data){
		if(data.status){
			var _this = $(obj);
			var _li = $('.my-channels .my-channel'+cid).parent();
			var _clone = _li.eq(0).clone();
			_clone.find('div').attr({'onclick':'channel_add(this,'+ cid +')','title':'添加频道','class':'hot-channel'+cid}).html('+');
			_li.remove();
			$('.hot-channels .acc-morechannel').before(_clone);
			$('.header-nav-list').find('li').each(function(){
				if($(this).children('a').attr('href')=="/portal/list/index/id/"+cid+".html")
				{
					$(this).remove();
				}
			});
		}
	});
	
}
/*添加频道*/
function channel_add(obj,cid){
	var userid=0;
			if(!userid){
				$('#noty_center_layout_container').show();
				$('#noty_center_layout_container').find('iframe').attr('src','/user/login/index.html');
			}
		else{
				$.post("/index/nav_add/",{id:cid},function(data){
					if(data.status){
						var _this = $(obj);
						var _li = $('.hot-channels .hot-channel'+cid).parent();
						var _clone = _li.eq(0).clone();
						_clone.find('div').attr({'onclick':'channel_remove(this,'+ cid +')','title':'移除频道','class':'my-channel'+cid}).html('-');
						_li.remove();
						$('.my-channels').append(_clone);
						$('.header-nav-list').append("<li class=\"header-nva-li\"> <a href=\"/portal/list/index/id/"+data.nav_id+".html\"> "+data.name+" </a></li>")
					}
				});
				}
	
}
$('.body').mouseover(function(){$('#user_down').attr('style','display:block')});
/*展开顶部添加频道*/
function showMoreChannel(obj){
	var _this = $(obj),add_channel=$('.add-channel').eq(0);
	//$('.add-channel').eq(1).removeClass('active');
	if(_this.hasClass('active')){
		_this.removeClass('active');
		add_channel.removeClass('active');
	}else{
		_this.addClass('active');
		add_channel.addClass('active');
	}
}

$(".shenqing").on("click",function(){
	var _this=$(this);
	$.post("/user/profile/sqmtr.html",{},function(e){
		window.location.href="/user/mtrindex/index";
		//setTimeout(function(){
					//_this.parent().parent().next().fadeOut(1000);
				//},1000);
					//_this.parent().parent().next().css("top","0");
//					_this.parent().parent().next().css("width","300px");
//					_this.parent().parent().next().html(e);
//					_this.parent().parent().next().fadeIn(100);
	});
});

$(".logout").on("click",function(){
	var _this=$(this);
	//$.cookie('userid', 'userid'); 
	$.post("/user/index/logout.html",{},function(e){
		setTimeout(function(){
					_this.parent().parent().next().fadeOut(1000);
					//location.reload(true);
					//window.location.replace(document.referrer);
					window.location.href="/";
					//location.reload();
					//history.go(0);
				},1000);
					_this.parent().parent().next().css("top","0");
					_this.parent().parent().next().css("width","200px");
					_this.parent().parent().next().css("z-index","1000");
					_this.parent().parent().next().css("background-color","#0F67B2");
					_this.parent().parent().next().css("border","2px #0F67B2 solid");
					_this.parent().parent().next().html(e);
					_this.parent().parent().next().fadeIn(100);
					$.cookie('userid', 'userid'); 
					
	});
});
</script> 
