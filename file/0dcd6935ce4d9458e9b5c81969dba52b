           <script language="JavaScript" type="text/javascript">
$('.dingyue').click(function(){
		var userid=0;
			if(!userid){
				$('#noty_center_layout_container').show();
				$('#noty_center_layout_container').find('iframe').attr('src','/user/login/index.html');
			}
		else{
		var dy_id=$(this).attr('data');
		var _this=$(this);
		$.post("/portal/channellist/add.html",{id:dy_id,type:'0'},function(data){
			if(data=="请先登录！"){
				alert(data);
				window.location.href="/user/login/index.html";
			}
			else{
				_this.html("已订阅");
				_this.attr('class','re-ch-con-btn');
				alert(data);
			}
		});
		}
	});
	
	$('.dingyue1').click(function(){
		var userid=0;
			if(!userid){
				$('#noty_center_layout_container').show();
				$('#noty_center_layout_container').find('iframe').attr('src','/user/login/index.html');
			}
		else{
		var dy_id=$(this).attr('data');
		var _this=$(this);
		$.post("/portal/channellist/add1.html",{id:dy_id,type:'1'},function(data){
			if(data=="请先登录！"){
				alert(data);
				window.location.href="/user/login/index.html";
			}
			else{
				alert(data);
				_this.html("已订阅");
				_this.attr('class','re-ch-con-btn');
				
			}
		});
		}
	});
</script> 
