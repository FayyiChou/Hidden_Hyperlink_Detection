  <script language="JavaScript" type="text/javascript">
$(document).on("click",'.zan',function(){
	var userid=0;
	if(!userid){
		$('#noty_center_layout_container').show();
		$('#noty_center_layout_container').find('iframe').attr('src','/user/login/index.html');
	}
	else{
		var _this=$(this);
		var id=_this.data('id');
		var num=$('.art-btn-text-gree').html();//alert(num);
	
		$.post("/user/favorite/do_zan.html",{id:id},function(e){
			if(e.status==1){
				setTimeout(function(){
					_this.parent().next().next('.msg').fadeOut(1000);
				},1000);
					_this.parent().next().next('.msg').css('background-color','greenyellow')
					_this.parent().next().next('.msg').html(e.info);
					_this.parent().next().next('.msg').css('left','-300px');
					_this.parent().next().next('.msg').css('color','#000000');
					_this.parent().next().next('.msg').fadeIn(100);
					$('.art-btn-text-gree').html(parseInt(num)+1);
				
			}
			else{
				setTimeout(function(){
					_this.parent().next().next('.msg').fadeOut(1000);
					//_this.parent().parent().next('div').css('left','-430px');
				},1000);
					//alert(2);
					_this.parent().next().next('.msg').css('left','-300px');
					_this.parent().next().next('.msg').html(e.info);
					_this.parent().next().next('.msg').fadeIn(100);
			}
		});
	}
});
/*顶部轮播全局变量控制当前显示第几屏*/
var top_slide_index = 0;
var topslidewrap = $('#top-slide-wrap');
var invental;
$(document).ready(function(){
	invental = setInterval(function(){
		lunbo(top_slide_index+1);
	},2000);
/*鼠标滚动事件*/

	(function(){
		var top_slide = $("#top-slide");
		var right_main_fixed = $('#right-main-fixed');
		var topH = top_slide.offset().top;//alert(topH);
		var _TOP = right_main_fixed.offset().top - 54;//alert(_TOP);
		var couldfix = $(window).height() - 54 > right_main_fixed.height();
		$(window).scroll(function() {
			var scroH = $(this).scrollTop(); //alert(topH);
			if (scroH > topH) {
				$('body').addClass('header-fixed');
				if(scroH > _TOP ){
					$('#right-main-fixed').attr('class','right-main right-main-fixed');//('');
						var ah=$('body').height()-_TOP-300+149;
						if(scroH>ah){
						$('#right-main-fixed').attr('style','z-index:1;bottom:388px;top: inherit;');
						}
				}else{
					right_main_fixed.removeClass('right-main-fixed');
					$('#right-main-fixed').attr('style','z-index:1;');
				}

			}else{
				$('body').removeClass('header-fixed');
				right_main_fixed.removeClass('right-main-fixed');
			}
		});		
	})();
	
});
</script> 
  <script language="JavaScript" type="text/javascript">
$(document).on("click",'.favorite1',function(){
	var userid=0;
	if(!userid){
		$('#noty_center_layout_container').show();
		$('#noty_center_layout_container').find('iframe').attr('src','/user/login/index.html');
	}
	else{
		var _this=$(this);
		var title=_this.data('title');
		var object_id=_this.data('id');
		var cid=_this.data('cid');
		var url="/article/index/id/"+object_id+"/cid/"+cid+".html";
		var fa_num=_this.children('.art-btn-text').children('span').html();
		var fa_num1=_this.children('span').html();
		
		$.post("/user/favorite/do_favorite/id/39651.html",{title:title,object_id:object_id,url:url},function(e){
			if(e.status==1){
				setTimeout(function(){
					_this.parent().next().next('.msg').fadeOut(1000);
				},1000);
					_this.parent().next().next('.msg').css('background-color','greenyellow')
					_this.parent().next().next('.msg').css('color','#000000');
					_this.parent().next().next('.msg').html(e.info);
					_this.parent().next().next('.msg').fadeIn(100);
					_this.children('.art-btn-text').children('span').html(parseInt(fa_num)+1);
					_this.children('span').html(parseInt(fa_num1)+1);
				
			}
			else{
				setTimeout(function(){
					_this.parent().next().next('.msg').fadeOut(1000);
				},1000);
					//alert(2);
					_this.parent().next().next('.msg').html(e.info);
					_this.parent().next().next('.msg').fadeIn(100);
			}
		});
	}
});
</script> 
