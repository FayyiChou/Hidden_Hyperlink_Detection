<script>
var $f=function(id) {
   return document.getElementById(id);
}
function moreBoxto(numcss1020){
	for (j=0; j<10; j++){
	 if($f('moreBox' + j)){
	 $f('moreBox'+j).style.display='none';

	 }
	}
	$f('moreBox'+numcss1020).style.display='block';
}
function hidden_moreBoxto(numcss1020) {
	timeout = setTimeout(function() {$f("moreBox"+numcss1020).style.display = "none";},100);
}
function show_moreBoxto(numcss1020){
	window.clearTimeout(timeout);
	$f("moreBox"+numcss1020).style.display = "";
}
function AddFavorite()
{
    var title=document.title;
    var url=document.location.href;
    if (window.sidebar) window.sidebar.addPanel(title, url,"");
    else if(window.opera && window.print){
        var mbm = document.createElement('a');
        mbm.setAttribute('rel','sidebar');
        mbm.setAttribute('href',url);
        mbm.setAttribute('title',title);
        mbm.click();
    }
    else if(document.all) window.external.addFavorite(url,title);
}
</script>
