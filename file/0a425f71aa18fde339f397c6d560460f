<script type="text/javascript">
    document.getElementById("cminPrice").onfocus = function () {
        document.getElementById("btnSearchErShouFangMoney").style.display = "";
    }
    document.getElementById("cminPrice").onblur = function () {
        if (document.getElementById("cminPrice").value == "" && document.getElementById("cmaxPrice").value == "") {
            document.getElementById("btnSearchErShouFangMoney").style.display = "none";
        }
    }
    document.getElementById("cmaxPrice").onfocus = function () {
        document.getElementById("btnSearchErShouFangMoney").style.display = "";
    }
    document.getElementById("cmaxPrice").onblur = function () {
        if (document.getElementById("cminPrice").value == "" && document.getElementById("cmaxPrice").value == "") {
            document.getElementById("btnSearchErShouFangMoney").style.display = "none";
        }
    }
    document.getElementById("cminArea").onfocus = function () {
        document.getElementById("btnSearchArea").style.display = "";
    }
    document.getElementById("cminArea").onblur = function () {
        if (document.getElementById("cminArea").value == "" && document.getElementById("cmaxArea").value == "") {
            document.getElementById("btnSearchArea").style.display = "none";
        }
    }
    document.getElementById("cmaxArea").onfocus = function () {
        document.getElementById("btnSearchArea").style.display = "";
    }
    document.getElementById("cmaxArea").onblur = function () {
        if (document.getElementById("cminArea").value == "" && document.getElementById("cmaxArea").value == "") {
            document.getElementById("btnSearchArea").style.display = "none";
        }
    }
