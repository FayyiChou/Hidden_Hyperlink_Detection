<script type="text/javascript">    
//js获取日期
function time()
{
  var now= new Date();
  var year=now.getFullYear();
  var month=now.getMonth();
  var date=now.getDate();
  var day=now.getDay();
//星期判断
switch (day)
 {
    case 0:{
      strDate = "星期日"
    }break;
    case 1:{
      strDate = "星期一"
    }break;
    case 2:{
      strDate ="星期二"
    }break;
    case 3:{
      strDate = "星期三"
    }break;
    case 4:{
      strDate = "星期四"
    }break;
    case 5:{
      strDate = "星期五"
    }break;
    case 6:{
      strDate = "星期六"
    }break;
    case 7:{
      strDate = "星期日"
    }break;
 }
//写入相应id
 document.getElementById("info1").innerHTML="今天是"+year+"年"+(month+1)+"月"+date+"日"+strDate;
} 
     
     <!-- function checkzl(){ $("#searchform2").submit();}-->
    
    function checkzl(){
      if(jQuery.trim(jQuery(".infx").val())=="" ){
          alert("关键字不能为空!");
          jQuery(".infx").focus();
          return;
       }else{
          if(jQuery("#categoryId").val()=="A"){
              jQuery("#categoryId").val("");
              jQuery("input[name='type']").attr("value","zwgk");
          }
          else if(jQuery("#categoryId").val()=="B"){
              jQuery("#categoryId").val("");
              jQuery("input[name='type']").attr("value","info");
          }
    else if(jQuery("#categoryId").val()=="10286"){
           window.open("/info/iList.jsp?cat_id=13835&title="+encodeURIComponent(jQuery.trim(jQuery(".infx").val())));
        return false;
          }
          else if(jQuery("#categoryId").val()==""){
        jQuery("input[name='type']").attr("value","");
          }
          else{
              jQuery("input[name='type']").attr("value","info");
          }
          jQuery("#searchform2").submit();
       }
 }
</script>
