        <script>
                   var mi = 0;
                   var miTimer = null;
                   var imgarray = new Array();
                   $(".imagesindex a").each(
                      function () {
                          imgarray.push($(this).attr("bigsrc"));
                      }
                   )
                   function siledImage(){
                       var bt_curLi = $(".imagesindex a").eq(mi);
                       $(".imagesindex a").removeClass("on");
                       bt_curLi.addClass("on")
                       var hr = bt_curLi.attr("href");
                       var title = bt_curLi.attr("title");
                       var detail = bt_curLi.attr("detail");
					   var idVedio= bt_curLi.attr("idVedio");
					   if(idVedio=="true")
					   {
						  $(".imagestitle .vedioNews").show();
					   }
					   else
					   {
						   $(".imagestitle .vedioNews").hide();
					   }
                       $("#bigimga").attr("href", hr);
                       $(".imagestitle .title").text(title);
                       $(".imagestitle .detail").text(detail);
                       $(".imagesshow img").fadeTo("fast",0.7);
                       $(".imagesshow img").attr("src", imgarray[mi]);
                       $(".imagesshow img").fadeTo("fast",1);
                       if (mi >= imgarray.length - 1) {
                           mi = 0;
                       }
                       else { mi++; }
                   }
                   $(document).ready(function () {
                       siledImage();
                       miTimer = setInterval("siledImage()", 4000);
                       $(".imagesindex a").mouseover(
         function () {
             clearInterval(miTimer)
             if($(".imagesshow img").is(":animated"))
             {
             $(".imagesshow img").stop(true);
             }
             mi = $(this).index(); ;
             siledImage();
         }).mouseleave(
       function () {
           miTimer = setInterval("siledImage()", 4000);
       });
                       $("#bigimga").mouseover(
       function () {
           clearInterval(miTimer);
       }
       ).mouseleave(
       function () {
           miTimer = setInterval("siledImage()", 4000);
       }
       )
                   })
    
        </script>
