    <script language="javascript" type="text/javascript">

/*第一个飘窗*/
$("#close").click(function(){$("#ad1").css("display","none");});
var x1 = 0,y1 = 0 ;
var xin1 = false, yin1 = true ;
var step1 = 1 ;
var delay1 = 10 ;
var obj1= $('#ad1'); 
function floatAD1() { 
var L=0,T=0 
var R= $(window).width()/2-712; 
var B = $(window).height()-obj1.height();
obj1.css("left",x1 + $(document.body).scrollLeft()); 
obj1.css("top",y1 + $(document.body).scrollTop()); 
x1 = x1 + step1*(xin1?1:-1)*0.3 
if (x1 < L) { xin1 = true; x1 = L} 
if (x1 > R){ xin1 = false; x1 = R} 
y1 = y1 + step1*(yin1?1:-1)
if (y1 < T) { yin1 = true; y1 = T } 
if (y1 > B) { yin1 = false; y1 = B } 
} 
var itl1= setInterval("floatAD1()", delay1);
obj1.mouseover(function(){clearInterval(itl1);})
obj1.mouseout(function(){itl1=setInterval("floatAD1()", delay1);})
    </script>

        
        <script type="text/javascript">
      $(function(){
		  var num=1;
		  var hzmtLiNum = $(".hzmt_ul li").length;
		  var times =Math.ceil(hzmtLiNum/14);
		  $(".hzmtMod .left_arrow").click(function(){
												   
												   if(num>1)
												   {
													   num--; 
													   showHZMT(num);
												   }
												   else if(num==1)
												   {
													   num=times;
													   showHZMT(num);
												   }
										  })
		  $(".hzmtMod .right_arrow").click(function(){
												   num++;
												   if(num>times)
												   {
													   num=1;  
													   showHZMT(num);
												   }
												   else
												   {
													   showHZMT(num);
												   }
										  
										  })
	   })
	  function showHZMT(num){
		   var firstdot=(num-1)*14-1;
		   var lastdot=firstdot+14+1;
		   $(".hzmtMod li").hide();
		   if(firstdot<0)
		   {
			    $(".hzmtMod li:lt("+lastdot+")").show();
		   }
		   else
		   {
		     $(".hzmtMod li:lt("+lastdot+"):gt("+firstdot+")").show();
		   }
	  }
    </script>
