    $(function () {
        $("#txtkey").keydown(function (ev) {
            if (ev.keyCode == 13) {
                $(".search_area:visible .search_btn")[0].click();
                return false;
            }
        });
        $("#searchBut").click(function () {
            window.open('/Article/Search.aspx?key=' + encodeURI($(".search_area:visible .search_input").val().replace('搜索', '')));
        });
    });

