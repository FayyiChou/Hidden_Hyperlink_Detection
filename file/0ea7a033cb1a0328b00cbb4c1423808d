    <script>
        var nowPicindex = 0;
        var _int=0;
        $().ready(function () {
            showpic();
            _int = setInterval(showpic, 2000);
            $("#picnewslist tr").hover(function() {
                clearInterval(_int);
                _int = 0;
                $("#newsPic a").hide();
                $("#pic" + $(this).attr("id")).show();
            },function () {
                if(_int==0){
                    _int = setInterval(showpic, 2000);
                }
            });
        });
        function showpic() {
            if (nowPicindex >= $("#newsPic a").length) {
                nowPicindex = 0;
            }
            nowPicindex++;
            $("#newsPic a").hide();
            $("#newsPic a:nth-of-type(" + nowPicindex + ")").show();
        }
    </script>
