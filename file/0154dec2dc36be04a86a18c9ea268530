var destoon_message = 0;
var destoon_chat = 0;
var destoon_cart = substr_count(get_cookie('cart'), ',');
var destoon_member = '';
destoon_member += '您好，<span class="f_b">欢迎来到餐厅网！&nbsp;&nbsp;</span>&nbsp;&nbsp;<a href="http://www.cantingwang.com/member/login.php">&nbsp;&nbsp;[登录]</a>&nbsp;<a href="http://www.cantingwang.com/member/register.php">[免费注册]</a>';
$('#destoon_member').html(destoon_member);
$('#destoon_city').html('全国');
$('#destoon_cart').html(destoon_cart ? '<strong>'+destoon_cart+'</strong>' : 0);
$('#back2top').click(function() {
$("html, body").animate({scrollTop:0}, 200);
});
/**
 * 阻止btn按钮click事件
 *
 */
var btns = document.getElementsByName("topbtn");
for ( var i = 0; i < btns.length; i++) {
btns[i].onclick = stopDefault;
}

/**
 * 阻止浏览器的默认行为
 * 
 * @public
 * @param <event>
 *            浏览器事件
 */
function stopDefault(e) {
if (e && e.preventDefault) {
e.preventDefault();
} else {
window.event.returnValue = false;
}
return false;
}
</script>
</body>
</html>