<script type="text/javascript">
    function OpenTheSearch() {
        var keyword = encodeURIComponent($("#txtContent").val());
        var language = "zh";
        //modify by Gordon 2015.04.27 全部转小写
        language = language.toLowerCase();
        if (language == "cn" || language == "zh" || language == "en") {
            //add by xwx282787 头导航搜索功能优化
            try{
                if ($("#categorytype").val() != undefined && $("#categorytype").val()!='') {
                    window.open("http://e.huawei.com/enterprisesearch/ebgSearch?keyword=" + keyword + "&category=" + $("#categorytype").val() + "&language=" + language);
                } else {
                    window.open("http://e.huawei.com/enterprisesearch/ebgSearch?keyword=" + keyword + "&category=ALL&language=" + language);
                }
            } catch (ex)
            {
                window.open("http://e.huawei.com/enterprisesearch/ebgSearch?keyword=" + keyword + "&category=ALL&language=" + language);
            }
        }
        else {
            window.open("/" + language + "/SearchUnified.aspx?keywords=" + keyword + "&category=searchall");
        }
    }

    function EnterSearch() {
        var event = window.event || arguments.callee.caller.arguments[0];
        if (event.keyCode == 13) {
            OpenTheSearch();
        }
    }
</script>

