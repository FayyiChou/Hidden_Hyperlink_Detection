<script src="/public/javascripts/laydate/laydate.js"></script>

<script type="text/javascript">
		$(function(){
			var jsAction = function(options) {var pattern = '/Application/getProvinces'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }

			$.post(jsAction({}),function(data) {
				var cityList = [];
				for(var i = 0; i < data.length; i ++){
					cityList.push("<option value='"+data[i].id+"'>"+data[i].name+"</option>");
				}
				$("#province").html(cityList);
			});
		});
		$("#province").change(function(){
			var selectVal = $(this).children('option:selected').val();
			var jsAction = function(options) {var pattern = '/Application/getCity?provinceId=:provinceId'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }

			$.post(jsAction({provinceId:selectVal}),function(data) {
				var cityList = [];
				for(var i = 0; i < data.length; i ++){
					cityList.push("<option value='"+data[i].id+"'>"+data[i].name+"</option>");
				}
				$("#city").html(cityList);
			});
		});

	//资产负债率
     function assetCount(){
    	 var total_assets_of_enterprises=$("#total_assets_of_enterprises").val();
    	 var total_corporate_liabilities=$("#total_corporate_liabilities").val();
    	 if(total_assets_of_enterprises!='' && total_corporate_liabilities!=''){
    		 $("#asset_liability_ratio").val((total_corporate_liabilities/total_assets_of_enterprises).toFixed(2));
    	 }
    	 
     }
     //销售利润率
     function salesProfitMarginCount(){
    	 var gross_profit=$("#gross_profit").val();
    	 var last_year_revenue=$("#last_year_revenue").val();
    	 if(gross_profit!='' && last_year_revenue!=''){
    		 $("#sales_profit_margin").val((gross_profit/last_year_revenue).toFixed(2));
    	 }
    	 
     }
     
   //流动比率
     function flowRateCount(){
    	 var current_assets=$("#current_assets").val();
    	 var current_liabilities=$("#current_liabilities").val();
    	 if(current_assets!='' && current_liabilities!=''){
    		 $("#flow_rate").val((current_assets/current_liabilities).toFixed(2));
    	 }
    	 
     }
     //资产收益率
     function returnOnssetsCount(){
    	 var net_profit=$("#net_profit").val();
    	 var average_balance_of_total_assets=$("#average_balance_of_total_assets").val();
    	 if(net_profit!='' && average_balance_of_total_assets!=''){
    		 $("#return_on_assets").val((net_profit/average_balance_of_total_assets).toFixed(2));
    	 }
    	 
     }
     function saveIsInstitutions(){
    	 if($("#con").val()==''){
    		 alert("请输入企业名称");
    		 return;
    	 }
    	 if($("#crertificate_name").val()==''){
    		 alert("请输入证件名称");
    		 return;
    	 }
    	 if($("#crertificate_id").val()==''){
    		 alert("请输入证件号码");
    		 return;
    	 }
    	 if($("#registration_authority").val()==''){
    		 alert("请输入登记机关");
    		 return;
    	 }
    	 if($("#registration_id").val()==''){
    		 alert("请输入登记注册号");
    		 return;
    	 }
    	 if($("#economic_composition").val()==''){
    		 alert("请输入经济成分");
    		 return;
    	 }
    	 if($("#registration_date").val()==''){
    		 alert("请输入注册登记日");
    		 return;
    	 }
    	 if($("#registered_capital").val()==''){
    		 alert("请输入注册资本");
    		 return;
    	 }
    	 if($("#paid_in_capital").val()==''){
    		 alert("请输入实收资本");
    		 return;
    	 }
    	 if($("#annual_inspection_id").val()==''){
    		 alert("请输入年检年度");
    		 return;
    	 }
    	 if($("#id_effective").val()==''){
    		 alert("请输入是否有效");
    		 return;
    	 }
    	 if($.trim($("#business_scope").val())==""){
    		 alert("请输入经营范围");
    		 return;
    	 }
    	 if($("#nature_of_company").val()==''){
    		 alert("请输入公司性质");
    		 return;
    	 }
    	 if($("#province").val()==''){
    		 alert("请输入公司地址");
    		 return;
    	 }
    	 if($("#city").val()==null){
    		 alert("请输入公司地址");
    		 return;
    	 }
    	 if($("#address_detail").val()==null){
    		 alert("请输入公司详细地址");
    		 return;
    	 }
    	 if($("#company_tel").val()==''){
    		 alert("请输入联系电话");
    		 return;
    	 }
    	 if($("#representative_name").val()==''){
    		 alert("请输入法定代表人姓名");
    		 return;
    	 }
    	 if($("#document_type").val()==''){
    		 alert("请输入证件类型");
    		 return;
    	 }
    	 if($("#document_num").val()==''){
    		 alert("请输入证件号码");
    		 return;
    	 }
    	 if($("#total_assets_of_enterprises").val()==''){
    		 alert("请输入企业总资产");
    		 return;
    	 }
    	 if($("#total_corporate_liabilities").val()==''){
    		 alert("请输入企业总负债");
    		 return;
    	 }
    	 if($("#asset_liability_ratio").val()==''){
    		 alert("请输入资产负债率");
    		 return;
    	 }
    	 if($("#last_year_revenue").val()==''){
    		 alert("请输入上年末销售收入");
    		 return;
    	 }
    	 if($("#gross_profit").val()==''){
    		 alert("请输入利润总额");
    		 return;
    	 }
    	 if($("#sales_profit_margin").val()==''){
    		 alert("请输入销售利润率");
    		 return;
    	 }
    	 if($("#current_assets").val()==''){
    		 alert("请输入流动资产");
    		 return;
    	 }
    	 if($("#current_liabilities").val()==''){
    		 alert("请输入流动负债");
    		 return;
    	 }
    	 if($("#flow_rate").val()==''){
    		 alert("请输入流动比率");
    		 return;
    	 }
    	 if($("#net_profit").val()==''){
    		 alert("请输入净利润");
    		 return;
    	 }
    	 if($("#average_balance_of_total_assets").val()==''){
    		 alert("请输入总资产平均余额");
    		 return;
    	 }
    	 if($("#return_on_assets").val()==''){
    		 alert("请输入资产收益率");
    		 return;
    	 }
    	 if($("#net_operating_cash_flow").val()==''){
    		 alert("请输入经营性现金净流量");
    		 return;
    	 }
    	 var curWwwPath=window.document.location.href;
    	 $("#curWwwPath").val(curWwwPath);
    	 $("#saveIsInstitutions").submit();
     }
</script>
