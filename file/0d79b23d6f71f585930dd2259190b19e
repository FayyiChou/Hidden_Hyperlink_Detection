<script type="text/javascript">
function closeAddBaseInfos(){
	$("#is_institutions_view").hide();
}
function closeEmilss_yy(){

    $("#ss_yy").hide();
}
  $(function(){
     $("#claim").hover(function () {
    	   $("#claim dl").show();
	 },function () {
		 $("#claim dl").hide();
	 });
	 $("#claim dl dd a").hover(function () {
    	   $(this).css({background:"#18B160"});
	 },function () {
		 $(this).css({background:"#1E5390"});
	 });
  });
  $(document).keydown(function(event){
    if(event.keyCode == 13){
      if(document.activeElement.id == 'name' || document.activeElement.id == 'code' || document.activeElement.id == 'password'){
    	  login();
      }
    }
  });
  function vipApply(){
	 if('' == 'true'){
		 showDiv($("#xf_mem_acc_sjvip_tcc"));
		 return;
     } 
     /*else if('' == 'false') {
     	 showDiv($("#email_box"));
     	 return;
     }*/
     else if('' == 'false'){//未绑定手机
    	 $.layer({
 		    shade: [0],
 		    area: ['auto','auto'],
 		    dialog: {
 		        msg: '尊敬的用户您好，请设置安全手机号码!',
 		        btns: 2,                    
 		        type: 4,
 		        btn: ['立即前往','取消'],
 		        yes: function(){
 		            window.location.href='/front/account/modifyMobile';
 		        }, no: function(){
 		            layer.closeAll();
 		        }
 		    }
 		});
    	 return;
     }
    showDiv($("#xf_mem_acc_sjvip_tcc"));
  }

  function login(){
    var name = $("#name").val();
           
    if(name == ''){
      alert("请输入用户名或邮箱");
      return;
    }

    if($("#password").val() == ''){
      alert("请输入密码");
      return;
    }

    if($("#code").val() == ''){
      alert("请输入验证码");
      return;
    }
    $("#submit").submit();
  }

  $(function(){
	$('img.lazy').lazyload({ 
	  //effect:'fadeIn' 
	});
    $("#xf_h_idx_1_hydjfdgdf").limit();
	   
    /* 所有继承页面flash.error的值弹框,被继承页面则无需再写 */
    if('' !='') {
      alert('');
      if('' !='') {
        window.open('');
      }
    }
	/*与重复提交冲突，注释，如功能发生冲突，请更改其他处
    if('' != ''){
      alert('');
    }
	*/  	
    //获取图片验证码
    if('' == '' && '' == '') {
      var setCode = function(options) {var pattern = '/front/account/setCode'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }

      $.post(setCode({}),function(data){
        var arr=eval(data);
        var img = arr.img;
        $("#img").attr("src","/front/account/getImg?id="+img)	
		$("#randomID").val(img);
	  });
	}
	//是否填写机构信息
	var setCode = function(options) {var pattern = '/front/account/queryInstitutions'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }

    $.post(setCode({}),function(data){
      	var arr=eval(data);
      	var code = arr.code;
      	if(code==1){
      		showDiv($("#is_institutions_view"));
    	}
	  });
    
  })

  function change(){
    var setCode = function(options) {var pattern = '/front/account/setCode'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }

	$.post(setCode({}),function(data){
	  var arr=eval(data);
	  var img = arr.img;
	  $("#img").attr("src","/front/account/getImg?id="+img);
	  $("#randomID").val(img);	
	});
  }
  function AddFavorite(url,title) {
    var ua = navigator.userAgent.toLowerCase();
    if(ua.indexOf("msie 8")>-1){
      external.AddToFavoritesBar(url,title,'');//IE8
    }else{
      try {
        window.external.addFavorite(url, title);
      } catch(e) {
        try {
          window.sidebar.addPanel(title, url, "");//firefox
        } catch(e) {
          alert("加入收藏失败，请使用Ctrl+D进行添加");
        }
      }
    }
  }


  function formatCash( cash ){
	  
  	var str_cash = cash + "";//转换成字符串
  	var ret_cash = "";
  	var counter = 0;
  	for(var i=str_cash.length-1;i>=0;i--){//去除0开始的字符串如：000212去除后为212
  	    if(str_cash.substring(0,1)=="0"){
  	    	str_cash=str_cash.substring(1,str_cash.length)
  	    }else{
                 break;
  		}
  	}
  	for(var i=str_cash.length-1;i>=0;i--){
  		ret_cash = str_cash.charAt(i) + ret_cash;
  		counter++;
  		if(counter==3){
  			counter = 0;
  			if(i!=0)
  			ret_cash = "," + ret_cash;
  		}
  	}
  	return ret_cash;
  }

  function appendspan(income,idstr){
  	if(income.length>8){
      	var income1=income.substring(0,income.length-8);
      	var income2=income.substring(income.length-8,income.length-4);
      	income1=formatCash(income1);
      	income2=formatCash(income2);
      	$("#"+idstr).after("<span>"+income1+"</span>亿"+"<span>"+income2+"</span>万");
  	}else{
           if(income.length>4){
          	 var income1=income.substring(income.length-8,income.length-4);
          	 var income2=income.substring(income.length-4,income.length);
          	 income1=formatCash(income1);
   	    	 income2=formatCash(income2);
          	 $("#"+idstr).after("<span>"+income1+"</span>万"+"<span>"+income2+"</span>");
           }else{
          	 if(income!=0){
          	 income=formatCash(income);
             }
          	 $("#"+idstr).after("<span>"+income+"</span>");
           }
      }
  }

</script>
</html>