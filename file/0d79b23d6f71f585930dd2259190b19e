<script type="text/javascript">
	$(function(){
		$("#email_box").center();
	});
   /* 激活 */
   function acitveEmail() {
		$.ajax({
			url : "/front/account/activeEmail",
			type : "POST",
			data : {},
			success : function(data) {
			
			alert(data[0].msg);
			
			if (data[0].code < 0) 
				return;
			
			$("#email_box").hide();
			window.open(data[1]) ;
		},
	 	error : function() {
			alert("对不起，出现错误!");
	 	}
	});
   }
   
   function closeEmil(){
   		$('#email_box').hide();
   		//location.reload();
   } 
</script>
		
