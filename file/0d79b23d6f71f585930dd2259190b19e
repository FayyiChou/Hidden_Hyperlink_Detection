<script type="text/javascript">
	function showProtocolF() {
		$.ajax({
			url : "/vipAgreement",
			type : "POST",
			data : {
			},
			success : function(data) {
				$("#showVipAgreement").html(data);
			},
			
			error : function() {
				alert("对不起，出现错误!");
			}
		});
	
		
		showDiv($("#xf_mem_zc_xytc"));
	}
	
	function showRechargeWin() {
		showDiv($("#rechargeWin"));
	}
</script>


<script type="text/javascript">
     
     $("#vipObject").html($("#currUserName").val());

	 var vipMinTypeF = '';
	 if($("#vipMinTimeType").val() == 0) {
         vipMinTypeF="年";
     }else{
        vipMinTypeF="个月";
     }
     
     var vipTypeF = "";
     if($("#vipTimeType").val() == 0) {
         vipTypeF="年";
     }else{
        vipTypeF="月";
     }
     
     $("#times").html(vipTypeF);
     
	var minTimeF = "至少开通"+$("#vipMinTimeLength").val()+vipMinTypeF;
	
    $("#time").val(minTimeF);
	
	$(function(){
		
		$(".xf_wyjkfb_4_windowClose").click(function(){
			$(this).parents(".xf_zqzr_znx_window").hide(300);
		});
        
		$("#time").focus(function(){	 
			if($(this).val() == minTimeF){  
              	$(this).val("");           
		    } 
		}).blur(function(){
			if ($(this).val() == '') {
               $(this).val(minTimeF);
            }
		});	
		
		$(".xf_wyjkfb_4_windowClose").click(function(){
			$(this).parents(".xf_zqzr_znx_window").hide(300);
		});
		
	});
	
		

   function clickDownF() {
		var vipFee = $("#vipAmount").val();
		var vipDiscount = $("#vipDiscount").val();
		var time = $("#time").val();
		if(time == '' || time== minTimeF) {
	 		$("#repaymentF").html("");
	 		return;
	 	}
	 	
	   if(isNaN(time)) {
	   		$("#time").val("")
	   		$("#repaymentF").html("");
	 		return;
	 	}
		
		$("#repaymentF").html(vipFee*time*vipDiscount/10);
	}
		 
	 function applyVip(){
	 	var timeF = $("#time").val();
	 	if(timeF == '' || timeF== minTimeF) {
	 		alert("请输入申请时长");
	 		return;
	 	}
	 	
	 	if(isNaN(timeF)) {
	 		alert("请输入正确的申请时间");
	 		return;
	 	}
	 	
	 	var vipMinTimeF = $("#vipMinTimeLength").val();
	 	if( $("#vipMinTimeType").val() == 0) {
         	 vipMinTimeF = vipMinTimeF * 12;
     	}
     	
     	if($("#vipTimeType").val() == 0) {
     		timeF = timeF * 12;
     	}
	 	
	 	if(timeF*1 < vipMinTimeF*1) {
	 		alert("不能小于最短申请时间");
	 		return;
	 	}
	 	
	 	if(!$("#vipAgreementF").attr("checked")){
	        alert("请认真阅读并同意会员服务条款");
	        return;
	    }
	    
	 	var vipApply = function(options) {var pattern = '/front/account/vipApply?serviceTime=:time&type=:type'; for(var key in options) { pattern = pattern.replace(':'+key, options[key] || ''); } return pattern }

		$.post(vipApply({time:timeF, type:1}),function(data){
			var arr=eval(data);
			
			if (arr.error.code == -999) {
					
						alert(arr.error.msg);
						window.open("/front/account/rechargePay");
						return;
					
					
					
			} else if (arr.error.code == -998) {
					
					
			}
			
			if(arr.error.code < 0){
			    alert(arr.error.msg);
			    return;
			}
		
		alert(arr.error.msg);
		location.reload();
	  });
	 }
	 
	 function oncheckF(){
		 if($("#vipAgreementF").attr("checked")){
	         $("#vipAgreementF").attr("checked",false);
	    }else{
	         $("#vipAgreementF").attr("checked",true);
	    }
	  }
</script>
		
