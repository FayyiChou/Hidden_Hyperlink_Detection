<script>
var num="0123456789";
function Validchar(g,str){
for( var i=0 ; i < g.length; i++ ){
if( str.indexOf(g.charAt(i)) == -1 )
return g.charAt(i);
}
return true;
}
	function IsSpace(s){
	  var flag= false;
	  for (i = 0; i < s.length; i++){
	     if (s.substring(i, i+1) == ' '){
	        flag = true;
	        break;
	     }
	  }
	  return(flag);
	}
	function validform(){
		var uname=loginform.username.value;
		var upwd=loginform.password.value;
		if ((uname=="")||(upwd=="")){
			alert('用户名和密码不能为空。');
			return false;
		}
		if ((IsSpace(uname))||(IsSpace(upwd))){
			alert('用户名和密码不能有空格。');
			return false;
		}

if(loginform.bdyz.value.length==0)
{
alert("请输入验证码！");
document.loginform.bdyz.focus();
return false;
}
if( (ret=Validchar(loginform.bdyz.value,num)) != true)

{
alert("验证码只能是数字 0-9 。");
loginform.bdyz.focus();
return false;
}
	return true;
	}
</script>
