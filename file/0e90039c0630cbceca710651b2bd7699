				  function navTab1(n){
						var tli=document.getElementById("con_r1_ul").getElementsByTagName("li");
						var mli=document.getElementById("con_r1_list").getElementsByTagName("ul");
						for(i=0;i<tli.length;i++){
   						tli[i].className=i==n?"hover":"";
  						mli[i].style.display=i==n?"block":"none";
						}
				 }
				  function navClear(){
					  	var tli=document.getElementById("con_r1_ul").getElementsByTagName("li");
						var mli=document.getElementById("con_r1_list").getElementsByTagName("ul");
						for(i=0;i<tli.length;i++){
							tli[i].className="";
  							mli[i].style.display="none";
						}
				 }
				 
