            <div class="mtcontengadctop"><img src="/templates/master/newmaotai/images/newmaotai/maotainew_26_03.jpg" onclick="shangfan();" width="163" style=" overflow:hidden" height="20" /></div>
            <script>

                function shangfan() {
                    var ulElement = $(".mtcontengadccote ul");
                    var lineHeight = -118;
                    var speed = 1000;
                    ulElement.animate({
                        marginTop: -118
                    }, speed, function () {
                        ulElement.find("li:first").appendTo(ulElement);
                        ulElement.css({ marginTop: 0 });
                    });
                }

                function xiafan() {
                    var ulElement = $(".mtcontengadccote ul");
                    var lineHeight = 118;
                    var speed = 1000;
                    ulElement.animate({
                        marginTop: 0
                    }, speed, function () {
                        ulElement.find("li:last").prependTo(ulElement);
                        ulElement.css({ marginTop: -118 });
                    });
                }
