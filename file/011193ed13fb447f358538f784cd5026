    <script type=text/javascript>

        var _hmt = _hmt || [];
        (function () {
            var hm = document.createElement("script");
            hm.src = "//hm.baidu.com/hm.js?3b97352bf7acd895b6c95cdc51ade387";
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(hm, s);
        })();


        var _c = _h = 0;
        $(function () {

            $('#play > li').click(function () {
                var i = $(this).attr('alt') - 1;
                clearInterval(_h);
                _c = i;
                play();
                change(i);
            })
            $("#pic div").hover(function () { clearInterval(_h) }, function () { play() });
            play();
            //删除组合推荐下的立即
            var parent = $("#products_6_item_1").children("ul").children("li").children("div.price").children("span").remove("span");
        })
        function play() {
            _h = setInterval("auto()", 5000);
            //轮播时间间隔 许飘注释
        }

        function change(i) {
            $('#play > li').addClass("bg normal");

            $("#li" + i).removeClass("bg normal");
            $("#li" + i).addClass("bg active");
            $("#pic div").hide().eq(i).fadeIn('slow');
        }
        function auto() {    //播发多少个后循环 许飘注释
            _c = _c >= 1 ? 0 : _c + 1;
            change(_c);
        }

    </script>

