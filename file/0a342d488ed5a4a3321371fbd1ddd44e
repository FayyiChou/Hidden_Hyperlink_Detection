													demo.scrollLeft--
													if(demo.scrollLeft=="0") demo.scrollLeft=demo1.offsetWidth
												}
											else
												{
													demo.scrollLeft++
													if(demo.scrollLeft>=demo1.offsetWidth) demo.scrollLeft=0
												}
										}
								}
							function setztcspeed(x)
								{
									ztcspeed=x;
								}
