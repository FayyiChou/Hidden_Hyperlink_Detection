    <script>
        function checkTime(i) {
            if (i < 10) {
                i = "0" + i;
            }
            return i;
        }
        function timer(id) {
            var timeData = $("#ptimer" + id).find('.time').eq(0);
            var NowTime = new Date();
            var ts = parseInt(timeData.attr('eventEnd')) - parseInt(NowTime.getTime()) / 1000;
            if (ts < 0) {
                return;
            }
            var dd = parseInt(ts / 60 / 60 / 24, 10);//计算剩余的天数
            var hh = parseInt(ts / 60 / 60 % 24, 10);//计算剩余的小时数
            var mm = parseInt(ts / 60 % 60, 10);//计算剩余的分钟数
            var ss = parseInt(ts % 60, 10);//计算剩余的秒数
            dd = checkTime(dd);
            hh = checkTime(hh);
            mm = checkTime(mm);
            ss = checkTime(ss);
            $("#dd" + id).html(dd);
            $("#hh" + id).html(hh);
            $("#mm" + id).html(mm);
            $("#ss" + id).html(ss);
            setTimeout(function () {
                timer(id)
            }, 1000);
        }
    </script>
