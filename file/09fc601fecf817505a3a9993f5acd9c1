<script>
    $("#shopFirst .tab").each(function (index) {
        $(this).hover(function () {
            $("#shopFirst .tab").removeClass("cur");
            $(this).addClass('cur');
            $("#shopFirst .product").hide();
            $("#shopFirst .product").eq(index).show();
        });
    });
    $("#typeProduct .tab").each(function (index) {
        $(this).hover(function () {
            $("#typeProduct .tab").removeClass("cur");
            $(this).addClass('cur');
            $("#typeProduct .product").hide();
            $("#typeProduct .product").eq(index).show();
        });
    });

    $("#jinrong .tab").each(function (index) {
        $(this).hover(function () {
            $("#jinrong .tab").removeClass("cur");
            $(this).addClass('cur');
            $("#jinrong .wape").hide();
            $("#jinrong .wape").eq(index).show();
        });
    });
