    // 百度地图API功能，定位并获取城市、省份名称
    var map = new BMap.Map("allmap");
    var point = "";
    var positions = "";
    var geoc = new BMap.Geocoder();
    //获取经纬度，并确定城市
    if(localStorage.ciicCity && localStorage.ciicCity != "未知" || localStorage.ciicProvince && localStorage.ciicProvince!= "未知"){
        console.log("run localStorage ciic areaInfo !");
        localInfo.city = localStorage.ciicAreaCity;
        localInfo.province = localStorage.ciicProvince;
        //测试数据
        localInfo.city = getareaid();
    }else{
        var geolocation = new BMap.Geolocation();
        geolocation.getCurrentPosition(function(r){
            // console.log("使用百度定位返回的状态码:"+this.getStatus());
            if(this.getStatus() == BMAP_STATUS_SUCCESS){
                var mk = new BMap.Marker(r.point);
                map.addOverlay(mk);
                map.panTo(r.point);
                point = new BMap.Point(r.point.lng,r.point.lat);
                // console.log(point);
                geoc.getLocation(point,function (rs){
                    setTimeout(function(){
                        getCity(rs);
                    },500);
                });
            }else {//使用IP定位进行辅助定位
                // console.log("地理定位失败，使用ip定位辅助定位");
                var myCity = new BMap.LocalCity();
                myCity.get(myFun);
            }        
        },{enableHighAccuracy: true});
    }
function getCity(rs){
    var addComp = rs.addressComponents;
    // console.log(addComp);
    if(addComp.province === ""){
        // console.log("没有获取到定位城市信息");
        localInfo.city = "未知";
        localInfo.province = "未知";
        getareaid();
    }else{
        localInfo.city = addComp.city.replace(/省|市|自治区/ig,"");
        localStorage.ciicAreaCity = localInfo.city;
        localInfo.province = addComp.province.replace(/省|市|自治区/ig,"");
        localStorage.ciicProvince = localInfo.province;
        // console.log(localInfo);//获取到城市、省份信息以后，查询areaid库，得到areaid
        getareaid();
    }
}
//使用地理定位获取到城市信息
function myFun(result){
    var cityName = result.name;
    localInfo.city = cityName.replace(/省|市|自治区/ig,"");
    console.log("ip获取的城市名称为:"+localInfo.city);
    localStorage.ciicAreaCity = localInfo.city;
    getareaid();
}
// 根据城市获取areaid，并展示
function getareaid(){
    if(localInfo.findArea.indexOf(localInfo.city) > 0){
        localInfo.areaid = localInfo.findArea.substring(localInfo.findArea.indexOf(localInfo.city)-9,localInfo.findArea.indexOf(localInfo.city));
        // console.log(localInfo.areaid);
    }else{
        if(localInfo.findArea.indexOf(localInfo.province) > 0){
            localInfo.areaid = localInfo.findArea.substring(localInfo.findArea.indexOf(localInfo.province)-9,localInfo.findArea.indexOf(localInfo.province));
        }
        else{
            localInfo.areaid = "101010100";
        }
    }
    // 根据 areaid 从中国网天气频道获取天气;
    (function (window) {
        var CIICWeather = window.CIICWeather || {};
        window.cb_forecast = function (data_forecast) {
            var tpl = '<span class="fl">{city}</span><img class="ml5 fl" width="20" height="20" src="{forecast}" /><span class="ml5 fl">{temperature}℃</span>';
            document.getElementById('weather').innerHTML = CIICWeather.template(tpl, {
                city: data_forecast.city.c3,
                forecast: 'http://images.china.cn/images1/ch/2015tq/widget/icon_s/' + (!!data_forecast.forecast.f1[0].fa ? 'd' + data_forecast.forecast.f1[0].fa : 'n' + data_forecast.forecast.f1[0].fb) + 's.jpg',
                temperature: (!!data_forecast.forecast.f1[0].fc ? data_forecast.forecast.f1[0].fc : data_forecast.forecast.f1[0].fd)
            });
        };
        CIICWeather.embed_script(CIICWeather.template(CIICWeather.SERVICE_FORECAST,{
            // 'areaid': CIICWeather.DEFAULT_AREAID
            'areaid': localInfo.areaid
        }));
        // 读取从中国网天气频道获取天气的数据，如果没有读到等待5s，然后放弃
        var waiteDate = setInterval(function(){
            if($("#weather").find("span.ml5").text() != "-℃"){// 读到数据以后，转义成本地数据，保存到localInfo中，
                // console.log($("#weather").find("span.ml5").text());
                localInfo.city = $("#weather").find("span.fl").not(".ml5").text();
                var temp = $("#weather").find("span.ml5").text();
                localInfo.temp = temp.substring(0,temp.length-1);
                localInfo.WeaImg = $("#weather").find("img.ml5").attr("src");
                localInfo.WeaCode = localInfo.WeaImg.substring(localInfo.WeaImg.length-7,localInfo.WeaImg.length-5);
                localInfo.WeaImg = "http://m.china.cn/weather/"+localInfo.WeaCode+".png";
                if(parseInt(localInfo.WeaCode,10) == "53" || parseInt(localInfo.WeaCode,10) == "99"){
                    if(parseInt(localInfo.WeaCode,10) == "53"){
                        localInfo.WeaText = "霾";
                    }else{
                        localInfo.WeaText = "未知";
                    }
                }else{
                    localInfo.WeaText = localInfo.findText[parseInt(localInfo.WeaCode,10)];
                }
                //给天气模块赋值
                $(".frontWendu").text(localInfo.temp+"°");
                $(".frontWeaInfo").find("span.frontWeaName").text(localInfo.WeaText);
                $(".frontWeaInfo").find("img.frontWeaImg").attr("src",localInfo.WeaImg);
                // $(".frontWeaP").text(localInfo.city);
                localInfo.getDate = true;
                clearInterval(waiteDate);
            }else{
                console.log("waite weather data");
            };
        },500);
        setTimeout(function(){  //8s实在读不到数据，就放弃
            clearInterval(waiteDate);
            //时间内没有拿到数据,赋假值
            if(!localInfo.getDate){
                // localInfo.city = "北京";
                // localInfo.temp = "18";
                // localInfo.WeaImg = "img/tianqi.png";
                // localInfo.WeaText = "多云";
                $(".frontWendu").text(localInfo.temp+"°");
                $(".frontWeaInfo").find("span.frontWeaName").text(localInfo.WeaText);
                $(".frontWeaInfo").find("img.frontWeaImg").attr("src",localInfo.WeaImg);
                // $(".frontWeaP").text(localInfo.city);
            }
        },8000);
    }(window));
}
</script>
