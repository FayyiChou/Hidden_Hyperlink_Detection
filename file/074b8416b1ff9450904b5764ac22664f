<script language="javascript">
function checkKeywords()
{
	if (document.all.keyword.value=='')
	{
		alert('请输入关键字!');
		document.all.keyword.focus(); 
		return false;
	}
	else
	{ 
	   var selid=document.getElementById('sType').selectedIndex;
	    if (selid==0 ||selid==1)
		{
		   
		   document.frmmarketDefualt.action='./pgwlist.asp';
		   
		   document.frmmarketDefualt.submit();
		}
		return true;
	}
}
</script>  

