case 1:window.location="/xinge/";break;  
case 2:window.location="/xinge/product/pigeon.htm";break;      
case 3:window.location="/xinge/product/newpigeon.asp";break;     
case 4:window.location="/xinge/product/pith_list.asp";break; 
case 51:window.location="/xinge/product/product_buy.htm";break;
case 5:window.location="http://www.chinaxinge.com/company/products.asp?cid=5&pid=5";break; 
case 6:window.location="/xinge/product/netshop.asp";break;    
case 7:window.location="default.asp?shop_id=";break;    
case 8:window.location="http://www.chinaxinge.com/company/products.asp?cid=4&pid=4";break;   
case 10:window.location="http://www.chinaxinge.com/company/products.asp?cid=7&pid=7";break; 
case 11:window.location="/xinge/pm/pgw.asp";break;    
case 12:window.location="/xinge/product/peidui_list.asp";break;
case 9:window.location="/xinge/shopmanage/";break;  
}    
}  
