function check_myformyy()
{
	if (document.myformyy.xm.value == "" || document.myformyy.xm.value == "您的称呼")
	{
		document.myformyy.xm.focus();
		return false;
	}
	if (document.myformyy.dh.value == "" || document.myformyy.dh.value == "联系电话")
	{
		document.myformyy.dh.focus();
		return false;
	}
	if (document.myformyy.dq.value == "" || document.myformyy.dq.value == "所在地区")
	{
		document.myformyy.dq.focus();
		return false;
	}
}
