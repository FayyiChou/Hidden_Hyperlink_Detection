	<script>
$(function(){
	$(".item5").hover(
		function(){
			var that=this;
			item5Timer=setTimeout(function(){
				$(that).find("div").animate({width:643,height:386},300,function(){
					$(that).find("h2").fadeOut(200);
					$(that).find("dl").fadeIn(200);
				});
			},100);
			
			},
		function(){
			var that=this;
			clearTimeout(item5Timer);
			$(that).find("dl").fadeOut(200);
			$(that).find("div").stop().animate({width:0,height:0},300);
			$(that).find("h2").fadeIn(200);
			}
		)
	
});

$(function(){
	$(".item4").hover(
		function(){
			var that=this;
			item5Timer=setTimeout(function(){
				$(that).find("div").animate({width:211,height:195},300,function(){
					$(that).find("h2").fadeOut(200);
					$(that).find("dl").fadeIn(200);
				});
			},100);
			
			},
		function(){
			var that=this;
			clearTimeout(item5Timer);
			$(that).find("dl").fadeOut(200);
			$(that).find("div").stop().animate({width:0,height:0},300);
			$(that).find("h2").fadeIn(200);
			}
		)
	
});

$(function(){
	$(".item3").hover(
		function(){
			var that=this;
			item5Timer=setTimeout(function(){
				$(that).find("div").animate({width:211,height:185},300,function(){
					$(that).find("h2").fadeOut(200);
					$(that).find("dl").fadeIn(200);
				});
			},100);
			
			},
		function(){
			var that=this;
			clearTimeout(item5Timer);
			$(that).find("dl").fadeOut(200);
			$(that).find("div").stop().animate({width:0,height:0},300);
			$(that).find("h2").fadeIn(200);
			}
		)
	
});
</script> 
	<!--精品案例结束--> 
	
