<script> 
var x_fly11 = 20,y_fly11 = 20
var xin_fly11 = true, yin_fly11 = true 
var step_fly11 = 1
var delay_fly11 = 24
var obj_fly11=document.getElementById("wjdc_fly11") 
function wj_fly11() { 
var L_fly11=T_fly11=0
var R_fly11= document.documentElement.clientWidth-obj_fly11.offsetWidth 
var B_fly11 = document.documentElement.clientHeight-obj_fly11.offsetHeight 
obj_fly11.style.left = x_fly11 + document.documentElement.scrollLeft + "px"
obj_fly11.style.top = y_fly11 + document.documentElement.scrollTop+ "px"
x_fly11 = x_fly11 + step_fly11*(xin_fly11?1:-1) 
if (x_fly11 < L_fly11) { xin_fly11 = true; x_fly11 = L_fly11} 
if (x_fly11 > R_fly11){ xin_fly11 = false; x_fly11 = R_fly11} 
y_fly11 = y_fly11 + step_fly11*(yin_fly11?1:-1) 
if (y_fly11 < T_fly11) { yin_fly11 = true; y_fly11 = T_fly11 } 
if (y_fly11 > B_fly11) { yin_fly11 = false; y_fly11 = B_fly11 } 
} 
var itl_fly11= setInterval("wj_fly11()", delay_fly11) 
obj_fly11.onmouseover=function(){clearInterval(itl_fly11)} 
obj_fly11.onmouseout=function(){itl_fly11=setInterval("wj_fly11()", delay_fly11)} 
</script>
