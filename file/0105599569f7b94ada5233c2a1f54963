			        if(data.s==1){
					  for(var $i=0;$i<data.num;$i++){
 						 $tempHtml +="<li><a href='/login/"+data['res'][$i]['gid']+"/"+data['res'][$i]['sid']+"' target='_blank'><span>"+data['res'][$i]['gamename']+"</span><em>"+data['res'][$i]['servername']+"</em></a></li>";
				    }
					}else{
					 $tempHtml +="暂无";
					} 
		        	$('#mygames').html($tempHtml);
	        		$.dialog({
					   id:'dialog1',
					   title:'提示信息',
					   content:"<div style='text-align:center;'><img style='width:14px; height:14px;' src='/themes/main_template/Public/images/loading.gif' alt='loading'><span>&nbsp;请稍候,操作正在进行中.</span>"+data.script+"</div>",
					   lock:true,
					   time:1.1
				   })
				  }
				location.reload();
				window.location.href = urls;
			}
			
		});
        }
    }
    }

});
