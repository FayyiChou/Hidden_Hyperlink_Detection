<script>
$(document).on('ready', function(){

    $('input#input').each(function(index, el) {
          $(this).rating({displayOnly: true, step: 0.1});      
    });
    $('div.prod_item div.img img').each(function(){

        var error = false;
        if (!this.complete) {
            error = true;
        }

        if (typeof this.naturalWidth != "undefined" && this.naturalWidth == 0) {
            error = true;
        }
      

        if(error){
            $(this).bind('error.replaceSrc',function(){
                this.src = "images/none.gif";

                $(this).unbind('error.replaceSrc');
            }).trigger('load');
        }
    });    

});
</script>
