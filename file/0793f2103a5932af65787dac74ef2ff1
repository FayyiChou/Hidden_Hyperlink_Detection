<script>
function openlist(me)
{
  var index=$("li[name='list']").index(me);
  //var aa=$(me).parent().children(ul).css(display,'block');
  var display=$(me).children("ul").css('display');
  if(display=="none")
  {
    $(me).children("ul").css('display','block');
  }
  else
  {
    $(me).children("ul").css('display','none');
  }
  //alert('error',index);
      
}
$("#create-course").on('click', function(event) {
  var btn = $(this);
  var coursetype = $('input:radio:checked[name="coursetype"]').val();
  var coursename = $('input[name="coursename"]').val();
  if(coursetype.replace(/^\s+|\s+$/g, '').length ==0){
    $("#course-error").text('請選擇一種課程類型。');
  }else if(coursename.replace(/^\s+|\s+$/g, '').length ==0){
    $("#course-error").text('課程名稱不可為空白。');
  } 
  else{
    btn.button('loading');
    $.ajax({
      url:"ajax.php", 
      type:"POST", 
      data:{status:"createCourse",coursetype:coursetype,coursename:coursename}, 
      dataType:"text",
      success:function(data) 
      {  
        if(data) location.href=data; 
        else btn.button('reset'); 
      },error:function(){
        btn.button('reset');
      }
    
    });

  }
});
</script>




