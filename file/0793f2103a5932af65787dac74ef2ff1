  <script>
	jQuery(document).ready(function($) {
	// 幫 div.qa_title 加上 hover 及 click 事件
	// 同時把兄弟元素 div.qa_content 隱藏起來
	$('#qaContent ul.accordionPart li div.qa_title').hover(function(){
		$(this).addClass('qa_title_on');
	}, function(){
		$(this).removeClass('qa_title_on');
	}).click(function(){
		// 當點到標題時，若答案是隱藏時則顯示它；反之則隱藏
		$(this).next('div.qa_content').slideToggle();
	})

	
	if (localStorage.getItem("imei") != null) {
    
	    $.ajax({
	          type:"POST",
	          url:"ajax.php",
	          data:{status:'mobile_login_reset',imei:localStorage.getItem('imei'),num:localStorage.getItem('num')},
	          async:true,
	          success(data){
	            if(data=="reset") location.reload();
	            else if(data=="relogin") location.href="login.html";
              
	          },error(){
	            myAlert("伺服器沒有回應，請稍後再試。");
              
	          }
	        });
	}
  
	

	});
	  function getParameter(name)
	  {
				name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
				var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
	   		results = regex.exec(location.search);
			
				return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
	  } 
    function replace(str) { 
        if(str!=null) return str.replace(new RegExp('\r', 'g'), '<br>').replace(new RegExp('\n', 'g'), '<br>');
        else return null;
    }
    var _validFileExtensions = [".jpg", ".jpeg", ".bmp", ".png"];    
    function ValidateSingleInput(oInput) {
        if (oInput.type == "file") {
            var sFileName = oInput.value;
             if (sFileName.length > 0) {
                var blnValid = false;
                for (var j = 0; j < _validFileExtensions.length; j++) {
                    var sCurExtension = _validFileExtensions[j];
                    if (sFileName.substr(sFileName.length - sCurExtension.length, sCurExtension.length).toLowerCase() == sCurExtension.toLowerCase()) {
                        blnValid = true;
                        break;
                    }
                }
                 
                if (!blnValid) {
                    alert("error","檔案必須使用: " + _validFileExtensions.join(", "));
                    oInput.value = "";
                    return false;
                }
            }
        }
        return true;
    }

  </script> 

   
  
