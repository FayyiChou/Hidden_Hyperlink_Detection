	<script type="text/javascript">
		$("#messageadd_btn").click(function(){
			 if($("#message_content").val()==0)
			 {
				alert('请输入留言内容');
				return false;
			 }
			 
			 mess_add_fun($("#message_tid").val(),$("#message_content").val());
			 
		});
		$('.column .enter-content').height($('.column .enter-content').height()<380?380:$('.column .enter-content').height());
		$("#messageinfoadd_btn").click(function(){
			 if($("#mess_content").val()==0)
			 {
				alert('请输入留言内容');
				return false;
			 }
			 
			 mess_add_fun($("#mess_tid").val(),$("#mess_content").val());
			 
		});
		function mess_add_fun(messid,content)
		{
			 switch($("#domain_flag").val())
			{
				case "domain_ok":
					var url = "/message/message_add.htm";
					break;
				case "domain_no":
					var url = "/manufacturer/message//message_add.htm"; 
					break;
			}
			var pid = +0;
			 $.ajax({
				   type: "POST",
				   url: url,
				   data: "mess_tid="+messid+"&message_content="+content+'&pid='+pid,
				   success: function(msg)
				   {
						 location.href=document.URL;
						 
				   }
			});
		}
		$(function(){
			$('#keyword').keypress(function(event){
				if(event.keyCode==13)
					productSearch(this, 'domain_ok');
			});
		});
		function productSearch(obj,domain_flag){
			var keyword = $(obj).parent().find('#keyword').val();
			var k_length = keyword.length;
			var trim_keyword = $.trim(keyword);
			var trim_k_length = trim_keyword.length;
			var noword = "请输入搜索关键字";
			//var noeng = "Sorry,the information you filled is not in English.Please input the information in English instead.";
			var shortnotice = "Too short search!";
			var longsearch = "请输入少于50个字符的搜索关键字。";
			var pattern = /^\s+$/;
			if(k_length == 0 || pattern.test(keyword)){//check space
				alert(noword);
				return;
			}else if(trim_k_length < 1){
				alert(shortnotice);
				return;
			}else if(trim_k_length > 50){
				alert(longsearch);
				return;
			}else{
				if(keyword == "" || keyword == "请输入查找关键字") return;
				switch(domain_flag)
				{
					case "domain_ok":
						var url = "/offersearch.htm?keyword=" + trim_keyword;
						break;
					case "domain_no":
						var url = "/manufacturer/offersearch/.htm?keyword=" + trim_keyword; 
						break;
				}
				window.location.href = url;
			}
		}
		function singleInquiryNow(act,pid,type)
		{
			var url = "http://www.baimao.com/contact/message/"+type+"/"+pid+".htm";
			$("#perPageItemsForm")[0].action = url;
			$("#perPageItemsForm")[0].target = "_blank";
			$("#perPageItemsForm")[0].submit();
		}

		function inquiryNow(type)
		{
			var ischeck = false;
			var pid = "";
			for(var i=0;i<$(":checkbox").length;i++)
			{
				if($(":checkbox")[i].checked && $(":checkbox")[i].value != "on")
				{
					ischeck = true;
					pid += $(":checkbox")[i].value+"_";
				}
			}
			pid = pid.substring(0,pid.length-1);

			if(!ischeck)
			{
				alert("请选择产品");
			}else {
				var url = "http://www.baimao.com/contact/message/"+type+"/"+pid+".htm";
				$("#perPageItemsForm")[0].action = url;
				$("#perPageItemsForm")[0].target = "_blank";
				$("#perPageItemsForm")[0].submit();
			}
		}
		var _bdhmProtocol = (("https:" == document.location.protocol) ? " https://" : " http://");
		document.write(unescape("%3Cscript src='" + _bdhmProtocol + "hm.baidu.com/h.js%3F66f69ae8350befb183840e5b46a03f61' type='text/javascript'%3E%3C/script%3E"));
	</script>
</span>
