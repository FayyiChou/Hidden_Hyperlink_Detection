		<script type="text/javascript">
$(function(){
	$('#listKeyword').keypress(function(event){
		if(event.keyCode==13)
			productSearch(this, 'domain_ok');
	});
});	
function productSearch(obj,domain_flag){
	var keyword = $('#keyword').val();
	var k_length = keyword.length;
	var trim_keyword = $.trim(keyword);
	var trim_k_length = trim_keyword.length;
	var noword = "请输入搜索关键字";
	var shortnotice = "Too short search!";
	var longsearch = "请输入少于50个字符的搜索关键字。";
	var pattern = /^\s+$/;
	if(k_length == 0 || pattern.test(keyword)){//check space
		alert(noword);
		return;
	}else if(trim_k_length < 1){
		alert(shortnotice);
		return;
	}else if(trim_k_length > 50){
		alert(longsearch);
		return;
	}else{
		if(keyword == "" || keyword == "请输入查找关键字") return;
		switch(domain_flag)
		{
			case "domain_ok":
				var url = "/offersearch.htm?keyword=" + trim_keyword;
				break;
			case "domain_no":
				var url = "/manufacturer/offersearch/.htm?keyword=" + trim_keyword; 
				break;
		}
		window.location.href = url;
	}
}
function singleInquiryNow(act,pid,type)
{
	var url = "http://www.baimao.com/contact/message/"+type+"/"+pid+".htm";
	$("#perPageItemsForm")[0].action = url;
	$("#perPageItemsForm")[0].target = "_blank";
	$("#perPageItemsForm")[0].submit();
}

function inquiryNow(type)
{
	var ischeck = false;
	var pid = "";
	for(var i=0;i<$(":checkbox").length;i++)
	{
		if($(":checkbox")[i].checked && $(":checkbox")[i].value != "on")
		{
			ischeck = true;
			pid += $(":checkbox")[i].value+"_";
		}
	}
	pid = pid.substring(0,pid.length-1);

	if(!ischeck)
	{
		alert("请选择产品");
	}else {
		var url = "http://www.baimao.com/contact/message/"+type+"/"+pid+".htm";
		$("#perPageItemsForm")[0].action = url;
		$("#perPageItemsForm")[0].target = "_blank";
		$("#perPageItemsForm")[0].submit();
	}
}
</script>
