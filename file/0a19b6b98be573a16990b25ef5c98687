<script type="text/javascript">
	function closeDiv(innerCall){
		if(document.getElementById('PcPoPmarket')==null)return;
		document.getElementById('PcPoPmarket').style.display = 'none';
	}
	$(function(){
		$(".ulSmallPic img").click(function(){
			var img_src =  $(this).attr('src');
			var title = $(this).attr('alt');
			var price = $(this).attr('price');
			$(this).parents("li").addClass("liSelected").siblings().removeClass("liSelected");
			$("#vertical img").attr("src",img_src);
			$(".textTitle").text(title).siblings('p').children('em').text(price);
			return false;
		});
		var ulSmallPic = $(".ulSmallPic img");
		var timer=null; //定时器返回值，主要用于关闭定时器
		var iNow=0;  //iNow为正在展示的图片索引值，当用户打开网页时首先显示第一张图，即索引值为0
		timer=setInterval(function(){  //打开定时器
			iNow++;       //让图片的索引值次序加1，这样就可以实现顺序轮播图片
			if(iNow>ulSmallPic.length-1){ //当到达最后一张图的时候，让iNow赋值为第一张图的索引值，轮播效果跳转到第一张图重新开始
				iNow=0;
			}
		  ulSmallPic.eq(iNow).trigger("click"); //模拟触发数字按钮的click事件
		 },2000); //2000为轮播的时间

		$(".pop-list ul li").click(function(){
			 $(this).addClass("selected").siblings().removeClass("selected");
		});
	
	});
	$(function(){
		$li1 = $(".dSmallPicBox li");
		$window1 = $(".dSmallPicBox .ulSmallPic");
		$left1 = $(".dSmallPicBox .sLeftBtn");
		$right1 = $(".dSmallPicBox .sRightBtn");
		
		$window1.css("width", $li1.length*68);

		var lc1 = 0;
		var rc1 = $li1.length-5;
		
		$left1.click(function(){
			if (lc1 < 1) {
				return;
			}
			lc1--;
			rc1++;
			$window1.animate({left:'+=68px'}, 1000);
		});

		$right1.click(function(){
			if (rc1 < 1){
				return;
			}
			lc1++;
			rc1--;
			$window1.animate({left:'-=68px'}, 1000);
		});
	})
</script>
