		$(function() {
			jQuery.jqtab = function(tabtit,tab_conbox,shijian) {
				$(tab_conbox).find("li").not('.tab_conbox2 li').hide();
				$(tabtit).find("li:first").addClass("sec1").show(); 
				$(tab_conbox).find("li:first").show();
			
				$(tabtit).find("li").bind(shijian,function(){
				  //$(this).addClass("sec1").removeClass("sec2").siblings("li").addClass("sec2").removeClass("sec1"); 
					var activeindex = $(tabtit).find("li").index(this);
					$(tab_conbox).children().eq(activeindex).show().siblings().hide();
					return false;
				});
			};
			jQuery.jqtab2 = function(conbox_1,tab_conbox3,shijian2) {
						$(tab_conbox3).find("li").hide();
					
						$(conbox_1).find("li").bind(shijian2,function(){
							var activeindex2 = $(conbox_1).find("li").index(this);		
							// alert(activeindex2);
							$(".tab_conbox3").children().eq(activeindex2).show().siblings().hide();
				});								   
			};
			/*调用方法如下：*/
			$.jqtab2("#conbox_1",".tab_conbox3","click");
			/*调用方法如下：*/
			$.jqtab("#tabs1","#tab_conbox","click");
		});
