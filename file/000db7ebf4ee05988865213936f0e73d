	//var education = eval('(' + education + ')');
	
	var degree = "";
	for(var i in education){
		var item = education[i];
		var child = JSON.stringify(item["childs"]);
		degree += '<li><a href="javascript:void 0;" did = "'+item["dic_key"]+'" childs=\''+child+'\'>'+item["dic_value"]+'</a></li>';
	}
	$("#degree").html(degree);
	//年级联动，学历赋值
	$("#degree").find("a").each(function(){
		$(this).click(function(){
			//学历赋值
			$("#degree").prev().find("input").attr("myval",$(this).attr("did"));
			var childs = eval($(this).attr("childs")) ;
			var grade = "";
			for(var j in childs){
				grade += '<li><a href="javascript:setGrade('+childs[j]["dic_key"]+');" gid="'+childs[j]["dic_key"]+'">'+childs[j]["dic_value"]+'</a></li>';
			}
			$("#grade").html(grade);
			$("#grade").prev().find("input").val("").attr("placeholder","年级");
			$("#xueli").val($(this).attr("did"));
		});
	});
	
	
	//默认英国选中
	if(user=="" || user['custom_hope_country']==0){
		setService(471);
	}
	//国家和服务联动
	$("a[cval]").each(function(i,n){
		$(this).click(function(){
			var cval = $(this).attr("cval");
			setService(cval);
		});
	});
	//设置已有信息用户显示相关信息，且不能修改
	if(user!="" && user['custom_hope_country']!=0){
		//意向国家
		$("a[cval]").each(function(i,n){
			if($(this).attr("cval") == user['custom_hope_country']){
				$(this).addClass("active");
				/* if(user['custom_hope_country'] == 471){
					$(".next").click();
				} */
			}
		});
		
		//服务
		setService(user['custom_hope_country']);
		$("a[sval]").each(function(i,n){
			if($(this).attr("sval") == user['country_business']){
				$(this).click();
			}
		});
		
		//时间
		$("a[yid]").each(function(i,n){
			if($(this).attr("yid") == user['custom_year']){
				$(this).click();
			}
		});
		//学历
		$("a[did]").each(function(){
			if($(this).attr("did") == user['custom_education']){
				$(this).click();
			}
		});
		//年级
		$("a[gid]").each(function(){
			if($(this).attr("gid") == user['custom_detail']){
				$(this).click();
			}
		});
		
		//昵称
		$("#nickname").val(user['custom_nickname']);
		//提交按钮样式改变
		$(".navbar-sigin1:eq(0)").html("已申请").css("background-color","#fff").css("color","#01a4e5").css("border","solid 2px #01a4e5").attr("onclick","");
		$(".pull-right").css("line-height","42px");//line-height: 42px;
		$(".navbar-sigin1:eq(1)").parent().html("芥末老师会尽快联系您，请保持联系方式畅通。");
		
		//设置不可修改
		//$(".zheZhao").css("display","block");
	}
	
	
	//取消自动完成
	$("input[type=text]").attr("autocomplete","off");
	//点击取消错误提示
	$(".mulForms").find("input[type=text],input[type=password],ul,a").each(function(){
		$(this).click(function(){$(this).parents("li").removeClass("mulErr")});
	});
	//点击取消错误提示
	$(".indexCountry").find("a").each(function(){
		$(this).click(function(){$(this).parents("li").removeClass("mulErr")});
	});
	//年级判断
	$(".mulClass.ms").on('click', function(e){
		var mulEducation = $('.mulEducation').find('input').val();
		if (mulEducation == '') {
			e.stopPropagation();
			_err("mulEducation","请选择学历");
		}
	});
	//院校库搜索
	$(".colleges-sear-btn").click(function(e){
		var value = $(".colleges-sear-input").val();
		var url = "http://www.jiemo.net/school/uk?sel_n=";
		if(value==""){
			return;
		}else{
			window.location = url+ value;
		}
	});
	
	//广告点击次数
	$('[data-friend]').friendLink('data-friend', function(){
		var coun = $(this).attr('data-count');
		if (coun) {
			var cc =$(this).attr("data-count");
			advsLog(cc);
		}
		
    });
	
});

function advsLog(id){
	//执行的地址
    var sendurl= '/index/advslog.html';
	jQuery.ajax({
	       url:sendurl,
	       data:{id:id},
	       type:'POST',
	       dataType:'html',
	       success:function(data){
	    	   
	       }
	   });
}

