//年份赋值
function setYear(val){
	$("#shijian").val(val);
}

//年级赋值
function setGrade(val){
	$("#nianji").val(val);
}

//统一错误格式处理
function _err(cs,text){
	var str = text+"<i></i><strong></strong><em></em>";
	if(cs == "indexCountry"){
		 str = text+"<strong></strong><em></em>";
	}
	if(cs == "mulPwd"){
		$(".mulPwdShow").hide();
	}
	// $("."+cs).find("span[class=mulInpError]").first().html(str).fadeIn(2000).fadeOut(1000);
	$("."+cs).find("span[class=mulInpError]").first().html(str);
	
	$("."+cs).addClass("mulErr aniMulErr");
	setTimeout("rmClass('"+cs+"')",300);// 延迟300毫秒去掉动画效果
	
}
//去掉class效果
function rmClass(cs){
	$("."+cs).removeClass("aniMulErr");
}
//提交
function checkInfo(obj){
	//国家检测，是否选择
	 if(!$("a[cval]").hasClass("active")){
		 _err("indexCountry","请选择一个国家");
	 	 return ;
	} 
	var country = $("a[cval][class$=active]").first().attr("cid");
	//年份
	var year = $("#shijian").val();
	if(year == ""){
		_err("mulPlan","请选择一个年份");
	 	 return ;
	}
	//学历
	var degree = $("#xueli").val();
	if(degree == ""){
		_err("mulEducation","请选择学历");
	 	 return ;
	}
	//年级
	var grade = $("#nianji").val();
	if(grade == ""){
		_err("mulClass","请选择年级");
	 	 return ;
	}
	//昵称
	var nickname = $("#nickname").val();
	if($.trim(nickname) == ''){
		 _err("mulAddress","请输入你的昵称");
		 return ;
	}
	var userid = "";
	if(userid =="0" ||userid ==""){
		//alert($("#nianji").val());
		//return;
		var userLay=$('.moduleUserLayFixed');
	    var thisClass = $(obj).hasClass('navbar-sigin1');
	    var btnTxt;
	    if (thisClass) {
	        btnTxt = '注册';
	        userLay.removeClass('mulSigin mulRegOth');
	    } else {
	        btnTxt = '登录';
	        userLay.addClass('mulSigin').removeClass('mulRegOth');
	    }
	    var thisRel = $(obj).attr('data-title') ? $(obj).attr('data-title') : btnTxt;
	    $('#mulTitle').html(thisRel);
	    $('#mulBtnSub').html(btnTxt);
	    $('.commonCountry,.mulOth').addClass("hide");
	    //国家传值
	    var country = $("a[cval][class$=active]").first().attr("cval");
	    $(".mulCountry").find("a[cid]").each(function(i,n){
	    	if($(this).attr("cid") == country){
	    		$(this).addClass("active");
	    	}else{
	    		$(this).removeClass("active");
	    	}
	    });
	    userLay.overlay('show').drag('.mulHead');
	}else{
		var tel,password,country,fuwu,shijian,nianji,xueli,nicheng,pwd_type;
		tel = user["tel"];
		password = user["password"];
		country = $("a[cval][class$=active]").first().attr("cval");
		fuwu = $("#fuwu").val();
		shijian = $("#shijian").val();
		nianji = $("#nianji").val();
		xueli = $("#xueli").val();
		nicheng = $("#nickname").val();
		pwd_type = 200;
		//alert(tel+","+password+","+country+","+fuwu+","+shijian+","+nianji+","+xueli+","+nicheng+","+pwd_type);
		//执行的地址
	    var sendurl= '/member/edit.html'; 
	    //当前页面地址
	    var url = document.URL;
	    jQuery.ajax({
	       url:sendurl,
	       data:{tel:tel,password:password,shijian:shijian,nianji:nianji,xueli:xueli,country:country,
	    	   lailu:url,nicheng:nicheng,fuwu:fuwu,pwd_type:pwd_type},
	       type:'POST',
	       dataType:'html',
	       success:function(data){
	    	   window.location=window.location;
	       }
	   });
	}
	
}
</script> 
