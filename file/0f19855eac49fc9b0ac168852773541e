function CDiv(para)

{

var Div = window.document.getElementById("list"+para);

for (i=0; i<11; i++){

	if (i!=para){

		var tem =window.document.getElementById("list"+i);

		var temm =window.document.getElementById("menu"+i);

		tem.style.display = "none";

	}

}

if (Div.style.display!="")

Div.style.display = Div.style.display == "none" ? "" : "none";



}

