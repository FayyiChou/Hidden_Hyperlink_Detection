        $(this).css('min-height',bodyH)
    });
    setTimeout(function(){
        mh = $('.titleBar').offset().top;
        //alert(mh);
        $(window).on('scroll', function() {
            var scrollTop = $(window).scrollTop();
                if(scrollTop>=mh){
                    $('.titleBar').css({
                        position:'fixed',
                        //top:44
                    });
                    $('.titleBarW').show();
                    $('.goTop2').show();
                }else{
                    $('.titleBar').css({
                        position:'relative',
                        top:0
                    });
                    $('.titleBarW').hide();
                    $('.goTop2').hide();
                }
        });
        },200)
    
