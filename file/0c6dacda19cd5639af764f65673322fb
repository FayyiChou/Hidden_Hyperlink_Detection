<script>
//口碑图片宽高获取
$(function() {
	$(".img_miao").each(function() {
		var img_url = $(this).attr("src");
		var img = new Image();
		img.src = img_url;
		if (img.width / img.height > 1) {
			$(this).css({
				"width": "auto",
				"height": "100%"
			});
		}
	})
	// 提示信息展开收缩
	$('.mjSection .openbtn').click(function(){
		var obj = $(this).parents().next('.shrink');
		if($(this).hasClass('openbtnCur')) {
			$(this).removeClass('openbtnCur');
			$('.ts-info').css('border-top','0');
			obj.hide()
		} else {
			$(this).addClass('openbtnCur');
			$('.ts-info').css('border-top','1px solid #ddd');
			obj.show()
		}
		
	});
	$('.ts-info .openbtn').click(function(){
		var obj = $(this).parent().next('.shrink');
		if($(this).hasClass('openbtnCur')) {
			$(this).removeClass('openbtnCur')
			obj.hide()
		} else {
			$(this).addClass('openbtnCur')
			obj.show()
		}
	});
	$('.mz .openbtn').each(function() {
		$(this).click(function(){
			var obj = $(this).parent().next('.swiper-container');
			if($(this).hasClass('openbtnCur')) {
				$(this).removeClass('openbtnCur');
				$('.ts-info').css('border-top','0')
				obj.hide()
			} else {
				$(this).addClass('openbtnCur');
				$('.ts-info').css('border-top','1px solid #ddd');
				obj.show()
			}
		});
	});
	$('.quan-list .openbtn').click(function() {
		if($(this).hasClass('openbtnCur')) {
			$(this).removeClass('openbtnCur')
			$(this).siblings('.quan-item').css('height','0.8rem');
		} else {
			$(this).addClass('openbtnCur')
			$(this).siblings('.quan-item').css('height','auto');
		}
	});
})

</script>
