<script type="text/javascript">
    function xiaoNeng(){
        nTalk.im_openInPageChat();
    }

    var xiaoneng_login_status = 0;
    $('#service_mia').bind('click', function() {
        if (xiaoneng_login_status) {
            xiaoNeng();
            return;
        } else {
            var islogin= getcookie("usernames");

            if (islogin!='') {
                xiaoneng_login_status = 1;
                xiaoNeng();
            } else {
                alert('请登录后再来点我哦');
                window.location.href = '/login?url=' + encodeURIComponent(window.location.href);
            }
        }

    });
</script>
