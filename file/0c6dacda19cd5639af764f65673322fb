    function _checkCentBuy(centObj){
		var _data = {
            groupon_id : centObj.data('gid'),
        };

		$.ajax({
            url:'/instant/cart/check_cent_buy',
            cache:false,
            dataType: 'json',
            data: {'groupon_id': centObj.data('gid')},
            type: "POST",
            beforeSend:function(){
                //$('#addToCart').css({visibility: 'hidden'}).before('<img src="/resources/images/ajax-loader.gif" class="loadimg" style="float:left;left:100px;position:relative;top:15px;">');
            },
            success:function(data){
                //没登陆
                if(data.code==2){
                    //for login
                    alert('请登陆');
                    var url = encodeURIComponent('/item-'+G_ITEM_ID+'.html');

                    window.location.href = '/login?url='+url; return;
                }else if(data.code == 3){
                    alert('您已经参加过啦，看看别的吧！');
                }
                else if(data.code == 1){
                	_checkGrouponBuy(centObj);
                }
                else if(data.code == 4){
                	alert('稍后再试！');
                }
            }
        });
    }

    function _checkGrouponBuy(grouponObj){
        //check out product size
        var sizeText =$("#item_size").val();
        var flag = true;
        if($('#is_spu').length > 0)sizeText = 'SINGLE';//for spu
        if($('span.guige').text() == '')sizeText = 'SINGLE';

        if (sizeText == '' || sizeText == '未选择') {
            flag = false;
            alert('请选择规格');
            //return;
        }

        if(!flag)return false;
        var _data = {
            groupon_id : grouponObj.data('gid'),
            item_id : G_ITEM_ID,
            item_size : sizeText
        };
        $.ajax({
            url:'/instant/cart/check_groupon_buy',
            cache:false,
            dataType: 'json',
            data: {'params' : _data},
            type: "POST",
            beforeSend:function(){
                //$('#addToCart').css({visibility: 'hidden'}).before('<img src="/resources/images/ajax-loader.gif" class="loadimg" style="float:left;left:100px;position:relative;top:15px;">');
            },
            success:function(data){
                //没登陆
                if(data.needLogin){
                    //for login
                    alert('请登陆');
                    var url = encodeURIComponent('/item-'+G_ITEM_ID+'.html');

                    window.location.href = '/login?url='+url; return;
                }else if(data.code != 200){
                    if(data.alert){
                        alert(data.alert);
                    }else{
                        alert('查询失败,请稍后重试');
                    }
                }
                else if(data.code == 200){
                    window.location.href = 'https://m.mia.com/order/groupon?groupon_id='+grouponObj.data('gid')+'&groupon_son_id=0&item_id='+G_ITEM_ID+'&item_size='+encodeURIComponent(sizeText);
                }
             
            }
        });


        

    }


