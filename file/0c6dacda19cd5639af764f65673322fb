            origin_scan_sku_arr.pop();
            origin_scan_sku_str = origin_scan_sku_arr.join(',');
            new_scan_sku =  item_id+','+origin_scan_sku_str;
        }
    }
    setcookie('scan_sku', new_scan_sku , date.toGMTString());
} else {
    setcookie('scan_sku', item_id, date.toGMTString());
