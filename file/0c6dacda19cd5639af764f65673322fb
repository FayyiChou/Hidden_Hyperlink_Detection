<script>
    $(' .smewm').click(function(event) {
        var url = 'miyabaobei://main';
        if ($('#appUrl').length>0) {
            var appUrl = $('#appUrl');
            url = 'miyabaobei://' + appUrl.data('type') + '?id=' + appUrl.val();
        };
        toAppPage(url);
    });

    function identifyUA() {
        var userAgent = navigator.userAgent.toLowerCase();
        var platform ='';
        if(userAgent == null || userAgent == ''){
            platform = 'web' ;
        }else{
            if(userAgent.indexOf("android") != -1 ){
                platform = 'android';
            }else if(userAgent.indexOf("ios") != -1 || userAgent.indexOf("iphone") != -1){
                platform = 'ios';
            }else if(userAgent.indexOf("ipad") != -1) {
                platform = 'ios';
            }else if(userAgent.indexOf("windows phone") != -1 ){
                platform = 'windowsphone';
            }else{
                platform = 'web';
            }
        }
        return platform;
    }

    function toAppPage(url){
        var platform = identifyUA();
        switch(platform){
            case "android":
                //$('#download').attr('href', 'https://www.miyabaobei.com/resources/miababy-m-latest.apk');
                toAndroid(url);
                break;
            case "ios":
                //$('#download').attr('href', 'https://itunes.apple.com/cn/app/mi-ya-bao-bei-zhong-guo-zui/id960993760?mt=8');
                openIos(url, function(t){});
                break;
            default:
                window.location.href = 'https://www.miyabaobei.com/app.html'
        }
    }

    function openIos(url, callback) {
        if (!url) {
            return;
        }
        var node = document.createElement('iframe');
        node.style.display = 'none';
        var body = document.body;
        var timer;
        var clear = function(evt, isTimeout) {
            (typeof callback==='function') &&  callback(isTimeout);
            window.removeEventListener('pagehide', hide, true);
            window.removeEventListener('pageshow', hide, true);
            if (!node) {
                return;
            }
            node.onload = null;
            body.removeChild(node);
            node = null;
        };
        var hide = function(e){
            clearTimeout(timer);
            clear(e, false);
        };
        window.addEventListener('pagehide', hide, true);
        window.addEventListener('pageshow', hide, true);
        node.onload = clear;
        node.src = url;

        body.appendChild(node);
        var now = +new Date();
        //如果事件失败，则1秒设置为空
        timer = setTimeout(function(){
            timer = setTimeout(function(){
                var newTime = +new Date();
                if(now-newTime>1300){
                    clear(null, false);
                }else{
                    clear(null, true);
                }
            }, 1200);
        }, 60);
    }

    function toAndroid (url) {
        // 通过iframe的方式试图打开APP，如果能正常打开，会直接切换到APP，并自动阻止a标签的默认行为
        // 否则打开a标签的href链接
        var ifrSrc = url;
        if(!ifrSrc){
            return;
        }
        var ifr = document.createElement('iframe');
        ifr.src = ifrSrc ;
        ifr.style.display = 'none';
        document.body.appendChild(ifr);
        setTimeout(function(){
            document.body.removeChild(ifr);
        },1000);
    }

</script>
