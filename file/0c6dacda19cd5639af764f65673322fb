    function GetRequest() {
        var url = location.search; //获取url中"?"符后的字串
        var theRequest = new Object();
        if (url.indexOf("?") != -1) {
            var str = url.substr(1);
            strs = str.split("&");
            for(var i = 0; i < strs.length; i ++) {
                theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);
            }
        }
        return theRequest;
    }
    var parameter = GetRequest();
    if(parameter.is_dx == 'remind'){
        if(platform == 'ios' || platform == 'android' ){
            if ($('#appUrl').length>0) {
                var appUrl = $('#appUrl');
                url = 'miyabaobei://' + appUrl.data('type') + '?id=' + appUrl.val();
                toAppPage(url);
            }
        }
    }
function TimeDown(id, endDateStr) {
    if (!document.getElementById(id)) {
        return;
    };
    //结束时间
    var endDate = new Date(endDateStr);
    //当前时间
    var nowDate = new Date();
    //相差的总秒数
    var totalSeconds = parseInt((endDate - nowDate) / 1000);
    //天数
    var days = Math.floor(totalSeconds / (60 * 60 * 24));
    //取模（余数）
    var modulo = totalSeconds % (60 * 60 * 24);
    //小时数
    var hours = Math.floor(modulo / (60 * 60));
    modulo = modulo % (60 * 60);
    //分钟
    var minutes = Math.floor(modulo / 60);
    //秒
    var seconds = modulo % 60;
    //输出到页面
    if(parseInt(days)<10){
        days="0"+days;
    };
    if(parseInt(hours)<10){
        hours="0"+hours;
    };
    if(parseInt(minutes)<10){
        minutes="0"+minutes;
    };
    if(parseInt(seconds)<10){
        seconds="0"+seconds;
    };
    //console.log(id);
    //console.log(endDateStr);
    document.getElementById(id).innerHTML = days + "天" + hours + "时" + minutes + "分" + seconds + "秒";
    //延迟一秒执行自己
    setTimeout(function () {
        TimeDown(id, endDateStr);
    }, 1000)
}
$(function(){
    var $line = $('.titleBar i');
    var len = $('.titleBar div').length;
    $line.css("width",toPer(1/len));
    var $sildeBox = $('.slideBox');
    var $showblock = $sildeBox.find(".showblock");

    $('.titleBar div').on('click', function(event) {
        var mh = $('.slideBox').offset().top-40;
        $('html,body').scrollTop(mh);
        var index = $(this).index();
        $(this).addClass('current').siblings().removeClass('current');
        $line.css("left",toPer(index/len));
        var per = -(index * 100) + '%';
        $sildeBox.css({
            '-webkit-transform' : 'translate3d('+ per +', 0px, 0px)',
            '-moz-transform' : 'translate3d('+ per +', 0px, 0px)',
            'transform' : 'translate3d('+ per +', 0px, 0px)',
            '-webkit-transition' : 'all 0.35s ease',
            '-moz-transition' : 'all 0.35s ease',
            'transition' : 'all 0.35s ease'
        })
        $showblock.eq(index).css("height","100%");
        $showblock.eq(index).siblings().css("height",0);
    });
    
    function toPer(num){
        return num.toFixed(4) * 100 + '%';
    }
    //$('.titleBar').
//头部副导航固定
    var bodyH = $(window).height()-132;
