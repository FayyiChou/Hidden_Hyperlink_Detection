				var MarqueePicControl=new Marquee("MarqueeDiv");		//箭头控制滚动方向、加速及鼠标拖动实例
				MarqueePicControl.Direction="left";
				MarqueePicControl.Step=1;
				MarqueePicControl.Width=955;
				MarqueePicControl.Height=47;
				MarqueePicControl.Timer=40;
				MarqueePicControl.ScrollStep=1;				//若为-1则禁止鼠标控制实例
				MarqueePicControl.Start();
				MarqueePicControl.BakStep=MarqueePicControl.Step;
				document.getElementById("LeftButton").onmouseover=function(){MarqueePicControl.Direction=2};
				document.getElementById("LeftButton").onmouseout=document.getElementById("LeftButton").onmouseup=function(){MarqueePicControl.Step=MarqueePicControl.BakStep};
				document.getElementById("LeftButton").onmousedown=document.getElementById("RightButton").onmousedown=function(){MarqueePicControl.Step=MarqueePicControl.BakStep+3};
				document.getElementById("RightButton").onmouseover=function(){MarqueePicControl.Direction=3};
				document.getElementById("RightButton").onmouseout=document.getElementById("RightButton").onmouseup=function(){MarqueePicControl.Step=MarqueePicControl.BakStep};
				</script>
        <script>mnhrsite.footer();</script>
    </div>
</div>
