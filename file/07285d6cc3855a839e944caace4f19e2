<script type="text/javascript">
//these variables are used in survey
var SP_DOC_ID = '';
var SP_DOC_TITLE = '';
var SP_DOC_LANG= '';
 
String.prototype.escape = function() {
    var tagsToReplace = {
        '&': '&amp;',
        '<': '&lt;',
        '>': '&gt;'
    };
    return this.replace(/[&<>]/g, function(tag) {
        return tagsToReplace[tag] || tag;
    });
};

document.title = ''.escape();
   
function printDocument() {
	var startHtml = "";
   
	startHtml +='<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html><meta http-equiv="X-UA-Compatible" content="IE=edge">'+
		'<link href="/resource3/hpsc/doc/css/document.css" rel="stylesheet" />'+
		'<style>'+
		'@media print{body{font-family:Arial;font-size:12px}.smalltext{font-size:9px;}.noprint{display:none;}}'+
		'body{font-family:Arial;font-size:12px}.smalltext{font-size:9px;}'+
		'table.hpui-standard-table tr td{font-size:12px}'+
		'</style>';
	
	var hpPrintLogo = '<div style="text-align:left;width:900px;padding-bottom:20px;">'+
		'<img class="hf_printable" src="" alt="HP 徽标" title="HP 徽标" style="vertical-align:middle;cursor:pointer">'+
		'<h2 class="hpui-emphasized-text" style="display:inline-block;vertical-align:middle;padding-left: 14px;font-size:24px;">&nbsp;惠普支持中心</h2>'+
		'</div>';
		
	var printButton='<FORM><center><INPUT TYPE="BUTTON" VALUE="打印" TITLE="打印" ONCLICK="window.print()" CLASs="noprint" '+
		'style="cursor:pointer"/></center></FORM><br />';
	var endHtml='</html>';
				
	var prtContent = document.getElementById("printContent");
	var disp_setting="toolbar=yes,location=no,directories=no,menubar=yes,scrollbars=yes, width=1000, height=765";
	var WinPrint = window.open('', '', disp_setting);
	WinPrint.document.open();
			
	WinPrint.document.write(startHtml);
	WinPrint.document.write(printButton);
	WinPrint.document.write(hpPrintLogo);
	WinPrint.document.write(prtContent.innerHTML);
	WinPrint.document.write(endHtml);
		
	WinPrint.document.close();
	WinPrint.focus();
}
</script>
