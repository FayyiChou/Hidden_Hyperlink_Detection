				function Change15s(no) {
					if (!no){
						no = 0;
					}
					var hot_total = $('div[name="indexsrh"]').length;
					for (var i = 0; i <= hot_total; i++) {
						a = $("div[name='indexsrh']").eq(i);
						if (i == no) {
							$(a).fadeIn(4000);
						} else {
							$(a).hide();
						}
					}
					no++;
					setTimeout('Change15s(' + (no == hot_total ? 0 : no) + ');', 8000);
				}
				$(document).ready(function() {
					Change15s();
				});
