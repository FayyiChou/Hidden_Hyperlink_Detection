    //function $(id) { return document.getElementById(id); }
    function tijiao(url) {

        if (navigator.userAgent.indexOf("MSIE") > 0) {
            var referLink = document.createElement('a');
            referLink.href = url;
            document.body.appendChild(referLink);
            referLink.click();
        } else {
            window.location.replace(url);
        }
    }

	function xianshi(id,uid){
		if(id=="hide"){
			$("#"+uid).css("display","none");
		}else{
			$("#"+uid).css("display","block");
		}
	}
	
	
	
	//周边楼盘和热门楼盘 鼠标放上显示楼盘详细信息
	$(function(){
		$("#zblp_brief_0").hide();
		$("#zblp_detail_0").show();
		$("#lprp_brief_0").hide();
		$("#lprp_detail_0").show();
	})
    
	function Toshow(type,point){
		var count = $("a[id^="+type+"_brief_]").size();
		var i = 0;
		$('#'+type+'_detail_'+point).show();	
		$('#'+type+'_brief_'+point).hide();
		for(i;i<count;i++){
			if(i != point){
				$('#'+type+'_detail_'+i).hide();	
				$('#'+type+'_brief_'+i).show();
			}
		}
	}
</script>



