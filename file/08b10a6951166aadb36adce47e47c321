<script type="text/javascript">
	parm_ori = "";
	var parm="";
	
	switch (parm_ori)
	{
		case 'pd':
			 parm = '价格由高到低';
			 break;
		case 'pa':
             parm = '价格由低到高';
		     break;
	　　 case 'saledate':
			 parm = '开盘时间由近到远';
		     break;
	　　 case 'saledateasc': 
		     parm = '开盘时间由远到近';
		     break;
         case 'dianpingdes':
			 parm = '点评数由高到低';
		     break;
         case 'dpd':
			 parm = '综合评分由高到低';
		     break;
		default:
		  break;
	}
	_ub.city ="太原"; 						
	_ub.biz = 'N'; 							
	_ub.location = 0; 					
	_ub.collect(1,{'vwn.order': encodeURIComponent(parm)});	
