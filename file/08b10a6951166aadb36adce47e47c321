<script language="javascript">
//电商运营位
function dsdisplay(){
	$("html,body").animate({scrollTop:0}, 500);
}
    function condi_other_saling(saling) {
        if(saling == "") {
            location.href = "/house/s/a73/";
        } else if(saling == "1") {
            location.href = "/house/s/a73-b81/";
        } else if(saling == "2") {
            location.href = "/house/s/a73-b82/";
        } else if(saling == "0") {
            location.href = "/house/s/a73-b80/";
        }
    }
    function condi_other_bedroom(url) {
        location.href = url;
    }
	function condi_other_price()
	{				
		var strPriceStart = $("#strPriceStart").val();
		strPriceStart = strPriceStart.replace("￥","");		
		var strPriceEnd = $("#strPriceEnd").val();
		strPriceEnd = strPriceEnd.replace("￥","");	
		//var searReg=new RegExp('/house/s/list/');
		var searReg=new RegExp('/house/s/');
		var Regx = /^[A-Za-z][0-9]*/;
		var searchurl = '/house/s/';
		var searchArr = new Array();
		var deliterArr = new Array();
		var searchDisArea = '';
		var p = 0;
		//判断是否是/house/s/还是/house/web/路径，
		if(searReg.test(strPriceUrl))
		{
			preUrlArr = strPriceUrl.replace("/house/s/","").split("/");
			//删除最后一个空数据
			preUrlArr.pop();
			if (preUrlArr.length == 0) {
				searchArr[0] = 'b2'+strPriceStart+","+strPriceEnd;
			} else {
				if(preUrlArr.length == 1)
				{
					//判断是否是以字母+数字形式，如果不是则是区县商圈
					if (!Regx.test(preUrlArr[0]))
					{
						searchDisArea = preUrlArr[0];
						searchArr[0] = 'b2'+strPriceStart+","+strPriceEnd;
					}
					else
					{
						deliterArr = preUrlArr[0].split("-");
						for(var j=0;j<deliterArr.length;j++)
						{
							strType = deliterArr[j].substr(0,2);
							if(strType == 'b2')
							{
								p++;
								//将现在的价格区间覆盖之前的
								searchArr[j] = 'b2'+strPriceStart+","+strPriceEnd;
							}
							else
							{
								//价格变化应删除排序分页等参数
								if(strType != 'b1' && strType != 'b9')
								{
									searchArr[j] = deliterArr[j];
								}
							}
						}
						if(p == 0)
						{
							searchArr[searchArr.length] = 'b2'+strPriceStart+","+strPriceEnd;
						}
					}
				}
				else
				{
					searchDisArea = preUrlArr[0];
					deliterArr = preUrlArr[1].split("-");
					for(var j=0;j<deliterArr.length;j++)
					{
						strType = deliterArr[j].substr(0,2);
						if(strType == 'b2')
						{
							p++;
							//将现在的价格区间覆盖之前的
							searchArr[j] = 'b2'+strPriceStart+","+strPriceEnd;

						}
						else
						{
							//价格变化应删除排序分页等参数
							if(strType != 'b1' && strType != 'b9' && strType != 'b3' && strType != 'c1')
							{
								searchArr[j] = deliterArr[j];

							}
						}
					}
					if(p == 0)
					{
						searchArr[searchArr.length] = 'b2'+strPriceStart+","+strPriceEnd;
					}

				}
			}
			if(searchDisArea != '')
			{
				searchurl += searchDisArea+"/";
			}
			var strParam = '';
			for(var k=0;k<searchArr.length;k++)
			{
				if(typeof(searchArr[k]) != 'undefined')
				{
					if(strParam == '')
					{
						strParam += searchArr[k];
					}
					else
					{
						strParam += "-"+searchArr[k];
					}
				}
				
			}
			if(strParam != '')
			{
				searchurl += strParam+"/";
			}	
		}
		else
		{
			
			if(strPriceStart && strPriceEnd) {
				searchurl += "b2"+strPriceStart+","+strPriceEnd+"/";
			}else if(strPriceStart){
				searchurl += "b2"+strPriceStart+"s/";
			}else {
				searchurl += "b2"+strPriceEnd+"x/";
			}
			//没有参数，直接将价格参数传进去即可
		}
		 location.href = searchurl;
	}
</script>


