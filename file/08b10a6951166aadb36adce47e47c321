		var ubKeyStr = _ub['vwn.key'].v+';'+_ub['vwn.key'].d+';'+_ub['vwn.key'].m;
		var ubPositionStr = _ub['vwn.position'].v+';'+_ub['vwn.position'].d+';'+_ub['vwn.position'].m;
		var ubSubwayStr = _ub['vwn.subway'].v+';'+_ub['vwn.subway'].d+';'+_ub['vwn.subway'].m;
		var ubGenreStr = _ub['vwn.genre'].v+';'+_ub['vwn.genre'].d+';'+_ub['vwn.genre'].m;
		var ubUnitpriceStr = _ub['vwn.unitprice'].v+';'+_ub['vwn.unitprice'].d+';'+_ub['vwn.unitprice'].m;
		var ubFeatureStr = _ub['vwn.feature'].v+';'+_ub['vwn.feature'].d+';'+_ub['vwn.feature'].m;

		var ubSalestatusStr = _ub['vwn.salestatus'].v+';'+_ub['vwn.salestatus'].d+';'+_ub['vwn.salestatus'].m;
		var ubHousetypeStr = _ub['vwn.housetype'].v+';'+_ub['vwn.housetype'].d+';'+_ub['vwn.housetype'].m;
		var ubLooplineStr = _ub['vwn.loopline'].v+';'+_ub['vwn.loopline'].d+';'+_ub['vwn.loopline'].m;
		var ubOpentimeStr = _ub['vwn.opentime'].v+';'+_ub['vwn.opentime'].d+';'+_ub['vwn.opentime'].m;
		var ubAllCondition = ubKeyStr+'|'+ubPositionStr+'|'+ubSubwayStr+'|'+ubGenreStr+'|'+ubUnitpriceStr+'|'+ubFeatureStr+'|'+ubSalestatusStr+'|'+ubHousetypeStr+'|'+ubLooplineStr+'|'+ubOpentimeStr;
        
		var requestUrl = "/house/web/ubRequest.php?num=4&frompage=xflb&from=xflb&condition="+ubAllCondition+"&time=" + new Date().getTime();
        Xml.Request(requestUrl, null, callback_displayMaylike, null);
