//对比
function compare(newcode){
	var onecom = $('#'+newcode);
	if(onecom.attr("checked")== "checked"){
		window.open(newhouseDomain+"/house/newhouse/newhouse_compare_house.aspx?str_id="+dianpingNewcode+"|"+newcode);
	}
}
//收藏
function shoucangProcess(newcode,projn,address,face,price,pricetype,Pricecategory,keynum){
	if($("#sc_star_"+keynum).hasClass("on")){
	    if(!isLogin){
			var tmpArr = location.href.split("#");
			$("#shHeadBackurl").val(urlencode(tmpArr[0]));
			showid('showlogindiv');			
		}else{
		   $.ajax({
			type:'get',
			dataType:'json',
			url:"/house/ajaxrequest/house_selected.php",
			data:{ newcode:newcode},
			error:function(){},
			success:function(result){
				if(result.result_code==100){
				    $("#selectedId").val(result.select_id);
				    $("#sc_star").addClass("on");
				}
			}
		  });		
		   var selectid = $("#selectedId").val();
			$.ajax({
				type:'get',
				dataType:'json',
				url:"/house/ajaxrequest/house_favorite_del.php",
				data:{ selectid: selectid, city: city ,name: projn, newcode: newcode },
				error:function(){},
				success:function(delResult){
					if(delResult.delres == 100 && delResult.list.result_code == 100) {
					    $("#selectedId").val('');
					    $("#sc_star_"+keynum).removeClass("on");
						//getHouseFavo();
						//$('#shoucang_tips').text('取消收藏成功').fadeIn();
						//setInterval(function(){$('#shoucang_tips').text('').fadeOut();}, 4000);
						//$('#shoucang_link').removeClass('have');
						$('#sc_starmr_'+keynum).text('收藏');
						
					} else {
						//$('#shoucang_tips').text('取消收藏失败').fadeIn();
						//setInterval(function(){$('#shoucang_tips').text('').fadeOut();}, 4000);
					}
				}
			});		
		}
	}else{
	    if(!isLogin){
			var tmpArr = location.href.split("#");
			$("#shHeadBackurl").val(urlencode(tmpArr[0]));
			showid('showlogindiv');			
		}else{	
			$.ajax({
				type:'get',
				dataType:'json',
				url:"/house/ajaxrequest/house_shoucang.php",
				data:{ face: face, name: projn,address: address, price: price,city: city, pricetype: pricetype,parentRefresh: 1, time:new Date().getTime(),newcode:newcode,Pricecategory: Pricecategory},
				error:function(){},
				success:function(result){
				    if(result.msg=='已经添加过该房源' || result.msg =='成功'){
						//$('#shoucang_tips').text(result.msg).fadeIn();
							//$('#shoucang_link').addClass('have');
							$("#sc_star_"+keynum).addClass("on");
							$('#sc_starmr_'+keynum).text('已收藏');
							//_ub.city = "太原"; 
							//_ub.biz = 'n'; 
							//_ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
							//_ub.collect(21,{"vwg.page":"nhpage","vwn.projectid":newcode});

						//setInterval(function(){$('#shoucang_tips').text('').fadeOut();}, 4000);
						//getHouseFavo();
						//initShoucang();
						$.ajax({
							type:'get',
							dataType:'json',
							url:"/house/ajaxrequest/house_selected.php",
							data:{ newcode:newcode},
							error:function(){},
							success:function(result){
								if(result.result_code==100){
									$("#selectedId").val(result.select_id);
									$("#sc_star").addClass("on");
								}
							}
					  });		
					}
				}
			});
				
		}
    }
}
/* 王牌广告click */
function wpclickId() {
	var wpdc_obj = document.getElementById('slist_ad');
	if( wpdc_obj!=null ) {
	   var div_obj = wpdc_obj.getElementsByTagName('div');
	   var len= div_obj.length;
	   var counter = 1;
	   for(i=0;i<len;i++) 
		   if(div_obj[i].className=='searchListNoraml'){
			  div_obj[i].id = "sjina_wpdc_"+counter;  
			  counter++;
		   }
	}
}
/* 区域广告click */
function areaclickId() {
	var area_obj = document.getElementById('ad_area');
	if( area_obj!=null ) {
	   var div_obj = area_obj.getElementsByTagName('div');
	   var len= div_obj.length;
	   var counter = 1;
	   for(i=0;i<len;i++) 
		   if(div_obj[i].className=='quydb830tr'){
			  div_obj[i].id = "sjina_qydb_"+counter;  
			  counter++;
		   }
	}
}
wpclickId();

var dc_obj = document.getElementById('tg_gg_div');
if( dc_obj!=null ) {
	var div_obj = dc_obj.getElementsByTagName('div');
	var len= div_obj.length;
	var left_obj = right_obj = "";
	for(i=0;i<len;i++) 
	{
		if(div_obj[i].className=='leftbox'){
			left_obj = div_obj[i];
		}
		if(div_obj[i].className=='rightbox'){
			right_obj = div_obj[i];
		}
	}
	if( left_obj )
	{
		left_obj.onmouseover = function() {
			//left_obj.style.backgroundColor="#e0f1ff";
			//left_obj.style.cursor = "pointer";
		}
		left_obj.onmouseout = function() {
			//left_obj.style.backgroundColor="";
		}
		//获取链接
		var a_leftobj = left_obj.getElementsByTagName('a');
		if(a_leftobj.length>0) {
			var sub_obj = left_obj.getElementsByTagName('div');
			var click_div_obj = "";
			for(i=0;i<sub_obj.length;i++) {
				if(sub_obj[i].className=='listad') {
					click_div_obj = sub_obj[i];
					break;
				}
			}
			if(click_div_obj) {
				click_div_obj.onclick=function() {
					//window.open(a_leftobj[0].href);    
				}
			}
		}
	}
	if( right_obj ) {
		right_obj.onmouseover = function() {
			//right_obj.style.backgroundColor="#e0f1ff";
			//right_obj.style.cursor = "pointer";
		}
		right_obj.onmouseout = function() {
			//right_obj.style.backgroundColor="";
		}
		//获取链接
		var a_rightobj = right_obj.getElementsByTagName('a');
		if(a_rightobj.length>0)
		{
			var sub_r_obj = right_obj.getElementsByTagName('div');
			var click_div_r_obj = "";
			for(i=0;i<sub_r_obj.length;i++) {
				if(sub_r_obj[i].className=='listad') {
					click_div_r_obj = sub_r_obj[i];
					break;
				}
			}
			if(click_div_r_obj){
				click_div_r_obj.onclick=function() {
					//window.open(a_rightobj[0].href);    
				}
			}
		}
	}
}
