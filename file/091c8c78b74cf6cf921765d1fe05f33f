    function setTabNews(name, cursel, n) {
        for (i = 1; i <= n; i++) {
            var menu = document.getElementById(name + i);
            var con = document.getElementById("con_" + name + i);
            if (i == cursel) {
                menu.className = "cur";
                con.style.display = "block";
            } else {
                menu.className = "";
                con.style.display = "none";
            }
        }
    }
</script> 
