    });
    $(".watch .box2 ul li a").mouseup(function(){
        var _til = $(".detail ._box ._ti h2").html();
        var _n = $(this).find("span").html();
        Tools.C_add(_til,_tp,my_url,_n);
    });
    ;(function(){
        var btns = $(".detail ._box .watch .ti p a");
        var len = btns.lenght;
        var boxes_1 = $(".detail ._box .watch .box2");
        var boxes_2 = $(".detPg .ago");
        var play = false;
        var N1 = 0;
        btns.click(function(){
            if(play) return;
            var _i = $(this).index();
            if(_i==N1) return;
            play = true;
            boxes_2.eq(N1).hide();
            boxes_2.eq(_i).show();
            $(this).addClass("active").siblings(".active").removeClass("active");
            boxes_1.eq(N1).fadeOut(200,function(){
                boxes_1.eq(_i).fadeIn(200,function(){
                    play = false;
                    N1 = _i;
                });
            });
        });
        var year = $(".detPg .ago .til .navs a");
        year.click(function(){
            if(play||$(this).hasClass("active")||$(this).hasClass("_more")) return;
            play = true;
            var _o,_i;
            if($(this).attr("name")){
                var p_nav = $(this).parents(".navs");
                _o = p_nav.find(".active").attr("name");
                _i = $(this).attr("name");
                if($(this).parent().hasClass("more")){
                    var _fir = p_nav.find("a:first");
                    if(_fir.attr("name")==0){
                        $(this).insertBefore(_fir);
                        p_nav.find("a").eq(7).insertBefore(p_nav.find(".more a").eq(0));
                    }
                    else{
                        if($(this).attr("name")==6){$(this).insertBefore(p_nav.find("a._more"));}
                        else{$(this).insertBefore(_fir);}
                        var _n = _fir.attr("name");
                        for(var l=0;l<p_nav.find(".more a").length;l++){
                            var ta = p_nav.find(".more a").eq(l);
                            var tn = ta.attr("name");
                            if(parseInt(tn)>parseInt(_n)){_fir.insertBefore(ta);break;}
                            if(l==p_nav.find(".more a").length-1){_fir.insertAfter(ta);break;}
                        }
                    }
                }
                else{
                    var _fir = p_nav.find("a:first");
                    if(_fir.attr("name")==0){}
                    else{
                        p_nav.find(".more a").eq(0).insertBefore(p_nav.find("a._more"));
                        var _n = _fir.attr("name");
                        for(var l=0;l<p_nav.find(".more a").length;l++){
                            var ta = p_nav.find(".more a").eq(l);
                            var tn = ta.attr("name");
                            if(parseInt(tn)>parseInt(_n)){_fir.insertBefore(ta);break;}
                            if(l==p_nav.find(".more a").length-1){_fir.insertAfter(ta);break;}
                        }
                    }
                }
                p_nav.find(".active").removeClass("active");
                $(this).addClass("active");
            }
            else{
                _o = $(this).siblings(".active").index();
                _i = $(this).index(); 
                $(this).addClass("active").siblings(".active").removeClass("active");
            }
            var par = $(this).parents(".ago");
            var _m = par.find(".til ._list");
            var _box = par.find(".liSy7");
            _m.eq(_o).fadeOut(200,function(){
                _m.eq(_i).fadeIn(200);
            });
            _box.eq(_o).fadeOut(200,function(){
                _box.eq(_i).fadeIn(200,function(){
                    play = false;
                });
            });
        });
        var y_more = $(".ago .til .navs a._more");
        if(y_more.length>0){
            var div_more = $(".ago .til .navs .more");
            var Tout = null;
            $(".ago .til .navs ._more").hover(function(){
                    if(Tout){clearTimeout(Tout);}
                    $(this).parent().find(".more").fadeIn(200);           
            },function(){
                Tout = setTimeout(function(){div_more.fadeOut(200);},1000);
            });
            div_more.hover(function(){
                if(Tout){clearTimeout(Tout);}
            },function(){
                Tout = setTimeout(function(){div_more.fadeOut(200);},800);
            });
            for(var j=0;j<y_more.length;j++){
                var obj = y_more.eq(j).parent();
                var _len = obj.find("a").length;
                for(var k=0;k<_len;k++){
                    var t_a = obj.find("a").eq(k);
                    if(!t_a.hasClass("_more")){
                        if(t_a.parent().hasClass("more")){var _k = k-1;obj.find("a").eq(k).attr("name",_k);}
                        else{obj.find("a").eq(k).attr("name",k);}
                    }
                }
            }    
        }
        var month = $(".detPg .ago .til ._list a");
        month.click(function(){
            if(play||$(this).hasClass("active")) return;
            play = true;
            var _o = $(this).siblings(".active").index()/2;
            var par = $(this).parents(".ago");
            var _par = $(this).parent();
            var _i = $(this).index()/2;
            var p_i = $(this).parents("._list").index()-2;
            var _box = par.find(".liSy7").eq(p_i).find("ul");
            $(this).addClass("active").siblings(".active").removeClass("active");
            _box.eq(_o).fadeOut(200,function(){
                _box.eq(_i).fadeIn(200,function(){
                    play = false;
                });
            });
        });
		var ago_mores = $(".detPg .ago ul.clearfix a.all").length;
        function fun_more(obj){  //点击查看全部方法
            var _span = obj.find("span").html();
            var open = false;
            var par = obj.parent();
            obj.click(function(){
                if(open){
                    par.css("height",284);
                    open = false; 
                    $(this).html("展开查看全部<span>"+_span+"</span><s></s>");   
                }
                else{
                    par.css("height","auto");
                    open = true;
                    $(this).html("收起<s class='up'></s>");
                }                
            })
        }
        for(var i=0;i<ago_mores;i++){
          fun_more($(".detPg .ago ul.clearfix a.all").eq(i));
        }  
    })();
</script>

