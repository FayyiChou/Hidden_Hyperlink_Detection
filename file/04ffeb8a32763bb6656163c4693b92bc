    $(".JqPg .watch").nortab(".ti a",".box");
    Tools.text_limit($("#intro"),149,136);
    var _tp = "zy";
    $(".watch .box2 .b a").mouseup(function(){
        if($(this).parent().hasClass("JiaB")) return;
