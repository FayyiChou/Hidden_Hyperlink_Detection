		}
	});
});
window.onload= function(){
	var page = performance.timing;
	//重定向
	var a0 = (page.redirectEnd > 0 && page.redirectStart> 0) ? (page.redirectEnd-page.redirectStart) : 0;
	//解析域名
	var a1 = (page.domainLookupEnd > 0 && page.domainLookupStart > 0) ? (page.domainLookupEnd - page.domainLookupStart) : 0;
	//tcp连接
	var a2 = (page.connectEnd > 0 && page.connectStart > 0) ? (page.connectEnd - page.connectStart) : 0;
	//request
	var a3 = (page.responseEnd > 0 && page.responseStart > 0) ? (page.responseEnd - page.responseStart) : 0;
	//dom解析
	var a4 = (page.domComplete > 0 && page.domInteractive > 0) ? (page.domComplete - page.domInteractive) : 0;
	//接收数据
	var a5 = (page.responseEnd > 0 && page.responseStart > 0) ? (page.responseEnd - page.responseStart) : 0;
	//js执行
	var a6 = (page.domContentLoadedEventEnd && page.domContentLoadedEventStart > 0) ? (page.domContentLoadedEventEnd - page.domContentLoadedEventStart) : 0;
	//onload方法执行
	var a7 = (page.loadEventEnd > 0 && page.loadEventStart > 0) ? (page.loadEventEnd - page.loadEventStart) : 0;
	//页面加载总耗时
	var a8 = (a0+a1+a2+a3+a4+a5+a6+a7);
	var pvs = a1+","+a2+","+a3+","+a4+","+a5+","+a6+","+a8;
	var tracker2 = Piwik.getTracker("http://uac.99114.com/accept?topics=web_performance_topic&params="+pvs, 100000);
	tracker2.trackPageView();
	tracker2.enableLinkTracking();
};
