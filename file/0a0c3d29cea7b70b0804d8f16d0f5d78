	<script type="text/javascript">
	var lastNum = document.getElementById("memberId").value % 10;
	var siteid = 1;
	switch(lastNum) {
	case 1 : siteid = 40111; break;
	case 2 : siteid = 40112; break;
	case 3 : siteid = 40113; break;
	case 4 : siteid = 40114; break;
	case 5 : siteid = 40115; break;
	case 6 : siteid = 40116; break;
	case 7 : siteid = 40117; break;
	case 8 : siteid = 40118; break;
	case 9 : siteid = 40119; break;
	default : break;
	}
	var tracker = Piwik.getTracker("http://uac.99114.com/accept?topics=piwikShopTopic", siteid);
	tracker.setDocumentTitle("shop"+lastNum+".99114.com/" + document.getElementById("memberId").value);
	tracker.trackPageView();
	tracker.enableLinkTracking();
	</script>

