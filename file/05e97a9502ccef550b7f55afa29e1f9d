<script language="javascript" type="text/javascript">
    <!--//--><![CDATA[//><!--
    //图片滚动列表 mengjia 070816
    var Speed1 = 10; //速度(毫秒)
    var Space1 = 10; //每次移动(px)
    var PageWidths = 255; //翻页宽度
    var fills = 0; //整体移位
    var MoveLocks = false;
    var MoveTimeObjs;
    var Comps = 0;
    var AutoPlayObjs = null;
    GetObj("List3").innerHTML = GetObj("List4").innerHTML;
    GetObj('ISL_Cont1').scrollLeft = fills;
    GetObj("ISL_Cont1").onmouseover = function(){clearInterval(AutoPlayObjs);}
    GetObj("ISL_Cont1").onmouseout = function(){AutoPlay1();}
    AutoPlay1();
    function GetObj(objName){if(document.getElementById){return eval('document.getElementById("'+objName+'")')}else{return eval

            ('document.all.'+objName)}}
    function AutoPlay1(){ //自动滚动
        clearInterval(AutoPlayObjs);
        AutoPlayObjs = setInterval('ISL_GoDown1();ISL_StopDown1();',5000); //间隔时间
    }
    function ISL_GoUp1(){ //上翻开始
        if(MoveLocks) return;
        clearInterval(AutoPlayObjs);
        MoveLocks = true;
        MoveTimeObjs = setInterval('ISL_ScrUp1();',Speed1);
    }
    function ISL_StopUp1(){ //上翻停止
        clearInterval(MoveTimeObjs);
        if(GetObj('ISL_Cont1').scrollLeft % PageWidths - fills != 0){
            Comps = fills - (GetObj('ISL_Cont1').scrollLeft % PageWidths);
            CompScr1();
        }else{
            MoveLocks = false;
        }
        AutoPlay1();
    }
    function ISL_ScrUp1(){ //上翻动作
        if(GetObj('ISL_Cont1').scrollLeft <= 0){GetObj('ISL_Cont1').scrollLeft = GetObj

                ('ISL_Cont1').scrollLeft + GetObj('List4').offsetWidth}
        GetObj('ISL_Cont1').scrollLeft -= Space1 ;
    }
    function ISL_GoDown1(){ //下翻
        clearInterval(MoveTimeObjs);
        if(MoveLocks) return;
        clearInterval(AutoPlayObjs);
        MoveLocks = true;
        ISL_ScrDown1();
        MoveTimeObjs = setInterval('ISL_ScrDown1()',Speed1);
    }
    function ISL_StopDown1(){ //下翻停止
        clearInterval(MoveTimeObjs);
        if(GetObj('ISL_Cont1').scrollLeft % PageWidths - fills != 0 ){
            Comps = PageWidths - GetObj('ISL_Cont1').scrollLeft % PageWidths + fills;
            CompScr1();
        }else{
            MoveLocks = false;
        }
        AutoPlay1();
    }
    function ISL_ScrDown1(){ //下翻动作
        if(GetObj('ISL_Cont1').scrollLeft >= GetObj('List4').scrollWidth){GetObj('ISL_Cont1').scrollLeft =

                GetObj('ISL_Cont1').scrollLeft - GetObj('List4').scrollWidth;}
        GetObj('ISL_Cont1').scrollLeft += Space1 ;
    }
    function CompScr1(){
        var num;
        if(Comps == 0){MoveLocks = false;return;}
        if(Comps < 0){ //上翻
            if(Comps < -Space1){
                Comps += Space1;
                num = Space1;
            }else{
                num = -Comps;
                Comps = 0;
            }
            GetObj('ISL_Cont1').scrollLeft -= num;
            setTimeout('CompScr1()',Speed1);
        }else{ //下翻
            if(Comps > Space1){
                Comps -= Space1;
                num = Space1;
            }else{
                num = Comps;
                Comps = 0;
            }
            GetObj('ISL_Cont1').scrollLeft += num;
            setTimeout('CompScr1()',Speed1);
        }
    }
    //--><!]]>
</script>
 
 </div> 
