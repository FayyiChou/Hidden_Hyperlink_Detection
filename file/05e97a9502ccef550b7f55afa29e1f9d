<script type="text/javascript">
            var speed=30; 
            var tabtscp=document.getElementById("demotscp");
            var tabtscp1=document.getElementById("demotscp1");
            var tabtscp2=document.getElementById("demotscp2");
            tabtscp2.innerHTML=tabtscp1.innerHTML;
            function Marquee(){
            if(tabtscp2.offsetWidth-tabtscp.scrollLeft<=0)
            tabtscp.scrollLeft-=tabtscp1.offsetWidth
            else{
            tabtscp.scrollLeft++;
            }
            }
            var MyMar=setInterval(Marquee,speed);
            tabtscp.onmouseover=function() {clearInterval(MyMar)};
            tabtscp.onmouseout=function() {MyMar=setInterval(Marquee,speed)}</script> 
