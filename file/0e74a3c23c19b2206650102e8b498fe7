<script type="text/javascript">
    var publish = (function () {
        var p = {};
        p.site='http://newbbs.tiancity.com'
        p.login=false;
        p.alt = 'csol';
        p.rno=19;
        p.fno=1;
        p.Url = function (name) {
            if(!p.login){
                    head_login.Login();
                    return;
            }
            location.href=p.site+'/'+p.alt+'/'+name+'/'+p.rno+'/'+p.fno;
        }
        return p;
    })()

