<script type="text/javascript">
    var head_login = (function () {
        var tcLogin = ['TcLoginWin.js'];
        var game_no = 8;
        var login = false;
        var time_status = 0;
        var time1;
        var title = document.title;
        function Online() {
            var submit = new Image();
            submit.src = 'http://newbbs.tiancity.com/Handler/OnlineHandler.ashx?gn=' + game_no;
            window.setTimeout(Online, 60000);
        }
        function UnRead() {
            var count = 0;
            var t_status = false;
            $.getJSON('http://msg.newbbs.tiancity.com/Handler/NotifyHandler.ashx?op=6&cl=?', function (result) {
                if (result.IsSuccess <= 0) {
                    return;
                }
                var read = result.ReturnObject;
                var msg = "";
                var count = read.inbox + read.notify;
                if (count > 99) {
                    msg = "99+";
                }
                else {
                    msg = count;
                }
                $("#un_msg").html(msg);
                if (count > 0) {
                    var t_s = "【新消息】" + title;
                    var space = '';
                    for (var i = 0; i <= 2; i++) space += '　'; //根据标题长度生产相应的空字符 
                    var t_h = "【" + space + "】" + title;
                    if (time_status != 1) {
                        time1 = setInterval(function () {
                            if (t_status) {
                                document.title = t_s;
                                t_status = false;
                            }
                            else {
                                document.title = t_h;
                                t_status = true;
                            }
                        }, 2000)
                        time_status = 1;
                    }
                }
                else {
                    document.title = title;
                    clearInterval(time1);
                    time_status = 0;
                }
            });
        };
        return {
            Login: function (uri) {
                jQuery.getScript("http://passport.tiancity.com/js/" + tcLogin[0],
                    function () {
                        if(uri != null){
                            TcLoginWin.Redirect = true;
                            TcLoginWin.RedirectUrl = uri;
                        }
                        TcLoginWin.Login();
                    });
            },
            InitOnline: function () {
                Online();
            },
            More: function () {
                var timeout;
                $(".bbs").hover(function () {
                    clearTimeout(timeout);
                    $("#gmore").show();
                }, function (e) {
                    timeout = setTimeout(function () {
                        $("#gmore").hide();
                    }, 500)
                })
            },
            UnRead:function(){
                if(login){
                    UnRead()
                    setInterval(UnRead, 20000);
                }
            },
            CheckUri:function(){
                $(".curi").click(function(){
                    var uri = $(this).attr("uri")
                    $.getJSON('http://newbbs.tiancity.com/Handler/IsLoginHandler.ashx', function (result) {
                        if(result.IsSuccess == 0){
                            head_login.Login(uri);
                        }
                        else{
                            location.href = uri;
                        }

                    });
                })
            }
        }
    } ());
    function TcLoginWin_Init() {
        TcLoginWin.MoveWindow = true;
        TcLoginWin.ReLoadPage = true;
//        TcLoginWin.LoginLayerStyle = "tcloginwin_frame_hi";
        TcLoginWin.SiteId = 970;
        TcLoginWin.RegFrom = "http://member.tiancity.com/Registration/accountReg.aspx?from=bbs&gb=1&fromurl=" + window.location.href + "";
    }
    $(function () {
        head_login.More();
        head_login.InitOnline();
        head_login.UnRead();
        head_login.CheckUri();
    })
</script>  
