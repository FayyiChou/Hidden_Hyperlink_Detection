<script type="text/javascript">
    var replylist = (function () {
        var r = {};
        r.searchhandler='http://newbbs.tiancity.com/Handler/PostsReplySearchHandler.ashx';
        r.replyhandler='http://newbbs.tiancity.com/Handler/ReplyHandler.ashx';
        r.size=20;
        r.index=1;
        r.total=3;
        r.floor=0;
        r.show_type=1;
        r.show_userno=0;
        r.Init=function(){
            r.InitSelect();
            r.ReplyCallBack(r.index,r.total);
            r.InitFloor();
        };
        r.InitSelect=function(){
            page.datatype='text';
            page.tag_name = 'pagenum';
            page.current=1;
            page.size=r.size;
            page.val = '&pn='+posts.pno+'&gn=' + posts.game + '&rn='+posts.rno+'&fn=' + posts.fno + '&st='+r.show_type+'&su='+r.show_userno;
            page.operate = 1;
            page.tag_name = 'pagenum';
            page.handler = r.searchhandler;         
            page.loading = function () {                 
                $('#reply_list').html('');            
            };  
            page.callback=r.ReplyCallBacks;
        };
        r.InitFloor=function(){
            $('#to_floor').click(function(){               
                 r.ToFloor();                 
            })
            $('#txt_floor').keypress(function(e) {
                if (e.which == 13 || e.which == 10) {
                    r.ToFloor();        
                } 
            });
        };
        r.ToFloor=function(){
             var floor=$('#txt_floor').val();
             if(floor==''){
                 inform.Alert('电梯直达数不能为空');
                 return;
             }
             if(floor=='0'){
                 inform.Alert('电梯直达数不能为零');
                 return;
             }            
             Validator.Value = floor;
             Validator.Verify(15, "电梯直达数必须填正整数");
             var msg = Validator.Result.Msg;
             if (!Validator.Prompt()) {
                 inform.Alert(msg);
                 return;
             }
             if(parseInt(floor)> r.total){
                 inform.Alert('电梯直达数大于回复总数');
                 return;
             }
             var index = Math.ceil(floor / r.size);
             r.floor=floor;   
             page.current=index;
             page.val = '&pn='+posts.pno+'&gn=' + posts.game + '&rn='+posts.rno+'&fn=' + posts.fno + '&st=1&su=0';
             page.Initialize();     
        };
        r.ReplyCallBacks=function(result){
            var is =result.match(/\(index=\d+\)/g);
            var ts =result.match(/\(total=\d+\)/g);
            if(is.length==0||ts.length==0){
                 return;
            }
            var index=parseInt(is[0].replace("(index=","").replace(")",""));
            var total=parseInt(ts[0].replace("(total=","").replace(")",""));
            result=result.replace(is,'').replace(ts,'');
            if(r.floor>total){
                inform.Alert('电梯直达数大于回复总数');
                return;
            }
            if(index==1){
                $("#posts_info").show();
            }else{
                $("#posts_info").hide();
            }
            $('#reply_list').html(result);
             if(r.show_userno==0){
                r.total=total;
             }
             page.count   = total;
             page.current = index;
             page.CreatePage();
             if(r.floor!=0){
                $('#show_auth').show();
                $('#show_allfloor').hide();
                $('#show_asc').hide();
                $('#show_desc').show();
                window.location.hash='f_'+r.floor;
                r.floor=0;
             }
        };
        r.ReplyCallBack=function(index,total){
            if(r.floor>total){
                inform.Alert('电梯直达数大于回复总数');
                return;
            }
             page.count   = total;
             page.current = index;    
             if(index==1){
                $("#posts_info").show();
             }else{
                $("#posts_info").hide();
             }       
             if(r.floor!=0){
                $('#show_auth').show();
                $('#show_allfloor').hide();
                $('#show_asc').hide();
                $('#show_desc').show();
                window.location.hash='f_'+r.floor;
                r.floor=0;
             }
             browser.loaded(function(){
                 page.CreatePage();
             })
        };
        r.ShowUser=function(userno){
            r.show_userno=userno;
            r.InitSelect();
            page.Initialize();
            $('#show_auth').hide();
            $('#show_allfloor').show();         
        };
        r.ShowAll=function(){
            r.show_userno=0;
            r.InitSelect();
            page.Initialize();
            $('#show_auth').show();
            $('#show_allfloor').hide();
        };
        r.ShowAsc=function(){
            r.show_type=1;
            r.InitSelect();
            page.Initialize();
            $('#show_asc').hide();
            $('#show_desc').show();
        };
        r.ShowDesc=function(){
            r.show_type=2;
            r.InitSelect();
            page.Initialize();
            $('#show_asc').show();
            $('#show_desc').hide();
        };    
        /*屏蔽*/
        r.BadReplyPosts = function (no) {
            complain.handler = postedit.detailhandler;
            complain.data = { gno: posts.game, rno: posts.rno, fno: posts.fno };
            complain.title = "屏蔽";
            complain.type = 0;
            complain.Show();
                    complain.callback =function () {
                        var reason = $('#sel_reason option:selected').text();                     
                        var remark = $('#txt_remark').val();
                        if (reason == '请选择') {
                            inform.Alert('请选择屏蔽原因');
                            return;
                        }
                        reason = string.Replace(escape(reason));
                        remark = string.Replace(escape(remark));
                        Sender.handler = r.replyhandler;
                        Sender.val = 'op=5&ry=' + no+ '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&rs=' + reason+ '&rk=' + remark;;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                complain.Remove();              
                                inform.Alert('屏蔽成功');                                
                                $('#replyposts_' + no).html('<div class="art clearfix"><div class=\"screen_box\"><p>此内容已被<span>管理员</span>屏蔽</p></div></div>');
                                $('#sel_reason').val('');
                                $('#txt_remark').val('');
                                $('#manager_pop').hide();
                            } else {
                                inform.Alert(result.ReturnString);
                            }

                        }
                        Sender.OnSubmit();
                    }
        };
        /*删除*/
        r.DeleteReplyPosts = function (no) {
            inform.Confirm("确定删除此回帖吗?", function () {
                        Sender.handler = r.replyhandler;
                        Sender.val = 'op=4&ry=' + no + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                inform.Alert('删除成功');
                                $('#replyposts_'+no).html('<div class="art clearfix"><div class=\"del_box\"><p>此内容已被<span>管理员</span>删除</p></div></div>');
                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                 })
        };  
        return r;
    })()
   replylist.Init();
</script>
        </div>
