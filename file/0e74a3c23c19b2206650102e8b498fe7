<script type="text/javascript">
    var detail = (function () {
        var d = {};
        d.user = { sno: 0, name: '', faceurl: '' };
        d.replyno = 0;
        d.Init = function () {
            $('.back_list').attr('href', posts.postslist);
            $('#add_reply').click(function () {
                if(!posts.login){
                    head_login.Login();
                    return;
                }
                if (!postedit.ValidateReply()) {
                    return;
                }
                d.Save();
            })

        };
        d.Save = function () {            
            var ct = string.Replace(escape(postedit.SaveDefine($("#txt_content").val())));
            var code = string.Replace(escape(validatecode.GetCode()));
            var close=0;;
            if($('#reply_close').attr('checked')){
                close=1;
            }
            Sender.handler = postedit.replyhandler;
            Sender.val = 'op=2&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno
            + '&ct=' + ct + '&cd=' + code + '&ry=' + d.replyno+'&sd='+validatecode.code+'&rc='+close;
            Sender.callback = function (result) {
                if (result.IsSuccess == 1) {
                    inform.Alert('保存成功');
                    location.reload();
                } else {
                    validatecode.Reload();
                    inform.Alert(result.ReturnString);
                    $('#add_replying').hide();
                    $('#add_reply').show();
                }
            }
            $('#add_reply').hide();
            $('#add_replying').show();
            Sender.OnSubmit();
        };
        d.GetReply = function (no) {
            Sender.handler = postedit.replyhandler;
            Sender.val = 'op=3&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno
            + '&ry=' + no;
            Sender.callback = function (result) {
                if (result.IsSuccess == 1) {
                    var reply = result.ReturnObject;
                    d.replyno = reply.ReplyNo;
                    $("#txt_content").val(postedit.QueryDefine(reply.Content));
                    $("#txt_content").focus();
                } else {
                    validatecode.Reload();
                    inform.Alert(result.ReturnString);
                }
            }
            Sender.GetData();
        };
        d.SendMessage=function(c){                  
            var receiver = { no: $(c).attr('no'), sno: $(c).attr('sno'), name: $(c).attr('name'), faceurl: $(c).attr('face') };
            sendmessage.sender=d.user;
            sendmessage.receiver=receiver;
            sendmessage.Show();
        };
        d.Complain=function(type,no){
            complain.handler=postedit.detailhandler;
            complain.data = { gno:  posts.game, rno: posts.rno, fno: posts.fno };
            complain.title="举报";
            complain.type=0;
            complain.Show();
            complain.callback =function () {
                        var reason = $('#sel_reason').val();                     
                        var remark = $('#txt_remark').val();
                        if (reason == '') {
                            inform.Alert('请选择举报理由');
                            return;
                        }
                        if (remark == '') {
                            inform.Alert('请填写举报备注');
                            return;
                        }
                        remark=string.Replace(escape(remark));
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=17&rt=' + type+ '&rd='+no+'&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&cy=' + reason+'&ct='+remark;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                inform.Alert(result.ReturnString); 
                                complain.Remove();                          
                            } else {
                                inform.Alert(result.ReturnString);
                            }

                        }
                        Sender.OnSubmit();
                    }
        };
        d.Reply=function(){
            if(!posts.login){
                head_login.Login();
            }else{
                window.location.hash='#a1';
            }
        };
        return d;
    })()
    detail.Init();
    browser.loaded(lazy.BackToTop);   
    $('#posts_detail').LoadImage();
</script><div style="display:none" id="elevator" class="Indicator"><img src="http://img01.bbs.tiancitycdn.com/public/top.jpg"></div></body></html>