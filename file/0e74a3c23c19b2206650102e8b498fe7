        <script type="text/javascript">
            var posthead = (function () {
                var r = {};
                r.buffer = new StringBuffer();
                r.Init = function () {
                    r.CopyToClipboard();

                };
                r.CopyToClipboard = function () {
                    var url = window.location.href;
                    var clip;
                    if (window.clipboardData) {
                        $('#copy_link').click(function () {
                            window.clipboardData.clearData();
                            window.clipboardData.setData("Text", url);
                            inform.Alert("复制成功");
                        });
                    }
                    else {
                        clip = new ZeroClipboard.Client();
                        ZeroClipboard.setMoviePath("http://js01.bbs.tiancitycdn.com/util/ZeroClipboard.swf");
                        clip.setHandCursor(true);
                        clip.addEventListener('load', function (clint) { });
                        clip.addEventListener('mouseOver', function (client) { clip.setText(url); });
                        clip.addEventListener('complete', function (client, text) { inform.Alert("复制成功"); });
                        clip.glue('copy_link');
                    }
                };
                /*屏蔽楼主*/
                r.BadUser = function (userno) {
                    complain.handler = postedit.detailhandler;
                    complain.data = { gno: posts.game, rno: posts.rno, fno: posts.fno };
                    complain.title = "屏蔽";
                    complain.type = 1;
                    complain.Show();
                    complain.callback =
                    function () {
                        var reason = $('#sel_reason option:selected').text();
                        var remark = $('#txt_remark').val();
                        var days = $('#sel_days').val();
                        if (days == '-1') {
                            inform.Alert('请选择屏蔽时间');
                            return;
                        }
                        if (reason == '请选择') {
                            inform.Alert('请选择屏蔽原因');
                            return;
                        }
                        reason = string.Replace(escape(reason));
                        remark = string.Replace(escape(remark));
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=14&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&rs=' + reason + '&rk=' + remark + '&un=' + userno + '&ds=' + days;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                complain.Remove();
                                inform.Alert('屏蔽成功');
                                window.location.reload()
                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                    };

                };
                /*屏蔽主贴*/
                r.BadPosts = function () {
                    complain.handler = postedit.detailhandler;
                    complain.data = { gno: posts.game, rno: posts.rno, fno: posts.fno };
                    complain.title = "屏蔽";
                    complain.type = 0;
                    complain.Show();
                    complain.callback = function () {
                        var reason = $('#sel_reason option:selected').text();
                        var remark = $('#txt_remark').val();
                        if (reason == '请选择') {
                            inform.Alert('请选择屏蔽原因');
                            return;
                        }
                        reason = string.Replace(escape(reason));
                        remark = string.Replace(escape(remark));
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=8&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&rs=' + reason + '&rk=' + remark;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                complain.Remove();
                                inform.Alert('屏蔽成功');
                                $('#posts_' + posts.pno).html('<div class="art clearfix"><div class=\"screen_box\"><p>此内容已被<span>管理员</span>屏蔽</p></div></div>');
                                $('#sel_reason').val('');
                                $('#txt_remark').val('');
                                $('#manager_pop').hide();
                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                    };
                };
                /*删除*/
                r.DeletePosts = function () {
                    inform.Confirm("确定删除此帖吗?", function () {
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=6&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                inform.Alert('删除成功');
                                location.href = posts.postslist;
                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                    })
                };
                /*关闭*/
                r.ClosePosts = function (c, ty) {
                    var title = '';
                    if (ty == 1) {
                        title = "确定关闭此帖吗?"
                    } else {
                        title = "确定开启此帖吗?"
                    }
                    inform.Confirm(title, function () {
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=10&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&is=' + ty;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                inform.Alert('操作成功');
                                var nty;
                                switch (ty) {
                                    case 0:
                                        $(c).text('关闭');
                                        nty = 1;
                                        break;
                                    case 1:
                                        $(c).text('开启');
                                        nty = 0;
                                        break;
                                }
                                $(c).click(function () {
                                    r.ClosePosts(this, nty);
                                })

                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                    })
                };
                /*置顶*/
                r.TopPosts = function (c, ty) {
                    var title = '';
                    if (ty == 1) {
                        title = "确定置顶此帖吗?"
                    } else {
                        title = "确定取消此帖置顶吗?"
                    }
                    inform.Confirm(title, function () {
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=12&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&is=' + ty;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                inform.Alert('操作成功');
                                var nty;
                                switch (ty) {
                                    case 0:
                                        $(c).text('置顶');
                                        nty = 1;
                                        break;
                                    case 1:
                                        $(c).text('取消置顶');
                                        nty = 0;
                                        break;
                                }
                                $(c).click(function () {
                                    r.TopPosts(this, nty);
                                })

                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                    })
                };
                /*精华*/
                r.GoodPosts = function (c, ty) {
                    var title = '';
                    if (ty == 1) {
                        title = "确定精华此帖吗?"
                    } else {
                        title = "确定取消此帖精华吗?"
                    }
                    inform.Confirm(title, function () {
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=13&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&is=' + ty;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                inform.Alert('操作成功');
                                var nty;
                                switch (ty) {
                                    case 0:
                                        $(c).text('精华');
                                        nty = 1;
                                        break;
                                    case 1:
                                        $(c).text('取消精华');
                                        nty = 0;
                                        break;
                                }
                                $(c).click(function () {
                                    r.GoodPosts(this, nty);
                                })

                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                    })
                };
                /*拉前*/
                r.FrontPosts = function () {
                    inform.Confirm("确定拉前此帖吗?", function () {
                        Sender.handler = postedit.detailhandler;
                        Sender.val = 'op=11&pn=' + posts.pno + '&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno;
                        Sender.callback = function (result) {
                            if (result.IsSuccess == 1) {
                                inform.Alert('操作成功');
                            } else {
                                inform.Alert(result.ReturnString);
                            }
                        }
                        Sender.OnSubmit();
                    })
                };
                r.ShareContent = function () {
                    var title = $('#em_title').html().replace(/\s/g, "");
                    var content = title + "  详情：" + window.location.href;
                    return content;
                };
                /*分享甜橙派*/
                r.TianCity = function () {
                    var content = r.ShareContent();
                    window.open(" http://hi.tiancity.com/interface/share/share.aspx?content=" + escape(content));
                };
                /*分享新浪*/
                r.Sina = function () {
                    var content = r.ShareContent();
                    window.open("http://service.weibo.com/share/share.php?title=" + encodeURIComponent(content));
                };
                /*分享腾讯*/
                r.Tencent = function () {
                    var content = r.ShareContent();
                    window.open("http://share.v.t.qq.com/index.php?c=share&a=index&f=q2&title=" + encodeURIComponent(content));
                };
                return r;
            })()
            posthead.Init();
</script>
