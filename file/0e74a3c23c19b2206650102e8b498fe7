<script type="text/javascript">
    var postdetail = (function () {
        var p = {};
        p.handler = 'http://newbbs.tiancity.com/Handler/PostsDetailHandler.ashx'
        p.Init = function () {
            p.InitPostFace();
            p.InitPostVote();
        };
        p.InitPostFace = function () {
            $('#post_face :radio').click(function () {
                if (!posts.login) {
                    head_login.Login();
                    return;
                }
                var faceno = $(this).attr('no');
                p.PostFace(faceno);
            })

            $('#post_face a').click(function () {
                if (!posts.login) {
                    head_login.Login();
                    return;
                }
                var faceno = $(this).attr('no');
                $('#post_face :radio[no=' + faceno + ']').attr('checked', true);
                p.PostFace(faceno);
            })
        };
        p.InitPostVote = function () {
            $('#btn_vote').click(function () {
                if (!posts.login) {
                    head_login.Login();
                    return;
                }
                var num;
                var positions = '';
                var type = parseInt($(this).attr('type'));
                var max = parseInt($(this).attr('max'));
                if (type == 1) {//单选
                    var radio = $('#vote_box input[type=radio]:checked');
                    num = radio.length;
                    if (num == 0) {
                        inform.Alert('请选择投票项');
                        return;
                    }
                    if (num > 1) {
                        inform.Alert('只能选择一项');
                        return;
                    }
                    positions = radio.attr('no');

                } else {//多选                    
                    num = $('#vote_box i[class=cur]').length;
                    if (num == 0) {
                        inform.Alert('请选择投票项');
                        return;
                    }
                    if (num > max) {
                        inform.Alert('超过多选投票的最大项');
                        return;
                    }
                    $('#vote_box i[class=cur]').each(function () {
                        positions += $(this).attr("name") + ',';
                    })
                }
                Sender.handler = p.handler;
                Sender.val = 'op=2&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&pn=' + posts.pno + '&ps=' + positions;
                Sender.callback = function (result) {
                    if (result.IsSuccess == 1) {
                        inform.Alert("投票成功");
                        $('#vote_box').html(result.ReturnString);
                    } else {
                        inform.Alert(result.ReturnString);
                    }
                }
                Sender.OnSubmit();
            });

            $('#vote_box i').click(function () {
                if ($(this).attr('class') == 'cur') {
                    $(this).removeClass('cur');
                } else {
                    $(this).addClass('cur');
                }
            });
        };
        p.PostFace = function (faceno) {
            Sender.handler = p.handler;
            Sender.val = 'op=1&gn=' + posts.game + '&rn=' + posts.rno + '&fn=' + posts.fno + '&pn=' + posts.pno + '&fc=' + faceno;
            Sender.callback = function (result) {
                if (result.IsSuccess == 1) {
                    inform.Alert("发表成功");
                    $('#post_face').html(result.ReturnString);
                    p.InitPostFace();
                } else {
                    inform.Alert(result.ReturnString);
                }
            }
            Sender.OnSubmit();
        };
        return p;
    })()
    postdetail.Init();
</script>
            
