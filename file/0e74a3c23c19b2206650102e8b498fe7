<script type="text/javascript">
    var postedit = (function () {
        var p = {};   
        p.type=3;//控件类型
        p.gno=8;//游戏编号
        p.rno=19;//主版块编号
        p.fno=1;//子版块编号  
        p.handler='http://newbbs.tiancity.com/Handler/PostsHandler.ashx';
        p.detailhandler = 'http://newbbs.tiancity.com/Handler/PostsDetailHandler.ashx';
        p.replyhandler = 'http://newbbs.tiancity.com/Handler/ReplyHandler.ashx';
        p.data={title:'',content:''};//保存草稿
        p.draft={"Title":"","Content":""};
        p.facelist=[];//表情列表
        p.faceloading=false;//是否已经加载表情列表
        p.login=false;
        p.is_draft=true;
        p.focus=0;//文本焦点
        p.Init = function () {
            /*加载草稿*/
            $('#txt_title').val(p.QueryDefine(p.draft.Title));
            $('#txt_content').val(p.QueryDefine(p.draft.Content));
            p.InitForm();           
        };
        p.InitForm=function(){
            switch(p.type){ 
                case 1://发新帖
                    p.InitEditText();    
                        window.onbeforeunload = function(event) {
                            p.SavaDraft(); 
                        }             
                    break; 
                case 2://修改帖
                    p.InitEditText();                  
                    break;   
                case 3://回帖
                    p.InitReplyText();
                    break;                
            }       
            $('#btn_edit').click(function(){
                if(!p.login){
                    head_login.Login();
                    return;
                }
                $('#preview_box').hide();
                $('#edit_btn').show();
                $('#edit_txt').show();               
            }) 
            $('#hold_ic').click(function(){
                 if(!p.login){
                    head_login.Login();
                    return;
                 }
                 p.hidepop();
                 p.Replace("b","要加粗的文字");
            })
            $('#tilt_ic').click(function(){
                 if(!p.login){
                    head_login.Login();
                    return;
                 }
                 p.hidepop();
                 p.Replace("i","要变为斜体的文本");
            })
            $('#under_ic').click(function(){
                if(!p.login){
                    head_login.Login();
                    return;
                }
                 p.hidepop();
                 p.Replace("u","要加下划线的文本");
            })
            $('#color_ic').click(function(){
                 if(!p.login){
                    head_login.Login();
                    return;
                 }
                 $('#show_face').hide();
                 $('#show_image').hide();
                 $('#show_video').hide();
                 $('#txt_color').show();
                 $('#txt_color li').unbind();
                 $('#txt_color li').click(function(){
                    $('#txt_color').hide();                 
                     p.Replace("color","要加颜色的文本",$(this).children('a').attr('name'));
                 })              
            })
            $('#link_ic').click(function(){
                 if(!p.login){
                    head_login.Login();
                    return;
                 }
                 p.hidepop();
                 p.Replace("url","链接");
            })
            $('#img_ic').click(function(){
                if(!p.login){
                    head_login.Login();
                    return;
                }
                var ct = $("#txt_content").val();
                if (ct.split("[img]").length > 10) {
                    inform.Alert("图片最多上传10张");
                    return;
                }       
                $('#txt_color').hide();
                $('#show_face').hide();
                $('#show_video').hide();
                p.focus=$("#txt_content").GetFocus();
                $(window.frames["form_upload"].document).find("#img_file").change(function(){
                    var value = $(this).val();
                    if (value == "") {
                        return;
                    }
                    $("#show_image").hide();
                    $("#uploading").show();
                    $(window.frames["form_upload"].document).find("#form1").submit();            
                })             
                $('#show_image').show();
                
            })
            $('#video_ic').click(function(){
                if(!p.login){
                    head_login.Login();
                    return;
                }
                var ct = $("#txt_content").val();
                if (ct.split("[flash]").length > 5) {
                    inform.Alert("视频最多上传5个");
                    return;
                }   
                p.focus=$("#txt_content").GetFocus();
                $('#txt_color').hide();
                $('#show_face').hide();
                $('#show_image').hide();
                $('#show_video').show();
                $('#add_video').unbind().click(function(){
                    var v=$('#txt_video').val();
                    var l=v.length;
                    if(v.substring(l-4,l)=='.swf'){
                        p.InsertContent('[flash]'+v+'[/flash]',p.focus); 
   
                        $('#txt_video').val('')
                    }else{
                        inform.Alert("视频地址格式不正确");
                    }                   
                    $('#show_video').hide();
                })
            })
            $('#face_ic').click(function(){
                if(!p.login){
                    head_login.Login();
                    return;
                }
                $('#txt_color').hide();
                $('#show_video').hide();
                $('#show_image').hide();
                $('#txt_content').Face(p.gno,'show_face');
                $('#show_face').show();
            })         
        }; 
        p.Image=function(img){
            $('#img_file').val('')
            $("#uploading").hide();
            p.InsertContent('[img]'+img+'[/img]',p.focus);
        }
        p.ImageError=function(msg){
            $(".select_video").val("");
            $("#uploading").hide();
            inform.Alert(msg);
        }  
        p.InsertContent=function(ct,ix){
            var cnt=$("#txt_content");
            var txtValue=cnt.val();
            var nValue = txtValue.substring(0, ix) + ct + txtValue.substring(ix);
            cnt.val(nValue);
        };
        p.hidepop=function(){
             $('#show_face').hide();
             $('#show_image').hide();
             $('#show_video').hide();
             $('#txt_color').hide();
        };  
        /*主贴内容编辑框下面的操作按钮，只有发主贴才有*/ 
        p.InitEditText=function(){
           $('#btn_view').click(function(){
                if(!p.ValidatePosts()){
                    return;
                }
                $('#edit_btn').hide();
                $('#edit_txt').hide();
                p.PreviewPosts();
                $('#preview_box').show();
            }) 
            $('#edit_info').show();
            /*清空*/
            $('#renew').click(function(){
                $('#txt_content').val('');
            }) 
            /*编辑文本框大小*/
            $('#increase').click(function(){
                var height = $("#txt_content").css("height").replace("px", "");
                height = parseInt(height) + 230;
                $("#txt_content").css("height", height);
            })
            $('#reduce').click(function(){
                var height = $("#txt_content").css("height").replace("px", "");
                height = parseInt(height) - 230;
                if ((parseInt(height) - 230) < 230) {
                    $("#txt_content").css("height", 230);
                }
                else {
                    $("#txt_content").css("height", height);
                }
            })
            /*保存草稿*/
            $('#save_draft').click(function(){
                p.SaveLocation();
            })
            setInterval(function() {
                p.SaveLocation();
            }, 30000);
            /*恢复数据*/
            $('#resume').click(function(){             
                $('#txt_title').val(p.data.title);
                $('#txt_content').val(p.data.content);
            })                   
        };
        p.SaveLocation=function(){
            p.data.title=$('#txt_title').val();
            p.data.content=$('#txt_content').val();
            $('#edit_message').html('保存成功!');
            setTimeout(function() {
                $('#edit_message').html('');
            }, 1000);
        };
        /*回帖操作*/
        p.InitReplyText=function(){
            $('#edit_info').remove();
            $('#btn_view').click(function(){
                if(!p.login){
                    head_login.Login();
                    return;
                }
                if(!p.ValidateReply()){
                    return;
                }
                $('#edit_btn').hide();
                $('#edit_txt').hide();
                p.PreviewReply();
                $('#preview_box').show();
            }) 
        };
        /*替换标签*/
        p.Replace=function(label, dft,style){           
            var cnt=$("#txt_content");
            var rangeData = { text: "", start: 0, end: 0 };
            if (cnt[0].setSelectionRange) {           
                var textarea = cnt[0];
                var txtValue = cnt.val();
                cnt.focus();
                rangeData.start = textarea.selectionStart;
                rangeData.end = textarea.selectionEnd;
                var middle = (rangeData.start != rangeData.end) ? txtValue.substring(rangeData.start, rangeData.end) : dft;
                if(typeof(style)=='undefined'){
                    rangeData.text = "[" + label + "]" + middle + "[/" + label + "]";
                }else{
                    rangeData.text = "[" + label + "="+style+"]" + middle + "[/" + label + "]";
                }                
                nValue = txtValue.substring(0, rangeData.start) + rangeData.text + txtValue.substring(rangeData.end);
                cnt.val(nValue);
            }
            else {              
                cnt.focus();
                var os = document.selection.createRange();
                var middle = os.text != "" ? os.text : dft;
                var content;
                if(typeof(style)=='undefined'){
                     content = "[" + label + "]" + middle + "[/" + label + "]";
                }else{
                     content ="[" + label + "="+style+"]" + middle + "[/" + label + "]";
                }
                document.selection.empty();
                os.text = content;
                os.collapse();
                os.select();                                  
            }
        };
       
        /*预览主贴*/
        p.PreviewPosts=function(){           
            var ct = p.PreviewDefine($("#txt_content").val().toLowerCase());
            ct=p.PvLink(ct);
            ct=p.PvHold(ct);
            ct=p.PvTilt(ct);
            ct=p.PvColor(ct);
            ct=p.PvUnder(ct);        
            ct=p.PvImg(ct);
            ct=p.PvVideo(ct);
            p.Face(ct);
            
        };
         /*预览回贴*/
        p.PreviewReply=function(){           
            var ct = p.PreviewDefine($("#txt_content").val().toLowerCase());
            ct=p.PvLink(ct);
            ct=p.PvHold(ct);
            ct=p.PvTilt(ct);
            ct=p.PvColor(ct);
            ct=p.PvUnder(ct);         
            ct=p.PvImg(ct);
            ct=p.PvVideo(ct);
            p.Quote(ct);
        };
        /*预览转换*/
        p.PreviewDefine= function(content) {
            if(content==null){
                return '';
            }
            return content.replace(/</g, "&lt;").replace(/>/g, "&gt;").replace(/\n/g,"<br>").replace(/\r\n/g, "<br>").replace(/\s/g, "&nbsp;");
        };
        /*查询帖子转换*/
        p.QueryDefine= function(content) {
            if(content==null){
                return '';
            }
            return content.replace(/\[br\]/g, "\n").replace(/\[\/br\]/g, "\r\n").replace(/\[nbsp\]/g, " ")
            .replace(/&amp;/g, "&").replace(/&#40;/g, "(").replace(/&#41;/g, ")")
            .replace(/&lt;/g, "<").replace(/&gt;/g, ">").replace(/&#43;/g, "+")
            .replace(/&#39;/g, "'").replace(/&quot;/g, '"');
        };
        /*保存帖子转换*/
        p.SaveDefine= function(content) {
            if(content==null){
                return '';
            }
            return content.replace(/\n/g, "[br]").replace(/\r\n/g, "[br]").replace(/\s/g, "[nbsp]");
        };
        p.PvHold=function(ct){
            return ct.replace(/\[b\]/g,'<strong>').replace(/\[\/b\]/g,'</strong>');
        };
        p.PvTilt=function(ct){
            return ct.replace(/\[i\]/g,'<em>').replace(/\[\/i\]/g,'</em>');
        };
        p.PvUnder=function(ct){
            return ct.replace(/\[u\]/g,'<span style="text-decoration:underline">').replace(/\[\/u\]/g,'</span>');
        };
        p.PvColor=function(ct){
            var colors = ct.match(/\[color=[0-9a-z]{6}\].*?\[\/color\]/g);            
            if (colors == null) {
                return ct;
            }
            for (var i = 0; i<colors.length; i++) {
                  var color=colors[i];
                  var style=color.match(/\[color=[0-9a-z]{6}?\]/g)                 
                  var c=color.replace(style[0], "").replace("[/color]", "");//内容                  
                  var s=style[0].replace("[color=","").replace("]","");
                  var rc='<span style="color:#'+s+';">'+c+'</span>';
                  ct = ct.replace(color, rc);
            }
            return ct;         
        };
        p.ReplaceLink=function(ct){
            ct=ct.replace(/\[b\]/g,'')
              .replace(/\[\/b\]/g,'')
              .replace(/\[i\]/g,'')
              .replace(/\[\/i\]/g,'')
              .replace(/\[u\]/g,'')
              .replace(/\[\/u\]/g,'')
              .replace(/\[color=[0-9a-z]{6}?\]/g,'')
              .replace(/\[\/color\]/g,'');
              return ct;
        };
        p.PvLink=function(ct){
            var links = ct.match(/\[url\].*?\[\/url\]/g);
            if (links == null) {
                return ct;
            }
            for (var i = 0; i<links.length; i++) {
                var beg_tag = "[url]";
                var end_tag = "[/url]";
                var link = links[i];
                var text = link.replace(end_tag, "").replace(beg_tag, "");
                var url=p.ReplaceLink(text);
                var flash = '<a class="pr_link" href="'+url+'">'+text+'</a>';              
                ct = ct.replace(link, flash);
            }
            return ct;
        };
        p.Face=function(ct){
            if(!p.faceloading){              
                 Sender.handler = site.url + '/Handler/FaceHandler.ashx';
                 Sender.val = 'gn=' + p.gno;
                 Sender.callback = function(data){
                    for(var i=0;i<data.length;i++){
                         p.facelist[data[i].Name]=data[i].Icon;             
                    }
                    p.faceloading=true;
                    p.ReplaceFace(ct);
                 };
                 Sender.GetData();
            }else{
                p.ReplaceFace(ct);
            }
        };
        p.ReplaceFace=function(ct){
            var face=ct.match(/\[.*?\]/g);
            if(face==null){
                $('#preview_content').html(ct);
                return;
            }
            for(var i=0;i<face.length;i++){
                var icon=p.facelist[face[i]];
                if(typeof(icon)!='undefined') {
                    ct=ct.replace(face[i],'<img src="'+icon+'">');
                }
            }
            $('#preview_content').html(ct);
        };
        p.Quote=function(ct){
            var quote=ct.match(/\[quote\]type=\d+,id=\d+?\[\/quote\]/g);          
            if(quote==null){
                p.Face(ct);
                return;
            }
            if(quote.length!=1){
                p.Face(ct);
            }          
            var qt=quote[0];
            var beg_tag = "[quote]";
            var end_tag = "[/quote]";
            var rs=qt.replace(end_tag, "").replace(beg_tag, "").replace("type=","").replace("id=","");
            var type=rs.split(',')[0];
            var id=rs.split(',')[1];
            Sender.handler = p.replyhandler;
            Sender.val = 'op=1&gn='+p.gno+'&rn='+p.rno+'&fn='+p.fno+'&rt='+type+'&rd='+id;   
            Sender.callback=function(result){
                if(result.IsSuccess==1){
                    ct=ct.replace(qt,"");
                    p.Face(result.ReturnString+ct);
                }else{
                    inform.Alert("删除,屏蔽的帖子不能引用");
                }
            }      
            Sender.GetData();
        };
        p.ReplaceQuote=function(type,id,status){
            Sender.handler = p.replyhandler;
            Sender.val = 'op=6&gn='+p.gno+'&rn='+p.rno+'&fn='+p.fno+'&rt='+type+'&rd='+id;   
            Sender.callback=function(result){
                if(result.IsSuccess==1){
                    var ct = $("#txt_content").val();
                     if(ct.split("[quote]").length > 1){
                        inform.Alert("只能引用1个帖子");
                        return false;
                     }   
                     p.Replace("quote","type="+type+",id="+id+"");
                     window.location.hash='a1';
                }else{
                    inform.Alert(result.ReturnString);
                }
            }      
            Sender.GetData();                    
        };
        p.PvImg=function(ct){
            var imgs = ct.match(/\[img\].*?\[\/img\]/g);
            if (imgs == null) {
                return ct;
            }
            for (var i = 0; i < 10; i++) {
                if (i >= imgs.length) {
                    break;
                }
                var beg_tag = "[img]";
                var end_tag = "[/img]";
                var img = imgs[i];
                var url = img.replace(end_tag, "").replace(beg_tag, "");
                var image = '<div class="img_b"><img src="'+url+'"></div>';              
                ct = ct.replace(img, image);
            }
            return ct;
        };
        p.PvVideo=function(ct){
            var videos = ct.match(/\[flash\].*?\[\/flash\]/g);
            if (videos == null) {
                return ct;
            }
            for (var i = 0; i < 5; i++) {
                if (i >= videos.length) {
                    break;
                }
                var beg_tag = "[flash]";
                var end_tag = "[/flash]";
                var video = videos[i];
                var url = video.replace(end_tag, "").replace(beg_tag, "");
                var flash = '<div class="img_b"><img src="http://img02.bbs.tiancitycdn.com/public/no_video.jpg"></div>';              
                ct = ct.replace(video, flash);
            }
            return ct;
        };
        p.ValidatePosts=function() {
            var ct = $("#txt_content").val();
            if (ct.split("[img]").length > 11) {
                inform.Alert("图片最多上传10张");
                return false;
            }
            if (ct.split("[flash]").length > 6) {
                inform.Alert("视频最多上传5个");
                return false;
            }   
            Validator.StartVerify(0, 'txt_title').CheckIsEmpty("请输入帖子标题").CheckByteMinLength(6, "标题太短,长度是3~40中文字,6~80字符！").CheckByteMaxLength(80, "标题太长,长度是3~40中文字,6~80字符！");       
            Validator.StartVerify(0, 'txt_content').CheckIsEmpty("请输入帖子内容").CheckByteMinLength(6, "内容太短,长度是3~5000中文字,6~10000字符！").CheckByteMaxLength(10000, "内容太长,长度3~5000中文字,6~10000字符！");           
            var msg = Validator.Result.Msg;
            if (!Validator.Prompt()) {
                inform.Alert(msg);
                return false;
            }
            return true;
        };
        p.ValidateReply=function() {
            var ct = $("#txt_content").val();
            if(ct.split("[quote]").length > 2){
                inform.Alert("只能引用1个帖子");
                return false;
            }   
            if (ct.split("[img]").length > 11) {
                inform.Alert("图片最多上传10张");
                return false;
            }
            if (ct.split("[flash]").length > 6) {
                inform.Alert("视频最多上传5个");
                return false;
            }           
            Validator.StartVerify(0, 'txt_content').CheckIsEmpty("请输入帖子内容").CheckByteMinLength(6, "内容太短,长度是3~5000中文字,6~10000字符！").CheckByteMaxLength(10000, "内容太长,长度3~5000中文字,6~10000字符！");           
            var msg = Validator.Result.Msg;
            if (!Validator.Prompt()) {
                inform.Alert(msg);
                return false;
            }
            return true;
        };
        /*保存草稿*/
        p.SavaDraft=function(){
             if(p.is_draft){
                var tit= string.Replace(escape($("#txt_title").val()));
                 var ct = string.Replace(escape(p.SaveDefine($("#txt_content").val())));    
                 if(tit==''||ct==''){
                    return;
                 }    
                 Sender.handler = p.handler;
                 Sender.val = 'op=5&gn='+p.gno+'&rn='+p.rno+'&fn='+p.fno+'&te='+tit+'&ct='+ct;   
                 Sender.callback=function(result){
                 }      
                 Sender.OnSubmit();
             }
             
        };
        return p;
    })()
    postedit.Init();
</script>

                    <div class="edit_bt ver_bt">                      
                         

     <script type="text/javascript">
         var validatecode = (function () {
             var c = {};
             c.code='';
             c.isopen=false;
             c.Init = function () {
                 $('#ucValidateCode_ucValidateImage').click(function () {
                     c.Reload();
                 })
             };
             c.GetCode = function () {
                 var code = $('#validatecode').val();
                 if (typeof (code) == 'undefined') {
                     return '';
                 } else {
                     return code;
                 }
             };
             c.Reload = function () {
                 if(!c.isopen){
                    return;
                 }
                 Sender.handler = 'http://newbbs.tiancity.com/Handler/ValidateImageHandler.ashx';
                 Sender.val='';
                 Sender.callback = function (result) {
                     if (result.IsSuccess == 1) {
                         c.code=result.ReturnObject;
                         $('#ucValidateCode_ucValidateImage').attr('src', result.ReturnString + '&t=' + Math.round(Math.random() * 10000));
                     }
                 }
                 Sender.GetData();

             };
             return c;
         })()
         validatecode.Init();
         
     </script>
