        playerManager.createPlayer(param);
      });
    </script><object id="flash" height="100%" width="100%" data-player-playerbody="flash" type="application/x-shockwave-flash" data="http://www.iqiyi.com/common/flashplayer/20170330/1421f98c2359.swf"><param name="quality" value="high"><param name="allowScriptAccess" value="always"><param name="wmode" value="opaque"><param name="align" value="middle"><param name="bgcolor" value="#000000"><param name="swLiveConnect" value="true"><param name="loop" value="true"><param name="play" value="true"><param name="DeviceFont" value="false"><param name="allowFullScreen" value="true"><param name="menu" value="true"><param name="flashVars" value="outsite=false&amp;components=fefff7e6e&amp;albumId=202969401&amp;autoplay=true&amp;isMember=false&amp;cyclePlay=false&amp;exclusive=0&amp;qiyiProduced=0&amp;share_sTime=&amp;share_eTime=&amp;playerUrl=http://www.iqiyi.com/common/flashplayer/20170330/1421f98c2359.swf&amp;flashP2PCoreUrl=http://www.iqiyi.com/common/flashplayer/20170216/15152a1b82aa.swf&amp;preloader=http://www.iqiyi.com/common/flashplayer/20170209/15525aa9a57.swf&amp;vipPreloader=http://www.iqiyi.com/common/flashplayer/20170209/155284b0df09.swf&amp;qiyiProducedPreloader=http://www.iqiyi.com/common/flashplayer/20170209/1552561520f8.swf&amp;exclusivePreloader=http://www.iqiyi.com/common/flashplayer/20170209/155221793893.swf&amp;adurl=http://www.iqiyi.com/common/flashplayer/20170331/036801ea7a2e24.swf&amp;barrageurl=http://www.iqiyi.com/common/flashplayer/20170313/1727c72eeb6.swf&amp;tipdataurl=http://static.iqiyi.com/ext/common/Tipdatavod_201610311735.xml&amp;preloaderTw=http://www.iqiyi.com/common/flashplayer/20161215/1156c9cdf8a4.swf&amp;qiyiProducedPreloaderTw=http://www.iqiyi.com/common/flashplayer/20161122/18235e1d0a85.swf&amp;exclusivePreloaderTw=http://www.iqiyi.com/common/flashplayer/20161122/1823dc809b19.swf&amp;vipPreloaderTw=http://www.iqiyi.com/common/flashplayer/20161122/1823df83b9a5.swf&amp;iconPreloader=http://www.iqiyi.com/common/flashplayer/20161122/1823c314a