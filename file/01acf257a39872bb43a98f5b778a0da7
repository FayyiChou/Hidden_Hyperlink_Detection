  <script type="text/javascript">
    /* 高度：更新策略、vip、活动消息、标题及其他、选集标题 */
    var getHeight = function (dom) {
      var h = 0;
      if(dom && dom.length){
        h += dom.css('paddingTop');
        h += dom.css('paddingBottom');
        h += dom.css('marginTop');
        h += dom.css('marginBottom');
        h += dom.css('borderTop');
        h += dom.css('borderBottom');
        h = parseInt(h, 10);
        h += dom.height();
      }
      return h;
    };
    /* 对应dom */
    var _title = Q('.J_drama_title');
    var _update = Q('.J_drama_update');
    var _vip = Q('.J_drama_vip');
    var _act = Q('.J_drama_activity');
    var _tabtag = Q('.J_drama_tabtag');
    /* 列表 */
    var _tabscroll = Q('.J_drama_scroll');
    /* 当前响应布局 */
    var resp = Q.PageInfo && Q.PageInfo.respInfo || {};
    
    var curlay = resp.curLayout || 0;
    var layout = resp.cnf[curlay] || {};
    /* 右侧总高度 */
    //var allHeight = layout.extraInfo.rightWrapHeight || 417;
    var allHeight = Q.$("#jujiPlayListRight").height(); 
    var titleH = getHeight(_title);
    
    /* 目标高度 */
    var _H = allHeight - titleH;
    
    var updateH = 0;
    if(_update && _update.css('display')!=='none'){
      updateH = getHeight(_update);
    }    
    _H = _H - updateH;
    
    var actH = getHeight(_act);
    _H = _H - actH;
    
    var vipH = 0;
    if(_vip && _vip.css('display')!=='none'){
      vipH = getHeight(_vip);
    }
    _H = _H - vipH;
    
    var tabH = getHeight(_tabtag);
    _H = _H - tabH;
    _H = _H + 'px';
    if(_tabscroll){
      _tabscroll.height(_H);
    }
  </script>
                      </div>
                    </div>                     
                  </div>
                  
 
