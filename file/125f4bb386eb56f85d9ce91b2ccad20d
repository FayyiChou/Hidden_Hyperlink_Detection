<script language=javascript>
$(function(){
	$('#indexstatepic').slides({
		preload: true,
		preloadImage: '',
		play: 8000,
		pause: 2500,
		effect: 'fade',
		hoverPause: true,
		generateNextPrev: false
	});
	var thisSRC = "",tmpSRC = "";
	$("#hoverlinks").find("a").mouseover(function(){
		thisSRC = $(this).attr("rel");
		tmpSRC = $(this).find("img").attr("src");
		$(this).find("img").attr("src",thisSRC).css({"height":"110px","position":"absolute","bottom":"0px"});
		$(".pagination").hide();
		
	});
	$("#hoverlinks").find("a").mouseout(function(){
		$(this).find("img").attr("src",tmpSRC).css({"height":"39px","position":"relative"});
		$(".pagination").show();
	});
});
</script>
