<script type="text/javascript">
$(function(){
	$(".colseservice").bind('click', function(event) {
				$(this).parents(".maincon").hide()
		});
	 var qqlist = [{ "qq": "4006260126", "id": "QQonline-d", "isqiye": true }];
		$.getScript("http://wpa.b.qq.com/cgi/wpa.php", function () {
			$.each(qqlist, function () {
				//有多个企业qq循环执行下面2句代码
				if (this.isqiye) {
					BizQQWPA.add({
						type: '1', //使用按钮类型 WPA
						nameAccount: this.qq, //营销 QQ 号码
						parent: this.id //将 WPA 放置在 ID 为 testAdd 的元素里
					});
				}
			})
	 });
	 $.get('line.ajaxright.html',{},function(result){
		   var str="";
		   var $oConResult=$(".contrast-result");
		   var $oResult=$oConResult.find('.contrast-have');
		   var $oA=$oConResult.find('a');
	       if(result){
	    	 $.each(result,function(){
		         str+='<tr data-id='+this.id+'><td class="t-line-name"><div class="t-line-title"><a href="/line.details.html?dateid='+this.id+'">'+this.title+'</a></div></td><td class="tr"><a href="javascript:;" class="close"><i class="iconfont">&#xe625;</i></a></td></tr>'
		     });
             if(str.length){
                $(".contranst-mid").append(str);
             }else{
                $oResult.removeClass('none');
                $oA.addClass('btn-default');
             }
        };
      },'json');
      $(".contranst-mid").delegate('i', 'click', function(event) {
    	  var result=window.confirm("你确定要删除该项吗?");
    	  var $oTr=$(this).closest('tr');
          var id=$oTr.attr('data-id');
    	    if(result){
			    $oTr.remove();
			    var len=$('.contranst-mid').find('tr').length;
			    if(len==1){
			    	layer.alert("至少两条线路才可以比价")
			    }
			    if(len<=0){
			    	$(".contrast-context").css({display:'none',right:"-999px"});
			    }
			    $.post("line.ajaxdelCookie.html", {id:id}, function (result) {
                    if(result==200){
                        layer.msg("取消成功",1,1);
                    }else{
                        layer.msg(result,1,13);
                    }
                }, "text");
    	    }

      });
      $(".contrast-result").find(".btn-now").on("click",function(e){
      	  var $aTr=$(".contranst-mid").find("tr");
      	  var $li=$(this).parents("li");
       	  if($li.hasClass('user-login-pop')) return;

      	  if($aTr.length<2){
      	  	 e.preventDefault();
      	  	 layer.msg("至少两条线路才可以比价",1,13);
      	  }else{
      	  	 window.location.href="line.contrast.html";
      	  }
      })
      $(".contranst-top").find(".clearall").on("click",function(){
      		var $oTbody=$(this).parent().next().find("tbody");
      		var $li=$(this).parents("li");
       	    if($li.hasClass('user-login-pop')) return;

      	    $.post("line.delAllCookies.html",function (result) {
                 if(result.Status==1){
                     layer.msg("取消成功",1,1);
                     $oTbody.html("");
                     $(".contrast-context").css({display:'none',right:"-999px"});
                 }
            },"json");
      });

      var regEx = /^([\u4E00-\u9FA5\uf900-\ufa2d]+)\|(\w+)\|(\w)\w*$/i;
      var regExChiese = /([\u4E00-\u9FA5\uf900-\ufa2d]+)/;
      var count = 40;

      var wrap;
      $(".logo-site").click(function () {
      	if ($(".sites-wrap").size() == 0) {
      		wrap = $('.logo-site').CitySelector({ top: 20, citys: allCity, regEx: regEx, regExChiese: regExChiese, count: count,defaultUrl:"/index.html?sid="});
      	} else {
      		if ($(".sites-wrap").is(":hidden")) {
      			$(".sites-wrap").show();
      		} else {
      			$(".sites-wrap").hide();
      		}
      	}
      	$(document).unbind("click.site").bind("click.site", function (e) {
      		if (!($(e.target).closest(".sites-wrap").size() > 0 || $(e.target).closest(".logo-site").size() > 0)) {
      			$(".sites-wrap").hide();
      		}
      	})
      })
});
</script>
<script>
 $(function(){
 	$(document).on("keyup", ".frmlogin input[name='username'],.frmlogin input[name='passwd']", function (e) {
		var key = e || event;
		var currkey = key.keyCode || key.charCode;
		if (currkey == "13") {
			if ($.trim($(this).closest("form").find("input[name='username']").val()).length == 0 || $.trim($(this).closest("form").find("input[name='passwd']").val()).length == 0) { return; }
			if ($(this).val().length < 4) { layer.msg('用户名、密码不正确！', 3); return; }
			$(this).closest("form").find(".loginin").trigger("click");
		}
	});
	$(document).on("click",".loginin",function () {
		var form = $(this).closest('form');
		var username = form.find("input[name='username']").val();
		var password = form.find("input[name='passwd']").val();
		var rememberMe = form.find("input[name=rememberPass]").prop('checked');
		if (!username || !password) {
			layer.msg('用户名、密码不能为空！',3);
			return;
		}

		username = encodeURIComponent($.trim(username));
		password = encodeURIComponent($.trim(password));
		var data = {username: username, passwd: password, rememberme:rememberMe};
		$.post("index.ajaxlogin.html", data, function (res) {
			if (res.code != 200) {
				layer.msg(res.message,3);
			} else {
				var path = $("#path").val();
				var loginUrl = $("#loginUrl").val();
				var code = $("#service").val();

				username = encodeURIComponent(username);
				loginUrl = encodeURIComponent(loginUrl);
				code = encodeURIComponent(code);
				var url = path + "?submit=true&username=" + username + "&password=" + password + '&rememberMe=' + rememberMe + "&service=" + code + "&loginUrl=" + loginUrl;
				window.location.href = url;
			}
		}, 'json');
	});
 })
</script>
