        <script>
            $(function(){
                 //所有线路
                 var $oAllLine=$(".allline");
                 for(var i=0;i<$oAllLine.length;i++){
                     var curAll=$oAllLine[i],
                         ali=$(curAll).find('li.line-item');
                     if(ali.length<28){
                        $(curAll).find('li.line-look-more').hide();
                     }
                     for(var j=0;j<ali.length;j++){
                         if(j>=27){
                             $(ali[j]).addClass('last').hide()
                         }
                     }
                 }
            })
        </script>
       
