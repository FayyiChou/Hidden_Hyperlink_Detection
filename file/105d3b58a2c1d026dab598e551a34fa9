  <script type="text/javascript">
function lxfEndtime(){
	$t=$('#t').html();
	if($t!=0){
		$('#t').html($t-1);
		$i=setTimeout("lxfEndtime()",1000);
	}else{		
		$('.box').hide();
		$('#t').html(6);
		clearTimeout($i);
	}
};

$('.box').show();
lxfEndtime();

$('.close').click(function(){
	$('.box').hide();
	$('#t').html(6);
	clearTimeout($i);
})
</script> 
