  <script>
$(function(){
	//换一换
	$('.switch').on('click',function(){
		var $this = $(this);
		var _v = $this.attr('_v');
		if(_v==1){
			$('#would_like_list_2').show();
			$('#would_like_list_1').hide();
			$this.attr('_v',2);
		}else{
			$('#would_like_list_2').hide();
			$('#would_like_list_1').show();
			$this.attr('_v',1);
		}
	});
	
	
	$('.h5-box li').slice(0, 4).show();
	$('.change').on('click',function(){
		$('.h5-box li').slice(4, 8).show(); 
    	$('.h5-box li').slice(0, 4).hide().appendTo('.h5-box');//appendTo追加到后边
	});	
		
})
</script> 
