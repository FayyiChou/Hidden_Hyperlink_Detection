<script type="text/javascript">
function submit_f(){ 
	loginJs.indexs.PopupHintHide('.m-ipt-account');
	loginJs.indexs.PopupHintHide('.m-ipt-password');
	if($("#userName").val().trim()==''){
		loginJs.indexs.accountPopupHint({
			txt : "请输入帐号！"
		});
		return false;
	}
	if($("#password").val()==''){
		loginJs.indexs.passwordPopupHint({
			txt : "请输入密码！"
		});
		return false;
	} 
	$("#loginForm").submit();
}

function qrLogin(){ 
	$("#qrImg").load("/mobileapp/teacher/qrLogin.do");
}
</script>















