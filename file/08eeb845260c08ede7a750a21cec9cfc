   <script type="text/javascript">
        // 左侧咨询的滑出效果
        var jseekMsg = $("#j-seekMsg li");
        jseekMsg.mouseenter (function() {
            var t = $(this),
                tChild = t.find('span');
            if(tChild.length > 0) {
                tChild.stop().show().animate({
                    left: -(tChild.innerWidth() + 2) + "px"
                })
            }
        })
        jseekMsg.mouseleave (function() {
            var t = $(this),
                tChild = t.find('span');
            if(tChild.length > 0) {
                tChild.stop().animate({
                    left: "0px"
                }).hide()
            }
        })
        </script> 
