<!--楼层当前状态-->
<script>
    $(function(){
        var teacherTop = $('#teacher').offset().top;//尚涛老师
        var schoolTop = $('#school').offset().top;//学校介绍
        var zuopinTop = $('#zuopin').offset().top;//作品展示
        var kechengTop = $('#kecheng').offset().top;//课程设置
        var videoTop = $('#video').offset().top;//相册视频
        var hezuoTop = $('#hezuo').offset().top;//合作伙伴
        var pingtaiTop = $('#pingtai').offset().top;//实习平台

        var clientHight = $(window).height();//浏览器可视区高度

        var objTop = $('.top ul li');

        $(window).scroll(function(){
            var f_top = window.pageYOffset || document.documentElement.scrollTop;//滚动高度（变动）
            if((f_top+clientHight-clientHight/2) > teacherTop){
                objTop.eq(0).children('a').addClass('cura');
                objTop.eq(0).siblings().children('a').removeClass('cura');
            }
            if((f_top+clientHight-clientHight/2) > schoolTop){
                objTop.eq(1).children('a').addClass('cura');
                objTop.eq(1).siblings().children('a').removeClass('cura');
            }
            if((f_top+clientHight-clientHight/2) > zuopinTop){
                objTop.eq(2).children('a').addClass('cura');
                objTop.eq(2).siblings().children('a').removeClass('cura');
            }
            if((f_top+clientHight-clientHight/2) > kechengTop){
                objTop.eq(3).children('a').addClass('cura');
                objTop.eq(3).siblings().children('a').removeClass('cura');
            }
            if((f_top+clientHight-clientHight/2) > videoTop){
                objTop.eq(4).children('a').addClass('cura');
                objTop.eq(4).siblings().children('a').removeClass('cura');
            }
            if((f_top+clientHight-clientHight/2) > hezuoTop){
                objTop.eq(5).children('a').addClass('cura');
                objTop.eq(5).siblings().children('a').removeClass('cura');
            }
            if((f_top+clientHight-clientHight/2) > pingtaiTop){
                objTop.eq(6).children('a').addClass('cura');
                objTop.eq(6).siblings().children('a').removeClass('cura');
            }
        })
    })
</script>

<script>
    window.onscroll=function(){
        var teacher = document.getElementById('teacher');
        var top = document.getElementById('top');
        var offsetTop = teacher.getBoundingClientRect().top;
        if(offsetTop<0){
            top.setAttribute('class','top block');
        }else
        if(offsetTop>0){
            top.setAttribute('class','top none ');
        }
    }
</script>
</body>
</html>