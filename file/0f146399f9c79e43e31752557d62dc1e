    }),unslider = unslider06.data('unslider'); $('.unslider-prev').click(function() { var fn = this.className.split(' ')[1]; data06[fn](); });
        var slides = jQuery('.lunbo'), i = 0; slides.on('swipeleft', function(e) { unslider.next(); }).on('swiperight', function(e) { unslider.prev(); });
    });
</script>
