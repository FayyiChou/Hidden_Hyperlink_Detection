<script>
$(function(){
	//浮动
	var floatNav = $(".floatNav");
	var h = $(window).height() / 2 - 120;
	floatNav.css({"top":h});
	$(window).scroll(function(){
		var top = $(this).scrollTop();
		floatNav.stop().animate({"top":top + h}, 300);
	});
	
	var navNum = 0;
	var nav = $(".mainNav");
	var navLi = nav.children();
	navLi.eq(navNum).addClass("current");
	navLi.hover(function(){
		if($(this).find("ul").size() != 0) {
			$(this).addClass("active").find("ul").stop(true).slideDown(300);	
		} else {
			$(this).addClass("current");
		}
	}, function(){
		if($(this).find("ul").size() != 0) {
			$(this).find("ul").stop(true).slideUp(300, function(){
				$(this).parent().removeClass("active");									 
			});	
		} else {
			$(this).removeClass("current");
		}
	});
	nav.mouseout(function(){
		navLi.eq(navNum).addClass("current");				  
	});
}); 
if(screen.width<'640'){
  location.href="http://m.51tsg.com";
}
</script>
<!--浮动链接 end-->

