<script type="text/javascript">
function do_sear()
{
	var v=$.trim($("#sch_key").val());
	if(v.length==0){
		alert("请输入搜索词！");
		$("#sch_key").focus();
		return;
	}else{
		top.location.href='/baidu.aspx?wd='+escape(v);
	}
}
</script>
