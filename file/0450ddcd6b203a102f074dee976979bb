        <script>
            $(document).ready(function (e) {
                $.ajax({
                    type: "get",
                    url: "/api/exb_async_data.ashx",

                    async: false,
                    success: function (msg) {
                        $("#ajax_show_list").html(msg);
                    }
                });

                $(".calendar .date>li.day").each(function (i) {
                    var t = $(this);
                    var d = parseInt(t.attr("rel"));
                    // alert(d);
                    $("#ajax_show_list>li").each(function (i) {
                        var tt = $(this);
                        var st = parseInt(tt.attr("start-time"));
                        var et = parseInt(tt.attr("end-time"));

                        if (d >= st && d <= et) {
                            t.addClass("light");
                        }
                    });
                    var len = $("#ajax_show_list").length;
                });
                $(".calendar .date>li").click(function () {
                    $(this).siblings().removeClass("current");
                    $(this).addClass("current");
                });
            });
			</script>
