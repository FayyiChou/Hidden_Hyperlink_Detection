    function checkSearch(){
	   if ($.trim($('#keyword').val())==''){
		   ShowErrorMsg('请输入你要查询的关键词!');
		   return false;
	   }
	   if ($('#id').val()=='18')
	   {
	       searchForm.action = '/Publication';
	   }
	   if ($('#id').val()=='19')
	   {
	       searchForm.action = '/Video';
	   }
	   searchForm.submit();
	}
</script>
