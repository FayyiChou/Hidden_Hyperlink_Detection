<script>
     function checkLogin(){
		 if ($.trim($('#username').val())==''){
			 ShowErrorMsg('请输入帐号!');
			 return false;
		 }
		 if ($.trim($('#password').val())==''){
			 ShowErrorMsg('请输入密码!');
			 return false;
		 }
		 var options = { 
		     beforeSubmit:  loginRequest,
             success:       loginProcessJson
         };
	     $('#loginForm').ajaxSubmit(options);
	     return false;
	 }
	 function loginRequest(formData, jqForm, options) { 
	     $("#LoginDiv").mask("正在登录，请稍后...");
         return true; 
     } 
     function loginProcessJson(data)  {
	     $("#LoginDiv").unmask();
	     var returnCode = $.evalJSON(data).returnCode;
	     var msg = $.evalJSON(data).returnMessage;
	     if (returnCode==200){
		     document.location='/Index';
	     }
		 else{
		    ShowErrorMsg(msg);
	     }
     }
</script>
