<script language="javascript">
function dosearcha()
{
if($$("keyA").value==''){alert('请输入关键字');return;}
if($$("sel_typeA").value==''){alert('请选择类别');return;}
window.location.href='que_company.aspx?colu='+GetRadioValue("Column")+'&field='+$$("sel_typeA").value+'&key='+URLencode($$("keyA").value);
}

function dosearchb()
{
if($$("keyB").value==''){alert('请输入关键字');return;}
if($$("sel_typeB").value==''){alert('请选择类别');return;}
window.location.href='que_designer.aspx?colu='+GetRadioValue("ColumnB")+'&field='+$$("sel_typeB").value+'&key='+URLencode($$("keyB").value);
}

function dosearchc()
{
	if($$("keyC").value==''){alert('请输入关键字');return;}
	if(GetRadioValue("cateidc")==1)
		window.location.href='que_member.aspx?key='+URLencode($$("keyC").value);
	else
		window.location.href='que_memberteam.aspx?key='+URLencode($$("keyC").value);
}

function $$(obj)
{
 return document.getElementById(obj);
}
function URLencode(sStr) 
{
    return escape(sStr).
             replace(/\+/g, '%2B').
                replace(/\"/g,'%22').
                   replace(/\'/g, '%27').
                     replace(/\//g,'%2F');
}
function GetRadioValue(RadioName){
    var obj;    
    obj=document.getElementsByName(RadioName);
    if(obj!=null){
        var i;
        for(i=0;i<obj.length;i++){
            if(obj[i].checked){
                return obj[i].value;            
            }
        }
    }
    return null;
}
</script>   

