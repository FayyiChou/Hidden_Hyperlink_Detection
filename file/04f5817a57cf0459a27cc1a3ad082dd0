		<script language="javascript">
function userlogin()
{
	if($("uName").value=='')
	{
	alert('请输入用户名.');
		return false;
	}
	if($("uPwd").value=='')
	{
	alert('请输入用密码.');
				return false;
	}
	if($("uCode").value=='')
	{
	alert('请输入验证码.');
		return false;
	}
//document.Form1.submit();
return true;
}
function $(obj)
{
 return document.getElementById(obj);
}
</script>
