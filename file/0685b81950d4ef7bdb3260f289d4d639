        <div id="goToTop"><a href="#">GO TOP</a></div>
        
 <!--=========================================================-->
 
<script type="text/javascript">
    $(document).ready(function(){
        $("#goToTop").hide()
        $(function(){
            $(window).scroll(function(){
                if($(this).scrollTop()>1){
                    $("#goToTop").fadeIn();
                } else {
                    $("#goToTop").fadeOut();
                }
            });
        });
     

        
        $("#goToTop a").click(function(){
            $("html,body").animate({scrollTop:0},800);
            return false;
        });
    });
</script>


    </body>

</html>