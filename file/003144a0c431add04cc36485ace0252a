	<script type="text/javascript">
	function select_vehicleType(vehicle_type, vehicle_id){
		if($(vehicle_id).is(":checked")==true){
			document.getElementById(vehicle_type).value=1;
			document.getElementById(vehicle_type+'_li').value=1;
		}else{
			document.getElementById(vehicle_type).value=0;
			document.getElementById(vehicle_type+'_li').value=0;
		}
	}
	</script>
