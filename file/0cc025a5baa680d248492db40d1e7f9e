    <script>
$(function(){
	//if(document.getElementById("zx").getElementsByTagName("li").length > 7){
		var _rTop = $("#right_compate_box").offset().top; 
		$(window).scroll(function(){

		　　var scrollTop = $(this).scrollTop();
		　　var scrollHeight = $(document).height();
		　　var windowHeight = $(this).height();


			if ($(this).scrollTop() >= _rTop && scrollTop + windowHeight != scrollHeight){
				$("#right_compate_box").addClass("fixed");
			}
			else{
				$("#right_compate_box").removeClass("fixed");
			}


		});
	//}
});
</script> 
