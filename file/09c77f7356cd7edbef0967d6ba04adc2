            <script>
                var type="g";
                $('li[nctype="selectlist"]').click(function(){
                    selectlist($(this));
                });
                function selectlist($this){
                    var data_str = '';
                    eval('data_str = ' + $this.attr('data-param'));
                    type=data_str.type;
                    $('#selectlist').html(data_str.name);
                }
                $('a[nctype="ibtnsearch"]').click(function(){
                    var txts=$('#txtS').val();
                    if(txts.length>0){
                        if(type=="h"){
                            location.href="/TradeNewsAll?S="+txts;
                        }else if(type=="g"){
                            location.href="/NewsALl?S="+txts;
                        }else if(type=="q"){
                            location.href="/WorkNewsAll?S="+txts;
                        }else if(type=="p"){
                            location.href="/Training?S="+txts;
                        }else if(type=="z"){
                            location.href="/QualifiedAgent?S="+txts;
                        }
                    }
                });
