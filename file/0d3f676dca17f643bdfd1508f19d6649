        var par=$(this).parent();
        if(par.hasClass("cur")) return false;
        var myindex=par.index();
        $(".zhiye .tabcontent:eq("+zhiyeindex+")").find(".group.cur").removeClass("cur");
        par.addClass("cur");
        $(".zhiye .tabcontent:eq("+zhiyeindex+")").find(".group .info").hide();
        var a = $(".zhiye .tabcontent:eq("+zhiyeindex+")").find(".group .info:eq("+myindex+")");
