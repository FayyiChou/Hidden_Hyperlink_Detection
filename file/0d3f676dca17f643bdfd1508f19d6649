        return false;
      });

      var sl=6;
      var snowpos=0;
      var soriginh=81;
      $(".samllInfo .topbtn").click(function(){
        if(snowpos!=0){
          snowpos--;
        }else{
          snowpos=sl-3;
        }
        $(".samllInfo ul").animate({"top":-(snowpos*soriginh)},500,"linear");
      });
      $(".samllInfo .bottombtn").click(function(){
        if(snowpos<sl-3){
          snowpos++;
        }else{
          snowpos=0;
        }
        $(".samllInfo ul").animate({"top":-(snowpos*soriginh)},500,"linear");
      });
  });
