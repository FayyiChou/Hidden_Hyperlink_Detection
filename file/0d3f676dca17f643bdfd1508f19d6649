        if(myindex==0){
          $(".se2 .more1 grow").attr("href","/exp/");
        }else{
          $(".se2 .more1 grow").attr("href","/pvp/");
        }
        $(".se2 .tabcontent").hide();
        $(".se2 .tabcontent:eq("+myindex+")").show();
