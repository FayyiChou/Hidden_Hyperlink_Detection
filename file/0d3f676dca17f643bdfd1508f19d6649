    if(item.length>0){
    for(var i = 0; i < item.length / 2; i++) {
    html += '<a href="' + item.eq(i).attr('href') + '">' + item.eq(i + hf).html() + '</a>';
    }
    g.parent().after(html).parent().show();
    //clearInterval(chan);
    }
    
    }
    }
