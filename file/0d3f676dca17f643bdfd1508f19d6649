    <script>
    function CreatePageGuide() {
      var g = $(".guide .ContnetPageGuide");
      if(g.html() != null) {
        var item = g.find('a');
        var html = "";
        var hf = (item.length + 1) / 2;
        for(var i = 0; i < item.length / 2; i++) {
          html += "<li class=\"n\"><a href=\"" + item.eq(i).attr('href') + "\">" + item.eq(i).html() + "</a></li>";
          if(i + hf < item.length) html += "<li><a href=\"" + item.eq(i + hf).attr('href') + "\">" + item.eq(i + hf).html() + "</a></li>";
        }
        html = "<ul>" + html + "</ul><div class=\"clearfix\"></div>";
        html = "<div class=\"l\">" + html + "</div>";
        html = "<div class=\"t f14\">本文导航</div>" + html;
        g.parent().after(html).parent().show();
      }
    }
    </script>
    <script> var pm=1;
    function Getpm(){
    var html='';
    if(pm>4){
    html='<span class="numbg4">'+pm+'</span>';
    }else{
    html='<span class="numbg'+pm+'">'+pm+'</span>';
    }
    pm++;
    return html;
    }
    function Getpm2(){
    var html='';
    if(pm<3){
    html=' | ';
    }
    pm++;
    return html;
    }
    function CreateKa_ol() {
    //if ($("#body .appe .ka").length == 0) document.write('<div class="con_all_bor_red css3" id="ol-ka"></div><div class="clear10"></div>');
    var root = $("#ol-ka").show();
    $.ajax({
    url: "http://ka.tgbus.com/api/getactivity.ashx?count=5&width=136&height=84&callback=?",
    type: 'GET',
    dataType: 'jsonp',
    cache: false,
    scriptCharset: 'utf-8',
    success: function(data) {
    if (data.length == 0) return;
    root.append('<table class="t1"><tr class="tr1"><th>游戏名称</th><th>帐号说明</th><th>测试时间</th><th>领号</th></tr><tbody></tbody></table>');
    var img_html = root.find("ul"); var html = root.find("tbody");
    $.each(data, function(i, n) {
    var ts = (n.TestTime.parseDate() - new Date()) / 86400000;
    ts = ts < 0 ? "进行中" : ts;
    ts = ts < 1 ? "今天" : ts;
    ts = ts < 2 ? "明天" : ts;
    ts = ts < 3 ? "后天" : ts;
    ts = ts >= 3 ? n.TestTime.parseDate().format('MM-dd') : ts;
    var t;
    var c = "yd";
    switch (n.ActivityCardState) {
    case "Tao":
    c = "th";
    t = "淘号";
    break;
    case "Grant":
    t = "发放中";
    break;
    case "Reserve":
    t = "预定";
    break;
    default:
    t = "即将开始";
    break;
    }
    var temp = ''
    temp = '<tr><td>$7</td><td class="red" title="$3">$8</td><td>$4</td><td><a href="$1" target="_blank">$6</a></td></tr>';
    temp = String.Format(temp, n.Url, n.GameName, n.Explain, ts, c, t,cutstr(n.GameName,4),cutstr(n.Explain,4));
    html.append(temp);
    });
    }
    });
    }
    function cutstr(str, len)
    {
    var str_length = 0;
    var str_len = 0;
    str_cut = new String();
    str_len = str.length;
    for (var i = 0; i < str_len; i++)
    {
    a = str.charAt(i);
    str_length++;
    if (escape(a).length > 8)
    {
    //中文字符的长度经编码之后大于8
    str_length++;
    }
    str_cut = str_cut.concat(a);
    if (str_length >= len)
    {
    str_cut = str_cut.concat("...");
    return str_cut;
    }
    }
    //如果给定字符串小于指定长度，则返回源字符串
    if(str_length < len)
    {
    return str;
    }
    }
    function CreateListPager(){
    //<b><font color="red">[1]</font>&nbsp;<a href="http://ol.tgbus.com/cccccccccc/201205/1000755_2.shtml">下一页</a></b>
    var pager = $('#pager');
    var pl = $('#body .contentpager');
    var html = '';
    var sty="";
    var tmp = pl.find("a:contains('上一页')");
    var stmp = pl.find("a:contains('首页')");
    //if(tmp.html() == null) html += '<li style="width:28px;" class="thisclass">首页</li>';
    //else html += '<li style="width:48px;"><a href="' + stmp.attr('href') + '">首页</a><li><li style="width:48px;"><a href="' + tmp.attr('href') + '">上页</a><li>';
	if(tmp.html() != null) html += '<a href="' + tmp.attr('href') + '"><</a>';
    tmp = pl.find("a, font");
    tmp.each(function(i, n){
    var p = n.innerHTML;
    if(p.match(/\d+/ig)) {
    if(typeof(n.href) == 'undefined') html += '<a class="cur" href="javascript:;" target="_self">' + p.replace("[","").replace("]","") + '</a>';
    else html += '<a href="' + n.href + '" target="_self">' + p.replace("[","").replace("]","") + '</a>';
    }
    });
    tmp = pl.find("a:contains('下一页')");
    stmp = pl.find("a:contains('尾页')");
    //if(tmp.html() == null) html += '<li style="width:28px;"  class="thisclass">尾页</li>';
    //else html += '<li style="width:48px;"><a href="' + tmp.attr('href') + '" >下页</a></li><li style="width:48px;"><a href="' + stmp.attr('href') + '" >尾页</a></li>';
    if(tmp.html() != null) html += '<a href="' + tmp.attr('href') + '" target="_self">></a>';
    pager.html(html);
    document.onkeydown = function(event) {
    var event = event || window.event;
    var url = '';
    if(event.keyCode == 39) url = pager.find("a:contains('下页')").attr('href');
    if(event.keyCode == 37) url = pager.find("a:contains('上页')").attr('href');
    if(typeof(url) != 'undefined' && url != '') window.location.href = url;
    }
    }
    //关键词
    function createEliteWord() {
    document.write($('#eliteword').html());
    }
    function createCorrelative() {
    var ct = $("#correlative_new");
    if (ct.length > 0) {
    var correArr = new Array();
    var homeArr = new Array();
    var count = 0;
    $.each($('#correlative_new a'), function (i, n) {
    //var t = n.title.split('更新：')[1].split('/');
    var t = n.title.split('更新：')[1];
    var split_f = '-';
    if (t.indexOf('-') == -1) {
    split_f = '/';
    }
    var t = n.title.split('更新：')[1].split(split_f);
    var url = window.location.href;
    
    n.createTime = new Date(parseInt(t[0]), parseInt(t[1]) - 1, parseInt(t[2]));
    if (url.indexOf("_") > -1) { url = url.replace(/(_\d+)/g, ""); }
    if (homeArr.indexOf(n.href) == -1 && n.href != url) {
    correArr.push(n);
    homeArr.push(n.href);
    };
    });
    correArr.sort(function sortdate(a, b) { return b.createTime - a.createTime; });
    $.each(correArr, function (i, n) {
    if (i < 6) {
    document.writeln('<li><a href="' + n.href + '">' + n.innerHTML + '</a>&nbsp;&nbsp;&nbsp;<span class="datestr data">' + n.createTime.format('yyyy-MM-dd') + '</span>&nbsp;&nbsp;</li>');
    }
    })
    }
    else {
    var value, time, max;
    var clist = document.getElementById('correlative').getElementsByTagName('a');
    max = clist.length;
    if (max > 6) max = 6;
    for (var i = 0; i < max; i++) {
    value = clist[i];
    document.writeln('<li><span class="txt_666 font_12">' + clist[i].title.split('更新：')[1].split(' ')[0] + '</span><a href=\"' + clist[i].href + '\" target=\"_blank\">' + clist[i].innerHTML + '</a></li>');
    }
    }
    }
    </script>
