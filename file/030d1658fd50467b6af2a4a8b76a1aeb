    function getCookieV2(name) {
        var reg = new RegExp("(^| )" + name + "(?:=([^;]*))?(;|$)"),
            val = document.cookie.match(reg);
        return val ? (val[2] ? decodeURIComponent(val[2]) : "") : null;
    }

