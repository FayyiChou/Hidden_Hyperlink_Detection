<script>
function cmtCount(data) {
var url, total;
if (data.total) {
total = data.total;
url = data.url;
target="_blank";
} else {
total = 0;
url = '#comment';
target="_self";
}
var HTML = '';
HTML = '<a href="' + url + '" target="'+target+'">评论<em class="highlight">' + total + '</em>条</a>';
document.getElementById("cmtCount").innerHTML = HTML;
};
</script>
