<script type="text/javascript">

$(function(){
	$('.nav li').mouseover(function(){
		//$(this).siblings().find("ul").hide(); 
		$(this).find("ul").show(); 
		$(this).addClass("nav_on"); 
	})
	$('.nav li').mouseout(function(){
	$(this).find("ul").hide(); $(this).removeClass("nav_on"); 	 
	})
})


 
</script>



