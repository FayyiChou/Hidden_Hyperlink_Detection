					$(document).ready(function(){
						
						$('.demo7').collapser({
							mode: 'block',
							target: 'next',
							effect: 'slide'
						});
						$('.fancybox').fancybox();
					});
					function showText(key, sType,src) {
						var id = "#pic" + key;
						if (sType == 'hide') { 
							$(id).hide();
							return;
						} 
						
						if($(id).size() == 0){
							var tbCopy = $("#textBox").clone();
							tbCopy.find("iframe").attr("src",src);
							tbCopy.attr("id",id.replace("#",""));
							$("#mainBox").append(tbCopy);
						}
						$(id).show();
					}
					</script>
