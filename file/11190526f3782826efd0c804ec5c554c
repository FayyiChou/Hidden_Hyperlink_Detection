<script type="text/javascript">

function txtInputHint(id){
	var txtInput = $("#" + id);
	if(txtInput.val() == ""){
		txtInput.val(txtInput.attr("initValue"));
	}
	txtPromptFocus(id,txtInput.attr("initValue"));
	txtPromptBlur(id,txtInput.attr("initValue"));
	txtInput.trigger("blur");
}
var txtPromptFocus = function(id,txt){
	var obj = $("#" +id);
	obj.bind("focus",function(){
		if(obj.val() == txt){
			obj.val("");
		}
		obj.css("color","black");
	});
};
var txtPromptBlur = function(id,txt){
	var obj = $("#" + id);
	obj.bind("blur", function(){
		if($.trim(obj.val()) == "" || $.trim(obj.val()) == txt){
			obj.val(txt);
			obj.css("color","#acacac");
		}else{
			obj.css("color","black");
		}
	});
};
var txtPromptRelateHidden = function(id){
	var inputs = $(":input[type='text']");
	$.each(inputs,function(i,o){
		if($(o).attr("initValue") != ""){
			var relateInput=$("#" + $(o).attr("id") + "_hidden");
			if($(relateInput) != null){
				if($(o).val() != $(o).attr("initValue")){
					relateInput.val($(o).val());
				}else{
					relateInput.val("");
				}
			}
		}
	});
};
function toProduct(obj){
	if(obj.value == "")
		return;
	window.location.href = "/product.jsp?pCode=" + obj.value;
}
</script>
