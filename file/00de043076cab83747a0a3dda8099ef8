	<script type="text/javascript">


		jQuery(document).ready(function() {
		     if($.cookie("CERP_LESSASS_D")!=undefined&&$.cookie("CERP_LESSASS_C")!=undefined){

				/* cookie中有值时，选中记住密码checkbox,给用户名和密码赋上值 */
				if(($.cookie("CERP_LESSASS_D")!='null')&&($.cookie("CERP_LESSASS_C")!='null')){
					
					if($.base64.atob($.cookie("CERP_LESSASS_D"))=='1'){
					
						$('#nagaLoginUrl').hide();
						$('#registerUrl').hide();
						$('#not_login_sep').hide();
						$('#registerQuick').hide();
						$('#nagaPersonUrl').show();
						$('#nagaPersonUrl').find('a').html($.cookie("CERP_LESSASS_C"));
						$('#logout').show();
						$('#login_sep').show();
					}
				}
			 }else{
			 	var href=$('#nagaLoginUrl').find('a').attr('href');
			 	if(typeof(href)!='undefined'&&href.length>0){
			 		href=href.substr(0,href.lastIndexOf('/'));
			 		href=href+'/getName';
			 	
				 	$.ajax({
						type : "get",
						async:false,
						url : href,
						dataType : "jsonp",
						jsonp: "callback",
						jsonpCallback:"callback",
						success : function(json){
							if(typeof(json)!='undefined'){
									$('#nagaLoginUrl').hide();
									$('#registerUrl').hide();
									$('#not_login_sep').hide();
									$('#registerQuick').hide();
									$('#nagaPersonUrl').show();
									$('#nagaPersonUrl').find('a').html(json.name);
									$('#logout').show();
									$('#login_sep').show();
								}
						},
						error:function(){
							//console.log('fail');
						}
					});
			 	}
			 }
			
		});
		
		 $('#applyForTestroom').click(function(){
                        window.location.href="http://order.roshan.top/newOrgBig/"+encodeURIComponent("1543725050@qq.com"); 
          });
		
	</script>
	
	<script type="text/javascript">
 	
 	
 	var CookieValues={
 			 noticeFlag:$.cookie("noticeFlag"),
 		 	 u:""
 	};
 	
 	$(document).ready(function() {
 		
 		//初始时(第一次进入或者未点击关闭按钮)
 		if (isStartInterval()) {
 			startrequest();

 		}
 		
 		//页面初始化时未显示
		if (typeof (CookieValues.noticeFlag)=="undefined" ){
			CookieValues.u = setInterval("startrequest()", 1000*60*5);
		}
		
		$('#close').click(function(){
			console.log("close");
			setCookie("noticeFlag","0");
			CookieValues.noticeFlag='0';

		}); 
		
 		
	});

 
	function startrequest() {


		$.ajax({
			url : "https://kaowu.etest.net.cn/notice/get?callback=?",
			dataType: 'jsonp',  
			async:false,
			success : function(data) {
				if (data.success) {
					
					setCookie("noticeFlag","1");
					CookieValues.noticeFlag='1';
					$('#alterConetntDiv').toggle("150");
					$('#alterConetnts').html(data.msg);
					clearInterval(CookieValues.u);
				}
			}
		});
	}
	
	//设置cookie
	function setCookie(key,value){
		var date = new Date();
	  	date.setTime(date.getTime() + (1 * 20 *60 * 1000));  
      	$.cookie(key,value,{expires:date,path:'/',domain:"etest.net.cn"})
	}

	//判断是否需要启动定时器
	function isStartInterval(){
		
		//初始时
		if (typeof (CookieValues.noticeFlag)=="undefined" ){
			return true;
		}
		
		//选择了关闭
		if (CookieValues.noticeFlag != "0" ){
			return true;
		}
		
		return false;
		
	}
	
 </script> 

    
	
