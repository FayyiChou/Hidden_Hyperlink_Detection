<script type="text/javascript">
var mydiv;

$(function(){
	mydiv = $('#demobox');
	var clearbox = setInterval("funDemoBox()",10000);
	$('#demoboxclose').live('click',function(){
		mydiv.hide();
		clearInterval(clearbox)
		clearbox = setInterval("funDemoBox()",10000);
	});
})

function funDemoBox(){	
	if(mydiv.is(":hidden")){
		mydiv.show();
	}else {
		mydiv.hide();
	}
}
</script>

