<script type="text/javascript">
  function PVTrack() {
  this.site=0;this.category=0;this.subcategory=0;this.url="";this.series=0;this.spec=0;this.type=0;this.typeid=0;this.object=0;this.bcTypeId="";this.src="";
  this.init = function() {
  this.src="http://photo1.pv.autohome.com.cn/pv_count_non3.php?siteid="+this.site;this.src+=this.category>0?"&categoryid="+this.category:"";this.src+=this.subcategory>0?"&subcategoryid="+this.subcategory:"";this.src+=this.series>0?"&seriesid="+this.series:"";this.src+=this.spec>0?"&specid="+this.spec:"";this.src+=this.type>0?"&type="+this.type:"";this.src+=this.typeid>0?"&typeid="+this.typeid:"";this.src+=this.object>0?"&objectid="+this.object:"";this.src+=this.bcTypeId!=null?"&bcTypeId="+this.bcTypeId:"";this.src+="&cur="+escape(document.URL);
  }
  this.send=function(){var c=new Image(1,1);c.onLoad=function(){};c.src=this.src};
  this.track=function(){this.init();this.send()}
  }
</script>  
