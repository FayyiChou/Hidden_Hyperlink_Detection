function gbcount(message,used,max) 
{
	var len=jc.util.getStrlen($("#"+message).val());
	if (len > max) 
	{
		var str = jc.util.substr($("#"+message).val(),max);
		$("#"+message).val(str);
		return false;
	}
	else 
	{ 
		$("#"+used).text((max - len)); 
	} 
	if(len == 0){
		return false;
	}
} 

//根据名称获取对应的代表工程
function subWeibo(){
	$("#subweibo").removeAttr('onclick');
	var content=$("#contents").val();
	var img=$("#weiboimg").attr('src');
	var url=document.URL;
	if(!content||!url){
		return ;
	}
	strname={};
	strname['url']=url;
	strname['content']=content
	strname['img']=img;
	$.ajax({
		type:"post",
		url:"/baseproject/rebroadcast",
		cache:false,
		data:strname,
		datatype:"json",
		success:callbackWeibo
	});		
}
//回调地址
function callbackWeibo(msg){
		var info=eval('('+msg+')');
		if(info.errNo==0){
			tb_remove();
		}else{
			$("#subweibo").attr('onclick','subWeibo()');
			$("#comment_error_message").html('转发微博失败');
		}
}

/**
 *	评分按钮点击事件
 */
function productMarkScore(){
	var ret;
	var productId=4219056;
	$.ajax({
		type: "GET",
		url: '/ajax/getProductScoreByUid',
		data: {productId:productId},
		async: false,
		success: function(data){
			data = eval('('+data+')');
			ret = data;
		}
	});
	if(ret.result>0){
		reviewFailed('您已评过分！');
	}else{
		$(".zhul_jer_usedBox").show();
	}
	
}
/**
 *	评分按钮点击事件
 */
function productMarkScoreSelect(obj){
	var title = $(obj).attr('title');
	if(title == '1分'){
		$("#outter li").css('background-position','left top');
		//$('.zhul_used_fenshu').text('1分 不好');
		$('#productScore').val('1');
	}else if(title == '2分'){
		$("#outter li").eq(1).css('background-position','left top');
		$("#outter li").eq(0).unbind('mouseout');
		//$('.zhul_used_fenshu').text('2分 一般');
		$('#productScore').val('2');
	}else{
		$("#outter li").eq(1).unbind('mouseout');
		//$('.zhul_used_fenshu').text('3分 很好');
		$('#productScore').val('3');
	}
}
/**
 *	评分提交
 */
function submitProductScore(productId){
	var score = $('#productScore').val();
	var isSendWeibo = 0;
	if($("#isSendWeibo").attr("checked")==true){
		isSendWeibo = 1;
	}
	var rand = Math.random();
	$.ajax({
		type: "GET",
		url: '/ajax/createScore',
		data: {productId:productId,score:score,isSendWeibo:isSendWeibo,rand:rand},
		success: function(data){
			data = eval('('+data+')');
			if(data.errNo==0){
				$(".zhul_jer_usedBox").hide();
				reviewSuccess('评分成功');
				var markedCount = $('#markedCount').text();
				markedCount++;
				$('#markedCount').text(markedCount);
			}else if(data.errNo == 2440){
				reviewFailed('您已评分,不能重复提交');
				$(".zhul_jer_usedBox").hide();
			}else{
				reviewFailed('提交失败！');
			}
		}
	});
}
(function(){
	var obj = $('#outter li');
	obj.eq(0).css('background-position','left -21px');
		obj.eq(1).css('background-position','left -21px');
	
	obj.eq(0).bind('mouseover',function(){
		obj.eq(0).css('background-position','left -21px');
		obj.eq(1).css('background-position','left top');
		$('.zhul_used_fenshu').text('2分 一般');
		
	}).bind('mouseout',function(){
		obj.eq(0).css('background-position','left -21px');
		obj.eq(1).css('background-position','left -21px');
		$('.zhul_used_fenshu').text('');
	})
	
	obj.eq(1).bind('mouseover',function(){
		obj.eq(0).css('background-position','left -21px');
		obj.eq(1).css('background-position','left -21px');
		$('.zhul_used_fenshu').text('3分 很好');
		
	}).bind('mouseout',function(){
		obj.eq(0).css('background-position','left -21px');
		obj.eq(1).css('background-position','left -21px');
		$('.zhul_used_fenshu').text('');
	})
	
	
	$('.zhul_user_star1 a').bind('mouseover',function(){
		obj.eq(0).css('background-position','left top');
		obj.eq(1).css('background-position','left top');
		$('.zhul_used_fenshu').text('1分 不好');
		
	}).bind('mouseout',function(){
		obj.eq(0).css('background-position','left -21px');
		obj.eq(1).css('background-position','left -21px');
		$('.zhul_used_fenshu').text('');
	})
	//获得产品评分人数
	$.ajax({
		type: "GET",
		url: '/ajax/getMarkedCount',
		data: {productId:4219056},
		success: function(data){
			data = eval('('+data+')');
			if(data.errNo==0){
				$('#markedCount').text(data.result);
			}
		}
	});
	
})()
