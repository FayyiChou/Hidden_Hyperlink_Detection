/**
*   转播弹框
*	content		转播内容
*	imgArray	图片url，数组
	coverId 	封面图的id
*/
function sendWeibo(contentId,coverId){
	tb_remove();
	var content = jc.util.substr($('#'+contentId).text(),97);         
	var contentHmtl= $("#"+contentId).html();   
	var parttern = /http:\/\/(?:attach|jcimg)\.zhulong.com\/[^\"\'\<\>]+/mi;
	//匹配封面图
	var match = parttern.exec($("#"+coverId).html());
	var url=document.URL;
	var arrImg = new Array();
	if(match){
		arrImg.push(match);
	}
    var divid = "show_relayBox";
    if (!document.getElementById(divid)) {
        var createDiv = document.createElement("div");
        createDiv.style.display = "none";
        createDiv.id = divid;
        document.body.appendChild(createDiv);
    }
    var box = document.getElementById(divid);
    var h = '';
	h += '	<div class="zhul_zb_content">';
	h += '		<span class="zhul_qyzx_zishu">还可以输入&nbsp;<span class="zhul_qyzx_shuzi" id="word">140</span>&nbsp;字</span>';
    h += '		<textarea class="zhul_zb_text" id="contents" onkeydown='+'gbcount("contents","word",140)'+'>'+content+'</textarea>';
