<script>
    //幻灯
    function switchbanner(elementID,elementClass,play_time){
        var n=0;
        var _div=$("."+elementID+">ul");
        var div_imglength=$("."+elementID+" li").length;
        if(div_imglength<=1||div_imglength>9){return false;}
        var temp_time;
        addnumber(div_imglength);
        play_img(n);
        _div.parent().hover(
                function(){_div.stop(true,true);clearTimeout(temp_time)},
                function(){temp_time=setTimeout(function(){play_img(n)},play_time);}
        )
        $(".number span").click(
                function(){_div.delay(500).stop(true,true);
                    n=$(".number span").index(this);
                    play_img(n);
                    clearTimeout(temp_time);
                }
        )
        function play_img(_n){
            if(temp_time){clearTimeout(temp_time);}
            update_location(_n);
            _div.find("li").eq(_n).css("z-index",10).fadeIn(600).siblings().css("z-index",5).fadeOut(400);
            _n++;
            if(_n==div_imglength){_n=0;}
            n=_n;
            temp_time=setTimeout(function(){play_img(_n)},play_time);
        }
        function addnumber(count){
            _html='';
            for(var j=0;j<count;j++){
                _html+='<span>'+(j+1)+'</span>'
            }
            $(".number").html(_html);
        }
        function update_location(_n){
            $(".number span").eq(_n).addClass(elementClass).siblings().removeClass(elementClass);
        }
    }

    /*
     *  图片轮播效果
     *  num：一屏显示个数, speed:速度(毫秒),fun:回调方法
     *  ImgScroll('.img-scroll',1,6000,fn);
     */
    function ImgScroll(wraper,num,speed,fn){
        var wraper = $(wraper);
        var prev = wraper.find(".prev");
        var next = wraper.find(".next");
        var list = wraper.find(".img-list").find("ul");
        var w = list.find('li').outerWidth(true);
        var s = speed;
        if(num > list.find('li').length) return;

        next.click(function(){
            list.stop(true,true).animate({'margin-left':-w},function(){
                list.stop(true,true).find('li').eq(0).appendTo(list);
                list.stop(true,true).css({'margin-left':0});
                if (fn) {
                    fn(list.find('li').eq(0));
                };
            });
        });
        prev.click(function(){
            list.stop(true,true).find('li:last').prependTo(list);
            list.stop(true,true).css({'margin-left':-w});
            list.stop(true,true).animate({'margin-left':0},function(){
                if (fn) {
                    fn(list.find('li').eq(0));
                };
            });
        });
        if (speed){
            ad = setInterval(function() {
                next.click();
            },s);
            wraper.hover(function(){clearInterval(ad);},function(){
                ad = setInterval(function() { next.click();},s);
            });
        }
    }

    //攻略
    $("#ys-gl-list li").click(function(){
        var _this = $(this);
        $("#ys-gl-list").find(".active").removeClass("active").find("p").slideUp(function(){
            _this.addClass("active").find("p").slideDown();
        });
    })

    switchbanner("ys-silder","active",3000);
    ImgScroll('.img-scroll',1,6000);
</script>
