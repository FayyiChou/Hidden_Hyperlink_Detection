       <script language="javascript" type="text/javascript">
           //用户名判断
           function txtNameCheck() {
               var txtName = $("#txtName").val();
               //判断用户名为能为空
               //alert(txtName);
               if (txtName == "请输入您的名字或昵称") {
                   //alert("内容呀");
                   $("#txtNameTips").text("*请填写用户名。");
                   $("#tipName1").html("<em>*</em> 请输入名字或昵称");
                   $("#tipName2").html("<em>*</em> 请输入名字或昵称");
                   $("#tipName2").css("display", "none");
                   $("#tipMobile1").css("display", "none");
                   return false;
               }
               else {
                   $("#tipName1").text("");
                   $("#tipName2").text("");
               }
           }


           //验证手机号码的正确性
           function txtMobileValCheck() {
               if ($.trim($("#txtMobile").val()) == "请输入11位数字手机号码") {

                   $("#tipMobile1").html("<em>*</em> 请输入11位数字手机号码");
                   $("#tipMobile2").html("<em>*</em> 请输入11位数字手机号码");

                   $("#tipMobile1").css("display", "none");
                   $("#tipMobile2").css("display", "block");
 

               }
               else {

                   //如果不为空验证正确性

                   var reg = /^1[3|4|5|7|8]\d{9}$/;
                   if (!reg.test($.trim($("#txtMobile").val()))) {
                       //alert("手机不正确");
                       $("#tipMobile1").html("<em>X</em> 您输入的格式有误");
                       $("#tipMobile2").html("<em>X</em> 您输入的格式有误");
                       $("#tipMobile1").css("display", "none");
                       $("#tipMobile2").css("display", "block");
                   }
                   else {
                       $("#tipMobile1").html("");
                       $("#tipMobile2").html("");
                   }

               }
           }


           //光标离开验证
           $(function () {
               //判断用户名文本框
               $("#txtName").blur(function () {
                   txtNameCheck();
               })
 

               //判断手机号码
               $("#txtMobile").blur(function () {
                   txtMobileValCheck();
               })

           })




           //提交确认框.text()
           function SubmitConfirm() {
               //用户名判断
               txtNameCheck();

               txtMobileValCheck();

               //alert("刚到这儿"); 
               //以下判断是否可以提交
               //alert($("#tipName1").text()); 
               if ($("#tipName1").text() != "") {
                   return false;
               }
               //alert($("#tipMobile1").text()); 
               if ($("#tipMobile1").text() != "") {
                   return false;
               }

               $("#layer_submit").css("display","block");
           }
       </script>
		<tr>
