<script type="text/javascript">
    $(window).scroll(function () {
        var uu = 200;
        var h0 = $("#focusImg-box-0").height();
        var windowH = $(window).scrollTop();
        if (windowH < h0 - uu) { $(".navbar li a").removeClass("cur"); }
        else if (windowH >= h0 - uu && windowH < h0 * 2 - uu) { $(".navbar li a").removeClass("cur"); $("a[href='#focusImg-box-1']").addClass("cur"); }
        else if (windowH >= h0 * 2 - uu && windowH < h0 * 3 - uu) { $(".navbar li a").removeClass("cur"); $("a[href='#focusImg-box-2']").addClass("cur"); }
        else if (windowH >= h0 * 3 - uu && windowH < h0 * 4 - uu) { $(".navbar li a").removeClass("cur"); $("a[href='#focusImg-box-3']").addClass("cur"); }
        if (windowH >= h0 * 4 - uu) { $(".navbar li a").removeClass("cur"); }

    });

</script>

