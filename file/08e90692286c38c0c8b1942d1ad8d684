	   <script type="text/javascript">
			(function($){
				$('#gtype').selectbox();
				if($('#search .selectbox').val() != '球场')
				{
                                        $("#keyword").css("width", '328px');
					$('#region').addClass('hide').hide();
					$('#play_time').hide();
				}
				else
				{
                                        $("#keyword").css("width", '137px');
					$('#region').removeClass('hide').show();
					$('#play_time').show();
				}
				$('#cityarea .pop-close').click(function(){
                                    $("#region").html('全国');
					$('#cityarea').hide();
				});
				$('#region').click(function(){
					$('#cityarea').show();
				});
				$('#cityarea .search-cities ul').hide().eq($('#cityarea .search-cities-tabs a').index($('#cityarea .search-cities-tabs a.current')[0])).show();
				$('#cityarea .search-cities-tabs a').each(function(i){					
					$(this).click(function(){
						$('#cityarea .search-cities-tabs a').removeClass('current');
						$(this).addClass('current');
						$('#cityarea .search-cities ul').hide().eq(i).show();
					});
				});
				
				$('#cityarea .search-cities a').each(function(i){					
					$(this).click(function(){
						$('#region').html($(this).html());
						$('#cityarea').hide();
					});
				});
				
				$('body').bind('click', function(evt){					
					if($(evt.target).attr('id') !='region' && $(evt.target).parents('#cityarea').length == 0)
					{
						$('#cityarea').hide();
					}
				});
				$('#play_time').click(function(){
					$('#cityarea').hide();				
				});
			})(jQuery)
	   </script>
	</div>
	
    
