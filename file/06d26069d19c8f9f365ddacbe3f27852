	var objHeader = document.getElementById('header');
	if(topNav && objHeader)
	{
		function topFixed(){
			var sheight=document.getElementById('header').offsetHeight;
			document.documentElement.scrollTop+document.body.scrollTop>sheight?topNav.style.display="block":topNav.style.display="none"
		  }
		  window.onscroll=topFixed,window.resize=topFixed,topFixed();
