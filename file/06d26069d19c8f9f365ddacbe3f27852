<script>
	//按钮中点击调用的高速下载按钮
	function heightDownloadButtonClick(softId, thisObj)
	{
		var q = $.cookie('q');
		var userAgentString = navigator.userAgent.toLowerCase();
		if(userAgentString.match(/MicroMessenger/i) == "micromessenger") //微信
		{
			$('#weixinNoticeLayer').show();
			return false;
		}		
		if(thisObj.hasClass('gao-su-xia-zai'))//详情页高速下载
		{
			var clickTongJi = 'm_detail_' + q ;
		}
		else if(thisObj.hasClass('gao-su-xia-zai-bottom'))//详情页底部高速下载
		{
			var clickTongJi = 'm_bottom_' + q ;
		}
		else if(thisObj.hasClass('gao-su-xia-zai-list'))//列表页高速下载
		{
			var clickTongJi = 'm_list_' + q ;
		}
		else if(thisObj.hasClass('gao-su-xia-zai-search'))//搜索页面高速下载
		{
			var clickTongJi = 'm_search_' + q ;
		}
		setTimeout(function(){tj(clickTongJi);}, 1);//统计请求
		/* 百度sem推广渠道增加百度的点击统计 */
		if($.inArray(q, ['baiduss51', 'baiduss52', 'baiduss53', 'baiduss54', 'baiduss55', 'baiduss56', 'baiduss57', 'baiduss58', 'baiduss59', 'baiduss71', 'baiduss72', 'baiduss73', 'baiduss74', 'baiduss75', 'baiduss76']) > -1 && $.inArray($.trim(thisObj.attr('id')), ['height-down-btn-baidu-count', 'height-down-btn-baidu-count-bottom']) > -1 )//百度推广渠道增加点击统计
		{
			//增加百度点击统计 2016-06-06 start
			setTimeout(function(){
				_hmt.push(['_trackEvent', $.trim(thisObj.attr('id')), 'click', softId]);
			}, 1);
			//增加百度点击统计 2016-06-06 end
		}
		
		heightDownloadApk(softId);
		return false;
	}
	
	//高速下载,下载功能主体
	function heightDownloadApk(softId)
	{
		var downloadUrl = 'http://app.2345.cn/zhushou/' + (softId%1000) + '/2345androidmarket_m_' + softId + '.apk';
		var q = $.trim($.cookie('q'));
		if(q === 'hlrenmin')//第三方换量的渠道母包
		{
			downloadUrl = downloadUrl.replace('2345androidmarket_m_', '2345androidmarket_hlrenmin_');
		}
		else if(q === 'ysyytj') //影视带量的母包
		{
			downloadUrl = downloadUrl.replace('2345androidmarket_m_', '2345androidmarket_ysyytj_');
		}
		else if(q === 'bpllq') //白牌浏览器的母包
		{
			downloadUrl = downloadUrl.replace('2345androidmarket_m_', '2345androidmarket_bpllq_');
		}
		else if($.inArray(q, ['01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11']) > -1) //白牌浏览器的母包
		{
			downloadUrl = downloadUrl.replace('2345androidmarket_m_', '2345androidmarket_' + q + '_');
		}
		else if(q === 'baiduss31') //sem进入wapzhushou的高速下载母包
		{
			downloadUrl = downloadUrl.replace('2345androidmarket_m_', '2345androidmarket_baiduss31_');
		}
                else if($.inArray(q, ['shenmass01', 'shenmass02', 'shenmass03', 'sougouss01', 'sougouss02', 'sougouss03']) > -1) //sem进入神马搜索和搜狗搜索的高速下载母包
		{
			downloadUrl = downloadUrl.replace('2345androidmarket_m_', '2345androidmarket_' + q + '_');
		}
		else if($.inArray(q, ['baiduss51', 'baiduss52', 'baiduss53', 'baiduss54', 'baiduss55', 'baiduss56', 'baiduss57', 'baiduss58', 'baiduss59', 'baiduss71', 'baiduss72', 'baiduss73', 'baiduss74', 'baiduss75', 'baiduss76']) > -1)
		{
			//sem 推广渠道
			downloadUrl = downloadUrl.replace('2345androidmarket_m_', '2345androidmarket_wap' + q + '_');
		}
		
		if($('#listCallBackUrlTestInput').val() === '1')//callback能调通
		{
			$.ajax({
				type : "get",
				async : false,
				data : {"softId" : softId}, 
				url : 'http://127.0.0.1:11111/?callback=?',  
				dataType : "jsonp",//数据类型为jsonp  
				jsonp: "jsonpCallback",//服务端用于接收callback调用的function名的参数  
				success : function(res){
					var jsonString = JSON.stringify(res);
					if(jsonString === "76779")//  失败
					{
						location.href = downloadUrl;
					}
				},
				error:function(res){
					location.href = downloadUrl;
				}
			});
		}
		else
		{
			location.href = downloadUrl;
		}
	}
	
	//浏览器判断，ios系统提示和安卓微信的提示信息
	var userAgentString = navigator.userAgent.toLowerCase();
	if( userAgentString.indexOf('iphone') > -1 || userAgentString.indexOf('ipad') > -1 )//ios
	{
		var htmlString = '<div class="wrapper"><section class="ios-error"><img class="img-logo" src="http://img1.2345.com/zhushouimg/images/m/v1/base64/ios-icon.png" alt="" /><p class="txt-tips">暂时不支持苹果手机哦~</p></section></div>';
		document.body.innerHTML = htmlString;
	}
	document.body.style.display  = 'block';
	//end   浏览器判断
	
	
	$(document).ready(function(){
		AddUrlParam();
		
		//解决2345浏览器go(-1)的bug
//		var q = getQueryString('q');
		var q = $.cookie('q');
		if(q == 'liulanqi_zw01') //2345手机浏览器渠道和网址导航渠道
		{
			//尝鲜页必备页回退
			if($('#goBackToIndex').length > 0)
			{
				$('#goBackToIndex').click(function(){
					location.href = '/index.html';
				});
			}
			
			//排行榜
			if($('#goBackToTop').length > 0)
			{
				$('#goBackToTop').click(function(){
					location.href = '/top.html';
				});
			}
			
			//搜索页
			if($('#gobackSearch').length > 0)
			{
				$('#gobackSearch').click(function(){
					location.href = '/index.html';
				});
			}
			//搜索结果页
			if($('#gobackSearchResult').length > 0)
			{
				$('#gobackSearchResult').click(function(){
					location.href = '/index.php?c=ard_m&d=search&q=' +  q;
				});
			}
			
			//详情页面统一跳首页
			if($('#mDetailGoBack').length > 0)
			{
				$('#mDetailGoBack').click(function(){
					location.href = '/index.html';
				});
			}
			//礼包详情页面
			if($('#giftDetailGoBack').length > 0)
			{
				$('#giftDetailGoBack').click(function(){
					location.href = '/index.html';
				});
			}
		}
		else
		{
			$('#goBackToIndex').length > 0 && $('#goBackToIndex').attr('href', 'javascript:history.go(-1);');
			$('#goBackToTop').length > 0 && $('#goBackToTop').attr('href', 'javascript:history.go(-1);');
			$('#gobackSearchResult').length > 0 && $('#gobackSearchResult').attr('href', 'javascript:history.go(-1);');
			$('#gobackSearch').length > 0 && $('#gobackSearch').attr('href', 'javascript:history.go(-1);');
			$('#mDetailGoBack').length > 0 && $('#mDetailGoBack').attr('href', 'javascript:history.go(-1);');
			$('#giftDetailGoBack').length > 0 && $('#giftDetailGoBack').attr('href', 'javascript:history.go(-1);');
		}
		//end go(-1)
	});

	function getQueryString(name)
	{ 
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i"); 
		var r = window.location.search.substr(1).match(reg); 
		if (r != null) return unescape(r[2]); return null; 
	}
	//给页面a标签href添加参数p和q
	function AddUrlParam()
	{
		var p = $.trim($('#in_var_p').val());
		q = $.cookie('q');
		if(q == null || q == '')
		{
			var q = getQueryString('q');
			if(q == null || q == '')
			{
				q = 'm_gf';//没有渠道号的 默认给 m_gf
			}
			//设置cookie
			$.cookie('q', q, { path: "/"});
		}
		
		$('a.btn-dl').each(function(){
			var thisHref = $(this).attr('href');
			var javaS = /javascript:/;
			if(!javaS.test(thisHref)) //可点击的a标签
			{
				var reWenHao = /\?/;
				//没有p参数的补p参数
				var reP = /[\?|\&]p=/;
				if(!reP.test(thisHref))
				{
					if(reWenHao.test(thisHref)) //有问号
					{
						thisHref += '&p=' + p;
					}
					else
					{
						thisHref += '?p=' + p;
					}
				}
				
				//没有q参数的补q参数
				var reQ = /[\?|\&]q=/;
				if(!reQ.test(thisHref) && q != '')
				{
					if(reWenHao.test(thisHref)) //有问号
					{
						thisHref += '&q=' + q;
					}
					else
					{
						thisHref += '?q=' + q;
					}
				}
				//添加到a标签中
				$(this).attr('href', thisHref);
			}
		});
	}
	//返回顶部
	function goToTop()
	{
		document.documentElement.scrollTop = document.body.scrollTop = 0;
	}

	setWebDomain();
	function setWebDomain()
	{
		$('a').each(function(){
			var strHref = $(this).attr('href');
			if(strHref.substr(0, 1) === '/')
			{
				strHref = 'http://wapzhushou.2345.com' + strHref
				$(this).attr('href', strHref);
			}
		})
	}


</script>
