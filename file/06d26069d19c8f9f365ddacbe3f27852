<script>
	var allPageLoad = false;
	var loading = false;	
	$(document).ready(function(){
		$(window).scroll(function(){
			var rankPage = $.trim($('#in_var_rankPage').val());
			if(rankPage == 'rankPage')
			{
				allPageLoad = true;
			}
			// 当滚动到最底部以上100像素时， 加载新内容  
			if (loading === false && ($(document).height() - $(this).scrollTop() - $(this).height()< 100))
			{
				loadMore();
			};
		});
		//返回顶部
		$('#goTop').click(function(){
			goToTop();
		});
	});
	
	function loadMore()
	{
		var p = $.trim($('#in_var_p').val());
		var q = $.trim($('#in_var_q').val());
		var page = parseInt($('#in_var_page').val()) + 1;
		var ajaxType = $.trim($('#in_var_ajaxType').val());
		var type = $.trim($('#in_var_type').val());
		var showTag = $.trim($('#showTag').val());
		var showNumber = $.trim($('#in_var_showNumber').val());
		if(allPageLoad)
		{
			$('#goTop').show();
		}
		else
		{
			loading = true;
			$('#goTop, #loadFail').hide();
			$('#listLoading').show();
			switch(ajaxType)
			{
				case 'index' ://首页
				case 'newApps' ://尝鲜
				case 'essential' ://必备
				case 'gamerec' : //游戏精选
				case 'softrec' ://软件精选
				case 'danJiGames': //单机
				case 'mustPlayGames': //必玩
				case 'newGames'://新游
				case 'onlineGames'://网游
					var sUrl = '/index.php?c=ard_m&d=' + ajaxType + 'Json&p=' + p + '&q=' + q;
					var param = {'page':page,'type':type,'returntype':'json'}
					break;
				case 'getListByCate' ://游戏二级分类全部
					var sUrl = '/index.php?c=ard_m&d=getListByCateJson&p=' + p + '&q=' + q;
					var param = {'page':page,'cateId':$.trim($('#in_var_cateId').val()),'returntype':'json'}
					break;
				case 'getListByTag' ://游戏二级分类tag
					var sUrl = '/index.php?c=ard_m&d=getListByTag&p=' + p + '&q=' + q;
					var param = {'page':page,'cateId':$.trim($('#in_var_cateId').val()),'tag':$.trim($('#in_var_tagName').val()),'type':$.trim($('#in_var_softType').val()),'returntype':'json'}
					break;
				case 'getSoftTopList' ://软件排行
				case 'getGameTopList' ://游戏排行
					var sUrl = '/index.php?c=ard_m&d=' + ajaxType + 'Json&p=' + p + '&q=' + q;
					var param = {'page':page,'type':$.trim($('#in_var_softType').val()),'returntype':'json'}
					break;
			}
			
			$.get(sUrl, param ,function(ret){
				ret = eval('(' + ret + ')');
				$.each(ret.list, function(key, values){
					var count = $('#softList .MlistA-item').length + 1;
					var html = '<div class="MlistA-item">'
							 + '	<a class="trig" href="http://wapzhushou.2345.com' + values.detailUrl + '">';
					 if(showNumber == 1)
					 {
						 html += '<i class="order">' + count + '</i>';
					 }
						 html += '		<img class="img" src="' + values.softlogo + '" alt="" />'
							 + '		<div class="name">' + values.sName;
					 if(showTag == 1 && values.sLabel !== '')
					 {
						 html += '			<i class="i-cate i-cate-wang">' + values.sLabel + '</i>'
					 }
						 html += '		</div>'
							  + '		<div class="prop">';
					 //精品、首发、新品 
					 if(values.recomIco == 4)
					 {
						 html += '			<i class="flag flag-jp">精品</i>';//flag-jp  flag-sf  flag-xp
					 }
					 else if(values.recomIco == 2)
					 {
						 html += '			<i class="flag flag-sf">首发</i>';//flag-jp  flag-sf  flag-xp
					 }
					 else if(values.recomIco == 1)
					 {
						 html += '			<i class="flag flag-xp">新品</i>';//flag-jp  flag-sf  flag-xp
					 }
						 html += '			<span class="piece">' + values.tDown + '次下载</span>'
							 + '			<span class="piece">' + values.sizeShow + '</span>'
							 + '		</div>'
							 + '		<div class="desc">' + values.seoKey + '</div>'
							 + '	</a>'
							 + '	<a rel="nofollow" class="btn-dl gao-su-xia-zai-list" href="javascript:;" onclick="heightDownloadButtonClick(' + values.softId + ', $(this))">高速下载</a>'
							 + '</div>';
					 $('#softList').append(html);
				});//end each
				
				$('#in_var_page').val(page);
				if(page >= ret.pager.page_num)
				{
					$('#goTop').show();
					$('#listLoading,#loadFail').hide();
					allPageLoad = true; //页面加载完了
					loading = false;
				}
				else
				{
					$('#listLoading,#loadFail,#goTop').hide();
					loading = false;
				}
				AddUrlParam();
			});
		}
		
	}
</script>

