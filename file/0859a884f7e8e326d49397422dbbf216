<script type="text/javascript">
(function(){
var scroT=0;
var scrollObj = document.getElementById('scrollBox');//相关文章定位
var JMark = document.getElementById('JMark');
var scrollObj1 = document.getElementById('JnavProWrap');//产品信息定位
var JnavProMark = document.getElementById('JnavProMark');
window.onscroll = window.onresize = function(){ 
if(scrollObj) {                             //相关文章定位
var cName = scrollObj.className;    
if(JMark.getBoundingClientRect().top <= 40){    
if(cName.indexOf('fixTop') == -1) scrollObj.className += ' fixTop'; 
}else{  
scrollObj.className = scrollObj.className.replace(' fixTop','');    
}
}
if(scrollObj1) {                             //相关文章定位
var cName1 = scrollObj1.className;          
if(JnavProMark.getBoundingClientRect().top < 0) {   
if(cName1.indexOf('navProfixTop') == -1) scrollObj1.className += ' navProfixTop';   
if(cName.indexOf('t40') == -1) scrollObj.className += ' t40';//相关文章改定位
}else{  
scrollObj1.className = scrollObj1.className.replace(' navProfixTop','');
scrollObj.className = scrollObj.className.replace('t40','');
}
}
};
})()
function cmtCount(){
var HTML='';
HTML ='<a href="'+ getTopicUrl() +'" target="_blank" rel="nofollow" >(评论<em class="red">'+ getTopicTotal() +'</em>条)</a>';
document.getElementById("cmtCount").innerHTML=HTML;
}
window.onload=function(){cmtCount()}
