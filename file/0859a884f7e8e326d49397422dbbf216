<script>
var tsID = document.getElementById("proBrandId")
tsID = tsID ? tsID.value : '';
if(tsID == '') {document.getElementById('hotDealer').style.display = 'none';}
function cpaCallback(cpa) {
var len = cpa.length;
if(len == 0) return true;
len = len > 10 ? 10 : len;
var html = "";
html += '<ul class="expTxtA hTxt26 ulDealer">';
for (i = 0; i < len; i++) {
if (!cpa[i]) continue;
var saller = cpa[i];
document.getElementById("bname").innerHTML = saller.brandName + "热门经销商";
html += '<li><i class="sTit"><a href="' + saller.shopLink + '" target="_blank" title="' + saller.companyName + '">' + saller.companyName + '</a></i><i class="iQQ"><a target="_blank" href="' + saller.qqLink + '"></a></i><i class="iPhone"><a title="' + saller.phone + '" href="' + saller.shopLink + '" target="_blank"><em></em>' + saller.phone + '</a></i></li>';
}
html += '</ul>';
document.write(html);
}
</script>
