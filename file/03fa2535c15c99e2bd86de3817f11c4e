			function resizeNom(obj) {
                $(obj).jqthumb({
                    classname : 'jqthumb',
                    width : '100%',
                    height : 472,
                    position : {
                        y : '50%',
                        x : '50%'
                    },
                    zoom : '1',
                    method : 'modern',
                    done : function(objs) {
                        $('html, body').animate({
                            scrolly : $('body').height(),
                            scrollx : $('body').width()
                        }, 800);
                    },
                    after : function(imgObj) {
                        fadeIn($(imgObj));
                    }
                });
            }

