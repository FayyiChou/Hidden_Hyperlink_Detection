function select_switch(status) {
    for (i = 0; i < document.form1.length; i++) {
        document.form1.elements[i].checked = status
    }
}
function select_back() {
    var status;
    for (i = 0; i < document.form1.length; i++) {
        status = document.form1.elements[i].checked;
        document.form1.elements[i].checked = !status
    }
}
