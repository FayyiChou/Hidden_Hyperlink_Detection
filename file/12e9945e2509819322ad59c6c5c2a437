<script type="text/javascript">
function currenturl(){
var cururl=location.href;
		if(cururl.indexOf("index.php")>0){
        $("#home").addClass("on");
		}
		
		if(cururl.indexOf("about.php")>0){
        $("#as").addClass("on");
		}
		
		if(cururl.indexOf("product.php")>0 || cururl.indexOf("productCon.php")>0){
        $("#ps").addClass("on");
		}
		
	    if(cururl.indexOf("case.php")>0|| cururl.indexOf("caseCon.php")>0){
        $("#cs").addClass("on");
		}
		
		
		if(cururl.indexOf("news.php")>0 || cururl.indexOf("newsCon.php")>0){
        $("#ns").addClass("on");
		}
		
		if(cururl.indexOf("contact.php")>0){
        $("#ls").addClass("on");
		}
		
		
}


currenturl();
</script></body>
