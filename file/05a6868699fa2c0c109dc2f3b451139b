    <script>


        $(document).ready(function(){
            $("#ind_aqbz").mouseenter(function(){
                $("#ind_aqbz_img").stop().animate({top:'-100px'});
            });
        })
        $(document).ready(function(){
            $("#ind_aqbz").mouseout(function(){
                $("#ind_aqbz_img").stop().animate({top:'0px'});
            });
        })
        $(document).ready(function(){
            $("#ind_tzlc").mouseenter(function(){
                $("#ind_tzlc_img").stop().animate({top:'-100px'});
