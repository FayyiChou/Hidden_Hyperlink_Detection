    <script>
                                                //window.onload = function () {


                                                    /*百分比值*/
                                                    //var ranNum = Math.floor(Math.random() * 101 + 0);
                                                    //var num = ranNum + '%';

                                            function draw(numb,c) {
                                                var num = numb + '%';
                                                var myid="c_"+c;
                                                var canvas = document.getElementById(myid);
                                                        var ct = canvas.getContext('2d');

                                                        /*最大圆形*/
                                                        ct.beginPath();
                                                        ct.arc(25, 25, 25, Math.PI / 180 * 0, Math.PI / 180 * 360, false);
                                                        ct.fillStyle = "#999999";
                                                        ct.closePath();
                                                        ct.fill();


                                                        /*角度弧形进度条*/
                                                        var deg = parseInt(num) / 100 * 360 - 90;//根据百分比获取弧度大小

                                                        ct.beginPath();
                                                        ct.moveTo(25, 25);
                                                        ct.arc(25, 25, 25, Math.PI / 180 * (0 - 90), Math.PI / 180 * deg, false);
                                                        ct.fillStyle = "#a50915";
                                                        ct.closePath();
                                                        ct.fill();



                                                        /*内部圆形*/
                                                        ct.beginPath();
                                                        ct.arc(25, 25, 20, Math.PI / 180 * 0, Math.PI / 180 * 360, false);
                                                        ct.fillStyle = "snow";
                                                        ct.closePath();
                                                        ct.fill();



                                                        /*画文字*/
                                                        ct.beginPath();
                                                        ct.font = "14px Arial";
                                                        ct.fillStyle = "#a50915";
                                                        ct.textAlign = "left";

                                                        var w = ct.measureText(num).width;//获取文本宽度
                                                        var h = 50;//高度和文字大小一样
                                                        var transX = (canvas.width - w) / 2;
                                                        var transY = (canvas.width - h) / 2;
                                                        ct.translate(transX, transY);//保持文字水平居中
                                                        ct.fillText(num, 0, 30);
                                                    }

                                                //    draw(num);
                                                //}
    </script>
    <script type="text/javascript">
<!--列表图片等比例缩放-->
    function AutoResizeImage(maxWidth, maxHeight, objImg) {
        var img = new Image();
        img.src = objImg.src;
        var hRatio;
        var wRatio;
        var Ratio = 1;
        var w = img.width;
        var h = img.height;
        wRatio = maxWidth / w;
        hRatio = maxHeight / h;
        if (maxWidth == 0 && maxHeight == 0) {
            Ratio = 1;
        } else if (maxWidth == 0) {//
            if (hRatio < 1) Ratio = hRatio;
        } else if (maxHeight == 0) {
            if (wRatio < 1) Ratio = wRatio;
        } else if (wRatio < 1 || hRatio < 1) {
            Ratio = (wRatio <= hRatio ? wRatio : hRatio);
        }
        if (Ratio < 1) {
            w = w * Ratio;
            h = h * Ratio;
        }
        objImg.height = h;
        objImg.width = w;
    }
    var nav = '1';
    var NowTime = new Date();

    $(document).ready(function () {
        $.post("/Home/GetServerTime", function (r) {
            NowTime = new Date(r.year, r.month, r.day, r.hour, r.mint, r.secd);
            //alert(NowTime.getFullYear() + "-" + NowTime.getMonth() + "-" + NowTime.getDate() + " " + NowTime.getHours() + ":" + NowTime.getMinutes() + ":" + NowTime.getSeconds());
            setInterval(function () {
                daojishi();
                $("[endtime]").show();
                $("[couting=1]").remove();
            }, 1000);
        });
        $("#btnlogin").click(function () {
            loginsubmit();
        });
        $("#vcimg").click(function () {
            $("#yzm").val("");
            $("#vcimg").attr("src", "/PB/VCImg?d=" + new Date());
            $("#yzm").focus();
        });
        $("#yzm").keydown(function (e) {
            if (e.keyCode == 13) {
                loginsubmit();
            }
        });
    });
    function loginsubmit() {
        if ($("#yzm").val().length < 4) {
            alert("请输入正确的验证码！");
            $("#yzm").focus();
            return;
        }
        $("#formlogin").submit();
    }
    function daojishi() {
        $("[endtime]").each(function (i, t) {
            var EndTime = new Date($(t).attr("endtime"));
            var nMS = EndTime.getTime() - NowTime.getTime();
            //alert(nMS);
            var nD = Math.floor(nMS / (1000 * 60 * 60 * 24));
            var nH = Math.floor(nMS / (1000 * 60 * 60)) % 24;
            var nM = Math.floor(nMS / (1000 * 60)) % 60;
            var nS = Math.floor(nMS / 1000) % 60;
            if (nMS < 0) {
                $(t).empty();
                $(t).html("已结束");
            } else {
                $(t).find("[day=1]").html(nD);
                $(t).find("[hour=1]").html(nH);
                $(t).find("[mint=1]").html(nM);
                $(t).find("[secd=1]").html(nS);
            }
        });
        NowTime = new Date(NowTime.getTime() + 1000);
    }


    </script>
    <script type="text/javascript">
        function doShouyiMath(inputnumb,lilv,jiangli,backtimelimit,backtimelimitunit){
            var jisuan_num=inputnumb;
            var jisuan_lilv= lilv;
            var jisuan_jiangli=jiangli;
            var jisuan_shijian=backtimelimit;
            if(backtimelimitunit==0){
                var jisuan_result=jisuan_lilv / 12 * jisuan_shijian * jisuan_num;
            }
            else if(backtimelimitunit==1)
            {
                var jisuan_result=jisuan_lilv /365 * jisuan_shijian * jisuan_num;
            }

            if(jisuan_jiangli==0){

                var jisuan_result_jiangli =0
            }
            else{
                var jisuan_result_jiangli = jisuan_jiangli * jisuan_num;
            }

            var jisuan_result2 =  parseFloat(jisuan_result) + parseFloat(jisuan_num) +parseFloat(jisuan_result_jiangli);
            if(!isNaN(jisuan_result2)){
                return jisuan_result2.toFixed(2);
            }
            else{
                return 0;
            }

        }
        function showValue(r,i){
            $("#"+i).text(r);
        }
    </script>
    <script>
    $(document).ready(function(){
        // Default options
        var options = {
            swipe: true,    // Add possibility to Swipe > note that you have to include touchSwipe for this
            interval: 7000, // Interval to slide on if slideOnInterval is enabled
            animateDuration: 1000, // Duration of an animation
            animationEasing: 'in-out', // Accepts: linear ease in out in-out snap easeOutCubic easeInOutCubic easeInCirc easeOutCirc easeInOutCirc easeInExpo easeOutExpo easeInOutExpo easeInQuad easeOutQuad easeInOutQuad easeInQuart easeOutQuart easeInOutQuart easeInQuint easeOutQuint easeInOutQuint easeInSine easeOutSine easeInOutSine easeInBack easeOutBack easeInOutBack
            pauseOnHover: true // Pause when user hovers the slide container
        };
        $(".slider").simpleSlider(options);


    });
    </script>

	
