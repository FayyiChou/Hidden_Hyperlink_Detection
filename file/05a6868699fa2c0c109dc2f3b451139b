<script type="text/javascript">
    var len = $('.slide .slide_bg').length;
    var index = 1;
    var intval;
    function showw(index) {
        $('.slide .slide_bg').fadeOut().eq(index).fadeIn();
        $('.slide #pointer li').removeClass('on').eq(index).addClass('on');
    }
    function selfChange() {
        if (index == len) {
            index = 0;
        }
        showw(index);
        index = index + 1;
    }
    intval = setInterval(selfChange, 2500);
    $('.slide #pointer li').live('click', function () {
        var index = $('.slide #pointer li').index(this);
        showw(index);
    })
    $('.slide').hover(function () {
        clearInterval(intval);
    },
    function () {
        intval = setInterval(selfChange, 2500);
    });
</script>

