    <script type="text/javascript">
        function CheckForm(form) {
            form.password.value = HmacMD5(form.password.value);
            return true;
        }

        function login() {
            var username = $("#username").val();
            var password = "'" + HmacMD5($("#password").val()) + "'";
            $.ajax({
                type: "GET",
                url: "login.aspx",
                data: { act: 'login', username: username, password: password },
                success: function (data) {
                    if (data == "ok") {
                        window.location.href = "/";
                    } else if (data == "usernameerr") {
                        alert("用户名不存在");
                    } else if (data == "passworderr") {
                        alert("密码不正确");
                    } else {
                        alert("登录异常");
                    }
                }
            });
        }

    </script>
