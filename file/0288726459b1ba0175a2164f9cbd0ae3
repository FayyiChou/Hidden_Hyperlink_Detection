<script type="text/javascript">

    (function ($) {

        $(function () {
            $('#form_dc1').submit(function () {
                return checkFormEx($("#form_dc1").get(0));
            });
        });

        $(".intro_box").hover(
            function () {
                $(this).find(".cover").fadeIn(300, "linear");
            }
            , function () {
                $(this).find(".cover").fadeOut(300, "linear");
        });

        var liclass = ['vps', 'autoweb', 'index_400', 'idc'];
        $(".strip ul li").mouseenter(function () {
            var $cur = $(this).attr("class");
            var $thisCur = $(this);
            var classname = $cur + "1";
            if ($cur.indexOf("1") == -1) {
                for (var i = 0; i < liclass.length; i++) {
                    if ($cur == liclass[i]) {
                        $thisCur.attr("class", classname);
                    } else {
                        $("#" + liclass[i]).attr("class", liclass[i]);
                    }
                }
            }
        });

        $('.news').Tabs({
            timeout: 200
        });

        $('.banner').unslider({
            fluid: true,
            dots: true
        }).find("li").show();

        $('.ad_slide').unslider({
            dots: true
        }).find("li").show();

        $("#name li").click(function () {
            if (!$(this).hasClass("current")) {
                $(this).addClass("current");
                var chk = $(this).find(":checkbox");
                chk.removeAttr("checked");
                chk.attr("checked", true);
            }
            else {
                $(this).removeClass("current");
                var chk = $(this).find(":checkbox");
                chk.removeAttr("checked");

            }
        });
        var duilian = $("div.duilian");
        var duilian_close = $("a.duilian_close");

        var window_w = $(window).width();
        if (window_w > 1000) { duilian.show(); }
        $(window).scroll(function () {
            var scrollTop = $(window).scrollTop();
            duilian.stop().animate({ top: scrollTop + 260 });
        });
        duilian_close.click(function () {
            $(this).parent().hide();
            return false;
        });
        $("#DomainNames").focusin(function () {
            if ($(this).val() == "域名注册首选联动天下") {
                $(this).val("");
                document.getElementById("DomainNames").style.color = "#000";
                //document.getElementById("DomainNames").style.font-size = "20px";
                document.getElementById("DomainNames").style.fontSize = "24px";
            }
        }).blur(function () {
            if ($(this).val() == "") {
                $(this).val("域名注册首选联动天下");
                document.getElementById("DomainNames").style.color = "#666";
                document.getElementById("DomainNames").style.fontSize = "18px";
            }
        });

    })(jQuery);
    function myload() {
        $("#divNews").slideToggle(500);
    }
    setTimeout("myload()", 3000);

    function engchecked() {
        //alert(document.getElementById("table1").style.display);
        document.getElementById("table1").style.display = "block";
        document.getElementById("table2").style.display = "none";
    };
    function chinachecked() {
        //alert(document.getElementById("table1").style.display);
        document.getElementById("table1").style.display = "none";
        document.getElementById("table2").style.display = "block";

    };
</script>
