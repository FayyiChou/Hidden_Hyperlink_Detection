  <script type="text/javascript">
 var min_height=600;
    $(window).scroll(function(){
        var s=$(window).scrollTop();
        if(s>min_height){
            $('.fix-header').fadeIn(500);
            $('.loop-index').fadeIn(500);
            $('.gotop').fadeIn(500);
        }else{
            $('.loop-index').fadeOut(500);
            $('.fix-header').fadeOut(500);
            $('.gotop').fadeOut(500);
        };
    });
    var jqindex_li=$('.loop-index ul li');
    jqindex_li.hover(function(){
        $(this).children('.en').toggleClass('close');
        $(this).children('.ch').toggleClass('on');
     });
</script> 
