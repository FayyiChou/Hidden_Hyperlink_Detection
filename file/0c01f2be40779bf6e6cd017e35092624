     <script type="Text/Javascript" language="JavaScript">
var btn_buy = "购买";
var is_cancel = "取消";
var select_spe = "请选择商品属性";
/******分类页商品数量加减****/
function modifyBuyNum(d, a) {
	var b;
    var c;
   if (a == -1) {
        c = $(d).parents(".shopping_num").find("input");
        b = parseInt(c.val()) || 1;
        if (b == 1) {
            return
        } else {
            if (b == 2) {
                $(d).attr("class", "dis_decrease")
            } else {
                $(d).prev().attr("class", "add")
            }
            c.val(b + a)
        }
    } else {
        c = $(d).parents(".shopping_num").find("input");
        b = parseInt(c.val()) || 1;
        $(d).next().attr("class", "decrease")
      	c.val(b + a)
    }		
}
	
function checkBuyNum(d) {
	var c = $(d);
	if($.isNumeric($(d).val()) && $(d).val() >= 1)
	{
		var b = c.next().find("a");
		if (c.val() == 1) {
            b.attr("class", "add");
            b.next().attr("class", "dis_decrease")
        } 
		else if(c.val() > 1)
		{
			 b.attr("class", "add");
             b.next().attr("class", "decrease")
		}
	}
	else
	{
		alert("请输入正确的商品数量");
		c.val(1);
	}
}
function close_div(goods_id,goods_recommend)
{
	if(goods_recommend && goods_recommend!='')
	{
		$("#addtocartdialog_retui_"+goods_id+"_"+goods_recommend).hide();
	}
	else
	{
		$("#addtocartdialog_retui_"+goods_id).hide();
	}
}
window.onload = function()
{
  Compare.init();
}
var button_compare = '';
var exist = "您已经选择了%s";
var count_limit = "最多只能选择4个商品进行对比";
var goods_type_different = "\"%s\"和已选择商品类型不同无法进行对比";
var compare_no_goods = "您没有选定任何需要比较的商品或者比较的商品数少于 2 个。";
var btn_buy = "购买";
var is_cancel = "取消";
var select_spe = "请选择商品属性";
function toggle_compare_box()
{
	if($('#compareBox').css('display') == 'none')
	{
		$('#compareBox').css('display', 'block');
		$('.compareHolder').css('display', 'none');
	}
	else
	{
		$('#compareBox').css('display', 'none');
		$('.compareHolder').css('display', 'block');
	}
}
</script> 
