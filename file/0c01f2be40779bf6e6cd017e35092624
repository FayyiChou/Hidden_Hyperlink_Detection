  <script type="text/javascript">
//用户中心
    $('.left-navbar .self').hover(function() {
        $('.left-navbar .self div').toggle();
    });
    //购物车
    $('.shop-cart').hover(function(){
        var _carts=$('.shop-cart .cart-intro span').text();
        if(_carts=='0'){
            $('.shop-cart .goods-no').show();
        }else{
            $('.shop-cart .goods-list').show();
        }
    },function(){
        $('.shop-cart .goods-no').hide();
        $('.shop-cart .goods-list').hide();
    })
	 //客服、关注、收藏
    $('.same').hover(function(){
        $(this).children('div').toggle();
    })
    //回到顶部
    $('.gotop').click(function(){
        $('html,body').animate({scrollTop:0},500);
    })
</script>   
