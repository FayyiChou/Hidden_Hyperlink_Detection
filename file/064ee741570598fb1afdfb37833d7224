			$('#moreBtn').html('');
		}
	});


$("#topLoginItem").unbind("click").on("click",function(){
	var url=$(this).data("url");
	__openloginDlg(url);
});

function __PopupMenu(){
	var that=this;
	var __clearMenu=null;
	var _btnobj="";
	var _menubox="";
	this.init=function(btnobj,menubox){
		_menubox=$(menubox);
		_btnobj=$(btnobj);

		_btnobj.hover(function(){
			that.show(this);
			clearTimeout(__clearMenu);

		},function(){
			var obj=this;
			__clearMenu=setTimeout(function() {
				that.hide(obj,0);
			},200);
		});
		_menubox.hover(function(){
			clearTimeout(__clearMenu);
		},function(){
			clearTimeout(__clearMenu);
			that.hide(this,1);
		});
	}

	this.hide=function(obj,isparent){
		if(isparent==1){
			$(obj).parent().removeClass("hover");
		}else{
			$(obj).removeClass("hover");
		}
	}
	this.show=function(obj){
		$("._menu").parent().removeClass("hover");
		$(obj).addClass("hover");
	}
}

$(function(){
	///顶部菜单-----------------------------------------------------
	var obj=new __PopupMenu();
	obj.init("._userinfo ._topmsg","._userinfo ._topmsg ._menu");
	obj=new __PopupMenu();
	obj.init("._userinfo ._topinfo","._userinfo ._topinfo ._menu");
})	
	</script>

