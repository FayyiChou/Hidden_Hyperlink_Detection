<script type="text/javascript">
	$(function() {
		$('#detail_content img').unbind('click').on('click',function(){
			var baseUrl = $('div .tContent').data('rooturl');
			var img_path = $(this).attr('src');
			if(img_path.indexOf('img/face/em') > 0){
				return;
			}
			if((img_path.indexOf('https://') >= 0 || img_path.indexOf('http://') >= 0) && img_path.indexOf('/uploads/topic/') > 0){
				img_path = img_path.replace(/\?x-oss-process\=image\/resize\,w_480/, '');
				window.open(img_path,'查看原图');
			} else {
				img_path = img_path.replace(/\/thumb\_480\_/, '/');
				window.open(baseUrl + img_path,'查看原图');
			}
		});
	});


	$(function(){
		//InterestedList.init('#interested_game_box');
	});


