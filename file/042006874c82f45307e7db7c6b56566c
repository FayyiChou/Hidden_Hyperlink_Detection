	setTimeout(function(){
		$('.menuIndex').animate({bottom:'20%'},'slow','easeOutBack',next2());
	},6000);
	function next2(){
		setTimeout(function(){$('.indexSlogon').animate({opacity:1},'slow','easeOutQuad',next3());}, 1000);
		
	}
	function next3(){
		$('.bgIndex').hide('slow');
		$('.indexCut').animate({opacity:1, position:'relative'},'slow','easeOutQuad', finish());
	}
	function finish() {
		setTimeout(function(){
			$('.indexPad').animate({opacity:1},1400,'easeOutCubic');
		}, 800);
	}
	
	$('.menuIndex').mouseover(function(){
		$('.menuIndex').animate({bottom:'20%'},'slow','easeOutBack');
