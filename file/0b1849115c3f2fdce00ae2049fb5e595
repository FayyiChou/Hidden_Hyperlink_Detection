hideBalanceIfModeHide();},complete:function(){$(".balLoding").remove();},error:function(){}
});}
function userTime(isTranslate){$.ajax({url:'/index/getUserTimeZoneDate.html',dataType:"json",async:false,success:function(data){timezoneTran =data.timezone;if(isTranslate!=undefined){timezoneTran =transTimeZone(timezoneTran)
}
