}
},success:function(data){if(data.isLogin){var apiHtml ='';for(var i =0;data.api[i];i++){apiHtml +='<li class="_apiBalance"><a href="#">'+data.api[i].apiName+'：'+'<span class="text-warning balance1 hide">--  </span>'+'<span class="text-warning balance2">'+data.api[i].balance+'</span></a></li>';}
