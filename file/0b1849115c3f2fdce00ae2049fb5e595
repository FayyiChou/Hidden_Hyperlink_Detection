userTimeTimerId=window.setInterval(function () {changeTimeTimer();},1000);}
});}
function changeTimeTimer(){var $userTime =$("._user_time");if(dateTimeFromat &&$userTime.attr("time")) {var date =new Date();date.setTime(parseInt($userTime.attr("time"))+1000);$userTime.attr("time",date.getTime());var theMoment=moment(date);theMoment.utcOffset(sessionStorage.getItem("timezone"),false);$userTime.text(timezoneTran + " " + theMoment.format(dateTimeFromat));}
}
