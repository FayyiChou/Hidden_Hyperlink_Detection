}else{if(leftCloseExist){$(".left_close_btn").addClass("hide");}else if(rightCloseExist){$(".right_close_btn").addClass("hide");}
}
transWebUrlTag($li);});$("._close").on("click",function(){var _this =this;if (window.sessionStorage){sessionStorage.setItem("is_float_close_"+$(_this).parent().data("fp"),"true");}
$(_this).parent().hide();});}
}
