$(e.target).text(loginText);},loginText);});function changeLoginStatus(){$.ajax({url:"/headerInfo.html?t="+ new Date().getTime().toString(36),type:"get",dataType:"JSON",beforeSend:function(){if (window.sessionStorage &&(sessionStorage.is_login ==="false" ||typeof sessionStorage.is_login ==="undefined")) {$("._vr_unLogin").show();sessionStorage.is_login =false;}
},success:function(data){if(data.isLogin){sessionStorage.is_login =true;var $loginSuccess =$("._vr_loginSuccess");$loginSuccess.find("._vr_nickname").text(data.nickname);$loginSuccess.find("._vr_messageCount").text(data.messageCount);$loginSuccess.find("._vr_player_balance").text(data.playerBalance);$loginSuccess.find("._vr_wallet_balance").text(data.walletBalance);$loginSuccess.show();$("._vr_unLogin").hide();$("._vr_player_balance").text(data.playerBalance);$("._vr_wallet_balance").text(data.walletBalance);changeBalanceHide();if (window.sessionStorage){sessionStorage.is_login =true;}
isOpenCaptcha =false;}else{if(data.isKickOut) {BootstrapDialog.show({type:BootstrapDialog.TYPE_PRIMARY,title:data.KickOutMessage.title,message:data.KickOutMessage.message
});}
