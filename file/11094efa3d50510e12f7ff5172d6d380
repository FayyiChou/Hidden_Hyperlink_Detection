$(".refer").click(function(){
	if(_consultUrl!=''){
		window.open(_consultUrl);
		return;
	}
 	var openUrl = getWap2AppUrl();
	var currentDate = new Date();
	var hour = currentDate.getHours();
	if((currentDate.getDay()>0 && currentDate.getDay() < 6) && (hour>8 && hour<18)){
   	    $("#tip").html("目前网页排队咨询人数较多<br>建议使用APP咨询");
   	    $("#wap2app").html("<a href='javascript:void(0);' onclick='javaScript:toapp(\""+openUrl+"39yyk/8\",1,\"zixun\");'>使用就医助手APP咨询</a>");
   	    $("#continueWap").html("<a class='grey' href='javascript:zixundiaoqi();'>继续网页排队咨询</a>");
	}else{
		$("#tip").html("现在非工作时间，<br>建议您下载APP，咨询导诊人员或同病患者");
   	    $("#wap2app").html("<a href='javascript:void(0);' onclick='javaScript:toapp(\""+openUrl+"39yyk/8\",1,\"liuyan\");'>使用APP咨询导诊/病友</a>");
   	    $("#continueWap").html("<a class='grey' href='javascript:zixundiaoqi();'>网页留言给我们</a>");
	}
   		document.getElementById('app-popbox').style.display='block';
});
 
//获取用户关注数
 function getCountFavorite(){
		$.ajax({
			url:"/user/getCountFavorite.json",
			async:false,
			data:"type="+type+"&hosId="+fHosId+"&docId="+fDocId,
			success:function(result){
				if(result.count>0){
 					$('.follow.flex').text('已关注');
 					$('.follow.flex').attr("class","follow follow-ed flex")
 					$('#favoriteId').text(result.favoriteId);
				}
			}
		});
	}
	//关注分发操作
 	function favorite(){
 		var text = $('.follow.flex').text();
 		if(text == '已关注'){
 			var favoriteId = $("#favoriteId").text();
 			delFavorite(favoriteId);
 		}else{
 			addFavorite();
 		}
 	}
 
	//关注操作
	function addFavorite(){
		$.ajax({
			url:"/user/addFavorite.json",
			async:false,
			data:"type="+type+"&hosId="+fHosId+"&docId="+fDocId,
			success:function(result){
				/* { "flag" : 1 , "favoriteId" : 8963 , "status" : 100} */
				if(result.status==100){
					$('#yyk_fave_span').removeClass("follow flex");
	 				$('#yyk_fave_span').addClass("follow follow-ed flex");
					$('#yyk_fave_span').text('已关注');
				}else if(result.status==110){//未登录
					window.location.href="/user/yuyue.html?returnUrl="+window.location.href;
				}
			}
		});
	}
	
	//取消关注操作
	function delFavorite(favoriteId){
		$.ajax({
			url:"/user/delFavorite.json",
			async:false,
			data:"&favoriteId="+favoriteId+"&type="+type+"&hosId="+fHosId+"&docId="+fDocId,
			success:function(result){
				if(result.status==100){
					$('#yyk_fave_span').text('关注');
					$('#yyk_fave_span').removeClass("follow follow-ed flex");
	 				$('#yyk_fave_span').addClass("follow flex");
	 				$("#favoriteId").text("");
				}else if(result.status==110){//未登录
					window.location.href="/user/yuyue.html?returnUrl="+window.location.href;
				}
			}
		});
	}
 
	//获取用户点赞数
	function getUserLikes(){
		$.ajax({
			url:'/user/getUserLikesCount.json',
			async:false,
			data:likeUrlData,
			error:function(errResult){
				console.log(errResult);
			},
			success:function(result){
				if(result.status==100){
					if(result.objCount>0){
						$(".opt.flex").text(result.objCount);
					}else{
						$(".opt.flex").text('点赞');
					}
					
				}
			}
		});
	}
	
	//点赞操作
	function addLikes(){
		$.ajax({
			url:'/user/addLikes.json',
			async:false,
			data:likeUrlData,
			error:function(errResult){
				console.log(errResult);
			},
			success:function(result){
				if(result.userCount<1){//同一个医生，一天内只能赞一次
					$(".opt.flex").text(result.objCount);
				}else{
					$('.des').html(result.tip);
					$('.pop').show();
				}
			}
		});
	}
	

	
	
	function registerdiaoqi(id){
		 $.ajax({
			   url:'/ajax/doctorRegisterFalg.jsonp?doctorId='+id,
			   async:true,
			   cache:false,
			   type: "get",
			   dataType: "json",
			   success : function(data){
				   var arrayList =data.list;
				   if($.isArray(arrayList)){
					   $.each(arrayList, function(idx, element) {
						   document.getElementById('app-popbox').style.display='none';
							if(element.REGISTER_FLAG=="Y"){
								var pid = getCookie("pid");
								 if(pid==undefined){//没登录的话，先登录
									 window.location.href="/user/login.html?returnUrl="+window.location.href;
								 }else{
									 $.post(
										'/user/GetUserExpandJson.json',
										{pid:pid},
										function(result){
											if(result=="false"){
												window.location.href="/user/validphone.html";
											}else{
												window.location.href="/doctor/"+id+"_registers.html";
											}
										});
								 }
							}	
					   });
				   }
			   }
		   });	
	}
	
	//咨询
	var _consultUrl = '';
 	if(_consultUrl=='') _consultUrl = '';
 	//咨询按钮调起app
 	function zixundiaoqi(){
 		document.getElementById('app-popbox').style.display='none';
 		if(_consultUrl!=''){
				window.open(_consultUrl);
				return;
			}
			var pid = getCookie("pid");
			 if(pid==undefined){//没登录的话，先登录
				 window.location.href="/user/login.html?returnUrl="+window.location.href;
			 }else{
				 $.post(
					'/user/GetUserExpandJson.json',
					{pid:pid},
					function(result){
						if(result){
							$.ajax({
								url:'/forwordComment.json',
								data:"type="+type+"&typeId="+(fDocId!=null?fDocId:fHosId),
								error:function(errResult){
								console.log(errResult);
								},
								success:function(result){
									if(result.status==100){//跳转咨询页面
										window.location.href=result.value;
									}else if(result.status==110){//未登录
										//alert('请先登录');
										window.location.href="/user/login.html?returnUrl="+window.location.href;
									}
								}
							});
						}else{
							window.location.href="/user/validphone.html";
						}
					}		
				);
			 }
 		
 	}
	
	function showShareBox(){
		$('.share-box').show();
	}
	
	function getCookie(objName){//获取指定名称的cookie的值
	  	 var arrStr = document.cookie.split("; ");
	  	 for(var i = 0;i < arrStr.length;i ++){
	  	 var temp = arrStr[i].split("=");
	  	 if(temp[0] == objName) return unescape(temp[1]);
	  	}
	  } 
	
	
	 $(function() {
	 	getCountFavorite();  //获取用户关注数
	 	getUserLikes();//获取用户点赞数
	 	if(false){
		   bindRegisterClick();
	    }
	 	if(_consultUrl==''){
		 	toapp(getWap2AppUrl(),0,'');
	 	}
	 });
 </script>

