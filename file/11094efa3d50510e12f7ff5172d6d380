        				}else{
        					var loginBtnStr = "<a class=\"loginBtn\" href=\"/user/login.html\" data-ajax=\"false\">登录</a>";
        					$(".topLoginInfo").html(loginBtnStr);
        				}
        			}
    	});
    });
    function topLogOut(){
    	$("#loginName").text('请稍候');
    	$.post('/user/logout.json', function(data) {
    		if (data.code == 0) {
    			window.location = "/user/yuyue.html?"+ Math.random();
    		}
    	});
    }
