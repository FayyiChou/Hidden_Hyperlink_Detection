<script type="text/javascript">
function ajaxUrlDoAction(ajaxUrl,querytype, nature){
	   $.ajax({
		   url:ajaxUrl,
		   async:true,
		   cache:false,
		   type: "get",
		   dataType: "json",
		   success : function(data){
			   var arrayList =data.list;
			   if($.isArray(arrayList)){
				   $.each(arrayList, function(idx, element) {
					   if(querytype=='doctor'){
						   var html = "";
						   if(element.REGISTER_FLAG =='Y'){
		  					   html = "<a id='doctorRegister' href='javaScript:void(0);' class='order'>预约挂号</a>";
							}else if(element.REGISTER_GUAHAO_FLAG=='Y' || element.REGISTER_JIAHAO_FLAG=='Y' || element.REGISTER_LVSE_FLAG=='Y'){
								html = "<a id='fullRegister' href='javaScript:$('.des').html('该医生号源已约满，请选择其他医生');$('.pop').show();' class='order order-full'>号已约满</a>";
							}else{
								html = "<a id='fullRegister' href=\"javaScript:$('.des').html('该医生暂未开通预约挂号，请选择其他医生');$('.pop').show();\" class='order order-full'>预约挂号</a>";
							}
	  					    if($("#"+querytype).length>0 && nature=="862"){
	  					    	$("#"+querytype+" .refer").before(html);
		  						bindRegisterClick();
	  					    }else{
	  					    	$("#"+querytype).append(html);
		  						bindRegisterClick();
	  					    }
					   }else if(querytype=='hospital'){
						   if(element.REGISTER_FLAG =='Y'){
							   var html = "<a  id='hospitalRegister'  href='javaScript:;' class='order' target='_self'>预约挂号</a>";
		  					    if($("#"+querytype).length>0 && nature=="862"){
			  						$("#"+querytype+" .refer").before(html);
			  						bindRegisterClick();
			  					}else{
			  						$("#"+querytype).append(html);
			  						bindRegisterClick();
			  					}
		  					  	
						   }
					   }
				   });
			   }
		   }
	   });	
}
function ajaxRegisterFlag(){
	var pageType = 'hospital';
	if(pageType =='doctor'){
		var doctorIds = '';
		var doctorNature = '';
		if(doctorIds!=''){
			var ajaxUrl = "/ajax/doctorRegisterFalg.jsonp?doctorId="+doctorIds;
			ajaxUrlDoAction(ajaxUrl,"doctor", doctorNature);
		}
	}else if(pageType =='hospital'){
		var hexId= '20368';
		var nature = '0';
		if(hexId!=''){
 			var ajaxUrl = "/ajax/hospitalRegisterFalg.jsonp?hospitalId="+hexId;
 			ajaxUrlDoAction(ajaxUrl,"hospital", nature);
		}
	}
}
ajaxRegisterFlag();


</script>

