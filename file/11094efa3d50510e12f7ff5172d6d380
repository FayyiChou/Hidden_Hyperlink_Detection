 //获取WAP跳APP的URL
 function getWap2AppUrl(){
	 var isDoctor = false;
 	 var isHospital = true;
 	 var isLable = false;
 	 var openUrl = "";
	 if(isDoctor){
	 	var doctorId = "";
	    openUrl =  'net.39.yyk://39yyk/4/'+doctorId;
	 }else if(isHospital){
		var hospitalId = "22674";
	    openUrl =  'net.39.yyk://39yyk/3/'+hospitalId;
	 }else if(isLable){
		var hospitalId = "";
 		var labId = "";
 		var labTypeId = '';
 		var labName = '';
 	    openUrl =  'net.39.yyk://39yyk/6/'+hospitalId+'/'+labId+'/'+labTypeId+'/'+labName;
	}
	 return openUrl;
 }
 
