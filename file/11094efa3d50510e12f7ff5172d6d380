    $(document).ready(function(){
    	$.ajax({
        			type : 'post',
        			url : '/user/checkLogin.json',
        			data : "",
        			dataType : 'json',
        			success : function(result) {
        				if(result.status==1){
        					var loginInfo = "<span id=\"loginName\"> <a href=\"/user/yuyue.html\" data-ajax=\"false\"><b>"+result.userName+"</b></a><a href=\"###\" onClick=\"topLogOut()\">退出</a></span>";
