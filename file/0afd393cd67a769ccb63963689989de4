<script style="text/javascript">
var speed=25;//数值越大，速度越慢
var demo215=document.getElementById("demo215");
var demo115=document.getElementById("demo115");
var demo15=document.getElementById("demo15");
function MarqueeLeft(){
if(demo215.offsetWidth-demo15.scrollLeft<=0)
demo15.scrollLeft-=demo115.offsetWidth
else{
demo15.scrollLeft++
}
}
var MyMar=setInterval(MarqueeLeft,speed);
demo15.onmouseover=function() {clearInterval(MyMar);}
demo15.onmouseout=function() {MyMar=setInterval(MarqueeLeft,speed);}
</script>
