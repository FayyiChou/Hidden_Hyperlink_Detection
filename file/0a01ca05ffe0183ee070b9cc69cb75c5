  <script type="text/javascript">
function resizePic(obj){
if(obj.width>600) {
obj.width=600;
}
}
function getTimeStr(nS) {
update=new Date(nS*1000);//时间戳要乘1000
year=update.getFullYear();
month=(update.getMonth()+1<10)?('0'+(update.getMonth()+1)):(update.getMonth()+1);
day=(update.getDate()<10)?('0'+update.getDate()):(update.getDate());
hour=(update.getHours()<10)?('0'+update.getHours()):(update.getHours());
minute=(update.getMinutes()<10)?('0'+update.getMinutes()):(update.getMinutes());
second=(update.getSeconds()<10)?('0'+update.getSeconds()):(update.getSeconds());
return (year+'-'+month+'-'+day+' '+hour+':'+minute+':'+second);
}
function _get_famoustfs(){
if($("#famousTF").html() != ''){
$("#famousTF").empty().hide();
}
$.getJSON("http://plus.kdslife.com/my/recommand_change/0/1?callback=?", function(json){
$.each(json, function(i, data){
$("#famousTF").append('<li><a href="http://plus.kdslife.com/my/topic/'+data.userName+'.html"><img src="'+data.pic_id+'" /><center><input name="userID" type="checkbox" value="'+data.userName+'" checked="checked"><em>'+data.nickName+'</em></center></a></li>');
});
});
$("#famousTF").fadeIn(1500);
}
$(".photobox .u_control").bind("click",function(){
_get_famoustfs();
});
$(".photobox .d_control").bind("click",function(){
_get_famoustfs();
});
var __onSubmit = false;
$("#tijiao").bind("click",function(){
alert("功能维护中!");
return;
if(__onSubmit){return false;}
$.post("/content/comment_60863.html", {content: $("#talk-con").val()},
function(data){
switch (data.error) {
case -1 :
alert("评论失败!");
break;
case -2 :
alert("评论内容不能为空!");
$("#talk-con").focus();
break;
case 0:
alert("评论成功，谢谢参与!");
window.location.reload();
break;
}
}, "json");
});
$(document).ajaxStart(function() {
__onSubmit = true;
});
$(document).ajaxComplete(function() {
__onSubmit = false;
});
_get_famoustfs();
$("#followBtn").bind("click",function(){
var _follow_users='';
$("#famousTF :input[name='userID']").each(function(i){
if(i !== 1)
_follow_users += $(this).val()+'@';
else
_follow_users = $(this).val();
});
$.getJSON("http://plus.kdslife.com/user/follow_batch/?callback=?", { toUserNames: _follow_users }, function(data){
switch (data) {
case -3 :
alert("您还未登录哦~亲~");
break;
case -1 :
alert("您已关注此人，去山上看看吧~");
break;
case -2 :
case 1 :
alert("关注成功！去山上看看吧~");
break;
}
});
});
</script> 
