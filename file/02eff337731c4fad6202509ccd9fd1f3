   seajs.loadModule = function(moduleName){
       var toString = Object.prototype.toString;
       moduleName = seajs.jsMode +  moduleName ;

       seajs.use(moduleName, function(Entry){
           if(toString.call(Entry) == '[object Function]'){
               var entry = new Entry();
               entry.init();
           }else if ( toString.call(Entry) == '[object Object]'  && Entry.init){
               Entry.init();
           }
       });
   };

