  <script>
    (function($) {
      $(document).ready(function() {
        $('.slider .show-after').removeClass('show-after');
        window.slider = $('.slides').bxSlider({
          auto: true,
          mode: 'fade',
          pause: 5000,
          controls: false,
          pagerCustom: '.bx-pager',
          onSliderLoad: function() {
            $('#sidebar .block-rm-blocks:first').height($('#clover-slideshow').height());
          }
        });

        $('.slider .bx-pager a:not(.active)').hover(
          function(){
            $('.clover-white', $(this)).fadeOut();
          },
          function(){
            $('.clover-white', $(this)).fadeIn();
          }
        );

        // правим размер шрифта заголовка
        function resize_text()
        {
          // средние параметры (на широком экране)
          var maxLines = 3;
          var maxFontSize = 50;
          var maxLineHeight = 59;
          var maxWidth = 511;
          
          // требуемая пропорция (w/h - title в maxLines строк на широком экране)
          var minProportion = maxWidth/(maxLineHeight * maxLines); 

          // получаем ширину заголовка
          slider.stopAuto();
          $('.slider .slides li').css('visibility', 'hidden').css('display', 'list-item');
          var width = $('.slide-text .content-title').width();
          $('.slider .slides li').css('display', 'none').css('visibility', 'visible');
          $('.slider .slides li:first').css('display', 'list-item');
          slider.startAuto();
          slider.goToSlide(0);
          
          if (width > 0) {
            var div = $('.slider .slide-text .content-title');

            // требуемая высота
            var height = Math.ceil(width/minProportion);
            // получаем нужный междустрочный интервал
            var lineHeight = height/maxLines;
            // получаем нужный размер шрифта относительно изменения междустрочного интервала
            var fontSize = maxFontSize * (lineHeight/maxLineHeight);

            div.css('font-size', fontSize + 'px');
            div.css('line-height', lineHeight + 'px');
          }
        }
        
        
        resize_text();
        $(window).resize(resize_text);
        
      });
    })(window.jQuery);
   </script>

  </div>
</div>
  </div>
            </div>
          
          
          
        </div>

