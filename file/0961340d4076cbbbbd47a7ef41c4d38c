<script type="text/javascript">
function get_Cookie(cookieName) {
    var cookieString = document.cookie;
    var start = cookieString.indexOf(cookieName + '=');
    if (start == -1)
        return null;
    start += cookieName.length + 1;
    var end = cookieString.indexOf(';', start);
    if (end == -1) return unescape(cookieString.substring(start));
    return unescape(cookieString.substring(start, end));
}

	    function wtTJ()
		    {
		        var stracc = document.getElementById("Top1:LoginAcc1:txtAcc").value;
		        var strpass = document.getElementById("Top1:LoginAcc1:txtPass").value;
		         var strcode = document.getElementById("Top1:LoginAcc1:txtYZ").value;
		        if(stracc == "")
		        {
		            alert('会员账号不能为空！');
		            return;
		        }
		         if(strpass == "")
		        {
		            alert('会员密码不能为空！');
		            return;
		        }
		         if(strcode == "")
		        {
		            alert('验证码不能为空！');
		            return;
		        }
		        var checkcode = get_Cookie('CheckCode');
		        if(checkcode.toLowerCase() != strcode.toLowerCase())
		        {
					alert('验证码错误，请重新填写验证码');
		            return;
		        }
		        var strID = LoginAcc.ImageBntAdd(stracc,strpass).value;
		        if(strID == -1)
		        {
					alert('用户账号还未通过审核，请您等待审核通过再登陆！');
					 document.getElementById("Top1:LoginAcc1:txtAcc").value = "";
		             document.getElementById("Top1:LoginAcc1:txtPass").value = "";
		             document.getElementById("Top1:LoginAcc1:txtYZ").value = "";
					return;
		        }
		        else if(strID == 1)
		        {
		             document.getElementById("Top1:LoginAcc1:txtAcc").value = "";
		             document.getElementById("Top1:LoginAcc1:txtPass").value = "";
		             document.getElementById("Top1:LoginAcc1:txtYZ").value = "";
		             window.location.href = '../Default/Index.aspx';
		        }
		        else if(strID == 2) 
		        {
					alert('密码或账号错误，请重新登陆！');
					return;
		        }
		        else
		        {
					alert('服务器繁忙，稍后登陆！');
		            return;
		        }
		    }
</script>
