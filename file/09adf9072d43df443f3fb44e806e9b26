<script type="text/javascript">
  function autoScroll(obj){
		$(obj).find("ul").animate({
			marginTop : "-50px"
		},500,function(){
			$(this).css({marginTop : "0px"}).find("li:first").appendTo(this);
		})
	}

  var scroll = setInterval('autoScroll(".maquee")',2500);
  $('.maquee li a').on('mouseover', function(){
  	clearInterval(scroll);
  }).on('mouseout', function(){
  	scroll = setInterval('autoScroll(".maquee")',2500);
  })

</script>
<script>
	$(document).ready(function(){
		$(".project").hover3d({
			selector: ".project__card",
			shine: false,
		});
		$("a").each(function() {
			var asrc = $(this).attr("href");
			if(asrc == "//www.bxd365.com/dingzhi.html"){
				$(this).parent("li").attr("id","asrc")
			}
		});
		var slider1 = $("#asrc");
		setInterval(show,1);
		function show(){
		    if(slider1.is(":hidden")){
		    	$("#window1").css("display","block");
		    	$("#window2").css("display","none");
		    }else{
		    	if($("#window2").is(":hidden")){
		    		$("#window1").css("display","none");
		    		$("#window2").css("display","block");
		    	}else{

		    	}
		    }
		}

		$(".banner-box").mouseenter(function(){
			clearInterval(show);
		})
		$(".banner-box").mouseleave(function(){
			show();
		})
		$("#insure").selectpick({
			container: '.test_1',
			onSelect: function(value,text){
			}
		});
		$("#demand_id").selectpick({
			container: '.test_2',
			onSelect: function(value,text){
			}
		});
        $('.custom-submit').click(function(){
            $('#custom_form').submit();
        });
	});
	$('ul ol').find('li').click(function(){
			var name = $(this).parent('ol').parent('li').attr('data-input');
			
			var val = $(this).attr('val');
			//alert(val);
			//$('input[name='+name +']').val(val);
			$('input[name='+name +']').attr("value",val);
		});
</script>
