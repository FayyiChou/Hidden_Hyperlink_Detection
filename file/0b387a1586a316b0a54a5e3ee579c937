jQuery.TWTWindow = function($){
    var winWidth = $(window).width();
    var winHeight = $(window).height();
    $('#pp_background').css({
        height: winHeight,
        width: winWidth
      });
    $('#pp_background_wait').css({
        height: winHeight,
        width: winWidth
      });
 };jQuery.TWTWindow(jQuery);
