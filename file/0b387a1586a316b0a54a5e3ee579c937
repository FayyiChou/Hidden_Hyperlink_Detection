  function remail(twt_uid, twt_email){
    jQuery('#cue').css('display','none');
    jQuery.post('/home/passport/register.php',{act:'remail',twt_uid:twt_uid,twt_email:twt_email},function(data){
       if(data== 'y'){
         jQuery('#cue').css('display','block');
       }
    });
