					shutCutsCloseiph();
					},30000);
				}
			}
		}
	}else{

		//return;//暂时屏蔽
		
		if (null == window.sessionStorage.getItem('showShortcutQingK-'+copid)){
		
		
		$("#shortCutMes").show();
	
		}
		
		
		return;
		var returnValue=brow();
		/*
		if(returnValue!="no"){
				if (null == window.sessionStorage.getItem('showShortcutQingK-'+copid)) {
					window.sessionStorage.setItem('showShortcutQingK-'+copid,1);
					$("#shortcutimage").css("background-image","url('img/shortcuts-"+returnValue+".png')");
					$("#shortcuts-iph").css("height","85px");
