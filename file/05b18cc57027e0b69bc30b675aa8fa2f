<script type="text/javascript">
//cookie添加缓存

		var cookie= document.cookie;
		
		var cooks=cookie.split(";");
		
		for(var i=0;i<cooks.length;i++){
		var cook=cooks[i];
		//mycook.innerHTML+=(cook.substring(0,cook.indexOf("="))+"==="++"<br/>")
		
		var cook=cooks[i];
		cook=cook.replace(/(^\s*)/, "");
		
		cook=cook.replace(/\"/g, "");
	
		var key=cook.substring(0,cook.indexOf("="));
		var value=cook.split(key+"=")[1];
		
		
		if(key!="sessionId"){
		//window.localStorage.setItem(key,value);
			if(window.localStorage.getItem(key)==null||window.localStorage.getItem(key)==""){
				if(value!=""&&value!=null&&value!=" "){	
				window.localStorage.setItem(key, value)
				}else{
				window.localStorage.removeItem(key);
				}
			}
			
			
		}
		var vall="";
		if(value!=null){
		vall=value.replace(/\"/g, "");
		}
		if(vall==null||vall==""){
		window.localStorage.removeItem(key);
		}
		
		}
		if(cookie.indexOf("customerId")==-1){
		window.localStorage.removeItem("customerId");
		}
	
	setTimeout(function(){
		var cus=window.localStorage.getItem("customerId");
		if(cus==null||""==cus||"null"==cus||"undefined"==cus){
			window.localStorage.removeItem("save_info");
			window.localStorage.removeItem("auto_login");
			window.localStorage.removeItem("loginFlag");
			window.localStorage.removeItem("customerId");
			window.localStorage.removeItem("loginName");
			window.localStorage.removeItem("cusName");
			window.localStorage.removeItem("doAusEnt");
			window.localStorage.removeItem("encrypPass");
		}
	},100)


	var openFlag=0;//0本机 1正式
  	//document.getElementById("noJsJudge").style.display="none";
  	var k_flag=8;
     var onmessage = function(e) {
       var data = e.data;
      
   		var bool=false;
   		try{
   		bool=(data.indexOf("1:")==0);
   		}catch(e0){
   		bool=false;
   		}
   	
   		if(bool){
   		
   		data=data.split("1:")[1];
       	var key=data.split(":;:")[0];
       	var value=data.split(":;:")[1];
       	window.sessionStorage.setItem(key,value);
   		}else{
       	try{
       	
       	var ds=data.split(":;:");
       	var d=ds[0];
       	var s=ds[1];
       	
       	if(d!=null&&d!=""&&d!="null"){
       	if(s!=null&&s!=""&&s!="null"){
       	window.localStorage.setItem(d,s);
       	}else{
   	   window.localStorage.removeItem(d);
       	}
       	}
       	k_flag+=1;
       	
       	if(k_flag>5){
       	checkeUserNameIs();
       	}
       	
       	}catch(e){}
       }
    }
    //监听postMessage消息事件
   
    if (typeof window.addEventListener != 'undefined') {
      window.addEventListener('message', onmessage, false);
    } else if (typeof window.attachEvent != 'undefined') {
      window.attachEvent('onmessage', onmessage);
    }
  
  
  	function defaultimg(ele){
  		ele.src="img/homeless.png"
  	}
  	function lazyAll(){

  //	return;
  		var array=new Array();
  		var imgs;
  		try{
  		imgs=$(".imgLazy").objs();
  		}catch(e){
  		imgs=$(".imgLazy");
  		}
  		for(var i=0;i<imgs.length;i++){
  		
  			var img=imgs[i];
  			
  			var data=img.getAttribute("data-original");
  		
  			var src=img.getAttribute("src");
			if(data!=null&&src!=null){
		
			var srcP=src;
		  	
		  	img.complete=false;
		  	if(data.indexOf("http://")!=0){
		  	img.src=ppqilu.IMG_BASEPATH+data;
		  	}else{
		  	img.src=data;
		  	}
		  	
		  	
		  	img.onload=function(){
		  	var name=this.className;
			this.className=name.split("imgLazy")[1]==null?"":name.split("imgLazy")[1];
		  	}
		  	img.onerror=function(){
		  	var name=this.className;
			this.className=name.split("imgLazy")[1]==null?"":name.split("imgLazy")[1];
			this.src=srcP;
		  	}
		  	
		  
		  	
		  	
			}
			
  		}
  
	}
	
	function lazyAll2(){
  		setTimeout(function(){
  		var imgs=$("img.imgLazy");
    		$(imgs).each(function(){
    			var im=$(this).attr("data-original");
    			if(im.indexOf("http://")!=0){
    			$(this).attr("data-original",ppqilu.IMG_BASEPATH+im);
    			}
    			$(this).lazyload({
    				effect:"fadeIn",
    			
    				load:function() {
    				$(this).removeClass("imgLazy");
    			}
    			});
    		});
  		},200)
	}
