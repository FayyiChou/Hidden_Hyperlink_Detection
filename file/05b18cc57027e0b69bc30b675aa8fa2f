					<span id="messDiv" style="margin-left:-2px; font-family:'Helvetica','Arial','黑体'; font-size:17px; font-weight:bold; color:#ffffff; text-shadow:2px 2px 2px #0455a9;">授权登录中</span>
				</div>
   			</div>
   		</div>
   		
   		<script type="text/javascript">
			//当前页面
			var nowUrl = window.location.href;
			//需要满足微信返回的参数信息
			if(nowUrl.indexOf("code")>0){
		  		//微信登录--获取code
				var getCode = "";
	            var getUrl = window.localStorage.getItem("wxBackUrl");
				var paraString = nowUrl.substring(nowUrl.indexOf("?")+1,nowUrl.length).split("&");
				for (i=0; j=paraString[i]; i++){
					var jKey = j.substring(0,j.indexOf("="));
					if("code"==jKey){
						getCode = j.substring(j.indexOf("=")+1,j.length);
					}
					//if("state"==jKey){
						//var getUrlQk = j.substring(j.indexOf("=")+1,j.length);
						//getUrl = decodeURIComponent(getUrlQk);
					//}
				}
				
				//获取copID
			    var getCopID = "800001";
			    var getDyCopID = "";
			    if(""==getUrl || null==getUrl || undefined==getUrl){
			    	getCopID = "800001";
			    	getDyCopID = "";
			    }else{
					var paraString = getUrl.substring(getUrl.indexOf("?")+1,getUrl.length).split("&");
					for (i=0; j=paraString[i]; i++){
						var jKey = j.substring(0,j.indexOf("="));
						if("copID"==jKey){
							getCopID = j.substring(j.indexOf("=")+1,j.length);
			    			getDyCopID = getCopID;
						}
					}
				}
				
				//处理返回地址
				if(""==getUrl || null==getUrl || undefined==getUrl){
					getUrl = ppqilu.BASEPATH;
				}
				if(""==getCode || null==getCode || undefined==getCode){
					//获取code失败
					document.getElementById("messDiv").innerHTML = "微信授权登录失败，请稍后重试";
				}else{
					//调用接口获取数据
					new $ppqilu_ajax({
						"cls" : "Customer_loginOther",
						"code" : getCode,
						"loginFlag" : "3",
						"getCopID" : getCopID
					}, function(ajaxObj){
						if (ajaxObj.code == ajaxObj.constants.SUCCESS) {
							var objs = ajaxObj.objs;
							var isHaveMob = "";
							var doBindMob = "";
							var loginFlag = "";
							var customerId = "";
							var loginName = "";
							var cusName = "";
							var doAusEnt = "";
							var encrypPass = "";
							var sameFlag = "";
							for(var i=0;i<objs.length;i++){
								var object=objs[i];
								//标记信息
								isHaveMob = object.get("isHaveMob");
								doBindMob = object.get("doBindMob");
								loginFlag = object.get("loginFlag");
								sameFlag = object.get("sameFlag");
								//用户信息
								customerId = object.get("customerId");
								loginName = object.get("loginName");
								cusName = object.get("cusName");
							    doAusEnt = object.get("doAusEnt");
								encrypPass = object.get("encrypPass");
								//老用户ID
								doToBindCusId = object.get("doToBindCusId");
								//新用户性别
								doGender = object.get("doGender");
							}
							//alert("isHaveMob: "+isHaveMob);
							//alert("doBindMob: "+doBindMob);
							//alert("loginFlag: "+loginFlag);
							//alert("sameFlag: "+sameFlag);
							//alert("customerId: "+customerId);
							//alert("loginName: "+loginName);
							//alert("cusName: "+cusName);
							//alert("doAusEnt: "+doAusEnt);
							//alert("encrypPass: "+encrypPass);
							//alert("doToBindCusId: "+doToBindCusId);
							//alert("doGender: "+doGender);
							
							//判断
							if("1"==isHaveMob){
								//微信登录，老用户：已经认证注册手机号--登陆成功
								//customerId不为空，为登录成功
								if (customerId != "") {
									//是否保存信息的标记，1表示保存。用于进入登录页面时，拿到信息放到输入框中
									window.localStorage.setItem("save_info", "0");
									//保存用户信息 自动登录
									window.localStorage.setItem("auto_login", "1");
									//登录类型--微信
									window.localStorage.setItem("loginFlag", "3");
									//用户id
									window.localStorage.setItem("customerId", customerId);
									//登录名称
									window.localStorage.setItem("loginName", loginName);
									//用户昵称
									window.localStorage.setItem("cusName", cusName);
									//toid
									window.localStorage.setItem("doAusEnt", doAusEnt);
									//mmPI
									window.localStorage.setItem("encrypPass", encrypPass);
									
									//认证手机
									window.localStorage.setItem("isHaveMob", isHaveMob);
									//绑定的手机号
									window.localStorage.setItem("doBindMob", doBindMob);
									
									//登陆成功调用接口(liuzheng使用)
									new $ppqilu_ajax({"cls":"KMall_updateCarLocal", "cusId":customerId}, function(ajaxObjTwo){});
									
									//老用户登录成功判断是否订阅站点
									if("2"==sameFlag){
										if(""==getDyCopID || null==getDyCopID || undefined==getDyCopID){
										}else{
											new $ppqilu_ajax({
												"cls":"UserCenter_updateOR",
												"ids":getDyCopID,
												"customer_id":customerId
											},function(ajaxObj){});
										}
									}
									
									//提示语
									document.getElementById("messDiv").innerHTML = "登录成功";
									//页面跳转
									window.setTimeout(function() {
										window.location.replace(getUrl);
									}, 1000);
									
								}else{
									//提示语
									document.getElementById("messDiv").innerHTML = "微信授权登录失败，请稍后重试";
								}
							}else if("0"==isHaveMob){
								//微信登录，老用户：未认证手机号--认证手机号
								document.getElementById("messDiv").innerHTML = "请绑定手机号";
								//认证手机
								window.localStorage.setItem("isHaveMob", isHaveMob);
								//老用户id
								window.localStorage.setItem("doToBindCusId", doToBindCusId);
								//用户登陆标示 1-新浪; 2-QQ; 3-微信;
								window.localStorage.setItem("doToBindLogFlag", "3");
								//登录类型--sina
								window.localStorage.setItem("loginFlag", "3");
								//页面跳转
								window.setTimeout(function() {
									window.location.replace(ppqilu.BASEPATH2+'cus_authMo.do');
								}, 1000);
							}else if("2"==isHaveMob){
								//微信登录，老用户：已经认证未注册手机号--注册手机号
								document.getElementById("messDiv").innerHTML = "请设置手机账号登录密码完成绑定";
								//认证手机
								window.localStorage.setItem("isHaveMob", isHaveMob);
								//老用户id
								window.localStorage.setItem("doToBindCusId", doToBindCusId);
								//用户绑定的手机号
								window.localStorage.setItem("doBindMob", doBindMob);
								//用户登陆标示 1-新浪; 2-QQ; 3-微信;
								window.localStorage.setItem("doToBindLogFlag", "3");
								//登录类型--微信
								window.localStorage.setItem("loginFlag", "3");
								//页面跳转
								window.setTimeout(function() {
									window.location.replace(ppqilu.BASEPATH2+'cus_bindMo.do');
								}, 1000);
							}else if("3"==isHaveMob){
								//微信登录，新用户
								document.getElementById("messDiv").innerHTML = "请绑定手机号";
								//登陆用户登陆名称
								window.localStorage.setItem("doToLoginName", loginName);
								//登陆用户昵称
								window.localStorage.setItem("doToCusName", cusName);
								//登陆用户性别
								window.localStorage.setItem("doToGender", doGender);
								//用户登陆标示
								window.localStorage.setItem("doToBindLogFlag", "3");
								//登录类型--微信
								window.localStorage.setItem("loginFlag", "3");
								//页面跳转
								window.setTimeout(function() {
									window.location.replace(ppqilu.BASEPATH2+'cus_authMoNoCus.do');
								}, 1000);
							}
			  	  		}else{
			  	  			//登录失败
			  	  			if(119==ajaxObj.code){
								document.getElementById("messDiv").innerHTML = "该账号已被禁用";
			   				}else if(115==ajaxObj.code){
								document.getElementById("messDiv").innerHTML = "该账号绑定手机号，已被使用，请稍后重试";
			   				}else if(377==ajaxObj.code){
			   					document.getElementById("messDiv").innerHTML = "整合用户记录失败";
			   				}else{
								document.getElementById("messDiv").innerHTML = "微信授权登录失败，请稍后重试";
			   				}
						}
					});
				}
			}else{
	 	  		//登录失败
				document.getElementById("messDiv").innerHTML = "微信授权登录失败，请稍后重试";
			}
   		</script>
	
</body></html>