		//微信
		document.addEventListener('WeixinJSBridgeReady', function onBridgeReady() {
		
        // 分享到朋友圈
        WeixinJSBridge.on('menu:share:timeline', function(argv){
        //alert("in");
       
            shareTimeline();
            });
 		  WeixinJSBridge.on('menu:share:appmessage', function(argv){
            shareFriend();
            });
        }, false);
    
        function shareTimeline() {
        var title="";
        var cop="";
    
        var t1='';
        var t2='';
        var t3='';
        var t4='';
        if(cop=="0"){
        title="轻快";
        }else if(t1!=null&&t1!=""&&t1!="null"){
        title=t1;
        }else if(t2!=null&&t2!=""&&t2!="null"){
        title=t2;
        }else  if(t3!=null&&t3!=""&&t3!="null"){
        title=t3;
        }else {
        title=t4;
        }
    
    
    	WeixinJSBridge.invoke('shareTimeline',{
                            "img_url":shareWexnIcon,
                            "img_width": "60",
                            "img_height": "60",
                            "link": window.location.href,
                            "desc":"",
                            "title": title
                         
                            }, function(res) {
                              
                            _report('timeline', res.err_msg);
                            });
}

function shareFriend() {
		
	    var value=document.getElementById("cont")==null?"":document.getElementById("cont").innerHTML; 
  		var title="";
        var cop="";
     
        var t1='';
        var t2='';
        var t3='';
        var t4='';
     
        if(cop=="0"){
       
        title="轻快";
        }else if(t1!=null&&t1!=""&&t1!="null"){
         
        title=t1;
        }else if(t2!=null&&t2!=""&&t2!="null"){
         
        title=t2;
        }else  if(t3!=null&&t3!=""&&t3!="null"){
         
        title=t3;
        value=$("#contentLine").html();
        
        }else {
         
        title=t4;
        }
      
        
       	if(value!=null){
   		 value=value.replace(/(^\s*)/, "")
   		 value=value.replace(/\r\n/g,"")  
       	 value=value.replace(/\n/g,"");  
   		 value=value.replace(/<\/?.+?>/g,""); 
   		 value=value.replace(/\&nbsp;/g," "); 
    	}else{
    	 value="";
    	}
    	if(value.length>100){
    	value=value.substring(0,100)+"...";
    	}
    	
        
    WeixinJSBridge.invoke('sendAppMessage',{
                            "appid": "",
                            "img_url": shareWexnIcon,
                            "img_width": "60",
                            "img_height": "60",
                            "link":window.location.href,
                            "desc": value,
                            "title":title
                            }, function(res) {
                            _report('send_msg', res.err_msg);
                            })
}
