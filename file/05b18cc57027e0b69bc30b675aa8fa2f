	//获取信息
	function checkeUserNameIs(){
		var customerId = window.localStorage.getItem("customerId");
		var sessionId = window.localStorage.getItem("doAusEnt");
		if(null==customerId || ""==customerId || undefined==customerId || 
			null==sessionId || ""==sessionId || undefined==sessionId){
	   		//用户未登录--清除用户信息
			window.localStorage.removeItem("save_info");
			window.localStorage.removeItem("auto_login");
			window.localStorage.removeItem("loginFlag");
			window.localStorage.removeItem("customerId");
			window.localStorage.removeItem("loginName");
			window.localStorage.removeItem("cusName");
			window.localStorage.removeItem("doAusEnt");
			window.localStorage.removeItem("encrypPass");
		}else{
			//调用接口获取数据
		    new $ppqilu_ajax({
		    	"cls" : "Customer_loginCheck",
				"tomerIdCheck" : customerId,
				"sessionId" : sessionId
			}, function(ajaxObj) {
		    	if (ajaxObj.code == ajaxObj.constants.SUCCESS) {
		    		//用户在新信息校验正确
				}else{
		    		//清除用户信息
					window.localStorage.removeItem("save_info");
					window.localStorage.removeItem("auto_login");
					window.localStorage.removeItem("loginFlag");
					window.localStorage.removeItem("customerId");
					window.localStorage.removeItem("loginName");
					window.localStorage.removeItem("cusName");
					window.localStorage.removeItem("doAusEnt");
					window.localStorage.removeItem("encrypPass");
					//跳转页面判断
					window.setTimeout(function() {
						if(window.location.href.indexOf("cus_")>0){
							//不作处理
						}else{
							//刷新当前页面
							window.location.reload();
						}
					}, 1500);
				}		
		    });
	    };
	}
	
	//用户公共方法--判断登录、登陆
	function islogin(){
		var cus=window.localStorage.getItem("customerId");
		if(cus==null||cus==""||cus=="null"){
			return false;
		}else return true;
	}
	function nullToLogin(){
		var cus=window.localStorage.getItem("customerId");
		if(cus==null||cus==""||cus=="null"){
			//微信校验
			if((/micromessenger/gi).test(navigator.appVersion.toLowerCase())){
				//微信设置
				var wxDoUrl = window.location.href;
				window.localStorage.setItem("wxBackUrl",wxDoUrl);
				var getParamsQk = wxDoUrl.substring(0,wxDoUrl.indexOf("qingk.cn/")+9);
				var paramUrl = encodeURIComponent(getParamsQk);
				//跳转页面
				window.location.replace("https://open.weixin.qq.com/connect/oauth2/authorize?appid=wx0c13307e1c35b5ff&redirect_uri=http://qingk.cn/customer_wxParaTwoLo.jsp&response_type=code&scope=snsapi_userinfo&state="+paramUrl+"#wechat_redirect");
			}else{
				location.href="cus_login.do";
			}
		}
	}
	
	//用户公共方法--判断认证、认证
	function isAutoMob(){
		var loginDoFlag = window.localStorage.getItem("loginFlag");
		var doHaveMob = window.localStorage.getItem("isHaveMob");
		//第三方登录
		if("1"==loginDoFlag || "2"==loginDoFlag || "3"==loginDoFlag){
			//是否认证手机号
			if("1"==doHaveMob){
				//认证
				return true;
			}else{
				//未认证
				return false;
			}
		}else{
			//手机登陆认证
			return true;
		}
	}
	function zeroToAutoMob(){
		var loginDoFlag = window.localStorage.getItem("loginFlag");
		var doHaveMob = window.localStorage.getItem("isHaveMob");
		var doAutoMobFlag = true;
		//第三方登录
		if("1"==loginDoFlag || "2"==loginDoFlag || "3"==loginDoFlag){
			//是否认证手机号
			if("1"==doHaveMob){
				//认证
				doAutoMobFlag = true;
			}else{
				//未认证
				doAutoMobFlag = false;
			}
		}else{
			//手机登陆认证
			doAutoMobFlag = true;
		}
		
		//未认证手机--去认证
		if(!doAutoMobFlag){
			//保存页面地址
			//var mobDoUrl = window.location.href;
			//window.localStorage.setItem("mobBackUrl",mobDoUrl);
			//跳转页面
			//window.location.replace(ppqilu.BASEPATH2+'cus_authMo.do');
			
			//清除登陆customerId
			window.localStorage.removeItem("customerId");
			//微信校验
			if((/micromessenger/gi).test(navigator.appVersion.toLowerCase())){
				//微信设置
				var wxDoUrl = window.location.href;
				window.localStorage.setItem("wxBackUrl",wxDoUrl);
				var getParamsQk = wxDoUrl.substring(0,wxDoUrl.indexOf("qingk.cn/")+9);
				var paramUrl = encodeURIComponent(getParamsQk);
				//跳转页面
				window.location.replace("https://open.weixin.qq.com/connect/oauth2/authorize?appid=wx0c13307e1c35b5ff&redirect_uri=http://qingk.cn/customer_wxParaTwoLo.jsp&response_type=code&scope=snsapi_userinfo&state="+paramUrl+"#wechat_redirect");
			}else{
				location.href="cus_login.do";
			}
		}
	}
	
