	function getElementsByClassName1(clsName,htmltag){   
    var arr = new Array();   
    var elems = document.getElementsByTagName(htmltag);  
    for ( var cls, i = 0; ( elem = elems[i] ); i++ ){  
    if ( elem.className == clsName ){   
    arr[arr.length] = elem;  
    }  
    }  
    return arr;  
}




