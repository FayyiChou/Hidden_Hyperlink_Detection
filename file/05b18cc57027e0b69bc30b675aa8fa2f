function is_weixn(){
var ua = navigator.userAgent.toLowerCase();
if(ua.match(/MicroMessenger/i)=="micromessenger") {
return true;
} else {
return false;
}
}

function goPageHome(){
	
	var homepageId="";
	
	if(homepageId==null||homepageId==""||homepageId=="null"){
		if(openFlag==0){
		location.href="home.do?copID=";
		}else{
		location.href="home.do?copID=";
		return;
		var loc=window.location.href;

		location.href=loc.substring(0,loc.lastIndexOf("/"));
		}
	}else{
		location.href="edulistorderContent.do?copID=&channelID="+homepageId;
	}
}
