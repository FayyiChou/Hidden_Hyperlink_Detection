function loadingShow(){
	
	var width=WindowWidth;
	var height=window.screen.height;
	 var t1=document.body.scrollTop;
	var top=(height-32)/2;
	var left=(width-32)/2;

	$("#loaddingimg").css("left",left+"px");
	$("#loaddingimg").css("top",(top+t1)+"px");
	$("#loaddingimg").show();
	
	
}
function loadingDis(){
	
	$("#loaddingimg").hide();
	
}

function leftJudge(end){
//return;
	if(end==0){
		$("#goLeftpannel").hide();
	}else{
		$("#goLeftpannel").show();
	}
}
function rightJudge(end){
//return;
		var wid=Swidth;
		var totalWid=wid-end;
		var liwidth=$(".promptu-menu li").width();
		if(totalWid>=(liwidth-5)){
			$("#goRightpannel").hide();
		}else{
			$("#goRightpannel").show();
		}	
}


  	
       
