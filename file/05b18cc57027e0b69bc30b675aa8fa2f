//主屏幕 qingk快捷方式
function addShourtCutsIphone(){

var isIDevice = (/iphone|ipad/gi).test(navigator.appVersion); // IOS手机
	if (isIDevice) {
		var safari = null;
		var ua = navigator.userAgent.toLowerCase();
		if (ua.match(/version\/([\d.]+)/) != null) {
			window.openDatabase ? safari = ua.match(/version\/([\d.]+)/)[1] : 0;
			if (safari != 0) {
	
				if (null == window.sessionStorage.getItem('showIphoneQingK')) {
					window.sessionStorage.setItem('showIphoneQingK',1);
					$("#shortcutimage").css("background-image","url('img/shortcuts-apple.png')");
					$("#shortcuts-iph").show();
					setTimeout(function(){
					shutCutsCloseiph();
					},30000);
				}
			}
		}
	}else{
	return;
	
	}
}
//微信主屏幕提示 
	
	function weixinisshow(flag){
	if(flag==null){
	flag="";
	}
	if(is_weixn()){
	
	var elem=document.getElementById("weixintishD");
	
	if(elem!=null){
	
		var isIDevice = (/iphone|ipad/gi).test(navigator.appVersion); // IOS手机
		if(isIDevice){
		elem.getElementsByTagName("img")[0].src="img/weixintishi_ios.png";
		}
		
		var copId="";
		var sessid=copId+"_weixin_"+flag;
			if(window.sessionStorage.getItem(sessid)==null){
					elem.style.display="";
			}
		}
		}	
	}
	function weixinclose(flag){
		if(flag==null){
		flag="";
		}
		var elem=document.getElementById("weixintishD");
			if(elem!=null){
				var copId="";
				var sessid=copId+"_weixin_"+flag;
				window.sessionStorage.setItem(sessid,1);
				elem.style.display="none";
			}
	}



	function brow(){
	var NV = {};
	var UA = navigator.userAgent.toLowerCase();
	NV.name=!-[1,]?'ie':
    (UA.indexOf("firefox")>0)?'firefox':
    (UA.indexOf("chrome")>0)?'chrome':
    window.opera?'opera':
    window.openDatabase?'safari':
    'unkonw';
	
    NV.shell=(UA.indexOf('360')>-1)?'360浏览器':
    (UA.indexOf('sogou')>-1)?'搜狗浏览器':
    (UA.indexOf('maxthon')>-1)?'遨游浏览器':
    (UA.indexOf('mqqbrowser')>-1)?'QQ浏览器':
    (UA.indexOf('baidu')>-1)?'百度浏览器':
    (UA.indexOf('ucbrowser')>-1)?'UC浏览器':
    '未知或无壳';
if(NV.shell=="搜狗浏览器"){
	//alert("搜狗：首先点击菜单栏旁的收藏按钮选择添加到首屏，然后在首页长按书签往上拖动至发送至桌面。");
	return "sougou";
}else if(NV.shell=="遨游浏览器"){
	//alert("遨游：点击底部菜单的收藏按钮，选择添加到桌面。");
	return "aoyou";
}else if(NV.shell=="百度浏览器"){
	//alert("百度：首先点击菜单栏旁的收藏按钮选择添加到首页。然后在首页长按图标添加到桌面。");
	return "baidu";
}else if(NV.shell=="UC浏览器"){
	//alert("UC：点击菜单栏旁的收藏按钮选择发送到桌面。");
	return "uc";
}else if(NV.shell=="QQ浏览器"){
	//alert("QQ：首先点击功能键选择添加书签到主页书签，然后在首页长按书签往上拖动至添加至桌面。");
	return "qq";
}else if(NV.shell=="360浏览器"){
	//alert("360：点击菜单栏旁的收藏按钮选择发至桌面。");
	return "360";
}else return "no";
}
