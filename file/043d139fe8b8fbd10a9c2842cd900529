            setTimeout(function () {
                msbulo.click.GetMsgCount();
            }, 1000);
        }
    });

    function GetNewMsg(data) {
        if (data == null) { return; }
        var tc_count = data.cc + data.fc + data.ac + data.mc;//短消息
        if(tc_count>0)
        {
            if (tc_count > 9) 
            { 
                tc_count = "9+"; 
            }
            $(".link-prolie").append("<i class=\"newfeed-dot\">" + tc_count + "</i>"); 
            $(".link-message").append("<i class=\"newfeed-dot\">" + tc_count + "</i>"); 
        }
    }
</script>

