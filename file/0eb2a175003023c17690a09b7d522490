			function goBack() {
				window.history.back()
			}
			$(function(){
				if($(".page").html()==""){
					$(".paging").css("display","none");
					return;
				}
				var prevHref = $(".prev");
				var aSpan = $(".paging span");
				var nextHref = $(".next");
				var ph = $("#pages b").prev().attr("href");
				var nh = $("#pages b").next().attr("href");
				var dNum = $("#pages b").text();
				var mNum;
				if (dNum==1) {
					prevHref.addClass("no");
				};
				if ($("#pages a:last-child").attr("href")=="#") {
					mNum = dNum;
					nextHref.addClass("no");
				} else{
					mNum = $("#pages a:last-child").prev().text();
				};
				aSpan.html(dNum+"/"+mNum);
				prevHref.attr("href",ph);
				nextHref.attr("href",nh);
				
			})

		</script>

