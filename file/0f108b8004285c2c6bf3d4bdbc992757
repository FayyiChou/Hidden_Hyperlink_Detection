<script language=javascript>
function picsize(obj,MaxWidth){

  img=new Image();//新建一个Image对象，用这个对象的width就不会出现width为0的现象
  img.src=obj.src;
  if (img.width>MaxWidth)
  {
    return MaxWidth;
  }
  else
  {
    return img.width;
  }
}
</script>
