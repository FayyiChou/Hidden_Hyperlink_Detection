                  <td id='demo2' valign=top></td> 
                </tr>
              </table>
            </div>
            <script>
var speed1=42
demo2.innerHTML=demo1.innerHTML
function Marquee2(){
if(demo2.offsetWidth-demo.scrollLeft<=0)
demo.scrollLeft-=demo1.offsetWidth
else{
demo.scrollLeft++
}
}
var MyMar2=setInterval(Marquee2,speed1)
demo.onmouseover=function() {clearInterval(MyMar2)}
demo.onmouseout=function() {MyMar2=setInterval(Marquee2,speed1)}
                </script>
                        
<!--滚动区的高度和宽度-->
