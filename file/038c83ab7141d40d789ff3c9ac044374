<script>
var ct = 0, nav_top = 0;
function slide() {
	$("#topSlider div.out").removeClass("current").removeClass("out");
	$("#topSlider div.current").addClass("out");
	$("#topSlider .visuals div:eq("+ct+")").addClass("current");
	$("#topSlider .copies div:eq("+ct+")").addClass("current");
	ct++;
	if (ct >= $("#topSlider div:first div").length) {
		ct = 0;
	}
}

$(window).on("load", function() {
	slide();
	var timer = setInterval(slide, 8000);
	$(window).on("resize orientationchange", function() {
		setTimeout(function() {
			nav_top = $("#header").height() + $("#topSlider").height();
			$(window).trigger("scroll");
		}, 350);
	}).trigger("resize");
	$(window).on("scroll", function() {
		var st = $(window).scrollTop();
		if (nav_top - st > 50) {
			$("#navigation").css({ top: nav_top - st });
		} else {
			$("#navigation").css({ top: 50 });
		}
	}).trigger("scroll");
});
</script>
    
    
