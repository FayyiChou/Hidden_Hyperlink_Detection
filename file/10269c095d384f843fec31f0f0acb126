<script>
$.ajax({
    type: "post",
        url: "/disease/ajaxloadcnt",
        data: {
            'diseaseId': 542,
                'from': 'common'
        },
        success: function(data) {
            if(data) {
                data = $.parseJSON(data);
                phone = data['phone'];
                ask = data['ask'];
                booking = data['booking'];

                for(var i = 0; i < phone.length; i++) {
                    goodVoteCount = phone[i].goodVoteCount;
                    cureExpCnt = phone[i].cureExpCnt;
                    presentCnt = phone[i].presentCnt;
                    lastCompleteTime = phone[i].lastCompleteTime;
                    $("#voteCnt4phone_" + i).html(goodVoteCount);
                    $("#expCnt4Phone_" + i).html(cureExpCnt);
                    $("#presentCnt4Phone_" + i).html(presentCnt);
                    $("#lastCompleteTime_" + i).html("最近通话：" + lastCompleteTime);
                }

                for(var i = 0; i < ask.length; i++) {
                    goodVoteCount = ask[i].goodVoteCount;
                    cureExpCnt = ask[i].cureExpCnt;
                    presentCnt = ask[i].presentCnt;
                    postCount = ask[i].postCount;
                    userName = ask[i].userName;
                    $("#voteCount4Ask_" + i).html(goodVoteCount);
                    $("#expCnt4Ask_" + i).html(cureExpCnt);
                    $("#presentCnt4Ask_" + i).html(presentCnt);
                    if(postCount > 0) {
                        $("#postCount_" + i).html("<a href='http://www." + userName + ".haodf.com/zixun/list.htm' target='_blank' class='gray3 look_a'>近两周回复<span class='look_num'>" + postCount + "</span>问<span class='ml5 f8'>>></span></a>");
                    } else {
                        $("#postCount_" + i).parent().parent().hide();
                    }
                }

                for(var i = 0; i < booking.length; i++) {
                    goodVoteCount = booking[i].goodVoteCount;
                    cureExpCnt = booking[i].cureExpCnt;
                    presentCnt = booking[i].presentCnt;
                    bookingCnt = booking[i].bookingCnt;
                    latestOrderTime = booking[i].latestOrderTime;
                    $("#goodVoteCount4jh_" + i).html(goodVoteCount);
                    $("#cureExpCnt4jh_" + i).html(cureExpCnt);
                    $("#presentCnt4jh_" + i).html(presentCnt);
                    $("#bookingCnt_" + i).html(" " + bookingCnt + " ");
                    $("#latestOrderTime_" + i).html("最新预约：" + latestOrderTime);
                }
            }
        }
});
</script>

