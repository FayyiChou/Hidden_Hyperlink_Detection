if(bvIsPc){
	BEVA.add("ERGE_KANERGE_PLAYER_PAUSE", function(thedata){
		var d, len, idx, dataArr=[], tmpCont;
		if(thedata.content && thedata.content.length > 0){
			d = thedata.content;
			for(idx=0, len=d.length; idx<len; idx++){
				tmpCont = d[idx];
				if(tmpCont.order_volume!=-1){
					if(tmpCont.order_volume < tmpCont.display_count){
						continue;
					}
				}
				dataArr.push({"res":tmpCont.order_resource, "jurl":tmpCont.order_link, "turl":tmpCont.order_track, 
					"statId":tmpCont.stat_id, "max":tmpCont.order_volume, "cnt":tmpCont.display_count});
			}
			if(dataArr.length>0){
				beva.kan.setPauseImage(dataArr);
			}
		}
	});
	BEVA.add("ERGE_KANERGE_PLAYER_TIEPIAN", function(thedata){
		var d;
		if(thedata.content && thedata.content.length > 0){
			d = thedata.content[0];
			beva.kan.setTiepianImage(d.order_resource, d.order_link, d.stat_id, d.order_track, 3);
		}
	});
	BEVA.add("ERGE_KANERGE_PLAYER_FOOTER", function(thedata){
		var d, len, idx, dataArr=[], tmpCont;
		if(thedata.content && thedata.content.length > 0){
			d = thedata.content;
			for(idx=0, len=d.length; idx<len; idx++){
				tmpCont = d[idx];
				if(tmpCont.order_volume!=-1){
					if(tmpCont.order_volume < tmpCont.display_count){
						continue;
					}
				} else {
					tmpCont.order_volume = 1e10; // large enough
				}
				dataArr.push({
					"res" : tmpCont.order_resource,
					"tres" : tmpCont.order_resource_small ? tmpCont.order_resource_small : tmpCont.order_resource,
					"jurl" : tmpCont.order_link,
					"turl" : tmpCont.order_track,
					"max" : tmpCont.order_volume,
					"cnt" : tmpCont.display_count,
					"statId" : tmpCont.stat_id	
				});
			}
			if(dataArr.length>0){
				beva.kan.setFooterImage(dataArr);
			}
		}
	});
}

