      <script type="text/javascript" language="JavaScript"> 
        var target_data = new Date("November 13 2016 00:00:00");
        var current_data = Date.now();
        var days_left = target_data - current_data;
        days_left /= 1000;
        days_left /= 60;
        days_left /= 60;
        days_left /= 24;
        days_left = Math.ceil(days_left);
        days_left_hundred = Math.floor ( days_left / 100 );
        days_left_ten = Math.floor (( days_left - ( days_left_hundred * 100 )) / 10 );
        days_left_units = days_left - (( days_left_hundred * 100 ) + ( days_left_ten * 10 ))
      </script>
