    <script>
        $(document).ready(function () {
            var slider = $('.bxslider').bxSlider({
                mode: 'fade',
                auto: true,
                autoStart: true,
                speed: 0,
                pause: 8000,
                captions: false,
                nextText: '',
                prevText: '',
                controls: false,
                infiniteLoop: true,
                autoHover: true
            });
            $('.bx-pager-item a').click(function (e) {
                var i = $(this).index();
                slider.goToSlide(i + 1);
                slider.stopAuto();
                restart = setTimeout(function () {
                    slider.startAuto();
                }, 100);

                //return false;
            });

            $("#showRAD").click(function (e) {
                slider.stopAuto();
                restart = setTimeout(function () {
                    slider.startAuto();
                }, 1000);
                return false;
            });
        });
    </script>
    
