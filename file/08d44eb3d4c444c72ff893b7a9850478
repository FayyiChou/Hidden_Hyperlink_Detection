<script type="text/javascript">
function link_se(obj){
var divId = obj.id;
var linkLa = document.getElementById("link_la");
var linkLb = document.getElementById("link_lb");
var linkLc = document.getElementById("link_lc");
var la = document.getElementById("la");
var lb = document.getElementById("lb");
var lc = document.getElementById("lc");
if(divId == "la"){
linkLa.style.display="block";
linkLb.style.display="none";
linkLc.style.display="none";
la.className="focus";
lb.className="";
lc.className="";
}else if(divId == "lb"){
linkLa.style.display="none";
linkLb.style.display="block";
linkLc.style.display="none";
la.className="";
lb.className="focus";
lc.className="";
}else if(divId == "lc"){
linkLa.style.display="none";
linkLb.style.display="none";
linkLc.style.display="block";
la.className="";
lb.className="";
lc.className="focus";
}
}
</script>
