</div>

<script type="text/javascript">
	var args = Loneve.GetQueryStringArgs();
	if(args['mid']){
		$("li[mid]").removeClass("on");
		$("li[mid='" + args['mid'] +"']").addClass("on");
		$(".jgw_subclass").hide();
		$("#jgw_submenu_" + args['mid'].slice(1,2) +"_content").show();
	}
	if(args['sid']){
		$("li[sid]").removeClass("on");
		$("li[sid='" + args['sid'] +"']").addClass("on");
	}
	
	$(document).ready(function() {
        Loneve.ScrollDoor(".jgw_tab_button",10);
		$('#jgw_lyms_list').Loneve_UniformSliding({direction:'left'});
		$('#search_btn').click(function(){
		    var keyword=Loneve.Trim($('#search_text').val());
			var model=$('#search_channel').val();
			if(keyword!=""){
			    window.location.replace("/plus/Search/?key=" + keyword + "&m=" + model);
			}
			else{
			    alert("请输入需要查询的关键字");
			}
		});
		$("#MyContent").css("font-size","14px");
    });

