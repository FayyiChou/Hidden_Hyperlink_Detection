    (function () {
        var ui = LETV.using("UI");
        ui.SimpleTab("#j-tab-b", {tabs: ".title>h2>span>a", tabsFor: ".sort div", switchEvent: "click"});
    })();


    function $TAB(x) {
        return document.getElementById(x);
    }
    function tab_show(a, b, c, d) {
        x = a, max_i = b, tn = c, tc = d;
        for (var i = 1; i <= max_i; i++) {
            $TAB(tn + i).className = $TAB(tc + i).className = "";
        }
        $TAB(tn + x).className = $TAB(tc + x).className = "on";
    }
    (function () {
        $(".j-morebtn").click(function (ev) {
            $(this).hide();
            $(".j-morebox").show();
            return false;
        });
        $(".j-retractbtn").click(function (ev) {
            $(".j-morebox").hide();
            $(".j-morebtn").show();
            return false;
        });
    })();


    (function () {
        var Logon = Spirit.UserValidate, //用户登录验证
                userLogin = Spirit.userLogin; //弹出层对象
        if (LETV.Plugin.DownLoader != undefined) {
            var DL = LETV.Plugin.DownLoader.getInstance();
            var a = LETV.cookie('m'), b = LETV.cookie('u');
            $('.btDown').click(function () {
                if (Logon.getUserInfo()) {
                    DL.downVideo(__INFO__, 'special');
                } else {
                    userLogin.openLetvLogin();
                    var event = window['Spirit'].Event;
                    var eventid = event.addEvent('loginSuccess', function (data) {
                        DL.downVideo(__INFO__, 'special');
                    });
                }
            });
        }
    })();


