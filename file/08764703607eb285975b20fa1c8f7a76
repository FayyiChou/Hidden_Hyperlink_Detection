	FrontProducts_list01['FrontProducts_list01-1439614926650_init'] = function() {
		jQuery(".specChangeImg").click(function(){
			var data = $(this).metadata();
			var srcStr=data.smallImgUrl;
			var defaultImg=data.defaultImg;
			$(this).parent().find(".specChangeImg").removeClass("click");
			$(this).addClass("click")
			if(srcStr!=""){
				jQuery("#FrontProducts_list01-1439614926650_"+data.id).attr("src",srcStr);
			}else{
				if(defaultImg!=""){
					jQuery("#FrontProducts_list01-1439614926650_"+data.id).attr("src",defaultImg);
				}else{
					jQuery("#FrontProducts_list01-1439614926650_"+data.id).attr("src","/images/common/noimage.jpg");
				}
			}
		});
	};
	jQuery(document).ready(FrontProducts_list01['FrontProducts_list01-1439614926650_init']);
