<script>
function delCookie(name){//为了删除指定名称的cookie，可以将其过期时间设定为一个过去的时间
   document.cookie = name + "=" + "; expires=Thu, 01-Jan-70 00:00:01 GMT;domain="+document.domain;
}
var getCookie = function(name) {
	var oRegExp = new RegExp("(^|)" + name + "=([^;]*)(;|$)","gi").exec(document.cookie), aCookie;
	if(aCookie = oRegExp) {
		return unescape(aCookie[2]);
	}
	return null;
};
var setCookie=function(){
	var fid=getCookie("fid");
    var uid=getCookie(fid+"UID");
    if(!uid){
            delCookie("fid");    
    		document.cookie="fid=145;path=/;domain="+document.domain; 
    }
}
setCookie();
</script>
