        function initInventory() {
            pageNum = 1;
            vpInventory('refres');
        };
        function nextInventory() {
            pageNum++;
            vpInventory();
        };
        var order = "item.price:desc";
        function headInventory() {
            $.ajax({
                url: 'http://www.vpgame.com/gateway/v1/user/' + uid + '/inventory/' + appid,
                type: "get",
                dataType: 'jsonp',
                data: {
                    'rarity': rarity,
                    'quality': quality,
                    'order': order,
                    'keyword': keyword,
                    'status': 'normal',
                    'session': CONFIG.session_id,
                    'bet': 'yes',
                    'virtual_type':'exchange',
                    'show_virtual':isCloseVirtual,
                    'lang': lang
                },
                success: function(body) {
                    if (body.success == true) {
                        var bHtml = template('my-bag-template', body);
                        if(appid=='570'){
                            $('#my-dota-bag').html(bHtml);
                        }else{
                            $('#my-bag').html(bHtml);
                        }
                    } else {
                        $('.back-num').html('0');
                        if(body.status == 403){//账户异常
                            window.location.href = CONFIG.sso_login + window.location.href;
                            return;
                        }
                    }
                },
                error: function() {
                }
            });
        };
        var isFirst = true;
        function vpInventory(type) {
            $.ajax({
                url: 'http://www.vpgame.com/gateway/v1/user/' + uid + '/inventory/' + appid,
                type: "get",
                dataType: 'jsonp',
                data: {
                    'rarity': rarity,
                    'quality': quality,
                    'order': order,
                    'page': pageNum,
                    'keyword': keyword,
                    'status': 'normal',
                    'limit': 48,
                    'show_virtual':isCloseVirtual,
                    'virtual_type':'exchange',
                    'session': CONFIG.session_id,
                    'bet': 'yes',
                    'lang': lang
                },
                success: function(body) {
                    if (body.status == 200) {
                        if(appid=='570'){
                            if (body.body.length > 0) {
                                isFirst = false;
                                $('.nodecor').addClass('hide');
                                $('.inventory-i').removeClass('hide');
                                var aHtml = template('vp-bag-template', body);
                                if (type == 'refres') {
                                    if (firstRoad == true) {
                                        dotaJscroll.getContentPane().html(aHtml);
                                        firstRoad = false;
                                    }
                                } else {
                                    dotaJscroll.getContentPane().append(aHtml);
                                }
                                if (dotaJscroll.getContentPane().height() <= 300) {
                                    $('#dota-bag-img').height(dotaJscroll.getContentPane().height());
                                } else {
                                    $('#dota-bag-img').height(300);
                                }
                                $('.inventory-i .decor-img').addClass('dota-decor-img');
                                $('.decor-num .decor-pic .decor-img').each(function() {
                                    var val = $(this).find('input').val();
                                    $('.inventory-i .decor-img').each(function() {
                                        var _this = $(this);
                                        var thisVal = _this.find('input').val();
                                        if (val == thisVal) {
                                            _this.remove();
                                        }
                                    });
                                });
                                dotaJscroll.reinitialise();

                            } else {
                                if (firstRoad == true) {
                                    $('.inventory-i').addClass('hide');
                                    $('.nodecor').removeClass('hide');
                                    if(isFirst==true){
                                        $('.nodecor .no-decor-class').addClass('hide');
                                        $('.nodecor .no-decor').removeClass('hide');
                                    }else{
                                        $('.nodecor .no-decor').addClass('hide');
                                        $('.nodecor .no-decor-class').removeClass('hide');
                                    }
                                }
                                isover = true;
                            }
                        }else{
                            $('.nodecor .no-decor').removeClass('hide');
                            if (body.body.length > 0) {
                                $('.nodecor').addClass('hide');
                                $('.inventory-i').removeClass('hide');
                                var aHtml = template('vp-bag-template', body);
                                if (type == 'refres') {
                                    if (firstRoad == true) {
                                        inventoryJscroll.getContentPane().html(aHtml);
                                        firstRoad = false;
                                    }
                                } else {
                                    inventoryJscroll.getContentPane().append(aHtml);
                                }
                                if (inventoryJscroll.getContentPane().height() <= 300) {
                                    $('#vp-bag-img').height(inventoryJscroll.getContentPane().height());
                                } else {
                                    $('#vp-bag-img').height(300);
                                }
                                $('.inventory-i .decor-img').addClass('csgo-decor-img');
                                $('.decor-num .decor-pic .decor-img').each(function() {
                                    var val = $(this).find('input').val();
                                    $('.inventory-i .decor-img').each(function() {
                                        var _this = $(this);
                                        var thisVal = _this.find('input').val();
                                        if (val == thisVal) {
                                            _this.remove();
                                        }
                                    });
                                });
                                inventoryJscroll.reinitialise();
                            } else {
                                if (firstRoad == true) {
                                    $('.nodecor').removeClass('hide');
                                }
                                isover = true;
                            }
                        }
                    } else {
                        if (firstRoad == true) {
                            $('.inventory-i').addClass('hide');
                            $('.nodecor').removeClass('hide');
                        }
                        if(body.status == 403){//账户异常
                            window.location.href = CONFIG.sso_login + window.location.href;
                            return;
                        }
                    }
                },
                error: function() {
                }
            });
        }
        //稀有度分类
        $('.decor-class-select.rarity-select').change(function() {
            firstRoad = true;
            isover = false;
            rarity = $(this).val();
            headInventory();
            initInventory();
        });
        //材质分类
        $('.decor-class-select.quality-select').change(function() {
            firstRoad = true;
            isover = false;
            quality = $(this).val();
            headInventory();
