<script type="text/javascript" src="http://static.dota2.vpgcdn.com/js/match/match.min.js"></script>
<script type="text/javascript" src="http://static.dota2.vpgcdn.com/js/quiz.js?v=201703312123"></script>
<script>
    var steam_match_id = 0;
    var isCloseVirtual = 'no';
    var loadersOpts = {
        scale:0.5,
        color: '#fff'
    };
    var guessLoaders = new Spinner(loadersOpts).spin();
    template.helper('imgHasHttp', function(src, type, width, height) {
        return imgHasHttp(src, type, width, height);
    });
    template.helper('parseInt', function(content) {
        // 处理字符串...
        return parseInt(content);
    });
    template.helper('toThousands', function(num) {
        num = (num || 0).toString();
        var result = '';
        while (num.length > 3) {
            result = ',' + num.slice(-3) + result;
            num = num.slice(0, num.length - 3);
        }
        if (num) { result = num + result; }
        return result;
    });
    function thousands(num){
        num = (num || 0).toString();
        var result = '';
        while (num.length > 3) {
            result = ',' + num.slice(-3) + result;
            num = num.slice(0, num.length - 3);
        }
        if (num) { result = num + result; }
        return result;
    }
    Chart.types.Line.extend({
        name: 'MMRLine',
        initialize: function(data) {
            Chart.types.Line.prototype.initialize.apply(this, arguments);
            this.originalClear = this.clear;
            this.mmr = MMRCanvas;
            this.mmr();
            this.clear = function() {
                this.mmr(true);
            };
        }
    });

    function MMRCanvas(clear) {
        if (clear) {
            this.originalClear();
        }
        ;
        var yHeight = this.scale.endPoint;
        this.chart.ctx.beginPath();
        this.chart.ctx.strokeStyle = "#ffffff";
        this.chart.ctx.lineWidth = 1;
        this.chart.ctx.moveTo(this.scale.xScalePaddingLeft - 1, this.scale.startPoint);
        this.chart.ctx.lineTo(this.scale.xScalePaddingLeft - 1, yHeight);
        this.chart.ctx.moveTo(this.scale.xScalePaddingLeft - 2, yHeight);
        this.chart.ctx.lineTo(this.scale.width, yHeight);
        this.chart.ctx.stroke();
    }

    Chart.types.Line.extend({
        // Passing in a name registers this chart in the Chart namespace in the same way
        name: "LineAlt",
        initialize: function(data) {
            Chart.types.Line.prototype.initialize.apply(this, arguments);
            this.originalClear = this.clear;
            this.Alt = LineAltCanvas;
            this.Alt();
            this.clear = function() {
                this.Alt(true);
            };
        }
    });

    function LineAltCanvas(clear){
        if(clear){
            this.originalClear();
        }
        // 1 x scale unit
        var yTop = this.scale.startPoint;
        var canvasWidth = this.scale.width - this.scale.xScalePaddingLeft;
        var yHeight = this.scale.endPoint - this.scale.startPoint;
        // change your color here
        var my_gradient = this.chart.ctx.createLinearGradient(0, 0,canvasWidth, 0);
        my_gradient.addColorStop(0, "#455238");
        my_gradient.addColorStop(1, "#34393f");
        this.chart.ctx.fillStyle = my_gradient;
        this.chart.ctx.fillRect(this.scale.xScalePaddingLeft, yTop,canvasWidth, yHeight / 2);
        this.chart.ctx.beginPath();
        this.chart.ctx.lineCap = "butt";
        this.chart.ctx.moveTo(this.scale.xScalePaddingLeft, this.scale.startPoint);
        this.chart.ctx.lineTo(this.scale.xScalePaddingLeft, (yTop + yHeight / 2));
        this.chart.ctx.lineWidth = 1;
        this.chart.ctx.strokeStyle = '#8ab71b';
        this.chart.ctx.stroke();
        // we shift it by half a x scale unit to the left because the space between gridline is actually a shared space
        var my_gradient1 = this.chart.ctx.createLinearGradient(0, 0, canvasWidth, 0);
        my_gradient1.addColorStop(0, "#55393f");
        my_gradient1.addColorStop(1, "#34393f");
        this.chart.ctx.fillStyle = my_gradient1;
        this.chart.ctx.fillRect(this.scale.xScalePaddingLeft, yTop + yHeight / 2,canvasWidth, yHeight / 2);
        this.chart.ctx.beginPath();
        this.chart.ctx.lineCap = "butt";
        this.chart.ctx.moveTo(this.scale.xScalePaddingLeft, (yTop + yHeight / 2));
        this.chart.ctx.lineTo(this.scale.xScalePaddingLeft, this.scale.endPoint);
        this.chart.ctx.lineWidth = 1;
        this.chart.ctx.strokeStyle = '#dd3c3d';
        this.chart.ctx.stroke();
        this.chart.ctx.beginPath();
        this.chart.ctx.strokeStyle = 'rgba(255,255,255,0.5)';
        this.chart.ctx.moveTo(this.scale.xScalePaddingLeft, (yTop + yHeight / 2));
        this.chart.ctx.lineTo(this.scale.width, (yTop + yHeight / 2));
        this.chart.ctx.lineWidth = 1;
        this.chart.ctx.stroke();

        for (var i = 0; i < 5; i++) {
            this.chart.ctx.font = '12px Arial';
            this.chart.ctx.fillStyle = '#afb0b2';
            if (i === 0) {
                this.chart.ctx.fillText('00:00', this.scale.xScalePaddingLeft + 30, yHeight + yTop + 12);
            } else if (i === 4) {
                this.chart.ctx.fillText(arrive_timer_format(window.LineAltData.body[window.LineAltData.body.length - 1].duration), this.scale.xScalePaddingLeft + canvasWidth, yHeight + yTop + 12);
            } else {
                var duration = (window.LineAltData.body[window.LineAltData.body.length - 1].duration / 5) * i;
                this.chart.ctx.fillText(arrive_timer_format(parseInt(duration)), this.scale.xScalePaddingLeft + 40 + canvasWidth / 5 * i, yHeight + yTop + 12);
            }
        }
    }

    function arrive_timer_format(s) {
        var t = '';
        if (s > -1) {
            var hour = Math.floor(s / 3600);
            var min = Math.floor(s / 60) % 60;
            var sec = s % 60;
            var day = parseInt(hour / 24);
            if (day > 0) {
                hour = hour - 24 * day;
                t = day + "day " + hour + ":";
            }
            else if (hour > 0)
                t = hour + ":";
            if (min < 10) {
                t += "0";
            }
            t += min + ":";
            if (sec < 10) {
                t += "0";
            }
            t += sec;
        }
        return t;
    }


