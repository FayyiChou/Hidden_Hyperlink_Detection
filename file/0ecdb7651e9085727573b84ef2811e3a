    $(function() {
        var time_count;
        if(game_time - current_time - lock_time*60 > 0 && game_time - current_time - lock_time*60 < 120){
            time_count = game_time - current_time - lock_time*60;
            setInterval(function timeDown(){
                time_count-=1;
                $('.match-detail-countdown').each(function(){
                    if(time_count > 0){
                        $(this).removeClass("green").html("距离竞猜截止：" + time_count + " S");
                    }else{
                        $(this).removeClass("green").html("竞猜已截止");
                    }
                })
            },1000);
        }else if(game_time - current_time > 0 && game_time - current_time < lock_time*60){
            $('.match-detail-countdown').removeClass("green").html("竞猜已截止");
        }
