        //达人评论
        var fisrtClick=true;
        $(document).on('click','.like-hate-btn',function(){
            if(fisrtClick==true){
                fisrtClick=false;
                var _this=$(this);
                var pid=_this.attr('data-pid');
                var type=_this.attr('data-type');
                $.ajax({
                url: 'http://www.vpgame.com/webservice/v2/match/comment/like',
                type: "post",
                dataType: 'json',
                data: {
                    'id':pid,
                    'match_id':schedule_id, // 比赛ID
                    'type':type,
                    'session':CONFIG.session_id,
                    'lang':lang
                },
                success: function(body) {
                    if (body.status == 200) {
                        var html=parseInt(_this.find('.like-hate-num').html());
                        _this.find('.like-hate-num').html(html+body.body.items.num);
                        if(_this.hasClass('active')){
                            _this.removeClass('active');
                        }else{
                            _this.addClass('active');
                        }
                        fisrtClick=true;
                    }else{
                        $.popModal({
                            options: {
                                title: '提示',
                                content: body.message,
                                times: -1
                            },
                            btnType: {
                                btn1: {
                                    name: '确定',
                                    Class: 'btn-info'
                                }
                            }
                        }).addClass('public-modal-bg1');
                        fisrtClick=true;
                    }
                },
                error: function() {
                }
            });
            }
        });
        $(document).on('click','.sent-reply',function(){
            var _this=$(this);
            var pid=_this.attr('data-pid');
            var comment=_this.parent().parent().find('.reply-textarea').val();
            $('.doyen-comment-box').css('height','auto');
            $.ajax({
                url: 'http://www.vpgame.com/webservice/v2/match/comment/reply',
                type: "post",
                dataType: 'json',
                data: {
                    'pid':pid,
                    'match_id':schedule_id,
                    'comment':comment,
                    'session':CONFIG.session_id,
                    'lang':lang
                },
                success: function(body) {
                    if (body.status == 200) {
                        initComment();
                        _this.parent().parent().find('.reply-textarea').val('');
                    }else{
                        $.popModal({
                            options: {
                                title: '提示',
                                content: body.message,
                                times: -1
                            },
                            btnType: {
                                btn1: {
                                    name: '确定',
                                    Class: 'btn-info'
                                }
                            }
                        }).addClass('public-modal-bg1');
                    }
                },
                error: function() {
                }
            });
        });
        $(document).on('click','.doyen-zan',function(){
            var _this=$(this);
            if(_this.parent().parent().find('.second-comment').hasClass('hide')){
                _this.parent().parent().find('.second-comment').removeClass('hide');
                _this.find('.close-reply').addClass('hide');
                _this.find('.open-reply').removeClass('hide');
                sCommentPid=_this.attr('data-pid');
                var i=_this.attr('data-list');
                var total=$(this).attr('data-total');
                getSeComment(i,total);
            }else{
                _this.parent().parent().find('.second-comment').addClass('hide');
                _this.find('.close-reply').removeClass('hide');
                _this.find('.open-reply').addClass('hide');
            }
        });
        $(document).on('click','.click-to-reply',function(){
            $(this).parent().parent().find('.second-input-box').toggleClass('hide');
        });
        $(document).on('click','.click-to-read',function(){
            sCommentPid=$(this).attr('data-pid');
            var i=$(this).attr('data-list');
            var total=$(this).attr('data-total');
            $(this).parent().addClass('hide');
            //$(this).parent().parent().parent().find('.pager').removeClass('hide');
            getSeComment(i,total);
        });
        var hmReply;
        $(document).on('click','.second-reply-btn',function(){
            $(this).parent().parent().parent().parent().find('.second-input-box').removeClass('hide');
            var html=$(this).parent().parent().find('.reply-user-name').html();
            hmReply='@'+html+':';
            $(this).parent().parent().parent().parent().find('.second-comment-input').val(hmReply);
        });
        initComment();
        $(document).on('click','.road-more-comment',function(){
            var height=$('.doyen-comment-box').height();
            $('.doyen-comment-box').css('height',height);
            nextComment();
        });
        var commentPage;
        var sCommentPage;
        var sCommentPid;
        function initComment() {
            commentPage = 1;
            getComment('first');
        };
        function nextComment() {
            commentPage++;
            getComment();
        };
        function getComment(type){
            $.ajax({
                url: 'http://www.vpgame.com/webservice/v2/match/comment/index',
                type: "get",
                dataType: 'json',
                data: {
                    'limit':4,
                    'match_id':schedule_id,
                    'page':commentPage,
                    'session':CONFIG.session_id,
                    'lang':lang
                },
                success: function(body) {
                    if (body.status == 200) {
                        var aHtml = template('doyen-comment-template', body);
                        if(type=='first'){
                            $('#doyen-comment-box').html(aHtml);
                        }else{
                            $('#doyen-comment-box').append(aHtml);
                            var height=$('#doyen-comment-box').outerHeight();
                            $('.doyen-comment-box').animate({height:height})
                        }
                        if(body.body.items.length>=4){
                            $('.road-more-comment').removeClass('hide');
                        }else{
                            $('.road-more-comment').addClass('hide');
                        }
                    }
                },
                error: function() {
                }
            });
        };
        var commentPager=null;
        function getSeComment(i,total){
            $.ajax({
                url: 'http://www.vpgame.com/webservice/v2/match/comment/index',
                type: "get",
                dataType: 'json',
                data: {
                    'limit':5,
                    'pid':sCommentPid,
                    'match_id':schedule_id,
                    'page':sCommentPage,
                    'session':CONFIG.session_id,
                    'lang':lang
                },
                success: function(body) {
                    if (body.status == 200) {
                        var aHtml = template('second-comment-list-template', body);
                        $('.second-comment-list[data-list='+i+']').html(aHtml);
                    }
                    var totalPages=Math.ceil(total / 5);
                    if(totalPages > 1){
                        $('.pagination[data-list='+i+']').show().append('<div id="pager" class="pagination"></div>');
                        commentPager = $('.pagination-pager[data-list='+i+']').twbsPagination({
                            totalPages:totalPages ,
                            initiateStartPageClick: false,
                            first: '|&lt;',
                            startPage:sCommentPage || 1,
                            last: '&gt;|',
                            next: '&gt;',
                            prev: '&lt;',
                            visiblePages: totalPages > 5 ? 5 : totalPages,
                            onPageClick: function (event, page) {
                                sCommentPage=page;
                                getSeComment(i,total);
                            }
                        });
                    }else{
                        $('.pagination').hide();
                    }
                },
                error: function() {
                }
            });
        };

        //个人信息
        $.ajax({
            url: 'http://www.vpgame.com/webservice/v2/user/guess',
            type: "get",
            dataType: 'jsonp',
            data: {
                'type': 'match',
                'session': CONFIG.session_id,
                'lang': lang
            },
            success: function(body) {
                if (body.success == true && uid > 0) {
                    var aHtml = template('people-mess-template', body);
                    $('#people-mess').html(aHtml);
                    $('#people-mess .mess-img img').attr('src', imgHasHttp(imgSrc, 'crop', '30', '30'));
                    $('#people-mess .mess-name').html(nickName);
                    $('#people-mess .mess-gread').addClass('level-'+level);
                    var itemGet = Number($('.week-item-get').html()).toFixed(2);
                    if (itemGet < 0) {
                        var itemGetD = -itemGet;
                        $('.week-item-get').html('-$' + ' ' + itemGetD);
                    } else {
                        $('.week-item-get').html('$' + ' ' + itemGet);

                    }
                    getGoldInventory();
                }
            },
            error: function() {
            }
        });
        function getGoldInventory(){
            $.ajax({
                url: 'http://www.vpgame.com/gateway/v1/user/balance',
                type: "get",
                dataType: 'jsonp',
                data: {
                    'session': CONFIG.session_id
                },
                success: function(body) {
                    if (body.status == 200) {
                        $('.pdou-inventory-num').html(body.body.gold);
                        $('.user-gold-inventory').html(thousands(body.body.gold));
                    }
                },
                error: function() {
                }
            });
        };
        //TV直播
        var tvLiveData;
        if(liveOpen==true){
            tvLive();
            var dota=$('#tv-tab .tw-league').data('category');
            if(dota=='dota'){
                mapAjax();
                twlive();
            }
        }


        function tvLive() {
            if (!tvLiveData) {
                var url = $('#guess-live').attr('data-api-live');
                ajaxRequest(url, {
                    session: CONFIG.session_id,
                    lang: lang
                }, 'get', 'jsonp').success(function(response){
                    if (response.success == true && response.body.length > 0) {
                        var tabs = '';
                        $.each(response.body, function(i, value) {
                            tabs += '<li><a href="#tvlive" data-live="'+ i +'" role="tab" data-toggle="tab" aria-controls="tv">'+ value.name +'</a></li>';
                        });
                        $('#tv-tab').prepend(tabs);
                        tvLiveData = response;
                        $('#tv-tab > li:first-child > a').tab('show');
                        $('#tvlive').html(response.body[0].channel);
                        $('#guess-live').removeClass('hide');
                    }
                });
            }
        }

        $(document).on('shown.bs.tab', '#tv-tab a[aria-controls="tv"]', function(e) {
            var id = $(e.target).attr('href');
            var key = $(e.target).attr('data-live');
            $('#tvlive').html(tvLiveData.body[key].channel);
        });


        //图文直播
        var textLiveScroll;
        $(document).on('click', '.tw-league', function() {
            $('.wz-league-zb').jScrollPane();
        });

        $('#hm-tab > #tw-league > a').on('show.bs.tab', function() {
            $('.wz-league-zb').jScrollPane();
        });

        var isFirst = true;
        var oldMapData;
        //$steam_match_id
        function mapAjax() {
            $.ajax({
                url: 'http://api.dota2.vpgame.com/live/map/?token=ff324afafa355d325b9fb93a7fd8bc98bb7164e5&match_id=0',
                type: "get",
                dataType: "jsonp",
                data: {
                    map_width:175,
                    map_height:175
                },
                success: function(body) {
                    if (body.status == 200) {
                        var response = oldMapData || body;
                        var twHtml = template('tp-league-zb-template', response);
                        $('#tp-league-zb').html(twHtml);
                        $('#guess-live').removeClass('hide');
                        $('#tv-tab .tw-league,#tw.tw-league-zb').removeClass('hide');

                        $.each($('.players-postion'),function(i) {
                            var _this = $(this);
                            var left;
                            var top;
                            var str = 'px';
                            var cssLeft;
                            var cssTop;
                            var psx = parseInt(response.body.players[i].position_x);
                            var psy = parseInt(response.body.players[i].position_y);
                                if (psx <= 0) {
                                    left = 175 - (-psx);
                                    cssLeft = left + str;
                                    _this.css('left',cssLeft);
                                } else {
                                    left = 175 + psx;
                                    cssLeft = left + str;
                                    _this.css('left',cssLeft);
                                }
                                if (psy <= 0) {
                                    top = 175 + (-psy);
                                    cssTop = top + str;
                                    _this.css('top',cssTop);
                                } else {
                                    top = 175 - psy;
                                    cssTop = top + str;
                                    _this.css('top',cssTop);
                                }
                        });
                        oldMapData = body;
                        $.each($('.players-postion'),function(i) {
                            var _this = $(this);
                            var left;
                            var top;
                            var psx = parseInt(body.body.players[i].position_x);
                            var psy = parseInt(body.body.players[i].position_y);
                                left = psx <= 0 ? 175-(-psx) : 175 + psx;
                                top = psy <=0 ? 175 + (-psy) : 175 - psy;
                                _this.animate({'top':top ,'left':left},500);
                        });
                    }
                    var is_live = $('.is_live').val();
                    if ($('#guess-live').is(":visible") && is_live!=0) {
                        setTimeout(mapAjax, 15000);
                    }
                },
                error: function() {
                }
            });
        };

        function twlive() {
            if(isfirst==true){
                var isall = 1;
            }else{
                isall=0;
            }
            var idd = $('#tw-live li:first').data('idd');
            $.ajax({
                url: 'http://api.dota2.vpgame.com/live/matchdetail/?token=ff324afafa355d325b9fb93a7fd8bc98bb7164e5&match_id=0',
                type: "get",
                dataType: "jsonp",
                data: {
                    'isall': isall,
                    'isup': 1,
                    'idd': idd ? idd : '',
                    'lang': lang
                },
                success: function(body) {
                    if (body.status == 200 && body.body) {
                        var scoreHtml=template('live-map-head-template',body);
                        $('#live-map-head').html(scoreHtml);
                        $('#tv-tab .tw-league,#tw.tw-league-zb').removeClass('hide');
                        var twHtml = template('tw-live-template', body);
                        isfirst=false;
                        if (!$('#tw-live>li').length) {
                            $('#tw-live').html(twHtml);
                        } else {
                            $('#tw-live').prepend(twHtml);
                        }
                        $('#guess-live').removeClass('hide');
                        if (!textLiveScroll) {
                            if (!tvLiveData) {
                                $('#tv-tab a[aria-controls="tw"]').tab('show');
                            }
                            textLiveScroll = $('#tw-live-scrollbar').jScrollPane().data('jsp');
                        } else {
                            textLiveScroll.reinitialise();
                        }

                    } else {
                    }
                    var radiant_win = body.body.radiant_win;
                    if (radiant_win == "-1") {
                        setTimeout(twlive, 15000);
                    }
                    ;
                    $('#tw-live li').each(function() {
                        var _this = $(this);
                        var sc = _this.find('.duration-second').html();
                        if (sc.length < 2) {
                            _this.find('.duration-second').html('0' + sc);
                        }
                    });
                },
                error: function() {
                }
            });
        };

        $(document).on('click', '.inventory-img .vp-decor-img', function() {
            $('.item-guess-title').addClass('hide');
            var _this=$(this);
            var len = $('#myForm .decor-pic .decor-img').length;
            var num=0;
            if(_this.hasClass('dota-decor-img')){
                num = parseInt($('#my-dota-bag .back-num').html());
            }else{
                num = parseInt($('#my-bag .back-num').html());
            }
            if (len <= 24-1) {
                var _this = $(this);
                var html = _this.clone();
                var price = 0;
                $('.back-num').html(num - 1);
                _this.remove();
                $('.decor-num .decor-pic').removeClass('hide').append(html);
                $('.decor-num .decor-pic .decor-img').each(function() {
                    price += Number($(this).find('.decor-img-price').html());
                });
                var pl = Number($('.guess-team-btn.active').find('.guess-team-odd').html());
                var getNum = Number(pl * price).toFixed(2);
                $('.item-guess-price').html(price.toFixed(2));
                $('.bean-guess-get').html(getNum);
            } else {
                $.popModal({
                    options: {
                        title: '提示',
                        content: '单次只接受24个饰品的竞猜',
                        times: -1
                    },
                    btnType: {
                        btn1: {
                            name: '确定',
                            Class: 'btn-info'
                        }
                    }
                }).addClass('public-modal-bg1');
            }

        });
        $(document).on('click', '.decor-num .decor-pic .vp-decor-img', function() {
            var _this = $(this);
            var html = _this.clone();
            var price = 0;
            var num = 0;
            if(_this.hasClass('dota-decor-img')){
                num = parseInt($('#my-dota-bag .back-num').html());
                $('#my-dota-bag .back-num').html(num + 1);
                dotaJscroll.getContentPane().append(html);
            }else{
                num = parseInt($('#my-bag .back-num').html());
                $('#my-bag .back-num').html(num + 1);
                inventoryJscroll.getContentPane().append(html);
            }

            _this.remove();
            if($('#myForm .decor-pic .decor-img').length < 1){
                $('.item-guess-title').removeClass('hide');
            }
            $('.decor-num .decor-pic .decor-img').each(function() {
                price += Number($(this).find('.decor-img-price').html());
            });
            var pl = Number($('.guess-team-btn.active').find('.guess-team-odd').html());
            $('.item-guess-price').html(price.toFixed(2));
            $('.bean-guess-get').html((pl * price).toFixed(2));
        });
        $('.gold-history-click a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
            var url=$(this).attr('data-goldguessurl');
            goldGuessHistory(url+'/'+infoId+'/gold');
        });
        $('.item-history-click a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
            var url=$(this).attr('data-itemguessurl');
            itemGuessHistory(url+'/'+infoId+'/item');
        });
    });

    /**
     * @Ajax模块
     */

    function ajaxRequest(url, data, type, dataType,beforesend) {
        return $.ajax({
            url: url,
            data: data,
            type: type,
            dataType: dataType,
            beforeSend:beforesend
        });
    }

    /**
     * @description 竞猜请求
     */
    //饰品竞猜

//p豆竞猜
    $('.pdou-submit').on('click', function() {
        var _btn = $(this).button('roading');
        var gold = parseInt($('.you-guess-num').html());
        var url = $(this).attr('data-api') + '&' + CONFIG.session_id;
        var match_odd_id = $('.guess-team-btn.active').attr('data-odd-id');

        guessRequest(url, {
            user_id: uid,
            match_odd_id: match_odd_id,
            gold: gold,
            session: CONFIG.session_id,
            lang: lang
        }, 'post', 'json',function(body){
            if (body.status == 200) {
                var num = parseInt($('.pdou-inventory-num').html());
                $('.pdou-inventory-num').html(num - gold);
                $('#guess-text').val('');
                $('.number-btn').removeClass('active');
                $('.pdou-guess-get,.you-guess-num').html('0');
            }
            _btn.button('reset');
        });
    });

//取消p豆竞猜

    function closePdouQuiz(url) {
        guessRequest(url, {
            'user_id': uid,
            'session': CONFIG.session_id,
            'lang': lang
        }, 'post', 'json',function(response){
            if (response.body.success) {
                $('#pdou-my-guess li').removeClass();
                $(this).parent().parent().parent().remove();
            }
        });
    };

//取消饰品竞猜

    function closeQuiz(url) {
        guessRequest(url, {
            'user_id': uid,
            'session': CONFIG.session_id,
            'lang': lang
        }, 'post', 'json',function(response){
            if (response.body.success) {
                $('#sp-my-guess li').removeClass();
                $(this).parent().remove();
            } else {

            }
        });
    }

    function guessRequest(url, data, type, dataType, callback) {
        ajaxRequest(url, data, type, dataType).success(function(response){
            if (response.status == 200) {
                $.popModal({
                    options: {
                        title: '提示',
                        content: response.message,
                        times: -1
                    },
                    btnType: {
                        btn1: {
                            name: '确定',
                            Class: 'btn-info'
                        }
                    }
                }).addClass('public-modal-bg1');
                guessHistory();
            } else {
                $.popModal({
                    options: {
                        title: '提示',
                        content: response.message,
                        times: -1
                    },
                    btnType: {
                        btn1: {
                            name: '确定',
                            Class: 'btn-info'
                        }
                    }
                }).addClass('public-modal-bg1');
            }
            try {
                callback(response);
            } catch (e) {

            }
        });
    }

//P豆竞猜记录ajax
    function goldGuessHistory(url) {
        $.ajax({
            url: url,
            type: "get",
            dataType: 'json',
            data: {
                lang:lang
            },
            beforeSend:function(){
                $('#gold-guess-history').html(guessLoaders.el);
            },
            success: function(response) {
                if(response.status==200){
                    var html = template('gold-guess-history-template', response);
                    $('#gold-guess-history').html(html);
                }
            },
            error: function() {

            }
        });
    };
//饰品竞猜记录ajax
    function itemGuessHistory(url) {
        $.ajax({
            url: url,
            type: "get",
            dataType: 'json',
            data: {
                lang:lang
            },
            beforeSend:function(){
                $('#item-guess-history').html(guessLoaders.el);
            },
            success: function(response) {
                if(response.status==200){
                    var html = template('item-guess-history-template', response);
                    $('#item-guess-history').html(html);
                }
            },
            error: function() {

            }
        });
    };
    /**
     * @description 比赛前瞻渲染
     */
    function gameProspective(leftUrl, rightUrl, listUrl) {
        //比赛左前瞻
        $('.no-qzlist').addClass('hide');
        $('.guess-sj-content').find(guessLoaders.el).remove();
        $('#left-graph').append(guessLoaders.el);
        var MMRLeft = ajaxRequest(leftUrl, '', 'get', 'jsonp').success(function(response){
            $('#left-graph').find(guessLoaders.el).remove();
            if (response.status == 200) {
                var aHtml = template('graph-template', response);
                $('#left-graph').html(aHtml);
                if (response.body.score_history.length > 0) {
                    var myData = new Array;
                    for (var i = 0; i < response.body.score_history.length; i++) {
                        var score = response.body.score_history[i].score;
                        myData[i] = score;
                    }
                    var max = Math.max.apply(null, myData);
                    var min = Math.min.apply(null, myData);

                    var obj = $('#left-graph').find('.qz-ateam-canvas').get(0);
                    mmrCtx(obj, myData, max, min);
                }else{
                    $('.all-qz-list').addClass('hide');
                    $('.no-qzlist').removeClass('hide');
                }
            }else{
                $('.all-qz-list').addClass('hide');
                $('.no-qzlist').removeClass('hide');
            }
        });
        //比赛右前瞻
        var MMRRright = ajaxRequest(rightUrl, '', 'get', 'jsonp').success(function(response){
            if (response.status == 200) {
                var aHtml = template('graph-template', response);
                $('#right-graph').html(aHtml);
                if (response.body.score_history.length > 0) {
                    var myData = new Array;
                    for (var i = 0; i < response.body.score_history.length; i++) {
                        var score = response.body.score_history[i].score;
                        myData[i] = score;
                    }
                    var max = Math.max.apply(null, myData);
                    var min = Math.min.apply(null, myData);

                    var obj = $('#right-graph').find('.qz-ateam-canvas').get(0);
                    mmrCtx(obj, myData, max, min);
                }else{
                    $('.all-qz-list').addClass('hide');
                    $('.no-qzlist').removeClass('hide');
                }
            }else{
                $('.all-qz-list').addClass('hide');
                $('.no-qzlist').removeClass('hide');
            }
        });
        //比赛前瞻列表
        var MMRList = ajaxRequest(listUrl, {
            lang: lang
        }, 'get', 'jsonp').success(function(response){
            if (response.status == 200) {
                var aHtml = template('history-match-template', response);
                $('#history-match').html(aHtml);
                //.history-head .pull-left.progress-bar-danger
                var team1 = parseInt(response.body.team_win_num1);
                var team2 = parseInt(response.body.team_win_num2);
                var leftWidth = Number((team1 / (team2 + team1)) * 100);
                if (leftWidth <= 10) {
                    leftWidth = 10;
                } else if (leftWidth >= 90) {
                    leftWidth = 90;
                }
                var rightWidth = 100 - leftWidth;
                $('.history-match .history-head .pull-left.progress-bar-danger').css('width', leftWidth + '%');
                $('.history-match .history-head .pull-right.progress-bar-danger').css('width', rightWidth + '%');
            }
        });
        $.when(MMRLeft,MMRRright,MMRList).done(function(){
            $('#left-graph').find(guessLoaders.el).remove();
        }).fail(function(){
            $('#left-graph').find(guessLoaders.el).remove();
        });
    }

    /**
     * @description 比赛前瞻chart
     */
    function mmrCtx(obj, data, max, min) {
        var array = new Array;
        for (var i = 0; i < data.length; i++) {
            array[i] = " ";
        }
        ;
        var lineChartData = {
            labels: array,
            datasets: [
                {
                    fillColor: "rgba(66,118,166,0.5)", //曲线颜色
                    strokeColor: "rgba(75,107,137,1)",
                    pointColor: "rgba(60,87,116,1)",
                    pointStrokeColor: "#33393F",
                    data: data
                }
            ]
        };
        if (/msie/.test(navigator.userAgent.toLowerCase()) && document.documentMode == 8) {
            obj = window.G_vmlCanvasManager.initElement(obj);
        }
        var ctx1 = obj.getContext("2d");
        var steps = data.length > 5 ? 5 : data.length;
        window.ateamLine = new Chart(ctx1).MMRLine(lineChartData, {
            scaleLineWidth: 1, //刻度线宽度
            scaleLineColor: "rgba(0,0,0,0)", //刻度线颜色
            responsive: true,
            scaleShowVerticalLines: false, //Y轴线
            scaleOverride: true,
            showTooltips: true,
            animation: false,
            pointDotRadius: 3, //半径
            scaleStepWidth: parseInt((max - min) / steps), //加
            scaleStartValue: min, //起始值
            pointDotStrokeWidth: 1, //圆边框宽度
            scaleSteps: 5, //行数
            scaleGridLineColor: "rgba(67,72,78,1)", //网线颜色
            scaleFontColor: '#fff',
            bezierCurveTension: 0,
            tooltipXPadding: 6,
            tooltipFontSize: 12,
            tooltipCaretSize: 3,
            tooltipTemplate: "<%= value %>"
        });
    };


    /**
     * @description 我的竞猜
     */

    function guessHistory() {
        var historyurl = CONFIG.api_domain + '/v1/user/' + uid + '/match/' + infoId;
        ajaxRequest(historyurl, {
            'session': CONFIG.session_id,
            'lang': lang
        }, 'get', 'json',function(){
            $('.guess-sj-content').find(guessLoaders.el).remove();
            $('#my-guess').append(guessLoaders.el);
        }).success(function(response){
            $('#my-guess').find(guessLoaders.el).remove();
            if (response.status == 200) {
                var bHtml = template('user-guess-history-template', response);
                $('#user-guess-history').html(bHtml);
            }
            if (isStart == 'yes'){
                $('.revise-team,.cancel-quiz').addClass('hide');
            }
            if (allowCancel != 'yes') {
                $('.cancel-quiz').addClass('hide');
            }
        }).error(function(){
            $('#my-guess').find(guessLoaders.el).remove();
        });
    };

    /**
     * @description  实时数据
     */
    //选手状态排序
    function compareF(up_or_down, key) {
        return function(a, b) {
            if (up_or_down == true) {
                return a[key] - b[key]
            } else {
                return b[key] - a[key]
            }
        }
    }
    function compareH(up_or_down, key) {
        return function(a, b) {
            if (up_or_down == true) {
                return (parseInt(a.gold) + parseInt(a.gold_spent))[key] - (parseInt(b.gold) + parseInt(b.gold_spent))[key]
            } else {
                return (parseInt(b.gold) + parseInt(b.gold_spent))[key] - (parseInt(a.gold) + parseInt(a.gold_spent))[key]
            }
        }
    }
    (function() {
        var up_or_down = true;
        var DataLive = {
            Match:null,
            Banpick:null,
            Canvas:null,
            oldMatch:null,
            init:function(){
              //this.Banpick = null;
              //this.Match = null;
            },
            render:function(){
                if(this.Banpick && this.Match && this.Canvas){
                    var dataLiveHtml = template('data-live-template',this);
                    $('#data-live').html(dataLiveHtml);
                    window.LineAltData = this.Canvas;
                    var goldData = new Array;
                    var xpData = new Array;
                    var absData = new Array;
                    var absXP = new Array;
                    for (var i = 0; i < this.Canvas.body.length; i++) {
                        var radiant_gold = this.Canvas.body[i].radiant_total_gold;
                        var dire_gold = this.Canvas.body[i].dire_total_gold;
                        $('.radiant-total-gold').html(radiant_gold);
                        $('.dire-total-gold').html(dire_gold);
                        goldData[i] = radiant_gold - dire_gold;
                        absData[i] = Math.abs(goldData[i]);
                    };
                    var max = Math.max.apply(null, absData);
                    if (max % 3 === 1) {
                        max = max + 2;
                    } else if (max % 3 === 2) {
                        max = max + 1;
                    }
                    var min = max - 2 * max;
                    for (var i = 0; i < this.Canvas.body.length; i++) {
                        var radiant_xp = this.Canvas.body[i].radiant_total_xp;
                        var dire_xp = this.Canvas.body[i].dire_total_xp;
                        $('.radiant-total-xp').html(radiant_xp);
                        $('.dire-total-xp').html(dire_xp);
                        xpData[i] = radiant_xp - dire_xp;
                        absXP[i] = Math.abs(xpData[i]);
                    }
                    ;
                    var xpMax = Math.max.apply(null, absXP);
                    if (xpMax % 3 === 1) {
                        xpMax = xpMax + 2;
                    } else if (xpMax % 3 === 2) {
                        xpMax = xpMax + 1;
                    }
                    var xpMin = xpMax - 2 * xpMax;
                    realTimeChart(goldData, xpData, max, min, xpMax, xpMin);
                    this.init();
                }
            }
        };
        $(document).on('click','.end-games-table thead tr th',function() {
            var _this = $(this);
            var notThis = $('.end-games-table thead tr th').not(_this);
            _this.addClass('active');
            _this.find('.glyphicon-show').addClass('glyphicon').toggleClass('active');
            notThis.removeClass('active');
            notThis.find('.glyphicon-show').removeClass('glyphicon glyphicon-arrow-up glyphicon-arrow-down active');
        });
        $(document).on('click','.end-games-gold',function() {
            var _this = $(this);
            if (_this.find('.glyphicon-show').hasClass('active')) {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-up').addClass('glyphicon-arrow-down');
            } else {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-down').addClass('glyphicon-arrow-up');
            }
            up_or_down = !up_or_down;
            DataLive.oldMatch.body.players.sort(compareF(up_or_down, 'gold'));
            $('#player-details').empty();
            var bHtml = template('player-details-tr-template', DataLive.oldMatch);
            $('#player-details').html(bHtml);
            $('.player-details-gold').addClass('gold-bg');
            $('.player-details-kda,.player-details-goldall,.player-details-level,.player-details-gpm,.player-details-lh').removeClass('gold-bg');
        });
        $(document).on('click','.end-games-net',function() {
            var _this = $(this);
            if (_this.find('.glyphicon-show').hasClass('active')) {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-up').addClass('glyphicon-arrow-down');
            } else {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-down').addClass('glyphicon-arrow-up');
            }
            up_or_down = !up_or_down;
            DataLive.oldMatch.body.players.sort(compareF(up_or_down, 'gold_spent'));
            $('#player-details').empty();
            var bHtml = template('player-details-tr-template', DataLive.oldMatch);
            $('#player-details').html(bHtml);
            $('.player-details-goldall').addClass('gold-bg');
            $('.player-details-kda,.player-details-gold,.player-details-level,.player-details-gpm,.player-details-lh').removeClass('gold-bg');
        });
        $(document).on('click','.end-games-level',function() {
            var _this = $(this);
            if (_this.find('.glyphicon-show').hasClass('active')) {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-up').addClass('glyphicon-arrow-down');
            } else {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-down').addClass('glyphicon-arrow-up');
            }
            up_or_down = !up_or_down;
            DataLive.oldMatch.body.players.sort(compareF(up_or_down, 'level'));
            $('#player-details').empty();
            var bHtml = template('player-details-tr-template', DataLive.oldMatch);
            $('#player-details').html(bHtml);
            $('.player-details-level').addClass('gold-bg');
            $('.player-details-kda,.player-details-gold,.player-details-goldall,.player-details-gpm,.player-details-lh').removeClass('gold-bg');
        });
        $(document).on('click','.end-games-kda',function() {
            var _this = $(this);
            if (_this.find('.glyphicon-show').hasClass('active')) {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-up').addClass('glyphicon-arrow-down');
            } else {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-down').addClass('glyphicon-arrow-up');
            }
            up_or_down = !up_or_down;
            DataLive.oldMatch.body.players.sort(compareF(up_or_down, 'kills'));
            $('#player-details').empty();
            var bHtml = template('player-details-tr-template', DataLive.oldMatch);
            $('#player-details').html(bHtml);
            $('.player-details-kda').addClass('gold-bg');
            $('.player-details-level,.player-details-gold,.player-details-goldall,.player-details-gpm,.player-details-lh').removeClass('gold-bg');
        });
        $(document).on('click','.end-games-gpm',function() {
            var _this = $(this);
            if (_this.find('.glyphicon-show').hasClass('active')) {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-up').addClass('glyphicon-arrow-down');
            } else {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-down').addClass('glyphicon-arrow-up');
            }
            up_or_down = !up_or_down;
            DataLive.oldMatch.body.players.sort(compareF(up_or_down, 'gold_per_min'));
            $('#player-details').empty();
            var bHtml = template('player-details-tr-template', DataLive.oldMatch);
            $('#player-details').html(bHtml);
            $('.player-details-gpm').addClass('gold-bg');
            $('.player-details-level,.player-details-gold,.player-details-goldall,.player-details-kda,.player-details-lh').removeClass('gold-bg');
        });
        $(document).on('click','.end-games-lh',function() {
            var _this = $(this);
            if (_this.find('.glyphicon-show').hasClass('active')) {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-up').addClass('glyphicon-arrow-down');
            } else {
                _this.find('.glyphicon-show').removeClass('glyphicon-arrow-down').addClass('glyphicon-arrow-up');
            }
            up_or_down = !up_or_down;
            DataLive.oldMatch.body.players.sort(compareF(up_or_down, 'last_hits'));
            $('#player-details').empty();
            var bHtml = template('player-details-tr-template', DataLive.oldMatch);
            $('#player-details').html(bHtml);
            $('.player-details-lh').addClass('gold-bg');
            $('.player-details-level,.player-details-gold,.player-details-goldall,.player-details-kda,.player-details-gpm').removeClass('gold-bg');
        });


        //选手状态接口
        template.helper('progress',function(data,other){
            if (data == 0 && other == 0) {
                var dataWidth = 50;
                var otherWidth = 50;
            } else {
                var all = parseInt(data) + parseInt(other);
                var dataWidth = data / all * 100;
                if (dataWidth <= 10) {
                    dataWidth = 10;
                } else if (dataWidth >= 90) {
                    dataWidth = 90;
                }
            }
            return dataWidth;
        });

        function matchDetails(matchurl) {
            return $.ajax({
                url: matchurl,
                type: "get",
                dataType: 'jsonp',
                data: {
                    "lang": lang
                },
                beforeSend:function(){
                    $('.guess-sj-content').find(guessLoaders.el).remove();
                    $('#data-live').append(guessLoaders.el);
                },
                success: function(response) {
                    $('#data-live').find(guessLoaders.el).remove();
                    if(DataLive.Match){
                        var tbody = template('player-details-tr-template',response);
                        $('#player-details').html(tbody);
                    }else{
                        DataLive.Match = response;
                        DataLive.oldMatch = response;
                        DataLive.render();
                    }
                },
                error: function() {
                    $('#data-live').find(guessLoaders.el).remove();
                }
            });
        }
        function realTime(banpickurl, liveurl, matchurl) {
            //banpick
            ajaxRequest(banpickurl, '', 'get', 'jsonp').success(function(response){
                DataLive.Banpick = response;
                DataLive.render();
                if($('#match-banpick li').length==20){
                    clearInterval(DataLive.canvas_timer);
                }
            });
            if(liveurl){
                //经济差经验差
                ajaxRequest(liveurl, '', 'get', 'jsonp').success(function(response){
                    DataLive.Canvas = response;
                    DataLive.render();
                });
            }
        }

        /**
         * @description 两队差距chart
         */

        function realTimeChart(goldData, xpData, max, min, xpMax, xpMin) {
            var array = new Array;
            for (var i = 0; i < goldData.length; i++) {
                array[i] = "";
            }
            ;
            var xpArray = new Array;
            for (var i = 0; i < xpData.length; i++) {
                xpArray[i] = "";
            }
            ;
            var lineChartData3 = {
                labels: array,
                datasets: [
                    {
                        fillColor: "rgba(0,0,0,0)",
                        strokeColor: "rgba(171,142,29,1)", //曲线颜色
                        pointColor: "rgba(60,87,116,1)",
                        pointStrokeColor: "#fff",
                        data: goldData
                    }
                ]
            };
            var lineChartData4 = {
                labels: xpArray,
                datasets: [
                    {
                        fillColor: "rgba(0,0,0,0)",
                        strokeColor: "rgba(171,142,29,1)", //曲线颜色
                        pointColor: "rgba(60,87,116,1)",
                        pointStrokeColor: "#fff",
                        data: xpData
                    }
                ]
            };
            var ateam = document.getElementById("ss-ateam-canvas");
            var bteam = document.getElementById("ss-bteam-canvas");
            if (/msie/.test(navigator.userAgent.toLowerCase()) && document.documentMode == 8) {
                ateam = window.G_vmlCanvasManager.initElement(ateam);
                bteam = window.G_vmlCanvasManager.initElement(bteam);
            }
            var leftChart = ateam.getContext("2d");
            var rightChart = bteam.getContext("2d");
            var goldDataSteps = goldData.length > 6 ? 6 : goldData.length;
            window.economicsLine = new Chart(leftChart).LineAlt(lineChartData3, {
                scaleLineWidth: 1, //刻度线宽度
                scaleLineColor: "rgba(0,0,0,0)", //刻度线颜色
                responsive: false,
                scaleOverlay: false,
                scaleShowVerticalLines: false, //Y轴线
                showTooltips: false, //鼠标指向显示
                scaleOverride: true,
                pointDotRadius: 3, //半径
                pointDot: false,
                scaleStepWidth: goldDataSteps > 0 ? parseInt((max - min) / goldDataSteps) : 0, //加
                scaleStartValue: min, //起始值
                pointDotStrokeWidth: 1, //圆边框宽度
                scaleSteps: 6, //行数
                scaleGridLineColor: "rgba(255,255,255,0.08)", //网线颜色
                bezierCurveTension: 0.5, //曲线张力
                scaleFontColor: '#898b8e'
            });
            var xpDataSteps = xpData.length > 6 ? 6 : xpData.length;
            window.experienceLine = new Chart(rightChart).LineAlt(lineChartData4, {
                scaleLineWidth: 1, //刻度线宽度
                scaleLineColor: "rgba(0,0,0,0)", //刻度线颜色
                responsive: false,
                scaleOverlay: false,
                scaleShowVerticalLines: false, //Y轴线
                scaleShowLabels: true,
                showTooltips: false, //鼠标指向显示
                scaleOverride: true,
                pointDotRadius: 3, //半径
                pointDot: false,
                scaleStepWidth: xpDataSteps > 0 ? parseInt((xpMax - xpMin) / xpDataSteps) : 0, //加
                scaleStartValue: xpMin, //起始值
                pointDotStrokeWidth: 1, //圆边框宽度
                scaleSteps: 6, //行数
                scaleGridLineColor: "rgba(255,255,255,0.08)", //网线颜色
                bezierCurveTension: 0.5, //曲线张力
                scaleFontColor: '#898b8e'
            });
        }


        /**
         * @description 竞猜tabs切换
         */
        $('#myTabs > li > a').on('shown.bs.tab', function(e) {
            var $this = $(e.target);
            var id = $this.attr('href'); //获取显示区域的ID
            var type = $this.attr('aria-controls'); //获取显示区域类型
            var url = $this.attr('data-api'); //获取显示区域接口地址
            switch (type) {
                case 'qz': //渲染前瞻
                    var lefturl = $this.attr('data-lefturl');
                    var righturl = $this.attr('data-righturl');
                    var listurl = $this.attr('data-listurl');
                    gameProspective(lefturl, righturl, listurl);
                    clearInterval(DataLive.canvas_timer);
                    break;
                case 'ss':
                    clearInterval(DataLive.canvas_timer);
                    if(steam_match_id != 0){
                        var liveurl = $this.attr('data-liveurl');
                        var banpickurl = $this.attr('data-banpickurl');
                        var matchurl = $this.attr('data-matchurl');
                        realTime(banpickurl, liveurl);//渲染实时数据
                        matchDetails(matchurl);
                        DataLive.canvas_timer = setInterval(function() {
                                realTime(banpickurl);
                        }, 15000);
                    }else{
                        var dataLiveHtml = template('data-live-template',this);
                        $('#data-live').html(dataLiveHtml);
                    }
                    break;
                default :
                    clearInterval(DataLive.canvas_timer);
                    break;
            }

        });

        function spinachTabs() {
            var tabsDom = $('#myTabs');
        }
        $(document).on('click','.freshen',function() {
            realTime($('#myTabs a[aria-controls=ss]').attr('data-banpickurl'), $('#myTabs a[aria-controls=ss]').attr('data-liveurl'));
            matchDetails($('#myTabs a[aria-controls=ss]').attr('data-matchurl'));
        });
    })();




    $(function() {

        $('#tv-tab li a[aria-controls="tw"]').on('shown.bs.tab', function(){
            $('#myTabs > li > a[aria-controls="ss"]').tab('show');
        });
        $('.button-loading').on('click', function() {
            $(this).button('loading');
        });
    });
</script>

