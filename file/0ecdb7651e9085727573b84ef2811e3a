        });
        //搜索
        $(document).on('click','#input-serch i',function(){
            firstRoad = true;
            isover = false;
            keyword=$('#input-serch input').val();
            headInventory();
            initInventory();
        });
        $('#input-serch input').keyup(function(){
            if($(this).val().length<=0){
                keyword='';
                firstRoad = true;
                isover = false;
                headInventory();
                initInventory();
            }
        });

        //更换队伍
        var quizType;
        function changeTeam(match_odd_id,btn) {
            $.ajax({
                url: 'http://www.vpgame.com/gateway/v1/match/10145629/change',
                type: "post",
                dataType: 'json',
                data: {
                    'type': quizType,
                    'match_odd_id': match_odd_id,
                    'user_id': uid,
                    'session': CONFIG.session_id,
                    'lang': lang
                },
                success: function(body) {
                    btn.button('reset');
                    if (body.status == 200) {
                        guessHistory();
                        $.popModal({
                            options: {
                                title: '提示',
                                content: body.message,
                                times: -1
                            },
                            btnType: {
                                btn1: {
                                    name: '确定',
                                    Class: 'btn-info'
                                }
                            }
                        }).addClass('public-modal-bg1');
                    } else {
                        $('.btn-confirm').hide();
                        $.popModal({
                            options: {
                                title: '提示',
                                content: body.message,
                                times: -1
                            },
                            btnType: {
                                btn1: {
                                    name: '确定',
                                    Class: 'btn-info'
                                }
                            }
                        }).addClass('public-modal-bg1');
                    }
                },
                error: function() {
                    btn.button('reset');
                }
            });
        };
        //右侧更换队伍
        $(document).on('click','.revise-team',function(){
            var matchOddId=$(this).attr('data-oddid');
            $('#change-team-modal').modal('show');
            $('.change-team-list').removeClass('active');
            $('.change-team-list[data-odd-id='+ matchOddId +']').addClass('active');
            $('.team-zcl-width').each(function(){
                $(this).css('width',$(this).attr('data-width'));
            });
            quizType=$(this).attr('data-type');
            if(quizType=='item'){
                $('.modal-gold-odds').addClass('hide');
                $('.modal-item-odds').removeClass('hide');
            }else{
                $('.modal-item-odds').addClass('hide');
                $('.modal-gold-odds').removeClass('hide');
            }
        });
        $(document).on('click','.change-team-list',function(){
            var _this=$(this);
            var notThis=$('.change-team-list').not(_this);
            _this.addClass('active');
            notThis.removeClass('active');
        });
        $(document).on('click','.change-team-modal .exchange-btn',function(){
            var _btn = $(this).button('loading');
            var id=$('.change-team-list.active').attr('data-odd-id');
            changeTeam(id,_btn);
        });
        //取消竞猜
        $(document).on('click', '.cancel-quiz', function() {
            var url = $(this).attr('data-url');
            var content;
            if (allowCancel == 'yes') {
                content = "确定要取消这场竞猜吗?<span class='text-color-red'>（注意：距离比赛开始前10分钟内不可取消！）</span>";
                $.popModal({
                    options: {
                        title: '提示',
                        content: content,
                        type: 'confirm'
                    },
                    btnType: {
                        btn1: {
                            name: '确定',
                            Class: 'btn-info btn-yes'
                        },
                        btn2: {
                            name: '返回',
                            Class: 'btn-danger'
                        }
                    }
                }).addClass('public-modal-bg1');
                $('.public-modal .modal-footer .btn.btn-yes').click(function() {
                    $('.public-modal-bg1').modal('hide');
                    closeQuiz(url);
                });
            } else {
                content = "比赛已经开始或者马上开始，不能取消竞猜";
                $.popModal({
                    options: {
                        title: '提示',
                        content: content,
                        times: -1
                    },
                    btnType: {
                        btn1: {
                            name: '确定',
                            Class: 'btn-info'
                        }
                    }
                }).addClass('public-modal-bg1');
            };
        });
        $(document).on('click', '#sp-my-guess .close-quiz', function() {
            var url = $(this).attr('url');
            var content;
            if (allowCancel == 'yes') {
                content = "确定要取消这场竞猜吗?<span class='text-color-red'>（注意：距离比赛开始前10分钟内不可取消！）</span>";
