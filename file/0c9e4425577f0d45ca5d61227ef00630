		//BAIDU_CLB_fillSlotAsync('756395','ad_0_3_0_9');//640*90
		if($("#ad_0_3_0_22").has("div").length == 0){
			BAIDU_CLB_fillSlotAsync('417795','ad_0_3_0_22');
		}
		if($("#ad_0_3_0_4").has("div").length == 0){
			BAIDU_CLB_fillSlotAsync('299816','ad_0_3_0_4');
		}
		if($("#ad_game_1").length > 0){
			BAIDU_CLB_fillSlotAsync('1110130','ad_game_1');
			BAIDU_CLB_fillSlotAsync('1110131','ad_game_2');
			BAIDU_CLB_fillSlotAsync('1110132','ad_game_3');
			BAIDU_CLB_fillSlotAsync('1110133','ad_game_4');
		}
});
