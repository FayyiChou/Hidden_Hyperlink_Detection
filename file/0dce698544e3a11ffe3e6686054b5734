                                        <script>
                                            var speedleft = 20
                                            var FGscrollleft = document.getElementById('scrollbox');
                                            var FGscrollleft1 = document.getElementById('scrollleft1');
                                            var FGscrollleft2 = document.getElementById('scrollleft2');
                                            FGscrollleft2.innerHTML = FGscrollleft1.innerHTML
                                            function Marqueeleft() {
                                                if (FGscrollleft2.offsetWidth - FGscrollleft.scrollLeft <= 0)
                                                    FGscrollleft.scrollLeft -= FGscrollleft1.offsetWidth
                                                else {
                                                    FGscrollleft.scrollLeft++
                                                }
                                            }
                                            var MyMarleft = setInterval(Marqueeleft, speedleft)
                                            FGscrollleft.onmouseover = function() { clearInterval(MyMarleft) }
                                            FGscrollleft.onmouseout = function() { MyMarleft = setInterval(Marqueeleft, speedleft) }
                                        </script>

