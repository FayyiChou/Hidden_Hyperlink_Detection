  var speed=20 
  marquePic2.innerHTML=marquePic1.innerHTML 
  function Marquee()
    {if(demo.scrollLeft>=marquePic1.scrollWidth)
       {demo.scrollLeft=0 }
	 else
	   {demo.scrollLeft++ } 
     } 
  var MyMar=setInterval(Marquee,speed) 
  demo.onmouseover=function()
    {clearInterval(MyMar)} 
  demo.onmouseout=function()
    {MyMar=setInterval(Marquee,speed)} 
