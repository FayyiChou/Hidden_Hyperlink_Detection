});
}

function delanswer(aid,qid){
if(confirm("确定删除该回答?")){
document.location.href='http://sos.45it.com/?question/deleteanswer/'+aid+'/'+qid+'.html';
}
}
function verifyanswer(aid,qid){
document.location.href='http://sos.45it.com/?question/verifyanswer/'+aid+'/'+qid+'.html';
}

function inform(name,type){
var content = name+'的回答';
if(type==1){
content = name+'的提问';
}
}

function showuserinfo(uid){
$("#userinfo-"+uid).load("http://sos.45it.com/?user/ajaxuserinfo/"+uid+".html");
}
function closeuserinfo(uid){
$("#userinfo-"+uid).html("");
}
function add_comment(type,aid,uid){
if(0==0){
alert("登陆之后才能进行该操作！");
return false;
}else if(0 == uid){
alert("不能给自己回答评分!");
return false;
}
var optionsstr = '<select name="credit3"><option value="1">+1</option><option value="2">+2</option><option value="3">+3</option><option value="4">+4</option><option value="5">+5</option><option value="6">+6</option><option value="7">+7</option><option value="8">+8</option><option value="9">+9</option><option value="10">+10</option></select>';
if(type == 0)
optionsstr = '<select name="credit3"><option value="-1">-1</option><option value="-2">-2</option><option value="-3">-3</option><option value="-4">-4</option><option value="-5">-5</option><option value="-6">-6</option><option value="-7">-7</option><option value="-8">-8</option><option value="-9">-9</option><option value="-10">-10</option></select>';

$.dialog({
id:'add_commentdiv',
position:'center',
align:'left',
width:200,
height:110,
title:'回答评分',
fnOk:function(){
if("" == $.trim($("#shortcomment").val())){
alert("简评内容不能为空!");
return false;
}
document.addcommentForm.submit();
$.dialog.close('add_commentdiv')
},
fnCancel:function(){$.dialog.close('add_commentdiv')},
content:'<p /><form name="addcommentForm" id="addcommentForm" action="http://sos.45it.com/?question/answercomment.html" method="POST"><input type="hidden" name="qid" value="'+1565+'" /><input type="hidden" name="aid" value="'+aid+'" /><input type="hidden" name="touid" value="'+uid+'" /><table cellspacing="0" cellpadding="0" width="80%" border="0" valign="top" align="center"><tr><td class="f14" valign="top" nowrap align="left" width="40%" height="35">魅力值 :&nbsp;&nbsp; </td><td valign="top" width="60%" height="35">'+optionsstr+'</td></tr><tr><td class="f14" valign="top" nowrap align="left" height="35"> 简&nbsp&nbsp评 :&nbsp;&nbsp; </td><td valign="top" height="35"><textarea id="shortcomment" name="content" cols="30"  rows="3" maxlength="30"></textarea></td></tr></table></form>'
});
}
$(document).ready(function () {
$("legend a").fancybox({'titlePosition':'inside'});
SyntaxHighlighter.highlight();
/*ajax imgpopup*/
$(".wtcont p img").each(function(i){
var img = $(this);
$.ajax({
type: "POST",
url: "http://sos.45it.com/?question/ajaxchkimg.html",
async: true,
data: "imgsrc="+img.attr("src"),
success: function(status){
if('1' == status){
img.wrap("<a href='"+img.attr("src")+"' title='"+img.attr("title")+"' ></a>");
img.parent().fancybox({
'overlayShow'	: false,
'transitionIn'	: 'elastic',
'transitionOut'	: 'elastic',
'titlePosition'	: 'inside'
});
}
}
});
});
});
</script> 
