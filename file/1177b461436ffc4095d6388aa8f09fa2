function showsupply(){
var displaytype = $("#mysupplydiv").css("display");
if(displaytype == 'none'){
$("#mysupplydiv").css("display","");
$("#addscorediv").css("display","none");
}else{
$("#mysupplydiv").css("display","none"); 
}
}
function check_mysupply(){
mysupply.sync();
var content = mysupply.getContentTxt();
if($.trim(content) == ''){
alert('问题补充不能只为图片，必须有文字描述');
return false;
}
$("form[name='supplyForm']").submit();

}
function showaddscore(){
var displaytype = $("#addscorediv").css("display");
if(displaytype == 'none'){
$("#addscorediv").css("display","");
$("#mysupplydiv").css("display","none"); 
}else{
$("#addscorediv").css("display","none");   
}
}
function check_addscore(){
$("form[name='addscoreForm']").submit();
}

function managequest(num){
switch(num){
case 3:
var displaytype = $("#editcategorydiv").css("display");
if(displaytype == 'none'){
$("#edittagdiv").css("display","none");
$("#editcategorydiv").css("display",""); 
}else{
$("#editcategorydiv").css("display","none");   
}
break;
case 4:
if(!confirm('确定关闭该问题?')==false){
document.location.href="http://sos.45it.com/?question/close/1565.html";
}
break;
case 5:
var displaytype = $("#edittagdiv").css("display");
if(displaytype == 'none'){
$("#editcategorydiv").css("display","none"); 
$("#edittagdiv").css("display","");
}else{
$("#edittagdiv").css("display","none");   
}
break;
case 6:
if(!confirm('确定删除问题？该操作不可返回！')==false){
document.location.href="http://sos.45it.com/?question/delete/1565.html";
}
break;
default:
alert("非法操作！");
break;
}
}

function showeditanswer(index){
var displaytype = $("#editanswerdiv"+index).css("display");
if(displaytype == 'none'){
$("#editanswerdiv"+index).css("display","");
//$("#mysupplydiv").css("display","none"); 
}else{
$("#editanswerdiv"+index).css("display","none");   
}
}

function check_editanswer(answereditor,index){
answereditor.sync();
var content=answereditor.getContentTxt();
if(bytes(content)<=10){
alert('回答内容不能少于10个字！');
return false;
}
$("#editanswerForm"+index).submit();
}

function adoptanswer(aid){
$.dialog({
id:'editanswerDiv',
position:'center',
align:'left',
width:400,
height:110,
title:'采纳感言',
fnOk:function(){document.editanswerForm.submit();$.dialog.close('editanswerDiv')},
fnCancel:function(){$.dialog.close('editanswerDiv')},
content:'对该回答的评价： <div><form name="editanswerForm"  action="http://sos.45it.com/?question/adopt.html" method="post" ><textarea id="mc" name="content" style="width: 99%; padding-top: 1px; font-size: 14px;" rows="4" cols="50" >谢谢您的解答！</textarea><input type="hidden"  value="1565" name="qid"/><input type="hidden" id="ma" value="'+aid+'" name="aid"/></form></div>'
});
}
function tagquestion(aid){
$.dialog({
id:'tag_ask',
position:'center',
align:'left',
width:400,
height:110,
title:'继续追问',
fnOk:function(){
if($.trim($("#mc").val()) == ""){
alert("追问内容不能为空");
return false;
}
document.tagAskForm.submit();
$.dialog.close('tag_ask')
},
fnCancel:function(){$.dialog.close('tag_ask')},
content:'追问内容： <div><form name="tagAskForm"  action="http://sos.45it.com/?question/tagask.html" method="post" ><textarea id="mc" name="tag_ask" style="width: 99%; padding-top: 1px; font-size: 14px;" rows="4" cols="300" ></textarea><input type="hidden"  value="1565" name="qid"/><input type="hidden" id="ma" value="'+aid+'" name="aid"/></form></div>'
});
}
