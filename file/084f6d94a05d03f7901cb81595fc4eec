    <script>
	var NewsHtml=""
	NewsHtml="<span style='display:block; float:right;'>2016-11-08</span><a href=/content/?394.html target=blank>2016年常德市全市律师培训班圆满闭幕!</a>|<span style='display:block; float:right;'>2016-10-25</span><a href=/content/?391.html target=blank>云济杨迪清律师出席省律协公司法专委论坛！</a>|<span style='display:block; float:right;'>2016-10-21</span><a href=/content/?390.html target=blank>云济所赵勇刚律师参加“一带一路背景下的中国涉外法律体系”高级研修班！</a>|<span style='display:block; float:right;'>2016-09-14</span><a href=/content/?388.html target=blank>云济所祝大家中秋快乐！</a>|<span style='display:block; float:right;'>2016-09-09</span><a href=/content/?387.html target=blank>云济所组织律师赴长沙参加《民法典（草案）》权威解读学习活动</a>|<span style='display:block; float:right;'>2016-09-09</span><a href=/content/?386.html target=blank>云济所杨迪清主任受邀主讲“全市房地产企业高管法律知识讲座”</a>|<span style='display:block; float:right;'>2016-08-25</span><a href=/content/?385.html target=blank>云济所举办“疑难案件研讨会”</a>|<span style='display:block; float:right;'>2016-08-24</span><a href=/content/?384.html target=blank>律师乡村行，普法零距离</a>|<span style='display:block; float:right;'>2016-08-16</span><a href=/content/?383.html target=blank>云济所杨迪清主任参加全市司法行政系统年中工作推进会</a>|<span style='display:block; float:right;'>2016-07-30</span><a href=/content/?382.html target=blank>湖南省律协携市州惩戒工作调研组莅临湖南云济律师事务所参观指导</a>|<span style='display:block; float:right;'>2016-07-26</span><a href=/content/?381.html target=blank>湖南云济律师事务所举行“新三板、PPP”业务培训活动</a>|<span style='display:block; float:right;'>2016-07-18</span><a href=/content/?380.html target=blank>湖南云济律师事务所支部委员会召开“两学一做”党员生活会</a>|";
	var str=NewsHtml.substr(0,NewsHtml.length-1)
	var strHtml=str.split("|")
	var marqueeContent=new Array();
	for(i=0;i<strHtml.length;i++)
	{
		marqueeContent[i]=(strHtml[i])
	}
	var marqueeInterval=new Array();  //定义一些常用而且要经常用到的变量
	var marqueeId=0;
	var marqueeDelay=4000;
	var marqueeHeight=38;
	function initMarquee() {
	 var str=marqueeContent[0];
	 document.write('<div id=marqueeBox style="overflow:hidden;height:'+marqueeHeight+'px" onmouseover="clearInterval(marqueeInterval[0])" onmouseout="marqueeInterval[0]=setInterval(\'startMarquee()\',marqueeDelay)"><div>'+str+'</div></div>');
	 marqueeId++;
	 marqueeInterval[0]=setInterval("startMarquee()",marqueeDelay);
	 }
	function startMarquee() {
	 var str=marqueeContent[marqueeId];
	  marqueeId++;
	 if(marqueeId>=marqueeContent.length) marqueeId=0;
	 if(marqueeBox.childNodes.length==1) {
	  var nextLine=document.createElement('DIV');
	  nextLine.innerHTML=str;
	  marqueeBox.appendChild(nextLine);
	  }
	 else {
	  marqueeBox.childNodes[0].innerHTML=str;
	  marqueeBox.appendChild(marqueeBox.childNodes[0]);
	  marqueeBox.scrollTop=0;
	  }
	 clearInterval(marqueeInterval[1]);
	 marqueeInterval[1]=setInterval("scrollMarquee()",10);
	 }
	function scrollMarquee() {
	 marqueeBox.scrollTop++;
	 if(marqueeBox.scrollTop%marqueeHeight==marqueeHeight){
	  clearInterval(marqueeInterval[1]);
	  }
	 }
	initMarquee();
	</script>
