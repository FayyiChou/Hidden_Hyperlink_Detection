<script src="/js/shopTemplate.js"></script>
<script>

    var GLOBAL_CONFIG = {
        shopId:null,
        followed:false
        //host:'//shop.m.jd.com/'
    };
    var floorTemplate = $('div[floorType="PD_TEMPLATE"]');
    var floorData = [];
    $.ajax({
        url:'/index/getShopTemplate.json',
        data: {
            shopId : 1000002688
        },
        dataType: "json",
        type:'get',
        success:function(data){
            GLOBAL_CONFIG.shopId = data.shopInfo?data.shopInfo.shopId:null;
            GLOBAL_CONFIG.followed = data.shopInfo?data.shopInfo.followed:null;
            if(data.success && data.data && data.data.length){
                handlerTemplate(data.data);
            }
        },
        error:function(){

        }
    })
    function handlerTemplate(data){
        for (var i = 0, len = data.length; i<len; i++){
            floorData.push({
                ele:CompRender.queryHTMLNode(data[i].dsConfig, data[i].moduleId),
                floorid:data[i].uid
            })
        };
        renderTemplateDom();
    }
    function renderTemplateDom(){
        for(var k=0;k<floorData.length;k++){
            if(floorData[k].floorid === $(floorTemplate[k]).attr('floorId')){
                $(floorTemplate[k]).append(floorData[k].ele);
            }
        }
        handlerEvent();
    }
    function handlerEvent(){
        $('.mo-slider').each(function(){
            var pageshow = '.mo-slider-focus';
            var that = this;
            if(parseInt(this.getAttribute('mo-slider-num')) <= 1 || !this.getAttribute('mo-ispointdisplay')){
                pageshow = null;
            }
            var moSlide = new Swiper(this, {
                wrapperClass : 'mo-slider-wrapper',
                slideClass : 'mo-slider-item',
                pagination: pageshow,
                bulletClass : 'mo-btn',
                bulletActiveClass : 'active',
                autoplay : 3000,
                autoplayDisableOnInteraction : false,
                spaceBetween: parseInt(that.style.padding,10) || 0
            });
        });
    }

</script>

