    <script type="text/javascript">
        $(function() {
            $(window).bind("scroll", $backToTopFun);
            $backToTopFun();

            function $backToTopFun() {
                var st = $(document).scrollTop(), winh = $(window).height();
                if (st > 100) {
                    $('.backtopDiv').show();
                } else {
                    $('.backtopDiv').hide();
                }
            }

            //回到顶部
            $('.backtopDiv').on("click", ".backtopBtn", function() {
                $("html, body").animate({ scrollTop: 0 }, 120);
            });
            //二维码
            $('.WeChatCodeDiv').hover(
                function() {
                    $('.WeChatCode').show();
                }, function() { $('.WeChatCode').hide(); });
            //意见反馈
            $('.BackFeedDiv').hover(function() {
                var _this = $(this);
                timeId2 = setTimeout(function() {
                    _this.children('.BackFeedBox').stop(true, true).show(0);
                }, 100);
            }, function() {
                clearTimeout(timeId2);
                $(this).children('.BackFeedBox').stop(true, true).hide(0);
            });

            $("#submitbtn").click(function() {
                    
                var description = $("#Description").val() + "〖 手机号码：" +  + ",QQ号码:" +  + " 〗";
                
                if ($.trim($("#Description").val()) == "") {
                    $(".fankui_emsg").html("请输入内容");
                } else {
                    $.post("/common/FeedBack", { description: description, categoryId: "110" }, function(e) {
                        $(".fankui_emsg").html(e.Message);
                        $("#Description").val(""); //清空内容
                    });
                }
            });
            $('.autoDiv').hover(function() {
                var _this = $(this);
                _this.children('.contentbox').show(0);
            }, function() {
                var _this = $(this);
                _this.children('.contentbox').hide(0);
            });
        });
    </script>

    <script type="text/javascript">
        $(function() {
            $('input[type=number], input[type=text]').placeholder();
            //一键说明
            $('i.icon_ques').hover(function() {
                $('.onekey_tip').show();
            }, function() {
                $('.onekey_tip').hide();
            });
            //展开
            $('a.btn_expend').click(function() {
                $(this).hide();
                $('.more_option').slideDown(500);
                $('a.btn_fold').show();
            });
            //收起
            $('a.btn_fold').click(function() {
                $(this).hide();
                $('.more_option').slideUp(500);
                $('a.btn_expend').show();
            });
            //表头排序
            $('.tabth li').click(function() {
                if ($(this).hasClass('nochoose')) { //只显未投
                    if ($('#chk').attr('checked') == "checked") {
                        $("#chkIsNoBid").val("on");
                    } else {
                        $("#chkIsNoBid").val("");
                    }
                    $("#formQuery").submit();
                }
                if ($(this).hasClass("selected")) {
                    $(this).find('i').toggleClass('down');
                } else {
                    $(this).addClass('selected');
                    $(this).siblings("li").removeClass("selected");
                }
                var order = $(this).attr("order").toString();
                $("#chkSortType").val(order);
                $("#formQuery").submit();
            });
            //条件选择
            $(".multipleoptions").click(function() {
                $(this).toggleClass('selected');

                if ($(this).hasClass("selected") && ($(this).parent('p').attr('datatype') == '1')) {
                    $(this).siblings(".multipleoptions").removeClass("selected");
                }

                if ($(this).hasClass("selected")) {
                    var firstType = $(this).parent().attr("datatype").toString();
                    var firstTypeName = $(this).parent().find("label").html();
                    var secondType = $(this).attr("value").toString();
                    var secondTypeName = $(this).html();

                    $.ajax({
                        type: "post",
                        url: "/loan/RecordClick",
                        async: false,
                        data: { firstType: firstType, secondType: secondType, firstTypeName: firstTypeName, secondTypeName: secondTypeName },
                        dataType: "json",
                        success: function() {}
                    });
                }

                //风险等级
                var LoanCategoryId = "";
                $(".multipleoptions[group='level'][class*='selected']").each(function() {
                    LoanCategoryId = $(this).attr("value");
                });
                $("#txtLoanCategoryId").val(LoanCategoryId);                
                var lastCategoryId = 0;
                if (lastCategoryId == 0) lastCategoryId = 8;
                //魔镜等级
                var CreditCodes = "";
                var creditCodeList = [];
                $("#txtCreditCodes").val("");
                $(".multipleoptions[group='mirrorlevel'][class*='selected']").each(function() {
                    var categoryId = $(this).attr("by").toString();
                    if (categoryId.indexOf(LoanCategoryId) >= 0&&lastCategoryId.toString()==LoanCategoryId) {
                        CreditCodes += $(this).attr("value") + ",";
                        creditCodeList.push($(this).attr("value"));
                    }
                });
                $("#txtCreditCodes").val(CreditCodes);
                
                //列表类型
                var ListTypes = "";
                $("#txtListTypes").val("");
                $(".multipleoptions[group='listtype'][class*='selected']").each(function() {
                    var categoryId = $(this).attr("by").toString();
                    if (categoryId.indexOf(LoanCategoryId) >= 0&&lastCategoryId.toString()==LoanCategoryId) {
                        ListTypes += $(this).attr("value") + ",";
                    }
                });
                $("#txtListTypes").val(ListTypes);

                //利率
                var Rates = "";
                $(".multipleoptions[group='rate'][class*='selected']").each(function() {
                    Rates += $(this).attr("value") + ",";
                });
                $("#txtRates").val(Rates);
                //期限
                var Months = "";
                $(".multipleoptions[group='time'][class*='selected']").each(function() {
                    Months += $(this).attr("value") + ",";
                });
                $("#txtMonths").val(Months);
                //认证信息
                var AuthInfo = "";
                $(".multipleoptions[group='accreditation'][class*='selected']").each(function() {
                    AuthInfo += $(this).attr("value") + ",";
                });
                $("#txtAuthInfo").val(AuthInfo);
                //借款次数
                var BorrowCount = "";
                $(".multipleoptions[group='successtimes'][class*='selected']").each(function() {
                    BorrowCount += $(this).attr("value") + ",";
                });
                $("#txtBorrowCount").val(BorrowCount);


                $("#formQuery").submit();
            });
            //输入金额筛选按钮
            $('.txt_min_money').bind('input propertychange', function() {
                var _min = $('.txt_min_money').val();
                if(_min=='')
                {
                    $('.label_error').html('');
                } else if (!(/^[1-9]\d*(\d+)?$/).test(_min)) {
                    $('.label_error').html('请输入正整数');
                } else if (_min < 100) {
                    $('.label_error').html('最小值不能少于100');
                } else {
                    $('.label_error').html('');
                }
            });
            $('.txt_max_money').bind('input propertychange', function() {
                var _max = $('.txt_max_money').val();
                if(_max=='')
                {
                    $('.label_error').html('');
                }else if (!(/^[1-9]\d*(\d+)?$/).test(_max)) {
                    $('.label_error').html('请输入正整数');
                } else if (_max > 500000) {
                    $('.label_error').html('最大值不能大于50W');
                } else {
                    $('.label_error').html('');
                }
            });
            $(".btn_filter").click(function() {
                var _min = $('.txt_min_money').val(),
                    _max = $('.txt_max_money').val();
                var _status=1;
                if(_min==''&&_max=='')//都为空时可以筛选
                {
                    _status=0;
                }
                if (_status&&(!(/^[1-9]\d*(\d+)?$/).test(_min) || !(/^[1-9]\d*(\d+)?$/).test(_max))) {
                    $('.label_error').html('请输入正整数');
                } else if (_status&&(_min < 100 || _max > 500000 || parseInt(_min) > parseInt(_max))) {
                    $('.label_error').html('最小值不能少于100，最大值不能大于50W');
                } else {
                    $('.label_error').html('');

                    var rangType = 1;
                    var rangTypeName = "金额";

                    $.ajax({
                        type: "post",
                        url: "/loan/RangeRecordClick",
                        async: false,
                        data: { rangType: rangType, rangTypeName: rangTypeName, minvalue: _min, maxvalue: _max },
                        dataType: "json",
                        success: function() {}
                    });

                    $("#txtMinAmount").val(_min);
                    $("#txtMaxAmount").val(_max);
                    $("#formQuery").submit();
                }
            });
        });

    </script>
    <script type="text/javascript">
        $(function() {
            //一键投标按钮
            $('.invest_onekey').click(function() {
                var modelType = $(this).attr("model");

                $('#modal' + modelType.toString()).modal('show');
                //$('#modal').modal('show');//中风险
                //$('#modal2').modal('show');//高风险
                //$('#modal3').modal('show');//一键投标
            });
            $("#openUserTrustCreditRank").click(function() {
                var data = {
                    //data 是Request DTO 定义的参数
                    ActionName: "middle"
                };
                $.post("/bid/SetUserTrustCreditRank", data, function(response) {
                    if (response.Code == 1) {
                        alert(response.Content);
                        window.location.reload();
                    } else {
                        if (response.Content == "") {
                            alert("开通失败");
                        } else {
                            alert(response.Content);
                        }
                    }
                });
            });

            //金额输入框事件
            function checktxtMoney() {
                var _input = $('#txt_money').val(),
                    _tip = $('#txt_money').next(),
                    _min = 50,
                    _max = 500;
                if (!(/^[1-9]\d*(\d+)?$/).test(_input)) {
                    _tip.addClass('error').html('请输入整数');
                } else if (_input > _max || _input < _min) {
                    _tip.addClass('error').html('请输入' + _min + '-' + _max + '之间的金额');
                } else {
                    _tip.removeClass('error').html('￥50-￥500');
                    $('.tip_all').html(parseInt($('#txt_money').val()) * parseInt($('#txt_count').val()));
                }
            }

            $('#txt_money').bind('input propertychange', function() {
                checktxtMoney();
            });

            //标数输入监控事件
            function checktxtCount() {
                var _input = $('#txt_count').val(),
                    _tip = $('#txt_count').next(),
                    _min = 1,
                    _max = Math.min(100, 100); //符合标个数和30取最小值
                if (!(/^[1-9]\d*(\d+)?$/).test(_input)) {
                    _tip.addClass('error').html('请输入整数');
                } else if (_input > _max || _input < _min) {
                    _tip.addClass('error').html('请输入' + _min + '-' + _max + '之间的数');
                } else {
                    _tip.removeClass('error').html('最多100个');
                    $('.tip_all').html(parseInt($('#txt_money').val()) * parseInt($('#txt_count').val()));
                }
            }

            $('#txt_count').bind('input propertychange', function() {
                checktxtCount();
            });

            //改变按钮状态
            function changeBtnState(bool) {
                var _this = $('.btn_sure');
                if (bool)
                    _this.removeAttr('disabled').removeClass('disabled').val('确定投标');
                else
                    _this.attr('disabled', 'disabled').addClass('disabled').val('投标中');
            };

            function getbiddata() {
                var data = {};
                data.FirstTypeName = "风险等级（必须）";
                var tit = "低风险";
                switch (0) {
                case 4:
                    tit = "中风险";
                    break;
                case 5:
                    tit = "高风险";
                    break;
                }
                data.SecondTypeName = tit;
                //魔镜等级
                var CreditCodes = "";
                var CreditCodeName = "";
                $(".multipleoptions[group='mirrorlevel'][class*='selected']").each(function() {
                    CreditCodes += $(this).attr("value") + ",";
                    CreditCodeName += $(this).html() + ",";
                });
                data.CreditCodes = CreditCodes;
                if (CreditCodeName.length > 0) {
                    data.FirstTypeName += ",魔镜等级";
                    data.SecondTypeName += ";" + CreditCodeName;
                }

                //列表类型
                var ListTypes = "";
                var ListTypeNames = "";
                $(".multipleoptions[group='listtype'][class*='selected']").each(function() {
                    ListTypes += $(this).attr("value") + ",";
                    ListTypeNames += $(this).html() + ",";
                });
                data.ListTypes = ListTypes;
                if (ListTypeNames.length > 0) {
                    data.FirstTypeName += ",列表类型";
                    data.SecondTypeName += ";" + ListTypeNames;
                }

                //利率
                var Rates = "";
                var RateNames = "";
                $(".multipleoptions[group='rate'][class*='selected']").each(function() {
                    Rates += $(this).attr("value") + ",";
                    RateNames += $(this).html() + ",";
                });
                data.Rates = Rates;
                if (RateNames.length > 0) {
                    data.FirstTypeName += ",利率";
                    data.SecondTypeName += ";" + RateNames;
                }

                //期限
                var Months = "";
                var MonthNames = "";
                $(".multipleoptions[group='time'][class*='selected']").each(function() {
                    Months += $(this).attr("value") + ",";
                    MonthNames += $(this).html() + ",";
                });
                data.Months = Months;
                if (MonthNames.length > 0) {
                    data.FirstTypeName += ",期限";
                    data.SecondTypeName += ";" + MonthNames;
                }

                //认证信息
                var AuthInfo = "";
                var AuthInfoNames = "";
                $(".multipleoptions[group='accreditation'][class*='selected']").each(function() {
                    AuthInfo += $(this).attr("value") + ",";
                    AuthInfoNames += $(this).html() + ",";
                });
                data.AuthInfo = AuthInfo;
                if (AuthInfoNames.length > 0) {
                    data.FirstTypeName += ",认证信息";
                    data.SecondTypeName += ";" + AuthInfoNames;
                }
                //成功借款次数
                var BorrowCount = "";
                var BorrowCountNames = "";
                $(".multipleoptions[group='successtimes'][class*='selected']").each(function() {
                    BorrowCount += $(this).attr("value") + ",";
                    BorrowCountNames += $(this).html() + ",";
                });
                data.BorrowCount = BorrowCount;
                if (BorrowCountNames.length > 0) {
                    data.FirstTypeName += ",成功借款次数";
                    data.SecondTypeName += ";" + BorrowCountNames;
                }

                data.LoanCategoryId = 0;

                //金额
                data.MinAmount = 0;
                data.MaxAmount = 0;
                data.FirstTypeName += ",金额";
                data.SecondTypeName += ";" + data.MinAmount + ',' + data.MaxAmount;

                data.SortType = 0;
                if ($('#chk').attr('checked') == "checked") {
                    data.DidIBid = "on";
                } else {
                    data.DidIBid = "";
                }
                return data;
            };

            //确认投标按钮
            $('.btn_sure').click(function() {
                changeBtnState(false);
                checktxtMoney();
                checktxtCount();
                if ($('.dv_input .error').length > 0) {
                    changeBtnState(true);
                    return false;
                } else {
                    var data = getbiddata();
                    data.Count = $('#txt_count').val();
                    data.BidAmount = $('#txt_money').val();
                    $.post("/loan/OneKeyBid", data, function(response) {
                        if (response.Code == 0) {
                            changeBtnState(true);
                            $('#modal3').modal('hide');
                            $('#successCount').html(response.SuccessCount);
                            $('#successAmount').html(response.SuccessAmount);
                            $('#modal4').modal('show');
                        } else if (response.Code == -2) {
                            $('.tip_error_btn').html('余额不足，请<a href="https://pay.ppdai.com/order/online" target="_blank">充值</a>');
                            //                $('.tip_error_btn').html('您有处理中的一键投资，请等待处理完成后再试');
                            //                $('.tip_error_btn').html('当前一键投标使用人数较多，请稍后再试');
                        } else {
                            $('.tip_error_btn').html(response.Message);
                        }
                        changeBtnState(true);

                    });
                }
            });
        })
    </script>
