                    $videoDescContent.setEllipsis('1130px', totalText);
                    var ellipsisText = $videoDescContent.text();

                    if (totalText !== ellipsisText) {
                        $('.btn-operate').addClass("btn-unfold").show();
                    }

                    $('.btn-operate').on('click', function () {
                        var _this = $(this);
                        if (_this.hasClass('btn-unfold')) {
                            $videoDescContent.html(totalText + spaceHolder);
                            _this.removeClass('btn-unfold')
                              .html('收起<i class="iconfont icon-arrow-sm-up"></i>');
                        } else {
                            $videoDescContent.html(ellipsisText);
                            _this.addClass('btn-unfold')
                              .html('展开<i class="iconfont icon-arrow-sm-down"></i>');
                        }
                    });
                }


