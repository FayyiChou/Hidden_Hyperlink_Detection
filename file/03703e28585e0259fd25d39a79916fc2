        function pusheid(eid, a1) {
            if (a1 != null) {
                _trackEvent.push({ 'eid': eid, 'val': 1, 'a1': a1 });
            } else {
                _trackEvent.push({ 'eid': eid, 'val': 1 });
            }
            //alert(eid);
        }

        function pusheidforimg(eid, a1, a2) {
            _trackEvent.push({ 'eid': eid, 'val': 1, 'a1': a1, 'a2': a2 });
        }


        function setHeadNavPv(obj) {
            //index, authors, notice, logo
            if (obj) {

                for (var key in obj) {
                    var href = $("#nav-" + key).attr("href");
                    if (!(/^#/.test(obj[key]))) {
                        obj[key] = "#" + obj[key];
                    }
                    $("#nav-" + key).attr("href", href + obj[key]);
                }

            }
        }


