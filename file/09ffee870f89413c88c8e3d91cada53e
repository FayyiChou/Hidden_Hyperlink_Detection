#focus ul {height:355px; position:absolute;}
#focus ul li {float:left; width:1200px; height:355px; overflow:hidden; position:relative; background:#000;}
#focus ul li div {position:absolute; overflow:hidden;}
#focus .btnBg {position:absolute; width:1200px; height:20px; left:0; bottom:0; background:#000;}
#focus .btn {position:absolute; width:1200px; height:10px; padding:5px 10px; right:0; bottom:0; text-align:right;background:none;}
#focus .btn span {display:inline-block; _display:inline; _zoom:1; width:25px; height:10px; _font-size:0; margin-left:5px; cursor:pointer; background:#fff;opacity: 0}
#focus .btn span.on {background:#fff;}
#focus .preNext {width:45px; height:100px; position:absolute; top:140px; background:url(Images/sprite.png) no-repeat 0 0; cursor:pointer;}
#focus .pre {left:0;}
#focus .next {right:0; background-position:right top;}
/* 浮动层
