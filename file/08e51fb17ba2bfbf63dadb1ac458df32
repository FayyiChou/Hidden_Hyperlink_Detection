so.addVariable("JcScpAutoPlay","yes");
so.addVariable("JcScpAFrontCanClose","yes");
//是否自动播放
so.write("CuPlayer");	

function LeftScr(m){
	var scrtop;
	if(m>1){
	scrtop=55*(m-1);
	}else{
		scrtop=0;
		}
	$(".list").animate({
		scrollTop:scrtop
		},130);
	}
