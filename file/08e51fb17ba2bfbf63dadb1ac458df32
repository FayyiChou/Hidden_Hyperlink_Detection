function getNext(pars)
{	
  if(video_index < num-1)
	{ 
		video_index++;
		so.addVariable("JcScpVideoPath",sp[video_index]);
		so.addVariable("JcScpAutoPlay","yes"); //是否自动播放
		so.addVariable("JcScpAFrontCanClose","yes");
