<script>
$(function(){
	$(".swiper1-2-a a, .zcdl").click(function(){
		$(".jh-ewm-tu img").attr("src","images/app1.jpg");
		$(".jh-ewm p").first().html("手机扫码下载窝客兼职");
		$(".jh-ewm p").last().html("APP报名职位");
		$(".mengban").show();
		$(".jh-ewm").show();	
	});
	$(".swiper1-2-b a").click(function(){
		$(".jh-ewm-tu img").attr("src","images/weixinhao.jpg");
		$(".jh-ewm p").first().html("关注窝客微信号");
		$(".jh-ewm p").last().html("获取更多职位");
		$(".mengban").show();
		$(".jh-ewm").show();	
	});
	//关闭显示	
	$("#close").click(function(){
		$(".mengban").hide()
		$(".jh-ewm").hide();
	});
});

var swiper = new Swiper('.swiper-container-h', {
	pagination: '.swiper-pagination', //分页容器
	paginationClickable: true,  //控制分页器点击切换
	direction: 'vertical',  //滑动方向垂直滑动
	mousewheelControl : true, //鼠标控制
	//initialSlide :5, //设置默认索引 默认从0开始
		
	onInit: function(swiper){ //Swiper2.x的初始化是onFirstInit
		swiperAnimateCache(swiper); //隐藏动画元素 
		swiperAnimate(swiper); //初始化完成开始动画
	}, 
	onSlideChangeEnd: function(swiper){ 
		swiperAnimate(swiper); //每个slide切换结束时也运行当前slide动画
	} 			
});
var swiperV = new Swiper('.swiper-container-v', {
    // pagination: '.swiper-pagination',
     nextButton: '.swiper-button-next',
     prevButton: '.swiper-button-prev',
     slidesPerView: 1, //设置slider容器能够同时显示的slides数量(carousel模式)。
     paginationClickable: true, //此参数设置为true时，点击分页器的指示点分页器会控制Swiper切换。
     spaceBetween: 30,
     loop: true
 });
</script>
