    <div id="main">
    <div class="clearfix topFtFix">
		
    <div class="section">
    <div id="topAreaL">
    <script type="text/javascript">
    			$(document).ready(function(){
    				//Examples of how to assign the Colorbox event to elements
    				$(".group1").colorbox({rel:'group1'});
    				$(".group2").colorbox({rel:'group2', transition:"fade"});
    				$(".group3").colorbox({rel:'group3', transition:"none", width:"75%", height:"75%"});
    				$(".group4").colorbox({rel:'group4', slideshow:true});
    				$(".ajax").colorbox();
    				$(".youtube").colorbox({iframe:true, innerWidth:640, innerHeight:390});
    				$(".vimeo").colorbox({iframe:true, innerWidth:500, innerHeight:409});
    				$(".iframe").colorbox({iframe:true, width:"80%", height:"80%"});
    				$(".inline").colorbox({inline:true, width:"50%"});
    				
            var baseUrl = "http://ss-search.emg-ss.jp";
            var detailBaseUrl = "http://ss-search.emg-ss.jp/b/emg/info/";
            var myLocation;
            var locationSuccess = function (position) {
              if (Modernizr.localstorage) {
                localStorage["location"] = position.coords.latitude + ',' + position.coords.longitude;
              }
            
              displayMap(position.coords.latitude + ',' + position.coords.longitude);
            };
            var locationError = function () {
              alert('現在地が取得できませんでした。');
            };
            var displayMap = function (location) {
              var myLatLon = location.split(',');

              var request = $.ajax({
                    type: 'GET',
                    url: "http://lbs.mapion.co.jp/map/uc/PoiClose",
                    dataType: "jsonp",
                    data: {
                      grp: "emg_dist",
                      el: myLatLon[1],
                      nl: myLatLon[0],
                      scl: 1,
                      vo: "mbml",
                      json: 1,
                      poi_status: 1
                    },
                    success: function(data){
                      if( data.mbml.Property && data.mbml.Property.hit > 0 ){

                        _gaq.push(['_trackEvent','Detect_Location','PageLoad','Yes', 1,true]);

                        var storeName = '';
                        var storeTitle = '';
                        var iconUrl = '';
                        
                        // truncate long name
                        if( data.mbml.PoiList.Poi[0].name.length > 9 ) {
                          storeName = data.mbml.PoiList.Poi[0].name.substr(0,7) + '...';  
                        } else {
                          storeName = data.mbml.PoiList.Poi[0].name;
                        }

                        if( data.mbml.PoiList.Poi[0].service_flag1 == '1' ) {
                          storeTitle = 'エッソ ' +  storeName;
                          iconUrl = '/top/img/mapIcoEsso02.png';
                        } else if( data.mbml.PoiList.Poi[0].service_flag1 == '2' ) {
                          storeTitle = 'モービル ' +  storeName;
                          iconUrl = '/top/img/mapIcoMobil02.png';
                        } else if( data.mbml.PoiList.Poi[0].service_flag1 == '3' ) {
                          storeTitle = 'ゼネラル ' +  storeName;
                          iconUrl = '/top/img/mapIcoZeneral02.png';
                        }

                        $('#searchResultStoreLink').attr("href", detailBaseUrl + data.mbml.PoiList.Poi[0].id + '?nl=' + data.mbml.PoiList.Poi[0].latitude + '&el=' + data.mbml.PoiList.Poi[0].longitude);
                        $('#searchResultStoreName').html( storeTitle );
                        $('#searchResultStoreAddress').html( data.mbml.PoiList.Poi[0].full_address );
                        $('#searchResultStorePhone').html( data.mbml.PoiList.Poi[0].tel );
                        $('#searchFromListLink').attr("href", baseUrl + '?nl=' + data.mbml.PoiList.Poi[0].latitude + '&el=' + data.mbml.PoiList.Poi[0].longitude);

                        var map = new MMap(document.getElementById("searchResultMapAreaDiv"));
                        map.autoScroll = true;
                        map.centerAndZoom(new MPoint( data.mbml.PoiList.Poi[0].longitude, data.mbml.PoiList.Poi[0].latitude ), 6);

                        // add marker
                        var customMarkerObj = new MIcon();
                        customMarkerObj.image = iconUrl;
                        customMarkerObj.width = 45;
                        customMarkerObj.height = 41;
                        customMarkerObj.iconAnchor = new MPoint(22,38);
                        marker = new MMarker(new MPoint( data.mbml.PoiList.Poi[0].longitude, data.mbml.PoiList.Poi[0].latitude ) , customMarkerObj);
                        map.addOverlay(marker);
                      } else {
                        alert('エラーが発生しました。しばらくしてからやり直してください。');
                      }
                    },
                    error: function(){
                      alert('エラーが発生しました。しばらくしてからやり直してください。');
                    }
                  });
              };

            var searchNearby = function () {
              if (Modernizr.geolocation) {
                    
                $('#searchBtnDiv').hide();
                $('#searchResultDiv').show();

                if (Modernizr.localstorage) {
                  myLocation = localStorage["location"];
                  if (!myLocation) {
                    navigator.geolocation.getCurrentPosition(locationSuccess, locationError);
                  } else {
                    displayMap( myLocation );
                  }
                } else {
                  navigator.geolocation.getCurrentPosition(locationSuccess, locationError);
                }
              } else {
                alert('ご使用のブラウザは位置情報の取得に対応しておりません。\n店舗検索の際は「店舗一覧から探す」をご利用ください。');
              }
            };

            var refreshNearby = function () {
              $('#searchResultStoreName').html( '' );
              $('#searchResultStoreAddress').html( '' );
              $('#searchResultStorePhone').html( '' );
              $('#searchResultMapAreaDiv').html( '' );

              navigator.geolocation.getCurrentPosition(locationSuccess, locationError);
            };

            $('#searchNearbyBtn').click(function(){
              searchNearby();
            });
            $('#refreshNearbyBtn').click(function(){
              refreshNearby();
            })

          });

    </script>

<div class="box mb20" id="slider" style="height:345px!important;">
