$(function() {
  $("#image img").click(function() {
		$(".multidisp").addClass("displayNon");
  });
  $("#lean_overlay,.modal_close").click(function() {
		$(".multidisp").removeClass("displayNon");
		$('.overray').fadeOut();
  });
  $( window ).scroll(function(){
		$(".multidisp").removeClass("displayNon");
		$('.overray').fadeOut();
  });
});

</script>



