<script>
var speeding=10
demotwo.innerHTML=demoone.innerHTML
function Marqueeing()
{
  if(demotwo.offsetWidth-demozero.scrollLeft<=0)
  demozero.scrollLeft-=demoone.offsetWidth
  else{
  demozero.scrollLeft++
  }
}
var MyMar=setInterval(Marqueeing,speeding)
demozero.onmouseover=function() {clearInterval(MyMar)}
demozero.onmouseout=function() {MyMar=setInterval(Marqueeing,speeding)}
