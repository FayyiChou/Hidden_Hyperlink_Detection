<script>
    $(function(){
        $(".navtag ul li.single").hover(function(){
            $(this).addClass("hover").siblings().removeClass("hover");
            $(this).find(".nav a").addClass("hover");
            $(this).find(".con").show();
        },function(){
            $(this).find(".con").hide();
            $(this).removeClass("hover");
            $(this).find(".nav a").removeClass("hover");
        });

        $('.head-v3 li.hide').hover(function ()
                {
                    $(this).addClass("navhover");
                    $(this).find('.white').css("display","block");
                    $(this).find('.hidebox').stop(true, true).show();
                    $(this).children("a").css("font-weight","bold").css('color', '#1078e0');
                },
                function () {
                    $(this).removeClass("navhover");
                    $(this).find('.white').css("display","none");
                    $(this).find('.hidebox').stop(true, true).hide();
                    $(this).children("a").css("font-weight","normal").css('color', '#777');
                })
    });
</script>
<style>
    .active > a{
        color: #1078E0 !important;
        font-weight:bold !important;
    }
</style>
