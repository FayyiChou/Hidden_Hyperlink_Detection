<!--市场行情-->
<script type="text/javascript">
(function($){
	$.fn.myScroll = function(options){
	//默认配置
	var defaults = {
		speed:40,  //滚动速度,值越大速度越慢
		rowHeight:24 //每行的高度
	};
	
	var opts = $.extend({}, defaults, options),intId = [];
	
	function marquee(obj, step){
	
		obj.find("table").animate({
			marginTop: '-=1'
		},0,function(){
				var s = Math.abs(parseInt($(this).css("margin-top")));
				if(s >= step){
					$(this).find("tr").slice(0, 1).appendTo($(this));
					$(this).css("margin-top", 0);
				}
			});
		}
		
		this.each(function(i){
			var sh = opts["rowHeight"],speed = opts["speed"],_this = $(this);
			intId[i] = setInterval(function(){
				if(_this.find("table").height()<=_this.height()){
					clearInterval(intId[i]);
				}else{
					marquee(_this, sh);
				}
			}, speed);

			_this.hover(function(){
				clearInterval(intId[i]);
			},function(){
				intId[i] = setInterval(function(){
					if(_this.find("table").height()<=_this.height()){
						clearInterval(intId[i]);
					}else{
						marquee(_this, sh);
					}
				}, speed);
			});
		
		});
	}

})(jQuery);

$(function(){
    $("div.ranklist").myScroll({
        speed:60,
        rowHeight:24
    });
});
</script>

<script type="text/javascript">
    $(function() {
        var rollspeed = 100;
        var freshTimer = 10000000;
        var scrollTcimerID;
        var sdiv = $("#scrollContentContainer");
        var d1 = $("#demo1");
        var d2 = $("#demo2");
        var startTime = 0;
        var endTime = 0;
        function getHQData() {
            var hqHtml;
            $.ajax({
                url: "/marketinfo",
                type: "get",
                dataType: "json",
                async: false,
                error: function(r, ri, c) {
                },
                success: function(data) {

                    //var hqDataJson = $.jsonToObject(data);
                    var hqDataDiv = "<table class=\"scrollContent\" cellspacing=\"0\" cellpadding=\"0\" width=\"899\"><tbody class=\"fixedHeader\">"
                    $.each(data.tables,function(idx,item){
                        if(idx == 0){
                            $('.zhzs_1').text(item.CurPrice);
                            $('.zhzs_2').text(dealCurrentGains(Number(item.CurrentGains)));
                        }

                        hqDataDiv += "<tr>";
                        //hqDataDiv += "<td style='color:#c3af70; font-weight:bold;' width='9%'>"+item.num+"</td>";
                        hqDataDiv += "<td class='first cloloro' width='164'>"+item.fullname+"</td>";
                        hqDataDiv += "<td class='cloloro' width='130'>"+item.code+"</td>";
                        hqDataDiv += "<td width='97'>"+item.CurPrice+"</td>";
                        hqDataDiv += "<td width='65'>"+dealCurrentGains(Number(item.CurrentGains))+"</td>";
                        hqDataDiv += "<td width='97' class='colorpik'>"+item.TotalAmount+"</td>";
                        hqDataDiv += "<td width='130' class='colorpik'>"+item.TotalMoney+"</td>";
                        hqDataDiv += "<td width='' class='colorpik'>"+item.HighPrice+"</td>";
                        hqDataDiv += "<td width='' class='colorpik'>"+item.LowPrice+"</td>";
                        hqDataDiv += "</tr>";
                    });

                    hqDataDiv += "</tbody>";
                    hqDataDiv += "</table>";
                    hqHtml = hqDataDiv;
                    d1.html(hqDataDiv);
                    $("#allCont").html(data.sumNum);
                    $("#allOrder").html(data.sumMoney);
                    //$("#tradeTime").html(data.time);
                    $("#tradeTime").html('2017-02-18 07:35:34');
                }
            });
            return hqHtml;
        }

        function getPaihang (){
            $.ajax({
                url: "/indexPhinfo",
                type: "get",
                dataType: "json",
                async: false,
                error: function(r, ri, c) {
                },
                success: function(data) {
                    for (var i in data){
                        var hqDataDiv = '';

                        if(i == 'chengjiaoe'){
                            for (var o in data[i]){
                                hqDataDiv += "<tr>";
                                hqDataDiv += "<td class='cloloro'>"+data[i][o].cp_name+"</td>";
                                hqDataDiv += "<td class='cloloro'>"+data[i][o].cp_id+"</td>";
                                hqDataDiv += "<td class=''>"+data[i][o].cp_cje+"</td>";
                                hqDataDiv += "</tr>";
                            }
                            $('#tbody1').html(hqDataDiv);
                        }else if(i == 'zhangfu'){
                            for (var o in data[i]){
                                hqDataDiv += "<tr>";
                                hqDataDiv += "<td class='cloloro'>"+data[i][o].cp_name+"</td>";
                                hqDataDiv += "<td class='cloloro'>"+data[i][o].cp_id+"</td>";
                                hqDataDiv += "<td class=''>"+dealCurrentGains(Number(data[i][o].cp_zd))+"</td>";
                                hqDataDiv += "</tr>";
                            }
                            $('#tbody2').html(hqDataDiv);
                        }else if(i == 'chengjiaol'){
                            for (var o in data[i]){
                                hqDataDiv += "<tr>";
                                hqDataDiv += "<td class='cloloro'>"+data[i][o].cp_name+"</td>";
                                hqDataDiv += "<td class='cloloro'>"+data[i][o].cp_id+"</td>";
                                hqDataDiv += "<td class=''>"+data[i][o].cp_cjl+"</td>";
                                hqDataDiv += "</tr>";
                            }
                            $('#tbody3').html(hqDataDiv);
                        }
                    }
                }
            });
            return true;
        }
        getPaihang();

        /**
         * 处理涨跌幅，显示颜色
         */
        function dealCurrentGains(currentGains) {
            if(currentGains==0){
                //return "<font color='#0000FF'>" + strNewValue + "</font>";
                return currentGains + '%';
            }else if(currentGains<0){
                return "<font color='#00BD00'>" + currentGains + "%</font>";
            }else{
                return "<font color='#ff0000'>" + currentGains + "%</font>";
            }
        }

        //对日期进行格式化
        function formatTime(lsTime){
            return lsTime.substr(0,4) + "年" + lsTime.substr(5,2) + "月" +
                    lsTime.substr(8,2) + "日" + lsTime.substr(10);
        }

        function refreshHqData() {
            var timerID = window.setInterval(getHQData,freshTimer);
        }

        function startHqData() {
            var hqDataDiv = getHQData();
            d2.html(hqDataDiv);
            d1.hide();
            refreshHqData();
            startScrollDiv();
        }

        function startScrollDiv() {
            if(scrollTcimerID)
                stopScrollDiv();
            scrollTcimerID = window.setInterval( scrollDiv, rollspeed);
            startTime = new Date().getTime();
        }

        function scrollDiv() {
            var h = d2.height();
            /* sdiv.stop().animate({ scrollTop: h }, 'slow', function(){
             if(d1.html != null && d1.html != "")
             d2.html(d1.html());
             sdiv.scrollTop(0)
             startScrollDiv();
             }); */
            //console.log(sdiv.scrollTop() + " -- " + d2.get(0).offsetHeight + " - " + d2.offset().top + " -- " + d2.get(0).clientHeight + " -- " + sdiv.height());
            if(d2.get(0).offsetHeight - sdiv.height() - sdiv.scrollTop() <= 0) {
                sdiv.scrollTop(0);
                endTime = new Date().getTime();
                var time = endTime - startTime;
                if(time >= freshTimer) {
                    d2.html(d1.html());
                    startTime = new Date().getTime();
                    endTime = startTime;
                }
                //console.log(startTime + " - " + endTime + " - " + time + " - " + freshTimer);
                //console.log(sdiv.scrollTop() + " - " + d2.get(0).offsetHeight);
                //sdiv.scrollTop(sdiv.scrollTop()-d1.get(0).offsetHeight);
            } else {
                sdiv.scrollTop(sdiv.scrollTop() + 1);
            }
        }

        function stopScrollDiv() {
            if(scrollTcimerID)
                try{
                    window.clearInterval(scrollTcimerID);
                } catch(e){
                    //console.log("1_2");
                }

        }

        sdiv.hover(stopScrollDiv,startScrollDiv).mouseout();

        startHqData();


        var marketleftH = $('.xxdasdf2').css('height');
        $('.xxdasdf').css('min-height', marketleftH);
        var len = $('#tbody3').find('tr').length;
        var mH = parseFloat((parseInt(marketleftH)-67) / parseInt(len), 2);
        $('#tbody3').find('tr').css('height', mH+'px');
    });


    /* $(document).ready(function () {
     sdiv.hover(stopScrollDiv,startScrollDiv).mouseout();
     startHqData();
     }); */
    //-->
</script>
<!--市场行情end-->

