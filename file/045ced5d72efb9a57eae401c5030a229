<script>
    var isopenlogin=false;
    function logmeth() {
        $('body').append('<div id="mask"></div>');
        $('#mask').fadeIn();
        $('#methodiv').css('display', 'block');
        $('#apply').css('display', 'none');
        $('.chaticons.cs3').css("border-left","1px solid #ccc");
        if(!isopenlogin)
        {
            $('#threeiframe').attr('src','/user/qqlogin');//默认qq登录
            isopenlogin=true;
        }

    }
    function meth_close() {
        $('#mask').fadeOut();
        $('#methodiv').css('display', 'none');
    }
    $(function(){
        $(".cbspan>h2 a").click(function(){
            $(".cbspan>h2 a").removeClass('cur');
            $(this).addClass("cur");
            $('#threeiframe').attr('src',$(this).attr('src'));
        });
    });
    <!--微信开户-->
    $(".publish").hover(function(){
        $(this).find("a").stop().animate({left:"0"},300);
    },function(){
        $(this).find("a").stop().animate({left:"-248px"},300);
    });
</script>





