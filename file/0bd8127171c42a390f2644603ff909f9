<script>
function show()  //显示隐藏层和弹出层
{
   var hideobj=document.getElementById("hidebg");
   hidebg.style.display="block";  //显示隐藏层
   scroll(0,0);
   hidebg.style.height=document.body.clientHeight+"px";  //设置隐藏层的高度为当前页面高度
   document.getElementById("hidebox").style.display="block";  //显示弹出层
}
function hide()  //去除隐藏层和弹出层
{
   document.getElementById("hidebg").style.display="none";
   document.getElementById("hidebox").style.display="none";
   if(document.getElementById("TeacherMovie1").paused == false)
   {document.getElementById("TeacherMovie1").pause();}
   if(document.getElementById("TeacherMovie2").paused == false)
   {document.getElementById("TeacherMovie2").pause();}

   if(document.getElementById("TeacherMovieIE1").playState == "3")
   {document.getElementById("TeacherMovieIE1").controls.pause();}
   if(document.getElementById("TeacherMovieIE2").playState == "3")
   {document.getElementById("TeacherMovieIE2").controls.pause();}

}
function MediaSampleState(x)
{
    if(x==0){
        document.getElementById("MediaSample").src='images/MediaSample_open.gif';
    }
    else{
        document.getElementById("MediaSample").src='images/MediaSample_close.gif';
    }
}
function TeacherMovieIEShow(x)  //放大视频
{
    var Img = "";
    var ImgOpenSrc = "";
    if(x=="TeacherMovieIE1"){
        Img = "TeacherMovieIEImg1";
        ImgOpenSrc = "../images/teachers1_video_open.png";
    }
    else if(x=="TeacherMovieIE2"){
        Img = "TeacherMovieIEImg2";
        ImgOpenSrc = "../images/teachers2_video_open.png";

    }
     
    document.getElementById(x).uimode = 'none'; 
    if(document.getElementById(x).playState == "8")
    {
        document.getElementById(x).close();
        document.getElementById(Img).src=ImgOpenSrc;
        document.getElementById(Img).style.height= "100%";
        document.getElementById(Img).style.display="block";
    }
    else if(document.getElementById(x).playState == "1")
    {
        document.getElementById(x).close();
        document.getElementById(Img).src=ImgOpenSrc;
         document.getElementById(Img).style.height= "100%";
        document.getElementById(Img).style.display="block";    
    }
    else if(document.getElementById(x).playState == "10")
    {
        document.getElementById(Img).src=ImgOpenSrc;
         document.getElementById(Img).style.height= "100%";   
    }
}
function TeacherMovieIEShow2(x)  //放大视频
{
    var Img = "";
    var ImgOpenSrc = "";
    if(x=="TeacherMovieIE1"){
        Img = "TeacherMovieIEImg1";
        ImgOpenSrc = "../images/teachers1_video_open.png";
    }
    else if(x=="TeacherMovieIE2"){
        Img = "TeacherMovieIEImg2";
        ImgOpenSrc = "../images/teachers2_video_open.png";

    }
  
    document.getElementById(x).uimode = 'none'; 
    if(document.getElementById(x).playState == "8")
    {
        document.getElementById(x).close();
        document.getElementById(Img).src=ImgOpenSrc;
        document.getElementById(Img).style.height= "100%";
        document.getElementById(Img).style.display="block";
    }
    else if(document.getElementById(x).playState == "1")
    {
        document.getElementById(x).close();
        document.getElementById(Img).src=ImgOpenSrc;
        document.getElementById(Img).style.height= "100%";
        document.getElementById(Img).style.display="block";    
    }
}

function TeacherMovieIEPlay(x)  //打开视频
{
    var Img = "";
    if(x=="TeacherMovieIE1"){
        Img = "TeacherMovieIEImg1";

    }
    else if(x=="TeacherMovieIE2"){
        Img = "TeacherMovieIEImg2";

    }


    document.getElementById(Img).style.display="none";
    
    document.getElementById(x).uimode = 'mini';
    if(document.getElementById(x).playState == "3")
    {
          document.getElementById(x).controls.pause();
          document.getElementById(x).uimode = 'none'; 
    }
    else if(document.getElementById(x).playState == "2")
    {
          document.getElementById(x).controls.play();
          document.getElementById(x).fullScreen = 'true';
    }    
    else if(document.getElementById(x).playState == "1")
    {
          document.getElementById(x).controls.play();
          document.getElementById(x).fullScreen = 'true';
    }
    else if(document.getElementById(x).playState == "10")
    {
          document.getElementById(x).controls.play();
    }
 
}

function TeacherMovieIEHide(x)  //缩小视频
{
    var Img = "";
    var ImgCloseSrc = "";
    if(x=="TeacherMovieIE1"){
        Img = "TeacherMovieIEImg1";
        ImgCloseSrc = "../images/teachers1_video_close.png";
    }
    else if(x=="TeacherMovieIE2"){
        Img = "TeacherMovieIEImg2";
        ImgCloseSrc = "../images/teachers2_video_close.png";

    }

    if(document.getElementById(x).playState == "3")
    {

    }
    else if(document.getElementById(x).playState == "1")
    {
          document.getElementById(x).close();
          document.getElementById(Img).src=ImgCloseSrc;
          document.getElementById(Img).style.height= "60%";
          document.getElementById(Img).style.display="block";
    }
    else if(document.getElementById(x).playState == "8")
    {
          document.getElementById(x).close();
          document.getElementById(Img).src=ImgCloseSrc;
          document.getElementById(Img).style.height= "60%";
          document.getElementById(Img).style.display="block";
    }   
    else if(document.getElementById(x).playState == "10")
    {
          document.getElementById(Img).src=ImgCloseSrc;
          document.getElementById(Img).style.height= "60%";
    }     
}

function TeacherAudioPlay(x) //打开音频
{
   var audio = document.getElementById(x);
   var audioplayer = "";
   var TeachervoiceState = "";
   var Img = "";
   if(x=="Music1"){
          audioplayer = document.getElementById("MusicPayer1");
          TeachervoiceState = "teachervoice1";
          Img = document.getElementById("MusicImg1");
    }
    else if(x=="Music2"){
        audioplayer = document.getElementById("MusicPayer2");
        TeachervoiceState = "teachervoice2";
        Img = document.getElementById("MusicImg2");
    }
    
    if(audioplayer.paused){
        audioplayer.play();
    }
    else{

        if(document.getElementById(TeachervoiceState).playState == "1"){
            document.getElementById(TeachervoiceState).controls.play();
        }
        else if(document.getElementById(TeachervoiceState).playState == "2"){
            document.getElementById(TeachervoiceState).controls.play();
        }
        else if(document.getElementById(TeachervoiceState).playState == "3"){
            document.getElementById(TeachervoiceState).controls.pasue();
        }
        else if(document.getElementById(TeachervoiceState).playState == "6"){
            document.getElementById(TeachervoiceState).controls.play();
        }
        else if(document.getElementById(TeachervoiceState).playState == "8"){
            document.getElementById(TeachervoiceState).controls.play();
        }
        else if(document.getElementById(TeachervoiceState).playState == "10"){
            document.getElementById(TeachervoiceState).controls.play();
        }
        else{
            audioplayer.pause();
        }
    }
    
     

    Img.style.height = "10%";
}


function TeacherAudioShow(x)  //放大音频
{
    var audio = document.getElementById(x);
    var ImgBGSrc = "";
    var Img = "";
    var ImgSrc = "";
    var TeachervoiceState = "";

   if(x=="Music1"){
        ImgBGSrc = "Url(images/ChTeachers1_open.png)";
        Img = document.getElementById("MusicImg1");
        ImgSrc = "../images/IEChTeachers1_open.png";
        TeachervoiceState = "teachervoice1";

    }
    else if(x=="Music2"){
        ImgBGSrc = "Url(images/ChTeachers2_open.png)";
        Img = document.getElementById("MusicImg2");
        ImgSrc = "../images/IEChTeachers2_open.png";
        TeachervoiceState = "teachervoice2";
    }
     
    audio.style.backgroundImage=ImgBGSrc;    
    audio.style.height = "100%";
    audio.style.width = "100%";
    Img.src = ImgSrc;
    if(document.getElementById(TeachervoiceState).playState == "2"){
       Img.style.height = "10%";     
    }
    else if(document.getElementById(TeachervoiceState).playState == "3"){
       Img.style.height = "10%";     
    }
    else{
       Img.style.height = "100%";
    }
 
 }
function TeacherAudioHide(x)  //缩小音频
{
    var audio = document.getElementById(x);
    var audioplayer = "";
    var ImgBGSrc = "";
    var Img = "";
    var ImgSrc = "";
    var TeachervoiceState = "";
    if(x=="Music1"){
        audioplayer = document.getElementById("MusicPayer1");
        ImgBGSrc = "Url(images/ChTeachers1_close.png)"; 
        Img = document.getElementById("MusicImg1");
        ImgSrc = "../images/IEChTeachers1_close.png";
        TeachervoiceState = "teachervoice1";

    }
    else if(x=="Music2"){
        audioplayer = document.getElementById("MusicPayer2");
        ImgBGSrc = "Url(images/ChTeachers2_close.png)"; 
        Img = document.getElementById("MusicImg2");
        ImgSrc = "../images/IEChTeachers2_close.png";
        TeachervoiceState = "teachervoice2";
    }
    

    if(audioplayer.paused){
        audio.style.backgroundImage= ImgBGSrc; 
    }
    else if(document.getElementById(TeachervoiceState).playState == "1"){
        document.getElementById(TeachervoiceState).close();
        Img.src = ImgSrc;
    }
    else if(document.getElementById(TeachervoiceState).playState == "2"){
        document.getElementById(TeachervoiceState).close();
        Img.src = ImgSrc;
    }
    else if(document.getElementById(TeachervoiceState).playState == "6"){
        document.getElementById(TeachervoiceState).close();
        Img.src = ImgSrc;
    }
    else if(document.getElementById(TeachervoiceState).playState == "8"){
        document.getElementById(TeachervoiceState).close();
        Img.src = ImgSrc;
    }
    else if(document.getElementById(TeachervoiceState).playState == "10"){
        document.getElementById(TeachervoiceState).close();
        Img.src = ImgSrc;
    }
    
    audio.style.height = "60%";
    audio.style.width = "60%";
  
    if(document.getElementById(TeachervoiceState).playState == "3"){
       Img.style.height = "10%"; 
    }
    else {
        Img.style.height = "60%";
    }    
     
}

