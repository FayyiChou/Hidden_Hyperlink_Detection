	function playClass(url) {
		window.open(url.replace(/\?source.*/g,""), "_blank", "height=660, width=850, toolbar= no, menubar=no, scrollbars=no, resizable=yes, location=no, status=no,top=100,left=300");
	}
	function GetUrl(){	
		var Url=window.location.href; 
		Url=Url.replace("http://www.hjenglish.com/",""); 
		Url=Url.replace("http://dev.www.hjenglish.com/","");  
		return Url;
	}		
	function GetSideBarSource(thisHref,urlContent){  
	    thisHref=thisHref+"?source=";
		urlContent=urlContent.replace(/\//g,"");
		var urlRegex=/[a-z]+/gi;
		var thisUrl=urlRegex.exec(urlContent); 
		var rtUrl=""; 
		rtUrl=AddSourceID(thisUrl.toString(),rtUrl,thisHref);
		return rtUrl;
	}
	
	function AddSourceID(thisUrl, rtUrl, thisHref){		
		switch(thisUrl){
			case "nce":
			rtUrl=IsIncludeSource(thisHref,456);break;
			case "ncexingainianqingshao": 
			rtUrl=IsIncludeSource(thisHref,457); break;
			case "ncexingainian": 
			rtUrl=IsIncludeSource(thisHref,457);break; 
			case "ncep":
			rtUrl=IsIncludeSource(thisHref,458);break; 
			default:break;
		}
		return rtUrl;
	} 
	function IsIncludeSource(url, sourceId){if(url.lastIndexOf("=")==(url.length-1))return url+sourceId;else return url;}
	$(window).load(function(){		
		if(GetUrl()!="nce/"){
			$("#rcmBookList").html("<div id='box_side'><div class='title_side'><span class='more'></span><strong>新概念英语书籍推荐</strong></div><ul class='billboard rcm_booklist'><li><a rel='nofollow' href='http://buy.hujiang.com/p586675/?source=242' target='_blank'><img src='https://i1b.hjfile.cn/shop/201312/201312203312415493.jpg' /><br />新概念英语全套1-4册+语法手册+词汇大全</a></li><li><a rel='nofollow' href='http://buy.hujiang.com/p24579/?source=242' target='_blank'><img src='https://i1b.hjfile.cn/shop/201402/201402104145473514.jpg' /><br />新概念英语1-4册系列套餐（练习册）共四册</a></li><li><a rel='nofollow' href='http://buy.hujiang.com/p11095/?source=242' target='_blank'><img src='https://i1b.hjfile.cn/shop/201402/201402104194350073.jpg' /><br />新概念英语1-4册(学生用书+配套练习册)套餐</a></li><li><a rel='nofollow' href='http://buy.hujiang.com/p1661/?source=242' target='_blank'><img src='https://i1b.hjfile.cn/shop/200803/2008032811074372351.jpg' /><br />新概念英语词汇大全</a></li><li><a rel='nofollow' href='http://buy.hujiang.com/p406/?source=242' target='_blank'><img src='https://i1b.hjfile.cn/shop/201403/2014032111321815171.jpg' /><br />新概念英语 新版 1-2册学习套餐</a></li></ul></div>");
		}
	});
</script>	 
