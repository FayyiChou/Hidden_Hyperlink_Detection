<script language="javascript" type="text/javascript">
  function urlFactory()
  {
    var url = "/n/search/";
    var cateID = document.getElementById("lstType").value;
    if (cateID == "2") url = "/dl/search/";
    url =url+ (encodeURI(document.getElementById("txtSearch").value.replace(/^\s+|\s+$/g, "")))+"/";
    return url;
  }

  function txtKeyDown(evt)
  {
    if (evt.keyCode == 13)
    {
      if (document.getElementById("txtSearch").value.replace(/^\s+|\s+$/g, "") != null && document.getElementById("txtSearch").value.replace(/^\s+|\s+$/g, "") != "")
      {
        searchRedirect();
      }
    }
  }

  function searchRedirect()
  {
    var recordName = "文章";
    if (document.getElementById("lstType").value == "2") recordName = "下载";
    window.location.href = urlFactory();
  }
</script>	
