	<script src="//cdn.bootcss.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>



	<script src="//cdn.bootcss.com/flexslider/2.6.0/jquery.flexslider-min.js"></script>

	<script src="//cdn.bootcss.com/Swiper/2.7.6/idangerous.swiper.js"></script>

	<script src="//cdn.bootcss.com/jQuery.dotdotdot/1.7.4/jquery.dotdotdot.min.js"></script>

	<script type="text/javascript">

		$(function() { 

			//幻灯片

			$(".flexslider").flexslider({

				slideshow: true,

				slideshowSpeed: 5000,

			});

			$('.flexslider').height($(window).height()/2);

			$('.banner').height($(window).height()/2);

			$('.slides').height($(window).height()/2);



			$('.item-description').dotdotdot();





			var mySwiper = new Swiper('.product-slider-container',{

				loop:'true',

			});

			/*项目按钮滑动*/

			var scroll_btn = $('.product-scroll>a');

			scroll_btn.each(function(index,item){

				scroll_btn.eq(index).click(function(){

					$(this).addClass('scroll-active').siblings().removeClass();

					mySwiper.swipeTo($(this).attr('data'), 1000, false);

				});

			});



			//页面大小改变时自适应

			$(window).resize(function(event) {

				$('.item-description').dotdotdot();

				$('.flexslider').height($(window).height()/2);

				$('.banner').height($(window).height()/2);

				$('.slides').height($(window).height()/2);

			});

		});

	</script>

