            jq(".deancity ul li").each(function(s){						
                jq(this).hover(function(){
                    jq(".deanliname").removeClass("deanboxlihover");
                    jq(this).children(".deanliname").addClass("deanboxlihover");
                    jq(".deanlicontent").hide();
                    jq(".deanlicontent").eq(s).show();
                    })
                })
        </script>
