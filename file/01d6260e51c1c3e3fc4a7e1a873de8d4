<script>
$(function(){
$('#roll').hide();
$(window).scroll(function() {
if($(window).scrollTop() >= 100){
$('#roll').fadeIn(400);
    }
    else
    {
    $('#roll').fadeOut(200);
    }
  });
  $('#roll_top').click(function(){$('html,body').animate({scrollTop: '0px'}, 800);});
});
</script>
<style type="text/css">
#roll_top{
position:relative;
cursor:pointer;
height:53px;
width:54px;
}
