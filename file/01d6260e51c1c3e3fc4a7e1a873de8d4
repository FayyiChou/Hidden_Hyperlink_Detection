<script>
$(function () {
    $(window).scroll(function () {
        var offsetTop = $(window).scrollTop() + 150 + "px";
        $('.zj-container').animate({top: offsetTop}, {duration: 500, queue: false});
        
    });
    //添加鼠标放上样式
    $(".zj-container .content-list .gn-list").hover(function(){
        $(this).find("span").addClass("span-hover");
    },function(){
        $(this).find("span").removeClass("span-hover");
    });
});
</script>    
