                $(".global_nav div .tall").hover(function (e) {
                        $(this).siblings().removeClass("hover").find(".gl_nav_con").hide();
                        $(this).addClass("hover");
                        $(this).find(".gl_nav_con").show();
                    },
                    function (e) {
                        $(this).removeClass("hover");
                        $(this).find(".gl_nav_con").hide();
                    }
                );
                $(".tall").last().css("border-bottom", "0");
                $(".user-login").mouseenter(function () {
                    $(".login-show-name").show();
                    $(".not-login .p2 i").css("background", "url(/usertpl//dsc_header/images/user-login.png) no-repeat 0 -38px")
                });
                $(".user-login").mouseleave(function () {
                    $(".login-show-name").hide();
                    $(".not-login .p2 i").css("background", "url(/usertpl//dsc_header/images/user-login.png) no-repeat 0 -43px")
                })
            </script>
