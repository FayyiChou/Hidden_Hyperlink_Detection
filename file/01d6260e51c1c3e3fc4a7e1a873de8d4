<script language="javascript">
    var SITEURL = "http://www.scygy.com/";
    $(function(){
        //导航的选中状态
        $(".nav .menu a").each(function(){
            var url= window.location.href;
            url=url.replace('index.php','');
            url=url.replace('index.html','');
            var ulink=$(this).attr("href");
            if(url==ulink)
            {
                $(this).parent().parent().children('li').children('a').removeClass('active');
                $(this).addClass('active');
                $(this).prev().addClass('bnone');
            }else{
                $(this).removeClass('active');
            }
        })
        //登陆状态
        $.ajax({
            type:"POST",
            url:SITEURL+"member/login/ajax_is_login",
            dataType:'json',
            success:function(data){
                if(data.status){
                    $txt = '<a class="dl" href="javascript:;">你好,'+data.user.nickname+'</a>';
                    $txt+= '<a class="dl" href="/member/login/loginout">退出</a>'
                    $txt+= '<a class="dl" href="/member">个人中心</a>"'
                }else{
                    $txt = '<a class="dl" href="/member/login">登录</a>';
                    $txt+= '<a class="zc" href="/member/register">免费注册</a>';
                }
                $("#_userLogin").html($txt);
                hoverCssChange();
            }
        })
        //云搜索点击
        $(".cloud_search").click(function(){
            var keyword = $(".g_search").val();
            var def = $(".g_search").attr('dsfdsf');
            if(keyword=='' || keyword == def){
                $(".g_search").focus();
            }
            else{
                var typeid = $("#typename").attr('data-id');var searchUrl = SITEURL+'search/cloudsearch?keyword='+encodeURIComponent(keyword);
                window.open(searchUrl,'_self');
            }
        })
        //微信指向
        $(".wx_ico").hoverDelay(
            function(){
                $(".wx_img").show();
            },
            function(){
                $(".wx_img").hide();
            }
        )
        //导航指向
        $(".top_web_map").hoverDelay(
            function(){
                $(".w_nav_map").show();
            },
            function(){
                $(".w_nav_map").hide();
            }
        )
        hoverCssChange();
    })
    function hoverCssChange()
    {
        //指向样式
        $(".bor_bag").mouseover(function(){
            $(this).css({"background":"#fff","border-left":"1px solid #e6e5e5","border-right":"1px solid #e6e5e5"})
            $(this).children(".web_nav_ico").css("background","#fff")
        })
        $(".bor_bag").mouseout(function(){
            $(this).css({"background":"none","border-left":"1px solid #f1f1f1","border-right":"1px solid #f1f1f1"});
            $(this).children(".web_nav_ico").css("background","#f1f1f1");
        })
    }
    $(".global_nav div .tall").hover(function(e){
            $(this).siblings().removeClass("hover").find(".gl_nav_con").hide();
            $(this).addClass("hover");
            $(this).find(".gl_nav_con").show();
        },
        function(e){
            $(this).removeClass("hover");
            $(this).find(".gl_nav_con").hide();
        }
    );
//自定义导航子菜单高度设置
$(".global_nav div .tall .gl_nav_con").each(function(){
var h1 = $(".global_nav .mc").offset().top;
var h2 = $(this).parent().offset().top;
$(this).css("top",(h1-h2)+"px");
});
    $(".tall").last().css("border-bottom","0")
    $(".user-login").mouseenter(function(){
        $(".login-show-name").show()
        $(".not-login .p2 i").css("background","url(images/user-login.png) no-repeat 0 -38px")
    })
    $(".user-login").mouseleave(function(){
        $(".login-show-name").hide()
        $(".not-login .p2 i").css("background","url(images/user-login.png) no-repeat 0 -43px")
    })
    $(document).ready(function() {
            var url = window.location.href;
            if(SITEURL != url)
            {
               // $('.global_nav').hide();
            }
            else
            {
                //$('.global_nav').show();
            }
        }
    );
</script>
