    <script>

        $(document).ready(function(){
            ClearVideoHtml();
            //var ="2b"

            //获取文件大小
            var fvid=$("#videoId").val();
            $(".fileSizeBtn").mouseover(function(){
                //通过ajax方式获取视频文件大小
                var dfSeek = $(this).attr("df");
                var title = $(this).parents("a").attr("title");
                var obj =$(this).parents("a");
                if(!title){
                    var urlfs="http://v3.polyv.net/videojson/"+fvid+".js";
                    $.ajax({
                        type: "POST",
                        url: urlfs,
                        data: "{}",
                        contentType: "application/json;charset=utf-8;",
                        dataType: "jsonp",
                        success: function (fs_data) {
                            //console.log(fs_data);
                            if(dfSeek=="10"){
                                var sourceFileSize=Math.round(fs_data.source_filesize/1024/1024) +"MB";
                                if(fs_data.source_filesize){
                                    $(obj).attr("title",sourceFileSize);
                                }
                            }

                            if(dfSeek=="0"){
                                var videoFileSize=Math.round(fs_data.filesize[0]/1024/1024) +"MB";
                                if(fs_data.filesize[0]){
                                    $(obj).attr("title",videoFileSize);
                                }
                            }

                            if(dfSeek=="1"){
                                var videoFileSize=Math.round(fs_data.filesize[2]/1024/1024) +"MB";
                                if(fs_data.filesize[2]){
                                    $(obj).attr("title",videoFileSize);
                                }
                            }

                            if(dfSeek=="2"){
                                var videoFileSize=Math.round(fs_data.filesize[1]/1024/1024) +"MB";
                                if(fs_data.filesize[1]){
                                    $(obj).attr("title",videoFileSize);
                                }
                            }

                            if(dfSeek=="3"){
                                var videoFileSize=Math.round(fs_data.filesize[0]/1024/1024) +"MB";
                                if(fs_data.filesize[0]){
                                    $(obj).attr("title",videoFileSize);
                                }
                            }

                        }
                    });
                }
            });

        });

        //去掉视频摘要的html
        function ClearVideoHtml(){
            var vtDom = $(".summary-v");
            vtDom.each(function(index){
                var DText = $(this).text();
                var Dvalue = DText.replace(/<\/?[^>]*>/g,"");
                $(this).text(Dvalue);
            });
        }

        var show = true;
        function listclick(){
            if(show){
                $(".col-vr").css("width","10px");
                $(".col-vl").css("width","1128px");
                $("#list_handle").find("div").html("‹");
                $("#textbox").hide();
            }else{
                $(".col-vr").removeAttr("style");
                $(".col-vl").removeAttr("style");
                $("#list_handle").find("div").html("›");
                $("#textbox").show();
            }
            show=!show;
        }

        $("#list_handle").click(function(){
            listclick();
        });


        $("#download-Btn").click(function(){
            $(".change-btn-list").toggle();
        });

        $("#switch-player").click(function() {
            if ($(this).hasClass("play")) {
                $(this).addClass("listen").removeClass("play").html("看视频");
                $("#player object").attr("height", "45");
                $(".video-border").css("height", "45px");
                show = true;
                listclick();
                $(".col-vr").hide();
                $(".col-vl").css("width", "100%");
                document.getElementsByTagName('body')[0].scrollTop = 120;
                player.j2s_skinFloat(false);
            } else {
                $(this).removeClass("listen").addClass("play").html("听视频");
                $("#player object").attr("height", "500");
                $(".video-border").css("height", "500px");
                show = false;
                listclick();
                $(".col-vr").show();
                $(".col-vl").css("width", "820px");
                document.getElementsByTagName('body')[0].scrollTop = 120;
                player.j2s_skinFloat(true);
            }
        });


        //滚动条,IE8不兼容
        var isIE = !!window.ActiveXObject;
        var isIE8 = isIE && !!document.documentMode;
        if(!isIE8){
            //$('#textbox').perfectScrollbar();
        }else{
            $(".c-right").css({"width":"120px"});
        }
        //Ps.initialize(document.getElementById('textbox'));

    </script>



