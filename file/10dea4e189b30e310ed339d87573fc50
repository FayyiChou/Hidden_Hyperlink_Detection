<script>
    $(function (){

        // 一级导航
        $(".video-menu > li").hover(function(e){
            //捕获焦点
            var $a = $(this).find("div a");
            if( !$a.hasClass('active') ) {
                $(this).find("ul").show();
                $(".video-menu .active").parent().parent().find("ul").hide();
            }
            var $ul = $(this).find("ul");
            moveLi($ul);
        },function(){
            //失去焦点
            var $a = $(this).find("div a");
            if( !$a.hasClass('active') ) {
                $(this).find("ul").hide();
                $(".video-menu .active").parent().parent().find("ul").show();
            }
        });
        // 二级导航
        $(".video-menu li ul").hover(function(){
            //捕获焦点
            $(this).parent().find("div a").addClass("act");

        },function(){
            //失去焦点
            $(this).parent().find("div a").removeClass("act");
        });
        $(".sub-active").parent().parent().parent().find("ul").show();
        $(".sub-active").parent().parent().parent().find("div a").addClass("active");

        // 判断是否越界，若是则做移动
        function moveLi($ul){
            if(undefined == $($ul).html()){
                return;
            }
            var isIE8 = navigator.userAgent.indexOf("MSIE") > 0 ? navigator.userAgent.indexOf("MSIE 8.0") > 0 : false;
            var li_size = $($ul).find("li").size();
            var ul_width = $($ul).find("li").eq(li_size-1).width() + $($ul).find("li").eq(li_size-1).offset().left - $($ul).find("li").eq(0).offset().left + 10;
            if(isIE8) {
                ul_width += 5;
            }
            $($ul).css({"width":ul_width+"px"});

            var li_left = $($ul).find("li").eq(li_size-1).offset().left;
            var lis_width = $($ul).find("li").eq(li_size-1).width() + li_left;
            var con_width = $(".video-nav").width();
            if(isIE8) {
                lis_width += 14;
            }
            if (lis_width > con_width ) {
                if(isIE8) {
                    lis_width -= 14;
                }
                var x = (con_width - lis_width);
                x -= 100;
                if(isIE8){
                    x -= 112;
                }
                $($ul).css({"margin-left":x+"px"});
            }
        }
        var $ul = $(".video-menu > li ").find(".active").parent().parent().find("ul");
        moveLi($ul);

    })
</script>
 
    
