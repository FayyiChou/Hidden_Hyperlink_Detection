            <SCRIPT type=text/javascript>
            /*********箭头控制滚动方向、加速及鼠标拖动***************/
            var MarqueeDiv2Control = new Marquee("MarqueeDiv2"); 	//箭头控制滚动方向、加速及鼠标拖动实例
            MarqueeDiv2Control.Direction = "left";
            MarqueeDiv2Control.Step = 1;
            MarqueeDiv2Control.Width = 670;
            MarqueeDiv2Control.Height = 143;
            MarqueeDiv2Control.Timer = 30;
            MarqueeDiv2Control.ScrollStep = 1; 			//若为-1则禁止鼠标控制实例
            MarqueeDiv2Control.Start();
            document.getElementById("LeftButton2").onmouseover = function () { MarqueeDiv2Control.Direction = 2 };
            document.getElementById("LeftButton2").onmouseout = document.getElementById("LeftButton2").onmouseup = function () { MarqueeDiv2Control.Step = MarqueeDiv2Control.BakStep };
            document.getElementById("LeftButton2").onmousedown = document.getElementById("RightButton2").onmousedown = function () { MarqueeDiv2Control.Step = MarqueeDiv2Control.BakStep + 3 };
            document.getElementById("RightButton2").onmouseover = function () { MarqueeDiv2Control.Direction = 3 };
            document.getElementById("RightButton2").onmouseout = document.getElementById("RightButton2").onmouseup = function () { MarqueeDiv2Control.Step = MarqueeDiv2Control.BakStep };
                </SCRIPT>
