<script> 
$(function(){
  
    var inputArr = $('#wap-form-login').find(".form-control");
    inputArr.each(function(index){
        $(this).focus(function(){
            $(this).addClass("textblack");
        });
    })
    inputArr.each(function(){
        $(this).blur(function(){
            if( $(this).val() == $(this)[0].defaultValue){
                $(this).removeClass("textblack");
            }
        })
    })
    $(window).resize(function(){
        inputArr.css("width","100%")
    });
    $('#wap-form-login').submit(function(e){
        //WAP版返回不刷新
        //if($('.btn-primary').text()=='登录中...') return false;      
        
        var uValue = $("#wap-usename").val();
        var pValue = $("#wap-passwd").val();
        var cValue = $("#wap-code").val();  
        var flag = true;
        $(".text-danger").remove();
    
        if(pValue=="" || uValue=="" ){
            var $tag = $('<p class="text-danger">请输入用户名和密码</p>');
            $("#wap-form-login").find(".form-group").eq(0).before($tag);    
            flag=false;
        }else if(cValue == ""){
            var $tag = $('<p class="text-danger">请输入图形验证码</p>');
            $("#wap-form-login").find(".form-group").eq(0).before($tag);    
            flag=false;
        }
//      if(cValue==""){
//          var $tag = $('<p class="text-danger">验证码输入有误</p>');
//          $("#wap-form-login").find(".form-group").eq(0).before($tag);    
//          flag=false;
//      }
        if(!flag){
            e.preventDefault();
        }else{
            //$('.btn-primary').text('登录中...');
        }
    }); 
    
    
});


//点击 文字"验证码登录注册"时执行,连接转向
function jumpQuickLoginCode(){ 
    //获取账号
    var userName = $("#wap-usename").val();
    var resource = $("#resource").val();
    var fromType = $("#fromType").val();
    var backUrl = $("#backUrl").val();
    var flag = $("#flag").val();
    //当用户输入手机号之后,才会把手机带过去
    
    window.location.href="/account/toQuickLoginCodePage.htm?mobile="+userName
                    +"&resource="+resource
                    +"&fromType="+fromType
                    +"&flag="+flag
                    +"&backUrl="+backUrl; 
}


window.chageCodeImage = function (id){
    document.getElementById(id).src = "/account/validateCode.htm?d=" + new Date().getTime();
}

</script>
