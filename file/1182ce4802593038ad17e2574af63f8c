	<script type="text/javascript" src="js/move-top.js"></script>
	<script type="text/javascript" src="js/easing.js"></script>
	<script src="js/jquery.swipebox.min.js"></script>
	<script src="js/wow.min.js"></script>
	<script src="interact.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script>new WOW().init();</script>


	<script type="text/javascript">
		jQuery(function($) {
			$(".swipebox").swipebox();
		});
	</script>

	<script type="text/javascript">
		$(document).ready(function() {
			$('#rightArrow').click(function(){
				$(this).toggleClass('kefu-hide-rightArrow');
				var arr = $(this).prop("class").split(" ");
				if(arr[1] != undefined){
					$(this).children().find('i').addClass('fa-angle-right');
					$(this).children().find('i').removeClass('fa-angle-left');
					$(this).css("right","170px");
					$('#floatDivBoxs').css("right","0");
				}else{
					$(this).children().find('i').addClass('fa-angle-left');
					$(this).children().find('i').removeClass('fa-angle-right');
					$(this).css("right","0");
					$('#floatDivBoxs').css("right","-175px");
				}
				$('#floatDivBoxs').toggleClass('kefu-hide');
			});

			$().UItoTop({
				easingType: 'easeOutQuart'
			});

		});
		function nav_click(obj){
			$('html,body').animate({
				scrollTop:$($.attr(obj, 'href')).offset().top
			},1000);
		}
	</script>
