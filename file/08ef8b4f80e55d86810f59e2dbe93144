    function getQueryString(key){
        var url = document.location;
        if(key){
            return (url.search.match(new RegExp("(?:^\\?|&)"+key+"=(.*?)(?=&|$)"))||['',null])[1];
        }
        else{
            var params = url.search,reg=/(?:^\?|&)(.*?)=(.*?)(?=&|$)/g,temp,args={};
            while((temp=reg.exec(params))!=null) args[temp[1]]=decodeURIComponent(temp[2]);
            return args;
        }
    };
    var options = getQueryString();
    var commentList = new discuss.CommentList(jq('#comment_pannel'),options.operationId,options.contentId,null,null);
    commentList.showPage(1,10,0);
</script>
