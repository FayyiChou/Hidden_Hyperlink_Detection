<script>
$(function()
{
	var timer = null;
	var num =0;
  $(".rows li").on('click' , function()
  {
	  var aIndex = $(this).index(".rows li");
	 show(aIndex)
  })
  
  function show(aIndex)
  {
	  $(".tab li").eq(aIndex).show().siblings().hide();
	  $(".rows li").eq(aIndex).addClass("cur").siblings().removeClass("cur");
  };
function start()
{
	  timer = setInterval(function(){
	  num++;
     if(num> $(".rows li").length-1)
	 {
		 num = 0;
     }
	 show(num);
  },1000)
};
start();
  
  $(".news_list").hover(function()
  {
	  clearInterval(timer);
  },function()
  {
	   start()
  });
});
</script>
