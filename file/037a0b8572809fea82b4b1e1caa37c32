		<script	type="text/javascript>
		$(function(){
			$(".mainNews").find('a').hide();
			$(".mainNews").find('a:first-child').show();
			setInterval("changeNew()",3000); 
		});
		//换新闻
		function changeNew(){
			var newsDiv=$(".mainNews"); 
			var firstChild = newsDiv.find('a:first-child');
			firstChild.hide();
			newsDiv.append(firstChild);
			newsDiv.find('a:first-child').show('slow');
			
		}
		</script>
