var index2=document.getElementById("index2"); 
var index1=document.getElementById("index1"); 
function marqss(){ 
if(index2.offsetTop-index.scrollTop<=0) 
 index.scrollTop-=index1.offsetHeight 
else{ 
index.scrollTop++ 
} 
} 
var MyMar=setInterval(marqss,speed) 
