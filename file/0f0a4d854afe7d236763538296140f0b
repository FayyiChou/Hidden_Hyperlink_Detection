﻿<script>
$(function() {
var cache = [];

// Arguments are image paths relative to the current page.
$.preLoadImages = function() {
var args_len = arguments.length;
for (var i = args_len; i--;) {
var cacheImage = document.createElement('img');
cacheImage.src = arguments[i];
cache.push(cacheImage);
}
}
jQuery.preLoadImages("/Uploads/201505/5555b4dd45f79.jpg","/Uploads/201405/537c011479274.jpg","/Uploads/201406/53952a27754eb.jpg","/Uploads/201405/5386a856df839.jpg","/Uploads/201406/539528157abd2.jpg",null);
})
</script>

