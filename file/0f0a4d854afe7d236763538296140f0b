<script>
var fixImg = $("#fixImg");
fixImg.css("opacity",0);
var fixTop = 0,fixLeft = 0; fixSpeed=10,normalHeight=fixSpeed,normalWidth=fixSpeed*2;
function fixAnimate() {
	if($("#fixImg").width() / $(window).width() > 0.3)
	$("#fixImg").css("width",$(window).width() * 0.3)
	var rangeWidth = $(window).width()-$("#fixImg").width(),rangeHeight = $(window).height()-$("#fixImg").height();
	fixTop += normalHeight;
	fixLeft += normalWidth;
	if(fixTop > rangeHeight)
	normalHeight=-fixSpeed;
	if(fixLeft > rangeWidth)
	normalWidth=-fixSpeed;
	if(fixLeft <=0)
	normalWidth =fixSpeed;
	if(fixTop <=0)
	normalHeight =fixSpeed
	
	fixImg.stop().animate({
		"top":fixTop,
		"left":fixLeft
	})
	//console.log(fixTop)
}
	setTimeout(function(){
	fixImg.animate({"opacity":1},0);
	var add = setInterval(fixAnimate,100);
	$("#fixImg").mouseenter(function(){
		clearInterval(add)
	})
	$("#fixImg").mouseleave(function(){
		add = setInterval(fixAnimate,100);
	})
	},total)
	$(".close").click(function(){
		fixImg.hide()
	})
</script>
