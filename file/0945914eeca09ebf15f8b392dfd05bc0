  $('#slideshow').cycle({
    fx:'fade',
    speed:  1000,
    timeout: 5000,
    before:  onBefore, 
    after:   onAfter
  });
});
function onBefore() { 
  $('#descrizione p').fadeOut('1000');}
function onAfter() {
  $('#descrizione p').fadeIn('1000');
  $('#descrizione').html('<p>' + this.alt + '</p>');
}
