    var basedReg = {
            _setTime: function(i,fn){
                this.number = fn(i);
            },
            tabsdiv: {
                _switch: true,
                _execute: function (_tabs_context,animateFn,reClick,speed) {
                    return function () {
                        if (basedReg.tabsdiv._switch == false) {
                            return false;
                        }
                        if (reClick == false) {
                            if ($(this).hasClass('cur')) {
                                return false;
                            }
                        }
                        if ($(this).hasClass('qd')) {
                            return false;
                        }
                        var num = $(this).index();
                        $(this).siblings().removeClass('cur');
                        $(this).addClass('cur');
                        if(animateFn && speed>=0){
                            basedReg.tabsdiv._switch = false;
                            animateFn(speed,$(_tabs_context),num);
                            setTimeout('basedReg.tabsdiv._switch = true',speed);
                        }else if(speed >= 0){
                            basedReg.tabsdiv._switch = false;
                            setTimeout('basedReg.tabsdiv._switch = true',speed);
                        }
                    }
                }
            }
        }

</script>

