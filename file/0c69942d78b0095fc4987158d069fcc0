        var _height = $('.header').height(),
            _footHeight = $('#foot').height(),
            _winHeight = $(window).height()-(_height+_footHeight),
            $mains = $('#page1 .main');

        $mains.height(_winHeight);
        $('.moive').width($('.moive').find('.video-pic').width());
        $('.speech-pic').width($('.speech-pic').find('img').width());
        $('.ph .box').width($('.ph .box').find('img').width());
//        $('.speech-list-bg').width($('.speech-list-bg').find('img').width());
        $('.fine .box').width($('.fine .box').find('img').width());

        $(window).resize(function(){
            _winHeight = $(window).height()-_height;

            $mains.height($(window).height()-(_height+_footHeight));
            $('.moive').find('.video-pic').css('width','auto');
            $('.moive').width($('.moive').find('.video-pic').width());
            $('.speech-pic').width($('.speech-pic').find('img').width());
            $('.ph .box').width($('.ph .box').find('img').width());
            $('.speech-list-bg').width($('.speech-list-bg').find('img').width());
            $('.fine .box').width($('.fine .box').find('img').width());
        });


        $('#control0 li').click(basedReg.tabsdiv._execute('.tocDiv',function(speed,elem,num){
//            elem.hide().eq(num).show();
            elem.css({opacity:0,zIndex:0}).eq(num).css({opacity:1,zIndex:100});
            $('.speech-list-bg').width($('.speech-list-bg').find('img').width());
