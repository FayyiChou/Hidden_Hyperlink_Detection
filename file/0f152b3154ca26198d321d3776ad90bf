	function Addjudge(form)
	{
		if (form.email.value=="" )
		{	
			alert("用户名(邮箱)必须填写")
			form.email.focus();
			return false;
		}
		if (form.password.value=="")
		{
			alert("密码必须填写")
			form.password.focus();
			return false;
		}	
		
	}
</script>
