	function getSrvData(curPage) {
		var actionUrl = '/ajax/servicePage.action?siteId=1&curPage=' + curPage  + '&pageSize=6&timestamp=' + new Date().getTime();
		$.ajax({
  	  url: actionUrl,
    	type: 'GET',
	    dataType: 'json',
  	  timeout: 30000,
    	error: function(){
        //alert('Error loading!');
	    },
  	  success: function(data){
				$("#srvlist").empty();
				$.each (
				 	data.list,
				 	function(i,row) {
				 		var item1 = ''; 
				  	var item2 = ''; 
				  	var item3 = ''; 
				  	var item4 = ''; 
				  	var item5 = ''; 
				  	var iid = 0;
					 	$.each (
					 		row.flows,
						  function(i,row) {  	
						  	if (row.name == "办事指南") {
						 // 		item1 = '<a href="/main/service/content/index.action?id=' + row.iid + '" target=_blank class="a2">办事指南</a>';
						  		iid = row.iid;
						  	}
						  	if (row.name == "表格下载") {
						  		item2 = '<a href="/main/service/content/index.action?fid=' + row.id + '" target=_blank class="a2">表格下载</a>';
						  	}
						  	if (row.name == "在线咨询") {
						  		item3 = '<a href="' + row.link + '" target=_blank class="a3">在线咨询</a>';
						  	} 
						  }
					 	);
						var flow =  item1 +  item2+  item3 ;
			 			if (iid == 0) {
				 			flow =  '<li>'+ row.name  + flow+ '</li>';		
				 		} else {
		 					flow =  '<li><a href="/main/service/content/index.action?id=' + iid + '" title="' + row.name + '" target=_blank class="name">' + row.name +'</a>' + flow+'</li>';				 				
			 			}
 
						$("#srvlist").append(flow);
					}
				); 
	//		  var pageHtml = buildPageButton(data.total, data.curPage, data.pageCount, 'getSrvData', '');
	//			$("#srvpageButtons").html(pageHtml);			  
  	  }
		});
	}	
	function getData(sign, curPage) { 
		
		if (sign) {
			$("#tousu").fadeOut("fast");
		}   
		var actionUrl = '/ajax/exchangePage.action?did=100&curPage=1&pageSize=6&timestamp=' + new Date().getTime();
 
		$.ajax({
  	  url: actionUrl,
    	type: 'GET',
	    dataType: 'json',
  	  timeout: 30000,
    	error: function(){
        alert('Error loading!');
	    },
  	  success: function(data){
				$("#tousu").empty();
				$.each (
				 data.rows,
				 function(i,row) {
				 	var item = '<td class="tleft t2">　<a href="/main/exchange/suggestView/index.action?id=' + row.id + '" target=_blank>' + row.title + '</td><td>'+row.type + '</td><td>'+row.date+'</td><td class="tright">' + row.state + '</td>';
					var li = $('<tr>' + item + '</tr>');
					$("#tousu").append(li);
				 }
				);   	
  	  }
		});
	}
 			$(".adclose").click(function(){
				$(this).parent().css("display", "none");
			}); 
	$(function(){
		getData(false, 1);
		getSrvData(1);
	})

</script>	
