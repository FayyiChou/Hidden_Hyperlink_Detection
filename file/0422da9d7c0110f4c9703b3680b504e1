var checkedpoll=0;
function ValuePoll(value){
	checkedpoll=value;
}

function FormActionPoll(form){
	if(checkedpoll==0)alert ("Âû íè÷åãî íå âûáðàëè !!!");
	else{
	document.poll.poll.value=checkedpoll;
    document.poll.submit();
	}
}
</script>



