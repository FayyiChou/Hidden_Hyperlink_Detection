	function CheckedEmailForm(form){
		var field=document.email.email;
		var goodEmail = field.value.match(/\b(^(\S+@).+((\.com)|(\.net)|(\.edu)|(\.mil)|(\.gov)|(\.org)|(\.ru)|(\..{2,2}))$)\b/gi);
		if (goodEmail){
			return  true;
		} else {
			   alert('Ââåäåí íåïðàâèëüíûé email àäðåñ.')
			   field.focus()
			   field.select()
			   return  false;
		}
	}
</script>

