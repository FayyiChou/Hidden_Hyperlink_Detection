jQuery('#captcha-form').yiiActiveForm([], []);
jQuery(document).pjax("#captch-pjax a", {"type":"post","push":true,"replace":false,"timeout":1000,"scrollTo":false,"container":"#captch-pjax"});
jQuery(document).on("submit", "#captch-pjax form[data-pjax]", function (event) {jQuery.pjax.submit(event, {"type":"post","push":true,"replace":false,"timeout":1000,"scrollTo":false,"container":"#captch-pjax"});});
jQuery('#create-modal').modal({"show":false});
jQuery('#create-modal').on('show.bs.modal', function(){
			return Passport.isLoggedIn();
		});
jQuery('#taot-form').yiiActiveForm([], []);
jQuery(document).pjax("#taot-pjax a", {"push":true,"replace":false,"timeout":1000,"scrollTo":false,"container":"#taot-pjax"});
jQuery(document).on("submit", "#taot-pjax form[data-pjax]", function (event) {jQuery.pjax.submit(event, {"push":true,"replace":false,"timeout":1000,"scrollTo":false,"container":"#taot-pjax"});});
jQuery('#taot-modal').modal({"show":false});
jQuery('#taot-modal').on('show.bs.modal', function(){
		return Passport.isLoggedIn();
	});
	$(".btn-th[data-target='#taot-modal']").click(function(){
		if(!Passport.isLoggedIn()){
		Passport.Dialog.show()
	    }else{
		if($('#taot-form-submitButton').length>0)
		$('#taot-form-submitButton').click();
		}
		
	});
$('#follow_id').click(function () {
	if(!Passport.isLoggedIn())
		Passport.Dialog.show()
	else{
		var followType = $(this).attr('data-type');
		if(followType=='del'){
			return true;
		}else{
			$.ajax({
				url: "/games/follow?id=945",
				type: "POST",
				dataType: "json",
				data: $('#follow-form').serialize(),
				success: function(Data) {
					if(Data.status){
						var followobj = $('#follow_id');
						followobj.attr('data-type','del');
						followobj.attr('href','/follow.shtml');
						followobj.html('取消关注');
						alert('关注成功！');
					}else{
						alert('关注失败！');
					}
				   
				},
				error: function() {
					alert('网络错误！');
				}
			});
		}
	}
    return false;
});
jQuery('#follow-form').yiiActiveForm([{"id":"follow-form-gid","name":"gid","container":".field-follow-form-gid","input":"#follow-form-gid","validate":function (attribute, value, messages, deferred, $form) {yii.validation.required(value, messages, {"message":"Gid不能为空。"});yii.validation.number(value, messages, {"pattern":/^\s*[+-]?\d+\s*$/,"message":"Gid必须是整数。","skipOnEmpty":1});}}], []);
	jQuery(document).on('click', '.card-btn', function () {
		if(!Passport.isLoggedIn())
		Passport.Dialog.show()
	});
	jQuery(".m-infoWrap").tabCur();
	jQuery(document).on({
		ajaxStart: function() { jQuery("body").addClass("loading");    },
		ajaxStop: function() { jQuery("body").removeClass("loading"); }    
	});
	$('img').lazyload({effect: 'fadeIn'});
	$('body').on('hidden.bs.modal', '#create-modal', function () {
		$(this).removeData('bs.modal');
	});
	Passport.on(Passport.EVENTS.loginSuccess,function(){window.location.reload()});
    var clipboard = new Clipboard('.dialog_btn');
    clipboard.on('success', function(e) {
		$('.alert-help').html('<div class="alert alert-danger" role="alert">复制成功</div>');
        console.log(e);
    });
    clipboard.on('error', function(e) {
		$('.alert-help').html('<div class="alert alert-danger" role="alert">复制失败，请手动选中礼包码进行复制。</div>');
        
        console.log(e);
    });
});</script>

