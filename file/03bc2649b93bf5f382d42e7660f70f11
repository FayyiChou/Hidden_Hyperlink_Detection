    <!-- The JavaScript -->
    <script type="text/javascript" src="/CN/style/js/jquery.min.js"></script>
    <script type="text/javascript" src="/CN/style/js/common.js"></script>
    <script type="text/javascript">
        $(function () {
            /*box-tab*/
            var $box = $(".box-tab"),
			boxH = $box.children(".view").height(),
			boxL = $box.find(".view li").length,
			boxViUl = $(".box-tab").find(".view ul"),
			boxIndex = 0, boxPicTimer;
            $(".box-tab .btn span").mouseenter(function () {
                boxIndex = $(".box-tab .btn span").index(this);
                showPics(boxIndex);
            }).eq(0).trigger("mouseenter");
            boxViUl.css("height", boxH * (boxL + 1));
            $(".box-tab").children(".view").hover(function () {
                clearInterval(picTimer);
            }, function () {
                picTimer = setInterval(function () {
                    if (boxIndex == boxL) {
                        showFirPic();
                        boxIndex = 0;
                    } else {
                        showPics(boxIndex);
                    }
                    boxIndex++;
                }, 7000);
            }).trigger("mouseleave");

            function showPics(index) {
                var nowLeft = -index * boxH;
                boxViUl.stop(true, false).animate({ "top": nowLeft }, 500);
                $(".box-tab .btn span").removeClass("on").eq(index).addClass("on");
            }

            function showFirPic() {
                boxViUl.stop(true, false).animate({ "top": 0 }, 500);
                $(".box-tab .btn span").removeClass("on").eq(0).addClass("on"); //为第一个按钮添加选中的效果
            }
        })

    </script>

