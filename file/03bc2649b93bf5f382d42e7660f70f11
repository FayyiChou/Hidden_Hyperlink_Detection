                        <script language="javascript" type="text/javascript">
                            function search_onclick() {
                                var keywords = $("#keywords").val();
                                var selectstr = $("#selectstr").html().trim();
                                if (keywords == "" || keywords == "搜索其实很容易") {
                                    alert("请输入要搜索的内容！");
                                }
                                if (selectstr == "请选择") {
                                    alert("请选择您要搜索的类型！");
                                }
                                var urlstr = "";
                                switch (selectstr) {
                                    case "视频":
                                        urlstr = "/CN/Video/List.aspx";
                                        break;
                                    case "图书":
                                        urlstr = "/CN/Book/List.aspx";
                                        break;
                                    case "资讯":
                                        urlstr = "/CN/Info/List.aspx";
                                        break;
                                }
                                window.location.href = urlstr + "?keywords=" + escape(keywords);
                            }
                        </script>
