</script>

<script type="text/javascript">
    function showService() {
        $("#service").show();
        $("#service .kf").css("left", ($(window.top.window).width() - $("#service .kf").width()) / 2 + "px");
        $("#service .kf").css("top", ($(window.top.window).height() - $("#service .kf").height()) / 2 + "px");
    }

    //加入收藏 
    function AddFavorite(sURL, sTitle) {
        sURL = encodeURI(sURL);
        try {
            window.external.addFavorite(sURL, sTitle);
        } catch (e) {
            try {
                window.sidebar.addPanel(sTitle, sURL, "");
            } catch (e) {
                msgbox.alert("加入收藏失败，请使用Ctrl+D进行添加,或手动在浏览器里进行设置.");
            }
        }
    }
</script>


