        $(function() {
            $(".li1").hover(function() {
                $(this).addClass("on").siblings(".li1").removeClass("on");
                $(".shoppingList:eq(" + $(this).index() + ")").siblings(".shoppingList").hide();
                $(".shoppingList:eq(" + $(this).index() + ")").stop(false, true).show(500);
            })

            $(".programa").mouseover(function() {
                $(this).stop(true, true).animate({ "width": "140px" }, 200).find("a").stop(true, true).animate({ "width": "110px" }, 200).parent().siblings(".programa").stop(true, true).animate({ "width": "98px" }, 200).find("a").stop(true, true).animate({ "width": "70px" }, 200);
            })

            $(".programa").mouseout(function() {
                $(this).stop(true, true).animate({ "width": "98px" }, 200).find("a").stop(true, true).animate({ "width": "70px" }, 200);
            })
            $(".qgList,.news").find("li:last-child").css("border", "0");
        })
    </script>

