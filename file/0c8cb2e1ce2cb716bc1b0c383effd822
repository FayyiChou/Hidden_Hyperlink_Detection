	function jsonCallBack(url,callback)
	{
		$.getScript(url,function(){
		callback(json);
	});
	}
	function echowin(){
	var $winhtm = '';
		$winhtm += '<div class="callwin" style="display:none;"><div class="body">';
		$winhtm += '<iframe id="if6" name="if6" frameBorder="0" scrolling="auto" src="/err/loading.html" width="100%" height="100%">';
		$winhtm += '</iframe></div><div class="tool"><a class="xx" href=" javascript:;">×</a></div></div>';
		$('body').append($winhtm);
	}
	function callwin(){
		var p61Width = ($(window).width()/2)-500;
		var p61Height = ($(window).height()/2)-($(window).height()-120)/2+ $(document).scrollTop();
		$('.callwin').css({top:p61Height+'px',left:p61Width+'px'});
		$('.callwin .body').css('height', $(window).height()-120-48+'px');
		$(".callwin .xx").click(function(){$(".callwin").hide();$('iframe#if6').attr('src', '/err/loading.html');});
		$('iframe#if6').attr('src', '/err/loading.html');
		$('.callwin').show();
	}
	function bindp61()
	{
	$(".panel6-1 a").click(function(){
		if($(this).attr("data")){//alert('+');
			var p61str = $(this).attr("data").split(',');
			var p61ip = 'http://bbs.newglady.com';
			switch (p61str[0]){
				case "T": $gogourl = p61ip + '/mini.php?mod=viewthread&tid=' + p61str[1];break;
				case "F": $gogourl = p61ip + '/mini.php?mod=forumdisplay&fid=' + p61str[1];break;
				case "#": $gogourl = p61ip + '/mini.php?mod=post&action=newthread&fid=' + p61str[1];break;
			}
			callwin();
			$('iframe#if6').attr('src', $gogourl);
		return false;
		}
	});
	}
	function unbindp61(){$(".panel6-1 a").unbind("click");}
	function funCallBack()
	{
		var loading = '<div class="loading">努力加载中...</div>';
		$(".panel6-1").html(loading);
		jsonCallBack('http://bbs.newglady.com/mini.php',function(json){
		if(json.result==200) {
			$(".panel6-1").html(json.message);
			$(function () {focus3('#focus3-2', 5000)});
			bindp61();echowin();
			$(".panel6-1 #new_win").click(function(){
				if($(this).attr("checked")) unbindp61();else bindp61();
			});
		}
		})
	}
	$(window).bind("scroll",function(){
    var p61x = $('.panel6-1').offset().top;
	var scrx = $(document).scrollTop();
		if($('.panel6-1').html()==''){
		if(scrx>p61x-$('.panel6-1').height()+100){funCallBack();//$(window).unbind("scroll");
		}
		}
    });
})
