	function focus3(fc,tm) {
		var num_tmp = "";
		var numlag = $(fc + " ul").children('li').length;
		$(fc + " ul li").each(function(index){
		if($(this).is(".block")) num_tmp += "<a class='cur'>"+(index+1)+"</a>";
		else num_tmp += "<a>"+(index+1)+"</a>";
		});
		$(fc + " .num").html(num_tmp);
		var sw = 0;
		$(fc + " .num a").click(function () {
			sw = $(this).index();
			myShow(sw);
		});
		var myShow = function(i) {
			$(fc + " .num a").eq(i).addClass("cur").siblings("a").removeClass("cur");
			$(fc + " .con div").eq(i).addClass("block").siblings("div").removeClass("block");
			$(fc +" ul li").eq(i).stop(true, true).fadeIn(600).siblings("li").fadeOut(600);
		}
		$(fc).hover(function () {
			if (myTime) {
				clearInterval(myTime);
			}
		}, function () {
			myTime = setInterval(function () {
				myShow(sw)
				sw++;
				if (sw == numlag) { sw = 0; }
			}, tm);
		});
		var myTime = setInterval(function () {
			myShow(sw)
			sw++;
			if (sw == numlag) { sw = 0; }
		}, tm);
	}
	$(function () {focus3('#focus3-1', 5000)});
