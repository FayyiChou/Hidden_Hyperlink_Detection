<script language="javascript" type="text/javascript">
var FocusImg = new FocusPic("FocusImg");
FocusImg.begin();
$(function() { 
	bindNavBarHandler();bindFocusNav();
	function bindNavBarHandler() {
		var $navBarItem = $('.mainnav_item');
		$navBarItem.bind('mouseenter', function() {
			var $that = $(this);
			if ($that.hasClass('mainnav_itemshow')) {
				return;
			}
			if ($(this).children('P').length==0) {
				return;
			}
			$(this).addClass('mainnav_itemshow').siblings().removeClass('mainnav_itemshow');
		});
	}
	function bindFocusNav() {
		var $navlength = $('.focus_nav').children('a').length;
		$(".focus_nav a").click(function(){
			if($(this).hasClass('current' + $(this).index())) return;
			$(this).addClass('current' + $(this).index());
			if($(this).index()==0) FocusImg.dplayBegin();
			if($(this).index()==2) $('.focus').children('div:eq('+ $(this).index() +')').css("visibility","visible");
			else $('.focus').children('div:eq('+ $(this).index() +')').show();
			if($(this).index()!=2) {
				$('#body'+ ($(this).index()+1) +'bt').show();//alert(0);
				$('.body1 .zuo').children('.body'+ ($(this).index()+1) +'bb').show();
			}
			if($(this).index()==2 && vstatus == 0) video_player("start");
			for(i=0;i<$navlength;i++) {
				if(i!=$(this).index()) {
					$('.focus_nav a:eq('+ i +')').removeClass('current' + i);
					if($(this).index()!=0) FocusImg.dplayEnd();
					if(i==2) $('.focus').children('div:eq('+ i +')').css("visibility","hidden");
					else $('.focus').children('div:eq('+ i +')').hide();
					if($(this).index()!=2) {
						$('#body'+ (i+1) +'bt').hide();
						$('.body1 .zuo').children('.body'+ (i+1) +'bb').hide();
						if(vstatus == 1) video_player("stop");
					}
				}
			}
		});
	}
	
});
var videoarr = new Array();var vstatus = 0;var bgbox;
function video_player(init){
	if(init == "start") {
		if($(".focusv #a1").length > 0) {
			CKobject.getObjectById('ckplayer_a1').videoPlay();vstatus = 1;//alert("play");
		} else {
			$(".focusv").html("<div class=\"loading\">努力加载中...</div>");
			$.post("/plus/ajax_video.php",{act:0},function(result){
			if(result){
				$(".focusv").html(result);
				$(".focusv .switch").attr("title", "展开列表");
				$(".focusv .switch").click(function(){
					if($(".focusv .fr").hasClass("on")) {
						$(".focusv .fr").removeClass("on");
						$(".focusv .switch").attr("title", "展开列表");
					} else {
						$(".focusv .fr").addClass("on");
						$(".focusv .switch").attr("title", "收起列表");
					}
				});
				var nowD=0;
				var vlength = $(".focusv .scroll_wrap a").length;
				for(i=0;i<vlength;i++) {
					videoarr.push($(".focusv .scroll_wrap #v_"+i).attr("date"));
				}
				playvideo(0);
			}
			},"html");
		}
	}
	else if(init == "stop" && $(".focusv #a1").length > 0) {
			CKobject.getObjectById('ckplayer_a1').videoPause();vstatus = 0;//alert("stop");
	}
}

function playvideo(n){
	nowD=n;
	var flashvars={f:videoarr[n],c:0,p:1,my_url:encodeURIComponent(window.location.href)};
	var params={bgcolor:"#FFF",allowFullScreen:true,allowScriptAccess:"always",wmode:"transparent"};
	CKobject.embed('http://www.newglady.com/ckplayer/ckplayer.swf','a1','ckplayer_a1','538','400',false,flashvars,params);
	for(i=0;i<videoarr.length;i++){
		if(i!=nowD){
			CKobject._K_('v_'+i).style.backgroundColor='';
		}
		else{
			CKobject._K_('v_'+i).style.backgroundColor='#222222';
		}
	}
	bgbox = new LightBox();vstatus = 1;
}
//function closelights(){bgbox.Show();}
//function openlights(){bgbox.Close();}

function getajaxrl(str){
	var y;var m;
	if(str.length>2) {
		y = str;
		m = $("[name='month']").val();
	} else {
		m = str; 
		y = $("[name='year']").val();
	}
	
	$.post("/plus/ajax_rl.php",{month:m ,year:y},function(result){
		if(result){
			$("#ajax_rl").html(result);
		}else{
			$("#ajax_rl").text("加载失败！");
		}
	},"html")
}
getajaxrl('02');

var Mouse = function(e){
	mouse = new MouseEvent(e);
	leftpos = mouse.x + 10;
	toppos = mouse.y + 0;
}
var MouseEvent = function(e) {
	this.x = e.pageX
	this.y = e.pageY
}

var __dropDownList="drop1|drop2|drop3"; 
__initDropDownList(__dropDownList);
addBodyClick(bodyClickHiddenUl); 

$(document).ready(function(){
	var hd_win = $("#hd_win");
	$("#hdrl td").live("mouseenter",function(e){
		Mouse(e);  
		var temp_date = $(this).attr("date");
		if(temp_date){
			var temp_html = null;
			temp_html = "<div class=\"hd_all <#hd_link#>\">";
			temp_html += "<div class=\"pp\">";
			temp_html += "<a href=\"<#url#>\" ><img src=\"<#pp#>\" title=\"<#ftitle#>\" alt=\"<#ftitle#>\" width=\"148\" height=\"98\"/></a>";
			temp_html += "</div>";
			temp_html += "<div class=\"txt\">";
			temp_html += "<span><a href=\"<#url#>\" class=\"title\" title=\"<#ftitle#>\"><#title#></a></span>";
			temp_html += "<p class=\"date\"><span>活动时间：</span><#bdate#> 至 <#edate#></p>";
			temp_html += "<p class=\"hdjj\"><span>活动简介：</span><#body#></p>";
			temp_html += "</div>";
			temp_html += "</div>";
			
			temp_date = temp_date.split("|");
			var new_html = null;
			for (box=0;box<temp_date.length;box++){
			temp_date[box] = temp_date[box].split(",");
			if(new_html==null){
			new_html = temp_html;
			new_html = new_html.replace("<#id#>",temp_date[box][0]);
			new_html = new_html.replace("<#title#>",temp_date[box][1]);
			new_html = new_html.replace(new RegExp("<#url#>","gm"),temp_date[box][2]);
			new_html = new_html.replace("<#bdate#>",temp_date[box][3]);
			new_html = new_html.replace("<#edate#>",temp_date[box][4]);
			new_html = new_html.replace("<#pp#>",temp_date[box][5]);
			new_html = new_html.replace("<#body#>",temp_date[box][6]);
			new_html = new_html.replace(new RegExp("<#ftitle#>","gm"),temp_date[box][7]);
			}else{
			new_html += temp_html;
			new_html = new_html.replace("<#hd_link#>","hd_link");
			new_html = new_html.replace("<#id#>",temp_date[box][0]);
			new_html = new_html.replace("<#title#>",temp_date[box][1]);
			new_html = new_html.replace(new RegExp("<#url#>","gm"),temp_date[box][2]);
			new_html = new_html.replace("<#bdate#>",temp_date[box][3]);
			new_html = new_html.replace("<#edate#>",temp_date[box][4]);
			new_html = new_html.replace("<#pp#>",temp_date[box][5]);
			new_html = new_html.replace("<#body#>",temp_date[box][6]);
			new_html = new_html.replace(new RegExp("<#ftitle#>","gm"),temp_date[box][7]);
			}
			}
			$(".hd_doc").html(new_html);
			var offsettd = $(this).offset();
			var offstr = $(document).width()-offsettd.left;
			if(offstr>=500){
			hd_win.css({top:(offsettd.top+50)+"px",left:offsettd.left+"px"});
			}else{
			hd_win.css({top:(offsettd.top+50)+"px",left:(offsettd.left-420)+"px"});
			}
			hd_win.show();
		}else{
			hd_win.hide();
		}
	})
	$(".hd_win .hd_x").click(function(){$("#hd_win").hide();});
	
	$(".email_order").click(function() {
		$("#dingyue").css("display","");
		$("#weixin").css("display","none");
		$("#msg_error").text("");
		$("#dopost").val("save");
		$("#msg_title").text("报刊订阅");
		$("#pwdmore").css("display","");
		msgwin();
		$("#vdimgck").attr("src", function () {this.src=this.src+'?'});
	});
	$(".email_quit").click(function() {
		$("#dingyue").css("display","");
		$("#weixin").css("display","none");
		$("#msg_error").text("");
		$("#dopost").val("quit");
		$("#msg_title").text("报刊退订");
		$("#pwdmore").css("display","none");
		msgwin();
		$("#vdimgck").attr("src", function () {this.src=this.src+'?'});
	});
	$("#goweibo").click(function() {
		$("#weixin").css("display","");
		$("#dingyue").css("display","none");
		msgwin();
	});
	$(".msg_win .win_x").click(function(){$("#msg_win").hide();});
	$("#button1").click(function() {
		if($("#email").val()=="")
		{
			$("#msg_error").css("color","red");
			$("#msg_error").text("邮箱不能为空！");
			$("#email").focus();
			return false;
		}
		if($("#vcode").val().length!=4  || $("#vcode").val()=="")
		{
			$("#msg_error").css("color","red");
			$("#msg_error").text("验证码格式错误！");
			$("#vcode").focus();
			return false;
		}
		if(!/^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/.test($("#email").val())){
			$("#msg_error").css("color","red");
			$("#msg_error").text("请输入正确的电子邮箱地址！");
			$("#email").focus();
			return false;
		} 
		$.post("/plus/email_order_ajax.php",{email:$("#email").val(),pwd:$("#pwd").val(),pwd2:$("#pwd2").val(),vcode:$("#vcode").val(),dopost:$("#dopost").val()},function(result){
		if(result){
			if(result=='订阅成功！' || result=='退订成功！'){
				$("#msg_error").css("color","green");
				$("#msg_error").text(result);
				$("#vdimgck").attr("src", function () {this.src=this.src+'?'});
				setTimeout("$('#msg_win').fadeOut()",2800);
				return false;
			}else{
				$("#msg_error").css("color","red");
				$("#msg_error").text(result);
				return false;
			}
			$("#rl_b").html(result);
		}else{
			$("#msg_error").css("color","red");
			$("#msg_error").text("提交失败，请检查网络！");
			return false;
		}
		},"html")
	});
	$(".nb_win .nb_x").click(function(){$("#nb_win").hide();});
	function msgwin(){
		var clientWidth = ($(window).width()/2)-($("#msg_win").width()/2);
		var clientHeight = ($(window).height()/2)-($("#msg_win").height()/2)+ $(document).scrollTop();
		$("#msg_win").css({top:clientHeight+"px",left:clientWidth+"px"});
		$("#msg_win").show();
	}
	
});
</script>
