<script type="text/javascript">
$(document).ready(function() {
	var $tab = $('.new-box-title');
	if($tab.length == 1) {
		window.st = $tab.position().top;
	} else window.st = -1;
	var timer = null;
	$(window).scroll(function () {
		if(-1 == window.st)
			return;
		if(null != timer)
			clearTimeout(timer);
		timer = setTimeout(function() {
			var $tab = $('.new-box-title');
			if($tab.length == 1) {
				var t = window.st;
				var tt = $(window).scrollTop();
				if(tt > t) {
					if ($.browser.msie && ($.browser.version == "6.0") && !$.support.style) {
						$tab.css({ 'position': 'absolute', 'top': tt + 'px' });
					}
					else {
						$tab.css({ 'position': 'fixed', 'top': 0 });
					}
				} else {
					$tab.css({ 'position': 'relative', 'top': 0 });
				}
			}
			clearTimeout(timer);
			timer = null;
		}, 10);
	});
	

})
.on("click", "a.j_huodongtixing", function() {
	if( (new RegExp("damai\.cn_user=[^;]+")).test(document.cookie) ) {
		var submenu = $("#submenu1");
		if(submenu.length == 0) {
			submenu = $('<p class="search_good_arrow hidden" id="submenu1">我们会将最新活动第一时间通知您哦~</p>');
			$(this).parent().after(submenu);
		}
		submenu.show("fast");
		var t = setTimeout(function() {
			submenu.hide("fast");
		}, 2000);
		return false;
	}
});
</script>


<!--展开收缩JS-->
<script type="text/javascript">
	$(document).on("click", ".new-list dd a.j_trigger", function() {
		var me = $(this);
		if(me.attr("data-type") == "1") {
			me.attr("data-type", "2").html("- 收起");
			setListState("2");
		} else {
			me.attr("data-type", "1").html("+ 更多");
			setListState("1");
		}
		return false;
	});
	function setListState(t) {
		if(t == "2") {
			$("#test1111").animate({"height": $("#filterCityList").height()}, "fast")
		} else {
			$("#test1111").animate({"height": 30}, "fast")
		}
	}
	function setToggleTrigger() {
		var h = $("#filterCityList").height();
		if(h > 40) {
			var trigger = $("#test1111 a.j_trigger");
			$("#test1111 .j_trigger").show();
			setListState(trigger.attr("data-type"));
		} else {
			$(".new-list dd a.j_trigger").hide();
			setListState("1");
		}
	}
	
	$(function() {
		setToggleTrigger();
	});
</script>

<script type="text/javascript">
	function tab(obj) {
	obj.find('.tab a').click(function () {
	obj.find('.con').hide().end().find('.con').eq(obj.find('.tab > a').index(this)).show().end().end().find('a').removeClass('active').end().find(this).addClass('active');
	});
	}
	tab($('.search_tab1'));
	tab($('.search_tab2'));
	$('#search_vedio1').maiPlayer({speedSwitch: 5000, imageTitle: true, autoPlay: true, numPage: false});
	$('#search_vedio2').maiPlayer({speedSwitch: 5000, imageTitle: true, autoPlay: true, numPage: false});
</script>
<script type="text/javascript">
    var ucode = 0;
    (function () {
        dplus.define('page', function (page) {
            page.init("7415364c9dab5n09ff68", {
                'page_duration': true, 
                'clean_url': true 
            });
        });
        dplus.define('page', function (page) {
            page.setType('category');
            page.setTitle(document.title);
            page.view({ '$source': 'ssite' });
        });
    })();
    $(document).ready(function () {
        var ret = /damai.cn_user=[^;]*/gi.test(document.cookie);
        if (ret) {
            jQuery.ajax({
                type: "get",
                async: true,
                url: "//piao.damai.cn/ajax/GetUserInfo.html",
                data: { t: Math.random() },
                dataType: "jsonp",
                jsonp: "jsonCallback", 
                jsonpCallback: "success_jsonpCallback", 
                success: function (json) {
                    if (json.Data) {
                        ucode = json.Data.userInfo.code;
                    }
                },
                error: function (info) {
                    //alert("System Error:\r\n" + info);
                }
            });
        }
    }).on("click", "#pageplist >li a", function () {
        var thishref = $(this).attr("href");
        if (thishref.indexOf("item") > -1) {
            //var cindex = $("#c1 ul li a").index(this);
            var ctdx = $(this).attr("cindex");
            var ctna = $(".new-hot-list ul li.on").html();
            var pitemli = $("#pageplist >li").eq(ctdx - 1);
            var pname = pitemli.attr("pname");
            var ms = pitemli.attr("pid");
            var pagerindex = $("#pager").attr("data-page");
            if(pagerindex==undefined){
                pagerindex=1;
            }
            dplus.track('searchclick', {
                '$tti': 's_category',
                'userid': ucode,
                '$tul': window.location.href,
                '$tna': ctna,
                '$tdx': ctdx,
                '$tdp': pagerindex,
                '$pid': ms,
                '$na': pname
            });
        }
       
    }).on("mouseover", 'div[id^="item_searchText_"]', function () {
        divclickindex = parseInt($(this).attr("index")) + 1;
        if (searchtxt_bak == '') {
            searchtxt_bak = $("#searchText").val();
        }
    });
</script>


