  <script type="text/javascript">
   function memberExit(){
   
		$.ajax({
				type : "post",
				url :  passportUrl+"/member/manager/memberExit.json",
				data : {uid:1},
				dataType : "json",
				xhrFields:{
					withCredentials:true
				},
				success : function(data) {
				window.location.reload();
				}
			});
			
	};
function toLogin(){
var _backHref = window.location.href;
if(_backHref.indexOf("member/manager/login.htm?backUrl=") > 0){
	window.location.reload();
}else{
	window.location.href= passportUrl+"/member/manager/login.htm?backUrl="+window.location.href;
}

}
	var apiPath = "http://file.aopayun.com/aopaFile/";
	
	$(function(){
		loadLoginInfo();
		loadFavInfo();
	})
	
	function loadFavInfo(){
		$.ajax({
				type : "post",
				url :  passportUrl+"/member/manager/getMemberFavInfo.json",
				data : {favoritesType:5,toid:$("#toId").val()},
				dataType : "json",
				xhrFields:{
					withCredentials:true
					},
				success : function(data) {
					if(data.code == 0){
						if(data.isFavorites ==1){
							$("#addFavDiv").hide();
							$("#delFavDiv").show();
						}else if(data.isFavorites ==0){
							$("#addFavDiv").show();
							$("#delFavDiv").hide();
						}
					}else{
						$("#addFavDiv").hide();
						$("#delFavDiv").hide();
					}
				}
			});
	
	}
	

	function loadLoginInfo(){
		var loadHtml = '';
		var toId = $("#toId").val();
		$.ajax({
				type : "post",
				url :  passportUrl+"/member/manager/getMemberCommentAryInfo.json",
				data : {cType:5,toId:toId},
				dataType : "json",
				xhrFields:{
					withCredentials:true
					},
				success : function(data) {
					if(data.code == 0){
						loadHtml = loadHtml + '<h2>网友评论</h2>';
						loadHtml = loadHtml + '<div id="commentAryDiv"><div class="com_area"><textarea name="commentAry" id="commentAry" placeholder="输入您的评论"></textarea><p><span>最多可输入200字</span>';
						loadHtml = loadHtml + '<span></span></p><input class="login_btn" name="" value="提 交" type="button" onclick="javascript:saveCommentAry();"/></div><div id="commentaryAddDiv"/></div>';
						var commentary_no ='';
						if(undefined != data.list && data.list.length >0){
							for(var i = 0;i< data.list.length;i++){
								loadHtml = loadHtml + '<dl><dt><a href="#"><img style="width:60px;height:60px;" src="'+ apiPath +data.list[i].commintary.headimageurl+'" /></a></dt>';
								loadHtml = loadHtml + '<dd><p class="p_com">'+data.list[i].commintary.commentaryContext+'</p><div class="p_com_info">';
								loadHtml = loadHtml + '<p class="pci_pleft"> 发表于    '+data.list[i].commintary.dateStr+'</p> </div></dd></dl>';
								
							}
							
						}else{
							commentary_no = '<div id="noCommentaryDiv"> 暂无评论</div>';
						}
						loadHtml = loadHtml + '</div>' +commentary_no ;
					}else{
						loadHtml = loadHtml + '<h2>网友评论 <input style="background:#none; color:#0f97ca; border:0; line-height:30px; vertical-align:middle; font-size:14px; text-decoration:underline; cursor:pointer;" class="login_btn" name="" value="请登录后评论" type="button" onclick="javascript:toLogin();"/> </h2>';
						loadHtml = loadHtml + '<div id="commentAryDiv"><div id="commentaryAddDiv"/></div>';
						var commentary_no ='';
						if(undefined != data.list && data.list.length >0){
							for(var i = 0;i< data.list.length;i++){
								loadHtml = loadHtml + '<dl><dt><a href="#"><img style="width:60px;height:60px;" src="'+ apiPath +data.list[i].commintary.headimageurl+'" /></a></dt>';
								loadHtml = loadHtml + '<dd><p class="p_com">'+data.list[i].commintary.commentaryContext+'</p><div class="p_com_info">';
								loadHtml = loadHtml + '<p class="pci_pleft"> 发表于    '+data.list[i].commintary.dateStr+'</p> </div></dd></dl>';
							}
						}else{
							commentary_no = '<div id="noCommentaryDiv"> 暂无评论</div>';
						}
						loadHtml = loadHtml + '</div>' +commentary_no ;
					};
					
					$("#load_commentary_div").html(loadHtml);
				}
			});
	
	}
	function saveCommentAry(){
	var commentAry = $("#commentAry").val().trim();
	if("" == commentAry){
		alert("评论内容不能为空!");
		return false;
	}
			
		$.ajax({
				type : "post",
				url :  passportUrl+"/merchantInfo/manager/addCommentary.json",
				data : {commentaryType:5,commentaryContext:commentAry,toid:$("#toId").val(),toTitle:$("#toTitle").val()},
				dataType : "json",
				xhrFields:{
					withCredentials:true
					},
				success : function(data) {
					if(data.code == 0){
					var member = data.members;
					var addLoad = '<div id="commentaryAddDiv"><dl><dt><a href="#"><img style="width:60px;height:60px;" src="'+apiPath+member.headimageurl+'" /></a></dt><dd><p class="p_com">'+commentAry
					+'</p><div class="p_com_info"><p class="pci_pleft"><a href="#">'+member.username+'</a> 刚刚</p></div></dd></dl></div>';
						alert("添加成功!");
						$("#commentAry").val("");
						$("#commentaryAddDiv").first().before(addLoad);
						$("#noCommentaryDiv").hide();
					}else{
						alert("操作失败!");
					};
				}
			});
	}
	
	function addMyFav(){
		
		$.ajax({
				type : "post",
				url :  passportUrl+"/merchantInfo/manager/addFavorites.json",
				data : {favoritesType:5,toid:$("#toId").val(),type:0},
				dataType : "json",
				xhrFields:{
					withCredentials:true
					},
				success : function(data) {
					if(data.code == 0){
						alert("收藏成功!");
						$("#addFavDiv").hide();
						$("#delFavDiv").show();
					}else if(data.code == -1){
						window.location.href= passportUrl+"/member/manager/login.htm?backUrl="+window.location.href;
					}else{
						alert("操作失败!");
					}
				}
			});
			
	};
	function delMyFav(){
		$.ajax({
				type : "post",
				url :  passportUrl+"/merchantInfo/manager/addFavorites.json",
				data : {favoritesType:5,toid:$("#toId").val(),type:1},
				dataType : "json",
				xhrFields:{
					withCredentials:true
					},
				success : function(data) {
					if(data.code == 0){
						alert("取消收藏成功!");
						$("#delFavDiv").hide();
						$("#addFavDiv").show();
					}else if(data.code == -1){
						window.location.href= passportUrl+"/member/manager/login.htm?backUrl="+window.location.href;
					}else{
						alert("操作失败!");
					}
				}
			});
	};
</script>  
