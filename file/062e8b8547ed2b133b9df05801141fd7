  <script type="text/javascript">
$(function(){
	$(window).scroll(function(){
		var oDiv1 = $('#Tony_1').offset().top;
		var oDiv2 = $('#Tony_2').offset().top;
		var oDiv3 = $('#Tony_3').offset().top;
		var winTop= $(window).scrollTop();	
		var meuns = $('.navbg li a');
	
		function toStyle(index){
			meuns.eq(index).addClass('active').parent('li').siblings().find('a').removeClass('active');
		};
		
		if(winTop >= oDiv1){
			toStyle(0);
		};
		if(winTop >= oDiv2){
		   toStyle(1)
		};
		if(winTop >= oDiv3){
		   toStyle(2);
		};
	});
	
	

	$(window).scroll(function(){
		var div1 = $('#fixed_01').offset().top;
		var div2 = $('#fixed_02').offset().top;
		var div3 = $('#fixed_03').offset().top;
		
		var winTop2= $(window).scrollTop();	
		var meuns2 = $('.fixed-fl a');

		function toStyle2(index){
			meuns2.eq(index).addClass('active').siblings().removeClass('active');
		};
		
		
		
		if(winTop2 >= div1){
			toStyle2(0);
		};
		if(winTop2 >= div2){
		   toStyle2(1)
		};
		if(winTop2 >= div3){
		   toStyle2(2);
		};
	});
});
</script> 
