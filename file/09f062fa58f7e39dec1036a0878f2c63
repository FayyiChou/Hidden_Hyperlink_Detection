	var widthclient=parseInt($("body").css("width"));
	var heightclient=parseInt($("body").css("height"));
	var heightpic=heightclient-70+"px";
	var heighttel=heightclient-180+"px";
	var heighttel2=166/750*widthclient+20+"px";
	
if(widthclient>540){	
	$(".swiper-container").css({"height":heightpic});	
	}else{
	$(".swiper-container").css({"height":heighttel});	
	$(".homepage-business>div").css({"height":heighttel2});	
	}

$(document).ready(function(){
	var body_width=parseInt($("body").css("width"));
/* 	console.log(body_width); */
	if(body_width<=540){
		$("#placeholder").addClass("place_m");
	}else{
		
	}
	if(!document.referrer||document.referrer.indexOf("powerlong")<0){
		 $("#placeholder").css("display","block"); 
	}
	setTimeout(function(){
		
		$("#placeholder").fadeOut();
		$("html").css("overflow-y","auto");
		$("body").css("overflow-y","auto");
	},2000);
	
	setInterval(function (){ msg();}, 200);
	function msg(){
	     var ys = $(".glyphicon-chevron-down");
	if(!($(ys).is(":animated"))){
	$(ys).animate({"top":"-6px",},200).animate({"top":"-0px",},200).animate({"top":"-3px",},200).animate({"top":"-0px",},200).animate({"top":"-1px",},200).animate({"top":"-1px",},200).animate({"top":"-1px",},200).animate({"top":"-0px",},200);
	}
		}
})	


