$(function (){
	/* alert(document.referrer); */
	
	var widthclient= document.documentElement.clientWidth;
	
	//var heightbody=window.screen.availHeight;
	var heightbody=document.documentElement.clientHeight;


	$(".navbar-toggle").bind("click",function(e){
		if($(this).hasClass("collapsed")){
			 $("html").css({
					"overflow-y":"hidden",
					"max-hegiht":heightbody+"px"
				}); 
			 $("body").css({
				"overflow-y":"hidden",
				"max-hegiht":heightbody+"px"
			}); 
			
		} else{
			$("html").css({
				"overflow-y":"inherit",
				"max-hegiht":"inherit"
			});
			$("body").css({
					"overflow-y":"auto",
					"max-hegiht":"inherit"
				});
			
			
			}
 	});
	 if(widthclient>540){		 
	 $('.dropdown').mouseover(function() {   
	     $(this).addClass('open');  
		}).mouseout(function() {    
			$(this).removeClass('open');   
	     });
	 
	}else{
		
		$(".center-block").css({"height":(heightbody-50+"px")});
		$(".dropdown-toggle").each(function(){
			$(this).attr("onclick","");
		})
		$('.dropdown').focus(function() {   
		     $(this).addClass('open');  
			})/* .blur(function() {    
				$(this).removeClass('open');   
		     }); */
		
			 $("#zcpt a").each(function(){
			    	// $(this).attr("onclick","location.href='/major/major!pho.action'");
			    	$(this).removeAttr("onclick");
			    	$(this).attr("href",'/major/major!pho.action');
			     });
		     
	} 
});


function logout(){
	var info=$("#js_i18n_logout").val();
	if (confirm("您确定要退出吗?"))
	top.location = "/j_spring_security_logout";
	return false;
}
