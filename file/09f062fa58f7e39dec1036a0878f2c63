var w1=$("body").css("width");



if($("#home").text()=="Home"&&clientwidth>540 ){
	$(".collapse").css({"width":"868px","padding":"0"})	;
	$(".navbar-default .navbar-nav>li>a").css({"line-height":"50px"})	;
	
}	if($("#home").text()=="Home"&&clientwidth<=540 ){
	$(".nav.navbar-nav.center-block a").css({"font-size":"13px"})	;
	$(".navbar-default .navbar-collapse, .navbar-default .navbar-form").css("width","50%");
	$(".navbar-default .navbar-nav .dropdown-menu>li>a").css({"font-size":"12px"})	;
}


$(".dropdown").each(function(i){	
    $(this).click(function(){
    	if($(".dropdown").eq(i).hasClass("open")){
    		$(".navbar-right").css("display","block");
    	}else{
    		$(".navbar-right").css("display","none");
    	}
     })
}); 
$("#homeButton").click(function(){
	
	$(".navbar-right").css("display","block");
});

