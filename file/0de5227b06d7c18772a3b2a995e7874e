				<script type="text/javascript" language="javascript">
				var showNum=10;
				var dataArray=new Array();
				for(var i=0;i<showNum;i++)
				{
				if(i >= affiches.length)
				{break;}
				else
				{
				var title='';
				if(affiches[i].title.length<32)
				{
				title=affiches[i].title;
				}
				else
				{
				title=affiches[i].title.substring(0,31)+'...';
				}
				dataArray.push('<dl class="xw_box1"><dt class="xw_text"><a href="http://www.cninfo.com.cn'+affiches[i].url+'" target="_blank">·'+title);
				if(affiches[i].format.toUpperCase()=='PPT' || affiches[i].format.toUpperCase()=='PPTX')
				dataArray.push('<span><img src="../images/ppt.jpg"   /></span>');
				else if(affiches[i].format.toUpperCase()=='XLSX' || affiches[i].format.toUpperCase()=='XLS')
				dataArray.push('<span><img src="../images/excel.jpg"   /></span>');
				else if(affiches[i].format.toUpperCase()=='GIF')
				dataArray.push('<span><img src="../images/gif.jpg"   /></span>');
				 else if(affiches[i].format.toUpperCase()=='JPG')
				dataArray.push('<span><img src="../images/jpg.jpg"   /></span>');
					  else if(affiches[i].format.toUpperCase()=='PDF')
				dataArray.push('<span><img src="../images/pdf.jpg"   /></span>');
					   else if(affiches[i].format.toUpperCase()=='PNG')
				dataArray.push('<span><img src="../images/png.jpg"   /></span>');
						else if(affiches[i].format.toUpperCase()=='DOC' || affiches[i].format.toUpperCase()=='DOCX')
				dataArray.push('<span><img src="../images/word.jpg"   /></span>');
				 else
				dataArray.push('<span><img src="../images/otherfile.jpg"   /></span>');
				dataArray.push('</a></dt>');
				dataArray.push('<dt class="sj_time">'+affiches[i].releaseDate+'</dt><dt class="clears"></dt></dl>');
				}
				}
				$("#disclosureId").html(dataArray.join(''));
				</script>

