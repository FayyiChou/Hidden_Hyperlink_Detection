		<script type="text/javascript">
			function checkIfEnterKey(event,varAction,adjForm){
				var keycode = (event.keyCode ? event.keyCode : event.which);
				if(keycode == '13'){	//Process after checking the "enter button" clicked
					proceed(varAction,adjForm);
				}
			}
			function proceed(varAction,adjForm){
				if(varAction=="add"){
					$.ajax({
						type: "POST",
						url: "add_cart.asp?customer_id=2339&lang=1&content_set=color_3",
						data: {
							varAction : varAction,
							styles    : adjForm.styles.value,
							Quatity   : adjForm.Quatity.value,
							shoppingid: adjForm.shoppingid.value
						},
						error: function (jqXHR, exception, error) {
							alert (jqXHR.responseText);
						},
						success: function(msg){
							$("div[id=Cart]").html(msg);
							//show fixed cart image
							if($('#shoppingCart').length < 1){
								$('body').append('<a href="shopping_cart.asp?lang=1&customer_id=2339&content_set=color_3&name_id=" style="cursor:pointer"><div id="shoppingCart"></div></a>');
							}
						},
						beforeSend:function(){
							$('body').append('<div id="resultAlarm" style="display:none"><div><div>已加入購物車 !!</div></div><div class="bg"></div></div>');
							$('body').append('<div id="resultLoading" style="display:none"><div><img src="images/ajax-loader.gif"><div>數據加載中...請稍候... !!</div></div><div class="bg"></div></div>');
							$('#resultLoading, #resultAlarm').css({
								'width':'100%',
								'height':'100%',
								'position':'fixed',
								'z-index':'10000000',
								'top':'0',
								'left':'0',
								'right':'0',
								'bottom':'0',
								'margin':'auto'
							});	
							
							$('#resultLoading .bg, #resultAlarm .bg').css({
								'background':'#000000',
								'opacity':'0.7',
								'width':'100%',
								'height':'100%',
								'position':'absolute',
								'top':'0'
							});
							
							$('#resultLoading>div:first, #resultAlarm>div:first').css({
								'width': '250px',
								'height':'75px',
								'text-align': 'center',
								'position': 'fixed',
								'top':'0',
								'left':'0',
								'right':'0',
								'bottom':'0',
								'margin':'auto',
								'font-size':'16px',
								'z-index':'10',
								'color':'#ffffff'
								
							});
							$('#resultLoading .bg').height('100%');
							$('#resultLoading').fadeIn(300);
							$('body').css('cursor', 'wait');
						},
						complete:function(){
							$('#resultLoading .bg').height('100%');
							$('#resultLoading').fadeOut(300);
							$('body').css('cursor', 'default');
							$('#resultAlarm').fadeIn(300).delay(1000).fadeOut();
						}
					});
				}else if(varAction=="update" || varAction=="clean"){
					$.ajax({
						type: "POST",
						url: "add_cart.asp?customer_id=2339&lang=1&content_set=color_3",
						data: "varAction="+varAction+"&"+$(adjForm).serialize(),
						error: function (jqXHR, exception, error) {
							alert (jqXHR.responseText);
						},
						success: function(msg){
							location.reload();
						},
						beforeSend:function(){
							$('body').append('<div id="resultLoading" style="display:none"><div><img src="images/ajax-loader.gif"><div>更新數據，請稍候... !!</div></div><div class="bg"></div></div>');
							$('#resultLoading').css({
								'width':'100%',
								'height':'100%',
								'position':'fixed',
								'z-index':'10000000',
								'top':'0',
								'left':'0',
								'right':'0',
								'bottom':'0',
								'margin':'auto'
							});	
							
							$('#resultLoading .bg').css({
								'background':'#000000',
								'opacity':'0.7',
								'width':'100%',
								'height':'100%',
								'position':'absolute',
								'top':'0'
							});
							
							$('#resultLoading>div:first').css({
								'width': '250px',
								'height':'75px',
								'text-align': 'center',
								'position': 'fixed',
								'top':'0',
								'left':'0',
								'right':'0',
								'bottom':'0',
								'margin':'auto',
								'font-size':'16px',
								'z-index':'10',
								'color':'#ffffff'
								
							});
							$(":button[name=Submit5]").attr("disabled", "true");
							$('#resultLoading .bg').height('100%');
							$('#resultLoading').fadeIn(300);
							$('body').css('cursor', 'wait');
						}
					});
				}else{	//go_order.asp
					window.location.href="go_order.asp?"+$(adjForm).serialize();
				}
			}
			function minus(id,sendForm){
				var value = parseInt($('#'+ id ).val());
				if (value > 1){
					$('#'+id).val(value - 1);
					proceed('update',$('#'+sendForm));
				}
			}
			
			function plus(id,sendForm){
				var value = parseInt($('#'+ id ).val());
				if (value < 9999){
					$('#'+id).val(value + 1);
					proceed('update',$('#'+sendForm));
				}
			}
			
		</script>
	
