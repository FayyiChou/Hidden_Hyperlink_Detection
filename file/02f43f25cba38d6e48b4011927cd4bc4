<script type="text/javascript">
//控制Tabs的CSS
function changeTabsCss(url)
{
	var href = url.replace('http://','').split('/');
	var tmp = href[1];
	if(tmp.indexOf('album_list') != -1 || tmp.indexOf('photo_list') != -1 || tmp.indexOf('photo_list_newest') != -1 || tmp.indexOf('photo_list_hot') != -1 || tmp.indexOf('photo_upload') != -1)
	{
		$("#link_album").attr("class","on");
	}
	else if(tmp.indexOf('article') != -1  )
	{
		$("#link_article").attr("class","on");
	}
	else if(tmp.indexOf('video') != -1)
	{
		$("#link_video").attr("class","on");
	}	
	else if(tmp.indexOf('award') != -1 || tmp.indexOf('honour_zhj') != -1 || tmp.indexOf('ware') != -1)
	{
		$("#link_honour").attr("class","on");		
	}
	else if(tmp.indexOf('active') != -1 || tmp.indexOf('msg') != -1 || tmp.indexOf('leavemsg') != -1)
	{
		$("#link_trends").attr("class","on");
	}
	else
	{
		$("#link_index").attr("class","on");
	}
	
}
//鼠标滚动
function onScroll(event) {
	if($(document).scrollTop() + $(window).height() > $(window).height())
	{
		$('#toTop').show();
		$('#toTop').css("bottom",$(document).height() - ($(window).height() + $(document).scrollTop()) +'px');
	}
	else
	{
		$('#toTop').hide();
	}
};
$(document).ready(function(){
    changeTabsCss(window.location.href);
    var win = ($.browser.msie) ? window : document;
    $(win).bind('scroll', onScroll);
});
</script>
