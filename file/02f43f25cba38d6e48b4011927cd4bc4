<script type="text/javascript">
function honourcontrolLeftMenu()
{
	var href = window.location.href.replace('http://','').split('/');
	var tmp = href[1];
	if(tmp.indexOf('award') != -1)
	{
		$('#award_list').attr('class','on');
	}
	else if(tmp.indexOf('honour_zhj') != -1)
	{
		$('#gold_list').attr('class','on');
	}
	else if(tmp.indexOf('ware') != -1)
	{
		$('#ware_list').attr('class','on');
	}
}
honourcontrolLeftMenu();
</script>
