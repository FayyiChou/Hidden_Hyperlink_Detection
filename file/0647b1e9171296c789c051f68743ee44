    <script type="text/javascript" src="http://cache.66law.cn/js/header_npc.js"></script>
    <script type="text/javascript">
        $(function () {
            String.prototype.startWith = function (str) {
                if (str == null || str == "" || this.length == 0 || str.length > this.length)
                    return false;
                return (this.substr(0, str.length) == str);
            }
            // 小搜索框
            $(".sh-box-sl .sh-text").focus(function () {
                $(".sh-box-sl").addClass("sh-box-sl-fcous");
            });
            $(".sh-box-sl .sh-text").blur(function () {
                $(".sh-box-sl").removeClass("sh-box-sl-fcous");
            });

            var $cityHolder = $('div.city-tk'),
                timer = 0;
            // 地区弹框
            $(".city-qh").click(function () {
                $cityHolder.removeClass("none");
            });
            $cityHolder.hover(function () {
                timer && clearTimeout(timer);
            }, function () {
                timer = setTimeout(function () {
                    $cityHolder.addClass("none");
                }, 500);
            });

            var $fmTips = $('#ctnFmTips');
            var $btnFmTips = $('#txtFmTips').on('keyup', function () {
                $fmTips.trigger('loadlist');
            });
            $('#btnFmTips').on('click', function () {
                $fmTips.trigger('loadlist');
            });
            $fmTips.on('loadlist', function () {
                var txt = $.trim($btnFmTips.val());
                if (txt) {
                    var $mi = $('<div></div>');
                    $fmTips.empty();
                    var c = 0;
                    $.each(dist, function (i, v) {
                        if (c > 9) {
                            return;
                        }
                        if (v['n'].startWith(txt) || v['p'].startWith(txt) || (v['c'] && v['c'].startWith(txt))) {
                            c++;
                            $('<p></p>').text(v['n']).data('id', i).appendTo($mi);
                        }
                    });
                    $fmTips.append($mi.children());
                    $fmTips.removeClass('none');
                } else {
                    $fmTips.empty().append('<p>请输入城市名称或区号</p>');
                }
            }).on('click', 'p', function () {
                $fmTips.addClass('none');
                var $this = $(this);
                var p = dist[$this.data('id') - 0];
                if (!p) return;
                $fmTips.trigger('jump', 'http://www.66law.cn/' + p['p'] + '/');
            }).on('jump', function (e, url) {
                var $form = $('<form action="' + url + '" method="post" target="_blank"></form>').appendTo(document.body);
                $form.submit().remove();
            });

            $('#btnFindLawyer').on('click', function () {
                var pid = $('#headerPro').val() - 0;
                var cid = $('#headerCity').val() - 0;
                var name = dist[cid > 0 ? cid : pid];
                name = name ? name['p'] : '';
                if (name) {
                    $fmTips.trigger('jump', 'http://www.66law.cn/' + name + '/');
                }
            });
            var $txtSearch = $('input.sh-text').on('keydown', function (e) {
                e = e || window.event;
                if ((e.keyCode || e.which) == 13) {
                    $btnSearch.trigger('submit');
                }
            });

            var $btnSearch = $('a.sh-btn').on('click submit', function () {
                var val = $.trim($txtSearch.val());
                if (val) {
                    $fmTips.trigger('jump', 'http://s.66law.cn/cse/search?q=' + val + '&s=18259188589626635806&nsid=1');
                }
            });


        });
    </script>



