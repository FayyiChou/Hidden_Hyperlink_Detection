<script language="javascript">
 $.ajax({
    url:'/ln/iframe/mall/prices/',
    type: "GET",
    dataType: 'json',
    timeout: 5000,
    success:function(json){
      if(json.code != '0') {
      alert(json.errorDescription);
      return;
      }
      $.each(json.dataObject,function(i,n){
        if(n.className) {
			$("."+n.className).empty();
			if( n.className == 'sj_cxq1' || n.className == 'sj_cxq2' || n.className == 'sj_cxq3' || n.className == 'sj_cxq4')
			{
				$("."+n.className).append('￥'+n.price);
			}else{
				$("."+n.className).append('<em class="ym">￥</em>'+n.price);
			}
        } 
        });
      },
    error:function(xhr){
       //alert("请求出错(请检查相关度网络状况.)");
       }
    });
