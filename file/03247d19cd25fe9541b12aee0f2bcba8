function AddFavorite(sURL, sTitle) {
	try {
		window.external.addFavorite(sURL, sTitle);
	} catch (e) {
		try {
			window.sidebar.addPanel(sTitle, sURL, "");
		} catch (e) {
			alert("您的浏览器不支持直接将页面添加为书签，请使用Ctrl + D手动操作。");
		}
	}
} 

</script>
<script>
jQuery(function(){
        $("#thisAdd li:first").addClass("thistitle");

	var $curChannel = '手机';
	var $list = $(".meun_down");
	//console.log($curChannel);
	if ($curChannel != "首页"){
		$list.css("display","none");
		$(".toptiocsa img").hover(function(){
			$(this).css({
				height:"45px",
				width:"190px",
				display:"block",
				cursor:"pointer"
			});
			$list.show();
		},function(){
			$list.hide();
		});
		$(".meun_down ul li div").css({visibility:"hidden"});
		$(".meun_down").hover(function(){
			$(".meun_down ul li div").removeAttr("style");
			$(this).show();
		},function(){
			$(this).hide();
			$(".meun_down ul li div").css({visibility:"hidden"});
		})
	}else{
		$list.css("display","block");
	}
	var cid = '107815';
		$("#h"+cid).addClass("headmain_bg");
		$(".toptiocs li").hover(function(){
			$(this).addClass("headmain_bg").siblings().removeClass("headmain_bg");
		},function(){
			$("#h"+cid).addClass("headmain_bg").siblings().removeClass("headmain_bg");
		})
})
</script>

