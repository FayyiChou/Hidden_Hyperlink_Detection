    <script type="text/javascript">
        $(window).load(function () {
            var Box = $("#News");
            var Shade = $('#Shade');

            //抓DIV載入內容後的寬高
            var BoxWidth = Box.outerWidth();
            var BoxHeight = Box.outerHeight();

            //動態置中Margin 負值Top & Left
            var MarginLeft = BoxWidth / 2;
            var MarginTop = BoxHeight / 2;

            $("#Hid_Box").val(MarginLeft + "," + MarginTop);

            //點Close鈕即淡出遮罩，並移除遮罩及刪除按鈕。
            $(document).on('click', 'a.close, #Shade', function (event) {

                Box.stop().animate({
                    'margin-top': -MarginTop - 50,
                    'opacity': 0
                }, 900, 'easeOutExpo').fadeOut(100);

                $(Shade, 'a.close').stop().fadeOut(1000);

            }); //end on

            $("#Open_Ad").click(function () {
                PopUpNews();
            }); //end click

        });   //end load
    </script>
    <script type="text/javascript">
        //messagebox 開啟對話框
        function PopUpNews() {

            var Box = $("#News");
            var Shade = $('#Shade');
            var Hid_Box_Val = ($("#Hid_Box").val()).split(",");

            //動態置中Margin 負值Top & Left
            var MarginLeft = Hid_Box_Val[0];
            var MarginTop = Hid_Box_Val[1];

            //加入遮罩
            Shade.stop().css('opacity', 0.7).fadeIn(1000);

            Box.css({
                'margin-left': -MarginLeft,
                'margin-top': -MarginTop - 50,
                'display': 'block',
                'opacity': 0
            });

            Box.stop().animate({
                'margin-left': -MarginLeft,
                'margin-top': -MarginTop,
                'opacity': 1
            }, 1000, 'easeOutExpo');
        }; //end fun
    </script>
    <script type="text/javascript">
        //messagebox 開啟對話框
        function PopUpNewsFirst() {
            $(window).load(function () {

                var Box = $("#News");
                var Shade = $('#Shade');

                //抓DIV載入內容後的寬高
                var BoxWidth = Box.outerWidth();
                var BoxHeight = Box.outerHeight();

                //動態置中Margin 負值Top & Left
                var MarginLeft = BoxWidth / 2;
                var MarginTop = BoxHeight / 2;

                //加入遮罩
                Shade.stop().css('opacity', 0.7).fadeIn(1000);

                Box.css({
                    'margin-left': -MarginLeft,
                    'margin-top': -MarginTop - 50,
                    'display': 'block',
                    'opacity': 0
                });

                Box.stop().animate({
                    'margin-left': -MarginLeft,
                    'margin-top': -MarginTop,
                    'opacity': 1
                }, 1000, 'easeOutExpo');
            }); //end window load
        }; //end fun
    </script>
