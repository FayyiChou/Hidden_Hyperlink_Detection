           <input type="text" name="validate" size="4" class="ipt-txt" style="text-transform:uppercase;" />
           <img src="http://www.ipaoche.com/include/vdimgck.php" id="validateimg" style="cursor:pointer" onclick="this.src=this.src+'?'" width="60" height="24" title="点击我更换图片" alt="点击我更换图片" /> 
