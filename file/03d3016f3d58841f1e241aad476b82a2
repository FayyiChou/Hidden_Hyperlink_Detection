		<script type="text/javascript">
			$j(function() {
				var en_US = "zh_HK";
				$j(window).resize(function() {
					var linksContainerW = $j('.nav-component-links').width() - 110;
					if (linksContainerW / 3 <= 75 && en_US == 'en_US') {
						$j('a', '.nav-component-links').css({
							width : 'auto',
							padding : '5px 8px'
						});
					} else {
						$j('a', '.nav-component-links').width(linksContainerW / 3);
					}
				}).resize();
			});

			$j(document).ready(function() {
				$j('.hk-flexslider').flexslider({
					animation : "slide",
					slideshow : true,
					animationLoop : true,
					directionNav : false,
					slideshowSpeed : 3000
				});

				shoesImgList();
				function shoesImgList() {

					var i = 0;
					$j(".shoesImgList").each(function() {
						var d_index = $j(this).attr("img_index");
						var sClass = ".swiper-container" + d_index;
						var pClass = ".pagination" + d_index;
						var mySwiper = new Swiper(sClass, {
							pagination : pClass,
							slidesPerView : 2
						});
						i++;
					});

					$j(".shoesImgList2").each(function() {
						var d_index = $j(this).attr("img_index2");
						var sClass = ".swiper-container-landscape" + d_index;
						var pClass = ".pagination-landscape" + d_index;
						var mySwiper = new Swiper(sClass, {
							pagination : pClass,
							slidesPerView : 3
						});
						i++;
					});
				}
			});
		</script>

