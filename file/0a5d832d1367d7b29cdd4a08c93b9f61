function livechangemoretag(imgobj,id)
{  

  //按钮图片切换
	    	  
	  var imgurl=document.getElementById(imgobj).src;
	    
	    if(id == 1)
  	     	imgurl= imgurl.replace("in01_01_02","in01_01_02b");  
        else	
	  	    imgurl= imgurl.replace("in01_01_02b","in01_01_02");	  
	   	  
	  document.getElementById(imgobj).src= imgurl;
  
}  

</script>


