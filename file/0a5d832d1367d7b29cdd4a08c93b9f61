<script type="text/javascript">

function topchange(imgobj1,divid1,hrf)
{  
  //切换按钮背景  
  for(var i = 1; i <= 4; i++){
     var imgurl1=document.getElementById(imgobj1 + i).src;     
     if(divid1 == i){
  	   imgurl1= imgurl1.replace("nv2","nv1");
      }
     else{
  	   imgurl1= imgurl1.replace("nv1","nv2");  
      }
     document.getElementById(imgobj1+i).src= imgurl1;
     
     
   // window.location = url+hrf;
  }
  if(divid1 == 2){
     //parent.location="http://hymail.ldrkzj.com/index.jsp";
     parent.location=document.getElementById("contextPath").value+hrf;
  }
  else{
     parent.location=document.getElementById("contextPath").value+hrf;
  }
}  

</script>
