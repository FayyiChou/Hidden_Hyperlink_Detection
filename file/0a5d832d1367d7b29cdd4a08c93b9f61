<script type="text/javascript">

function bjdchangemoretag(imgobj,id)
{  

  //按钮图片切换
	    	  
	  var imgurl=document.getElementById(imgobj).src;
	    
	    if(id == 1)
  	     	imgurl= imgurl.replace("in01_03_02","in01_03_02b");  
        else	
	  	    imgurl= imgurl.replace("in01_03_02b","in01_03_02");	  
	   	  
	  document.getElementById(imgobj).src= imgurl;
  
}  

</script>


