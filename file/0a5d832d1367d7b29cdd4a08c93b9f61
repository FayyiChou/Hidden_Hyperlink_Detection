            <!-- 调用都市生活公益活动我参与 --> 
            










<script type="text/javascript">
<!-- 
/**
 * 通过javascript切换“公益活动我参与”相关标签页
 * imgobj   : td对象
 * arrowid  : td下面的箭头图标
 * divid    : 内容展示区
 * flg      : 是否切换线面内容  #切换  非# 则是URL打开对应的页面
 */
GY_TAGS_COUNT = 2
function gychangetag(imgobj,arrowid,divid,flg)
{  
  //按钮图片切换
  for (var i = 1; i <=GY_TAGS_COUNT; i++){  	  
  	  //上边图片 
  	  var tdbg = document.getElementById(imgobj + i).background;
	  //切换箭头图片背景
	  var imgurl=document.getElementById(arrowid + i).src;
	  if(divid == i){
  	  	tdbg= tdbg.replace("gray","main15");  
	  	imgurl= imgurl.replace("main02","main16");  
	  }else{
	  	tdbg= tdbg.replace("main15","gray");
	  	imgurl= imgurl.replace("main16","main02");	  
	  }  	  
	  document.getElementById(imgobj + i).background= tdbg;
	  document.getElementById(arrowid + i).src= imgurl;
  }
  //切换tag页面
  if(flg=='#'){
	  for(var i = 1; i <= GY_TAGS_COUNT; i++)  
	  {  
	  	  var tagid = "gyhd" + i;
	      var s = document.getElementById(tagid);
	      
	      //对于“我要参与”特殊处理
	      if(s==null){ continue; }
	      
	      if(divid == i)  
	      {  
	          s.style.display="";
	      }  
	      else  
	      {  
	          s.style.display='none';  
	      }  
	  } 
  }
  else{
  	  //打开对应的网页
  	  window.location=flg;
  }  
}  
//-->
</script>

