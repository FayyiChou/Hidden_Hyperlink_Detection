 <script type="text/javascript" >
var map;
var marks = new Array();
function init(){
 map  = new BMap.Map("map");
  var point = new BMap.Point(102.03, 25.08);   
   map.centerAndZoom(point, 5);
   map.addEventListener("click", function(){
 window.location.href = 'http://www.ynsglj.org.cn:9834/Lz.do?method=redToTZ&full=full';
});
   getBoundary();
}
function addMarks(){
    addOverLay(1,100.823165, 22.019453,'2013年9月1日至2015年9月1日，对S327景仑线K0+000(景洪江北环岛)至K25+600(勐罕镇入口大门)实行全封闭施工，途经车辆从小磨公路绕行。');

        addOverLay(2,102.543943, 24.353012,'2月28日：昆磨高速G8511K 95+390至K95+970（红塔区高仓附近）因新哨坡隧道下穿高速公路施工需要，经相关部门批准，玉溪市红塔区高仓以南4公里处向西临时改移至便道通行，预计2014-12-30恢复正常通行。');

        addOverLay(1,99.722871, 23.345295,'临沧支队报：沧源大队辖区路线耿沧线S314线K25-K41共计15公里进行道路改造，该路段将限时封闭施工，时间：2014年2月21日至2014年5月10日，每天以两个时间段对车辆放行（上午12：00-13：00，傍晚20：00-次日8：00）。');

        addOverLay(2,102.527701, 24.331786,'昆磨高速(G8511)昆明至景洪段方向K 90 + 000 至 K 93 + 100（高仓立交），因高仓立交改扩建工程全面开启，2014年3月13日起对K90+000米-K93+100米路段进行交通管制 。上下行线车辆并入上行线通行，请过往车辆依交通指示谨慎行驶！ 预计9月13日08时恢复通行。');

        addOverLay(2,100.235525, 25.605374,'大理支队报：辖区国道G214线(K2252至K2288)和洱炼线X089（K37+200M至K60+0M）从2014年3月至2015年6月进行公路改扩建及公路大修，公路未封闭，施工期间实行临时性交通管制。');

        addOverLay(2,102.551646, 24.433468,'昆磨高速下行线(G8511)K78+000至K87+000(玉溪北城至九龙池)于3月20日进行路面养护施工，施工期间北城至九龙池路段下行线全封闭，车辆改由上行线并道通行，预计4月20日恢复通行。');

        addOverLay(2,100.055885, 26.052663,'3月26日，G214线K2295+500M至K2302+0M（洱源县城至大理市上关镇）进行公路改扩建工程（由三级改为一级），施工工期从2014年3月至2015年3月，施工期间采取边施工边通行，未封闭道路。');

        addOverLay(2,102.577079, 24.571502,'8日12时，昆富线(S102)K 4 + 0 至 K 4 + 160 （大营村处），封闭道路施工。过往车辆从新修便道通行。预计8月08日23：59恢复通行。');

        addOverLay(1,104.674895, 27.737683,'镇风公路（S112）K0+000至K41+000  （镇雄县罗坎镇风翥），2014年6月21日起至2014年9月25日，道路封闭施工。车辆绕行火石坳至罗坎公路或镇威、凤大二级公路。  路政人员协助、配合相关部门对该路段实施交通管制，确保车辆、人员安全。');

        addOverLay(1,103.233913, 22.782969,'16:00，鸡那线(S212)鸡街至那发方向K 164 + 000 至 K 164 + 050 （金平县金河镇枯岔河村委会大瑶山村路段)发生山体滑坡，导致交通双向阻断.预计7月21日18：00恢复通行。');

        addOverLay(1,103.310268, 22.91618,'鸡那线(S212)鸡街至那发段K 123 + 590 至 K 123 + 610 （金平县大寨乡大平台村）。路基受强降雨冲刷，造成路基塌陷，导致交通单向阻断单向通行。路政部门已通知养护部门进行修复，并在阻断路段设置临时警示标志提示驾乘人员谨慎驾驶。预计7月23日23时恢复通行。');

    }
function getBoundary(){       
    var bdary = new BMap.Boundary();
    var name = "云南";
    bdary.get(name, function(rs){       //获取行政区域
        map.clearOverlays();        //清除地图覆盖物       
        var count = rs.boundaries.length; //行政区域的点有多少个
        for(var i = 0; i < count; i++){
            var ply = new BMap.Polygon(rs.boundaries[i], {strokeWeight: 2,   strokeColor: "#0033FF",fillOpacity:0.1}); //建立多边形覆盖物
            map.addOverlay(ply);  //添加覆盖物
            //map.setViewport(ply.getPath());    //调整视野         
        } 
		       
 addMarks(); 
		window.setTimeout(changeIcon,500);              
    });   
}
function changeIcon(){
	if(marks){
	for(i =0 ; i < marks.length; i++){
		if(marks[i].isVisible() &&  marks[i].getTitle()=='阻断'){marks[i].hide();}
		else {marks[i].show();}
		}
		window.setTimeout(changeIcon,500);
	}
}

function addOverLay(type,lng,lat){
var pmaker,myIcon,title;
	if(type == 1){
 myIcon = new BMap.Icon("http://www.ynlz.com.cn/lz/blue/red.png", new BMap.Size(15,15));
 title = '阻断';
 }
else if(type == 2){
		 myIcon = new BMap.Icon("http://www.ynlz.com.cn/lz/blue/yellow.png", new BMap.Size(15,15));
		 title = '阻塞';}
		else	{
		retun;
		 }
		
pmaker = new BMap.Marker(new BMap.Point(lng, lat),{icon:myIcon,title:title});  // 创建标注
  marks[marks.length] = pmaker;
map.addOverlay(pmaker);
}
</script>
