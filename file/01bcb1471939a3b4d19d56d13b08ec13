<script type="text/javascript">
    var HJ_New_BuloID = 0;
    $(function () {
        $('#topTxtKeyWord').keypress(function (e) {
            if (e.which == 13 && $.trim($('#topTxtKeyWord').val())) {
                window.location.href = 'http://st.hujiang.com/?key=' + encodeURIComponent($('#topTxtKeyWord').val());
            }
        });
        $('body').bind('click.hideIndexSearch',function(e){
            if(!$(e.target).hasClass('search_submit') && !$(e.target).hasClass('search_key_input')){
                $('#topTxtKeyWord').attr('placeholder','');
                $('#hideSearchBox').addClass('hide_search_box');
                setTimeout(function(){
                    $('#topTxtKeyWord').attr('placeholder','输入您要搜索的话题、社刊、社团');
                },500);
            }

        });

        $('#topsearch_submit').click(function () {
            if($('#topTxtKeyWord').is(':visible') && $.trim($('#topTxtKeyWord').val())) {
                window.location.href = 'http://st.hujiang.com/?key=' + encodeURIComponent($('#topTxtKeyWord').val());
            }else{
                $('#hideSearchBox').removeClass('hide_search_box');
                $('#topTxtKeyWord').focus();
            }
        });
        

        $('.search_key_input').focus(function(){
            $(this).parent().addClass('active');
        }).blur(function(){
            $(this).parent().removeClass('active');
        })
    });
   
</script>
