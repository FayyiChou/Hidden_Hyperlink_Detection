            $(function() {
                $("<div id='class_gotop' style='position:fixed;right:10px;bottom:10px;z-index:100;display:none;'><a id='class_a_gotop' title='回到顶部' style='display:block;_display:none;height:50px;width:50px;background:url(\"http://common.hjfile.cn/site/images/bg_all_wide.jpg\") repeat scroll -503px -29px transparent;' href='javascript:'></a></div>").appendTo("body");
                $(window).scroll(function() {
                    if ($(window).scrollTop() > 200) {
                        $("#class_gotop").fadeIn("slow");
                    } else {
                        $("#class_gotop").fadeOut("slow");
                    }
                });
                $("#class_a_gotop").click(function() { $("html,body").animate({ scrollTop: 0 }, 600); }).hover(function() { $(this).attr("style", "display:block;_display:none;height:50px;width:50px;background:url('http://common.hjfile.cn/site/images/bg_all_wide.jpg') repeat scroll -503px -129px transparent;"); }, function() { $(this).attr("style", "display:block;_display:none;height:50px;width:50px;background:url('http://common.hjfile.cn/site/images/bg_all_wide.jpg') repeat scroll -503px -29px transparent;"); });
                var isShowTip=HJBulo.fun.getCookie('league_tip_temp1'),
           tempTipObj=$('<div class="top_tip_box"><div class="content_wrap"><a href="javascript:;" class="close">关闭</a>【重要通知】亲爱的沪友，沪元将于2016年2月25日起正式停止发放，沪元（新）将闪亮登场。<a href=\"http://st.hujiang.com/topic/167173131437\">点击查看详情</a>！</div></div>'),
           isShowTipTimeout;
                function showTipemp(){
                    isShowTipTimeout=setTimeout(function(){
                        tempTipObj.hide()
                    },5000);
                }

                //按时间区间关闭公告栏
                var _isShowTip = 1;
                if(_isShowTip == 1)
                    isShowTip = 1;

                if(!isShowTip || isShowTip==0){
                    tempTipObj.show().appendTo('#hjpassport');
                }
                showTipemp();
                tempTipObj.on('click','.close',function(){
                    HJBulo.fun.setCookie('league_tip_temp1', 1, 99 * 24 * 60 * 60);
                    tempTipObj.hide();
                }).hover(function(){
                    clearTimeout(isShowTipTimeout)
                },function(){
                    showTipemp();
                });

            });
            //End goto 返回顶部

            function showUserSetting() {
                if (document.getElementById("usersetting").style.display == "none") {
                    var tabs = $("#app_cnt_tabs");
                    var pnl = $("#usersetting");
                    pnl.find(".content").width(260);
                    pnl.fadeIn();
                    pnl.css("top", tabs.offset().top + 20);
                    pnl.css("left", tabs.offset().left + 480);
                    setbackground(pnl, true);
                } else {
                    hideshowUserSetting();
                }
            }

            function hideshowUserSetting() {
                var pnl = $("#usersetting");
                pnl.hide();
                setbackground(pnl, false);
            }

            function doSetDefaultLanguage() {
                var lang = $("#SelectDefaultLanguage").val();
                var shortkey = $("[name=shortkey]:checked").val();
                Hjef.ajax("AjaxComs.SaveUserSetting", lang, shortkey, function(res) {
                    if (shortkey == "") {
                        alert('请选择播放器快捷键版本');
                    }
                    if (res.value) {
                        hideshowUserSetting();
                        window.location.reload();
                    }
                });
            }

        </script>

