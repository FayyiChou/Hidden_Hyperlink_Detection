    </ul>
</div>
<script type="text/javascript">
    var ListenLeftBar = {};
    $(function () {
        /*$("#leftbar .fir_bar,.sec_bar li").hover(function ()
        {
            if (!$(this).hasClass("active")) {
                $(this).toggleClass("bar_hover");
            }
        })*/
        $(".icon_dropdown").click(function () {
            if($.browser.msie && $.browser.version === '6.0' || $.browser.version === '7.0'){
                $(this).toggleClass("active").next("ul").toggle();
            }else{
                $(this).toggleClass("active").next("ul").slideToggle();
            }
        });
        ListenLeftBar.Init = function (activeTab, lang, menuId) {
            if (activeTab=="listen") {
                $("#sideBar .mylisten").addClass("active")
            }
            else if (activeTab == "mymenu") {
                $("#sideBar .mymenu").addClass("active")
            }
            else {
                if (location.pathname == "/") {
                    $("#sideBar .home").addClass("active")
                }
                if (lang != "") {
                  $("#sideBar_type .listen_" + lang).parent("li").find(".sec_bar").show();
                  $("#sideBar_type .listen_" + lang).parent("li").find(".icon_dropdown").addClass("active");
                    if (menuId != ""&&menuId!="0") {
                        $("a[href='/" + lang + "/" + menuId + "/']").addClass("active");
                    }else{
                        $("a[href='/" + lang + "/']").addClass("active");
                    }
                }
            }
        }
    })
</script>
