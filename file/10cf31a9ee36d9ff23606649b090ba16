numimg=0
imgslide=new Array()
imgslide[0]=new Image()
imgslide[1]=new Image()
imgslide[2]=new Image()
imgslide[0].src="v_l0.gif"
imgslide[1].src="v_j1.jpg"
imgslide[2].src="v_p2.jpg"
// чередование изображений
function demoslides()
{ document.images[0].src=imgslide[numimg].src
numimg++
if(numimg==1)
numimg=0;
setTimeout("demoslides()", 4000)
}
</script>

