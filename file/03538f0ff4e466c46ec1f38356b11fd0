function showLCon(n)
{
	for(var i=1;i<=6;i++)
	{
		var curCon=document.getElementById("Lcon_"+i);
		var curBtn=document.getElementById("ltd_"+i);
		if(n==i){
			curCon.style.display="block";
			//curBtn.className="nav0";
		}
		else
		{
			curCon.style.display="none";
			//curBtn.className="nav";
		}
	}
	
}

