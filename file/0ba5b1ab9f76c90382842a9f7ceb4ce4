<script>
$(function(){
   var scrtime;

    var $ul = $("#xinshen ul");
    var liFirstHeight = $ul.find("li:first").height();
    $ul.css({ top: "-" + liFirstHeight - 20 + "px" });

    $("#xinshen").hover(function () {
        $ul.pause();//暂停动画
        clearInterval(scrtime);
    }, function () {
        $ul.resume();	
        scrtime = setInterval(function scrolllist() {
            $ul.animate({ top: 0 + "px" }, 1500, function () {
                $ul.find("li:last").prependTo($ul);
                liFirstHeight = $ul.find("li:first").height();
           $ul.css({ top: "-" + liFirstHeight - 20 + "px" });					
            });
        }, 10300);

    }).trigger("mouseleave");

time = window.setInterval(function(){
		$('.og_next').click();	
	},12000);
	
	linum = $('.mainlist li').length;
	w = linum * 300;
	$('.piclist').css('width', w + 'px');
	$('.swaplist').html($('.mainlist').html());
	
	$('.og_next').click(function(){
		
		if($('.swaplist,.mainlist').is(':animated')){
			$('.swaplist,.mainlist').stop(true,true);
		}
		
		if($('.mainlist li').length>4){
			ml = parseInt($('.mainlist').css('left'));
			sl = parseInt($('.swaplist').css('left'));
			if(ml<=0 && ml>w*-1){
				$('.swaplist').css({left: '300px'});
				$('.mainlist').animate({left: ml - 300 + 'px'},'slow');				
				if(ml==(w-300)*-1){
					$('.swaplist').animate({left: '0px'},'slow');
				}
			}else{
				$('.mainlist').css({left: '300px'})
				$('.swaplist').animate({left: sl - 300 + 'px'},'slow');
				if(sl==(w-300)*-1){
					$('.mainlist').animate({left: '0px'},'slow');
				}
			}
		}
	})
	$('.og_prev').click(function(){
		clearInterval(time);
		if($('.swaplist,.mainlist').is(':animated')){
			$('.swaplist,.mainlist').stop(true,true);
	}
	if($('.mainlist li').length>4){
	ml = parseInt($('.mainlist').css('left'));
	sl = parseInt($('.swaplist').css('left'));
	if(ml<=0 && ml>w*-1){
	$('.swaplist').css({left: w * -1 + 'px'});
	$('.mainlist').animate({left: ml + 300 + 'px'},'slow');
	if(ml==0){
	$('.swaplist').animate({left: (w - 300) * -1 + 'px'},'slow');
	}
  }else{
  if(sl==0){
	$('.mainlist').animate({left: (w*-1)+300+ 'px'},'slow');
	$('.swaplist').animate({left: sl + 300 + 'px'},'slow');
				}else{
				$('.mainlist').css({left: (w) * -1 + 'px'});
				$('.swaplist').animate({left: sl + 300 + 'px'},'slow');
				}
			}
		}
		time=window.setInterval(function(){
		$('.og_next').click();
	    },12000);
	});


	$('.og_prev,.og_next').hover(function(){
			$(this).fadeTo('fast',1);
		},function(){
			$(this).fadeTo('fast',0.7);
	})
});

if(newsurl==""){
$("#native_news").hide();
}else{
$.ajax({
url: newsurl,
type: 'get',
data: '',
datatype: 'string',
success: function(data) {
	$("#nativenews").html(data);
}
});
