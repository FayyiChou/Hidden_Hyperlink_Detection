<script>
function buyitem(){
	var temp_pkey = $("#pkey").val();
	window.location.href = BASE_URL+"/item/show/pkey/"+temp_pkey;
}

$.ajax({
	type: "POST",
	url: BASE_URL+"/server/list",
	dataType: "json",
	success: function(data) {
		var dianxin_str = "";
		var wangtong_str = "";
		for (var i in data) {
			if (data[i]['type'] == 1) {
				//电信
				dianxin_str += '<option value="' + data[i]['id'] + '">' + data[i]['name'] + '</option>';
			} else if (data[i]['type'] == 2) {
				//网通
				wangtong_str += '<option value="' + data[i]['id'] + '">' + data[i]['name'] + '</option>';
			}
		}
		$("#area_select_1,#area_select_2,#area_select_3").append(dianxin_str+wangtong_str);
	}
});
//保留wap首页快速购买信息
setTimeout(function(){
	var strCookie=document.cookie;
	var arrCookie=strCookie.split("; ");
	for(var i=0;i<arrCookie.length;i++){
		var arr=arrCookie[i].split("=");
		if(arr[0]=="au_mobile_lb_num"){
			$("#num_input_1").val(arr[1]);
		}else if(arr[0]=="au_mobile_lb_get"){
			$("#get_input_1").val(arr[1]);
		}else if(arr[0]=="au_mobile_lb_server"){
			$("#area_select_1").val(arr[1]);
		}else if(arr[0]=="au_mobile_lb_pay"){
			$("#pay_select_1").val(arr[1]);
		}else if(arr[0]=="au_mobile_jy_num"){
			$("#num_input_2").val(arr[1]);
		}else if(arr[0]=="au_mobile_jy_get"){
			$("#get_input_2").val(arr[1]);
		}else if(arr[0]=="au_mobile_jy_server"){
			$("#area_select_2").val(arr[1]);
		}else if(arr[0]=="au_mobile_jy_pay"){
			$("#pay_select_2").val(arr[1]);
		}else if(arr[0]=="au_mobile_qlx_num"){
			$("#num_input_3").val(arr[1]);
		}else if(arr[0]=="au_mobile_qlx_server"){
			$("#area_select_3").val(arr[1]);
		}else if(arr[0]=="au_mobile_qlx_pay"){
			$("#pay_select_3").val(arr[1]);
		}
	}
},1000);
</script></div>

