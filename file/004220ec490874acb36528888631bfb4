    function qie(src) {
        var _dataimg = $(src).attr("dataimg");
        var _dataurl = $(src).attr("dataurl");
        var _datatit = $(src).attr("datatit");
        var _datadesc = $(src).attr("datadesc");

        $("#come").html(_datatit);
        $("#comeshort").html(_datadesc);
        $("#comimg").attr("href", _dataurl);
        $("#comimg").attr("title", _datatit);
        $("#comimg2").attr("href", _dataurl);
        $("#comimgs").attr("src", _dataimg);
        $("#comimgs").attr("alt", _title);
        $("#comimgs").attr("title", _title);



    }

