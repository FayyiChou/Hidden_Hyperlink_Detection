	<table width="158" border="0" align="center" cellpadding="0" cellspacing="0" id="service_index">
  <tr>
    <td align="center">
    <A HREF="#" onClick="window.open('http://www.edubook.com.tw/OnlineService.aspx', 'Joseph', config='height=800,width=1000')"><img src="./../File/Image/service_BT_icon_offline.gif" alt="線上客服" title="線上客服"  vspace="8" border="0"></a>
