<script type="text/javascript">
    
    var NavigationMoudle = (function ()
    {
        function toogleShare()
        {
            var shareE = document.getElementById("share");
            var popShareE = document.getElementById("popShare");
            if (shareE.className == "b-news-share")
            {
                shareE.className = "b-news-share b-news-share-show";
                popShareE.style.display = "block";
            } else
            {
                shareE.className = "b-news-share";
                popShareE.style.display = "none";
            }
        }

        return {
            ToogleShare: toogleShare
        }
    })();

    $(document).bind('touchmove click', function (event)
    {
        event.stopPropagation();
        var clickEle = event.target.id;
        if (event.target.tagName.toLowerCase() != 'a')
        {
            if (clickEle != 'share')
            {
                $('#popShare').hide();
                $('#share').removeClass('b-news-share-show');
            }
        }
    });
</script>

    
