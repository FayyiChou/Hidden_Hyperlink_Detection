<script type="text/javascript">
    function t(){var e=window.innerWidth;document.getElementsByTagName("html")[0].style.fontSize=.9765625*e+"%",document.getElementsByTagName("body")[0].style.opacity=1}window.onresize=t,window.onorientationchange = function(){setTimeout(t,500)},setTimeout(t,100);
    var isShowValidate = false;

    // PRD文档：http://wiki.sankuai.com/pages/viewpage.action?pageId=570269482
    // 金额为0时视为该区域无活动
    if (money === 0) {
        $('.form').hide();
        $('.no-ctn').show();
        $('.memo').hide();
    }

    var u = navigator.userAgent;
    var isIOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/);
    if (isIOS) {
        $('.ios').show();
    }

    switch($(".desc b").text().length){
        case 4:
            $(".desc").css("font-size","0.29rem");
            break;
        case 5:
            $(".desc").css("font-size","0.27rem");
            break;
        case 6:
            $(".desc").css("font-size","0.25rem");
            break;
    }
    var countDown;
    var delay = 60;
    var clearCountDown = function(){
        $(".code button").hide();
        $(".code a").show();
        clearInterval(countDown);
        delay = 60;
    };
    $(".code a").click(function(){
        $(".code button").show();
        countDown = setInterval(function(){
            delay--;
            $(".code button").text("重新获取("+delay+"s)");
            if(delay==0){
                clearCountDown();
            }
        },1000);
         $.ajax({
            url:"/recommend/getValidateCode?mobile=" + $("#mobile").val(),
            async:true,
            dataType:'json',
            success: function (data) {
                if (data == null) {
                    myAlert("获取验证码失败,请稍后重试");
                    clearCountDown();
                } else if (data.code != 0) {
                    myAlert(data.msg);
                    clearCountDown();
                } else{
                    myAlert("验证码发送成功");
                }
            },
            error: function () {
                myalert("获取验证码失败,请稍后重试");
               clearCountDown();
            }
        }); 
    });
    $(".memo img").click(function(){
        $(".input").hide();
        $(".notice").show();
    });
    $(".notice a").click(function(){
        $(".input").show();
        $(".notice").hide();
    });
    if(isShowValidate===true){
        $(".code").show();
    };
    var senderId = $("form#params-form input[name=mtUserId]").val();
    var promoteType = $("form#params-form input[name=promoteType]").val();

    var timer;
    function showToast() {
        $('.toast').show();
        if (timer) {
            clearTimeout(timer);
            timer = null;
        }
        timer = setTimeout(function() {
            $('.toast').hide();
        }, 2000);
    }

    $("#submit").click(function() { 
        var phone = $("#mobile").val();
        var phoneReg = /^1\d{10}$/;
        if (!phoneReg.test(phone)) {
            showToast();
            return;
        }

        var validateCode = $("#code").val();
        if(validateCode=="")
            validateCode = 0;
        if (!!phone) {
            var param = new Object();
            param.mtUserId = senderId;
            param.mobile = phone;
            param.promoteType = promoteType;
            param.validateCode = validateCode;
            $.post("/recommend/getReward", $.param(param), function(data) {
                var code = data.code;
                switch(data.code){
                    case 700:
                         showNotice('<p>恭喜，您已获得<span class="money">' + money + '</span>元奖励!</p><p>按规定完成<span class="money">1</span>单任务后自动发放</p>');
                    break;
                    case 701:
                        showNotice('<p>抱歉，老用户不能领取此奖励</p><p>邀请新伙伴，各得<span class="money">' + money + '</span>元</p>');
                    break;
                    case 702:
                         showNotice('<p>您已领取过<span class="money">' + money + '</span>元奖励</p><p>按规定完成<span class="money">1</span>单任务自动发放</span></p>');
                    break;
                    case -2:
                        myAlert(data.msg);
                        $(".code").show();
                    break;
                    case 40001:
                        myAlert(data.msg);
                        $(".code").show();
                    break;
                    // 接口文档：http://wiki.sankuai.com/pages/viewpage.action?pageId=259770580
                    // 处理新的799 code
                    case 799:
                        location.reload();
                    break;
                    case -1://不明错误
                        myAlert("服务器遇到问题，请稍后重试");
                    break;
                    case 40000://不明错误
                        myAlert("服务器遇到问题，请稍后重试");
                    break;
                    default:
                        myAlert(data.msg);
                    break;
                }
            });
        }
    });
    var showNotice = function(msg){
        $(".desc").html(msg);
        $(".down").show();
        $(".mobile,.code,.button").hide();
    };
    var myAlert = function(msg){
        $(".alert").text(msg).show();
        setTimeout(function(){$(".alert").hide()},3000);
    }
</script>
<script type="text/javascript">
    var shareTitle = $('form#shareInfo-form input[name=shareTitle]').val();
    var shareDesc = $('form#shareInfo-form input[name=shareDesc]').val();
    var nonceStr = $('form#shareInfo-form input[name=nonceStr]').val();
    var timestamp = $('form#shareInfo-form input[name=timestamp]').val();
    var signature = $('form#shareInfo-form input[name=signature]').val();
    var shareUrl = $('form#shareInfo-form input[name=shareUrl]').val();
    var logo = $('form#shareInfo-form input[name=logo]').val();

    wx.config({
        debug: false, // 开启调试模式,调用的所有api的返回值会在客户端alert出来，若要查看传入的参数，可以在pc端打开，参数信息会通过log打出，仅在pc端时才会打印。
        appId: 'wx40e82d6d63d99ff6', // 必填，公众号的唯一标识
        timestamp: timestamp, // 必填，生成签名的时间戳
        nonceStr: nonceStr, // 必填，生成签名的随机串
        signature: signature,// 必填，签名，见附录1
        jsApiList: [
            'onMenuShareTimeline', 
            'onMenuShareAppMessage',
            'onMenuShareQQ'
        ]
    });

    wx.ready(function() {
        // 分享至微信朋友圈
        wx.onMenuShareTimeline({
            title: shareTitle, // 分享标题
            link: shareUrl,
            imgUrl: logo // 分享图标
        });
        // 分享至微信朋友
        wx.onMenuShareAppMessage({
            title: shareTitle, // 分享标题
            desc: shareDesc, // 分享描述
            link: shareUrl,
            imgUrl: logo, // 分享图标
            type: 'link', // 分享类型,music、video或link，不填默认为link
        });
        // 分享至QQ
        wx.onMenuShareQQ({
            title: shareTitle, // 分享标题
            desc: shareDesc, // 分享描述
            link: shareUrl, // 分享链接
            imgUrl: logo, // 分享图标
        });
    });
</script>
