		function display2() {
			lastson2 = son2;
			son2++;
			if (son2 > sslides2) {
				son2 = 1;
				lasttofirst2 = 1;
			}
			
			ease2 = 0;
	
			rundisplay2 = setTimeout('display2()', (sseconds2 * 1000));
		}
		
		function easein2() {
			if ((ease2 >= 0) && (ease2 < 100)) {
				if (((son2 == 1) && (lasttofirst2 == 0)) || ((son2 > 1) && (son2 <= sslides2))) {
					ease2 += 5;
					switch (effect2) {
						case 'fade':
							document.getElementById('slide2_' + son2).style.opacity = (ease2 / 100);
							document.getElementById('slide2_' + son2).style.filter = 'alpha(opacity=' + ease2 + ')';
							if (lastson2 > 0) {
								document.getElementById('slide2_' + lastson2).style.opacity = (1 - (ease2 / 100));
								document.getElementById('slide2_' + lastson2).style.filter = 'alpha(opacity=' + (100 - ease2) + ')';
							}
							document.getElementById('slide2_' + son2).style.display = 'block';
						break;
						
						case 'right to left':
							document.getElementById('slide2_' + son2).style.left = (100 - ((100 * ease2) / 100)) + '%';
						break;
						
						case 'left to right':
							document.getElementById('slide2_' + son2).style.left = (((100 * ease2) / 100) - 100) + '%';
						break;
						
						case 'top to bottom':
							document.getElementById('slide2_' + son2).style.top = (((362 * ease2) / 100) - 362) + 'px';
						break;
						
						case 'bottom to top':
							document.getElementById('slide2_' + son2).style.top = (362 - ((362 * ease2) / 100)) + 'px';
						break;
					}
				} else if ((son2 == 1) && (lasttofirst2 == 1)) {
					ease2 += 5;
					switch (effect2) {
						case 'fade':
							document.getElementById('slide2_' + sslides2).style.opacity = (1 - (ease2 / 100));
							document.getElementById('slide2_' + sslides2).style.filter = 'alpha(opacity=' + (100 - ease2) + ')';
							document.getElementById('slide2_1').style.opacity = (ease2 / 100);
							document.getElementById('slide2_1').style.filter = 'alpha(opacity=' + ease2 + ')';
							if (ease2 == 100) {
								document.getElementById('slide2_' + sslides2).style.display = 'none';
							}
						break;
						
						case 'right to left':
							document.getElementById('slide2_' + sslides2).style.left = (0 - ((100 * ease2) / 100)) + '%';
						break;
						
						case 'left to right':
							document.getElementById('slide2_' + sslides2).style.left = ((100 * ease2) / 100) + '%';
						break;
						
						case 'top to bottom':
							document.getElementById('slide2_' + sslides2).style.top = ((362 * ease2) / 100) + 'px';
						break;
						
						case 'bottom to top':
							document.getElementById('slide2_' + sslides2).style.top = (0 - ((362 * ease2) / 100)) + 'px';
						break;
					}
				}
			}
			if ((son2 > 2) && (son2 <= sslides2) && (ease2 == 100)) {
				switch (effect2) {
					case 'fade':
						document.getElementById('slide2_' + parseInt(son2 - 1)).style.opacity = '0.0';
						document.getElementById('slide2_' + parseInt(son2 - 1)).style.filter = 'alpha(opacity=0)';
						document.getElementById('slide2_' + parseInt(son2 - 1)).style.display = 'none';
					break;
					
					case 'right to left':
						document.getElementById('slide2_' + parseInt(son2 - 1)).style.left = '100%';
					break;
					
					case 'left to right':
						document.getElementById('slide2_' + parseInt(son2 - 1)).style.left = '-100%';
					break;
					
					case 'top to bottom':
						document.getElementById('slide2_' + parseInt(son2 - 1)).style.top = '-362px';
					break;
					
					case 'bottom to top':
						document.getElementById('slide2_' + parseInt(son2 - 1)).style.top = '362px';
					break;
				}
				ease2 = 101;
			}
	
			runeasein2 = setTimeout('easein2()', 10);
		}
		
		function pause2() {
			clearTimeout(rundisplay2);
			clearTimeout(runeasein2);
		}
		
		easein2();
		display2();
		
		window.addEventListener('resize', autoScale2);
		
		function autoScale2() {
			var slideHeight = ((window.innerWidth * 425) / 2000);
			document.getElementById('slidebox2').style.height = slideHeight + 'px';
				document.getElementById('slidelink2_1').style.left = (window.innerWidth - 79) + 'px';
					document.getElementById('slidelink2_1').style.top = (slideHeight - 27)  + 'px';
					document.getElementById('slidelinkon2_1').style.left = (window.innerWidth - 79) + 'px';
					document.getElementById('slidelinkon2_1').style.top = (slideHeight - 27)  + 'px';
					document.getElementById('slidelink2_2').style.left = (window.innerWidth - 52) + 'px';
					document.getElementById('slidelink2_2').style.top = (slideHeight - 27)  + 'px';
					document.getElementById('slidelinkon2_2').style.left = (window.innerWidth - 52) + 'px';
					document.getElementById('slidelinkon2_2').style.top = (slideHeight - 27)  + 'px';
					
		}
			
