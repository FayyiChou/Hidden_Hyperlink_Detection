    <script type="text/javascript">
	//缓冲运动
	function bufferMove(obj, json, endFn) {
	    clearInterval(obj.timer);
	    obj.timer = setInterval(function() {
	        var bBtn = true;
	        for (var attr in json) {
	            var iCur = 0;
	            if (attr == 'opacity') {
	                if (Math.round(parseFloat(getStyle(obj, attr)) * 100) == 0) {
	                    iCur = Math.round(parseFloat(getStyle(obj, attr)) * 100);
	                } else {
	                    iCur = Math.round(parseFloat(getStyle(obj, attr)) * 100) || 100;
	                }
	            } else {
	                iCur = parseInt(getStyle(obj, attr)) || 0;
	            }

	            var iSpeed = (json[attr] - iCur) / 4;
	            iSpeed = iSpeed > 0 ? Math.ceil(iSpeed) : Math.floor(iSpeed);
	            if (iCur != json[attr]) {
	                bBtn = false;
	            }
	            if (attr == 'opacity') {
	                obj.style.filter = 'alpha(opacity=' + (iCur + iSpeed) + ')';
	                obj.style.opacity = (iCur + iSpeed) / 100;
	            } else {
	                obj.style[attr] = iCur + iSpeed + 'px';
	            }
	        }
	        if (bBtn) {
	            clearInterval(obj.timer);
	            if (endFn) {
	                endFn.call(obj);
	            }
	        }
	    },
	    50);
	}

	function getStyle(obj, attr) {
	    if (obj.currentStyle) {
	        return obj.currentStyle[attr];
	    } else {
	        return getComputedStyle(obj, false)[attr];
	    }
	}



	//window.onload
	(function(){

		var setCookie = function(name,value,expireHours){
			var cookieString=name+"="+escape(value);
			if(expireHours>0){
				var date=new Date();
				date.setTime(date.getTime+expireHours*3600*1000);
				cookieString=cookieString+"; expire="+date.toGMTString();
			}
			document.cookie=cookieString;
		}

		var getCookie = function(name){
			var strcookie=document.cookie;
			var arrcookie=strcookie.split("; ");
			for(var i=0;i<arrcookie.length;i++){
				var arr=arrcookie[i].split("=");
				if(arr[0]==name)return arr[1];
			}
			return "";
		}

		var jrzbWrap = document.getElementById('jrzbwrap');
		var jrzbLeft = document.getElementById('jrzbleft');


		//当鼠标移入向左滑动
		//是否展开
		var flag2 = getCookie("sg_cblzb"),flag = true;
		if(flag2 == ""){
			setCookie("sg_cblzb","1",24);
		}
		if(flag2 == "0"){
			flag = false;
			bufferMove(jrzbWrap,{right:-350});
			removeClass(jrzbLeft, 'jrzblefton');
			setCookie("sg_cblzb","0",5);
		}
		
		jrzbWrap.onclick = function(){
			if(flag){
				bufferMove(jrzbWrap,{right:-350});
				removeClass(jrzbLeft, 'jrzblefton');
				setCookie("sg_cblzb","0",5);
			}else{
				bufferMove(jrzbWrap,{right:0});
				addClass(jrzbLeft, 'jrzblefton');
				setCookie("sg_cblzb","1",5);
			}
			flag = !flag;
		}
	})()

	//添加class样式
    function addClass(obj, sClassName){
        var sClass =  obj.className;
        if (sClass == '') {
            obj.className = sClassName;
        } else {
            var aClass = sClass.split(' ');             
            for (var i=0; i<aClass.length; i++) {
                if (aClass[i] == sClassName) {
                    return ;
                }
            }
            obj.className = sClass + ' ' + sClassName;
        }
    }

    //删除class样式
    function removeClass(obj, sClassName){
        var aClass =  obj.className.split(' ');
        var arr = [];
        for (var i=0; i<aClass.length; i++) {
            if (aClass[i] != sClassName) {
                arr.push(aClass[i]);
            }
        }
        obj.className = arr.join(' ');
    }	
</script>
