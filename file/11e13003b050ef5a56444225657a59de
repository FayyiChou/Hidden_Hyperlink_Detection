jQuery(function(){
      function subStr(str,n){
		 var filename=str.replace(/.*(\/|\\)/, "");
		 var fileExt=(/[.]/.exec(filename)) ? /[^.]+$/.exec(filename.toLowerCase()) : '';
		 var strReg=/[^\x00-\xff]/g;  
         var _str=filename.replace(strReg,"**");  
	     var _len=_str.length;
		 if(_len>n){  
		 var _newLen=Math.floor(n/2);  
		 var _strLen=str.length;  
		 for(var i=_newLen;i<_strLen;i++){  
			 var _newStr=str.substr(0,i).replace(strReg,"**");  
			 if(_newStr.length>=n){  
				 return str.substr(0,i)+"...."+fileExt;  
				 break;  
			}  
		 }  
	   }else{  
		 return str;  
	   }  
   	}
	  jQuery(".origin").each(function(){
	  	var content=jQuery(this).text();
		jQuery(this).text(subStr(content,20));
	  })
});	
	
	var $ =mootools ;
</script>
