<script>
	function loadVideo() {
		var oMoocPlayer = jQuery('#moocplayer');
		 var sVideoData =  oMoocPlayer ? oMoocPlayer.attr('data') : null;
		 if (sVideoData) {
		        sVideoData=sVideoData.replace(/\\~/g,"'");// 单引号替换过来
			    var oVideoData = JSON.parse(sVideoData);
        		var videoUrl=oVideoData.http;
        		var priviewUrl=oVideoData.screenshot;
        		var hd_videoUrl=oVideoData.httphd;
        		if(document.domain=='haust.edu.cn'){
        			videoUrl=videoUrl.replace("chaoxing.com",document.domain);
        			priviewUrl=priviewUrl.replace("chaoxing.com",document.domain);
        			hd_videoUrl=hd_videoUrl.replace("chaoxing.com",document.domain);
        		}
				var player = new SimplePlayer({
				render: 'moocplayer',
				videoUrl: videoUrl,
				priviewUrl:priviewUrl,
				hd_videoUrl:hd_videoUrl
				
			});
			
		}
	}
	
	loadVideo();
    var play=document.getElementById("zvideo_play");
    if(play){
    play.onclick=function(){
        var zplay=document.getElementById("zvideo");
        zplay.style.display="block";
        this.style.display="none";
    	
    }
 }

</script>
