<script type="text/javascript">
 window.onload=function(){
 	var knowledgeId = $("#knowledgeId").val();
    var indexnum=1;
	$(".module").css('display','none');
	$(".module"+indexnum).css('display','block');
    var ali =$(".navBg td");
	
	ali.find("a").removeClass('click');
	$(".navBg td").eq(indexnum-1).find("a").addClass("click")
    var modulesize=Math.ceil(ali.length);
		ali.click(function(){
			var indexnum=ali.index(this)+1;
			var module="module"+indexnum;
			$(".module").css('display','none');
			var date = new Date().getTime();
			location.href="?edit=false&knowledgeId="+knowledgeId+"&module="+indexnum+"&v="+date+"#content";
		});
		



$("#iboxAlertOk").click(function(){
	$('.courseAlert').hide();
});
$(".courseTilte .ibox_close").click(function(){
	$('.courseAlert').hide();
});

	jQuery(".courseTilte-confirm .ibox_close").click(function(){
    	jQuery('.courseAlert-confirm').hide();
    });
    jQuery("#iboxAlertOk-confirm-cancel").click(function(){
    	jQuery('.courseAlert-confirm').hide();
	});
		
 
	var heit = $(".showBox").height();
	if(heit<=186){
		$(".showall").hide()
	} else {
		$(".showall").show();
		$(".showBox").css("height","192px")
	}
	$(".showall").toggle(function(){
		$(".showBox").css("height","auto")
		$(this).text("[收起]")
	},function(){
		$(".showBox").css("height","186px")
		$(this).text("[展开]")
		})
		
		
		
	$(".secondCap ul li").click(function(){
		$(".secondCap ul li").removeClass("click")
		$(this).addClass("click")
	})
	$(".teachers").hover(function(){
		$(this).find(".teachersBottom").show();
		$(this).find("b").show()
		},function(){
		$(this).find(".teachersBottom").hide();
		$(this).find("b").hide()		
		})	
	
uParse(".ans-cc");

}
</script>
