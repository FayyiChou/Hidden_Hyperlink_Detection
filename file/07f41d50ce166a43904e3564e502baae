<script type="text/javascript">
	$(function(){
		var msgView;// 信息窗口
		var reserveid = $("#reserveid").val();
		var reserve = $("#reserve");// 查返程
		var metromap = $("#metromap");
		reserve.onclick=function(){
			if(msgView && !msgView.closed){
				return;
			}
		// 判断是否有返程
			if ( !reserveid) {
				msgView=$.showmsg("抱歉，还没有返程的线路！");
			} else {
				location.replace("routeinfo.jspx?" + $("#myform").serialize());
			}
		};
		if (metromap){
			metromap.onclick=function(){
				$("#formmetro").submit();
			};
		}
	});
</script>

<script>

	function stationinfo(id){
		location.href="stationinfo.jspx?mark=&id="+id;
	}
	
	
	var url = "aj_jmrtb.jspx?lineno=&sid=&eid=&isreturnfocus=true";
	var onSucc = function(data){
		try{
			var imgs = $(".station_bus_in");
			for(var i = 0;i<imgs.length;i++){
				imgs[i].hideView();
			}
			
			imgs = $(".station_bus_out");
			for(var i = 0;i<imgs.length;i++){
				imgs[i].hideView();
			}
			
			var station;
			var testInfo = "*";
			for(var i = 0;i<data.length;i++){
				station = data[i];
				if(station){
					var iid = "station_bus_in_"+station.stopno;
					var oid = "station_bus_out_"+station.stopno;
					var stopnumafter = station.stopnumafter;
					if(0 == stopnumafter){
						//抵达站点
						$("#"+iid).showView();
					}else if(1== stopnumafter){
						//还有一站
						$("#"+iid).showView();
					}
				}
			}
		}catch(e){
		}
		
	};
	
	var msgView;// 信息窗口
	var onFail = function(){
		$.showmsg("网络异常或系统繁忙");
	};
	var onFinish = function(){
// 		msgView.hide();

	};
	
	var aj_rtb = function(){
		$.getJSON(url+"&f="+(new Date()).getTime(), onSucc , onFail, onFinish);
	}
	function refresh(){
		if("江门市" == ''){
// 			msgView = new $.msgView({msg:"实时公交",iswait:true}).show();
			setTimeout(aj_rtb,200);
		}
	}
	refresh();
	setInterval(refresh,10000);
</script>

