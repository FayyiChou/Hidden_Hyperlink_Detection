<script language="JavaScript" type="text/JavaScript">
function MM_openBrWindow(theURL,winName,features) {window.open(theURL,winName,features);}

function reset()
{
	document.mainform.yz.value="";
	document.mainform.inputtext.value="";
	document.MAINFORM.InputText.focus();
}
function check()
{

   if (isNaN(document.MAINFORM.yz.value)||document.MAINFORM.yz.value=="")
	{
		window.alert("请输入验证码！");
		document.MAINFORM.yz.value="";
		document.MAINFORM.yz.focus();
		return false;
	}
	else
	{
		if (isNaN(document.MAINFORM.InputText.value))
		{
			window.alert("请正确输入标签上的数码！");
			document.MAINFORM.InputText.value="";
			document.MAINFORM.InputText.focus();
			return false;
		}
		else
		{
			if (document.MAINFORM.InputText.value.length!=21)
			{
				if (document.MAINFORM.InputText.value.length!=16)
				{
					window.alert("请正确输入标签上的数码！");
					document.MAINFORM.InputText.value="";
					document.MAINFORM.InputText.focus();
					return false;
				}
			   else
				{
				  var strInput = document.MAINFORM.InputText.value;	
				  var strYZ = document.MAINFORM.yz.value;	
				  document.MAINFORM.InputText.value = "" ;
				  document.MAINFORM.yz.value = "" ;
				  //window.open('tool/fwmain.asp?InputText='+strInput + '&yz=' + strYZ,'popupnav','height=350,width=400,resizable=no,scrollbars=no');
				  window.open('http://ccn.yesno.com.cn/fwMain-1.asp?InputText='+strInput + '&yz=' +strYZ);
				}
			}
			else
			{
				var strInput1 = document.MAINFORM.InputText.value;
				var strYZ1 = document.MAINFORM.yz.value;
				document.MAINFORM.InputText.value = "" ;
				document.MAINFORM.yz.value = "" ;
				window.open('http://ccn.yesno.com.cn/tool/fwmain.asp?InputText='+strInput1 + '&yz=' + strYZ1,'popupnav','height=350,width=400,resizable=no,scrollbars=no');
				//window.location="http://ccn.yesno.com.cn";
			}
		}
	}
}
function check1()
{

   if (isNaN(document.MAINFORM.yz.value)||document.MAINFORM.yz.value=="")
