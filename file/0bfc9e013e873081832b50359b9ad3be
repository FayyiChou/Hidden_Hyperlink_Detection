<script type="text/javascript"> 
$(function(){
   var hasMP4='';
   var hasVideo='';
    if(iPx()){
        $('#pcList').remove();
        $('#padList').show();
        }
        else{
         $('#padList').remove();
         $('#pcList').show();
        }
       if(hasMP4=="0"){
      //alert("mp4");
        $('#padList').remove();
       }
       if(hasVideo=="0"){
      // alert("video");
        $('#pcList').remove();
       }
});
    
                
</script>
 
