    <script type="text/javascript">

        //        <!-- 锁死窗口-->
        function lockWindow() {            
            $('html').css({ 'height': '100%' });
            $('body').css({ 'height': '100%', 'overflow': 'hidden' });
        }
        //      解锁窗口
        function unlockWindow() {
            $('html').css({ 'height': 'auto' });
            $('body').css({ 'height': 'auto', 'overflow': 'auto' });
        }

        var replacingImage = function() {
        document.getElementById("captchaimage").src = "https://dealer.360che.com/Ajax/AllowPageCaptcha.aspx?id=" + Math.random();
    }
    function fistImg() {
        var src = $("#captchaimage").attr("src");
        if (src == "https://dealer.360che.com/images/fistImg.jpg") {
            replacingImage();
        }
        return false;
    }
    
        //登录
        function login() {
            var name = $(".login-name").val();
            var pwd = $(".login-pwd").val();
            var yzm = $(".auth-code").val();
            if (name == '') {
                $(".warning-wrap").show();
                lockWindow();
                $("#mssg").html("用户名不能为空!");
                $(".login-name").focus();
                return false;
            }
            if (pwd == '') {
                $(".warning-wrap").show();
                lockWindow();
                $("#mssg").html("密码不能为空!");
                $(".login-pwd").focus();
                return false;
            }
            if (yzm == '') {
                $(".warning-wrap").show();
                lockWindow();
                $("#mssg").html("验证码不能为空!");
                $(".auth-code").focus();
                return false;
            }
            ILogin();
        }
        function ILogin() {
            var iscookie = 0;
            if ($('#auto-login').is(':checked')) {
                iscookie = 1;
            }
            $.post("Ajax/jsonLoginAccount.aspx", { loginname: $(".login-name").val(), pwd: $(".login-pwd").val(), captcha: $(".auth-code").val(), iscookie: iscookie }, function(data, status) {
                if (data.result == 1) {
                    getLoginHtml();
                }
                else {
                    $(".warning-wrap").show();
                    $("#mssg").html(data.msg);
                    if (data.msg.indexOf("登录名") > -1 || data.msg.indexOf("账户") > -1) {
                        $(".login-name").focus();
                    }
                    if (data.msg.indexOf("密码") > -1) {
                        $(".login-pwd").focus();
                    }
                    if (data.msg.indexOf("验证码") > -1) {
                        replacingImage();
                        $(".auth-code").focus();
                    }
                }
            }, "json");
        }
        function ILoginOut() {
            $.post("Ajax/jsonLoginAccount.aspx", { signout: 1 }, function(data, status) {
                //alert("退出登录成功");
                lockWindow();
                getLoginHtml();                
            }, "json");
        }
        function getLoginHtml() {
            $.ajax({
                url: "Ajax/JsonLogin.aspx",
                data: {},
                cache: false,
                dataType: "json",
                type: "post",
                success: function(data) {
                    $("#login_html").html("<span class='out' style='margin-top:6px;cursor:pointer'></span>" + data.html);
                    var loginSec = $('.login-section');
                    var outButton = loginSec.find('.out');

                    outButton.click(function() {
                        loginSec.css('display', 'none');
                        unlockWindow();
                    })
                }, error: function(data) {
                    window.location.reload(); //刷新当前页面.
                }
            });
        }
        getLoginHtml();    
    </script>
