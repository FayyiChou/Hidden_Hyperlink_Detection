                <script type="text/javascript">
                    var b = true;
                    function show() {
                        var o = document.getElementById('pop');
                        o.style.display = "block";
                        if (o.style.display == "block" && b) {
                            b = false;
                            var aj = $.ajax({
                                url: 'Ajax/ChangesCity.aspx',
                                data: {
                                    "cateId": 0,
                                    "BrandId": -1,
                                    "CityId": 161
                                },
                                type: 'post',
                                dataType: 'html',
                                success: function(data) {
                                    var mainhtml = new Array();
                                    mainhtml = data.split("$");
                                    $("#pop").append(mainhtml[0]);
                                    var menlist = mainhtml[1];
                                    var zhixias = $('#mm').innerHeight();
                                    menlist = menlist.substring(0, menlist.length - 1);
                                    var strs = new Array();
                                    var a = new Array();
                                    a[0] = zhixias;
                                    strs = menlist.split("#");
                                    if (strs.length > 1) {
                                        strs.pop();
                                    }
                                    var i = 1;
                                    var bianliang = zhixias;
                                    $.each(strs, function(index, tx) {
                                        //todo:tx=menuX
                                        var sheng = $('#' + tx + '').outerHeight();
                                        bianliang += sheng;
                                        a[i] = bianliang;

                                        i++;
                                    });
                                    var j = 0;
                                    $("#divid a").each(function() {
                                        $(this).attr({ onclick: "$('#testt').animate({ scrollTop: " + a[j] + " }, 500);$(this).addClass('current1').siblings().removeClass();" });
                                        j++;
                                    });
                                    //eacha();
                                },
                                error: function() {
                                    //alert("异常！");
                                }
                            });
                        }
                    }
                    function hide() {
                        $("#pop").html("");
                        b = true;
                        var o = document.getElementById('pop');
                        o.style.display = "none";
                        unlockWindow();
                    }

                </script>

                <!-- 显示城市 E-->
                <span class="search">
                    <input class="search-input" type="text" placeholder="请输入要查询的经销商" onkeydown="kswicth(event);"><input class="search-subm" type="button" value="搜索" onclick="search()"></span>

                <script type="text/javascript">
                    //搜索
                    function search() {
                        var key = $(".search-input").val();
                        //window.open();
                        var url = "http://so.360che.com/cse/search" + "?q=" + encodeURI(key) + "&s=3494890232603857038&nsid=0&";
                        window.open(url);
                    }

                    function kswicth() {
                        document.onkeydown = function mykeyDown(e) {
                            e = e || event;
                            if (e.keyCode == 13) {
                                search();
                                return false;
                            }
                            return;
                        }
                    };
                </script>

