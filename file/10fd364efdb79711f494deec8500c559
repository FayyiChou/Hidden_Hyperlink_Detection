        <script>
            $(document).ready(function() {

                $("#cate-list li").each(function() {
                    var index = $("#HiddenCateId").val();
                    if ($(this).attr("_val") == index) {
                        $(this).attr("class", "now");
                    }
                    else {
                        $(this).attr("class", "");
                    }
                    $(this).click(function(event) {
                        var cateid = $(this).attr("_val");
                        if (cateid == 3) {
                            return false;
                        }
                        var url = 'https://dealer.360che.com/Dealer_' + cateid + '_0_410000_161_0_0.html';
                        //var url = 'http://localhost:1389/Dealer_' + cateid + '_0_410000_161_0_0.html';
                        //alert(url);
                        window.location = url;
                    });
                });
            });

            function lxQQ(url) {
                //给QQ添加跳转                
                //var url = $(this).attr("_href");
                window.open(url);
            }
    
        </script>

        <script type="text/javascript">        

         //索引业务范围
        function getValue() {
            var glvsum = 0;            
            var glv = document.getElementsByName("ServiceType");
            for (var i = 0; i < glv.length; i++) {
                if (glv[i].checked == true) {
                    glvsum += parseInt(glv[i].value);
                }
            }
            //alert(glvsum);
            var url = 'https://dealer.360che.com/dealer_0_0_' + 410000 + '_'+161+'_0_' + glvsum + '.html';
            window.location = url;
        }
        
        //业务范围
        $(document).ready(function() {
            var ServiceTypeStr = $("#HiddenServiceTypeStr").val();
            if (ServiceTypeStr != null) {
                ServiceCheck(ServiceTypeStr);
            }
        });
        function ServiceCheck(str) {
            //alert(str);
            var obj = document.getElementsByName("ServiceType");
            var list = str.split(',');
            for (var i = 0; i < list.length; i++) {
                var id = list[i];
                if (id != "") {
                    for (var k = 0; k < obj.length; k++) {
                        var a = obj[k].value;                        
                        if (a == id) {
                            obj[k].checked = true;
                        }
                    }
                }
            }
        }
        
        //显示图片
        function showTel(shopId){        
           var div_ = "shop_" + shopId + "_tel";                    
           var brandname = $("#"+ div_).attr("_brandname");
           var tel = $("#"+ div_).attr("_tel");
           var html = "<a href='https://dealer.360che.com/"+ shopId + "/' target='_blank' style='color: #333;'>" +brandname + "</a> <span class=\"tel\">" + tel + "</span>";
           $("#"+ div_).html(html);
           
            $.ajax({ url: 'https://dealer.360che.com/Ajax/AddLinkView.aspx?d=' + shopId,
                type: 'POST',
                dataType: 'html',
                async: false,
                error: function() {
                },
                success: function(result) {
                }
            });
        }
        
          function unfold() {
            $('#unfold').one('click', function() {
                $(".dealer-card").show();
                var current = $(this);
                current.text('收起');
                current.parent().animate({ height: '630px' }, function() { $('.shade').css('display', 'none'); current.parent().addClass('unfold') });
                current.one('click', function() {
                    $(this).text('展开');
                    $(this).parent().animate({ height: '80px' }, function() { $('.shade').css('display', 'block'); current.parent().removeClass('unfold') });
                    $(".dealer-card").hide();
                    unfold();                    
                })
            });
        }
        unfold();
        

        </script>

