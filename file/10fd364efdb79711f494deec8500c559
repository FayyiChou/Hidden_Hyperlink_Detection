                    <script>
                        var indexWordLis = $('#index-word li');
                        var brandList = $('#brand-list');
                        var brandListLis = brandList.find('>li');
                        var oneLiH = $('.brand-name li').height();

                        for (var i = 0, l = indexWordLis.length; i < l; i++) {
                            indexWordLis.eq(i).attr('count', i);
                        }

                        indexWordLis.click(function() {
                            var num = 0;
                            $(this).addClass('now');
                            $(this).siblings().removeClass('now');
                            for (var i = 0; i <= $(this).attr('count') - 1; i++) {

                                num += brandListLis.eq(i).find('li').length;
                            }
                            brandList.scrollTop(num * oneLiH);
                        })
                    </script>

