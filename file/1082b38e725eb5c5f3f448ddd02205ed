var Shop = {"set":{"path":"\/","buytarget":"3","dragcart":null,"refer_timeout":15},"url":{"addcart":"http:\/\/www.cakeflowerexpress.com\/?cart-ajaxadd.html","shipping":"http:\/\/www.cakeflowerexpress.com\/?cart-shipping.html","payment":"http:\/\/www.cakeflowerexpress.com\/?cart-payment.html","total":"http:\/\/www.cakeflowerexpress.com\/?cart-total.html","viewcart":"http:\/\/www.cakeflowerexpress.com\/?cart-view.html","ordertotal":"http:\/\/www.cakeflowerexpress.com\/?cart-total.html","applycoupon":"http:\/\/www.cakeflowerexpress.com\/?cart-applycoupon.html","diff":"http:\/\/www.cakeflowerexpress.com\/?product-diff.html"}};
