                   if(el&&$type(el)=='element')
                   el.setHTML(el.retrieve('defaultInnerHTML'));
               });
               if($E('#goods-viewer .mprice'))$E('#goods-viewer .mprice').hide();
               updatepic();
               buyBtn.show();
               notifyBtn.hide();

          };




          var PRODUCT_HASH=new Hash({"6106":{"bn":"G52DDEFD143B62-1","price":99,"mktprice":118.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706155"]},"6107":{"bn":"G52DDEFD143B62-2","price":129,"mktprice":154.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706151"]},"6108":{"bn":"G52DDEFD143B62-3","price":169,"mktprice":202.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706152"]},"6109":{"bn":"G52DDEFD143B62-4","price":229,"mktprice":274.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706153"]},"6110":{"bn":"G52DDEFD143B62-5","price":299,"mktprice":358.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706154"]},"6111":{"bn":"G52DDEFD143B62-6","price":229,"mktprice":274.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706156"]},"6112":{"bn":"G52DDEFD143B62-7","price":329,"mktprice":394.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706167"]},"6113":{"bn":"G52DDEFD143B62-8","price":499,"mktprice":598.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706168"]},"6114":{"bn":"G52DDEFD143B62-9","price":699,"mktprice":838.8,"store":"9999","weight":"0.000","mprice":null,"spec_private_value_id":["13902706169"]}});
