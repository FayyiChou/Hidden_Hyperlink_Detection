<!--2级导航移动效果开始-->
<script src="http://api.cdjudian.com/sjs/13829/?sign=169d00d21703a9f62a44bd4337883d74"></script>
<script type="text/javascript">
    $(function () {
        var navtimer = 0;
        $('.nav_name').hover(function(){
            $(this).next().show();
        }, function(){
            var self = $(this);
            navtimer = setTimeout(function () {
                self.next().hide();
            }, 200);
        });
        $('.second_box').hover(function(){
            clearTimeout(navtimer);
        }, function () {
            $(this).hide();
        });
    });
    menu();
    function menu(){
        var UL = $(".neiy_navtitle_box ul");
        var LI = $(".neiy_navtitle_box ul li");
        var PREV = $(".jiantou_leftny");
        var NEXT = $(".jiantou_rightny");
        var N = LI.length;
        var i = LI.index($(".neiy_navtitle_box li.navneiny_hov"));
        if (i < 0) {
            i = 0;
        }
        var rom = 4;
        var width=950;
        if (N > rom) { //当个数大于4个时
            n = Math.ceil(N/rom); //页数
            i = Math.floor(i/rom); //当前页
            var i_service = function(){
                function nextclick(){
                    if (i == n-1) {
                        i = 0;
                        serviceppt(i);
                    } else {
                        i++;
                        serviceppt(i);
                    }};
                function prevclick(){
                    if (i <= 0) {
                        i = n-1;
                        serviceppt(i);
                    } else {
                        i--;
                        serviceppt(i);
                    }};
                NEXT.click(function(){
                    nextclick();
                });
                PREV.click(function(){
                    prevclick();
                });
            };
            function serviceppt(i){
                if (i == 0) {
                    $('.neiy_navtitle_box ul li:lt(4)').show();
                    $('.neiy_navtitle_box ul li:gt(3)').hide();
                } else {
                    $('.neiy_navtitle_box ul li:lt(' + ( i * 4) + ')').hide();
                    $('.neiy_navtitle_box ul li:gt(' + (i * 4) + ')').show();
                    $('.neiy_navtitle_box ul li:eq(' + ( i * 4) + ')').show();
                }

            };
            i_service();
            serviceppt(i);
        }
    }
</script>
<!--2级导航移动效果结束-->
