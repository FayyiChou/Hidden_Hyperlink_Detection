<script> 
	window.hasLogIn = false;
	window.ROOT = 'http://m.pclady.com.cn/'; 
	function login_(type){
		if(type == 1){
			$("#returnUrl").val('http://m.pclady.com.cn/uploadgoods/');
		} 
		PCDialog.open("登录美搭","iframe:http://m.pclady.com.cn/common/newPopBox.jsp","450","480");
	}
	
	function share(){
		if(!hasLogIn){
			PCDialog.open("登录美搭","iframe:http://m.pclady.com.cn/common/newPopBox.jsp","450","480"); 
		    return ;
		}else{
			window.location.href="http://m.pclady.com.cn/uploadgoods/";
		} 
	}
</script>  

