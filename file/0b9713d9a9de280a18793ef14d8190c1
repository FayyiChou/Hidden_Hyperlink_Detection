<script>
document.title = "铭鸿一族秋冬季黑色百褶裙韩版女半身百褶短裙a字裙通勤裙子A1162_PClady美搭_太平洋时尚网";
var isAmazon = "false";
var tocs = $('#slide01_control .tocs li'),
    pics = $('#slide01 .pics li');
     
window.tab0802 = new Slide({
    target: tocs,
    control: false,
    height:66,
    effect: 'slide',
    mrege:true,
	direction:'y'
});
window.tab08 = new Slide({
    target: pics,
    control: tocs,
    type: 'click',
    effect: 'fade',
	stay:4000,
    autoPlay:true,
	link:true,
    onchange: function(){
        var to = this.curPage && this.curPage - 1; //保持当前选项卡前有元素可点击
        tab0802.playTo(to);
        if (this.curPage == this.length - 1) {
        	changeA();
        } else {
        	removeA();
        }
    }
}); 
var taobaoFlag = '5';
//data-itemid="13520652522" data-rd="1"  removeAttr
var isTaobao = false;

function changeA() {
	//if (isAmazon == "true") changeAmazonA(); else changeTbA();
	changeAmazonA();
}
function removeA() {
	//if (isAmazon == "true") removeAmazonA(); else removeTbA();
	removeAmazonA();
}

function changeAmazonA() {
	$(".aNext").attr("href", 'http://item.taobao.com/item.htm?id=13520652522&spm=2014.12518207.0.0');
	$(".aNext").attr("onclick","");
	$(".aNext").attr("target","_blank");
}
function removeAmazonA() {
	$(".aNext").attr("href", 'javascript: void(0);');
	$(".aNext").attr("onclick","return nextPage();");
	$(".aNext").removeAttr("target");
}
function changeTbA() {
	$(".aNext").attr("data-itemid", '13520652522');
	$(".aNext").attr("data-rd", '1');
	$(".aNext").attr("target", '_blank');
	$(".aNext").attr("onclick","nextPage();");
	isTaobao = true;
}
function removeTbA() {
	var itemid = $(".aNext").attr("data-itemid");
	var rd = $(".aNext").attr("data-rd");
	var target = $(".aNext").attr("target");
	var aHref = $(".aNext").attr("href");
	if (itemid) {
		$(".aNext").removeAttr("data-itemid");
	}
	if (rd) {
		$(".aNext").removeAttr("data-rd");
	}
	if (target) {
		$(".aNext").removeAttr("target");
	}
	if (isTaobao) {
		$(".aNext").attr("href", "javascript: void(0);");
		$(".aNext").attr("onclick","return nextPage();");
	}
}
function nextPage(){
  if( tab08.curPage != tab08.length - 1 ){
		tab08.next();
  }
  return false;
}
function prevPage(){
  if( tab08.curPage != 0 ){
		tab08.prev();
  }
}
</script> 



