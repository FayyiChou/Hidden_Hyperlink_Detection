<script type="text/javascript" src="http://js.3conline.com/js/mustache.min.js"></script>
<script type="text/javascript" src="http://js.3conline.com/js/pclady/lady2012/md/waterfall.min.js"></script>
<script src="http://js.3conline.com/js/pclady/lady2012/md/popbox.min.js"></script> 
<script src="http://js.3conline.com/js/pclady/lady2013/mdNav.js"></script>
<script type="text/javascript">
	window.hasLogIn = false;
	window.ROOT = 'http://m.pclady.com.cn/';
	window.commentTimes = 5; //每一分钟评论次数，超过此变量，出现验证码	
	$(document).ready(function(){
		if('1'== '1'){
			setAboutGoods();
		}
		initCookie();
		try{
			initWf();
		}catch (e){}
	});
	function setAboutGoods(){
		var parent = $(".expPicA");
		var itemTmpl = $('#about').html();
		$.get('http://m.pclady.com.cn/goods/action/getAboutGoods.jsp', 
	    		{'goodsId':'191484',
	    		'refresh':''},
	    		function(data) {
	    			parent.append(Mustache.to_html(itemTmpl, data));
	    			
	            }, 'json');
	}
	
    // 测试用计数器
    var n = 0;    
    var itemTmpl = $('#tmpl1').html();
    var $main = $('#J-mdList');
    var curPage = '2';    
    var goodsId = 191484;
	var dressNames = '半身裙';
	var color = '0';
	var goodsTagIds = '150,126,18,75,68';
	var dressType = '9';
	var dressName = '半身裙';
	var tagIds = ''; 
	var colorId   = '312';	
	var wf = null;
	
	function initWf(){
		// 瀑布流
	     wf = new Waterfall({
	        container: $main,
	        colWidth: 300,
	        maxCol: 3,
	        minCol: 3,
	        preDistance: 50,
			gapWidth:45,
			gapHeight:45,
			resizeable:false,
	        load: function(){
	            // 触发滚动加载时的具体操作
	            // 当前作用域下，this指向正创建的对象
	            var self = this;
	            $.get('http://m.pclady.com.cn/action/getMaybeLikeGoods.jsp',{
	            	n: n,
	        		'dressType':dressType,
	        		'dressNames':encodeURI(dressNames),
	        		'colorId':colorId,
	        		'goodsId':goodsId,
	        		'tagIds':encodeURI(goodsTagIds),
	        		'pag' : '1',
	        		'pageSize':'12'		 ,
	        		'pageNo':curPage++, 
	        		'refresh':''}, function(data) {
	                var res = [];
	                if (data != "" && data != undefined && data.items != undefined) {
	                	$.each(data.items, function(i, item){
		                    res.push(Mustache.to_html(itemTmpl, item));
		                });
	                }
	                self.success(res);
	                self.end();     // 终止滚动load
	                $('#Jloading').hide();  
					$('#JPage').show(); 
	            }, 'json');
	        }
	    });
	}
    
// 显示边框
$main.find(".mdPic").live({
    mouseenter: function(){
        $(this).addClass("Hov");        
    },
    mouseleave: function(){
        $(this).removeClass("Hov");        
    }
})

//显示所有商品
$main.find(".pBrand a").live({
    mouseenter: function(){
        $(this).find("em").show();        
    },
    mouseleave: function(){
        $(this).find("em").hide();        
    }
})

//喜欢
var likeData = {};

$main.delegate('.aLike', 'mouseenter', function(e){    
    var $this = $(this),
        $lCount = $('.eNub', $this),
        $plus = $('.add' , $this);
	$plus.width($lCount.width());
    likeData.likeCount = $lCount.text();  
	$lCount.stop(true, true).animate({'margin-top': '-20px'});
  
});

$main.delegate('.aLike', 'mouseleave', function(e){
	$(this).find('.eNub').stop(true, true).animate({'margin-top': 0});
});


$("#J-dLike").hover(function(){
	$width=$(this).find(".eNub").width();
	$(this).find("em").css("width",$width);
	$(this).find(".eNub").slideUp();
	},function(){
	$(this).find(".eNub").slideDown();	
})	

function foucs(){
	$("#CMT .dComm .inpTxt").focus();
	}
	
$("#CMT .aAll").click(function(){
	$("#CMT .cmtAll").show();
	})
$("#CMT .aHide").click(function(){
	$("#CMT .cmtAll").hide();
	})
$("#J-pBtn").hover(function(){
	$(this).find(".eBuy").stop().animate({"margin-left":"0px"},500);
	},function(){
	$(this).find(".eBuy").stop().animate({"margin-left":"-200px"},500);})	

//返回顶部
var $goTop = $('#goTopButton'),
    deciScrollHeight = $(window).height();
$(window).scroll(function(){
    var $this = $(this);

    // show or hide the go-top-button
    if($this.scrollTop() > deciScrollHeight){
        if($.browser.msie&&($.browser.version == "6.0")){
            $goTop.css('top', $this.scrollTop() + $this.height() - 426);
        }
        if($goTop.css('display') == 'none'){
            $goTop.fadeIn();
        }
    } else {
        $goTop.fadeOut();
    }
});
$('#goTopButton').click(function(event){
    $('html, body').animate({scrollTop:0}, 300);
});


$main.delegate('.aLike', 'click', function(e){    
	var $this = $(this);
	var goodsId = $this.parent().find('#goodsId').val();
	like(goodsId , $(this));
});


/**
 * popPart:弹窗部分
 * obj:原对象this
 * hasLikeObj:已经喜欢的HTML对象
 */
function like(goodsId ,obj){
	if(!hasLogIn){
		PCDialog.open("登录美搭","iframe:http://m.pclady.com.cn/common/newPopBox.jsp","450","480"); 
		return;
	}
	$.getJSON("http://m.pclady.com.cn/action/likeGoods.jsp",{'goodsId':goodsId,'date':(new Date()).getTime()},function(data){  		
		if(data.success != null && data.success == 1){
			if(obj == null){
				// 取消弹窗
				
			}else{
				obj.replaceWith('<p class="pLiked" title="已喜欢">'+(obj.find(".eNub").html()-0+1)+'</p>');
			}
		}else{
			obj.replaceWith('<p class="pLiked" title="已喜欢">'+(obj.find(".eNub").html())+'</p>');
		}
	});
}

//发表评论
var postCmt = function(data){
	var goodsId = '191484';
	var captcha = $('#captcha').val();
	$.post(ROOT + 'action/postGoodsComments.jsp', 
		    {
			  goodsId: goodsId,
		      respId: 0,    // -1 表示非回复
		      respCmtId: 0,      // -1 表示非回复
		      content: encodeURI(data.content),
		      captcha:captcha},
		    function(response){  		    	
		      if(!response.success){  		    	  
		    	  if(response.captcahStatus != null && response.captcahStatus > 0){
	    		  $("#captchaTip").show();
	    	  }	  		    	
		      }
		      
		  }, 'json'); 
};

$("#postComment").click(function(){
	var $commentInfo = $("#commentText");
	checkComment({'content':$commentInfo.val()});
});

function checkComment(data){
	var len = $.trim(data.content).length;
	if(!hasLogIn){
		PCDialog.open("登录美搭","iframe:http://m.pclady.com.cn/common/newPopBox.jsp","450","480"); 
		return;
	}else if(len <= 0 || data.content == '说两句吧'){    // 没有评论时        
        new popBox({
          	boxClass: 'popBox error',
            content: '请输入评论',
            closeDelay: 2000
        });
		return;
    } else if (len > 100){ // 字数超过时
    	new popBox({
          	boxClass: 'popBox error',
            content: '评论不要太长，已超过100字了',
            closeDelay: 2000
        });
    	return;
    } else {
	   	var times = getInputTimes();
	   	if(times < commentTimes){
	   		new popBox({
	          	boxClass: 'popBox success',
	            content: '发布成功',
	            closeDelay: 2000
	        });
	   		setInputTimes(times - 0 + 1);
	   		postCmt(data);	   		
	   		$("#commentText").val('');
		    //setCommentNum();
	   	}else{
	   		var captcha = $('#captcha').val();
	   		if(captcha == ""){
	   			new popBox({
		          	boxClass: 'popBox error',
		            content: '请输入验证码',
		            closeDelay: 2000
		        });
	   			return;
	   		}else{
	   			new popBox({
		          	boxClass: 'popBox success',
		            content: '发布成功',
		            closeDelay: 2000
		        });
	   			postCmt(data);   			
	   			changeCaptcha();
	   			$("#commentText").val('');
			   // setCommentNum();
	   		}
	   	}
    }
}

var date;
//设置cookie
function setInputTimes(num){
	$.cookie("meiDaGoodsCommentCount" , num, {expires:date,path:"/"});
	if(num >= commentTimes){
		$(".pBtn-a").show();
	}
}
//设置cookie
function getInputTimes(){
	var tempCount = 0;
	tempCount = $.cookie("meiDaGoodsCommentCount");	
	return tempCount;
}
function initCookie(){	
	tempCount = $.cookie("meiDaGoodsCommentCount");	
	if(tempCount == null){
		date = new Date();
		date.setTime(date.getTime() + (60 * 1000));
		$.cookie("meiDaGoodsCommentCount" , 0 , {expires:date,path:"/"});
	}
	if(tempCount >= commentTimes){
		$(".pBtn-a").show();
	}
}
function changeCaptcha(){
	document.getElementById("valid_code").src = "http://captcha.pclady.com.cn/captcha/v.jpg?"+new Date().getTime();
}
</script>

