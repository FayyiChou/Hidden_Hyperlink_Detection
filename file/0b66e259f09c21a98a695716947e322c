    <script>
    function scroll(p) {
        var d = document, w = window, o = d.getElementById(p.id), ie6 = /msie 6/i.test(navigator.userAgent);
        if (o) {
            o.style.cssText += ";position:" + (p.f && !ie6 ? 'fixed' : 'absolute') + ";" + (p.r ? 'left' : "right") + ":0;" + (p.t != undefined ? 'top:' + p.t + 'px' : 'top:0');
            if (!p.f || ie6) {
                -function () {
                    var t = 500, st = d.documentElement.scrollTop || d.body.scrollTop, c;
                    c = st - o.offsetTop + (p.t != undefined ? p.t : (w.innerHeight || d.documentElement.clientHeight) - o.offsetHeight);//如果你是html 4.01请改成d.body,这里不处理以减少代码
                    c != 0 && (o.style.top = o.offsetTop + Math.ceil(Math.abs(c) / 10) * (c < 0 ? -1 : 1) + 'px', t = 10);
                    setTimeout(arguments.callee, t)
                }()
            }
        }
    }
    scroll({
        id: 'leftsead'
    })
    </script>

    <script type="text/javascript">
        $(document).ready(function () {
            $("#leftsead a").hover(function () {
                if ($(this).prop("className") == "youhui") {
                    $(this).children("img.hides").fadeIn(300);
                } else {
                    $(this).children("img.hides").fadeIn(300);
                    $(this).children("img.shows").fadeOut(300);
                    $(this).children("img.hides").animate({ marginRight: '0px' }, '300');
                }
            }, function () {
                if ($(this).prop("className") == "youhui") {
                    $(this).children("img.hides").fadeOut(300);
                } else {
                    $(this).children("img.hides").animate({ marginRight: '-143px' }, '300', function () { $(this).fadeOut(300); $(this).next("img.shows").fadeIn(300); });
                }
            });

            $("#top_btn").click(function () { if (scroll == "off") return; $("html,body").animate({ scrollTop: 0 }, 600); });

        });
    </script>
