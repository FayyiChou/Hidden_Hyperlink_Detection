        $(window).load(function(){
            var filter = [jr.ImageUtil.getListFilter('thumbnailUrl', 'U380'), jr.Scroller.FILTER.getViewUrlFilter('/s/{{= serviceId}}/view?sort=')];
            

            var scroller =  new jr.Scroller('_vod_list_2', {
                listType : jr.Scroller.prototype.LISTTYPE_HORIZONTAL,
                autoLoad : true,
                offsetEnd : 0,
                target : 'div.thumb_list__listbox_wrapper',
                pageSize : 10,
                url : "/tag/auto_song_popularity?offset={{= offset}}&amp;size={{= pageSize}}&amp;serviceNo=001",
                filter : filter,
                template : jr.Scroller.TEMPLATES.GET_MOBILE_LIST_DEFAULT()

            });

            
                scroller.load(true);
            

            $('._PLAY_ALL').on('click', function(){
                var elSection = $("#_vod_section_2");
                var elFirstItem = $(elSection.find(".thumb_list__listitem")[0]);
                if (typeof elFirstItem  !="undefined") {
                    var contentsNo = $(elSection.find(".thumb_list__listitem")[0]).attr("data-contentsno");
                    elSection.find('._PLAY_ALL').attr("href", " /t/auto_song_popularity/view?sort=&contentsNo=" + contentsNo + "&continuousPlay=true");
                }
            });
        });
    </script>
