<script type="text/javascript">
	$(function() {
		var elFlick = $('div.snb_flick');

		var setScroll = function() {
			if (window.localStorage) {
				window.localStorage.setItem("JR_MOBILE_SNB_REFERRER", location.href);
				window.localStorage.setItem("JR_MOBILE_SNB_SCROLL", elFlick.scrollLeft());
			}
		};

		var loadScroll = function() {
			if (!window.localStorage) {
				return;
			}

			var referrer = window.localStorage.getItem("JR_MOBILE_SNB_REFERRER");
			if (!referrer || !document.referrer || (document.referrer != referrer)) {
				window.localStorage.removeItem("JR_MOBILE_SNB_REFERRER");
				window.localStorage.removeItem("JR_MOBILE_SNB_SCROLL");
//				return;
			}

			var scroll = window.localStorage.getItem("JR_MOBILE_SNB_SCROLL");

			if (!scroll) {
				var elSelected = $('a.snb__link.is__selected');
				if (elSelected && elSelected.length > 0) {
//					scroll = (elSelected.offset().left > $(window).width()) ? elSelected.offset().left - ($(window).width() - elSelected.width()) / 2 : 0; //중앙
					scroll = (elSelected.offset().left > $(window).width()) ? elSelected.offset().left - $(window).width() + elSelected.width() : 0; //우측끝
				}
			}

			if (!scroll) {
				return;
			}

			elFlick.scrollLeft(scroll);
		}

		$('ul.snb__listbox li a.snb__link').on('click', function() {
			setScroll();
		});

		loadScroll();

		var menuNc = new jr.Nclick('song', 'menu');
		menuNc.delegate('snb', 'a.snb__link', 'songmenu', true);
	})
</script>
