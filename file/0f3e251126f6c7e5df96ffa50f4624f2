  <script type="text/javascript">
	//页面需要显示unicode编码的字符，可以后台或前端进行转换,数据库保存的是utf8mb4编码,读出来后，需要转换一次
	/*$('.name').emoji();*/

	$(document).on('click','.watozcityname >.atoz > span',function(){
		var array = [];
		$(this).siblings('span').each(function(index, el) {
			$(this).removeClass('active');
		});
		$(this).addClass('active');
		
		array = $(this).attr('data-letter').split(",");
		$('.atozA').siblings('.atozA').each(function(index, el) {
			$(this).css({display:'none'});
		});
		for(var key in array){
			$('#s'+array[key]+'').css({display: 'block'});
		}
	});

	$(document).on('click','.ico',function(event){
		if($('.destination-select-itemss').css('display') == 'block'){
			return;
		}else{
			$('.destination-select-itemss').css({display: 'block'});
		}		
	});

	$(document).on('click',function(e){
			
		if(!$(e.target).closest('.dingwei').hasClass('dingwei')){
			$('.destination-select-itemss').css({display:'none'});
		}
			
	});

	$(document).on('click','.chck span',function(){
		$('.cselect').text($(this).text());
		setTimeout(function(){
			$('.destination-select-itemss').css({display:'none'});
		},100)
	});


</script> 
