      <SCRIPT>
var speed=25
friend2.innerHTML=friend1.innerHTML
function Marquee(){
if(friend2.offsetWidth-friend.scrollLeft<=0)
friend.scrollLeft-=friend1.offsetWidth
else{friend.scrollLeft++}
}
var MyMar=setInterval(Marquee,speed)
friend.onmouseover=function() {clearInterval(MyMar)}
friend.onmouseout=function() {MyMar=setInterval(Marquee,speed)}
