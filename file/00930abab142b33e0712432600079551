var versTab={
scrollwheel:function(slide,page){
if(!versTab.oUl) return false;
document.querySelector('.m-tA-box').scrollLeft=0;
document.querySelector('.m-timeAxis .m-tA-th a:first-child').className=document.querySelector('.m-timeAxis .m-tA-th a:last-child').className='';
switch(slide.getAttribute('data-type')) {
case 'baby':
document.querySelector('.m-timeAxis .m-tA-th a:last-child ').className="current";
break;
case 'yunma':
document.querySelector('.m-timeAxis .m-tA-th a:first-child').className="current";
break;
}
if(page<4){
versTab.oUl.style.webkitTransform='translateX(0px)';
}else if(page>=4){
if(page==versTab.length-1){
versTab.oUl.style.webkitTransform='translateX('+(versTab.ulWid*-1+versTab.oBoxWid)+'px)';
}else{
versTab.oUl.style.webkitTransform='translateX('+(versTab.ileft*(page-3)*-1)+'px)';
}
}
},
timeAxisClick:function(){
var lis=document.querySelectorAll(".m-tA-s-wrap li"),iliLen=lis.length,iwid=lis[0].offsetWidth,ileft=lis[1].offsetLeft,oUl=document.querySelector('.m-tA-s-wrap'),oBoxWid=oUl.parentNode.offsetWidth;
var ulWid=ileft*iliLen;
versTab.iliwidth=iwid;/*li 的宽度*/
versTab.ulWid=ulWid;/*ul 宽度*/
versTab.length=iliLen;
versTab.ileft=ileft;
versTab.oUl=oUl;
versTab.oBoxWid=oBoxWid;
document.querySelector('#wrapper .m-tA-s-wrap').style.width=ulWid+'px';
},
tab_touchmove:function(e){
e.preventDefault();
var nMoveX=e.touches[0].clientX;
versTab.move=versTab.tranX+(parseFloat(nMoveX)-versTab.oMoveX);
// console.log(versTab.tranX+'___'+nMoveX+'_'+e.touches[0].clientY);
versTab.boxWidth=document.querySelector(".m-tA-box").offsetWidth;
versTab.oMoveX=nMoveX;
},
animatieFun:function(){
var maxright=-versTab.ulWid+versTab.boxWidth;
if(versTab.tranX>=0){
versTab.oUl.style.webkitTransform='translateX(0)';
if(!versTab.ifEnd){
versTab.ifEnd=0;
}
}else if(versTab.tranX<=-(versTab.ulWid-versTab.boxWidth)){
versTab.oUl.style.webkitTransform='translateX('+maxright+'px)';
if(!versTab.ifEnd){
versTab.ifEnd=maxright;
}
}else{
versTab.ifEnd=null;
versTab.oUl.style.webkitTransform='translate3d('+versTab.move+'px,0,0)';
}
if((versTab.ifEnd==0||versTab.ifEnd==maxright)&&(versTab.move>0||versTab.move<maxright)){
versTab.move=versTab.ifEnd;
}
versTab.tranX=versTab.move;
versTab.ifMove=false;
},
get_translate:function(str){
var start=str.indexOf("(");
var end=str.indexOf(")");
return parseFloat(str.substring(start+1,end));
},
subNav:function(){/* 导航 */
