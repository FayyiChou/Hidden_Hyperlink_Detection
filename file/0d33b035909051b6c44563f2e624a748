function regStrLength(_sVal){
	var iLen;
	var sVal	= escape(_sVal);
	iLen		= sVal.length - (sVal.length - sVal.replace(/\%u/g,"u").length) * 4;
	sVal		= sVal.replace(/\%u/g,"uu");
	iLen		= iLen - (sVal.length - sVal.replace(/\%/g,"").length) * 2;
	return iLen;
}
	function CheckLogin()
	{
		has_sites="163.com,yahoo.com.cn,tom.com,126.com,sina.com,sohu.com,163.net,263.net,vip.163.com,vip.sina.com,mail.china.com,china.com,netease.com,21cn.com,xinhuanet.com,eyou.com,email.com.cn,ynmail.com,citiz.net,hotmail.com,56.com,gmail.com,yahoo.cn,yahoo.com";
		e_mail=document.formlogin.UserLogin160_UserID.value;
		password=document.formlogin.UserLogin160_Password.value;
		if(regStrLength(e_mail)<1){
			alert("请输入电子邮箱!"); document.formlogin.UserLogin160_UserID.focus();
			return false;
		}
		if(!(/^[a-z0-9_@\.-]+@([a-z0-9_-]+\.)+[a-z0-9_-]{2,3}$/).test(e_mail)){
			alert("请输入正确的电子邮箱!"); document.formlogin.UserLogin160_UserID.focus();
			return false;
		}
		if(regStrLength(password)<1||regStrLength(password)>16){
			alert("请输入邮箱密码！");  document.formlogin.UserLogin160_Password.focus();
			return false;
		}
		temp=e_mail.split("@");
		if(temp.length!=2){
			alert("请输入正确的电子邮箱!"); document.formlogin.UserLogin160_UserID.focus(); return false;
		}else{
			siteval=temp[1];
			has_site=has_sites.split(",");
			flag=true;
			for(i=0;i<has_site.length;i++){if(siteval==has_site[i]){flag=false;break;}}
			if(flag){alert("不支持此电子邮箱!"); document.formlogin.UserLogin160_UserID.focus(); return false;}
			if(temp[1]=="yahoo.com.cn"||temp[1]=="yahoo.cn"||temp[1]=="yahoo.com") siteval="cn.yahoo.com";
			<!--document.formlogin.user.value=temp[0];-->
			<!--document.formlogin.site.value=siteval;-->
			<!--document.formlogin.pass.value=password;-->
		}
		document.formlogin.UserLogin160_UserID.value="";
		document.formlogin.UserLogin160_Password.value="";
		return true;
	}
