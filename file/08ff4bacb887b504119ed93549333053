<script language="javascript">
ChangeStyle(1);  //改变第一个<li>的样式，别的页面只要把Nav1改成相对应的ID即可
function $(ID){return typeof(ID) == "[object]" ? ID : document.getElementById(ID);}
function ChangeStyle(Code)
{
	var NavLength = 11; //菜单的长度
	var I = 0;
	var Code = parseInt(Code);
	for(I=1;I<=NavLength;I++)
	{
		if(I == Code)
		{
			$("menu"+I.toString()).className = "TitleMoseOver";
			$("TitleInfo").innerHTML = $("ListInfo"+I.toString()).innerHTML;
		}else{
			$("menu"+I.toString()).className = "TitleMoseOut";
		}
	}
}
</script>
