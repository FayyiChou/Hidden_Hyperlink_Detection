<script type="text/javascript">
//提交表单
function formSubmit(){
	var header_vcareas=$("#jieguo_header_vcareasid").val();
	var indexName1=document.getElementById('indexName').value;
	var form=document.getElementById("indexSearchFormId");
	if(indexName1==2 || indexName1==1){
		form.action="/yzww/"+indexName1;
		form.submit();
	}else{
		if(header_vcareas){
			document.getElementById('jieguo_header_vcareasname').value = header_vcareas;
		}
		form.action="/sww/";
		form.submit();
	}
}
//传栏目
function onlyItem(indexName){
	document.getElementById('indexName').value = indexName;
	formSubmit();
}
//传日期
function onlyTime(sTimeSlot){
	document.getElementById('sTimeSlot').value = sTimeSlot;
	formSubmit();
}
//传关键字位置
function onlyItemPosition(a){
	document.getElementById('resultItemPosition').value = a;
	formSubmit();
}
//传城市
function onlyCity(id,name){
	document.getElementById('sAreaid').value = id;
	document.getElementById('sArea').value = name;
	$("#jieguo_header_vcareasid").val('');
	formSubmit();
}
//正选反选sVoc_ww_
function selectAllVoc(obj){
	if(obj.checked){
		$("input[name^='sVoc_ww_']").attr("checked", true);
	}else{
		$("input[name^='sVoc_ww_']").attr("checked", false);
	}
}
//提交关键字和行业
function surefun(){
	var key=$("#inKeywordId").val();
	document.getElementById('keyword').value = key;
	var voc="";
	var allvoc=document.getElementById('selectAllVocId');
	if(allvoc.checked){
	}else{
		$('input[name^="sVoc_ww_"]:checked').each(function(){
			voc += ","+$(this).val();
		});
		voc = voc != "" ?voc.substring(1):voc;
	}
	document.getElementById('sVocid').value = voc;
	formSubmit();
}
//翻页
function jumpPage(page){
	document.getElementById('ss_page').value = page;
	formSubmit();
}
function zhuandao(a){
	var page=$("#zhuandao"+a).val();
	if(page == ''){
		alert('请输入数字');
		return;
	}else{
		if(isNaN(page)){
			alert('请输入数字');
			return;
		}else{
			jumpPage(page);
		}
	}
}
//验证关键字
function yanzhengKey(){
	var key=$("#inKeywordId").val();
	if(key=="请输入关键字"){
		$("#inKeywordId").val("");
	}
}
</script>

