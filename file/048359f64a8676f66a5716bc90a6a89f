<script language="JavaScript">
// 申请成为供货商
function ask_supp()
{
	jQuery.get('supp.php', {act:'ask_supp'}, ask_sup_response, 'json');
}
function ask_sup_response(result)
{
	if (result.error > 0)
	{
		// SESSION 过期
		if (result.error == 2)
		{
			alert(result.message);
			location.href = 'user.php';
		}
		else
		{
			alert(result.message);
		}
	}
	else
	{
		alert(result.message);
		location.href = '/sell';
	}
}
</script>
                <a href="javascript:ask_supp();">卖家入驻</a>
