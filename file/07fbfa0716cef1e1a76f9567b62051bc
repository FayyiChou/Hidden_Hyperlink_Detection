function hidezd(url) {
	if($('pDiv').style.display == 'none') {
		$('zdf').style.display = '';
		$('zdf').src = url;
		$('pDiv').style.display = '';
		$('zdBG').style.display = '';
		if(document.body.clientWidth<650) $('pDiv').style.left = 0;
		else $('pDiv').style.left = (document.body.clientWidth-610)/2 + 'px';
		if(document.body.clientHeight<200) $('pDiv').style.top = 0;
		document.body.style.overflow = 'hidden';
	} else {
		$('zdf').src = '';
		$('zdf').style.display = 'none';
		$('pDiv').style.display = 'none';
		$('zdBG').style.display = 'none';
		document.body.style.overflow = 'auto';
	}
}
