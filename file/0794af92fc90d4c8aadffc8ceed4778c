<script type="text/javascript">
$("input[name=name]").focus(function(){if($(this).val()=="姓名"){$(this).val("")}});$("input[name=name]").blur(function(){if($(this).val()==""){$(this).val("姓名")
}});$("input[name=email]").focus(function(){if($(this).val()=="E-mail"){$(this).val("")}});$("input[name=email]").blur(function(){if($(this).val()==""){$(this).val("E-mail")
}});$("input[name=phone]").focus(function(){if($(this).val()=="电话"){$(this).val("")}});$("input[name=phone]").blur(function(){if($(this).val()==""){$(this).val("电话")
}});$("textarea[name=content]").focus(function(){if($(this).val()=="留言"){$(this).val("")}});$("textarea[name=content]").blur(function(){if($(this).val()==""){$(this).val("留言")
}});
$("input[name=btn]").click(function(){var name=$("input[name=name]").val();var phone=$("input[name=phone]").val();if(name.length<1||name=="姓名"){alert("请填写联系姓名");
return false}else{if(phone.length<1||phone=="电话"){alert("请填写联系电话");return false}}$("#cont > form").submit()});
</script>

