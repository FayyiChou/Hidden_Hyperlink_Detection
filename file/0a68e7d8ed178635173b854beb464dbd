            }
            else if (document.searchBlock.c.value == "04") {
                window.ac.setURL("http://www.langlang.cc/sbtipword.aspx?type=mv&");
            }
            else {
                window.ac.setURL("http://www.langlang.cc/sbtipword.aspx?type=product&");
            }
        }
    </script>
</body>
</html>
