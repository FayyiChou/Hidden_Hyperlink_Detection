function cookiePermanent(){
	CookieUtil.setCookie({
		name: "cozytopshow",
		value: 'no',
		expires: 1000*60*60*24,
		path: "/",
		domain: CookieUtil.getDomain()
	});
	$("#cozytop").css({"display":"none"});
}
function toploginsubmit(){
	if(window.user!=null && window.user!=undefined){
		return false;
	}else{
		var forwardUrls='';
		if(forwardUrls.indexOf("author.php")==-1){
			ltype='normal';
		}
		else {
			ltype='author';
		}
		var topusername = $("#topusername");
		var toppassword = $("#toppassword");
		var topimgcode = $("#topimgcode");
		if(topusername.val()==""){
			alert("请输入用户名");
			return false;
		}
		if(toppassword.val()==""){
			alert("请输入密码");
			return false;
		}
		if(topimgcode.val()=="" || topimgcode.val()=="点此显示"){
			alert("请输入验证码");
			return false;
		}
		 //$("#toplogsubmit").attr('disabled',true);
		 //var queryStrings=$("#toploginform").serialize();
		 loginform.submit();
	}
	return false;
	
}
