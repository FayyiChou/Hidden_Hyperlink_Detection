function startmarquee(){ 
var t; 
var p=false; 
var lh =19;//每次滚动高度
var  speed = 30;
var o=document.getElementById("nav3R"); //容器id
var delay ;
o.innerHTML+=o.innerHTML; 
o.onmouseover=function(){p=true} 
o.onmouseout=function(){p=false} 
o.scrollTop = 0; 
var i=0;
var di=-1;//滚动总数-1
	function start(){ 
		
		t=setInterval(scrolling,speed); 
		if(!p){ 
			o.scrollTop += 1;
		}
	} 

	function scrolling(){
		//alert(o.scrollTop%lh);
		if(o.scrollTop%lh!=0){ 
				o.scrollTop += 1; 
				if(o.scrollTop>=o.scrollHeight/2) {
					o.scrollTop = 0; 
			 	}
			}else{
				
				clearInterval(t); 
				i++;
				
				if(parseInt(i)>parseInt(di)){
					i=0;	
				}
				
				delay = document.getElementById("ff"+i).value; 
				
				setTimeout(start,delay); 
			} 
	} 
	setTimeout(start,document.getElementById("ff0").value); 
}

