jQuery(document).ready(function(){
	jQuery(".lhbtn1").mouseover(function(){
		$("#fm2-div-1").show();
		$("#fm2-div-2").hide();
		$(".lhbtn .lhbtn1").css({"background-image":"url(http://img.yc.ireader.com.cn/img/lhbtn.png)"});
	});
	jQuery(".lhbtn2").mouseover(function(){
		$("#fm2-div-1").hide();
		$("#fm2-div-2").show();
		$(".lhbtn_2 .lhbtn1").css({"background-image":"url(http://img.yc.ireader.com.cn/img/lhbtn2.png)"});
	});
});
function rotation(){
	var style = $("#fm2-div-1").css("display")
	if(style=="block"){
		$("#fm2-div-1").hide();
		$("#fm2-div-2").show();
		$(".lhbtn_2 .lhbtn1").css({"background-image":"url(http://img.yc.ireader.com.cn/img/lhbtn2.png)"});
	}else{
		$("#fm2-div-1").show();
		$("#fm2-div-2").hide();
		$(".lhbtn .lhbtn1").css({"background-image":"url(http://img.yc.ireader.com.cn/img/lhbtn.png)"});
	}
}
setInterval('rotation()',8000);
</script>
