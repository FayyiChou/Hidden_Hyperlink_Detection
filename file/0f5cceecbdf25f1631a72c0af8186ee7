<script src="js/jquery-1.9.1.min.js"></script><!--/ JQ库-->
<script src="js/jquery.SuperSlide.2.1.1.js"></script>
<script type="text/javascript">
jQuery(".slideIndex").slide({mainCell:".bd ul",autoPlay:true,interTime:5000});

$('.nav_er ul li').hover(function(){
	var index=$(this).index();
	$(this).parents('.container1').find('.zhanshi').eq(index).stop(true,true).fadeIn();
});
$('.nav_er ul li').mouseleave(function(){
    $(this).find('.zhanshi').stop(true,true).hide() ;
});
$('.zhanshi').mouseleave(function(){
    $(this).stop(true,true).hide();
});

$('.nav_er ul li:last').css('width','250');

$(".wenda ul li.exam_li").hover(function(){
	$(this).find(".exam_main").stop().fadeToggle();
	$(this).find(".exam_title").toggleClass("hide");
	$(this).siblings(".exam_li").find(".exam_main").hide();
});

$(function(){
    //航班起始到达选择
    $('.balance_nav ul li').click(function(){
        var index=$(this).index();
        $(this).addClass('on').siblings().removeClass('on');
        
        if(index==0){
            $('.balance_from').show().siblings('.balance_to').hide();
            $('.balance_city').text('到达城市');
            $('.city_select p').removeClass('to').addClass('from');
        }
        else{
            $('.balance_to').show().siblings('.balance_from').hide();
            $('.balance_city').text('出发城市');   
            $('.city_select p').removeClass('from').addClass('to');
        }
    });
    
    //城市下拉选择、航空公司下拉选择
    $('body').on('click','.city_select p,.company_select p',function(){
        $(this).siblings('ul').slideDown(200);
    });
    
    $('body').on('mouseleave','.city_select ul,.company_select ul',function(){
        $(this).slideUp(200);
    });     

    $('body').on('click','.company_select li',function(){
        var text=$(this).text();
		
        $(this).parent().slideUp(200).siblings('p').text(text);
		//var en_text=encodeURIComponent(text);
		//var en_text = urlencode()
        $('.balance_number .fly_com').val(text);
    }); 
    
    $('body').on('click','.city_select li',function(){
        var text=$(this).text();

        $(this).parent().slideUp(200).siblings('p').text(text);
		//var en_text=encodeURIComponent(text);
		//var city=encodeURIComponent("萧山");
        if($(this).parent().siblings('p').hasClass('to')){
			//$('.balance_to .to_city').val(city);
            $('.balance_to .from_city').val(text);  
        }
        else{
            $('.balance_from .to_city').val(text);
			//$('.balance_from .from_city').val(city);
        }
    });
});


//调取xml新闻
if (window.XMLHttpRequest)
  {// code for IE7+, Firefox, Chrome, Opera, Safari
  	xmlhttp=new XMLHttpRequest();
  }
else
  {// code for IE6, IE5
  	xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
  }

xmlhttp.open("GET","DataSource.xml",false);
xmlhttp.send();
xmlDoc=xmlhttp.responseXML; 

var NewsTitle =new Array();
var newsId = new Array();
var NoticeTitle =new Array();
var NoticeId = new Array();

// 获取数条数 （空港新闻）
$(xmlDoc).find("NewsLine").each(function(i)
{   
    // 获取内容
    NewsTitle[i]=$(this).children("NewsTitle").text();
    newsId[i]= $(this).children("NewsId").text();
    // 内容显示
});
$(function(){
   var path="http://www.hzairport.com/news_info1.aspx?nid=";
   var path1=path+newsId[0];
   var path2=path+newsId[1];
   
   $("#test_1").text(NewsTitle[0]); 
   $("#test_1").parent('a').attr('href',path1);
   
   $("#test_2").text(NewsTitle[1]); 
   $("#test_2").parent('a').attr('href',path2);
});


//获取数据条数（底部新闻）
$(xmlDoc).find("NoticeLine").each(function(i)
{   
    // 获取内容
    NoticeTitle[i]=$(this).children("NoticeTitle").text();
    NoticeId[i]= $(this).children("NoticeId").text();
    // 内容显示
});
$(function(){
   var path="http://www.hzairport.com/gg_info.aspx?gid=";
   var path1=path+NoticeId[0];
   var path2=path+NoticeId[1];
   var path3=path+NoticeId[2];
   
   $("#test").text(NoticeTitle[0]); 
   $("#test").parent('a').attr('href',path1);
   
   $("#test2").text(NoticeTitle[1]); 
   $("#test2").parent('a').attr('href',path2);
   
   $("#test3").text(NoticeTitle[2]); 
   $("#test3").parent('a').attr('href',path3);
});
</script>
</body>
