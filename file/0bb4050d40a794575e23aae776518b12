<script language="javascript">
  
  function ShowLeft()
  {    　
  　var isIE='NotIE';
		if (window.XMLHttpRequest) 
		{ //Mozilla, Safari,...IE7 			
			if(window.ActiveXObject)
			{
			isIE='IE'; 			 
			}
		} 
		else 
		{
			isIE='IE'; 			
		}		
		if(isIE=='IE')
		{
			if (document.readyState=="complete")
			{
				order('1,15,14,6,2,9,7,19,8,12,3,11,16,5,21,13,10,17,18,20,4');
			} 
			else 
			{
				document.onreadystatechange=function(){ if(document.readyState=="complete")order('1,15,14,6,2,9,7,19,8,12,3,11,16,5,21,13,10,17,18,20,4');}
			}
		}
		else
		{
			order('1,15,14,6,2,9,7,19,8,12,3,11,16,5,21,13,10,17,18,20,4');
		}
  } 

</script>
