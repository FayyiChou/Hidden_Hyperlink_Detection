      function include_js(file) {
          var _doc = document.getElementsByTagName('head')[0];
          var js = document.createElement('script');
          js.setAttribute('type', 'text/javascript');
          js.setAttribute('src', file);
          _doc.appendChild(js);
          var options = {
              sysId: CONFIG.SYSIDS.UC,             //系统id
              loginStatusUrl: CONFIG.NAV_PARAM.loginStatusUrl,    //登录态验证接口
              loginUrl: CONFIG.NAV_PARAM.loginUrl,           // 登录接口
              loginRedirectUrl: CONFIG.NAV_PARAM.loginRedirectUrl, // 登录后跳转地址
              registerUrl: CONFIG.NAV_PARAM.registerUrl,   // 注册页面地址
              registerRedirectUrl: CONFIG.NAV_PARAM.registerRedirectUrl, // 注册后跳转地址
              logoutUrl: CONFIG.NAV_PARAM.logoutUrl,           // 登出接口
              logoutRedirectUrl: CONFIG.NAV_PARAM.logoutRedirectUrl    //登出后跳转地址
          };
          if (!/*@cc_on!@*/0) { //if not IE
              js.onload = function () {
                  topBarConfig.exec(options)
              }
          } else {
              js.onreadystatechange = function () {
                  if (js.readyState == 'loaded' || js.readyState == 'complete') {
                      topBarConfig.exec(options)
                  }
              }
          }
          return false;
      }
      if (window == top) {
          include_js(CONFIG.NAV_JS_FILE);
      }
      function inject_js(file) {
          if (file) {
              var _doc = document.getElementsByTagName('head')[0];
              var js = document.createElement('script');
              js.setAttribute('type', 'text/javascript');
              js.setAttribute('src', file);
              _doc.appendChild(js);
          }
      }
      inject_js(CONFIG.ANALYTICS_FILE);
      inject_js(CONFIG.DCS_MD5_FILE);
      inject_js(CONFIG.DCS_UPDCS_FILE);
      
    </script>
  
