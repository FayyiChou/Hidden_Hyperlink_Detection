<script type="text/html" id="message_module">
	<div class="message" style="width:600px;">
		<form action="result.php" name="" method="post" id="msgform">
			<table width="100%" border="0" cellspacing="0" cellpadding="0" class="msg_tab">
				<tbody>
					<tr>
						<td><p>您的姓名<span class="needtip">*</span></p><input type="text" name="realname" value="" id="realname" class="inputext"></td>
						<td><p>您的邮箱<span class="needtip">*</span></p><input type="text" name="email" value="" id="email" class="inputext"></td>
					</tr>
					<tr>
						<td><p>公司名称</p><input type="text" name="company" value="" class="inputext"></td>
						<td><p>电话号码<span class="needtip">*</span></p><input type="text" name="tel" value="" class="inputext" id="tel"></td>
					</tr>
					<tr>
						<td colspan="2"><p>留言内容<span class="needtip">*</span></p><textarea name="content" placeholder="如果您有任何疑问或建议，请填写留言反馈给我们" class="inputtextarea" id="msg"></textarea></td>
					</tr>
					<tr>
						<td colspan="2"><p>验证码</p><input type="text" name="code" value="" class="inputext" id="code" style="float:left;"><img src='../captcha' onclick="this.src='../captcha?'+Math.random()" style="float:left;position:relative;margin-top:10px;margin-left:-84px;cursor:pointer;" /><a href="javascript:void(0)" id="reset_code" style="float:left;line-height:52px;margin-left:12px;">看不清，换一张</a><div style="clear:both"></div></td>
					</tr>
					<tr>
						<td colspan="2"><input type="submit" name="" value="提交留言" class="inputsubmit" id="submit_msg"></td>
					</tr>
				</tbody>
			</table>
		</form>
	</div>
</script>

<script type="text/javascript">
	$(function(){
		
		$(document).on('click','#reset_code',function(){
			$(this).siblings('img').attr('src','../captcha?'+Math.random());
		});
			
		$(document).on('click','#msgforus,#messages',function(){			
			$.poper({
				type:'poper', //窗口类型 [ poper:默认窗体类型 || confirmer:带按钮的窗口类型 ]
				frame:null, //整个弹出层
				wrap:null, //窗口主体
				title:function(_title){  //窗口标题
					_title.text('在线留言')
				},
				contect:function(_contect){ //窗口内容主题
					_contect.append($('#message_module').html());
					
					$('#reset_code').siblings('img').attr('src','../captcha?'+Math.random());
					
					$('#submit_msg').on('click',function(){
						
						if($('#realname').val()=='' || $('#realname').val().length<2){
							alert('请填写您的姓名');
							$('#realname').focus();
							return false;
						}
						if($('#tel').val()==''){
							alert('请填写您的电话');
							$('#tel').focus();
							return false;
						}
						if(!/^0\d{2,3}-?\d{7,8}$/.test($('#tel').val())){
							alert('电话号码不正确');
							$('#tel').focus();
							return false;
						}						
						if($('#email').val()=='' || $('#email').val().indexOf('@')<0){
							alert('邮件格式不对');
							$('#email').focus();
							return false;
						}
						if($('#msg').val()=='' || $('#msg').val().length<6){
							alert('留言内容太少，至少6个字符');
							$('#msg').focus();
							return false;
						}
						
						if($('#msg').val().length>=234){
							alert('留言内容超过限制字符，请删减后再提交');
							$('#msg').focus();
							return false;
						}
						if($('#code').val()==""){
							alert('请输入验证码');
							$('#code').focus();
							return false;
						}
						
						return true;
					})
				}, 
				closefn:null, //默认动作:关闭并移除当前窗口
				save:null, //保存按钮 
				cancel:null, //取消按钮
			});
		});
		
		
		
		
		//首页轮播图
		var _speed=2400;
		var _num=$('.scroll a').length;
		$('.scroll a:first').fadeIn();
		
		if(_num==1){
			return false;
		}
		
		pointHtml='';
		for(i=0;i<=_num-1;i++){
			if(i==0){
				pointHtml+='<a href="javascript:void(0)" class="current"></a>';
			}else{
				pointHtml+='<a href="javascript:void(0)"></a>';
			}
		}
		$('.points').append(pointHtml);
		
		_index=0;
		function autoplay(){
			_index==_num-1 ? _index=0 : _index++;
			$('.scroll a').eq(_index).stop(true,true).fadeIn().siblings('a').stop(true,true).fadeOut();
			$('.points a').eq(_index).addClass('current').siblings('a').removeClass('current');
		}
		_timer=setInterval(autoplay,_speed);
		
		$('.points a').on('click',function(){
			var _aindex=$(this).index();
			$(this).addClass('current').siblings('a').removeClass('current');
			$('.scroll a').eq(_aindex).stop(true,true).fadeIn().siblings('a').stop(true,true).fadeOut();
		});
		
		$('.banner').hover(function(){
			clearInterval(_timer);
			$('.controbtn').css('display','block');
		});
		$('.banner').on('mouseleave',function(){
			_timer=setInterval(autoplay,_speed);	
			$('.controbtn').css('display','none');
		});
		$('.prevbtn').on('click',function(){
			_index==0 ? _index=_num-1 : _index--;
			$('.scroll a').eq(_index).stop(true,true).fadeIn().siblings('a').stop(true,true).fadeOut();
			$('.points a').eq(_index).addClass('current').siblings('a').removeClass('current');	
		});
		$('.nextbtn').on('click',function(){
			_index==_num-1 ? _index=0 : _index++;
			$('.scroll a').eq(_index).stop(true,true).fadeIn().siblings('a').stop(true,true).fadeOut();
			$('.points a').eq(_index).addClass('current').siblings('a').removeClass('current');	
		});
		
		
		
		$(".casebox").slide({ mainCell:".casebox .casebox_scroll",effect:"leftLoop",vis:3,interTime:2000,autoPlay:true });
		
		
			
	})
</script>


</body>
</html>