  <script type="text/javascript">
var sort = 1;
var hostUrl = window.location.host;

function onloadAq(){
$.ajax({          
    url: "ask_detail_answered.jsp",
    data:"topicId=10005224"+"&sort=1",
    cache: false,
    success: function(html){
	//$("#qusans_bddetails").html(html);

	//ppstore.setItem("localAskDetailKeys",$("#qusans_alldetails").html());
    }
});

}

//onloadAq();

function onloadhotans(){
	$("#hot_ans").addClass("on");
	$("#new_ans").removeClass("on");
	sort = 1;
	$.ajax({          
	      url: "ask_detail_answered.jsp",
	      data:"topicId=10005224"+"&sort=1",
	      cache: false,
	      success: function(html){
		$("#qusans_bddetails").html(html);
		//window.location.hash="hotnew";
		ppstore.setItem("localAskDetailKeys",$("#qusans_alldetails").html());

	      }
	});

}

function onloadnewans(){
	$("#new_ans").addClass("on");
	$("#hot_ans").removeClass("on");
	sort = 0;
	$.ajax({  
	      url: "ask_detail_answered.jsp",
	      data:"topicId=10005224"+"&sort=0",
	      cache: false,
	      success: function(html){
		$("#qusans_bddetails").html(html);
		//window.location.hash="newhot";
		ppstore.setItem("localAskDetailKeys",$("#qusans_alldetails").html());

	      }
	}); 
}


//var anstotop = $("#ask_app_down01").height()+$(".header").height()+$(".user_bio").height()+$(".user_related").height();
//var anstotop2 = $("#topic_ans_slide").offset().top;
var anstotop3 = document.getElementById("topic_ans_slide").offsetTop;
//alert(anstotop3);

var bt = $('#toolBackTop');
$(window).scroll(function() {
	bt.show();
	var st = $(window).scrollTop() || document.documentElement.scrollTop || document.body.scrollTop;
	$("#more_bt").attr("data-disabled","true");
	var sts = document.documentElement.scrollTop || document.body.scrollTop;
	ppstore.setItem("askdetailscrollTop",sts);
	if(st > 100){
		bt.show();
	}else{
		bt.hide();
	}
	if (st > anstotop3){
			//$("#topic_ans_slide").addClass("chnfloat");
	}else{
			//$("#topic_ans_slide").removeClass("chnfloat");
	}
});


$(document).ready(function(){

	window.onscroll = function () { 
		$("#more_bt").attr("data-disabled","true");
		var sts = document.documentElement.scrollTop || document.body.scrollTop;
		ppstore.setItem("askdetailscrollTop",sts);
	}

});

$(function () {
	$.history.init(function (hash) {
		$("#more_bt").attr("data-disabled","true");
		var timestamp = $("#timestamp").val();
		if (timestamp == ppstore.getItem("askdetailtimestamp")){
			if (ppstore.getItem("localAskDetailKeys")!=null && ppstore.getItem("localAskDetailKeys")!=""){
				$("#qusans_alldetails").html(ppstore.getItem("localAskDetailKeys"));
				window.scrollTo(0,ppstore.getItem("askdetailscrollTop"));
			}
		}else{
			ppstore.setItem("askdetailtimestamp",timestamp);
			ppstore.setItem("localAskDetailKeys","");
		}
	});
});

if(hostUrl.indexOf("m.thepaper.cn")>-1){
var nextpageUrl = "http://m.thepaper.cn/load_ask_detail_answered.jsp";
} else if(hostUrl.indexOf("www.thepaper.cn")>-1) {
var nextpageUrl = "http://www.thepaper.cn/wap/resource/v3/jsp/load_ask_detail_answered.jsp";
} else {
var nextpageUrl = "http://"+hostUrl+"/wap/resource/v3/jsp/load_ask_detail_answered.jsp";
}

var g_pageidx = 1;
function loadnextpage(){
if($("#more_bt").attr("data-disabled")=="false"){
return;
}
$("#more_bt").attr("data-disabled","false");
g_pageidx = parseInt($("#loadnextpage").prev(".newspath").attr("pageIndex"));
var n_pageidx = g_pageidx + 1;
$("#more_bt").html("加载中...");
	$.ajax({
		type	:	"get",
		url  	:	nextpageUrl,
		data	:	"pageidx=" + n_pageidx + "&topicId=10005224&sort=" + sort + "&mrd=" + Math.random(),
		timeout :	30000,
		dataType	:	"html",
		success:function(dataHtml){
$("#more_bt").attr("data-disabled","true");
if(dataHtml.indexOf("topic_ans_li")>-1){
			$("#loadnextpage").before(dataHtml);
			$("#more_bt").html("加载更多…");
			ppstore.setItem("localAskDetailKeys",$("#qusans_alldetails").html());
} else if(dataHtml.indexOf("newspath")>-1 && dataHtml.indexOf("topic_ans_li")<0) {
			$("#more_bt").html("已经到底");
} else {
			$("#more_bt").html("加载出错，请重新点击加载");
}
		},
		error:function(XMLHttpRequest, textStatus, errorThrown){
$("#more_bt").attr("data-disabled","true");
			$("#more_bt").html("加载失败，请重新点击加载");
		}
	});

}

function showShareBox(){
    $('.bg-opacity30').show();
    $('.v3-shareBox').slideToggle();
}

$("#toptit").click(function(){
    window.location.href='ask_index.jsp';
});
</script> 
