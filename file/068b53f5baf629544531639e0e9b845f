  }
 }
}//tab effects
var tabList = $_class("menu_zzjs_net").getElementsByTagName("li")
 tabCon = $_class("sub_zzjs").getElementsByTagName("ul");
for(i=0;i<tabList.length;i++){
 (function(){
  var t = i;
  tabList[t].onmouseover = function(){
   for(o=0;o<tabCon.length;o++){
