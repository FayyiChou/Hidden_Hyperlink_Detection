    $("#divLiftCode").html(rowStr).css({ position: getOs(), top: __oran_top_offset }).css(imInfo.imPosition, "0px");
    $("#divHome").html(rowStr).css({ position: getOs(), top: __oran_top_offset }).css(imInfo.imPosition, "0px");
    function getOs() {
        var OsObject = "";
        if (navigator.userAgent.indexOf("MSIE") > 0) {
            if (isIE6 = navigator.appVersion.split(";")[1].replace(/[ ]/g, "").indexOf("6") > 0)
            { return "absolute"; } else {
                return "fixed";
            }
        }
        if (isFirefox = navigator.userAgent.indexOf("Firefox") > 0) {
            return "fixed";
        }
        if (isSafari = navigator.userAgent.indexOf("Safari") > 0) {
            return "fixed";
        }
        if (isCamino = navigator.userAgent.indexOf("Camino") > 0) {
            return "fixed";
        }
        if (isMozilla = navigator.userAgent.indexOf("Gecko/") > 0) {
            return "absolute";
        } else { return "fixed"; }
    }
    $("#hm_t_undefined").css("display", "none");
</script>


