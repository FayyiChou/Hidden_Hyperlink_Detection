var speedw30=20//速度数值越大速度越慢
demow20.innerHTML=demow10.innerHTML
function Marqueew0(){
if(demow20.offsetWidth-demow0.scrollLeft<=0)
demow0.scrollLeft-=demow10.offsetWidth
else{
demow0.scrollLeft++
}
}
var MyMarw0=setInterval(Marqueew0,speedw30)
demow0.onmouseover=function() {clearInterval(MyMarw0)}
demow0.onmouseout=function() {MyMarw0=setInterval(Marqueew0,speedw30)}
</SCRIPT>
