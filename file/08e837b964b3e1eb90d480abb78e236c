      getJSONP("http://auth.sinosig.com/auth/login_retriveCustInfo.action",{type:"jscallback",callback:getData});

	  function getData(data)

	  {

		var url = window.location.href;

		

		if(url=='http://www.sinosig.com/' || url=='http://www.sinosig.com/index.html' || url=='http://www.sinosig.com/index.shtml' || url=='http://sinosig.com/')

		{

			

			$("#denglu").attr("href","http://auth.sinosig.com/login.html?redirectUrl=http://www.sinosig.com/");

		    $("#zhuce").attr("href","http://auth.sinosig.com/regist.html?redirectUrl="+encodeURIComponent(url));

		}

		else

		{

			$("#denglu").attr("href","http://auth.sinosig.com/login.html?redirectUrl="+encodeURIComponent(url));

		    $("#zhuce").attr("href","http://auth.sinosig.com/regist.html?redirectUrl="+encodeURIComponent(url));

		}

		

		$("#tuichu").attr("href","http://"+url.split("/")[2]+"/logout");

		

		

		if($("#mysun").length > 0)

		{

			if(url=='http://www.sinosig.com/' || url=='http://www.sinosig.com/index.html' || url=='http://www.sinosig.com/index.shtml' || url=='http://sinosig.com/')

			{

				

				$("#dengluTwo").attr("href","http://auth.sinosig.com/login.html?redirectUrl=http://www.sinosig.com/");

				$("#zhuceTwo").attr("href","http://auth.sinosig.com/regist.html?redirectUrl="+encodeURIComponent(url));

			}

			else

			{

				$("#dengluTwo").attr("href","http://auth.sinosig.com/login.html?redirectUrl="+encodeURIComponent(url));

			    $("#zhuceTwo").attr("href","http://auth.sinosig.com/regist.html?redirectUrl="+encodeURIComponent(url));

			}

	    }

			if(data.isLogin){

			var ago=document.getElementById("ago");

			var after=document.getElementById("after");

			ago.style.display='none';

			after.style.display='block';

				 if(data.name!='' && data.name!=null)

				 {

					$("#usernameAfter").html("<a href='http://home.sinosig.com/pc/home/home!index.action'>"+data.name+"</a>");

				 }

				 else

				 {

					$("#usernameAfter").html("<a href='http://home.sinosig.com/pc/home/home!index.action'>客户</a>");	

				 }

			if($("#mysun").length > 0)

			{

				var mysun=document.getElementById("mysun");

				mysun.style.display='none';

				var mysunafter=document.getElementById("mysunafter");

				mysunafter.style.display='block';

			    if(data.name!=''||data.name!=null)

			    {

				$("#usernametwo").html(data.name)

				   }else{

				$("#usernametwo").html("客户")

			    }

		    }

		}

	}

//});

	  

