var metaValue = $("meta[name=track_pageId]").attr("content");

function userBehaviorTrack(url,content){

	var platformStr = metaValue;

    var paramData = 'platformStr='+platformStr+'&dataType=00&extraction=00&fieldName='+content2+'&fillContent='+url;

    sendUserBeHaviorData(paramData);

	if(url!=null&&url!=""){

		window.open(url);

	}

	if (window["console"]){ console.log(paramData); }

}

