var metaValue = $("meta[name=track_pageId]").attr("content");

function userBehaviorTrack(url,content){

	var platformStr = metaValue;

    var paramData = 'platformStr='+platformStr+'&dataType=00&extraction=00&fieldName='+content+'&fillContent='+url;

    sendUserBeHaviorData(paramData);

	if(url!=null&&url!=""){

		window.open(url);

	}

}

function userBehaviorTrackToself(url,content,target){

	var platformStr = metaValue;

    var paramData = 'platformStr='+platformStr+'&dataType=00&extraction=00&fieldName='+content+'&fillContent='+url;

    sendUserBeHaviorData(paramData);

	if(url!=null&&url!=""){

		window.open(url,target);

	}

}

