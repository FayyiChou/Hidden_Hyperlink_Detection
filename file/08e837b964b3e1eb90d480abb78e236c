<script type="text/javascript">


 var startDate=null;

 var metaValue = $("meta[name=track_pageId]").attr("content");	

function userBehaviorTrackcheck2(startDate,fieldName,fillContent){ 

		var platformStr = metaValue;

		var fillTime = getAccessTime();

		var lengthOfStayField = getLengthOfStayHere(startDate);

		var paramData ='platformStr='+platformStr+'&fieldName='+fieldName+"&fillContent="+fillContent+"&fillTime="+fillTime+"&lengthOfStayField="+lengthOfStayField;

		sendUserBeHaviorData(paramData);

}


var metaValue = $("meta[name=track_pageId]").attr("content");

function userBehaviorTracksearch2(fieldName){   

	var platformStr = metaValue;

	var fieldName ="搜索按钮";

    var paramData = 'platformStr='+platformStr+'&fieldName='+fieldName;

    sendUserBeHaviorData(paramData);

}


	//产品搜索，点击搜索按钮

	function checkSearchBar(){

		

		var url;

		var keyword=document.getElementById("keyword");


		//搜索类型,关键词都不能为空

		if(keyword.value == null || trimStr(keyword.value) == "")

		{

			alert("请输入搜索条件！");

			keyword.focus();

			return false;

		}

		userBehaviorTracksearch2('搜索按钮');

		//url='http://product.sinosig.com/product_prdtListNew.action?RankTag=1';//产品汇总,关键词搜索

		url='http://s.sinosig.com/web/'+keyword.value;

		var form=document.getElementById("form1");

		form.action = url;

		//记录搜索框客户输入的关键字

		var tmp = $("#keyword").val(); 

		$.ajax({

			url:"product_saveKeyword.action",

			type:"post",

			dataType:"json",

			data:"productKey=" + tmp,

			success: function(msg) {	

				if(msg.returnMessage=='true'){

					form.submit();

				}

			}

		});	

	}


	function focusInSearchBar(obj)

	{

		startDate = new Date();

		if (obj.value == obj.defaultValue || obj.value == null || trimStr(obj.value) == "")

		{

			obj.value = "";

			obj.style.color="#000000";

		}

	}

	

	function blurFromSearchBar(obj)

	{

		var fieldName = "搜索框内容";

		var content= obj.value;

		userBehaviorTrackcheck2(startDate,fieldName,content);

		if (obj.value == null || trimStr(obj.value) == "")

		{

			obj.value = obj.defaultValue;

			obj.style.color="#CCCCCC";

		}

	}


	function trimStr (strVal)

	{

	    var reVal;

	    var strTmp;

	    strTmp = strVal + "";

	    if (strTmp.length == 0)

	        return (strTmp);

	    reVal = /^(\s|　)*/;

	    strTmp = strTmp.replace (reVal, '');

	    reVal = /(\s|　)*$/;

	    return (strTmp.replace (reVal, ''));

	}

	

function sendUserBeHaviorData(paramData){


var params = "referer=" + referer + "&clientProvinceName=" + clientProvinceName + "&clientCityName=" + clientCityName + "&clientOSName=" + clientOSName;

if(paramData != undefined && "" != paramData){

	params = paramData + "&" + params;

}

//alert(params);

//return;

$.ajax({

    url: "http://wecare.sinosig.com/userBehaviorRecord_sendUserBeHaviorData.action",

    type: "post",

    dataType: "json",

    data: params,

    success: function(data) {}

});

}	


function sendUserInteractionData(paramData){

	var params = "referer=" + referer + "&tableFlag=I"+"&clientProvinceName=" + clientProvinceName + "&clientCityName=" + clientCityName + "&clientOSName=" + clientOSName;

	if(paramData != undefined && "" != paramData){

		params = paramData + "&" + params;

	}

	//alert("paySuccess-------"+params);

	//return;

	$.ajax({

	    url: "http://wecare.sinosig.com/userBehaviorRecord_sendUserBeHaviorData.action",

	    type: "post",

	    dataType: "json",

	    data: params,

	    success: function(data) {}

	});

}


$(function(){

    var metaValue = $("meta[name=track_pageId]");   

    var pfs =metaValue.attr("content");

    if(pfs==undefined||""== pfs) return;

	 var dt ="00";

	 var para =  "platformStr=" + pfs + "&dataType=" + dt;

	 sendUserBeHaviorData(para)

    

});	


</script>

 

