<script type="text/javascript">
var navCount=8;


function ClearNav(){
	for(var i=1;i<=navCount;i++){
		var target=$("#navBG_"+i);
		target.toggleClass("navButton",false);
		target.toggleClass("navButtonNoBG",true);
		
		if(i==2){
			target.toggleClass("navButtonBJ",false);
			target.toggleClass("navButtonNoBG",true);
			$(".navItemBJ a",target).css("color","#ff0");
			
			var navIMG=$("#navHot");
			navIMG.attr("src","images/hot_blue.jpg");
		}
		else{
			target.toggleClass("navButton",false);
			target.toggleClass("navButtonNoBG",true);
			$(".navItem a",target).css("color","#fff");
		}
		
	}
}



function navOver(ndx){
	ClearNav();

	var nav=$("#navBG_"+ndx);
	nav.toggleClass("navButtonNoBG",false);
	
	
	if(ndx==2){
		nav.toggleClass("navButtonBJ",true);	
		$(".navItemBJ a",nav).css("color","#214CAA");	
		
		var navIMG=$("#navHot");
		navIMG.attr("src","images/hot.jpg");
	}
	else{
		nav.toggleClass("navButton",true);	
		$(".navItem a",nav).css("color","#214CAA");	
	}
	
	var menu=$("#nav_menu");
	var menuLeft=nav.position().left-220;
	menu.css("left",menuLeft);
	
	
	//内容
	var menuCntx=$("#menuInner",menu);	
	var contextText=$("#navContext_"+ndx);
	menuCntx.html(contextText.html());
				
	menu.show();
}


function navOut(ndx){		
	var menu=$("#nav_menu");
	menu.mouseleave(function(){	
		var target=	$("#navBG_"+ndx);
		
		
		if(ndx==2){
			target.toggleClass("navButtonBJ",false);
			target.toggleClass("navButtonNoBG",true);
			$(".navItemBJ a",target).css("color","#ff0");
			
			var navIMG=$("#navHot");
			navIMG.attr("src","images/hot_blue.jpg");
		}
		else{
			target.toggleClass("navButton",false);
			target.toggleClass("navButtonNoBG",true);
			$(".navItem a",target).css("color","#fff");
		}
		
		menu.hide();
	});
	
}
</script>

