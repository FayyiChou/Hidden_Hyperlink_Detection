<script type="text/javascript">
function leftButtonOver(ndx){
	$("#leftbutton_"+ndx).toggleClass("leftButtonBG",false);
	$("#leftbutton_"+ndx).toggleClass("leftButtonNoBG",true);
	
	$("#leftbuttonCtx_"+ndx).toggleClass("leftButton",false);
	$("#leftbuttonCtx_"+ndx).toggleClass("leftButtonNo",true);
}


function leftButtonOut(ndx){
	$("#leftbutton_"+ndx).toggleClass("leftButtonNoBG",false);
	$("#leftbutton_"+ndx).toggleClass("leftButtonBG",true);
	
	$("#leftbuttonCtx_"+ndx).toggleClass("leftButtonNo",false);
	$("#leftbuttonCtx_"+ndx).toggleClass("leftButton",true);
}

function leftButtonClick(pid,infID){
	var goUrl="pdtView.asp?pid="+pid+"&id=150&infID="+infID.toString();
	location.href=goUrl;
}

function newsButtonClick(pid){
	var goUrl="newsView.asp?id=0&infid=200&vid="+pid.toString();
	location.href=goUrl;
}
</script>


