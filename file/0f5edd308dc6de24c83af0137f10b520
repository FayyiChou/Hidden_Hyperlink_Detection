<!--打分、评论、献花、投票功能js文件开始-->
<script language="javascript">


    var hostid = 0;
    
    var server_url = "http://comments.k618.cn/";
    
    
	function parseTopicData(embedCommentsData,countingData){
		//alert(countingData);
		//alert(embedCommentsData.url);
		this.hostid = embedCommentsData.hid;
		var totalScore = countingData.totalscore;
		var markcount = countingData.markcount;
                $('#total_score').val(totalScore);
                $('#mark_count').val(markcount);
		
		if(markcount>0){
			document.getElementById("mean_score").innerHTML = "("+parseInt(totalScore/markcount*10)/10+")";
		}
				


		drawVoting();
		var js_src = embedCommentsData.url + "?hid="+embedCommentsData.hid+"&chs=0&sh=0&cid=commentsdiv&css=plain";
		var _date = new Date();
		var y = document.createElement("script");
		y.src = js_src+"&r="+_date.getTime();
		y.defer = true; 
		document.getElementsByTagName("head")[0].appendChild(y);
	}
	
	function drawVoting(){
		var fullwidth = 100;
		var _total = 0;
		votecounts = allcountData.extcount;
		if(votecounts.length==6){
			_total = votecounts[3]+votecounts[4]+votecounts[5];
			if(_total>0){
				$("#vi3").html(votecounts[3]);
				$("#vi4").html(votecounts[4]);
				$("#vi5").html(votecounts[5]);
				$("#bar3").css("width",Math.floor(fullwidth*votecounts[3]/_total));
				$("#bar4").css("width",Math.floor(fullwidth*votecounts[4]/_total));
				$("#bar5").css("width",Math.floor(fullwidth*votecounts[5]/_total));
			}
		}
	}

    function getTopicEmbedata(){
			hosturl = window.location.href;
			var titleObj = document.getElementsByTagName("title")[0];
			 title = titleObj.textContent;
			//alert(encodeURI(hosturl)+"\n"+encodeURI(title));
			var _date = new Date();
			var x = document.createElement("script");
			x.src = server_url+"app/embedata.jsp?from=klcbxxtdty2n&tid=./t20120328_2041430.htm&subject="+encodeURI(title)+"&url="+encodeURI(hosturl)+"&sp=1&ebtype=0&cf=parseTopicData&r="+_date.getTime();
			x.defer=true; 
			document.getElementsByTagName("head")[0].appendChild(x);
			//alert("Finish.\n"+x.src);
	}

   function afterMakeposition(voteResult){
		if(voteResult.success){
			alert("投票成功!");
			allcountData.extcount[voteResult.idx] = allcountData.extcount[voteResult.idx]+1;
			//drawVoting(allcountData.extcount);
			drawVoting();
		}
	}

	
	//支持、反对、献花....
	function support(obj,yourattitude){
		if(typeof(this.hostid)!=='undefined' && this.hostid>0){
			var _date = new Date();
			var z = document.createElement("script");
			z.src = server_url+"app/makeposition.jsp?what=topic&id="+this.hostid+"&ci="+yourattitude+"&r="+_date.getTime();
			z.defer = true; 
			document.getElementsByTagName("head")[0].appendChild(z);
			if($(obj).html().indexOf("已")<0)
				$(obj).html("已"+$(obj).html());
			//alert(obj.innerHTML+"\n"+z.src+"\n"+this.hostid);
		}
	}
	
	function simplevote(obj,idx){
		if(typeof(this.hostid)!=='undefined' && this.hostid>0){
			var _date = new Date();
			var z = document.createElement("script");
			z.src = server_url+"app/makeposition.jsp?what=topic&id="+this.hostid+"&ci="+idx+"&r="+_date.getTime();
			z.defer = true; 
			document.getElementsByTagName("head")[0].appendChild(z);
			if($(obj).html().indexOf("已")<0)
				$(obj).html("已"+$(obj).html());
		}
	}
	
		
	//打分
	function markscore(obj,score){
		if(typeof(hostid)!='undefined' && hostid>0 && parseFloat(score)>=0 && parseFloat(score)<=100){
                  var totalScore = parseInt($('#total_score').val());
                  var markcount = parseInt($('#mark_count').val());
                  $('#total_score').val(totalScore+parseFloat(score));
                  $('#mark_count').val(markcount+1);
                  totalScore = parseInt($('#total_score').val());
                  markcount = parseInt($('#mark_count').val());
                  $('#mean_score').html('('+parseInt(totalScore/markcount*10)/10+')');
		  var a = $('#mean_score').parent();
                  if(a.html().indexOf("已")<0)
				a.html("已"+a.html());

			var _date = new Date();
                  $.getJSON(server_url+"app/markscore.jsp?id="+hostid+"&score="+parseFloat(score)+"&callback=?&r="+_date.getTime(), function(data){
                  });
/*			var z = document.createElement("script");
			z.src = server_url+"app/markscore.jsp?id="+hostid+"&score="+parseFloat(score)+"&callback=rateSuccess&r="+_date.getTime();
			z.defer = true; 
			document.getElementsByTagName("head")[0].appendChild(z);
			alert(parseFloat(score)+"MarkScore"+hostid);
                    */
		}
		//alert(obj.innerHTML+"\n"+z.src);
	}
	
	//动态引入css
	function importCssLink(url){
   		var doc=document;
    	var link=doc.createElement("link");
    	link.setAttribute("rel", "stylesheet");
    	link.setAttribute("type", "text/css");
    	link.setAttribute("href", url);
    	var heads = doc.getElementsByTagName("head");
    	if(heads.length)
    		heads[0].appendChild(link);
    	else
    	    doc.documentElement.appendChild(link);
    }

	//动态引入js
    function importScript(url){
    	var x = document.createElement("script"); 
    	x.src = url;
    	x.defer = true;
    	document.getElementsByTagName("head")[0].appendChild(x);
    }
    
	</script>
<!--打分、评论、献花、投票功能js文件结束-->
