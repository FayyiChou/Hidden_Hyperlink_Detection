jQuery(function() {
	if (jQuery("#NewMemberFlow").val() == "1" && jQuery.cookie("LoginSuccess") != "1" && jQuery.cookie("to_forbid_pop") != "1" && !mobilecheck()) {
		jQuery("#NewMemberLayer").show();
		jQuery("#NewMemberPop").show();
	}
	
	jQuery(".CPM-Popup .CPM-Popup1").click(function() {
		jQuery("#NewMemberPop").hide();		
		jQuery("#NewMemberLayer").hide();
		if (jQuery("#to_forbid_pop").attr("checked")) {
			jQuery.cookie("to_forbid_pop", "1", {expires: 30, path:"/"});	//标记曾经登录成功
		}
		return false;
	});
	
	jQuery(".CPM-Popup .CPM-Popup2").click(function() {
		jQuery("#NewMemberPop").hide();		
		jQuery("#NewMemberLayer").hide();
		return true;
	});
});

