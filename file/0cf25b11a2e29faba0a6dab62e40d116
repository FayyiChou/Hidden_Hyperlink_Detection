<script>
var cscroll=function(config){this.box=config.box;this.nav=config.nav;this.on=config.on;this.fn=config.fn||"mouseover";this.lbtn=config.lbtn;this.rbtn=config.rbtn;this.auto=config.auto||false;this.during=config.during||3000;this.pause=config.pause||false;this.callback=config.callback;this.cur=0;this.timer;var that=this;if(this.nav)this.nav.bind(this.fn,function(){if($(this).hasClass(that.on))return;that.change($(this).index())});if(this.lbtn)this.lbtn.bind("click",function(){that.prev()});if(this.rbtn)this.rbtn.bind("click",function(){that.next()});if(this.auto)this.start();if(this.pause)this.box.hover(function(){that.stop()},function(){that.start()})};cscroll.prototype={prev:function(){this.change(this.cur-1<0?this.box.length-1:this.cur-1)},next:function(){this.change(this.cur+1>=this.box.length?0:this.cur+1)},change:function(i){this.cur=i;if(this.nav)this.nav.eq(i).addClass(this.on).siblings().removeClass(this.on);this.box.eq(i).show().siblings().hide();if(this.callback)this.callback(i);if(this.auto)this.start()},start:function(){var that=this;this.stop();this.timer=setTimeout(function(){that.next()},this.during)},stop:function(){clearTimeout(this.timer)}};


var zt_slider=new cscroll({
	box:$("#zhuanti .inner li"),
	lbtn:$("#zt-prev"),
	rbtn:$("#zt-next"),
	auto:true
});



function IE(){var undef,ie,v=3,div=document.createElement('div'),all=div.getElementsByTagName('i');while(div.innerHTML='<!--[if gt IE '+(++v)+']><i></i><![endif]-->',all[0]);ie=v>4?v:undef;return ie}

/******    index-r-fixed-adv-effect    ******/
var fixCheck = function(){
	var limit = $(window).scrollTop() + $(window).height(),	o = $("#fixed-r"),
	t1 = o.parent().offset().top + o.height(), t2 = o.parent().offset().top + o.parent().height(), t3 = (o.parent().width() / 2) - o.width();
	if($(window).width()>=784 && o.height()<o.prev().height() && limit >= t1){
		if(IE()<=6){
			var a = limit>=t2 ? t2+3 : limit, b = a - o.parent().offset().top - o.height()-3;
			o.css("margin-top", b+"px")
		}else if(limit >= t2){
			o.css({"position":"fixed", "left":"50%", "margin-left":t3+"px", "bottom":(limit-t2)+"px"})
		}else{
			o.css({"position":"fixed", "left":"50%", "margin-left":t3+"px", "bottom":"3px"})
		}
	}else{
		o.css({"position":"relative", "bottom":"auto", "left":"auto", "margin-left":"-"+o.width()+"px", "margin-top":"auto"})
	}
};
$(window).load(fixCheck);
$(window).resize(fixCheck);
$(window).scroll(fixCheck);
</script>


