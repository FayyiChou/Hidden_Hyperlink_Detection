	<script>
	    ///导航
         
         $('#navgator').ready(
		 function loadnavagator(){
        		 $.get("/json/root.json", function(data){
        			 $.each(data, function(i,item){
        				 
        				 $('#navigatorui').append('<li id='+item.navid+'><a target="'+item.action+'" href="'+item.url+'" ><span style="font-weight:bold">'+item.name+'</span></a>');
        			//二级导航开始
                   $.get("/json/"+item.navid+".json", function(data1){
        			 $.each(data1, function(j,item1){
						 if(j==0){
							 $('#'+item.navid+' a').attr('class','dropdown-toggle');
							 $('#'+item.navid+' a').attr('data-toggle','dropdown');
							 $('#'+item.navid+' a').append('<span class="caret"></span>')
							 $('#'+item.navid+'').append('<ul id='+item.navid+''+j  +'  class="dropdown-menu"></ul>'); 
                         $('#'+item.navid+''+j+'').append('<li><a target='+item1.action+' href='+item1.url+'>'+item1.name+'</a></li>');
						 }
						 else{
							  $('#'+item.navid+'0').append('<li><a target='+item1.action+' href='+item1.url+'>'+item1.name+'</a></li>');
						 }
					  
                                     
                  									 

					});
				   });



					});
        		
        		 })
         
         }
         );
		 
		 
		 
	
	
	
	
	</script>
