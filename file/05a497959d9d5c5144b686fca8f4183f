    	var userName = '';
    	$(function(){
	   		$("#logo_user_name_1").remove();
	   		$("#login-on").remove();
			$("#register").remove();
			$("#login-out").remove();
    		var flag = 'false';
    		if(flag == "true"){//已经成功登录
    			$("#logo_user_name").append("<span id='logo_user_name_1'>欢迎您，"+userName+"</span>");
    			$("#user-cont").append("<a href='javascript:void(0)' id='login-out' class='login-out' onclick='logout();'>退出</a>");
    		}else{//登录失败或者未登录
    			$("#logo_user_name").append("<span id='logo_user_name_1'>请登录&nbsp;</span>");
    			$("#user-cont").append("<a href='javascript:void(0)' id='login-on' class='login-on'  onclick='toLogin();'>登录&nbsp;</a>");
    			$("#user-cont").append("<a href='javascript:void(0)' id='register' class='register' onclick='userRegister();'>注册</a>");
    		}
    	}
		)
    	function userRegister(){
			window.parent.location.href="http://www.yjhlwsj.gov.cn:80/userRegister.shtml";
		}
		function toLogin(){
			window.parent.location.href="http://www.yjhlwsj.gov.cn:80/jumpToLogin.shtml";
		}
		function logout(){
			window.parent.location.href="http://www.yjhlwsj.gov.cn:80/logout.shtml";
		}
		$.ajax({  
				url:"http://www.yjhlwsj.gov.cn:80/getNowWeather.shtml",// 跳转到 action  
				type:"post",
				dataType:"json",
				success:function(data) { 
				var weatherData = data.nowWeather;
				var arr = weatherData.split("||");
				var aqi = arr[0];
				var pm25 = arr[1];
				var txt = arr[2];
				var dir = arr[3];
				var sc = arr[4];
				var maxTmp = arr[5];
				var minTmp = arr[6];
				var weatherDate = arr[7];

				var weatherData0 = txt;
				var weatherData1 = sc+"级"+dir;
				var weatherData2 = minTmp+"-"+maxTmp;
				var weatherData3 = "空气质量指数："+aqi;
				var weatherData4 = "PM2.5指数："+pm25;
				
				var htmlWeather = weatherDate+"&nbsp;"+weatherData0+"&nbsp;"+weatherData1+"&nbsp;"+weatherData2+"℃"+"&nbsp;"+weatherData3+"&nbsp;"+weatherData4;
				$("#weather").remove();
				$("#top_left").append("<span class='weather' id='weather'>"+htmlWeather+"</span>");
				},  
				error : function() {  
			        alert("异常！");  
			    }  
			})
    </script>
