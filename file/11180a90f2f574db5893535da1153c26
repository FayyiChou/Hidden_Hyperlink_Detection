	 s1.setAttribute('crossorigin','*');
	 s0.parentNode.insertBefore(s1,s0);
	 })(); */
	 //End of Tawk.to Script
	 //查看消息下拉框
	function News_select(obj) {
		if ($('.news-select').is(":hidden")) {
			$('.news').addClass("select")
			$.ajax({
				type: "POST",
				dataType: "JSON",
				url: "/front/customer/messageCenter/getMsgCountAjax.action",
				data: {
					type: 'all'
				},
			success: function(data) {
				$("#Announcement").html("("+data.sysCount+")"); //系统消息
				$("#News").html("("+data.inCount+")"); //收件箱
				$("#Inbox").html("("+data.toall+")"); //系统通告
				}
			});
			$('.news-select').show();
			}else {
				if($(obj).attr("class") != "news select"){
					$('.news-select').hide();
					$('.news').removeClass("select")
				}
			}
		
	}
	$(document).click(function(e){ 
		 e = window.event || e; // 兼容IE7
		 obj = $(e.srcElement || e.target);
		    if (!$(obj).is(".news *")) { 
		     $('.news-select').hide();
		     $('.news').removeClass("select")
		   }
	});

    </script>



	
		
