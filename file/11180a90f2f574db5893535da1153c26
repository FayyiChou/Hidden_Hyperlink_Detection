	initCountryMsg();
	
	function getCount(){
		$.ajax({
			type: "POST",
			dataType: "JSON",
			url: "/front/customer/messageCenter/getMsgCountAjax.action",
			data: {type:'all'},
			success: function(data){
				if(data.isSuccess=="true"){
					if(data.inCount!='' && data.inCount!='0'){
						$("#inCount").html(data.inCount);
					}
					if(data.sysCount!='' && data.sysCount!='0'){
					    $("#sysCount").attr("value",data.sysCount);
					}
					$(".M_msg").html(data.inCount+data.sysCount);
				}
			}
		});
	}

	//点击topSearch
	function topSearch(keyWord){
		var newKey = escape(keyWord);
		if(newKey.length<300){
			$("#searchContentValue").val("  "+newKey);
			$("#searchForm").submit();
		}
	}

	//转向一级分类,根据分类查询
	function goFirstType(firstTypeId,level){
		var winopen = window.open ("/frontBrands/getBrandsAndProductInfos.action?productTypeId="+firstTypeId+"&level="+level+"&orderBy=normal");
	}


	//点击查询商品或店铺
	function searchImg(num){
		var keyWord = $.trim($("#searchContent"+num).val());
		var className1 = $(".Choice"+num).children('span').attr('spantype');
		if(className1=="shop_span"){
			$("#isShopSearch").val("1");
		}else {
			$("#isShopSearch").val("");
		}
		if(keyWord != null && keyWord != ""){
			var newKey = escape(keyWord);
			if(newKey.length<300){
				$("#searchContentValue").val(newKey);
				$("#searchForm").submit();
			}
		}
	};


	$(function(){
		$("#searchContent1").keydown(function(event){ 
			var evt = event ? event : (window.event ? window.event : null);
			if(evt.keyCode == 13){
				var keyWord = $.trim($("#searchContent1").val());
				if(keyWord != null && keyWord != "" && keyWord != "Please enter the brand name and products name"){//请输入品牌，商品名称
					var newKey = escape(keyWord);
					if(newKey.length<300){
						$("#searchContentValue").val(newKey);
						$("#searchForm").submit();
					}
				}
			};
		});	
		
		$("#searchContent2").keydown(function(event){ 
			var evt = event ? event : (window.event ? window.event : null);
			if(evt.keyCode == 13){
				var keyWord = $.trim($("#searchContent2").val());
				if(keyWord != null && keyWord != "" && keyWord != "Please enter the brand name and products name"){//请输入品牌，商品名称
					var newKey = escape(keyWord);
					if(newKey.length<300){
						$("#searchContentValue").val(newKey);
						$("#searchForm").submit();
					}
				}
			};
		});
		loadHeadShoppingCartHtml()	
	});


	/**
	 * 初始化国家信息
	 */
	function initCountryMsg(){
		$.ajax({
             type: "POST",
             dataType: "JSON",
             url:contextPath+"shopHome/selectCountryAjax.action",
             success: function(data){
            	 var countryImgUrl = data.countryImgUrl;
            	 var countryCode = data.countryCode;
            	 if(countryCode!=null && countryCode !=""){
            		 showOrHideCountryShop(countryCode,countryImgUrl);
            	 }
             }
		 });
	}

	//显示和隐藏国家商品
	function showOrHideCountryShop(country,imgurl){
		if(country == "Global"){
			for (var int = 2; int < 7; int++) {
				$(".Recommend-con"+int+" li").each(function(){
				    if($(this).attr("name") == country || $(this).attr("name") == "ALL" || $(this).attr("name")== undefined || $(this).attr("name")==""){
				    	 $(this).show();
				    }else {
				    	 $(this).hide();
				    }
				 });
			}
			 $(".defultCountry").html("<span class='flag-"+country+"'></span><Lable>"+country+"</Lable>");
		}else {
			for (var int = 2; int < 7; int++) {
				$(".Recommend-con"+int+" li").each(function(){
				    if($(this).attr("name") == country){
				    	$(this).show();
				    }else {
				    	$(this).hide();
				    }
				 });
			}
			 $(".defultCountry").html("<span class='flag-"+country+"'></span><Lable>"+country+"</Lable>");
		}
	}

	//保存选择国家
	function saveChangeCountry(country,imgurl){
		if(country != null && country !=""){
			$.ajax({
	             type: "POST",
	             dataType: "JSON",
	             url:contextPath+"shopHome/setCountrySession.action?countryImgUrl="+imgurl+"&countryCode="+country,
	             success: function(data){
	            	 location.reload();
	            	 showOrHideCountryShop(country,imgurl);
			     }
			 });
		}
	}

	//购物车信息
	 function loadHeadShoppingCartHtml(){
	 	$('.header-right>li').hover(function() {
	 		if($(".header-box2").css("display")=='block'){
 				$.ajax({
		          type: "POST",
		          dataType: "JSON",
		          url:contextPath+"shopFront/shoppingCar/gotoShoppingCartAjax.action",
		          success: function(data){
		    	  	var listMap=data;
		    	  	if(listMap!=null&&listMap.length>0){
		    			$("#shopcar_list").html("<span>Cart has added&nbsp;<i style='color:red;'>"+listMap.length+"</i> &nbsp;Goods &nbsp;</span><a href='"+contextPath+"shopFront/shoppingCar/gotoShoppingCart.action'>Settle Accounts</a>");
		    		}else{
		    			$("#shopcar_list").html("<span>Cart does not have any Product!</span>" +
		    			"<a href='"+contextPath+"shopFront/shoppingCar/gotoShoppingCart.action'>View Cart </a>");
		    		}
		          }
				});
 			}
	 	})
	  }

	 //Start of Tawk.to Script
	/*  var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
	 (function(){
