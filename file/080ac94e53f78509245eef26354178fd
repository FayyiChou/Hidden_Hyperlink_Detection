	/**
	 * 显示申请超额借款页面
	 */
	function applyForOverBorrowInit() {
	    if('' == 'false') {
     	  showDiv($("#email_box"));
     	  return;
        }
		$.ajax({
			url : "/front/account/applyForOverBorrowWin",
			type : "POST",
			data : {
			},
			success : function(data) {
				if (data.code < 0) {
					alert(data.msg);

					return;
				}
				
				var ttc = $("#over_borrow_window");
				ttc.html(data);
				showDiv(ttc);
			},
			error : function() {
				alert("对不起，出现错误!");
			}
		});
	}
</script>
		
