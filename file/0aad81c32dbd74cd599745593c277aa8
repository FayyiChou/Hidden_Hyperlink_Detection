   <script>


function deletetest(o,pid){

  var url=$(o).attr('rel');
   malatouch.Open('footer','删除中..')
  $.post(url,{reason:'手机删帖'},function(json){
      var jsonL = json.Message
  if(jsonL.messageval=='admin_succeed'){
  $('#pid'+pid).fadeOut();
  malatouch.Open('footer','操作成功');
  }
  
  },'json')
  
  

}

function bantest(o){

  var url=$(o).attr('rel');
   malatouch.Open('footer','屏蔽中...')
  $.post(url,{reason:'手机管理',banned:1},function(json){
      var jsonL = json.Message
  if(jsonL.messageval=='admin_succeed'){
  malatouch.Open('footer','操作成功')
  }
  
  },'json')
  
  

}


$('#fav').click(function(){
    var favurl=$(this).attr('href');
var iobj=$(this).find('.am-icon-star-o')
    $.get(favurl,function(res){
   var jsonL=res.Message
    if(jsonL.messageval=='favorite_do_success'){
    iobj.css({color:'red'})
}else if(jsonL.messageval=='favorite_repeat'){
    iobj.css({color:'red'})
}

    malatouch.Open('footer',jsonL.messagestr)
},'json')
})
var pid=0;
function quto(o){
  var q=$(o).attr('message')
  $('#reply-popup').find('.quto').html(q)
  pid=$(o).attr('pid');
}



$('#replyThread').click(function(){
  var url=$(this).attr('href');
  var dh=$(this).attr('dh');
  var message=$('#reply-popup').find('textarea').val();
  $.post(url,{pid:pid,message:message},function(json){
      var jsonL = json.Message
  if(jsonL.messageval=='comment_add_succeed'){
      $('#reply-popup').modal('close');
  malatouch.Open('footer','回复成功')
  $('#reply').button('reset');
  window.location.href=dh;
  }
  
  },'json')
  
  
})

$('.am-icon-thumbs-o-up').click(function(){


  var left = 20, top =  0, obj=$(this);

        




var iobj=$(this);
    var url=iobj.attr('href');
    $.get(url,function(res){
   var jsonL=res.Message
    if(jsonL.messageval=='thread_poll_succeed'){
    iobj.find('span').css({color:'red'})
iobj.append('<div id="zhan" style="position:absolute"><b>+1<\/b></\div>');
            $('#zhan').css({'position':'absolute','z-index':'1','color':'#C30','left':left+'px','top':top+'px'});
            $('#zhan').animate({top:top-20,opacity: 0},1500,
           function(){
              $('#zhan').fadeOut(1500).remove();
           });
   
iobj.find('span').html(parseInt(parseInt(iobj.find('span').html())+1));
            iobj.css({color:'red'})
}else if(jsonL.messageval=='noreply_voted_error'){
    iobj.css({color:'red'})
}

},'json')

})



$('.replyqukickbtn').click(function(){
  $('#fastpostmessage').val('');
  $('.btn-loading-example').button('reset');
  //$('.quickmain').hide();
  $('.replyqukick').show();
  $('#zhezhao').show()
  $('#fastpostmessage').focus();
  $('#bottom_nav').hide();
})

$('#blockpost').click(function(){
var url = $(this).attr('data');
  if(confirm('是否推送到手机首页？')){
$.get(url,function(res){
  if(res.code>0){
   alert(res.msg);  
  }else if(res.code==0){
   if(confirm(res.msg)){
                  $.get(url+'&del=1');
  alert('取消成功');
}  
  }
},'json')
  }

})

function movereply(){

   var offset1 = $('#newreply').offset(); 
   var nh = offset1.top - $('#newreply').prev().height();
   var w = $(window);
   w.smoothScroll({position: nh});
}

$('.messagemain').find('img').error(function(){
var imgs = $(this).attr('src')
if(imgs.indexOf('nocache')>0){
console.log(1);

}else{
console.log(2);
        $(this).attr('src', imgs+'&nocache=yes');
}
});
