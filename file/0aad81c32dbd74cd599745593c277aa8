$(function() {
$('#loading').scrollspy({

})
var maxpage =  '7';
var replies =  '140';
var thread_page='1';
 $('#loading').on('inview.scrollspy.amui', function() {
  thread_bottom=1;
  thread_page++;
  if(openclock==1){
    $('#loading').html('<div style="color:#999; text-align:center; padding:10px 15px;color:#E6242B;font-size:20px" onclick="loadmore()"><i class="am-header-icon am-icon-angle-double-down" style="position:relative;top:2px;font-size:20px"></i>&nbsp;&nbsp;查看剩余的'+replies+'条回复</div>').css({width:'100%'});
    return false;  
  }else{
    $('#loading').html('<i class="am-header-icon am-icon-spinner am-animation-spin" style="font-size:26px;color:#666; opacity:0.5"></i>').css({width:'20px'});
 }
  if(thread_page>maxpage){
    $('#loading').html('');
return false;
  }
  var url = 'forum.php?mod=viewthread&ajaxformobile=1&tid='+tid+'&t=1&page='+thread_page
   $.get(url,function(res){
   if(res){
  $('.postlist').append(res);
                        }
  })
  })
});

