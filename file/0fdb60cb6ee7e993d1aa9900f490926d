var windid	= '';
var groupid	= 'guest';
var charset = 'utf-8';
var basename = 'apps.php?q=weibo&';
var temp_basename = '';
var modeimg = 'images/apps';
function resizeImg(ele){
	var a=ele;
	if(a.width<30||a.height<30){
		setTimeout(function(){
resizeImg(a);
		},10)
		return false;
	}
	if(a.width>100){
		
a.width = 100;

		} else if(a.height>100){
a.height=100;
		}
}
</script> 
