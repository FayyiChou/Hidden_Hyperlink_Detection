<script type="text/javascript">
    var tmpUrl = 'http://hr.highpin.cn';
    var adminaccount = '';
    var tmpparam = '02566C54745722';
    var tmpRegisterType = '1';
    var accountType = '0';
    var isadmin = '';
    var pagetype = 1;

    var ToRegister = function () {
        if (accountType == 1) {
            var accountName = $("#AccountName").val();
            if (accountName.length == 0) {//当无此用户时,跳转到注册页面，否则跳转到登录页面
                //弹框提示
                $("#AdminAccout").html(adminaccount);
                ShowSubLayer("HadOpenLayer", false);
            }
        } else if (accountType == 2) {
            //弹框提示
            ShowSubLayer("ToOpenLayer", false);
        } else {
            //跳转到注册页面
            pagetype = 2;
            ToRedirect(pagetype, tmpRegisterType, tmpparam, isadmin);
        }

    }

    var ToRedirect = function (pagetype, tmpRegisterType, tmpparam, isadmin) {
        var url;
        if (isadmin.toLowerCase() == "true") {
            switch (tmpRegisterType) {
                case "0":
                    url = tmpUrl;
                    break;
                case "1":
                    url = "/Recruiter/RDRegisterProtocol?RandomNum=" + tmpparam;
                    break;
                case "2":
                    url = "/Recruiter/RDPerfectInfo?RandomNum=" + tmpparam;
                    break;
                case "3":
                    url = "/Recruiter/MyIndex";
                    break;
                default:
                    url = tmpUrl;
                    break;
            }
        }
        else {
            var accoutNumber = $("#AccoutNumber").val();
            var wangPinUser = $("#WangPinUser").val();
            var accountName = $("#AccountName").val();

            url = "/Recruiter/LoginRegister?pagetype=" + pagetype + "&accountnumber=" + accoutNumber + "&wangpinuser=" + wangPinUser;
            if (accountName.length > 0) {
                url += "&username=" + accountName;
            }
        }
        window.location.href = url;
    }

</script>
<input type="hidden" id="AccoutNumber" value="">
<input type="hidden" id="WangPinUser" value="">
<input type="hidden" id="AccountName" value="">
