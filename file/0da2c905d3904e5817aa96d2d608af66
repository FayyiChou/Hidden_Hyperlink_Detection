        init: function () {
            var self = this;
            _.each(self.getIds(), function (fn, value) {
                $('#' + value).append(self.render(_.filter(self.reData, fn)));
            });
        },
        //重置img 数据
        getReData: function (data) {
            return _.map(data, function (value) {
                //裁减 图片规则是picture  picture1  picture2 开头
                var img = value['img'],
                        _newImg = img.indexOf('/picture/') != -1 ?
                        'http://mpic.spriteapp.cn/crop/135x93/picture/' + img.split('/picture/')[1] :
                                img.indexOf('/picture1/') != -1 ?
                                'http://mpic.spriteapp.cn/crop/135x93/picture1/' + img.split('/picture1/')[1] :
                                        img.indexOf('/picture2/') != -1 ?
                                        'http://mpic.spriteapp.cn/crop/135x93/picture2/' + img.split('/picture2/')[1] :
                                                img;
                return _.extend({}, value, {
                    img: img.indexOf('/x/') != -1 ||
                    img.indexOf('/crop/') != -1 ||
                    img.indexOf('/abscrop/') != -1 ? img : _newImg
                });
            });
        },
        getIds: function () {
            return {
                'j_r_tj_1': function (val, key) {
                    return key <= 3;
                },
                'j_r_tj_2': function (val, key) {
                    return key >= 4 && key <= 5;
                },
                'j_r_tj_3': function (val, key) {
                    return key >= 6 && key <= 7;
                },
                'j_r_tj_4': function (val, key) {
                    return key >= 8 && key <= 9;
                },
                'j_r_tj_5': function (val, key) {
                    return key >= 10 && key <= 11;
                }
            }
        }
    };

    window["_BFD"] = window["_BFD"] || {};
    _BFD.LandRecVAV = function (data, bid, req_id) {
        new Recommended(data);
        _BFD.showBind(data, "LandRecVAV", bid, req_id);//此处是推荐返回时百分点绑定事件的方法，请不要修改。
    };

    _BFD.BFD_INFO = {
        "id": "24082663",   //文章id
        "title": "韩国新闻：韩国掀起抵制中国货品浪潮，出租拒载中国人",  //当前名称
        "author": "繁花落尽",   //作者
        "love": "7158",   //点赞数
        "ccnt": "1065",   //评论人数
        "ptime": "1489134136.0",  //发表时间   时间戳格式
        "pic": "http://wimg.spriteapp.cn/picture/2017/0310/24082663_975.jpg",   //图片链接
        "tag": "视频",   //标签
        "user_id": "0", //网站当前用户id，如果未登录就为0或空字符串
        "valid": "True",
        "essence": "True",
        "page_type": "Landing_dt", //当前页面全称，请勿修改
        "playcount": "128710"
    };
</script>
