<script type="text/javascript">
$(function(){
	$("#focus_pic").hover(function(){
		$("#focus-prev,#focus-next").show();
	},
	function(){
		$("#focus-prev,#focus-next").hide();
	});
	$("#focus_pic").slide({
		mainCell:"#focus-bar-box ul",  	//图像内容
		targetCell:"#focus-title a",	//标题内容
		titCell:"#focus-num a",			//序号
		prevCell:"#focus-prev",
		nextCell:"#focus-next",
		effect:"fade",
		autoPlay:true,
		interTime:6000
	});
	
	
	$("#picFocus1").hover(function(){
		$("#focus-prev1,#focus-next1").show();
	},
	function(){
		$("#focus-prev1,#focus-next1").hide();
	});
	$("#picFocus1").slide({
		mainCell:"#focus-bar-box1 ul",  	//图像内容
		targetCell:"#focus-title1 a",	//标题内容
		titCell:"#focus-num1 a",			//序号
		prevCell:"#focus-prev1",
		nextCell:"#focus-next1",
		effect:"fade",
		autoPlay:true,
		interTime:6000
	});
	
	$("#picFocus2").hover(function(){
		$("#focus-prev2,#focus-next2").show();
	},
	function(){
		$("#focus-prev2,#focus-next2").hide();
	});
	$("#picFocus2").slide({
		mainCell:"#focus-bar-box2 ul",  	//图像内容
		targetCell:"#focus-title2 a",	//标题内容
		titCell:"#focus-num2 a",			//序号
		prevCell:"#focus-prev2",
		nextCell:"#focus-next2",
		effect:"fade",
		autoPlay:true,
		interTime:6000
	});
	

	jQuery("#XYFC").slide({
		mainCell:"dl",
		autoPlay:true,
		effect:"leftMarquee",
		vis:5,
		interTime:25,
		trigger:"click"
	});	
	jQuery("#TWZX").slide({
		mainCell:"dl",
		autoPlay:true,
		effect:"leftMarquee",
		vis:5,
		interTime:30,
		trigger:"click"
	});
		
	$("#AD1").KinSlideshow({
		/*幻灯片效果*/
			moveStyle:"left",
			intervalTime:5,
			mouseEvent:"mouseover",
			isHasTitleBar:false,
			isHasTitleFont:false,
			btn:{btn_bgColor:"#999999",btn_bgHoverColor:"#FF0000",
     			 btn_fontColor:"#EEE",btn_fontHoverColor:"#FFF",btn_fontFamily:"Verdana",
