	});
		
	showScroll();
	function showScroll(){
			$(window).scroll( function() { 
				var scrollValue=$(window).scrollTop();
				scrollValue > 100 ? $('div[class=scroll]').fadeIn():$('div[class=scroll]').fadeOut();
			} );	
			$('#scroll').click(function(){
				$("html,body").animate({scrollTop:0},800);	
			});	
		}
});
$(document).ready(function() { 
/*导航菜单*/
	$("#Nav li").hover(function(){
		$(this).parent().find('.menucur').hide();
		$(this).find('.nav_menu').show();
	},function(){
		$(this).find('.nav_menu').hide();
		$(this).parent().find('.menucur').show();
	});

	jQuery.jqtab = function(tabtit,tab_conbox,shijian,CLink) {
		
		$(tab_conbox).find("li").hide();
		$(CLink).find("a").hide();
		$(tabtit).find("li:first").addClass("thistab"); 
		$(tab_conbox).find("li:first").show();
		$(CLink).find("a:first").show();
		$(tabtit).find("li").bind(shijian,function(){
		  $(this).addClass("thistab").siblings("li").removeClass("thistab"); 
			var activeindex = $(tabtit).find("li").index(this);
			$(tab_conbox).children().eq(activeindex).show().siblings().hide();
			$(CLink).children().eq(activeindex).show().siblings().hide();
			return false;
		});
	};
	$.jqtab("#tabs1","#tab_conbox1","click","#CLink1");	
	$.jqtab("#tabs2","#tab_conbox2","click","#CLink2");
	$.jqtab("#tabs3","#tab_conbox3","click","#CLink3");
	$.jqtab("#tabs4","#tab_conbox4","click","#CLink4");
	$.jqtab("#tabs5","#tab_conbox5","click","#CLink5");
	$.jqtab("#tabs6","#tab_conbox6","mouseover","#CLink6");
	$.jqtab("#tabs7","#tab_conbox7","click","#CLink7");
	$.jqtab("#tabs8","#tab_conbox8","click","#CLink8");
	$.jqtab("#tabs9","#tab_conbox9","click","#CLink9");
	$.jqtab("#tabs10","#tab_conbox10","click","#CLink10");
	$.jqtab("#tabs11","#tab_conbox11","click","#CLink11");
	$.jqtab("#tabs12","#tab_conbox12","click","#CLink12");
	$.jqtab("#tabs13","#tab_conbox13","click","#CLink13");
	$.jqtab("#tabs14","#tab_conbox14","click","#CLink14");
	$.jqtab("#tabs15","#tab_conbox15","click","#CLink15");
	$.jqtab("#tabs16","#tab_conbox16","mouseover","#CLink16");
	$.jqtab("#tabs17","#tab_conbox17","click","#CLink17");
});
</script>
