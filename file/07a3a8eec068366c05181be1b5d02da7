		var a = $("#divSortList");
		var b = false;
		a.children("dl").each(function() {
			$(this).hover(function(c) {
				b = true;
				bI(c);
				$(this).addClass("hover");
				if ($(this).next().length > 0) {
					$(this).append("<i></i>")
				}
			}, function(c) {
				b = false;
				$(this).removeClass("hover").children("i").remove()
			})
		})
		
		var bI = function(a) {
			if (a && a.stopPropagation) {
				a.stopPropagation()
			} else {
				window.event.cancelBubble = true
			}
		};

