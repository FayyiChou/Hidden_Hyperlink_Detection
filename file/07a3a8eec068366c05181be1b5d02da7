var DAOJISHI;



function huanhuis( tdid , $fanhui ){

         if( $fanhui.canyu > 0){
                html = '<div class="w-goods-record"><p class="w-goods-owner f-txtabb">获得者：<a href="'+HTTP+'user.php?y=userjilu&uid='+$fanhui['uid']+'" title="'+$fanhui['uname']+'"><b>'+$fanhui['uname']+'</b></a></p><p>本期参与：'+$fanhui['canyu']+'人次</p><p>幸运号码：'+$fanhui['zhonghao']+'</p></div>';

               
                $("#j"+tdid+" .ico-label").removeClass('ico-label-revealing');
                $("#j"+tdid+" .ico-label").addClass('ico-label-newReveal');
                $("#j"+tdid+" .w-goods-counting").html(html);

           }

}

function huidiaos( result ){

               

                  DATA =  jieshou( result );

                  if ( DATA ){

                      if(DATA.code == 1){

                        fanhui =  DATA.data;
                  
                        huanhuis( fanhui.cpid+'_'+fanhui.cpqs , fanhui );
                         

                      }


                  
                  }

 }

    function CountDown(){


        if($(".djtime").length < 1){
        
           clearInterval(DAOJISHI);
           return false;

        }
    
    
    
      $(".djtime").each(function(){


         var tdid = $(this).attr('data');
         var time = $(this).attr('djtime');

        if( time < 0){ 

                 $(this).attr({'djtime':time -1 }) ;

                if( $("j"+tdid+" .w-goods-counting .w-goods-owner").length < 1 && time % 500 == 0){
                    var CP = tdid.split('_'); 

                    
                    
                    $fanhui = duqudange( CP['0'] , CP['1'],huidiaos);


                }
            return false;

        }

         if(time < 1){

              $(this).attr({'djtime':-1 });

              $(this).find('.w-countdown-nums').hide();

              $(this).find('.w-countdown-ing').show();

              return false;

         }

         var strtime = shijian( time ,100);

          if(time > 360000){

           hh = String(strtime['0']).split('');
           ii = String(strtime['1']).split('');
           ss = String(strtime['2']).split('');

        
         }else{
      
           hh = String(strtime['1']).split('');
           ii = String(strtime['2']).split('');
           ss = String(strtime['3']).split('');

         }

         
         thsss = '<b>'+hh['0']+'</b><b>'+hh['1']+'</b>:<b>'+ii['0']+'</b><b>'+ii['1']+'</b>:<b>'+ss['0']+'</b><b>'+ss['1']+'</b>';


         $(this).find('.w-countdown-nums').html(  thsss);

         $(this).attr({'djtime':time -1 })

      
      

      
      
      });
    
    
    
    
    
    }


