function add_shopping(prod_id , prod_type_id , num){
		$.ajax({
		url:"./includes/controller/shopping_input.php",
		type:"POST",
		cache:false,
		data:{
			prod_id:prod_id,
			prod_type_id:prod_type_id,
			prod_num:num
		}
	}).done(function(data){
		alert(data);
		reflush_cart();
	});
	}
function reflush_cart(){
	$.ajax({
		url:"./includes/controller/shopping_input.php?op=cart_num",
		type:"POST",
		cache:false,
		data:{
		}
	}).done(function(data){
		$("#cart_num").html(data);
	});
}
/*scroll top top*/
$(function(){
	$('.fa-toggle-up').click(function(){
		var $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
		$body.animate({
			scrollTop: $('.navbar-header').offset().top
		}, 600);
		return false;
	});
	
	var max_store_li_re = 0;
	var cnt_store_li_row = 0;
	$(window).resize(function(){
		max_store_li_re = 0;
		$(".store li").each(function(){
			$(this).css("height","");
		});
		cnt_store_li_row = Math.round($(".store ul").width() / ($(".store li").width() + 28));
		if(cnt_store_li_row > 1) {
			$(".stores li").each(function(){
				var tmp_re = $(this).css("height").split("px");
				if((parseInt(tmp_re[0])+28) > max_store_li_re)
					max_store_li_re = (parseInt(tmp_re[0])+28);
			});
			$(".store li").each(function(){
				$(this).css("height",max_store_li_re);
			});
		}
		$(".custom_nav_btn").css("margin-top",(parseInt($(this).width()/13)-17));
		$(".custom_nav_btn").css("margin-bottom",(parseInt($(this).width()/13)-30));
		$(".custom_nav_btn").css("margin-left",(parseInt($(window).width()*$(window).width()/40000)));
		$(".navbar-header").css("height",$("#header_logo").css("height"));
	});
	
	setTimeout(function(){
		var img = document.getElementById('header_logo'); 
		var height = img.clientHeight;
		$(".navbar-header").css("height",height);
		$(".custom_nav_btn").css("margin-top",(parseInt($(this).width()/13)-17));
		$(".custom_nav_btn").css("margin-bottom",(parseInt($(this).width()/13)-30));
		$(".custom_nav_btn").css("margin-left",(parseInt($(window).width()*$(window).width()/40000)));
	}, 10);
	
	var max_store_li = 0;
	var cnt_store_li_row = Math.round($(".store ul").width() / ($(".store li").width() + 28));
	if(cnt_store_li_row > 1) {
		$(".stores li").each(function(){
			var tmp = $(this).css("height").split("px");
			if((parseInt(tmp[0])+28) > max_store_li)
				max_store_li = (parseInt(tmp[0])+28);
		});
		$(".store li").each(function(){
			$(this).css("height",max_store_li);
		});
	}
});
</script>
