//<![CDATA[
jQuery(document).ready(function() {
	    jQuery('a.findItButton').click(function(e){
			window.name = 'main';
		    var target = jQuery(this).attr('href');
		    e.preventDefault();    
			try{
				//Execute room key method
				roomKeyExecute.call();
			}catch(err){
				// doing nothing
			}

			// Set time out interval based on the browser (30 ms for Chrome and 0 for others)
			var roomKeyTimeoutInterval = navigator.userAgent.toLowerCase().indexOf('chrome') > -1 ? 30 : 0;

			if (navigator.userAgent.toLowerCase().indexOf('msie') > -1 ||
			navigator.userAgent.toLowerCase().indexOf('trident') > -1) {
			roomKeyTimeoutInterval = 1000;
			}
			// Redirect to the href value
			// IMPORTANT: This must be wrapped in a timeout
			window.setTimeout(function() {
				document.location.href = target;
				var mainWindow = window.open("",'main');
				mainWindow.focus();
			}, roomKeyTimeoutInterval);
    	});
	    jQuery('a.findItAltButton').click(function(e){
			window.name = 'main';
		    var target = jQuery(this).attr('href');
		    e.preventDefault();    
			try{
				//Execute room key alternate method
				roomKeyAltExecute.call();
			}catch(err){
				// doing nothing
			}

			// Set time out interval based on the browser (30 ms for Chrome and 0 for others)
			var roomKeyTimeoutInterval = navigator.userAgent.toLowerCase().indexOf('chrome') > -1 ? 30 : 0;

			if (navigator.userAgent.toLowerCase().indexOf('msie') > -1 ||
			navigator.userAgent.toLowerCase().indexOf('trident') > -1) {
			roomKeyTimeoutInterval = 1000;
			}
			// Redirect to the href value
			// IMPORTANT: This must be wrapped in a timeout
			window.setTimeout(function() {
				document.location.href = target;
				var mainWindow = window.open("",'main');
				mainWindow.focus();
			}, roomKeyTimeoutInterval);
    	});
	});
	/* Unload the room key popup */
	roomKeyUnload();
//]]>
</script>

