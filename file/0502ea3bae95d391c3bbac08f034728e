//<![CDATA[
var roomKeyExecute = function() {
		if ('' != '' && '' != null) {
			if (jQuery('input[name="searchType"]').val() == 'ALL' && (BRAND.id == 'HP' || BRAND.id == 'HT' || BRAND.id == 'HW' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.luxuryService)){
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', 'http://www3.hilton.com/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', '', 'https://secure3.hilton.com/en_US/gi/reservation/book.htm?ctyhocn=&internalDeepLinking=true');
			} else if (jQuery('input[name="searchType"]').val() == 'PROP' && (BRAND.id == 'HT' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.luxuryService)) {
				// get the ctyhocn from the "Home2 Suites location" dropdown widget as we need to pass selected option to room key
				var ctyhocnDropdownVal = jQuery('#frmfindHotel').find('select[id=hotelSearchOneDropDown]').val();
				var ctyhocn = ctyhocnDropdownVal != null ? ctyhocnDropdownVal : '';
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', 'http://www3.hilton.com/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', ctyhocn, 'https://secure3.hilton.com/en_US/gi/reservation/book.htm?ctyhocn=&internalDeepLinking=true');
			} else if (!(jQuery('input[name="searchType"]').val() == 'PROP' && BRAND.luxuryService)) {
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', 'http://www3.hilton.com/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', '', 'https://secure3.hilton.com/en_US/gi/reservation/book.htm?ctyhocn=&internalDeepLinking=true');
			}
		} else {
			if (jQuery('input[name="searchType"]').val() == 'PROP' && (BRAND.id == 'HT' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.luxuryService)) {
				// get the ctyhocn from the "Home2 Suites location" dropdown widget as we need to pass selected option to room key
				var ctyhocn = jQuery('#frmfindHotel').find('select[id=hotelSearchOneDropDown]').val();
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', 'http://www3.hilton.com/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', ctyhocn, '');
			} else if (jQuery('input[name="searchType"]').val() == 'ALL' || BRAND.id == 'HT' || BRAND.id == 'QQ' || BRAND.id == 'PY' || BRAND.luxuryService) {
				setRoomKeyParameters('frmfindHotel', 'c8fb3636c239e34248d19fc92fbe6d6e', 'http://www3.hilton.com/skins/common/img/logo_brand-GI.gif', 'GI_GW_Home', '', '');
			}
		}
	};
