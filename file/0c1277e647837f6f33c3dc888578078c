   jQuery(function(){
     jQuery('.baoming').click(function(){
    	var uid=jQuery(this).attr('uid');
    	if (uid<1 || uid==0) {
    		location.href='member.php?mod=logging&action=login';
    		return false;
    	}
jQuery('.baoming_pop').show();
 });
 jQuery('.bao_close').click(function(){
jQuery('.baoming_pop').hide();
 });
     })
   
   jQuery('.submit').click(function(){
   var username=jQuery('#username').val();
   var mobile=jQuery('#mobile').val();
         //var address=jQuery('#address').val();
   var tid=jQuery('.baoming_pop').attr('ids');
   if (jQuery.trim(username)=='' || jQuery.trim(username)==null) {
   alert('请输入姓名！');
   jQuery('#username').focus();
   return false;
   }	  
   if (jQuery.trim(mobile)=='' || jQuery.trim(mobile)==null) {
   alert('请输入手机号码');
   jQuery('#mobile').focus();
   return false;
   }
   if (!mobile.match(/^(((1[0-9][0-9]{1})|159|153)+\d{8})$/)) { 
   alert("手机号码格式不正确");  //就弹出提示信息
   jQuery("#mobile").focus();       //输入框获得光标
   return false;         //返回一个错误，不向下执行
   }
   
   /*if (jQuery.trim(address)=='' || jQuery.trim(address)==null) {
   alert('请输入地址');
   jQuery('#address').focus();
   return false;
   }*/
   
                      jQuery.ajax({
   type:'post',
   	   url:url,
   	   success:
   	   function(data) {
   		   if (data=='done') {
   			   alert('请勿重复报名！');
   			   jQuery('.baoming_pop').hide();
   			   jQuery('.baoming_bg').hide();
   			   return false;
   		   }
   		   if(data=='true') {
   			   alert('恭喜您，报名成功！');
   			   jQuery('.baoming_pop').hide();
   			   jQuery('.baoming_bg').hide();
   			   return true;
   		   } else if(data=='false') {
   			   alert('报名失败，请稍后重试！');
   			   return false;
   		   } else {
   			   alert('报名失败，请稍后重试');
   			   return false;
   		   }
   	   }
   })
   return true;
   })
