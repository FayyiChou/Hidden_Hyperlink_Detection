jQuery(function(){
var mi = 1;
jQuery('.manager').on('click',
function () {
if (mi == 1) {
jQuery('.menu-manager').show();
jQuery('.menu-container').hide();
mi = 0;
} else {
jQuery('.menu-manager').hide();
jQuery('.menu-container').show();
mi = 1;
}
})

var mim = 1;
jQuery('.hdc .menu_switch').on('click',function() {
if (mim == 1) {
jQuery('aside').addClass('showMenu');
jQuery('.wrap').addClass('showMenu')
mim = 0;
} else {
jQuery('aside').removeClass('showMenu');
jQuery('.wrap').removeClass('showMenu');
mim = 1;
}
})
})
