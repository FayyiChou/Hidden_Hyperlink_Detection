   <script>
function openVideo(a,b) {
jQuery(a).html('<img src="static/image/mobile/images/icon_load.gif" />');
var w = jQuery(a).width();
var h = w * 0.85;

if(/youku/.test(b)){ //解析优酷
b = b.match(/id_(.*?)\.html|sid\/(.*?)\/v/);
b = 'http://player.youku.com/embed/' + (b[1] ? b[1] : b[2]);
}else if(/bilibili|acg\.tv/.test(b)){ //解析B站
b = b.match(/o\/av(.*?)\/|aid=(.*?)&/);
b = 'http://www.bilibili.com/video/av' + (b[1] ? b[1] : b[2]);
}else if(/acfun.tv/.test(b)){ 		// 解析acfun
b = b.split("/v/");
b = 'http://www.acfun.tv/v/' + (b[1] ? b[1] : b[2]);
h = w * 0.65;
}else{ 								//解析腾讯
b = b.split("url=");
//$(a).html('不支持此视频地址显示：' + b);
//return;
}
jQuery(a).append('<iframe width=0 height=0 src="' + b + '" scrolling=no frameborder=0 allowfullscreen></iframe>');
jQuery(a).children('iframe').load(function(){
jQuery(a).children('img').remove();
jQuery(a).children('iframe').css({'width':w,'height':h});
})
}
</script> 
