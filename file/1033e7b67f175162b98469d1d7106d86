function nextAd(){
  if(adNum<bannerAD.length-1)adNum++ ;
     else adNum=0;
  setTransition();
  document.images.bannerADrotator.src=bannerAD[adNum];
  playTransition();
  theTimer=setTimeout("nextAd()", 5000);
}

