<script>
$(function(){
	var loginName = getcookie("LoginID");
	if(loginName){
	a1 = $(".myCenter").find("a").eq(0);
	a1.attr("href","http://mlogin.hc360.com/manager/mindex.html");
	}
	
	
	
});
//登录
function checkUser(){
	var username = jQuery("#username").val();
	var password = jQuery("#password").val();
	var valicode = jQuery("#valicode").val();
	var nologin = jQuery("#nologinweek").val();
	if(jQuery.trim(username)==''){
		jQuery('[data-query="userNameMsg"]')[0].innerHTML="请输入用户名";
		jQuery('[data-query="userNameMsg"]').show(300).delay(2400).hide(300);
		return;
	}
	if(jQuery.trim(password) ==''){
		jQuery('[data-query="passwordMsg"]')[0].innerHTML="请输入密码";
		jQuery('[data-query="passwordMsg"]').show(300).delay(2400).hide(300);
		return;
	}
	if(jQuery.trim(valicode)==''){
		jQuery('[data-query="checkCodeMsg"]')[0].innerHTML="请输入验证码";
		jQuery('[data-query="checkCodeMsg"]').show(300).delay(2400).hide(300);
		return;
	}
	//登录置灰不可点击
	notclick();
    //调用登录接口
    jQuery.ajax({
       type: "get",
       url: "https://mlogin.hc360.com/mobilelogin",
       data: {username:username,password:password,valicode:valicode,ctoken:ctoken,nologin:nologin},
       dataType: "jsonp",
	   jsonp: "callback",
       success:function(data){
    	   	   console.info(data);
               if("200"==data.code){
            	   //如果登录成功，跳转到特定URL,如果没有ReturnURl,则默认跳转个人中心
					/* a = window.location.href;
					b = "";
					a.indexOf("ReturnURL") > -1 ?(a = a.split("ReturnURL=")[1], b = a) : b = "http://mlogin.hc360.com/manager/index.html";
					window.location.href = b; */
 					window.location.href="http://weshop.mob.hc360.com/msite/turbine/template/transaction,shoppingcart.html";
                }else if("201"==data.code){
                	jQuery(".alertPrompt").html("请输入完整的登录信息").show(300).delay(2400).hide(300);
                	jQuery('[data-query="userNameMsg"]').show(300).delay(2400).hide(300);
                	toclick()
                	loadCaptcha();
                }else if("202"==data.code){
                	jQuery('[data-query="checkCodeMsg"]')[0].innerHTML="请输入正确的验证码";
            		jQuery('[data-query="checkCodeMsg"]').show(300).delay(2400).hide(300);
                	toclick()
                	loadCaptcha();
                }else if("203"==data.code){
                	jQuery('[data-query="userNameMsg"]')[0].innerHTML="用户名或密码错误";
            		jQuery('[data-query="userNameMsg"]').show(300).delay(2400).hide(300);
                	toclick()
                	loadCaptcha();
                }else if("204"==data.code){
                	jQuery('[data-query="userNameMsg"]')[0].innerHTML="您的密码过于简单，3秒后自动跳转重置密码页";
            		jQuery('[data-query="userNameMsg"]').css("display","block");
                	//跳转重置密码页
                	setTimeout("window.location.href = 'http://mlogin.hc360.com/findpwdfir.html?flag='",5000);
                	loadCaptcha();
                }else if("500"==data.code){
                	toclick()
                	jQuery('[data-query="userNameMsg"]')[0].innerHTML="系统错误，请稍后再试";
                	jQuery('[data-query="userNameMsg"]').show(300).delay(2400).hide(300);
                	loadCaptcha();
                }
               
             }
        });
// 	  document.getElementById("formid").submit();
}
//刷新验证码
function loadCaptcha(){
	jQuery.ajax({
		url: "https://sso.hc360.com/LoginTicket.jsp",
		async:true,
		dataType:"jsonp",
		jsonp: "callback",
		jsonpCallback:"success_jsonpCallback",
		success:  function(json){
			if(json!=null){
		
				jQuery("#valicode").val("");
				
			  	jQuery('#ctoken').val(json.key);
			  	jQuery('#validimg').attr("src","https://sso.hc360.com/ValidImage.jsp?seed="+json.value);
			  	ctoken = json.key;
			}
		}
	});
}
//第一次初始化验证码
$(document).ready(function(){
	loadCaptcha();
	jQuery(".close_window").on("click",function(){
		jQuery('[data-query="loginBox"]').css("display","none");
		!window.lockedHtml&&(window.lockedHtml=function(){})
		jQuery(document).off("touchmove",lockedHtml);
	});
});

//校验验证码
jQuery("#valicode").keyup(nextbtncode) ;
function nextbtncode(){
	var cexp = jQuery("#valicode").val().match(/^[0-9]{4}$/);
	var username = jQuery("#username").val();
	var password = jQuery("#password").val();
	if(cexp && jQuery.trim(username)!='' && jQuery.trim(password) !=''){
		jQuery.ajax({
			"url": "https://sso.hc360.com/ValidJsonCode.jsp?ctoken="+ctoken+"&validCode="+jQuery("#valicode").val(),
			"data": "",
			"timeout":3000,
			"dataType": "jsonp",
			"jsonp": "callback",
			"success":function(data){
				if(data.msg!="1"){
					jQuery(".alertPrompt").html("验证码有误").show(300).delay(2400).hide(300);
					jQuery(".regBtnGray").show();
					jQuery(".regBtn").hide();
				}
			}  
		});
		
	}
}


function notclick(){
	jQuery("#loginButton").attr("disabled","disabled");
	jQuery("#loginButton").removeClass("mBtnLogin");
	jQuery("#loginButton").addClass("grayBtn");
}
function toclick(){
	jQuery("#loginButton").removeAttr("disabled");
	jQuery("#loginButton").removeClass("grayBtn");
	jQuery("#loginButton").addClass("mBtnLogin");
}


</script>
