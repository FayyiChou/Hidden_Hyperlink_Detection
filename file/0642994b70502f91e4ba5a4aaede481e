		            }else{
		                jQuery("#Minfo").hide();
		            }
		        },
		        error:function(result){
		        	jQuery("#Minfo").hide();
		        	console.info("获取商机资讯错误");
		        }

		    });  
	  }
		  
	});
	
	
	
	


