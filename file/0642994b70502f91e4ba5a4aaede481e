<script>
//--------------------------2016.8.10 公共头吸附功能 beginning---------------------------
(function(M,doc,win){
    M(function(){
        setTimeout(function(){
            var mHeader=M("header");
            var mH=mHeader.offset();
            var mEle=M('<div style="width:20%;height:2.322rem"></div>');
            var flag;
            var mBody=M("body");
            doc.addEventListener("scroll",function(){

                var h=mH.top;
                var scrollH=mBody.scrollTop();
                if(scrollH>h){
                    if(!flag){
                        mHeader.css({
                            "position":"fixed",
                            "top":0,
                            "left":0,
                            "z-index":8888
                        });
                        mHeader.after(mEle);
                        flag=true;
                    }
                }else{
                    if(flag){
                        mHeader.css({
                            "position":"relative"
                        });
                        flag=false;
                        mEle.remove();
                    }

                }
            });
        });

    },300);

})(window.Zepto||window.jQuery,document,window);
//--------------------------2016.8.10 公共头吸附功能 ending---------------------------
</script>
