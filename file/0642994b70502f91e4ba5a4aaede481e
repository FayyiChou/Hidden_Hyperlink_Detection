//设定倒数秒数  ,10秒内不可重复点击
var click = 10;  
//显示倒数秒数  
function notClick(){
	  $('#publish').css("color","gray");
    click -= 1;  
    if(click==0){  
  	  $('#publish').removeAttr("disabled");
  	  $('#publish').css("color","");
  	  click = 10; 
		  //清空表单
		  clearNum();
    }else{
  	  //每秒执行一次,showTime()  
        setTimeout("notClick()",1000);  
    }  
   
}  


//长度验证
function checkLenth(obj,n){
	var value = $(obj).val();
	valuetirm = jQuery.trim(value);
	var length = value.length;
	//假设长度限制为n
	if(length>n){
    		//截取前n个字符
    		value = value.substring(0,n);
    		alert(length);
    		jQuery(obj).attr("value",value);
	}
	return;
}
//采购数量校验
function checkNum(obj){
	var value = $(obj).val();
	if(jQuery.trim(value) == ''){
		jQuery("#numCheck").show(0).delay(800).hide(0);
		return false;
	}else{
		jQuery("#numCheck").hide();
		return true;
	}  
}
//手机号验证
function checkMobile(obj){ 
    var sMobile = $(obj).val();
    if(jQuery.trim(sMobile) == ''){
    	$(obj).val('请输入您的手机号码');
    	$(obj).css("color","gray");
    }
    if(!(/^1[2-9][0-9]\d{8}$/.test(sMobile))){ 
    	jQuery("#phoneCheck").show(0).delay(800).hide(0);
    	return false;
	}else{
		jQuery("#phoneCheck").hide();
		return true;
	}  
} 
//只能录入数字，默认为1
function checkProductNum(obj){
    obj.value = obj.value.replace(/[^\d]/g,"");
    if(parseInt(obj.value) <= 0 ){obj.value = 1;}
}
//采购数量
function numAdd(){
	 var num = jQuery("input[rel=txtInquiryNum]").val();
	 if(num<9999999999){
		 num++;
		 jQuery("#numCheck").hide();
	 }
	 jQuery("input[rel=txtInquiryNum]").val(num)
	 
}

function numMin(){
	 var num = jQuery("input[rel=txtInquiryNum]").val();
	 if(num>1){
		 num--;
		 jQuery("#numCheck").hide();
	 }
	 jQuery("input[rel=txtInquiryNum]").val(num)
}

function clearNum(){
	 jQuery("input[rel=txtInquiryNum]").val(1);
	 jQuery("input[rel=txtTelphone]").val('请输入您的手机号码');
	 jQuery("#phoneCheck").hide();
}
function alertInquiryInfo(mtype){
	  var u=navigator.userAgent;
	  var isiOS = !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X/);
	  var isUC=navigator.userAgent.indexOf("UCBrowser")!=-1;
	  jQuery(".InquiryBox").show();
	  jQuery(".InquiryBg").show();

	  document.addEventListener("touchmove",lockedHtml,false);
	  if(!(isiOS && isUC)){
	  jQuery("input[rel=txtTelphone]").focus();
		  
	  }
	  jQuery("#phoneCheck").hide();
	  $('#fibHref').attr('onclick','saveInquiryInfo('+mtype+')');
	  jQuery("#fibHref").removeClass("grayBtn");
	  sendUserlogsElement('UserBehavior_m_supplyself_bargaining?detailuserid=59133878&detailbcid=611921117');
	  
}
var t1;
function saveInquiryInfo(mtype){
	if(t1 == null){t1 = +(new Date());}else{var t2 = +(new Date());if(t2 - t1 < 500){t1 = t2;return;}else{t1 = t2;}}
	  jQuery(".InquiryBg").show();	  
	 
	 //从发送询价单的表单中获取
    var inquiryNum = jQuery("input[rel=txtInquiryNum]");
    var telPhone = jQuery("input[rel=txtTelphone]");
	
    var flag1 = checkNum(inquiryNum);
	  var flag2 = checkMobile(telPhone);
	  if(!flag1||!flag2){
	    	return;
	   }
    
    var inquiryParamVO = new Object();
    
    inquiryParamVO.inquiryNum = inquiryNum.val();
    inquiryParamVO.telPhone = telPhone.val();
    inquiryParamVO.type = mtype;
    inquiryParamVO.sellerProviderId='100027393832';
    inquiryParamVO.businId='611921117';
    inquiryParamVO.supcatId='009146';
    inquiryParamVO.areaid='059';
    inquiryParamVO.comeUrl='http://m.hc360.com/supplyself/611921117.html';
    inquiryParamVO.businTitle=encodeURIComponent('欧立华EPS模块 空腔模块 保温幕墙板 保温墙体 冷库墙体 节能抗震材料');
    inquiryParamVO.buyerSourceId='u_m_short_inquiry';
    $('#fibHref').removeAttr('onclick')
    jQuery("#fibHref").addClass("grayBtn");
    console.info(jQuery("#publish").disabled);
    console.info(inquiryParamVO);
    var url = "http://my.b2b.hc360.com/my/turbine/action/inquiry.InquiryActionM/eventsubmit_doPerform/doperform?callback=?";
    jQuery.ajax({
        type:"POST",
        url:url,
        dataType:"jsonp",
        contentType: "application/x-www-form-urlencoded; charset=utf-8",
        data:inquiryParamVO,
        timeout:2000,
        async:false,
        success:function(result){
            if(result && result.code){
                if(result.code == "null"){
                    alert("系统异常，请重试");
                }else if(result.code == "yes"){
                    alert("发布成功，\n请保持电话畅通以保证卖家联系到您");
                    //2秒后关闭窗口
                    showTime();
                    //10秒内按钮不可点击
//                    notClick();
                }else if(result.code == "self"){
                    alert("您不能给自己发询价单");
                }
            }
        },
        error:function(){
            alert("系统异常，请重试");
        }
    });
};
//获取cookie
function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
    }
    return "";
}
//商机收藏
function saveBusin(){
	var cookieStr = getCookie("HC360.SSOUser");
	if(cookieStr == ''){
		var paramUrl = 'http://m.hc360.com/supplyself/611921117.html';
		window.location.href="https://mlogin.hc360.com/login.html?ReturnURL="+paramUrl;
	}else{
		//var url = "http://my.b2b.hc360.com/my/turbine/action/favorites.FavoritesAction/eventsubmit_doAddinfonew/doAddinfonew?infoid=100001790735&infotype=9";
		var url = "http://my.b2b.hc360.com/my/turbine/action/favorites.FavoritesAction/eventsubmit_doAddinfonew/doAddinfonew?infoid=611921117&infotype=0";

		jQuery.ajax({
		       type:"GET",
		       url:url,
		       dataType:"jsonp",
		       contentType: "application/x-www-form-urlencoded; charset=utf-8",
		       timeout:2000,
		       async:false,
		       jsonp:'jsoncallback',
		       success:function(result){
		           if(result && result.code){
		               if(result.code == "007"){
		            	   jQuery(".alertPrompt").html("收藏成功").show(300).delay(2400).hide(300);

		            	   $("#saveBusinId").hide();
		            	   $("#cancelBusinId").show();
		               }else if(result.code == "012"){
		            	   jQuery(".alertPrompt").html("对不起，您收藏的商铺信息已达到上限，暂不能进行操作！").show(300).delay(2400).hide(300);
		               }else if(result.code == "006"){	            	   
		            	   jQuery(".alertPrompt").html("您想收藏的信息已经存在！").show(300).delay(2400).hide(300);
		               }else if(result.code == "005"){
		            	   jQuery(".alertPrompt").html("您收藏的信息不存在，收藏失败！").show(300).delay(2400).hide(300);
		               }else{
		            	   
		               }
		           }
		       },
		       error:function(){
		           alert("系统异常，请重试");
		       }
		   });
	}
	
};
function cancelBusin(){
	
	
	var url = "http://my.b2b.hc360.com/my/turbine/action/favorites.FavoritesAjaxAction/eventsubmit_doDelfavorite/doDelfavorite?id=611921117&infotype=0";
	jQuery.ajax({
	       type:"GET",
	       url:url,
	       dataType:"jsonp",
	       contentType: "application/x-www-form-urlencoded; charset=utf-8",
	       timeout:2000,
	       async:false,
	       jsonp:'jsoncallback',
	       success:function(result){
	           if(result && result.code){
	               if(result.code == "0"){
	            	   jQuery(".alertPrompt").html("已取消收藏").show(300).delay(2400).hide(300);
	            	   $("#saveBusinId").show();
	            	   $("#cancelBusinId").hide();
	            	   
	               }
	           }
	       },
	       error:function(){
	           alert("系统异常，请重试");
	       }
	   });
};
function onlineService(companyName){
	url="http://m.hc360.com/supplyself/611921117-onlineservice.html";
	window.location.href=url;
}

