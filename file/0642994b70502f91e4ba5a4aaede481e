$(function(){
	jQuery.ajax({
		url: "http://weshop.mob.hc360.com/msite/turbine/action/ajax.MCartAjax/eventsubmit_doGetcartcount/doGetcartcount",
		data: "",
		timeout:3000,
		dataType: "jsonp",
		jsonp: "callback",
		success:function(data){
			if(data.cartCount != '0'){
				jQuery('[data-query="addCartIcon"]').html("<em>"+data.cartCount+"</em>");	
			}
		
		}  
	});
})
