<!-- 拖拽跳转图文详情页 -->
<script>
    (function($,win,doc){
        var start,end,slideNum,winH,bodyH,
                isUC=navigator.userAgent.indexOf("UCBrowser")!=-1,
                isQQBrowser=navigator.userAgent.indexOf("MQQBrowser")!=-1,
                bodyEle=doc.querySelector("body"),
                docEle=doc.documentElement;

        slideNum=isUC?8:70;

        doc.addEventListener("touchstart",touchStartHandle,false);
        doc.addEventListener("touchend",touchEndHandle,false);
        doc.addEventListener("touchmove",function(evt){
            touchEndHandle(evt)
        },false);

        function touchStartHandle(evt){
            start=evt.touches[0].pageY
        }

        function touchEndHandle(evt){
        	if(jQuery.BUDS)return;
            var scrollTop=bodyEle.scrollTop;
            winH=docEle.clientHeight;
            bodyH=docEle.scrollHeight;
            end=evt.changedTouches[0].pageY;
            if(start-end>slideNum&&scrollTop+winH>=bodyH){
                window.location.href='http://m.hc360.com/supplyself/611921117-detail.html'
            }
        }

    })(window.Zepto||window.jQuery,window,document);

</script>


<script>
/*屏蔽词跳转功能-start*/
var badWordUrl = "http://wsdetail.b2b.hc360.com/checkAjax";
//if(typeof checkon!=="undefined"&&checkon){
	goToOtherPage();
//}
function goToOtherPage(){
	var param = {supplyBcId:'611921117'};

	jQuery.ajax({
		"url": badWordUrl,
		"data": param,
		"timeout":3000,
		"dataType": "jsonp",
		"jsonp": "jsoncallback",
		"success":function(data){
			if(data.success){
				window.location.href="http://m.hc360.com/forbidden.html";
			}
		}
	});
}


</script>



