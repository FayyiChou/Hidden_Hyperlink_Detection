<div id="divx0" style="position:fixed;_position:absolute;">
<a href="http://www.dgjy.net/moreInfo.aspx?menuId=1634" target="_blank">
<img src="./images/uploadfiles/201702/20170217110546_2127.jpg" title="社会主义核心价值观" border="0" width="400" height="213" />
</a><a href="javascript:closedivx0();" class="adclose">close</a>
</div>

<script type="text/javascript"> 
var itl;
var x = 0,y = 0 
	var xin = true, yin = true 
	var step = 1 
	var delay = 20 
	var obj=$("#divx0") 
	obj.find('img').load(function(){
		var float = function(){
			var L = T = 0;
			var OW = obj.width();//当前广告的宽
			var OH = obj.height();//高
			var DW = $(document).width(); //浏览器窗口的宽
			var DH = $(window).height(); 
			 x = x + step *( xin ? 1 : -1 ); 
			if (x < L) { 
				xin = true; x = L
			} 
			if (x > DW-OW-1){//-1为了ie
				xin = false; x = DW-OW-1
			} 
			y = y + step * ( yin ? 1 : -1 ); 
			if (y > DH-OH-1) { 
				yin = false; y = DH-OH-1 ;
			}
			if (y < T) {        
				yin = true; y = T
			} 
			var left = x ; 
			var top = y; 
			obj.css({'top':top,'left':left});
		}
		itl = setInterval(float,delay);
		obj.mouseover(function(){clearInterval(itl)}); 
		obj.mouseout(function(){itl=setInterval(float, delay)} )
	});
	
function closedivx0()
{
	obj.hide();
	obj.mouseout=function(){}
	clearInterval(itl);
}
</script>
