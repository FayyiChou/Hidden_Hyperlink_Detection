<script language="javascript">

function nonull()
{
    if (document.all._ctl30_KeyTextBox.value == "")
	{
		alert("请输入搜索内容");
		document.all._ctl30_KeyTextBox.focus();
		return false;
	}else{
	    return true ;
	}
}

function showDetail(pkId)
{
    w=600;
    h=520;
    LeftPosition = (screen.width) ? (screen.width-w)/2 : 0;
    TopPosition = (screen.height) ? (screen.height-h)/2 : 0;
    settings ='height='+h+',width='+w+',top='+TopPosition+',left='+LeftPosition+',scrollbars='+scroll+',toolbar=no,menubar=no,scrollbars=yes,resizable=yes,location=no,status=no';
    window.open("MessageBoardDetail.aspx?pkId="+pkId,"" ,settings);

}

function showRelease(bigclassid,menuid)
{
    w=600;
    h=520;
    LeftPosition = (screen.width) ? (screen.width-w)/2 : 0;
    TopPosition = (screen.height) ? (screen.height-h)/2 : 0;
    settings ='height='+h+',width='+w+',top='+TopPosition+',left='+LeftPosition+',scrollbars='+scroll+',toolbar=no,menubar=no,scrollbars=yes,resizable=yes,location=no,status=no';
    window.open("MessageBoardRelease.aspx?bigclassid="+bigclassid+"&menuid=" + menuid,"" ,settings);

}

function showSearchDetail(name, pwd)
{
	if (name == "")
	{
		alert("请输入查询编号");
		document.all.name.focus();
		return false;
	}
	
	if (pwd == "")
	{
		alert("请输入查询密码");
		document.all.pwd.focus();
		return false;
	}
	
    w=448;
    h=440;
    LeftPosition = (screen.width) ? (screen.width-w)/2 : 0;
    TopPosition = (screen.height) ? (screen.height-h)/2 : 0;
    settings ='height='+h+',width='+w+',top='+TopPosition+',left='+LeftPosition+',scrollbars='+scroll+',toolbar=no,menubar=no,scrollbars=yes,resizable=yes,location=no,status=no';
    window.open("MessageBoardDetail.aspx?name="+name+"&pwd="+pwd ,"" ,settings);

    document.all.pwd.value = "";
    document.all.name.value = "";
}

function CheckSearch()
{
	if (document.all._ctl30_NameTextBox.value == "")
	{
		alert("请输入查询编号");
		document.all._ctl30_NameTextBox.focus();
		return false;
	}
	
	if (document.all._ctl30_PwdTextBox.value == "")
	{
		alert("请输入查询密码");
		document.all._ctl30_PwdTextBox.focus();
		return false;
	}
	return true;
}
</script>
 
