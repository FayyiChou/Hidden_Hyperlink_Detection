  <script>
		var cid = 4650,pid=10060,json_finance = [{"round":"A轮","money":"100 万美元","unit_money":100,"time":1351699200000,"investor":[{"id":-1,"name":"\r\n\t\t\t\t\t\t\t\t\t\t不公开的投资者\r\n\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t\t\t"}]}],project_data_name ='';
		;var svg_circle = function(percents,color1, color2,r1,width1,fill_color){
    var color = color1;
    var color2 = color2;
    fill_color = fill_color||'white';
    var r = r1||25,width=width1||10,fontsize=27;
    var hw = width/2;
    var html = [];
    html.push('<svg xmlns="http://www.w3.org/2000/svg" width="',(r*2+width),'" height="',(r*2+width),'">');
    html.push('<circle fill="',fill_color,'" r="',r,'" cx="',(r+hw),'" cy="',(r+hw),'" style="stroke:',color2,';stroke-width:',width,'px"></circle>');
    if(percents>=100){
        html.push('<circle fill="',fill_color,'" r="',r,'" cx="',(r+hw),'" cy="',(r+hw),'" style="stroke:',color,';stroke-width:',width,'px"></circle>');
    }else if(percents==0){
        html.push('<circle fill="',fill_color,'" r="',r,'" cx="',(r+hw),'" cy="',(r+hw),'" style="stroke:',color2,';stroke-width:',width,'px"></circle>');
    }else{
        var ofx = (percents+'').length*(fontsize/2)/2,ofy = fontsize/2;
        var du = parseInt(percents*360/100);
        var m = [r,0];
        var a = [r,r,0,(du>180?1:0),1,0,100];
        var pi = du/180*Math.PI;
        var of1 = Math.sin(pi)* r,of2 = Math.cos(pi)*r;

        if(du<=90){
            a[5] = r+of1;
            a[6] = r-of2;
        }else{
            a[5] = r+of1;
            a[6] = r-of2;
        }

        $.each(m,function(i,e){
            m[i]+=hw;
        });
        $.each(a,function(i,e){
            if(i==5||i==6) {
                a[i] += hw;
            }
        });

        var d = ['M',m.join(','),' ','A', a.join(',')].join('');

        a[3] = 1-a[3];
        a[4] = 1-a[4];
        var d2 = ['M',m.join(','),' ','A', a.join(',')].join('');
        if(du>180){
            html.push('<path fill="',fill_color,'" d="',d,'" style="stroke:',color,';stroke-width:',width,'px"></path>');
            html.push('<path fill="',fill_color,'" d="',d2,'" style="stroke:',color2,';stroke-width:',width,'px"></path>');
        }else{
            html.push('<path fill="',fill_color,'" d="',d2,'" style="stroke:',color2,';stroke-width:',width,'px"></path>');
            html.push('<path fill="',fill_color,'" d="',d,'" style="stroke:',color,';stroke-width:',width,'px"></path>');
        }

    }

    html.push('</svg>');
    return html.join('');
};

		;
'use strict';
var templateSettings = {
	evaluate: /<#([\s\S]+?)#>/g,
	interpolate: /<#=([\s\S]+?)#>/g,
	escape: /<#-([\s\S]+?)#>/g
};

//data-score
$(function(){
    $('[data-score]').each(function(){
        var $this = $(this);
        var score=parseInt($this.data("score"));
        var color = $this.data("score-color")||'#22ab38';
        var color2 = $this.data("score-color2")||'#b4b4b5';
        var fill = $this.data("score-fill");
        var width = parseInt($this.data("score-width"))||6;
        var l = parseInt($this.data("score-l"))||80;
        var r = (l-width)/2;
        $this.find('svg').remove();
        $this.prepend(svg_circle(score/10,color,color2,r,width,fill));
    });
});

/* finance echarts */
$(function(){
    var $echart=$('#chart_finance'),json = json_finance;
    !function init(){
        draw();
    }();

    function offsetMonth(a,b){
        var by = b.getFullYear(),bm = b.getMonth();
        var ay = a.getFullYear(),am = a.getMonth();

        return (by - ay)*12+bm-am;
    }

    function getSize(minmoney,maxmoney,money){
        var minsize = 20;
        var maxsize = 100;
        if(money==0){
            return 50;
        }
        if(maxmoney==minmoney){
            return 50;
        }
        return minsize + (maxsize-minsize)*Math.sqrt((money-minmoney)/(maxmoney-minmoney));
    }

    function draw(){
        if($echart.length <= 0){
            return ;
        }

        var myChart = echarts.init($echart.get(0));
        var times = [],maxtime= 0,mintime= 0,minmoney= 0,maxmoney=0;

        for(var i=0;i<json.length;i++){
            var time = json[i].time;
            if(maxtime==0){
                maxtime = time;
            }else{
                maxtime = Math.max(maxtime,time);
            }

            if(mintime==0){
                mintime = time;
            }else{
                mintime = Math.min(mintime,time);
            }

            var money = json[i].unit_money;
            if(maxmoney==0){
                maxmoney = money;
            }else{
                maxmoney = Math.max(maxmoney,money);
            }

            if(minmoney==0){
                minmoney = money;
            }else{
                minmoney = Math.min(minmoney,money);
            }
        }

        var mindate = new Date(mintime),maxdate = new Date(maxtime);
        mindate.setMonth(mindate.getMonth()-1);
        maxdate.setMonth(maxdate.getMonth()+1);
        var tmp=new Date(mindate.getTime());
        do{
            var year = tmp.getFullYear();
            var month = tmp.getMonth()+1;
            times.push(year+"/"+month);
            tmp.setMonth(tmp.getMonth()+1);
        }while((tmp.getFullYear()*100 + tmp.getMonth()) <= (maxdate.getFullYear()*100 + maxdate.getMonth()));

        //json

        var data = [];

        for(var i=0;i<json.length;i++){
            data.push({
                name:json[i].round,
                data:json[i],
                value:[offsetMonth(mindate,new Date(json[i].time)),getSize(minmoney,maxmoney,json[i].unit_money)]
            });
        }

        var option = {
            grid:{
                containLabel:true
            },
            tooltip: {
                position: 'top',
                formatter:function(params){
                    var data = params.data.data;
                    return data.round+':'+data.money;
                }
            },
            singleAxis: [{
                bottom:20,
                left: 50,
                right:50,
                type: 'category',
                //boundaryGap: false,
                data: times,
                //top: '0%',
                height: '100%',
                axisLabel: {
                    show:true,
					textStyle:{
						color:'#404040'
					}
                },
                axisLine:{
                    show:true,
                    lineStyle:{
                        color:'#eee',
                        width:1
                    }
                },
                axisTick:{
                    show:false
                },
				splitLine:{
					show:true,
					lineStyle:{
                        color:'#ccc',
                        width:1
                    }
				}
            }],
            series: [{
                singleAxisIndex: 0,
                coordinateSystem: 'singleAxis',
                type: 'scatter',
                data: data,
                symbolSize: function (dataItem) {
                    return dataItem[1];
                },
                itemStyle:{
                    normal:{
                        color:'#5db6e7'
                    }
                }
            }]
        };
        myChart.setOption(option);
    }
});

//team box open
$(function(){
	var $box = $('#team_box');
	$box.on('click','.j-open',function(){
		var $this = $(this),$item = $this.parents('._item_'),$exp = $item.find('.j-exp');
		$this.css("visibility","hidden");
		$exp.show();
	});
	$box.on('click','.j-close',function(){
		var $this = $(this),$item = $this.parents('._item_'),$exp = $item.find('.j-exp'),$open = $item.find('.j-open');
		$open.css("visibility","visible");
		$exp.hide();
		$(window).scrollTop($item.offset().top-120);
	});
});

//similar tab switch
$(function(){
	var $box = $('#similar_tab'),$line = $box.find('div'),on='details_1221_d09_d01_d01_a01',$items=$('.j-similar');
	$box.on('click','a',function(){
		var $this = $(this),index=$this.index();
		if(!$this.hasClass(on)){
			return;
		}
		var left = $this.offset().left-$this.parent().offset().left;
		$line.show().animate({"left":left+"px","width":$this.width()+"px"},'fast',function(){
			$this.removeClass(on).siblings('a').addClass(on);
			$items.hide().eq(index).show();
		});
	});
});

//fianance op
$(function(){
    var $open = $('#btn_finance_more_open'),$close = $('#btn_finance_more_close'),$list=$('#finance_list'),scrollTop;
    $open.on('click',function(){
       $list.show();
        $open.hide();
        $close.show();
        scrollTop = $(window).scrollTop();
    });
    $close.on('click',function(){
        $list.hide();
        $open.show();
        $close.hide();
        $(window).scrollTop(scrollTop);
    });
});

