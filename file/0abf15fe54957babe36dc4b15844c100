    var $open = $('#business_open'),$close = $('#business_close'),$show = $('.j-business-show'),scrollTop;
    $show.hide();
    $open.on('click',function(){
        $show.show();
        $open.hide();
        $close.show();
        scrollTop = $(window).scrollTop();
    });
    $close.on('click',function(){
        $show.hide();
        $open.show();
        $close.hide();
        $(window).scrollTop(scrollTop);
    });
});

//follow company
$(function(){
    var $btn = $('#btn_follow'),$title=$btn.find('span'),doing=false;
    var txt_follow = '\u5df2\u6536\u85cf';//已收藏
    var txt_follow_fail = '\u6536\u85cf\u5931\u8d25';//收藏失败
    var txt_follow_fail2 = '\u8d85\u65f6\u6216\u8005\u670d\u52a1\u9519\u8bef';//超时或者服务错误
    $btn.on('click',function(){
        if(doing){
            return;
        }
        doing = true;
        $.ajax({
            url:'/ajax/follow/company',
            data:{
                id:cid,
                type:'c'
            }
        }).done(function(ret){
            if(ret.code==0){
                $title.html(txt_follow);
                $btn.addClass('details_1221_d01_02_d03_a01_ho');
            }else{
                alert(txt_follow_fail);
                doing = false;
            }
        }).fail(function(){
            alert(txt_follow_fail2);
            doing = false;
        });
    });
});

