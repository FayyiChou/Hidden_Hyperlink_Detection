'use strict';
var templateSettings = {
	evaluate: /<#([\s\S]+?)#>/g,
	interpolate: /<#=([\s\S]+?)#>/g,
	escape: /<#-([\s\S]+?)#>/g
};
//project data ehcarts
$(function(){
	function formatNumber(num){
        return (num+'').replace(/\d{1,3}(?=(\d{3})+(\.\d*)?$)/g, '$&,');
    }
    function showmohunum(num){
        //return num;
        num = parseInt(num);
        var numstr = num.toFixed(0);
        var newstr = numstr[0];
        for(var i=1;i<numstr.length;i++){
            newstr+="0";
        }
        var newnum = parseInt(newstr);
        if(newnum>=100000000){
            return newnum/100000000+"\u4ebf+";//亿
        }else if(newnum>=10000){
            return newnum/10000+"\u4e07+";//万
        }else if(newnum>=1000){
            return newnum/1000+"\u5343+";//千
        }else{
            return "1\u5343\u4ee5\u4e0b";//千以下
        }
    }
    function showtimenum(num){
        //XX小时XX分XX秒
        var seconds = num;
        var h = Math.floor(seconds/3600);
        seconds = seconds - h*3600;
        var m = Math.floor(seconds/60);
        seconds = seconds - m*60;
        var html = [];
        if(h>0){
            html.push(h,'\u5c0f\u65f6');//小时
        }
        if(m>0){
            html.push(m,'\u5206');//分
        }
        if(seconds>0){
            html.push(seconds,'\u79d2');//秒
        }
        if(!html.length){
            html.push(seconds,'\u79d2');//秒
        }
        return html.join('');
    }

    var id=pid,$box=$('#project_data'),$position = $('#data_position'), $one_project = $('#one_project'),json,tpl_project_data_item= _.template($('#tpl_project_data_item').html(),templateSettings);
	var map=[{name:"\u6d4f\u89c8\u9875\u9762\u6570(PV)",key:"pv",tip:"\u5bf9\u76ee\u6807\u516c\u53f8\u7f51\u7ad9\u5728\u5168\u7f51\u7684\u65e5\u5747\u8bbf\u95ee\u91cf\u52a0\u603b\uff0c\u5e76\u7ecf\u8fc7\u52a0\u6743\u8ba1\u7b97\u7684\u503c\u3002\u503c\u8d8a\u9ad8\uff0c\u8bbf\u95ee\u91cf\u8d8a\u5927\u3002\u8be5\u6307\u6807\u53cd\u6620\u4e86\u7528\u6237\u5bf9\u76ee\u6807\u516c\u53f8\u5b98\u65b9\u7f51\u7ad9\u7684\u8bbf\u95ee\u60c5\u51b5\uff0c\u662f\u53ef\u4ee5\u8868\u660e\u76ee\u6807\u516c\u53f8\u7f51\u7ad9\u5185\u5bb9\u53d7\u8bbf\u70ed\u5ea6\u7684\u6307\u6807\u3002"},
			{name:"\u8bbf\u95ee\u7528\u6237\u6570(UV)",key:"uv",tip:"\u5bf9\u76ee\u6807\u516c\u53f8\u7f51\u7ad9\u5728\u5168\u7f51\u7684\u65e5\u5747\u72ec\u7acb\u8bbf\u95ee\u7528\u6237\u91cf\u52a0\u603b\uff0c\u5e76\u7ecf\u8fc7\u52a0\u6743\u8ba1\u7b97\u7684\u503c\u3002\u503c\u8d8a\u9ad8\uff0c\u72ec\u7acb\u8bbf\u95ee\u7528\u6237\u6570\u8d8a\u591a\u3002\u8be5\u6307\u6807\u53cd\u6620\u4e86\u76ee\u6807\u516c\u53f8\u5b98\u65b9\u7f51\u7ad9\u5438\u5f15\u7528\u6237\u7684\u80fd\u529b\uff0c\u662f\u53ef\u4ee5\u8868\u660e\u76ee\u6807\u516c\u53f8\u7f51\u7ad9\u7684\u83b7\u5ba2\u80fd\u529b\u7684\u6307\u6807\u3002"},
			{name:"\u8bbf\u95ee\u65f6\u957f",key:"time",tip:"\u6240\u6709\u8bbf\u95ee\u7528\u6237\u5728\u7edf\u8ba1\u65f6\u95f4\u6bb5\u5185\uff0c\u4ece\u8fdb\u5165\u5230\u79bb\u5f00\u8be5\u7f51\u7ad9\u7684\u65f6\u957f\u5e73\u5747\u503c\u3002\u662f\u53ef\u4ee5\u8868\u73b0\u516c\u53f8\u7f51\u7ad9\u7528\u6237\u7c98\u6027\u7684\u6307\u6807\u3002"},
			{name:"APP\u4e0b\u8f7d\u91cf",key:"total_download",tip:"\u7d2f\u8ba1\u4e0b\u8f7d\u91cf\uff1a\u7efc\u5408\u5404\u5927\u5e94\u7528\u5e02\u573a\u7684\u5386\u53f2\u7d2f\u8ba1\u4e0b\u8f7d\u91cf\uff0c\u52a0\u6743\u8ba1\u7b97\u540e\u7684\u503c\u3002\u8be5\u6307\u6807\u662f\u53ef\u4ee5\u8868\u660eAPP\u5b58\u91cf\u7528\u6237\u91cf\u7684\u6307\u6807\u3002"},
			{name:"\u65e5\u6d3b\u8dc3\u7528\u6237\u6570\uff08DAU\uff09",key:"dau",tip:"\u65e5\u6d3b\u8dc3\u7528\u6237\u6570\uff0c\u5373\u6bcf\u65e5\u8bbf\u95ee\u8fc7APP\u7684\u7528\u6237\u6570\u3002"}];

	var getConfig = function(key){
		return _.findWhere(map,{key:key});
	};

	var getOrder = function(){
		var order1=['dau','total_download','pv','uv','time'],order2 = ['pv','uv','time','dau','total_download']
		return function(data){
            var uv = data.uv,dau = data.dau;
            if(uv && uv[0] && !uv[0].is_empty && dau && dau[0] && !dau[0].is_empty){
                uv = uv[0].value;
                dau = dau[0].value;
                uv = uv[uv.length-1];
                dau = dau[dau.length-1];
                if(uv>dau && uv>dau*10){
                    return order2;
                }
            }
			return order1;
		};
	}();

    function getmid(key){
        return json.data[key+"_mid"];
    }

    var getItemHTML = function(){
        var _pid = pid,_cid = cid,name='';
        if(typeof project_data_name != 'undefined'){
            name = project_data_name;
        }
        return function(obj){
            return tpl_project_data_item({obj:obj,pid:_pid,cid:_cid,name:name});
        }
    }();

    function getItemHTML(obj,pid){
        return tpl_project_data_item({obj:obj,pid:pid});
    }
    var item_index = 0;
    function addItem($item){
		$item.insertBefore($position);
        item_index++;
    }

    var echart_arr={};
    function getechart(key){
        var echart = echart_arr[key];
        if(echart){
            //return echart;
            echart.dispose();
        }
        var $echart = $('#'+key+"_container ._chart_");
        echart = echarts.init($echart[0]);
        echart_arr[key] = echart;
        return echart;
    }
    var colors = ['#6FBA2C','#e84d3d','#9b59b5','#3598db','#2abb9a','#ea9532'];
    function cutstring(str,maxLen,appended,appendLength) {
        if( appended != '' )
            appended = appended||"...";
        if( appendLength != 0 )
            appendLength = appendLength||2;
        var l = str.replace(/[^\x00-\xff]/g,"rr").length;
        if(l>maxLen){
            do{
                str = str.substring(0, str.length-1);
                l = str.replace(/[^\x00-\xff]/g,"rr").length;
            }while(str && (l+appendLength > maxLen));
            return str+appended;
        }
        return str;
    }
    function draw(key,time,data,other){
        var echart = getechart(key);
        var mid = getmid(key);

        var series = [],legend=[],markLine = [];
        for(var i=0;i<data.length;i++){
            data[i].short_name = cutstring(data[i].name,16,'',0);
            if(data.length<5){
                data[i].short_name = data[i].name;
            }
            var item = {
                name:data[i].short_name,
                type:'line',
                symbol:'circle',
				symbolSize:'5',
                smooth:true,
                itemStyle:{
                    normal:{
                        color:colors[i]
                    }
                },
				lineStyle:{
					normal:{
						width:1
					}
				},
                data: data[i].value

            };
            if(i==0){
                item.markLine= {
                    symbol: ['none', 'none'], data: markLine, tooltip: {
                        show: true, formatter: function (obj) {
                            return obj.name+'\uff1a'+obj.value;
                        }
                    }
                }
            }
            legend.push({name:data[i].short_name, icon:'pin'});
            series.push(item);
        }

        var option =  {
            tooltip : {
                trigger: 'axis',
                formatter:function(arr){
                    var html = [];
                    html.push(arr[0].name,'<br>');
                    if(mid.length){
                        var value = mid;
                        if(key=='dau' || key == 'mau'){
                            value = showmohunum(value);
                        }else if(key=='time'){
                            value = showtimenum(value);
                        }else if(key=='cost'){
                            value = formatNumber(value)+'\u5143/UV';//元
                        }else{
                            value = formatNumber(value);
                        }
                        html.push('\u884c\u4e1a\u4e2d\u4f4d\u6570\uff1a',value,'<br>');//行业中位数:
                    }
                    for(var i=0;i<arr.length;i++){
                        var value = arr[i].data;
                        if(key=='dau' || key == 'mau'){
                            value = showmohunum(value);
                        }/*else if(key=='download'){
                         value = formatNumber(value)+'(新增) '+formatNumber(other[i].value[arr[i].dataIndex])+'(累计)';
                         }*/else if(key=='time'){
                            value = showtimenum(value);
                        }else if(key=='cost'){
                            value = formatNumber(value)+'\u5143/UV';//元
                        }else{
                            value = formatNumber(value);
                        }
                        html.push(data[i].name, '\uff1a', value, '<br>');//:
                    }
                    return html.join('');
                }
            },
            grid:{
                containLabel:true,
                left:30,
                right:30,
                top:20,
                bottom:50
            },
            xAxis : [
                {
                    name:'',
                    type : 'category',
                    boundaryGap:false,
                    axisLine: {
                        lineStyle: {
                            color: '#efefef'
                        }
                    },
                    axisTick: {
						show: false,
                        lineStyle: {
                            color: '#727171'
                        }
                    },
                    axisLabel: {
                        formatter:' {value} ',
                        //interval:,
                        textStyle: {
                            color: '#727171'
                        }
                    },
                    splitLine: {
                        show: true,
						lineStyle: {
                            color: '#efefef'
                        }
                    },
                    data:time
                }
            ],
            yAxis : [
                {
                    name:'',
                    type : 'value',
                    axisLine: {
                        show: true,
                        lineStyle: {
                            color: '#404040'
                        }
                    },
                    axisTick: {
						show: false,
						lineStyle: {
                            color: '#727171'
                        }
                    },
                    axisLabel: {
						show: true,
                        textStyle: {
                            color: '#404040'
                        }
                    },
                    splitLine: {
						show: false,
                        lineStyle: {
                            color: '#efefef'
                        }
                    },
                }
            ],
			dataZoom: [/*{
		        type: 'inside',
		        start: 0,
		        end: 100
		    }, */{
                type:'slider',
                showDetail:false,
		        start: 0,
		        end: 100,
		        handleIcon: 'M10.7,11.9v-1.3H9.3v1.3c-4.9,0.3-8.8,4.4-8.8,9.4c0,5,3.9,9.1,8.8,9.4v1.3h1.3v-1.3c4.9-0.3,8.8-4.4,8.8-9.4C19.5,16.3,15.6,12.2,10.7,11.9z M13.3,24.4H6.7V23h6.6V24.4z M13.3,19.6H6.7v-1.4h6.6V19.6z',
		        handleSize: '80%',
		        handleStyle: {
		            color: '#fff',
		            shadowBlur: 3,
		            shadowColor: 'rgba(0, 0, 0, 0.6)',
		            shadowOffsetX: 2,
		            shadowOffsetY: 2
		        }
		    }],
            series : series
        };
        if(mid.length){
			//中位数
            markLine.push({name:'\u4e2d\u4f4d\u6570',yAxis:Number(mid),label:{normal:{position:'middle'}},itemStyle:{normal:{color:'#aaa'}}});
        }


        echart.setOption(option);
    }
    function callback(data){
        var empty = true,item,$item,time,key;
		var keys = getOrder(data);
		for(var i=0;i<keys.length;i++){
			key = keys[i];
			item = getConfig(key);
			if (!data[key][0].is_empty) {
                empty = false;
                $item = $(getItemHTML(item));
                addItem($item);
                time=data.three_month;
                if(item.key=='dau' && data.one_month){
                    time = data.one_month;
                }
                draw(key, time, data[key]);
            }
		}
		if(!empty){
			$position.hide();
		} else {
            $one_project.hide();
        }
    }

    function getdata(ids){
        $.ajax({
            url: "/ajax/bigdata/project/factorV2",
            type: 'get',
            data: {pids:ids},
            dataType:'json'
        }).done(function(data){
            if(data.code==0) {
				json = data;
                callback(data.data);
            }
        }).fail(function(){
            alert("服务错误!")
        });
    }
    if($position.length){
        getdata(id);
    }

});
		;$(function(){
    var $box,$btn=$('#btn_feedback_company'),$name,$contact,$content,name='',contact='';
    function addEvent(){
        var doing = false;
        $box.on('click','.pop_submit',function(){
            if(doing){
                return;
            }
            var name = $name.val(),contact=$contact.val(),content=$content.val();
            if(!$.trim(contact)){
                alert("\u8054\u7cfb\u65b9\u5f0f\u4e0d\u80fd\u4e3a\u7a7a!");//联系方式不能为空
                return;
            }
            if(!$.trim(content)){
                alert("\u7ea0\u9519\u5185\u5bb9\u4e0d\u80fd\u4e3a\u7a7a!");//纠错内容不能为空
                return;
            }
            $.ajax({
                type:'post',
                url:'/ajax/feedback/insert',
                data:{type:'company',name:name,contact:contact,content:content,refer:location.href},
                dataType:'json'
            }).always(function(){
                alert("\u63d0\u4ea4\u6210\u529f!");//提交成功
                close();
                doing = false;
            });
        });
        $box.on('click','.pop_close',function(){
            close();
        });

        $content.on('change input',function(){
            if($content.val()){
                $box.find('.tt_a').hide();
            }else{
                $box.find('.tt_a').show();
            }
        });
    }
    function close(){
        $box.hide();
    }
    function openpop(){
        if(!$box){
            $box = $($('#feedback_company').html());
            $box.appendTo('body');
            $name = $box.find('[name=name]');
            $contact = $box.find('[name=contact]');
            $content = $box.find('[name=content]');
            $name.val(name);
            $contact.val(contact);
            addEvent();
        }else{
            $content.val('');
            $content.trigger("change");
            $box.show();
        }
    }
    $btn.on('click',function(){
        openpop();
    })
});
	</script> 
