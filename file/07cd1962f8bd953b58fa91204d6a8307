	function shownav(){
		clearTimeout(t);
		document.getElementById('pgn').style.display='block';
		if(isIe){
			setSelectState('hidden');
		}
		
	}
	function closenav(time){
		t=setTimeout(function(){document.getElementById('pgn').style.display='none';},time);
		if(isIe){
			setSelectState('');
		}
	}
