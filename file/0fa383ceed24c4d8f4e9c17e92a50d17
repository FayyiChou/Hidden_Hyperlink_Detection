<script>
    var _gl_tracker = getCookie('_gl_tracker');
    var ca_s = getJson('ca_s', JSON.parse(_gl_tracker));
    var ca_i = getJson('ca_i', JSON.parse(_gl_tracker));
    var ca_kw = getJson('ca_kw', JSON.parse(_gl_tracker));
    var ca_n = getJson('ca_n', JSON.parse(_gl_tracker));
    function getJson(key, jsonObj){
        var eValue=eval('jsonObj.'+key);
        return eValue;
    }

    function getCookie(name){
        var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
        if(arr != null){
            return unescape(arr[2]);
        }else{
            return null;
        }
    }
    function request_url(url){
        var sign = url.indexOf('?') != '-1' ? '&' : '?';
        var flag = "ca_s=" + ca_s + "&ca_n=" + ca_n + "&ca_kw=" + ca_kw + "&ca_i=" + ca_i + "&";
        $.getJSON(url + sign + flag +'callback=?');
    }
