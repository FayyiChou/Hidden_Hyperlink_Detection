 <script>

$(function(){
//内容查看更多
	$("#contentmore").click(function(){
		var id =$("#contentid").val();		
		var page =$("#page").val();;			
		var morepage =Number(page)+1;
		var path = document.domain;
		$.getJSON("http://"+path+"/more/"+id+"/"+morepage+"/",function(json){	
			if(json.status=='y'){			
				$("#page").val(0);				
				$("#page").val(json.page);							
				$("#showid").append(json.content);
			}else{
				$(".zg_lian").html('<a href="javascript:;" class="zg_rukou" >没有更多了</a>');
			
			}	
		
		})
	
	})	
	//加入阅读
$('.zg_lban').click(function(){//已加入列表提示框
		//获取文章id_标题_当前栏目id 
		var wx_id=$('#contentid').val();
		var wx_nowcatid=$('#nowcatid').val();
		var wx_ab = $("#ab").val();		 
		//存cookie 
		IntoWeixin(wx_id,wx_nowcatid,wx_ab);
		
		
})

function IntoWeixin(wx_id,wx_nowcatid,wx_ab){
	
	var expiresDate= new Date();
	var wx_time =  expiresDate.valueOf();	
	var getcooks=GetCookie("cookiename");
	if(getcooks!=null){
		var cooks=getcooks.split("|");//计算添加的数量
		if(cooks.length>=10){
				alert("中公教育网：目前最多收藏10条。");return false;
		}else{
			
			var flag = 0;
			var cooks=getcooks.split("|");//计算添加的数量
			for(var i = 0;i < cooks.length; i++) {//判断文章是否已加入，禁止重复加入	
				var gwy_arr=cooks[i].split("_");//计算添加的数量		
				if(gwy_arr[0]== wx_id && gwy_arr[1]==wx_ab){
				 	alert("此篇已经在阅读列表咯。");
					var flag = 1;
					break;							
				}
			}
			
			if(flag==1) {
				$('.zg_z5').hide();
				$('.zg_z4').show();	
				$(this).addClass('zg_lbact');
				$('html,body').css({'height':'100%','overflow':'hidden'});
				$('html,body').animate({scrollTop: '0px'}, 100);
				return false;
			}else{
				var cookval=wx_id+"_"+wx_ab+"_"+wx_time+'|'+getcooks;
				SetCookie("cookiename",cookval);
				$('.zg_z4').hide();
				$('.zg_z5').show();
				$(this).addClass('zg_lbact');
				$('html,body').css({'height':'100%','overflow':'hidden'});
				$('html,body').animate({scrollTop: '0px'}, 100);
			}
		}

	}else{//首次存
		var cookval=wx_id+"_"+wx_ab+"_"+wx_time;		
		SetCookie("cookiename",cookval);
		$('.zg_z4').hide();
		$('.zg_z5').show();
		$(this).addClass('zg_lbact');
		$('html,body').css({'height':'100%','overflow':'hidden'});
		$('html,body').animate({scrollTop: '0px'}, 100);
	}		

}
//阅读列表
//设置cookie
function SetCookie(cookiename,cookval) {
	//alert("值是"+cookval);	
    var expiresDate= new Date();
	expiresDate.setTime(expiresDate.getTime() + (30 * 60 * 1000));
	var expires = expiresDate.toUTCString();
	//?替换成分钟数如果为60分钟则为 60 * 60 *1000
	$.cookie("cookiename", cookval, {path : '/',expires : expiresDate});
}//获得COOKIE
function GetCookie(cookiename) {
		var getcookieVal=$.cookie(cookiename); 
		return getcookieVal;
}
}) 
    
