var speed=50;
var hq=$("#hq")[0];
var hq_1=$("#hq_1")[0];
var hq_2=$("#hq_2")[0];
hq_2.innerHTML=hq_1.innerHTML;
function Marquee(){ 
if(hq_2.offsetTop-hq.scrollTop<=0) 
hq.scrollTop-=hq_1.offsetHeight;
else{ 
hq.scrollTop++ ;
} 
} 
var MyMar=setInterval(Marquee,speed); 
hq.onmouseover=function() {clearInterval(MyMar)}; 
hq.onmouseout=function() {MyMar=setInterval(Marquee,speed)} ;
function getHQData() { 
var url = "http://hshq.luyutx.com/front/hq/delay_hq.json";
jQuery.ajax({
	url : url,
	type : "get",
	dataType : "jsonp",
	async : false,
	data : {},
	error : function(r, ri, c) { 		
	},
	success : function(data) {
	}
});
} 
var arr;
function jsonpCallback(res) {
if (res == null || res == undefined) {
} else { 
	arr = res[0].stockList;
	$("#totalAmount").html(res[0].totalAmount);
	$("#totalFund").html(res[0].totalFund.toFixed(2));
	arr=arr.sort(function(a,b){return a.hqzqdm-b.hqzqdm});
	createHtml();
}
}
function createHtml(){
$("#hq_1").html('');
$("#hq_2").html('');
var tr="";
for(i=0;i<arr.length;i++){
var index=i+1;
	//tr=tr+"<tr onclick='goToProductDetail("+arr[i].hqzqdm+")'>";
	tr=tr+"<tr>";
	tr=tr+"<td class='gold' width='8%'>"+arr[i].hqzqdm+"</td>";
	tr=tr+"<td class='gold' width='12%'>"+arr[i].hqzqjc+"</td>";
	tr=tr+"<td width='10%' class='equal'>"+arr[i].hqzrsp.toFixed(2)+"</td>";
	if(arr[i].hqjrkp==0){
		tr=tr+"<td width='10%' class='equal'>--</td>";
		tr=tr+"<td width='10%' class='equal'>--</td>";
		tr=tr+"<td width='10%'  class='equal'>--</td>";
		tr=tr+"<td width='10%' class='equal'>--</td>";
		tr=tr+"<td width='10%' class='equal'>--</td>";
		tr=tr+"<td width='10%' class='equal'>--</td>";
		tr=tr+"<td width='10%' class='equal'>--</td>";
	}else{
		if(arr[i].hqjrkp>arr[i].hqzrsp){
			 tr=tr+"<td width='10%' class='equal' style='color:#ff3333'>"+arr[i].hqjrkp.toFixed(2)+"</td>";
		}else if(arr[i].hqjrkp<arr[i].hqzrsp){
			 tr=tr+"<td width='10%' class='equal' style='color:#66cc33'>"+arr[i].hqjrkp.toFixed(2)+"</td>";
		}else if(arr[i].hqjrkp==arr[i].hqzrsp){
			tr=tr+"<td width='10%' class='equal'>"+arr[i].hqjrkp.toFixed(2)+"</td>";
		}
		if(arr[i].hqjrzdf>0){
			tr=tr+"<td width='10%'><div style='color:#ff3333;'>"+arr[i].hqzjcj.toFixed(2)+"</div></td>";
			tr=tr+"<td width='5%' style='color:#ff3333'>"+arr[i].hqjrzdf.toFixed(2)+"</td>";
		}else if(arr[i].hqjrzdf<0){
			tr=tr+"<td width='10%'><div style='color:#66cc33'>"+arr[i].hqzjcj.toFixed(2)+"</div></td>";
			tr=tr+"<td width='5%' style='color:#66cc33'>"+arr[i].hqjrzdf.toFixed(2)+"</td>";
		}else{
			tr=tr+"<td width='10%' class='equal'>"+arr[i].hqzjcj.toFixed(2)+"</td>";
			tr=tr+"<td width='5%' class='equal'>"+arr[i].hqjrzdf.toFixed(2)+"</td>";
		}
		tr=tr+"<td width='10%' class='gold'>"+arr[i].hqcjsl+"</td>";
		tr=tr+"<td width='12%' class='gold'>"+arr[i].hqcjje.toFixed(2)+"</td>";
		if(arr[i].hqzgcj>arr[i].hqzrsp){
			tr=tr+"<td width='6%' class='equal' style='color:#ff3333;'>"+arr[i].hqzgcj.toFixed(2)+"</td>";
		}if(arr[i].hqzgcj<arr[i].hqzrsp){
			tr=tr+"<td width='6%' class='equal' style='color:#66cc33'>"+arr[i].hqzgcj.toFixed(2)+"</td>";
		}else if(arr[i].hqzgcj==arr[i].hqzrsp){
			tr=tr+"<td width='6%' class='equal'>"+arr[i].hqzgcj.toFixed(2)+"</td>";
		}
		if(arr[i].hqzdcj>arr[i].hqzrsp){
			tr=tr+"<td width='6%' class='equal' style='color:#ff3333;'>"+arr[i].hqzdcj.toFixed(2)+"</td>";
		}else if(arr[i].hqzdcj<arr[i].hqzrsp){
			tr=tr+"<td width='6%' class='equal' style='color:#66cc33'>"+arr[i].hqzdcj.toFixed(2)+"</td>";
		}else if(arr[i].hqzdcj==arr[i].hqzrsp){
			tr=tr+"<td width='6%' class='equal'>"+arr[i].hqzdcj.toFixed(2)+"</td>";
		}
		
	}
	tr=tr+"</tr>";
}
$("#hq_1").append("<table width='100%' align='center' cellspacing='0' cellpadding='0' style='border��0px;'>"+tr+tr+"</table>");
$("#hq_2").append("<table width='100%' align='center' cellspacing='0' cellpadding='0'>"+tr+tr+"</table>");
}
function curentTime(){ 
	var now = new Date();
	var year = now.getFullYear();       //年
	var month = now.getMonth() + 1;     //月
	var day = now.getDate();            //日
	var hh = now.getHours();            //时
	var mm = now.getMinutes();          //分
	var ss = now.getSeconds();          //秒
	var clock = year + "-";
	if(month < 10)
	    clock += "0";
	clock += month + "-";
	if(day < 10)
	    clock += "0";
	clock += day + " ";
	var myweekday = now.getDay();
	var weekday = "";
	if(myweekday == 0)
	weekday=" 星期日";
	else if(myweekday == 1)
	weekday=" 星期一";
	else if(myweekday == 2)
	weekday=" 星期二";
	else if(myweekday == 3)
	weekday=" 星期三";
	else if(myweekday == 4)
	weekday=" 星期四";
	else if(myweekday == 5)
	weekday=" 星期五";
	else if(myweekday == 6)
	weekday=" 星期六";
	clock +=weekday+" ";
	if(hh < 10)
	    clock += "0";
	clock += hh + ":";
	if (mm < 10) clock += '0'; 
	clock += mm + ":";
	if (ss < 10) clock += '0'; 
	clock += ss; 
	return(clock); 
}  
function showTime(){
	var date= curentTime();
	$("#tradeTime").html(date);
}
function goToProductDetail(code){
	var url="/index.php?m=Stock&a=lylinkjson&code="+code;
	jQuery.ajax({
		url: url,
		type: "get",
		dataType: "json",
		async: false,
		data:{},
		error: function(r, ri, c) {		
			//alert(r.status);			
		},
		success: function(data) {
			if(data.lylink){
				window.location.href=data.lylink;
			}
		}
	});
}
