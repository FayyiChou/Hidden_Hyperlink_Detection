function isMobel(values)
{
var ab=/^(13[0-9]|15[0-9]|18[0-9])\d{8}$/;
return ab.test(values);
if(/^13\d{9}$/g.test(values))
{ return true; }
if(/^15\d{9}$/g.test(values))
{ return true; }
if(/^18\d{9}$/g.test(values))
{ return true; }
return false;
}
