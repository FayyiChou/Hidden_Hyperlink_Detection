<script>
    $(function(){
        var sw=$(window).width();
        var w=1200;
        var aw=(sw-w)/2- $(".mx_rightlink").width()-2;
        $(".mx_rightlink").css("right",aw+"px");
    })
    $(window).scroll(function () {			
                        var ws_top = $(this).scrollTop();
                        var sw=$(window).width();
                        var w=1200;
                        var aw=(sw-w)/2- $(".mx_rightlink").width()-2;
                        $(".mx_rightlink").css("right",aw+"px");
                        if(ws_top){
                                $(".mx_topd").css('display','');
                        }else{
                                $(".mx_topd").css('display','none');
                        }
                });

</script>
