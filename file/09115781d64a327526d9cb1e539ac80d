$(document).ready(function(){
  $("#rightad span").click(function(){
  $("#rightad").hide(300);
    $("#divMenu").show(300);
  });

 $("#divMenu").click(function(){
$("#rightad").slideDown(300);
 $("#divMenu").hide(200);
});
 
 
   });
   

