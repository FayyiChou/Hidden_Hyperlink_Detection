   <script LANGUAGE="javascript">
  var tem1 = "";
  var Cacheid="";
 function tsala(ic,id) {
 block2();
 var sela= document.getElementById("sela"+id);
 if(ic==1){
 sela.style.display="";
 //alert(Cacheid);
 if(Cacheid!=""&&Cacheid!=id){
 //alert(Cacheid);
 document.getElementById("sela"+Cacheid).style.display="none";
 }
 }else if(ic==0){
 sela.style.display="none";
 }
 Cacheid=id;
 
 }
  function yin1(ic,id) {
        tem1 = setTimeout("tsala("+ic+","+id+")", 200);
    }
  function block2() {
        clearTimeout(tem1);
  }
 </script>

