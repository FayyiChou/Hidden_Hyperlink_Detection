<script type="text/javascript">
var headCommon = (function() {
function logFn() { /*登陆*/
var logBtn = topCommonFn.getId("JuserLog"),
JuserInfo = topCommonFn.getId("JsideUserInfo"),
JuserInfo2 = topCommonFn.getId("JdropMenuLog"),
JuserInfo3 = topCommonFn.getId("JuserInfo");
if(JuserInfo2 && !JuserInfo3){/*如果页面没引入logo栏,显示登陆按钮*/
JuserInfo2.style.visibility = "visible";
}
if (Cookie.get("common_session_id") && Cookie.get('cmu')) {
function jumpToMy(){
Cookie.set("userBackUrl", window.location.href, 30, "/", "pcauto.com.cn");
Cookie.set("userBackUrl", window.location.href, 30, "/", "pcauto.pcvideo.com.cn");
window.location.href = location.protocol+"//m.pcauto.com.cn/my/?t=" + (new Date).getTime();
}
if(logBtn){
logBtn.addEventListener('click', function() {
jumpToMy();
}, false); 
}
if(JuserInfo2){
JuserInfo2.addEventListener('click', function() {
jumpToMy();
}, false);
}
if(JuserInfo3){
JuserInfo3.addEventListener('click', function() {
jumpToMy();
}, false);
}
function insertLogedUser(id, imgSrc, nickName) {
topCommonFn.getJson(location.protocol+'//bip.pcauto.com.cn/intf/message.jsp?act=getNumToJson&siteId=2&accountId=' + id, function(json) {
var userMessageConut = json.messageCount + json.systemNoticeCount + json.forumReplyCount + json.commentReplyCount;
if (userMessageConut == 0) {
JuserInfo.innerHTML = '<img src="' + imgSrc + '" />';                        
if(JuserInfo2){JuserInfo2.innerHTML = '<img src="' + imgSrc + '" />';}
if(JuserInfo3){JuserInfo3.innerHTML = '<img src="' + imgSrc + '" />';}                        
} else if (userMessageConut >= 100) {
JuserInfo.innerHTML = '<img src="' + imgSrc + '" /><i>99+</i>';
if(JuserInfo2){JuserInfo2.innerHTML = '<img src="' + imgSrc + '" /><i>99+</i>';}
if(JuserInfo3){JuserInfo3.innerHTML = '<img src="' + imgSrc + '" /><i>99+</i>';}
} else {
JuserInfo.innerHTML = '<img src="' + imgSrc + '" /><i>' + userMessageConut + '</i>';
if(JuserInfo2){JuserInfo2.innerHTML = '<img src="' + imgSrc + '" /><i>' + userMessageConut + '</i>';}
if(JuserInfo3){JuserInfo3.innerHTML = '<img src="' + imgSrc + '" /><i>' + userMessageConut + '</i>';}
};
document.getElementById("JlogStatus").innerHTML = nickName;
}, "getMessageConut");
}
function getAvatar(id) {
id = "" + id;
var ret = [],
i = 0,
l = id.length;
while (i + 2 <= l) {
ret.push(id.slice(i, i + 2));
i += 2;
}
if (i < l) {
ret.push(id.charAt(i));
}
return location.protocol+'//i6.3conline.com/images/upload/upc/face/' + ret.join('/') + '/' + id + '_50x50';
}
setTimeout(function() {
if (Cookie.get("logedUserId")) {
var logedUserId = Cookie.get("logedUserId");
var userImgSrc = getAvatar(logedUserId);
var nickName = Cookie.get("logedUserName");
insertLogedUser(logedUserId, userImgSrc, nickName);
} else {
topCommonFn.getJson(location.protocol+'//my.pcauto.com.cn/intf/getLogedUser.jsp', function(data) {
Cookie.set("logedUserId", data.id, 30, "/", "pcauto.com.cn");
Cookie.set("logedUserName", data.nickName, 30, "/", "pcauto.com.cn");
Cookie.set("logedUserId", data.id, 30, "/", "pcauto.pcvideo.com.cn");/*视频频道域名*/
Cookie.set("logedUserName", data.nickName, 30, "/", "pcauto.pcvideo.com.cn");
var logedUserId = Cookie.get("logedUserId");
var userImgSrc = getAvatar(logedUserId);
var nickName = data.nickName;
insertLogedUser(logedUserId, userImgSrc, nickName);
}, "getLogedUserInfo");
}
}, 100);
} else {
document.getElementById("JlogStatus").innerHTML = "点击登录";
Cookie.remove("logedUserId");
Cookie.remove("logedUserName");
function jumpToLog(){
Cookie.set("userBackUrl", window.location.href, 30, "/", "pcauto.com.cn");
Cookie.set("userBackUrl", window.location.href, 30, "/", "pcauto.pcvideo.com.cn");                
window.location.href = location.protocol+"//m.pcauto.com.cn/my/passport/login.jsp?return=" + window.location.href;
}
if(logBtn){
logBtn.addEventListener('click', function() {
jumpToLog();
}, false); 
}
if(JuserInfo2){
JuserInfo2.addEventListener('click', function() {
jumpToLog();
}, false);
}
if(JuserInfo3){
JuserInfo3.addEventListener('click', function() {
jumpToLog();
}, false);
}
}
}
function loadAppPic() { /*加载app图标*/
var imgs = document.querySelectorAll("#JautoHeadTg img"),
len = imgs.length;
if (len <= 0) return;
for (var i = 0; i < len; i++) {
var curImg = imgs[i],
url = curImg.getAttribute("src2");
if (!url) break;
curImg.setAttribute("src", url);
curImg.removeAttribute("src2");
}
}
function closeFn() { /*关闭*/
var closeBtn = document.getElementById("Jclose"),
cover = document.getElementById("JautoHeadCover"),
m_autoHead = document.getElementById("JautoHead");
closeBtn.addEventListener("click", function() {
m_autoHead.className = "m_autoHead off";
var sto = this.mySto;
sto && clearTimeout(sto);
sto = setTimeout(function() {
document.getElementById("JautoHeadCover").style.display = "none"; /*隐藏遮罩层*/
document.getElementById("JautoHead").style.display = "none"; /*隐藏弹层*/
document.querySelector("html").style.cssText = document.body.style.cssText = ""; /* 恢复 body 可滚动状态*/
}, 400);
}, false);
}
function hideBody() {
var pageHtml = document.querySelector("html"),pageBody = document.querySelector("body");
pageHtml.style.cssText = pageBody.style.cssText = "position:absolute;width:100%;height:100%;overflow:hidden";
}
window.addEventListener("resize", function() {
if (document.getElementById("JautoHead").className == "m_autoHead on") {
hideBody();
}
});
function dropMenuHead(obj) { /*点击展开*/
var dropBtn = document.getElementById(obj),
cover = document.getElementById("JautoHeadCover"),
dropCon = document.getElementById("JautoHead"),
body = document.querySelector("body");
if (!dropBtn) return;
dropBtn.addEventListener("click", function() {
var className = dropCon.className;
if (className == "m_autoHead" || className == "m_autoHead off") {
cover.style.display = "block";
dropCon.className = "m_autoHead on";
dropCon.style.display = "";
loadAppPic();
hideBody();
}
}, false);
}
closeFn();
window.addEventListener("DOMContentLoaded",function(){
logFn();
},false);
return{
dropMenuHead:function(obj){
dropMenuHead(obj);
} 
}
})();
</script>
<script>
!(function() {
/*
* 用于加载js
* fn {Function || variable} : 如果该函数或者变量已经定义，则直接调用回调函数；否则加载指定js
* src {String}: 要加载的js地址
* callback {Function} : js加载完毕后的回调函数
*/
function needJS(fn, src, callback) {
callback = callback || function() {};
if (fn) return callback(false);
var scripts = document.___needJS__ || (document.___needJS__ = []);
var script = scripts[src] || (scripts[src] = {
loaded: false,
callbacks: []
});
if (script.loaded) return callback(false);
var cbs = script.callbacks;
if (cbs.push(callback) == 1) {
var js = document.createElement("script");
js.onload = js.onreadystatechange = function() {
var st = js.readyState;
if (st && st != "loaded" && st != "complete") return;
js.onload = js.onreadystatechange = null;
script.loaded = true;
for (var i = 0; i < cbs.length; i++) cbs[i](true);
};
js.src = src;
document.getElementsByTagName("head")[0].appendChild(js);
}
}
