<script src="http://js.3conline.com/min2/temp/v2/plugin-locate,plugin-locate_auto.js" charset="utf-8"></script> <!-- 全站ip定位 -->
<script>
// 城市定位
var locationPub = {
locateData: null,
showCity: function(city) { // 显示当前城市
var locationArr = document.body.querySelectorAll('.u-ipLocation');
for (var i = 0; i < locationArr.length; i++) {
locationArr[i].innerHTML = city;
}
},
onLocated: function(data, fn) {
var city = data.city && data.city.slice(0, -1) || '广州';
this.showCity(city);
this.locateData = data;
if ('function' == typeof fn) fn(data);
},
doAfterLocate: function(fn) { // 获取定位信息之后执行回调
var self = this;
if (self.locateData) {
// 已定位
if ('function' == typeof fn) fn(self.locateData);
} else {
// 未定位
// 考虑兼容旧版定位用的 cookie
var cookie = Locate.getCookie('locationWap');
if (!cookie) {
// 没有旧版定位 cookie，用新版定位（cookie 名称为 pcautoLocate）
LocateAuto.init({
callback: function(data) {
var obj = {
regionId: data.cityId,
regionName: data.city.slice(0, -1)
};
// 车型库经销商用的 cookie [AplocationWap]
Locate.setCookie('AplocationWap', Locate.stringifyResult(obj));
self.onLocated(data, fn);
}
});
} else {
// 有旧版定位 cookie
var value = JSON.parse(cookie);
if (!value.regionCity || !value.regionPro) {
// 解决旧版 cookie 错误，没有经销商 id 信息，用新版定位获取
LocateAuto.setByCityCode(value.cityCode,
function(data) { //成功请求接口后执行方法
self.onLocated(data, fn);
}
);
} else {
// 有经销商 id 信息，与新版定位信息对象统一字段名称
value.cityId = value.regionCity;
value.proId = value.regionPro;
self.onLocated(value, fn);
}
}
}
}
};
var carVersion = "1";
var carField = "4";
locationPub.doAfterLocate(function(locateData) {
if( carVersion != 2 && carVersion != 3 && carField != 1 ){
//瓜子二手车
getSecondHandCar(locateData.cityId);
}
});
function getSecondHandCar(areaId){
$.ajax({//车型导航
url: 'http://price.pcauto.com.cn/price/api/v1/cooperation/guazi/local_second_data/',
data: {
serialGroupId: "1074",
regionId: areaId,
from: 2,
orderIndex : 2
},
type: 'get',
dataType: 'jsonp',
cache: true,
success: function (data2) {
var ele2;
ele2 = document.getElementById('guaziSecond4WapSg');
if( ele2){
ele2.href = "http://price.pcauto.com.cn/price/api/v1/cooperation/second_hand_car/load/?from=2&source=6&url="+data2.url;
}
}});
}
</script>
<!--Edit 201609 城市定位 E-->
<script type="text/javascript">
//core.selectMod("JselectZ");
//图片延时加载
var xx = Lazy.create({
lazyId: "Jlazy_img",
trueSrc: '#src',
delay: 100, //该毫秒时间内触发则延时100毫秒再加载
delay_tot: 1000 //超过该毫秒时间触发则直接加载
});
Lazy.init(xx);
</script>
<script type="text/javascript">
//for Toutiao
if (window.location.search.indexOf('FromTouTiao') != -1) {
sessionStorage['fromtoutiaoCookie'] = 'seted';
} else {
sessionStorage['fromtoutiaoCookie'] = 'unseted';
}
//控制某些模块隐藏
if(sessionStorage['fromtoutiaoCookie'] == 'seted'){
if(document.querySelector('.m-footplate') != null){
document.querySelector('.m-footplate').style.display = 'none';
}
if(document.querySelector('.menu') != null){
document.querySelector('.menu').style.display = 'none';
}
if(document.querySelector('.m-footer') != null){
document.querySelector('.m-footer').style.display = 'none';
}
if(document.querySelector('.nav') != null){
document.querySelector('.nav').style.display = 'none';
}
if(document.querySelector('#JtgBtn1601') != null){
document.querySelector('#JtgBtn1601').style.display = 'none';
}
}
</script>

