    $(".pro_nav_side .sitem1").click(function () {
        var $this = $(this);
        var _sid = $this.attr("sid");
        if ($this.is(".h_o") || !$this.next("ul[sid=" + _sid + "]")[0]) return true;
        $(".pro_nav_side .h_o").removeClass("h_o").siblings("ul").slideUp("20");
        $this.addClass("h_o");
        $this.next("ul[sid=" + _sid + "]").slideDown("90");
        return false;
    });
</script> 
