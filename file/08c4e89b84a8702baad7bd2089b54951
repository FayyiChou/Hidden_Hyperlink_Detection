    <script type="text/javascript">
        $(function () {
            $("a.upload.down2").bind("click", function () {
                statistics(1);
            });
            $("a.upload.down1").bind("click", function () {
                statistics(2);
            });
            $(".mobileTabWrap").eq(0).hide();
            $(".mobileTab span").each(function (index) {
                $(this).click(function () {
                    $(this).addClass("hover").siblings("span").removeClass("hover");
                    $(".mobileTabWrap").eq(index).show().siblings(".mobileTabWrap").hide();
                });
            });

        });

        function statistics(type) {
            $.ajax({
                url: "http://zpmobiledata.zhaopin.com/statistics",
                dataType: "jsonp",
                jsonp: "callback",
                data: {
                    type: type,
                    redirect: ""
                },
                cache: false,
                success: function (response, status, xhr) {
                }
            });
        }
        //za start add by george.liu 20140109
        function getCookieVarByName(str) {
            var ret = '', cookies, search = str + '=', offset, end;
            cookies = document.cookie;
            if (cookies.length > 0 && cookies.indexOf(search) > -1) {
                offset = document.cookie.indexOf(search);
                if (offset != -1) {
                    offset += search.length;
                    end = cookies.indexOf(';', offset);
                    if (end == -1) {
                        end = cookies.length;
                    }

                }
                ret = cookies.substring(offset, end);
                ret = encodeURIComponent(ret);
            }


            return ret;
        }
        function trackMobileDownLoad(downType) {
            var imgUrl = 'http://pv.zhaopin.cn/track.gif?dda_category=appdownload', dImg = document.createElement("img");
            dImg.style.visibility = 'hidden';
            imgUrl = imgUrl + "&dda_source=pc";
            imgUrl = imgUrl + "&dda_action=" + downType;
            imgUrl = imgUrl + "&dda_dywea=" + getCookieVarByName('dywea');
            imgUrl = imgUrl + "&dda_suid=" + getCookieVarByName('JsNewlogin');
            imgUrl = imgUrl + "&dda_sid=" + getCookieVarByName('urlfrom');
            imgUrl = imgUrl + "&dda_site=" + getCookieVarByName('adfcid');
            imgUrl = imgUrl + "&dda_path=" + encodeURI(location.href);
            imgUrl = imgUrl + "&dda_referer=" + encodeURI(document.referrer);
            dImg.src = imgUrl;
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(dImg, s);
        }

        function mobileDownLoad(e) {
            var mHref = e.currentTarget.href, downType = '';
            if (mHref.indexOf("itunes.apple.com") > -1) {
                downType = "iphone";
            } else if (mHref.indexOf("zhaopin_zhilian.apk") > -1) {
                downType = "android";
            }
            trackMobileDownLoad(downType)
        }
        function bindDownLoadZa() {
            $('[ga_download="ga_download"]').bind('click', function (e) {
                mobileDownLoad(e);
            });
        }
        $(function () {
            bindDownLoadZa();
        });
        //za end
    </script>



