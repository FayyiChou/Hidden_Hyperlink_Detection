<script type="text/javascript">
$(function(){
	$('.shrink').click(function(){
		if($(this).hasClass('up')){
			$(this).removeClass('up').addClass("down")
			$(this).siblings('.coupon-item').css('height','auto');
		}else{
			$(this).removeClass('down').addClass("up")
			$(this).siblings('.coupon-item').css('height','21px');
		}
	});
	// 	领券
	$('.layer').on('click', '.surebtn,.close_mia_popup', function() {
		$('.layer,.popup_mask').hide();
	});
})
