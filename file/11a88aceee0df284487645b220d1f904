<script>
	$(function(){

		$('.largessWrap').each(function(){
			var $tab = $(this).find('.largessTit_tab').find('span');
			var $tabCon = $(this).find('.largessCon .tabCon');
			$tab.on('click', function(index) {
		        $tab.eq($(this).index()).addClass('curSpan').siblings().removeClass('curSpan');
	        	$tabCon.eq($(this).index()).show().siblings().hide(); 
		    });
		})
		
	})
</script>

