<script type="text/javascript">
item_id = '1679868';
origin_scan_sku_str = new_scan_sku = '';
origin_scan_sku_arr = [];
origin_scan_sku_str = getcookie('scan_sku');
var date = new Date();
var expireDays = 3650;
date.setTime(date.getTime()+expireDays*24*3600*1000);
if (origin_scan_sku_str) {
    new_scan_sku = origin_scan_sku_str
    origin_scan_sku_arr = origin_scan_sku_str.split(',');
    if (origin_scan_sku_arr.indexOf(item_id) == -1) {
        if (origin_scan_sku_arr.length < 5) {
            new_scan_sku =  item_id+','+origin_scan_sku_str;
        } else if(origin_scan_sku_arr.length == 5) {
            origin_scan_sku_arr.pop();
            origin_scan_sku_str = origin_scan_sku_arr.join(',');
            new_scan_sku =  item_id+','+origin_scan_sku_str;
        }
    }
    setcookie('scan_sku', new_scan_sku , date.toGMTString());
} else {
    setcookie('scan_sku', item_id, date.toGMTString());
}
