<script>
$(function() {
    var $li = $('.moduleFixed li');
    var $con = $('.p10 .area');
    $li.on('click', function() {
        var index = $(this).index();
        showTab(index)
    });

    function showTab(index){
        if (index == 2) {
            //蜜芽优势切换
            $("#advantageTitle").tabs({
                cntSelect: "#imgBlock",
                tabEvent: "click",
                tabStyle: "normal",
                onStyle: "mia_current"
            });
        };
        $li.eq(index).addClass('current').siblings().removeClass('current');
        $con.eq(index).show().siblings().hide();
    }
    if (window.location.hash == '#hash_myq') {
        showTab(1);
    };
});

</script>
<script>
    //阶梯满赠js
    $('.relevant .itemlist').eq(0).removeClass('hidden');
    $('.taghead .tag').click(function(){
         if($(this).hasClass('active')){
             return;
         }
         var $parent=  $(this).closest('.relevant')
         $parent.find('.tag').removeClass('active');
         $(this).addClass('active');
         var tagtype  = $(this).data('tag');
         $parent.find('ul.itemlist').addClass('hidden');
         $parent.find('ul.itemlist').filter(function(){
             return $(this).data('tag')==tagtype;
         }).removeClass('hidden');
    })
    //阶梯满赠js结束
    
    var jiathis_config = {
        url: document.location.href,
