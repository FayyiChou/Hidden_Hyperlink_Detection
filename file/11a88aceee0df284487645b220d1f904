<script>
    $.get('/item/detail/tuijian', { id : 1679868} ,
        function(data) {
            var str='';
            data = JSON.parse(data);
            $.each(data,function(i,item){
                //alert(item.id);
                str += '<div class="cell"><a href="/item-'+item.id+'.html'+item.tag+'" target="_blank" title="'+item.name+'"><img src="'+item.img+'" alt="'+item.name+'" width="217" class="img"></a>';
                str += '<p class="p1"><a href="/item-'+item.id+'.html'+item.tag+'">"'+item.brand_name+'  '+item.name+'"</a></p>';
                str += '<p class="p2">&yen;'+item.sale_price;
                str += '</div>';
            });
            $(".poppro").append(str);
            // if ($('.moduleFixed').length > 0) {
            //     $('.moduleFixed').moduleFixed({
            //         Zindex: 5
            //     });
            // };
    })
    $.get('/item/detail/seeing', { id : 1679868} ,
        function(data) {
            var str2 ='';
            var show =false;
            data = JSON.parse(data);
            str2 += '<div class="pubtitle yahei">看了又看</div><div class="pubcon cp">';
            $.each(data,function(i,item){
                if(item.id > 0){ show = true; }
                str2 += '<div class="block clearfix"><a href="/item-'+item.id+'.html'+item.tag+'" target="_blank">';
                str2 += '<img src="'+item.img2+'" alt="'+item.name+'" width="80" height="80"/></a><p class="tit">';
                str2 += '<a href="/item-'+item.id+'.html'+item.tag+'" target="_blank" title="'+item.name+'">"'+item.name.substring(0,26)+'"</a></p>';
                str2 += '<span class="pink">&yen;'+item.sale_price+'</span><br/>';
                if(item.total_sale_amount > 0){
                 str2 += '<span class="gray">&nbsp;</span>';
                }
                str2 += '</div>';
            });
            str2 += '</div>';
            if(show){
                $("#seeing").append(str2);
            }
        })
</script>
