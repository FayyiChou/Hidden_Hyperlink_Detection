  window.onload = function(){
    var x = document.getElementById("koubei_con");
    if(!x) return;
    var last = x.lastElementChild || father.lastChild;
    last.style.border = "none";
}
//商品详情 点击收起与展开
    $(function() {
        var slideHeight = 100; // px
        var defHeight = $('#wrap_con').height();
        if (defHeight >= slideHeight) {
            $('#wrap_con').css('height', slideHeight + 'px');
            $('#read_more').append('<a href="#">展开</a>');
            $('#read_more a').click(function() {
                var curHeight = $('#wrap_con').height();
                if (curHeight == slideHeight) {
                    $('#wrap_con').animate({
                        height: defHeight
                    }, "normal");
                    $('#read_more a').html('收起');
                    // $('#gradient').fadeOut();
                } else {
                    $('#wrap_con').animate({
                        height: slideHeight
                    }, "normal");
                    $('#read_more a').html('展开');
                    // $('#gradient').fadeIn();
                }
                return false;
            });
        }
    });

</script>
