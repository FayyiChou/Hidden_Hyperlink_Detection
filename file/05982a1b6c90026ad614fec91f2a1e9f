    <script type="text/javascript">
        var aWidth = $(".adscroll em").width();
        $(function () {
            if (aWidth > 860) {
                $(".adscroll em").clone().appendTo(".adscroll span.spanbox");
                setInterval("showad()", 100);
            }
        });
        function showad() {
            var aLeft = parseInt($(".adscroll em").eq(0).css("left"));

            if (aLeft < -aWidth) {
                $(".adscroll em").css("left", 0);
            } else {
                aLeft -= 2; //改变速度
                $(".adscroll em").animate({ left: aLeft }, 10);
            }
        }
        </script>


