    <script type="text/javascript">
        var cou = 2;
        var current = 1;
        $(function () {
            ShowJT();
            if (cou > 1)
                setInterval(ShowJT, 5000);
        });

        function ShowJT() {
            if (cou == 0)
                return;
            $("#aJT" + current).hide();
            if (current == cou)
                current = 1;
            else
                current++;
            $("#aJT" + current).show(1000);
        }
    </script>

    
<script type="text/javascript">
    $(function () {

        var $e = $(".inBanner");
        var n = $e.find("li").size();
        $e.find(".switch").width(1190 / 2 + (n * 30) / 2).css("padding-left", 1190 / 2 - (n * 30) / 2 + "px");

        $.fn.cycle.transitions.fcFade = function ($cont, $slides, opts) {
            opts.fxFn = function (curr, next, opts, after) {
                $(curr).animate({ opacity: 0 }, 1000, function () {
                    $(curr).css('display', 'none');
                });
                $(next).css({ opacity: 0, display: 'block' }).animate({ opacity: 1 }, 1000, function () {
                    after();
                });
            }
        }

        $e.find("ul").cycle({
            activePagerClass: 'item',
            fx: 'fcFade',
            timeout: 5000,
            speed: 1000,
            pager: $e.find(".switch"),
            pauseOnPagerHover: true,
            pause: true
        });
    });
</script>
