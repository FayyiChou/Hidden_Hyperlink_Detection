        var current = 1;
        $(function () {
            ShowJT();
            if (cou > 1)
                setInterval(ShowJT, 3000);
        });

        function ShowJT() {
            if (cou == 0)
                return;
            $("#bJT" + current).hide();
            if (current == cou)
                current = 1;
            else
                current++;
            $("#bJT" + current).show(500);
