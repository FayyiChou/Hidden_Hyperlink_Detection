function initFriendLink(){
	$.ajax({
		url: "/getFriendLinkList.do",
		type: "post",
		dataType:"json",
		success : function(data){
			
			var htmlVal = ""; 
			for(var i = 0;i<data.length;i++){
				htmlVal += " <a target='_blank' title="+data[i].name+" href="+data[i].linkUrl+">"+data[i].name+"</a> ";
			}
			$("#friendLink").html("<span>友情链接：</span>"+htmlVal);
		}
	});
}

