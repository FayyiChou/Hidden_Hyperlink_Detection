<script type="text/javascript">

function removeIndexShare(){
	
	$("#bodydiv").animate({top:'61%',height:'82px',width:'72px'});
	$("#bodydiv").animate({right:'0px'});
	$("#indexShare").fadeOut(1000);
}

function shareShow(){	
	if(!$.cookie){
		var $head = document.getElementsByTagName('HEAD').item(0);
	    var $sc= document.createElement("script");
	    $sc.type = "text/javascript";
	    $sc.src="/js/jquery.cookie.js";
	    $head.appendChild($sc);
	}
    $.ajax({
    	url : constants.Domain.passport+"/isLogin.do",
		type : "post",
		dataType:"jsonp",
		success : function(data) {
		   if(data.state=='notLogin'){//判断是否登陆
			   alert("亲亲！您还没有登录，请先登录在来分享^_^");
			   window.location.href=constants.Domain.passport+"/page/login.jsp";
			 //  showLoginDialog();
		   }else{
			   $("#indexShare").fadeIn(1000);
			   $("#bodydiv").animate({right:'55%'},'1000');
			   $("#bodydiv").animate({top:'20%',height:'384px',width:'525px'},'1000');			   
		   }
		},
		error : function(e) {
			alert("系统异常！");
		}
	});
}
var shareUrl = "";
/**
 * 初始化百度分享api
 */
window._bd_share_config = {
		common : {			
			bdDesc : "来自震海批发网",						
			bdMiniList:["qzone","weixin","renren","tqq","tqf","tieba","meilishuo","mogujie"],
			onBeforeClick : function(cmd,config){		
				var shareUrl1 = "";	
				var userId = $.cookie("userId");
				$.ajax({
					url : "/indexshare/url/getIndexShareUrl.do",									
					type : "post",	
					data:{"shareChannel":cmd,"userId":userId},
					async: false,
					success:function(data) {													
						if(data!="notLogin"){
							shareUrl1 = data;					
						}							
					},error:function(){
						alert("记录分享失败哦，亲！^_^");	
					}
				});			
				config.bdUrl=shareUrl1;
				config.bdPic=$("#zhlg").attr("src");
				config.bdText=$("#indexshareText").text();
				return config;
		
			},
			onAfterClick : function(cmd,config){}
		},
		share : [{
			"tag" : "share_index",
			"bdSize" : 16
		}]
};

//滚动条到一定距离 （返回顶部显示）
$(document).scroll(function(){
  	if($(window).scrollTop()>=500){
		   $(".backtop a").fadeIn();
		}else{
			$(".backtop a").fadeOut(); 
		}		  
});

function createShareQQUrl(){
	var userId = $.cookie("userId");
	$.ajax({
		url : "/indexshare/url/getIndexShareUrl.do",
		async: false,
		type : "post",
		data:{"shareChannel":"sqq","userId":userId},
		success : function(data) {			
			if(data!="notLogin"){
				shareUrl = data;					
			}						
		},
		error : function(e) {
			alert("记录分享失败哦，亲！^_^");
		}
	});	
	
	var p = {
            url: shareUrl,/*获取URL，可加上来自分享到QQ标识，方便统计*/
            desc: "来自震海批发网", /*分享理由(风格应模拟用户对话),支持多分享语随机展现（使用|分隔）*/
            title : $("#indexshareText").text()+"（分享自@震海批发网",/*分享标题(可选)*/
            summary : '来自震海批发网',/*分享描述(可选)*/
            pics : $("#zhlg").attr("src"),/*分享图片(可选)*/
          //  flash : '',	/*视频地址(可选)*/
          //  commonClient : true, /*客户端嵌入标志*/
            site: '震海批发网'/*分享来源 (可选) ，如：QQ分享*/
            };

    var s = [];
        for (var i in p) {
        s.push(i + '=' + encodeURIComponent(p[i] || ''));
    }
	
	window.location.href = "http://connect.qq.com/widget/shareqq/index.html?" + s.join('&');
}
