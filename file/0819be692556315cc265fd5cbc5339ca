<script type="text/javascript">
$(function(){
	//加载用户登录信息
	loadUserInfo();
	//加载楼层
	loadFloor();
	//改变帮助中心地址
	changeLoginHeplerUrl();
	//判断是否登陆
	indexLeftIsLogin();
	//初始化下来菜单事件
	initbottomMenuEvent();

	getCityCookie();
	//初始化楼层导航条数据
	initFloorScrollFun();
	//初始化右侧滚动条
	initRightScrollFun();
	//初始化右侧滚动条
	initRightScrollFun();
	//修改导航url
	changeNavigationUrl();
	//城市切换按钮
	changeCityBtnStyle();
	//初始化市场滚动样式
	initmarketScrollStyle();
	
	//初始化时根据北京城市显示不同的城市标题
	if($.cookie("currentSelectCity")==""||$.cookie("currentSelectCity")==undefined){
	 	initLoadCityByIpLocation();
	}else{
		var cityCode=$.cookie("currentSelectCity");
		var cityName=$.cookie("currentSelectCityName");
		setCookie(cityCode,cityName);	
		if(cityName=="北京"){
			$("#ISBJ").html("<div class=topadbox id=ISBJ><a  href=javascript:void(0) style=background:url(http://www.zhenhaipifa.com/images/public/bjtoppic.jpg) no-repeat center;height=90px;></a></div>");
		}else{
			$("#ISBJ").html("<div class=topadbox id=ISBJ><a target='_blank' href='javascript:void(0)' style='background:url(http://img03.zhenhaipifa.com/p/201601/a45c7e57266c4cd98b78e3718ab2df6807523.jpg) no-repeat center;height=90px;'></a></div>");
		}
	}
	   

	//图片懒加载
	$("img.lazy").lazyload({
		effect : "fadeIn",
		placeholder : "/images/public/imgLoading.gif",   
		skip_invisible : false,
		threshold : 200
	});
	

	//首页二维码链接
   var indexUrl =  constants.Domain.www+"/index.do"; 
   $("#qrcode").qrcode({
       render  : "table",//也可以替换为table
       width   : 111,
       height  : 111,
       text    : indexUrl
   });
	
	$(".floormain").not(".pifafloor").not(".sczxfloor").each(function(index){//循环每一个楼层
		var floorMain = $(this).find(".floormaincon");
		$(this).find(".floormaintit li").each(function(cIndex){//找出每个楼层的类目
			$(this).hover(function(){//绑定hover事件
				floorMain.each(function(){$(this).hide();});//隐藏所有主界面
				$(floorMain.get(cIndex)).show();//显示相应类目主界面
				$(this).addClass("on").siblings().removeClass("on");
				//图片懒加载 切换类目要动一下图片才能加载
				$(document).scrollTop($(document).scrollTop()-1);
				$(document).scrollTop($(document).scrollTop()+1);
			},function(){
// 				$(this).removeClass("on");
			});
		});
	});
	          	
 	//轮播
	var demoname='demo';	
	for (var i=0;i<=30;i++){
		var demoid=demoname+i;
		$('#'+demoid).slideBox({
				duration : 0.3,//滚动持续时间，单位：秒
				easing : 'linear',//swing,linear//滚动特效
				delay : 5,//滚动延迟时间，单位：秒
				hideClickBar : false,//不自动隐藏点选按键
				clickBarRadius : 10
		});
    }
	
   $(".mobilebox").hover(function(){
       $(".mobilebox .code").show();
   },function(){
       $(".mobilebox .code").hide(); 
   });

});


//初始化市场滚动
function initmarketScrollStyle(){
	//recommed market 2015-12-05
	var speet=238;
    var scrolltime=1200;
    var time=30;
    var box1=document.getElementById("box1");
    var box2=document.getElementById("box2");
    var box3=document.getElementById("box3");
    box2.innerHTML+=box2.innerHTML;
    box1.scrollLeft=speet;
    timer=setInterval(scrleft,time)
    function scrleft(){
	     if(box1.scrollLeft>=scrolltime){
		 	box1.scrollLeft=0;
		 }else{
			box1.scrollLeft++;
		 }
	}
   
    box2.onmouseover=function(){
	      clearInterval(timer);
	   }
    box2.onmouseout=function(){
	      timer=setInterval(scrleft,30);
    } 
		
}


//初始化右侧滚动条
function initRightScrollFun(){
	//导航栏下拉到一定位置顶部显示
	var nav=$(".search-layout02"); //得到导航对象
	var win=$(window); //得到窗口对象
	var sc=$(document);//得到document文档对象。
	win.scroll(function(){
		  if(sc.scrollTop()>=700){
		    nav.addClass("fixednav"); 
		 
		  }else{
		   nav.removeClass("fixednav");
		 
		  }
	});
}

//初始化城市切换按钮
function changeCityBtnStyle(){
	 //切换城市
	 /*鼠标移动至 ”切换地区“ 的事件*/
    $(".change-font").hover(function(){
      $(".change .change-font").addClass("bottomline");
      $(".change .change-font > em").addClass("open");
      $(".change .citybox").show();
    },function(){
      $(".change .change-font > em").removeClass("open");
      $(".change .citybox").hide();
      $(".change .change-font").removeClass("bottomline");
    })
    /*鼠标移动至 ”选择城市内容块“ 的事件*/            
    $(".change .citybox").hover(function(){
      $(".change .change-font").addClass("bottomline");
      $(".change .change-font > em").addClass("open");
      $(this).show();
    },function(){
      $(".change .citybox").hide();
      $(".change .change-font").removeClass("bottomline");
      $(".change .change-font > em").addClass("open");
    })
    /*取消 ”选择城市内容块“ 每3个 ul li 标签的外边距*/
    $('.citybox .city-name a:nth-child(5n)').css('margin', 'auto');
}


//初始化楼层导航条
function initFloorScrollFun(){
	$(".loubox").hover(function(){
		var index=$(this).index(".loubox")
		$(this).css({"color":"#f00"});
		$(this).html($(this).siblings().html())
		},function(){
			var index=$(this).index(".loubox")
			$(this).html(index+1+"F")
		    $(this).css("color","#2b2b2b");
		});
		 $(".lou1").on("click",function(){
			   $("body,html").stop().animate({scrollTop: 1550}, 600);
				return false;
		 });
	 	$(".lou2").on("click",function(){
			   $("body,html").stop().animate({scrollTop: 2860}, 600);
				return false;
		 });
	
	 	$(".lou3").on("click",function(){		       
			   $("body,html").stop().animate({scrollTop: 3270}, 600);
				return false;
		 });
	
	 	$(".lou4").on("click",function(){		       
			   $("body,html").stop().animate({scrollTop: 3690}, 600);
				return false;
		 });
		 
	 	$(".lou5").on("click",function(){		       
			   $("body,html").stop().animate({scrollTop: 4100}, 600);
				return false;
		 });
	
	 	$(".lou6").on("click",function(){		       
			   $("body,html").stop().animate({	scrollTop: 4515}, 600);
				return false;
		 });
		 
		$(".lou7").on("click",function(){		       
			   $("body,html").stop().animate({scrollTop: 4930}, 600);
				return false;
		 });
		
		
	  var windowwidth=$(window).width();
	  if(windowwidth<=1200){
		    $(".lou").css("left","-50px");
		  }
	  if(windowwidth>1200){
		    $(".lou").css("left",(windowwidth-1200)/2-49);
		  }
	$(window).resize(function(){ 
	  var windowwidth=$(window).width()
	  if(windowwidth<=1200){
		    $(".lou").css("left","-50px");
		  }
	  if(windowwidth>1200){
		    $(".lou").css("left",(windowwidth-1200)/2-49);
		  }
	});  
	
	//滚动条到一定距离 （返回顶部显示）
   $(document).scroll(function(){
     	if($(window).scrollTop()>=500){
		   $(".backtop").fadeIn();
		   $(".lou").fadeIn();
		}else{
			$(".backtop").fadeOut(); 
			$(".lou").fadeOut();
		}		  
   });
	
	$(".backtop").click(function(){
		$("body,html").stop().animate({
            scrollTop: 0
        }, 600);
        return false;    
    });
}


