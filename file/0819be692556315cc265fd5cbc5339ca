<script type="text/javascript" src="/js/public/searchPrompt.js"></script>	  
<script type="text/javascript">
		
//阻止冒泡事件，点击其他空白处下拉框隐藏
$(".set").on("click", function(e){
	
    $(document).one("click", function(){
        $(".new").hide();
    });

    e.stopPropagation();
});
//阻止冒泡事件，点击其他空白处下拉框隐藏 end
		
	$(function(){
		var searchType = "";
		if(""!=""){
			searchType = "商品";			
			$("#prodWrap").addClass("backr");
		}else if(""!=""){
			searchType = "商铺";
			$("#storeWrap").addClass("backr");
		}else if(""!=""){
			searchType = "市场";
			$("#marketWrap").addClass("backr");
		}else if(""!=""){
			searchType = "震海代";
			$("#prodzhdWrap").addClass("backr");
		}else{
			searchType = "商品";
			$("#prodWrap").addClass("backr");
		}
		$("#searchType").val(searchType);
		
	});	
		
	function onSearch(){
		$(document).keyup(function(event){
			if(event.keyCode == 13){
				if($("#keyword").is(":focus")){
					searchKeyword();
				}else{
					
				}
				
		    }
		});
		
	}
	 
	function searchKeyword(){
		var keyword = $('#keyword').val();
		var searchType = $('#searchType').val();
		if(searchType == "商品"){
			if(keyword){
				document.location.href = constants.Domain.search+'/prodSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/prodSearch.do';
			}
		}else if(searchType == "商铺"){
			if(keyword){
				document.location.href = constants.Domain.search+'/storeSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/storeSearch.do';
			}
		}else if(searchType == "震海代"){
			if(keyword){
				document.location.href = constants.Domain.search+'/prodZHDSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/prodZHDSearch.do';
			}
		}else{
			if(keyword){
				document.location.href = constants.Domain.search+'/marketSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/marketSearch.do';
			}
		}
	} 
	 
	 function searchKeyword1(){
		var keyword = $('#keyword1').val();
		var searchType = $('#searchType').val();
		if(searchType == "商品"){
			if(keyword){
				document.location.href = constants.Domain.search+'/prodSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/prodSearch.do';
			}
		}else if(searchType == "商铺"){
			if(keyword){
				document.location.href = constants.Domain.search+'/storeSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/storeSearch.do';
			}
		}else if(searchType == "震海代"){
			if(keyword){
				document.location.href = constants.Domain.search+'/prodZHDSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/prodZHDSearch.do';
			}
		}else{
			if(keyword){
				document.location.href = constants.Domain.search+'/marketSearch.do?c.keyword='+encodeURIComponent(keyword);
			}else{
				document.location.href = constants.Domain.search+'/marketSearch.do';
			}
		}
	} 
	 
	$(".search-box .choose-list ul a").click(function(){
	    $(this).parent().siblings().removeClass("backr")
		$(this).parent().siblings().children("a").css({
			"color":"#2b2b2b",
		    "display":"block"
			
		})
	    $(this).parent().addClass("backr")
		$(this).css({
			"color":"#ffffff",
		    "text-decoration":"none"
		})
	 	var searchType = $(this).text();
	 	$("#searchType").val(searchType);
		
		var type = $(this).text();
	 	
	 	$("p[class=set]").text(type);
	});
	 
	
	$("#keyword").change(function(){
		$("#keyword1").val($(this).val()); 
	});
	
	
	$("#keyword1").change(function(){
		$("#keyword").val($(this).val()); 
	});
</script>

 
