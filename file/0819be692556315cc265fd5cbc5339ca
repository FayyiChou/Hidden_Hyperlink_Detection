<div class="alert" id="activityPage">
		<div class="alert_zq">
			<i class="close" id="activityPageClose"></i>
		</div>
</div>
<script type="text/javascript" src="/js/jquery-1.7.2.min.js"></script>
<script type="text/javascript" src="/js/jquery.cookie.js"></script> 
<script type="text/javascript">
$(function(){
	$("#activityPageClose").click(function  () {
		$("#activityPage").hide();
    });

	$("#activityPageClose").click(function(){
		$("#activityPage").hide();
    });
	
	isActivityPageTic();
});


function isActivityPageOpen(){
	//如果获取isOpenPage的cookie为空,说明第一次进，显示中秋提示和设置顺便设置cookie		
    var time = $.cookie("isOpenPage");
    var now  = new Date();
    if(isNaN(time)){
        time = undefined;
    }
    if(time == undefined || now - time > 300000){
        //创建cookies对象并且给其赋值，设置了生命周期为1天
        $.cookie("isOpenPage",now.getTime(),{expires:1,path:'/'});
        $("#activityPage").show();
    }
}


    


//活动弹窗
function isActivityPageTic(){
	

	var userAgent = window.navigator.userAgent; 
	
    var isOpera = userAgent.indexOf("Opera") > -1;
    
    if (isOpera) {//判断是否Opera浏览器
    	isActivityPageOpen();
    }else if (userAgent.indexOf("Firefox") > -1) {//判断是否Firefox浏览器
    	isActivityPageOpen();
    }else if (userAgent.indexOf("Chrome") > -1){//判断是否Chrome浏览器
    	isActivityPageOpen();
    }else if (userAgent.indexOf("Safari") > -1) {//判断是否Safari浏览器
    	isActivityPageOpen();
    }else if (userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1 && !isOpera) {
    	isActivityPageOpen();
    } //判断是否IE浏览器
}
</script>

	
	
