//判断用户名是否包含汉字
function strlen(str,n){ 
	var r=/[^\x00-\xff]/g; 
	if(str.replace(r,"**").length<=n){
		return str;
	} 
	var m=Math.floor(n/2); 
	for(var i=m;i<str.length;i++){ 
		if(str.substr(0,i).replace(r,"**").length>=n){ 
		return str.substr(0,i)+".."; 
		} 
	} 
	return str; 
} 

//切换商圈市场
function showCityMarket(){
	var cityCode = $.cookie("currentSelectCity");
	var cityName = $.cookie("currentSelectCityName"); 

	$("#citySpan").html("<span>"+cityName+"市实体批发市场</span>");
	$("#showCityMarket").empty();
	var cityMarket = "/city/"+cityCode+".jsp";
	if( typeof(cityCode) == "undefined" ||cityCode == ""){
		$.get("/city/440100.jsp", function(data){ 
			$("#showCityMarket").append(data);
		});
	}else{
		$.get(cityMarket, function(data){ 
			$("#showCityMarket").append(data);
		});
	}	
}

//设置cookie
function setCookie(value,name){

	$.cookie('currentSelectCity',value, {expires: 999999,path: '/' ,domain: '.zhenhaipifa.com'}); 
	$.cookie('currentSelectCityName',name, {expires: 999999,path: '/' ,domain: '.zhenhaipifa.com'});
	//$("#citySpan").html("<span>"+name+"市实体批发市场</span>"); 
 	//showCategoryJson();

	showCityMarket();
	loadMarketPage(value);
	
	//如果每次切换城市时，如果是北京的话，显示北京相应的轮播图模块
	if(name=="北京"){
		$("#citySpan").html("<span>"+name+"市批发市场</span>"); 
		$("#banner").hide();
		$("#banner_id").hide();
		$("#controler").hide();
		$("#bannerBj").show();
		$("#banner_id_bj").show();
		$("#controler2").show();
		$("#ISBJ").html("<div class=topadbox id=ISBJ><a  href=javascript:void(0) style=background:url(http://www.zhenhaipifa.com/images/public/bjtoppic.jpg) no-repeat center;height=90px;></a></div>");
		$("#box1").hide();
	}else{
		$("#citySpan").html("<span>"+name+"市实体批发市场</span>"); 
		$("#banner").show();
		$("#banner_id").show();
		$("#controler").show();
		$("#bannerBj").hide();
		$("#banner_id_bj").hide();
		$("#controler2").hide();
		$("#ISBJ").html("<div class=topadbox id=ISBJ><a target='_blank' href='javascript:void(0)' style='background:url(http://img01.zhenhaipifa.com/p/201601/a45c7e57266c4cd98b78e3718ab2df6807523.jpg) no-repeat center;height=90px;'></a></div>");
		$("#box1").show();
	}
	
	
}


//登录~判断是否登陆
function indexLeftIsLogin() {
	$.ajax({
		url : ""+constants.Domain.passport+"/isLogin.do",
		type : "POST",
	    dataType:"jsonp",
		success : function(data){
			if(data.state=='notLogin'){
				$("#leftLoginBox").show(); 
			}else{
				$("#userconlogin").hide();
				$("#leftLoginSuccess").show(); 
				//显示头像
				var userName = decodeURIComponent(getCookie("username"));
				if(userName != null && userName != ''){ 
					$(".hyy").html("<span>"+strlen(userName,6)+"</span>,亲亲欢迎您！");
 
				}
				var userPic = getCookie("userPic");
				if(userPic!=null&&userPic!=''){
					$("#userPic").attr("src",decodeURIComponent(userPic));
				}
				//判断用户类型
				var userType = getCookie("userType");
				var storeId = getCookie("storeId");
				var storeState = getCookie("storeState");
				if(userType==1 && storeState == 1){
					$.ajax({
						url : "/getWaitPayAndWaitDelivery.do?storeId="+storeId,
						type : "POST",
						success : function(data){
							$("#orderinfonum").append("<span><a href='"+constants.Domain.trade+"/trade/sellerOrder/waitPayOrder.do'>待付款订单（<em class='red'>"+data.WAITPAY+"</em>）</a></span>");
							$("#orderinfonum").append("<span><a href='"+constants.Domain.trade+"/trade/sellerOrder/waitDeliveryOrder.do'>未发货商品（<em class='red'>"+data.WAITDELIVERY+"</em>）</a></span>");
						},error : function(){
							alert("系统出现异常");
						}
					});
				}else{
					$.ajax({
						url : "/getWaitPayAndWaitConfirm.do?userName="+userName,
						type : "POST",
						success : function(data){
							$("#orderinfonum").append("<span><a href='"+constants.Domain.trade+"/trade/buyerOrder/waitPayOrder.do'>待付款订单（<em class='red'>"+data.WAITPAY+"</em>）</a></span>");
							$("#orderinfonum").append("<span><a href='"+constants.Domain.trade+"/trade/buyerOrder/waitConfirmOrder.do'>待确认收货（<em class='red'>"+data.WAITCONFIRM+"</em>）</a></span>");
						},error : function(){
							alert("系统出现异常");
						}
					});
				}
			}
		},error : function(){
			alert("系统出现异常");
		}
	});
}





//加载用户信息~用于显示
function loadUserInfo(){	
	$.ajax({
		url:ctx + "/user/userInfo.do",
		dataType:"json",
		success:function(data){
			if(data!=null){
				var userRole = "";
				$("#user_info_name").parent().siblings("a").remove();				
				if(data.userType==0){
					userRole = '采购商';
					$("#user_info_name").parent().after("<a class='aa' href='"+constants.Domain.trade+"/trade/buyerOrder/waitConfirmOrder.do' >"+data.waitConfirm+"<br/><span>待收货</span></a>");
					$("#user_info_name").parent().after("<a class='aa' href='"+constants.Domain.trade+"/trade/buyerOrder/waitPayOrder.do' >"+data.waitpay+"<br/><span>待付款</span></a>&nbsp;&nbsp;&nbsp;&nbsp;");	

					$("#user_info_name_rule").text(userRole);
					$("#user_info_name_rule").show();
				}else if(data.userType==1){
					userRole = '批发商';
					$("#user_info_name").parent().after("<a href='"+constants.Domain.trade+"/trade/sellerOrder/waitDeliveryOrder.do' class='aa'>"+data.waitdelivery+"<br/><span>待发货</span></a>");
					
					$("#user_info_name").parent().after("<a class='aa' href='"+constants.Domain.trade+"/trade/sellerOrder/waitPayOrder.do'>"+data.waitpay+"<br/><span>待付款</span></a>&nbsp;&nbsp;&nbsp;&nbsp;");							
						$("#user_info_name_rule").text(userRole);
						$("#user_info_name_rule").show();
				}else{
					$("#user_info_name_rule").hide();
				}
				if(data.userPic!=null){
					$("#userPicimg").attr("src",$("#imageSevUrl").val()+'/'+data.userPic);	
				}				
				
				$("#user_info_name").html(data.userName);	
				$("#user_info_name").attr({'title':data.userName});
			}else{
				//$("#user_info_name").html("你好");
				$("#user_info_name_rule").hide();
			}	
		}
	});
	$("#userPicimg").on("click",function(){
		window.location.href = ""+constants.Domain.member+"/index.do"
	});
	
}

//加载左边楼层导航
function loadFloor(){
	// 楼层开始
		$(window).scroll(function(){
			if($(window).scrollTop()>=600){
					   $(".backtop").fadeIn();
					   $(".boxlou").fadeIn();
					  }else{
						   $(".backtop").fadeOut(); 
						   $(".boxlou").fadeOut();
						  
						  }		  
			$(".backtop").click(function(){
					$("body,html").stop().animate({
						scrollTop: 0
					}, 600);
			})
			var ling = $(document).scrollTop();
			// 在标题栏显示滚动的距离
			//document.title = ling;
			// 如果滚动距离大于900的时候让滚动框出来
			if(ling>900){
				$('#boxlou').show();
			}
			if(ling>900 && ling<1550){
				// 让第一层的数字隐藏，文字显示，让其他兄弟元素的li数字显示，文字隐藏
				$('#boxlou ul li').eq(1).find('.num').hide().siblings('.word').css({
					'display':'block',
					'background':'#ea5f8d',
					'color':'#fff'
					});
				$('#boxlou ul li').eq(1).siblings('li').find('.num').css('display','block').siblings('.word').hide();
			}else if(ling<2200){
				$('#boxlou ul li').eq(2).find('.num').hide().siblings('.word').css({
					'display':'block',
					'background':'#615c99',
					'color':'#fff'
				});
				$('#boxlou ul li').eq(2).siblings('li').find('.num').css('display','block').siblings('.word').hide();
			}else if(ling<2750){
				$('#boxlou ul li').eq(3).find('.num').hide().siblings('.word').css({
					'display':'block',
					'background':'#19c8a9',
					'color':'#fff'
				});
				$('#boxlou ul li').eq(3).siblings('li').find('.num').css('display','block').siblings('.word').hide();
			}else if(ling<3450){
				$('#boxlou ul li').eq(4).find('.num').hide().siblings('.word').css({
					'display':'block',
					'background':'#6492d8',
					'color':'#fff'
				});
				$('#boxlou ul li').eq(4).siblings('li').find('.num').css('display','block').siblings('.word').hide();
			}else if(ling<4040){
				$('#boxlou ul li').eq(5).find('.num').hide().siblings('.word').css({
					'display':'block',
					'background':'#ec9e7f',
					'color':'#fff'
				});
				$('#boxlou ul li').eq(5).siblings('li').find('.num').css('display','block').siblings('.word').hide();
			}else if(ling<4600){
				$('#boxlou ul li').eq(6).find('.num').hide().siblings('.word').css({
					'display':'block',
					'background':'#64c333',
					'color':'#fff'
				});
				$('#boxlou ul li').eq(6).siblings('li').find('.num').css('display','block').siblings('.word').hide();
			}
			if(ling<900){
				// $('#box').css('display','none');  // @ 这一句和下一句效果一样。
				$('#boxlou').hide();
			}
			
			
		});

	$('#boxlou ul li').hover(function(){
			$(this).find('.num').hide().siblings('.word').css({'display':'block','background':'#CB1C39','color':'white'});
	},function(){
		
			$(this).find('.num').css({'display':'block','background':'white','color':'#666'}).siblings('.word').hide().css({'display':'none','background':'','color':''});
	});
	
	
	  //点击楼层，滚动到对应位置
	  $(".word").eq(1).on("click",function(){
			   $("body,html").stop().animate({scrollTop:1100},600)
		  });
	  $(".word").eq(2).on("click",function(){
			   $("body,html").stop().animate({scrollTop:1748},600)
		  });
	  $(".word").eq(3).on("click",function(){
			   $("body,html").stop().animate({scrollTop:2395},600)
		  });
	  $(".word").eq(4).on("click",function(){
			   $("body,html").stop().animate({scrollTop:3045},600)
		  });
	  $(".word").eq(5).on("click",function(){
			   $("body,html").stop().animate({scrollTop:3695},600)
		  });
	  $(".word").eq(6).on("click",function(){
			   $("body,html").stop().animate({scrollTop:4345},600)
		  });
	  var windowwidth=$(window).width();
	  if(windowwidth<=1200){
		    $("#boxlou").css("left","-60px");
	  }
	  if(windowwidth>1200){
		    $("#boxlou").css("left",(windowwidth-1200)/2-42);
	  }
	  $(window).resize(function(){ 
		  var windowwidth=$(window).width();
		  if(windowwidth<=1200){
				$("#boxlou").css("left","-60px");
		  }
		  if(windowwidth>1200){
				$("#boxlou").css("left",(windowwidth-1200)/2-42);
		  }
	  });
	
}



// 初始化时根据IP地址加载城市信息
function initLoadCityByIpLocation(){
	
	if(remote_ip_info["city"]!=null){
		var city = remote_ip_info["city"];
		var value = null;
		if(city=="广州")
			value = "440100";
		else if(city=="北京")
			value = "110100";
		else if(city=="德州")
			value = "371400";
		else if(city=="杭州")
			value = "330100";
		else if(city=="义乌")
			value = "330782";
		else if(city=="沈阳")
			value = "210100";
		else if(city=="天津")
			value = "120000";
		else if(city=="上海")
			value = "310100";
		else if(city=="重庆")
			value = "500100";
		else if(city=="南京")
			value = "320100";
		else if(city=="杭州")
			value = "330100";
		else if(city=="哈尔滨")
			value = "230100";
		else if(city=="合肥")
			value = "340100";
		else if(city=="福州")
			value = "350100";
		else if(city=="南昌")
			value = "360100";
		else if(city=="济南")
			value = "370100";
		else if(city=="郑州")
			value = "410100";
		else if(city=="武汉")
			value = "420100";
		else if(city=="长沙")
			value = "430100";
		else if(city=="南宁")
			value = "450100";
		else if(city=="成都")
			value = "510100";
		else if(city=="贵阳")
			value = "520100";
		else if(city=="大连")
			value = "210200";
		else if(city=="无锡")
			value = "320200";
		else if(city=="温州")
			value = "330300";
		else if(city=="厦门")
			value = "350200";
		else if(city=="青岛")
			value = "370200";
		else if(city=="烟台")
			value = "370600";
		else if(city=="深圳")
			value = "440300";
		else if(city=="昆明")
			value = "530100";
		else if(city=="拉萨")
			value = "540100";
		else if(city=="西安")
			value = "610100";
		else if(city=="兰州")
			value = "620100";
		else if(city=="西宁")
			value = "630100";
		else if(city=="银川")
			value = "640100";
		else if(city=="乌鲁木齐")
			value = "650100";
		else if(city=="长春")
			value = "220100";
		else if(city=="石家庄")
			value = "130100";
		else if(city=="呼和浩特")
			value = "150100";
		else
			vlaue = null;
		if(value != null){
			setCookie(value,city);
		}
	}
	
}

//初始化导航栏下拉到一定位置顶部显示    下拉菜单
function initbottomMenuEvent(){
	$(".choose-list02 p").click(function(){
		var ul=$(".new");
		if(ul.css("display")=="none"){
			ul.slideDown();
		}else{
			ul.slideUp();
		}
	});
	
	$(".set").click(function(){
		var _name = $(this).attr("name");
		if( $("[name="+_name+"]").length > 1 ){
			$("[name="+_name+"]").removeClass("select");
			$(this).addClass("select");
		} else {
			if( $(this).hasClass("select") ){
				$(this).removeClass("select");
			} else {
				$(this).addClass("select");
			}
		}
	});
	
	$(".choose-list02 li").click(function(){
		var li=$(this).text();
		$(".choose-list02 p").html(li);
		$(".new").hide();
		/*$(".set").css({background:'none'});*/
		$("p").removeClass("select") ;   
		$("#searchType").val(li);
		$(".search-box .choose-list ul li").each(function(){			
			if($(this).children("a").text()==li){
				 $(this).siblings().removeClass("backr");
				 $(this).siblings().children("a").css({"color":"#2b2b2b","display":"block"});
				 $(this).addClass("backr")
			     $(this).children("a").css({"color":"#ffffff","text-decoration":"none"});
			}
		});
	});
	
	
}


</script>


</body>
</html>
