<script type="text/javascript">
$(function(){ 
	init();
	islogin();
	changeTopNavigationUrl();
	custServ();
	window.setInterval(function(){
		var prodCount = getCookie("cartProdCount");
		var userId = getCookie("userId");
		if(prodCount==null||prodCount==""||userId==""){
			$("#cartProdCount").html("(0)");
		}else{
			$("#cartProdCount").html("(" + prodCount + ")");
		}
	}, 1000);
	
	window.setTimeout("lick()",10000);
});

function loadappPage(){
	window.open('http://www.zhenhaipifa.com/page/app/appindex.jsp');
}

function lick(){$("#layer").hide();}

function init(){
	//顶部导航显示与隐藏子菜单
	$(".navxl").hover(
			function(){
				$(this).addClass("account-hover");	
				$(this).children("ul").show();
			},function(){
				$(this).children("ul").hide()
				$(this).removeClass("account-hover");
				
			}
		);
	$(".xiala").hover(function(){
		$(this).addClass("dis-win");
		
	},function(){
		$(this).removeClass("dis-win");	
	});

}

function getCookie(name){
	var cookies=document.cookie;
	var arrCookie=cookies.split("; ");
	for(var i=0;i<arrCookie.length;i++){
		var arr=arrCookie[i].split("=");
		if(arr[0]==name)return arr[1];
	}
	return "";
}

//跳转(如果登录就到相应页面，如果没登录就到登录页面)
function openUrl(obj){
// 	if($("#sessionUser").val() != null || $.cookies.get("username") != null){
	if(getCookie("username") == null){
		window.open(constants.Domain.passport +"/page/login.jsp");
		
	}else{
		window.open(obj.getAttribute("ctx"));
	}
}

function openSellerUrl(obj){
// 	if($("#sessionStore").val() != null || $.cookies.get("storeId") != null){
	if(getCookie("username") == ""){
		window.open(constants.Domain.passport +"/page/login.jsp");
	}
	if(getCookie("storeId") != ""){
		window.open(constants.Domain.trade+obj.getAttribute("ctx"));
	}
	if(getCookie("username") != "" && getCookie("storeId") == ""){
		alert("您还没有开通店铺！");
	}
}

function openCategoryUrl(obj){
// 	if($("#sessionStore").val() != null || $.cookies.get("storeId") != null){
	if(getCookie("username") == ""){
		window.open(constants.Domain.passport +"/page/login.jsp");
	}
	if(getCookie("storeId") != ""){
		window.open(constants.Domain.member+obj.getAttribute("ctx"));
	}
	if(getCookie("username") != "" && getCookie("storeId") == ""){
		alert("您还没有开通店铺！");
	}
}

//已买到的商品
function openBuyerUrl(obj){
	if(getCookie("username") == null){
		window.open(constants.Domain.passport +"/page/login.jsp");
	}else{
		window.open(constants.Domain.trade+obj.getAttribute("ctx"));
	}
}

//收藏本站
function addFavorite(url,title) {
    try {
        window.external.addFavorite(url, title);
    }catch (e) {
        try {
            window.sidebar.addPanel(title, url, "");
        }catch (e) {
            alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请使用Ctrl+D进行添加");
        }
    }
}

//判断是否登陆
function islogin() {

	$.ajax({
		url : constants.Domain.passport+"/isLogin.do",
		type : "POST",
		 dataType:"jsonp",
			success : function(data){
				if(data.state=='notLogin'){
				$("#loginStateHtml").html("<a href='"+ constants.Domain.passport +"/page/login.jsp' class='q-login' target='_self' style='font-size:14px;color:#1470bb'><img src='/images/public/qin.png' style='vertical-align:middle;margin-top:-4px'/> 请登录</a> | <a href='"+ constants.Domain.passport +"/page/register.jsp' target='_self'  style='font-size:14px'>免费注册</a>");
			}else{
				var userName = decodeURIComponent(getCookie("username"));
				if(userName != null && userName != ''){
					$("#loginStateHtml").html("<a href='"+constants.Domain.member+"/index.do' class='q-login' target='_self'><span>"+ userName +"</span></a>&nbsp;亲亲欢迎您的访问&nbsp;<a href='"+ constants.Domain.passport +"/loginOut.do' target='_self'>&nbsp;退出</a>");;
				}
				var storeId = getCookie("storeId");
				if(storeId != null && storeId != ''){
					$(".account.a").prepend("<li><a href='"+constants.Domain.member+"/storeView/index.do?storeId="+storeId+"' target='_blank'>我的店铺</a></li>");
					$(".account.a").prepend("<li><a href='javascript:void(0)' ctx='/prod/cate/category.do' onclick='openCategoryUrl(this)'>产品发布</a></li>");
				 }
			}
		},error : function(){
			alert("系统出现异常");
		}
	});
}




