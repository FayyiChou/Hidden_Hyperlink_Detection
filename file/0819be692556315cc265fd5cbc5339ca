<script>
function getCityCookie(){
	var cityCookie = $.cookie("currentSelectCity"); 
	var cityName = $.cookie("currentSelectCityName"); 

    if(cityCookie==undefined || cityCookie == ""){
	 	$("#citySpan").html("<span>全国实体批发市场</span>"); 		
	}else{
		if(cityCookie=="110100"){
			$("#citySpan").html("<span>"+cityName+"市批发市场</span>");
		}else{
			$("#citySpan").html("<span>"+cityName+"市实体批发市场</span>");
		}
		
	}
		 
}


</script>
