// 	}else{
// 		$("#loginStateHtml").html("<b>震海网</b>，<a href='/member/index.do' class='q-login' target='_self'>"+ userName +"</a>欢迎您的访问<a href='/loginOut.do' target='_self'>退出</a>");
// 	}
// }

//修改网站导航url
function changeTopNavigationUrl(){
	$("#topHelperUrl").attr("href",constants.helperCenterUrl);
	$("#topIndexUrl").attr("href",constants.indexUrl);
	$("#topMarketUrl").attr("href",constants.marketUrl);
	$("#topRentUrl").attr("href",constants.rentUrl);
	$("#topInfoUrl").attr("href",constants.infoUrl);
}

//login
function login(){
	window.open(""+constants.Domain.passport+"/page/login.jsp");
}

//cartProd
function cartProd(){
	if(getCookie("username") == null || getCookie("username") == ""){
		window.open(constants.Domain.passport +"/page/login.jsp");
	}else{
		window.location.href = ""+constants.Domain.trade+"/cart/cartStoreList.do";
	}
}

//memberUrl
function openMemberUrl(obj){
	if(getCookie("username") == null){
		window.open(""+constants.Domain.passport +"/page/login.jsp");
		
	}else{
		window.open(""+constants.Domain.member+obj.getAttribute("ctx"));
	}
	
}

//openRegisterUrl
function openRegisterUrl(){
	window.location.href = ""+constants.Domain.passport+"/page/register.jsp";
}

function webQQ(num){
	if(num==1){
		window.open("http://wpa.b.qq.com/cgi/wpa.php?ln=1&key=XzkzODAxNTk1Nl8zMTU3OTlfNDAwOTkyMTE3N18yXw");
	}
	else if(num==2){
		window.open("http://wpa.b.qq.com/cgi/wpa.php?ln=1&key=XzkzODAxNTk1Nl8zMTU3OTVfNDAwOTkyMTE3N18yXw");
	}
	else{
		window.open("http://wpa.b.qq.com/cgi/wpa.php?ln=1&key=XzkzODAxNTk1Nl8zMTU4MDFfNDAwOTkyMTE3N18yXw");
	}
}

function custServ(){
 
$.ajax({
		url : constants.Domain.www+"/custServ/list.do", 
		type : "POST",
		dataType:"jsonp",
		success : function(data){
			for(var i = 0;i<data.length;i++){	
				 
				$("#custServLi").prepend("<li class='online'><a href='http://wpa.qq.com/msgrd?v=3&uin="+data[i].account+"&site=qq&menu=yes'  target='_blank'><img border='0' src='http://wpa.qq.com/pa?p=2:"+data[i].account+":52' alt='点击这里给我发消息' title='点击这里给我发消息'/>"+data[i].name+"</a></li>");
			} 
		},error : function(){
			alert("系统出现异常");
		}
	});
}
