<script type="text/javascript">
//点击类目进行搜索
function searchCategory(catId,catLabel) { 
//  	alert("Sfsdf");
	var url = ""+constants.Domain.search+"/prodSearch.do"; 
	$('#parentCatId').val(catId); 
	$('#parentCatLabel').val(catLabel);
// 	$('#categoryForm').attr("action", url);
// 	$('#categoryForm').submit();
	
	window.open(url+"?c.pId="+catId+"&c.pLabel="+catLabel);
} 

//点击市场进入详情页面
function queryMarket(marketId) { 
//  	alert("Sfsdf");
	var url = ""+constants.Domain.search+"/market/marketDetail/detail.do"; 
	window.open(url+"?marketId="+marketId);
} 


 
</script>  
