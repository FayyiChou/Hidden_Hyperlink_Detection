	window.onload = function(){
		var oSertwo = document.getElementById("seurm-two");
		var oStwoli = oSertwo.getElementsByTagName("li");
		var oTwozj    = document.getElementById("serum-twozj");
		var oTwozjli = oTwozj.getElementsByTagName("div");
			for(var i = 0; i<oStwoli.length; i++){
				oStwoli[i].index = i;
				oStwoli[i].onmouseover = function(){
					for( var j= 0 ; j<oStwoli.length; j++){
							oTwozjli[j].style.display = "none"
						}
					oTwozjli[this.index].style.display ="block"
				}
				
				oStwoli[i].onmouseout = function(){
					for( var j= 0 ; j<oStwoli.length; j++){
							oTwozjli[j].style.display = "none"
						}
				}
			}
		var oSerum = document.getElementById("serum-price");
		var oSli = oSerum.getElementsByTagName("li");
		var oZj    = document.getElementById("serum-zj");
		var oZjli = oZj.getElementsByTagName("div");
			for(var i = 0; i<oSli.length; i++){
				oSli[i].index = i;
				oSli[i].onmouseover = function(){
					for( var j= 0 ; j<oSli.length; j++){
							oZjli[j].style.display = "none"
						}
					oZjli[this.index].style.display ="block"
				}	
				
				oSli[i].onmouseout = function(){
					for( var j= 0 ; j<oSli.length; j++){
							oZjli[j].style.display = "none"
						}
				}
			}
		
	}
	
    var tempArr = [];
	$('.step').each(function(i, o){
         var top = $(o).offset().top;
         tempArr.push(top);
    });

    $('.placetrigger').bind('click', function(){
        var index = $(this).index(),
            dist = tempArr[index];
        $('html,body').animate({scrollTop: dist}, 500);
    });
