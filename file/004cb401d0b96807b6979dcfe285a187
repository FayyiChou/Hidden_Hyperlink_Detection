function TemplateReplace(str_from ,str_to , str_body)
{
	var a = str_body.replace('{'+ str_from + '}',str_to);
	var b = a.replace('%7B' + str_from + '%7D' , str_to);
	return b.replace('%7b' + str_from + '%7d' , str_to);
}
function TrackOnLoad( data ,log)
{
	var template1 = document.getElementById("recent_visitor").innerHTML;
	var template2 = document.getElementById("my_visited").innerHTML;
	var user_id;
	var html1='';
        var html2='';
	var tmp;
	var flag1,flag2;
