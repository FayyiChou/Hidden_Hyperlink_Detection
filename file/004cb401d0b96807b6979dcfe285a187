<script language="javascript">
function array(n) {
	for (var i=0; i < n; i++)
		this[i] = null;
	this.length = n;
}
var imgurl = new array(5);
var i = 0;
function add(url)
{
	imgurl[i] = url;
	i++;
}
var img_currentIndex = 0;
//向数组中添加图片
add("http://static.ci123.com/uploadfiles/161119/211054G9iM55oocGG00Z77s.jpg");
add("http://static.ci123.com/uploadfiles/161119/2108567odd22vZZNBBppU2s.jpg");
add("http://static.ci123.com/uploadfiles/161119/210854eI1kk8DD66V33xxls.jpg");
add("http://static.ci123.com/uploadfiles/161119/210745d00C4UU2008866s.jpg");
add("http://static.ci123.com/uploadfiles/161119/21071020P886ggKyymma88s.jpg");
function imageTransition(){
	document.getElementById("myimg").src = imgurl[img_currentIndex];			
	img_currentIndex = (++img_currentIndex)%5;
}
function init(){	
	var interval = setInterval(imageTransition,1500);
}
init();
</script>


