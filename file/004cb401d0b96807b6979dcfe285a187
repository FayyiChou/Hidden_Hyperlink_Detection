       	for (user_id  in data)
	{
		flag1 = true;
		tmp = TemplateReplace('username' , data[user_id]['username'] , template1);
		tmp  = TemplateReplace('avatar' , "src='"+data[user_id]['avatar']+"'" , tmp);
		html1 += TemplateReplace('nickname' , data[user_id]['nickname'] , tmp);
	}
