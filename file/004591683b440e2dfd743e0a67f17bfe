            <script>
              //TODO добавление вспомогательных классов для выпадающего меню верхнего меню
              jQuery(document).ready(function($){

                //если нет пунктов меню 3-го уровня
                $('body.html #header .navbar .aside-navigation>ul.menu>li.expanded>ul.clearlist').each(function(){
                  var t = $(this),
                      c = t.children('li').length,
                      c_leaf = t.children('li.leaf').length;

                  if(c==c_leaf){
                    t.addClass('ul_level_single');
                  }
                });
                //если пунктов меню 3-го уровня больше 5, то выводим в 2 колонки
                $('body.html #header .navbar .aside-navigation>ul.menu>li.expanded>ul.clearlist ul').each(function(){
                  var t = $(this),
                      c = t.find('li').length;

                  if(c>5){
                    t.addClass('ul_col_2');
                  }

                });
              });
            </script>
