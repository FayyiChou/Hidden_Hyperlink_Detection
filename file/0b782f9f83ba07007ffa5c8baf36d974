<script type="text/javascript" src="//m.4008823823.com.cn/activities//libs/requirejs/require.js?v=1491081312523"></script>
<script type="text/javascript">
	//是否走offline模式
	function isOffline(key,version){
		var resourceVersion = null;
		for(var i=0;i<localStorage.length;i++){
			var arr = localStorage.key(i).split('@');
			if (arr && arr.length > 1 && key == arr[0]) {
				resourceVersion = arr[1];
				break;
			}
		}
		//要获取的版本号和本地版本是否一致，一致的话，返回true
		if (version == resourceVersion) {
			return true;
		}
		localStorage.removeItem(key+"@"+resourceVersion);
		return false;
	}
	function getJs(sUrl,key,version) {
		if (isOffline(key,version)) {
			eval(localStorage.getItem(key+"@"+version));
		}else{
			var ie = navigator.userAgent.indexOf('MSIE') > 0;
       		var o = ie ? new ActiveXObject('Microsoft.XMLHTTP') : new XMLHttpRequest();
	        o.open('get', sUrl + "?" + version, false);
	        o.send(null);
	        window.localStorage.setItem(key+"@"+version,o.responseText);
	        eval(o.responseText);
		}
	}
	var jqueryjs = '//m.4008823823.com.cn/activities//libs/jquery/jquery.min.js';
	getJs(jqueryjs,"jquery.min","1.0.0");
	var canjs = '//m.4008823823.com.cn/activities//libs/canjs/can.jquery.min.js';
	getJs(canjs,"can.jquery.min","1.0.0");
</script>
