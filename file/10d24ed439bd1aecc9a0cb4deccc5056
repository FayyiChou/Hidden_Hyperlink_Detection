$(function(){

	$("#xiaotu").toggle(function(){
		$(this).next().hide();
		$(this).css("background","url(images/fang.gif) no-repeat left center");

	},function(){
		$(this).next().show();
		$(this).css("background","url(images/suo.gif) no-repeat left bottom");
	})

})
</script>
