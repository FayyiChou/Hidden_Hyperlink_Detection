					function diaocha(obj1) {
						var tmp = (obj1.scrollTop)++;
						if (obj1.scrollTop==tmp) obj1.innerHTML += obj1.innerHTML;
						if (obj1.scrollTop>=obj1.firstChild.offsetHeight) obj1.scrollTop=0;
					}
					var speed1=setInterval("diaocha(document.getElementById('diaocha1'))",40);
					document.getElementById('diaocha1').onmouseover=function() {clearInterval(speed1)};
					document.getElementById('diaocha1').onmouseout=function(){speed1=setInterval("diaocha(document.getElementById('diaocha1'))",40)};
				</script>
