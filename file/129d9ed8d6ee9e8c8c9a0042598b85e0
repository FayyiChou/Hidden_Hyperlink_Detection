<script>
    $(document).ready(function () {

        var adds = '//' + $('.locale-font').attr('data-id') + '.17zwd.com/sks.htm';
        var market = '//' + $('.locale-font').attr('data-id') + '.17zwd.com/market.htm';

        $('.search-tab-container').find('.search-tab-item').on('click', function () {
            var $_search_tab = $('.search-tab-container').find('.search-tab-item');
            $_search_tab.removeClass('tab-clicked');
            $(this).addClass('tab-clicked');
            var returnName = $(this).text().trim() == '店铺' ? true : false;
            if (returnName) {
                $('.search-hide-tab-container')
                    .find('.tab-change-name')
                    .text('店铺')
                    .attr('data-index', '1');
                $('.search-hide-tab-drop-down-container')
                    .find('a')
                    .text('宝贝')
                    .attr('data-index', '0');;
                $('#J_search').attr('action', market);
            } else {
                $('.search-hide-tab-container')
                    .find('.tab-change-name')
                    .text('宝贝')
                    .attr('data-index', '0');
                $('.search-hide-tab-drop-down-container')
                    .find('a')
                    .text('店铺')
                    .attr('data-index', '1');
                $('#J_search').attr('action', adds);
            }
        });
    });

    $('.server-icon-container').hover(function () {
        $('.server-icon-float-container')
            .hide();
        $(this)
            .find('.server-icon-float-container')
            .show();
        $(this)
            .css('color', '#f40');
    }, function () {
        $('.server-icon-float-container')
            .hide();
        $(this)
            .css('color', '#999');
    });

    var locationUrlNow = window.location.href;
    var locationUrlNow_index = locationUrlNow.lastIndexOf('###');
    locationUrlNow_index = locationUrlNow.substr(0, locationUrlNow_index) && locationUrlNow;

    locationUrlNow_index = locationUrlNow.lastIndexOf('.') && locationUrlNow;
    locationUrlNow_index = locationUrlNow.substr(0, locationUrlNow_index) || locationUrlNow;
    //过滤了###还有后缀名
 
  var _temp = '',
        returnVal = false,test;

    function eachHref(hrefTmp) {
        $('#J_Nav')
            .find('.florid-nav-container a')
            .removeClass('florid-nav-clicked');
        $('#J_Nav')
            .find('.florid-nav-container a')
            .each(function (i, element) {

                _temp = $(element).attr('href');
                if (_temp.indexOf(hrefTmp) != -1) {
                    $(element).eq(0).addClass('florid-nav-clicked');
                    returnVal = true;
                    return returnVal;
                }

            });
        return returnVal;

    }

    var tmp = eachHref(locationUrlNow_index); if (tmp != true) {
        var locationUrlNow_href = window.location.host;
        eachHref(locationUrlNow_href);
        if ($('#J_Nav').find('.florid-nav-clicked').length == 0) {
            $('#J_Nav').find('.florid-nav-container a').eq(0).addClass('florid-nav-clicked');
        }
    }


$('.search-btn')
    .click(function () {
        $('#J_search').submit();
    });
</script>

