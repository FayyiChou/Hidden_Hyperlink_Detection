var $imgOriginal = $("img[original]");
//360图片不显示问题
if($.browser.msie){
$imgOriginal.each(function(){
$(this).attr('src',$(this).attr('original'));
});
}else{
$imgOriginal.lazyload({threshold:600,effect: lazyload_effect});
}
});
