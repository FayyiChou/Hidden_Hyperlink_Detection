<script>
//前580px不显示客服
$().ready(function(){
$('.cs_close a').click(function(){
$('#kefu1').remove();
})
$(window).scroll(function(){  
var t=document.body.scrollTop || window.pageYOffset || document.documentElement.scrollTop;
if(t && t>580) $('#kefu1').css('display', 'block');
});	
})
//楼层按钮显示隐藏
$(window).scroll(function() {
var s = window.screen.height;
($(document).scrollTop()>s)?$("#backtop").show():$("#backtop").hide();
($(document).scrollTop()>s)?$("#home_nav_bar a").show():$("#home_nav_bar a").hide();
}).resize(function(){
var s = window.screen.height;
($(document).scrollTop()>s)?$("#backtop").show():$("#backtop").hide();
($(document).scrollTop()>s)?$("#home_nav_bar a").show():$("#home_nav_bar a").hide();
});
//楼层跳转
$("#home_nav_bar #floor222").click(function(){
var top=$("#woman").offset().top;$(window).scrollTop(top);
});
$("#home_nav_bar #floor66").click(function(){
var top=$("#man").offset().top;$(window).scrollTop(top);
});
$("#home_nav_bar #floor92").click(function(){
var top=$("#qingqu").offset().top;$(window).scrollTop(top);
});
$("#home_nav_bar #floor127").click(function(){
var top=$("#qiju").offset().top;$(window).scrollTop(top);
});
$("#backtop").click(function(){
var top=0;$(window).scrollTop(top);
});
//轮播
$(function(){
$(".focus_mainl").each(function(){
var timer;
var time = 9000;
$(".focus_mainl li").mouseover(function(){
var index = $(".focus_mainl li").index($(this));
changeImg(index);
}).eq(0).click();
$(".focus_mainl").hover(function(){
clearInterval(timer);
},function(){
timer = setInterval(function(){
var show = $(".focus_mainl li.focus").index();
if (show >= $(".focus_mainl li").length-1)
show = 0;
else
show ++;
changeImg(show);
},time);
});
function changeImg (index)
{
$(".focus_mainl li").removeClass("focus").eq(index).addClass("focus");
$(".focus_mainl .focus_link").attr("href",$(".focus_mainl li").eq(index).attr("link"));
var b = $(".focus_mainl li").eq(index).attr("uri");
$(".focusbox").css('background-color',$(".focus_mainl li").eq(index).attr("bgcolor"));
$(".focusbox").css('background-image',"url("+b+")");
$(".focus_mainl .bigImg").css('background-image',"url("+b+")");
}
timer = setInterval(function(){
var show = $(".focus_mainl li.focus").index();
if (show >= $(".focus_mainl li").length-1)
show = 0;
else
show ++;
changeImg(show);
},time);
});
});
$(function() { 
//精选评论悬浮效果
$("a[cat]").hover(
function () {
var catName = $(this).attr("cat");
$("a[cat]").removeClass("on");
$(this).addClass("on");
$("ul[com_mark]").css("display", "none");
$("ul[com_mark="+catName+"]").css("display", "block");
},
function () {}
);
//TOP排行榜hover效果
$("li[data='topListMark']").hover(
function () {
$(this).addClass('on');
$(this).siblings().removeClass('on');
},
function () {}
);
//母婴用品知道悬浮hover效果
$("a[zhidaoCat]").hover(
function () {
var catName = $(this).attr("zhidaoCat");
$("a[zhidaoCat]").removeClass("on");
$(this).addClass("on");
$("ul[zhidaoinfo]").css("display", "none");
$("ul[zhidaoinfo="+catName+"]").css("display", "block");
},
function () {}
);
});
</script>
