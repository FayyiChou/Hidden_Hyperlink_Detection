     $.ajax({type : "get",async:false,url : "http://www.u17.com/log/ajax.php?mod=statistics&act=showtimes&typeId=7",dataType : "json",success:function(o){}});
}

function logPageClick(stat_id) {
    if (typeof (stat_id) != 'undefined' && stat_id) {
        $.get(_cfg_host_base + "/log/ajax.php?mod=statistics&act=showtimes&typeId=" + stat_id);
    }
}

