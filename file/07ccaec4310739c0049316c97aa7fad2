<script>
var comic_id = "139048";var timestamp = "1491059354";
var comic_name = "穿梭时空追寻你";
var user_id = "";
var fans_id = "14455055";
var limit = "1";
var vote_id = "108585";
var favorite_total = "19900";
var cfg_host_static = "http://static.u17i.com";
var cfg_host_base = "http://www.u17.com";
var cfg_host_user = "http://user.u17.com";
var cfg_host_passport = "http://passport.u17.com";
var feel_1 = "6";
var feel_2 = "2";
var feel_3 = "5";
var feel_4 = "2";
var feel_5 = "3";
var feel_6 = "4";
var cover_url = "http://cover.u17i.com/2016/11/14455055_1477995725_39pnpah38wqP.big.jpg";
var siteUrl = cfg_host_base;
var comoic_tag_count = "8";
$("body").data("comic_id",comic_id);

// 别人还在看---推荐漫画
var comic_rand_num = 1;
var comic_page = 0;

function get_recommend_comic(){
    $.ajax({
        url : "/comic/ajax.php?mod=comic&act=get_recommend_comic_info",
        data : {comic_rand_num:comic_rand_num,comic_page:comic_page,comic_num:6,comic_id:comic_id},
        type : 'get',
        dataType : 'json',
        success : function(data){
          if(data.is_recommend == 1){
            var html = "";
            var num = 0;
            $.each(data.comic_info,function(k,v){
                num ++;
                html += "<li><a href='http://www.u17.com/comic/"+ v.comic_id + ".html' target='_blank' class='cover' onclick=\"_czc.push(['_trackEvent', '漫画信息页', '别人还在看', '"+ num +"','1','"+ num +"']);logPageClick(66);\">";
                html += "<img src='"+ v.cover +"'/></a><a href='http://www.u17.com/comic/"+ v.comic_id + ".html' target='_blank' class='name'>"+ v.name +"</a>"+ v.short_description +"</li>";
            });
            $('.other').empty();
            $('.other').append(html);

            comic_page = data.comic_page;
            comic_rand_num = 0;
          }else{
            $('#left_empty').show();
          }
        }
    });
}

$('#huan_recommend_comic').click(function(){
    comic_page ++;
    get_recommend_comic();
});
// 判断是否是 别人还在看 添加  换一换 连接
$(".tab a").click(function(){
    var tabname = $(this).attr("rel");
    if(tabname == 'rec-1'){
        $('#huan_recommend_comic').show();
    }else{
        $('#huan_recommend_comic').hide();
    }
});


/*广告图片跑到页面顶部*/
var oScrollBot=$('#scrollBot');
var _left=oScrollBot.offset().left;
var oldTop;
function adLayout(){
    var currentTop = oScrollBot.offset().top - $("#topbar").height();
    var scrollTop=document.documentElement.scrollTop||document.body.scrollTop;
    if((scrollTop>=currentTop && oldTop == undefined) || scrollTop>=oldTop) {
        if(!oldTop) {
            oldTop = currentTop;
        }
        oScrollBot[0].style.position='fixed';
        oScrollBot[0].style.top = $("#topbar").height()+'px';
    } else if(scrollTop < oldTop){
        oScrollBot[0].style.position='';
    }
}


//评论内容延时加载
function commentOutTime(){
    var scrollTop = $(document).scrollTop();
    var top_height = $(".recomend").offset().top;
    if(scrollTop>top_height){
        $.xcomment({
                        thread_id:20125587,
            paginate:'.page',
            comic_id:comic_id
        });
        $(window).unbind("scroll",commentOutTime)
    }
}


$(window).bind('scroll',commentOutTime);
$(window).bind('scroll',adLayout);


var user = get_user();
if(user && (user["group_admin"]==1 || user["group_admin"]==2)){
    var html=$('<div></div>');
    $.ajax({
      url: "/comic/ajax.php?mod=comic&act=get_group_admin&comic_id=139048",
      type: "GET",
      dataType : 'json',
      success: function(o){
        if (o.code >=1 ) {
          $('#admin_body').html(o.html);
        };
        
      }
    });          
}

$(document).ready(function(){
    if($('#open_gift_page').val()==1){
        show_action_dialog($('#comic_id_flag').val(),'send_gift');
    }else if($('#open_gift_page').val()==2){
        show_action_dialog($('#comic_id_flag').val(),'dashang');
    }

    
    // 首次加载推荐漫画
    get_recommend_comic();

    if($('#other_comic_look').is('.curr')){
        $('#huan_recommend_comic').show();
    }
});

$(window).resize(function() {
    //set_client_width_class();
    //set_client_tidth();
});

function set_client_tidth() {
    var w = getTotalWidth();
    if (w < 1280) {
        $('#words').html($('#words2').html());
        $('#first').hide();
        $('#tuijian').find('a').addClass('comic_gg');
        $('#start_read_em').hide();
        $('#help3').find('.helpCon').addClass('dsCon');
    } else {
        $('#words').html($('#words1').html());
        $('#start_read_em').show();
        $('#btn_wrap').show();
        $('#tuijian').find('a').removeClass('comic_gg');
        $('#help3').find('.helpCon').removeClass('dsCon');
        $('#first').show();

};
    if (w < 1666) {
        $('.ad_box_right').hide();
    } else {
        $('.ad_box_right').show();
    }
}

$('#get_coin').click(function(){
    if (user) {
         $.ajax({type : "get",async:false,url : "http://www.u17.com/log/ajax.php?mod=statistics&act=showtimes&typeId=3",dataType : "json",success:function(o){
            window.open('http://list.offer99.com/index.php?action=offerlist&pid=g49ab034cd7c61cdcc7b18cfb65c2086&userid='+o.user_id+'&alloffers=1&type=-1');
         }});
    } else {
        window.open("http://passport.u17.com/member_v2/login.php?url=renwu");
    };
     
});

