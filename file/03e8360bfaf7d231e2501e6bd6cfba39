function setContentTab2(name2, curr2, n) {
    for (i = 1; i <= n; i++) {
        var menu = document.getElementById(name2 + i);
        var cont = document.getElementById("con2_" + name2 + "_" + i);
        menu.className = i == curr2 ? "up" : "";
        if (i == curr2) {
            cont.style.display = "block";
        } else {
            cont.style.display = "none";
        }
    }
}
