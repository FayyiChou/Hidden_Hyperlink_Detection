        //在浏览窗口底部的文件类型下拉菜单中显示的文本
        'fileTypeDesc': '文档',
        //允许上传的文件后缀
        'fileTypeExts': '*.jpg; *.jpeg; *.png; *.gif; *.doc; *.docx; *.xls; *.xlsx; *.txt',
        //发送给后台的其他参数通过formData指定
        //'formData': { 'someKey': 'someValue', 'someOtherKey': 1 },
        //上传文件页面中，你想要用来作为文件队列的元素的id, 默认为false  自动生成,  不带#
        //'queueID': 'upload-process',
        //选择文件后自动上传
        'auto': true,
        //设置为true将允许多文件上传
        'multi': false,
        'removeCompleted': true,
        'uploadLimit': 3,
        'queueSizeLimit':3,
        'fileSizeLimit': '5MB',
        'overrideEvents': ['onUploadStart','onSelect','onSelectError','onCancel', 'onDialogClose', 'onQueueComplete','onUploadSuccess','onSWFReady'],
        'onUploadStart' : function (file){
          var name=file.name;
          var myself = this; 
          //不同弹框可能传相同文件，顾此处不做是否存在判断
        },
        'onSWFReady' : function () {
          var limit = $("#uploadifybottom").uploadify('settings','queueSizeLimit');
        },
    'onSelect': function (file) {
      var id = file.id;
      var name = file.name;
      var fileHeadName=name.substring(0,name.lastIndexOf('.'));
      if(fileHeadName.length>6){
              fileHeadName=fileHeadName.substring(0,6)+"...";
            }
      var suffix = name.substring(name.lastIndexOf('.')+1);
      var fileDiv = '<div id="'+id+'_DIV" class="updone">';
        //fileDiv += '<a href="/api/shop/fileUpload!downloadWord.do?(\''+id+'\');">';
        fileDiv += '<a href="/api/shop/fileUpload!downloadFile.do?name='+name+'">';
        fileDiv += fileHeadName;
        fileDiv += '<a class="del-file" href="javascript:void(0);" onclick="delUploadFile(\''+id+'\');">删除</a>';
        fileDiv += '</div>';
      $('#uploadifybottom').before(fileDiv);
      $(".tcbox .introa").hide();
      docNum++;
    },
    'onUploadSuccess': function(file, data, response){
      var obj=eval("("+data+")");
      var name=obj.message.name;
            var ivalues=name+"#"+obj.message.path;
            var suffix = name.substring(name.lastIndexOf('.')+1);
            $.post('/api/shop/requirement!updateUploadList.do',{firstFile:name,path:obj.message.path,suffix:suffix},function(data){
              if(data.result==1){
                var values="<input type='hidden' name='uploadFiles1' value='"+ivalues+"' id='"+file.id+"_f'/>";
                $("#form").append(values);
              }
            },'JSON');
            $("#subbutton").attr("disabled",false);
      $("#subbutton").attr("class","btn1");
      $("#tip").html("已上传文档：");
    },
        'onSelectError': function(file, errorCode, errorMsg, errorString){
          // $("#uploadifybottom").uploadify('disable', true);
          if(file != null){
        if(errorCode == -110){
          showUploadErrorBox();
        }else if(errorCode == -130){
          showUploadError1Box();
        }
          }
        },
    'onDialogClose': function(){},
    'onCancel': function(queueItemCount){
      if(!flag){
      var limit = $("#uploadifybottom").uploadify('settings','queueSizeLimit');
      }
    },
        'onQueueComplete' : function(queueData) {//当队列中的所有文件全部完成上传时触发
        //alert('上传成功，提交需求！');
    }
    });
  
  var options = {
			url : "/api/shop/member!doLogin.do?ajax=yes",
			type : "POST",
			async:false,
			dataType : 'json',
			success : function(data) {
				if(data.result==1){
					if(data.forward){
						window.open(data.forward);
						$(".close").trigger("click");
						location.reload();
					 	return;
					}
					//alert($("#hideform").html()+":"+$("#hideform").serialize());
					var href = location.href;
					var index = href.indexOf("?");
					//alert(index+":"+href);
					if(index!=-1){
						href =href.substring(0,index);
					}
					//alert(index+":"+href);
					var param = $("#hideform").serialize();
					if(param!=''){
						location.href=href+"?"+param;
					}else{
						location.href=data.url;
					}
				    
				}
				else{
					showError0(data.id);
				}
		  	},
			error : function(e) {
				showError0("5");
			}
		};
  
  $('#loginBtn0').click(
      function(){ 
        var userId0 = $("#userId0").val();
        var userPass0 = $("#userPass0").val();
        if(userId0 == "" && userPass0 == ""){
          showError0("0");
          return false;
        }
          if(userId0 == ""){
          showError0("1");
          return false;
        }else if(userPass0 == ""){
          showError0("2");
          return false;
        }
        $('#loginBtn0').val("正在登录...");
        $('#login_form').ajaxSubmit(options);
    });
  
    $("#userId0").focus(function(){
      $(".log_name,#userId0").css("border","1px solid #fd8240");
    });
    $("#userId0").blur(function(){
      $(".log_name,#userId0").css("border","1px solid #dbdbdb");
    });
    $("#userPass0").focus(function(){
      $(".log_pass,#userPass0").css("border","1px solid #fd8240");
    });
    $("#userPass0").blur(function(){
      $(".log_pass,#userPass0").css("border","1px solid #dbdbdb");
    });
    
    $("#userPass0").focus(function(){
      $(".log_pass,#userPass0").css("border","1px solid #fd8240");
    });
    
