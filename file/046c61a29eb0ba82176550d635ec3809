  <script type="text/javascript">
    var url = window.location.href;
    function yesNavgaoliang(){
        var url = window.location.pathname;
        var index1 = '/index.html',
                cloudService = '/v35/cloud/index.html',
                market = '/v35/market/index.html',
                itMall ='/v35/itmall/index.html',
                personPurchase='/delegatePurchase/delegatePublish.html',
                jobs='/aboutus.html'
        index = $(".pos-box li");
        if(url == cloudService){
            index.eq(1).addClass("topactive").siblings().removeClass("topactive");
        }else if(url == market){
            index.eq(2).addClass("topactive").siblings().removeClass("topactive");
        }else if(url == itMall){
            index.eq(3).addClass("topactive").siblings().removeClass("topactive");
        }else if(url == personPurchase){
            index.eq(4).addClass("topactive").siblings().removeClass("topactive");
        }else if(url == jobs){
            index.eq(5).addClass("topactive").siblings().removeClass("topactive");
        }
    }
    yesNavgaoliang(); //顶部导航高亮
</script> 
  <script type="text/javascript">
	var searchTipText = "请输入产品关键字，如品牌、型号等";
	var ctx = '';
	$("#keyword").keypress(function(event) {
		if (event.keyCode == "13") {
			search();
		}
	});
	$("#keyword").blur(function() {
		$("#hot_words").show();
	});
	$("#keyword").focus(function() {
		$("#hot_words").hide();
	});
	
	function search() {
		var keyword = $.trim($("#keyword").val());
		if (keyword == searchTipText) {
			keyword = '';
		}
		if (keyword.indexOf('\\') != -1) {
			alert("请输入正确的关键词");
			return false;
		}
		if (keyword == '') {
			alert("请输入关键字");
		} else {
			keyword = keyword.replace(/\//g, "-xie-")
			var url = encodeURIComponent("search-keyword-" + keyword + ".html");
			window.open("/" + url);
		}
	}
	
	var goodsDetailObj = {
		      pageIndex:1,
		      pageSize:20,
		      total:0,
		      getSource:function(page_index, page_size){
		    	goodsDetailObj.pageIndex = page_index;
		    	goodsDetailObj.pageSize = page_size;
		        param = this.getCondition();
		        $.ajax({
		          url : '/api/shop/comment!getCommentListJson.do',
		          type : 'post',
		          data : param,
		          dataType : 'json',
		          success : function(data){
		            if(data){
		              goodsDetailObj.total = data.totalCount;
		              $("#totalPage").text(data.totalPageCount);
		              goodsDetailObj.registePageCallBack();
		              if(data.result){
		                var list = data.result;
		                var s = "";
		                $.each(list,function(i,v){
		                	s+= '<tr>'+
						      '<td class="com_name">'+v.nick_name+'</td>'+
						      '<td class="com_com">'+v.content+'</td>'+
						      '<td class="com_num"><span class="comment_stars"><b style="width:'+v.star+'%"></b></span></td>'+
						      '<td class="com_time">'+v.ctime+'</td>'+
						    '</tr>';
		                });
		                $("#com_body").empty();
		                $("#com_body").append(s);
		                if(data.totalCount==0){
		                	$("#commentDetail").css("display", "none");
		                	$("#noCommentDetail").css("display", "block");
		                }else{
		                	$("#commentDetail").css("display", "block");
		                	$("#noCommentDetail").css("display", "none");
		                }
		              }

		            }
		          }
		        });
		      },
		      getCondition:function(){
		    	  var param={
		    	  page:goodsDetailObj.pageIndex,
			      pageSize:goodsDetailObj.pageSize,
			      'filter.sn':'103782-3'
		    	  }
		    	  return param;
		      },
		    registePageCallBack:function(){
					$("#page_numbers").pagination(goodsDetailObj.total, {
						callback: goodsDetailObj.pageselectCallback,//PageCallback() 为翻页调用次函数。
						prev_text: " &lt;",
						next_text: "&gt;",
						items_per_page: goodsDetailObj.pageSize, //每页的数据个数
						current_page: goodsDetailObj.pageIndex-1 //当前页码
					});
				},
				 /***分页回调函数**/
			    pageselectCallback:function(pageIndex, jq) {
			      goodsDetailObj.getSource(pageIndex+1,goodsDetailObj.pageSize);
			    },
			    changePageSize:function(pageSize){
			    	goodsDetailObj.pageSize =pageSize;
			    	goodsDetailObj.getSource(1, pageSize);
			    }
		  }

	$(function() {
		$("#searchBtn").click(function() {
			search();
		});
		
		$("#logout").click(function() {
			$.ajax({
				url : "../api/shop/member!logout.do?ajax=yes",
				dataType : "json",
				cache : false, //清楚缓存，暂时测试，如果产生冲突，请优先考虑是否是这条语句。
				success : function(result) {
					if (result.result == 1) {
						alert(result.message);
						location.href = "/index.html";
					} else {
						alert(result.message);
					}
				},
				error : function() {
					$.alert("出错了:(");
				}
			});
			return false;
		})
		goodsDetailObj.getSource(1,goodsDetailObj.pageSize);
	});
		
	
		
	var data = {
				goodsSourceId:137624,
				catId:98,
				product_id:10055,
				requirementTitle:'华为AR1220W-S',
				brandId:'374',
				goods_name:'华为AR1220W-S',
				stockNumber:10
		};
	
	function popxj(memberId, obj){
		data.accountDays=$(obj).attr("accountDays");
		data.tradeStyle=$(obj).attr("tradeStyle");
		if(memberId>0){
			data.supplierId=memberId;
			var json =$(obj).attr("json");
			if(json!="[]"){
				data.yescaiService=false;
				data.services=eval(json);
			}else{
				data.yescaiService=true;
			}
			data.supplyAreaServiceName=$(obj).attr("region");
			data.supplyAreaServiceId=$(obj).attr("regionId");
			data.stockNumber=$(obj).attr("stockNumber");
		}else{
		 	 data.yescaiService=true;
			 data.services=[];
			data.supplyAreaServiceName='河南>郑州市';
			data.supplyAreaServiceId=7;
		}
		show_xjbox(data);
	}
	
	function poprg(memberId, obj){
		data.accountDays=$(obj).attr("accountDays");
		data.tradeStyle=$(obj).attr("tradeStyle");
		data.gpic="http://image.yescai.com/goods/2015/08/08/201508081104193996_thumbnail.jpg";
		data.ghref="/goods/103782-3/14631/goods_detail.html";
		data.serviceFee=$(obj).attr("serviceFee");
		data.price=$(obj).attr("price");
		if(memberId>0){
			data.supplierId=memberId;
			data.goodsSourceId=$(obj).attr("goodsSourceId");
			data.ghref="/goods/103782-3/14631/goods_detail.html";
			var json =$(obj).attr("json");
			data.services="";
			if(json!="[]"){
				data.yescaiService=false;
				data.services=eval(json);
			}else{
				data.yescaiService=true;
			}
			data.supplyAreaServiceName=$(obj).attr("region");
			data.supplyAreaServiceId=$(obj).attr("regionId");
			data.stockNumber=$(obj).attr("stockNumber");
		}else{
			data.supplierId="14631";
		 	 data.yescaiService=true;
			 data.services=[];
			data.supplyAreaServiceName='河南>郑州市';
			data.supplyAreaServiceId=7;
		}
		show_rgbox(data);
	}
</script> 
