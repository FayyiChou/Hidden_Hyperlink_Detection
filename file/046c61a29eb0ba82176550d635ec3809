    	var userId0 = $("#userId0").val();
        var userPass0 = $("#userPass0").val();
        if(userId0 == "" && userPass0 == ""){
          showError0("0");
          return false;
        }
          if(userId0 == ""){
          showError0("1");
          return false;
        }else if(userPass0 == ""){
          showError0("2");
          return false;
        }
        //$('#loginBtn0').trigger("click");
        $('#loginBtn0').val("正在登录...");
        $('#login_form').ajaxSubmit(options);
    });
  
  function showError0(errorType){
		var errorHtml = $(".login_name_c"),
		    errorHtml_c = $(".login_pass_c");
		    
		if(errorType == "0"){
			errorHtml.html("请输入账户名");
			$("#userId0").focus();
		}
		else if(errorType == "1"){
			errorHtml.html("请输入账户名");
			$("#userId0").focus();
		}else if(errorType == "2"){
     
			errorHtml_c.html("请输入登录密码");
      
			$("#userPass0").focus();
		}else if(errorType == "5"){
			$("#userPass0").focus();
			errorHtml.html("出现错误，请重试");
		}else if(errorType == "6"){
			$("#userId0").focus();
			errorHtml.html("账户名密码不匹配");
		}else if(errorType == "7"){
			$("#userId0").focus();
			errorHtml.html("账户名已经被停用");
		}
		$("#userId0").keyup(function(){
			errorHtml.html("");
		});
		$("#userPass0").focus(function(){
			errorHtml_c.html("");
		});
		$('#loginBtn0').val("立即登录");		
	}
  if (memberFlag==false) {
    autologin();
  }
  
  //登录判断
function cookie(name){ 
    var cookieArray=document.cookie.split("; "); //得到分割的cookie名值对    
    for (var i=0;i<cookieArray.length;i++){    
       var arr=cookieArray[i].split("=");       //将名和值分开   
       if(arr[0]==name)return unescape(arr[1]); //如果是指定的cookie，则返回它的值    
    } 
    return "";  
}
  
function autologin(){
    var myuser=cookie("MJUser");
    if(myuser!=null&&myuser!=''){
      $.post('/api/shop/member!autoLogin.do?ajax=yes',{cookieStr:myuser},function(data){
        var obj = eval('(' + data + ')');
        if(obj.result==1){
          var forward=''; 
                     location.href=obj.url;
        }
      });
    }else{
      //查询cookie,自动填充用户登录名
      var url = "/api/shop/member!getCookie.do";
      $.post(url,{},function(data){
          if(data!=""){
            var userNames = data.split(";");
            var length=userNames.length-1;
              $("#userId").val(userNames[length]);
            $("#userId").autocomplete(userNames,{
              minChars: 0,
              width: 340,
              max:5,
              multiple:false,
              autoFill:true,
              highlight: false,
              multiple: true,
              multipleSeparator: " ",
              autoFill:true,
              scroll: true,
              scrollHeight: 300,
            });
          }
        },'TEXT'); 
    }
  }


