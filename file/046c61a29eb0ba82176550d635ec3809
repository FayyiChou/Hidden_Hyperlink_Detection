	var timer;
	function getMessageCountAndSet() {
		//获取消息
		getMessageCount();
	}

	//获取未毒消息数
	function getMessageCount(memberId){
		if (!memberId)
			return;
		var url = "/api/shop/messageApi!getMessageCount.do";
		var d = {};
		d['memberId'] = memberId;
		d['status'] = "0";
		$.post(url,d,function(data){
			$("#message_count").text(data);
			if (data>0) {
				if ($(".myms b").length==0) {
					$(".myms").append("<b></b>");
				}
				yesMessHover();
			}else{
				$(".myms b").replaceWith("");
			}
		});
	}

