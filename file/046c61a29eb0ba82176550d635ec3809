	function logOut() {
		$.ajax({
			type : "post",
			url : "/api/shop/member!logout.do",
			dataType : "json",
			data : null,
			async : false,
			success : function(data) {
				window.location.href = "/";
			},
			error : function(e) {
				alert(e);
			}
		});
	}
	function goToMyCenter() {
        if (memberFlag!=true){
            $("#logbox").show();
            $(".tcbg").show();
            $("#loginForward").val("/member/user_center.html");
            return;
        }
        window.open("/member/user_center.html");
    }
	function gotoMessage(){
        if (memberFlag!=true){
            $("#logbox").show();
            $(".tcbg").show();
            $("#loginForward").val("/member/message.html");
            return;
        }
        window.open("/member/message.html");
    }
	
	function gotoMyOrder() {
		if (memberFlag!=true){
            $("#logbox").show();
            $(".tcbg").show();
            $("#loginForward").val("/purchase/direct_order_list.html");
            return;
        }
        window.open("/purchase/direct_order_list.html");
	}

	//点击关注亿采弹框
