  <script>
    //进入购物车
    function toCart(){
        if(memberFlag==false){
            $(".tcbg").show();
            $("#logbox").show();
            return;
        }
        window.location.href = "/purchase/maincart.html";
    }
    /*回到顶部*/
    var winH = parseInt($(window).height()),scrH;
    window.onscroll = function(){
        scrH = parseInt($(window).scrollTop());
        scrH - winH >= 0?$(".fixed-top").show():$(".fixed-top").hide();
    };

    $(".fixed-top").on("click",function(){
        $(this).hide();
        $("body,html").animate({"scrollTop":0},500);
    });
    //右侧固定的二维码

    var oBtnCon = document.getElementById('contactus');
    var oBoxCon = document.getElementById('contactus-box');
    var contactusTime=null;
    oBtnCon.onmouseover=oBoxCon.onmouseover= over;
    oBoxCon.onmouseout=oBtnCon.onmouseout=out;
    function over(){
        clearTimeout(contactusTime);
        oBoxCon.style.display='block';
        oBtnCon.className="contactus contactusactive";
    }

    function out(){
        contactusTime= setTimeout(function(){
            oBoxCon.style.display='none';
            oBtnCon.className="contactus";
        },500);
    }

    $("#rqcode").mouseover(function(){
        clearTimeout(contactusTime);
        oBoxCon.style.display='none';
        oBtnCon.className="contactus";
        $("#fix_rqcode").show();
    });
    $("#rqcode").mouseout(function(){
        $("#fix_rqcode").hide();
    });

    $("#shopcart").mouseover(function(){
        clearTimeout(contactusTime);
        oBoxCon.style.display='none';
        oBtnCon.className="contactus";
    });

    //购物车大于99显示加号
    var carcount = $('#cartNumber').html();
    if(carcount>=0 && carcount<10){
        $("#moreAdd").hide();
    }else if(carcount>=10 && carcount<99){
        $("#moreAdd").hide();
    }else{
        $('#cartNumber').html("99");
        $("#moreAdd").show();
    }

</script> 
