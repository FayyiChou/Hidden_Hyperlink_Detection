    /**是否重复文件*/
  var flag=false;
  var docNum = 0;
  function submitPur(obj){
    var form=$("#purForm");
    var len=$(form).find("input").length;
    if(len==null||len==0){
      alert("请上传需求文件");
      return false;
    }else{
      var oDiv=document.getElementById("shadow");
      var oBody=document.getElementsByTagName("body")[0];
      createLoad(oBody,"亿采正在为您提交的文档加密中...");
      //
      $(obj).attr("disabled","disabled");
      $(form).submit();
    }
  }
  
  function download(obj){
    var inputVal=$(".file-name").val();
    var path=inputVal.split("#")[1];
    var name=inputVal.split("#")[0];
    window.open("/api/shop/fileUpload!downloadDoc.do?attachmentPath="+path+"&attachmentName="+encodeURIComponent(name), "");
  }
  
  function delUploadFile(obj){
    var path=$("#"+obj+"_DIV").val().split("#")[1];
    $.post('/api/shop/requirement!delUploadList.do',{path:path},function(data){
      if(data.result==1){
        $("#"+obj+"_DIV").remove();
        $("#"+obj+"_f").remove();
        docNum--;
        var uploadLimit = $("#uploadifybottom").uploadify('settings','uploadLimit');
        $('#uploadifybottom').uploadify('cancel', obj);
        $('#uploadifybottom').uploadify('settings','uploadLimit', ++uploadLimit);
        $("#uploadifybottom").uploadify('disable', false);
        /**判断是否已全部删除*/
        if(docNum==0){
          $("#tip").html("");
          $("#subbutton").attr("disabled","disabled");
          $("#subbutton").attr("class","purbtn");
          $(".tcbox .introa").show();
        }
      }
    },'JSON');
  }

  $("#uploadifybottom").uploadify({
         //指定swf文件
        'swf': '/themes/default/js/b2b/plugins/uploadify/uploadify.swf',
        //后台处理的页面
        'uploader': '/api/shop/fileUpload!uploadFile.do',
        //按钮显示的文字
        'buttonText': '上传附件',
        //显示的高度和宽度，默认 height 30；width 120
        'height': 35,
        'width': 75,
        //上传文件的类型  默认为所有文件    'All Files'  ;  '*.*'
        //在浏览窗口底部的文件类型下拉菜单中显示的文本
        'fileTypeDesc': '文档',
        //允许上传的文件后缀
        'fileTypeExts': '*.jpg; *.jpeg; *.png; *.gif; *.doc; *.docx; *.xls; *.xlsx; *.txt',
        //发送给后台的其他参数通过formData指定
        //'formData': { 'someKey': 'someValue', 'someOtherKey': 1 },
        //上传文件页面中，你想要用来作为文件队列的元素的id, 默认为false  自动生成,  不带#
        //'queueID': 'upload-process',
        //选择文件后自动上传
        'auto': true,
        //设置为true将允许多文件上传
        'multi': false,
        'removeCompleted': true,
        'uploadLimit': 3,
        'queueSizeLimit':3,
        'fileSizeLimit': '5MB',
        'overrideEvents': ['onUploadStart','onSelect','onSelectError','onCancel', 'onDialogClose', 'onQueueComplete','onUploadSuccess','onSWFReady'],
        'onUploadStart' : function (file){
          var name=file.name;
          var myself = this; 
          //不同弹框可能传相同文件，顾此处不做是否存在判断
        },
        'onSWFReady' : function () {
          var limit = $("#uploadifybottom").uploadify('settings','queueSizeLimit');
        },
    'onSelect': function (file) {
      var id = file.id;
      var name = file.name;
      var fileHeadName=name.substring(0,name.lastIndexOf('.'));
      if(fileHeadName.length>6){
              fileHeadName=fileHeadName.substring(0,6)+"...";
            }
      var suffix = name.substring(name.lastIndexOf('.')+1);
      var fileDiv = '<div id="'+id+'_DIV" class="updone">';
        //fileDiv += '<a href="/api/shop/fileUpload!downloadWord.do?(\''+id+'\');">';
        fileDiv += '<a href="/api/shop/fileUpload!downloadFile.do?name='+name+'">';
        fileDiv += fileHeadName;
        fileDiv += '<a class="del-file" href="javascript:void(0);" onclick="delUploadFile(\''+id+'\');">删除</a>';
        fileDiv += '</div>';
      $('#uploadifybottom').before(fileDiv);
      $(".tcbox .introa").hide();
      docNum++;
    },
    'onUploadSuccess': function(file, data, response){
      var obj=eval("("+data+")");
      var name=obj.message.name;
            var ivalues=name+"#"+obj.message.path;
            var suffix = name.substring(name.lastIndexOf('.')+1);
            $.post('/api/shop/requirement!updateUploadList.do',{firstFile:name,path:obj.message.path,suffix:suffix},function(data){
              if(data.result==1){
                var values="<input type='hidden' name='uploadFiles1' value='"+ivalues+"' id='"+file.id+"_f'/>";
                $("#form").append(values);
              }
            },'JSON');
            $("#subbutton").attr("disabled",false);
      $("#subbutton").attr("class","btn1");
      $("#tip").html("已上传文档：");
    },
        'onSelectError': function(file, errorCode, errorMsg, errorString){
          // $("#uploadifybottom").uploadify('disable', true);
          if(file != null){
        if(errorCode == -110){
          showUploadErrorBox();
        }else if(errorCode == -130){
          showUploadError1Box();
        }
          }
        },
    'onDialogClose': function(){},
    'onCancel': function(queueItemCount){
      if(!flag){
      var limit = $("#uploadifybottom").uploadify('settings','queueSizeLimit');
      }
    },
        'onQueueComplete' : function(queueData) {//当队列中的所有文件全部完成上传时触发
        //alert('上传成功，提交需求！');
    }
    });
  
  var options = {
			url : "/api/shop/member!doLogin.do?ajax=yes",
			type : "POST",
			async:false,
			dataType : 'json',
			success : function(data) {
				if(data.result==1){
					if(data.forward){
						window.open(data.forward);
						$(".close").trigger("click");
						location.reload();
					 	return;
					}
					//alert($("#hideform").html()+":"+$("#hideform").serialize());
					var href = location.href;
					var index = href.indexOf("?");
					//alert(index+":"+href);
					if(index!=-1){
						href =href.substring(0,index);
					}
					//alert(index+":"+href);
					var param = $("#hideform").serialize();
					if(param!=''){
						location.href=href+"?"+param;
					}else{
						location.href=data.url;
					}
				    
				}
				else{
					showError0(data.id);
				}
		  	},
			error : function(e) {
				showError0("5");
			}
		};
  
  $('#loginBtn0').click(
      function(){ 
        var userId0 = $("#userId0").val();
        var userPass0 = $("#userPass0").val();
        if(userId0 == "" && userPass0 == ""){
          showError0("0");
          return false;
        }
          if(userId0 == ""){
          showError0("1");
          return false;
        }else if(userPass0 == ""){
          showError0("2");
          return false;
        }
        $('#loginBtn0').val("正在登录...");
        $('#login_form').ajaxSubmit(options);
    });
  
    $("#userId0").focus(function(){
      $(".log_name,#userId0").css("border","1px solid #fd8240");
    });
    $("#userId0").blur(function(){
      $(".log_name,#userId0").css("border","1px solid #dbdbdb");
    });
    $("#userPass0").focus(function(){
      $(".log_pass,#userPass0").css("border","1px solid #fd8240");
    });
    $("#userPass0").blur(function(){
      $(".log_pass,#userPass0").css("border","1px solid #dbdbdb");
    });
    
    $("#userPass0").focus(function(){
      $(".log_pass,#userPass0").css("border","1px solid #fd8240");
    });
    
    $(document).keydown(function(event){
      
      if(event.keyCode ==13){
    	  $("#loginBtn0").focus();
      }
    }); 
    
    $("#loginBtn0").bind("keydown", function(event){
    	var userId0 = $("#userId0").val();
        var userPass0 = $("#userPass0").val();
        if(userId0 == "" && userPass0 == ""){
          showError0("0");
          return false;
        }
          if(userId0 == ""){
          showError0("1");
          return false;
        }else if(userPass0 == ""){
          showError0("2");
          return false;
        }
        //$('#loginBtn0').trigger("click");
        $('#loginBtn0').val("正在登录...");
        $('#login_form').ajaxSubmit(options);
    });
  
  function showError0(errorType){
		var errorHtml = $(".login_name_c"),
		    errorHtml_c = $(".login_pass_c");
		    
		if(errorType == "0"){
			errorHtml.html("请输入账户名");
			$("#userId0").focus();
		}
		else if(errorType == "1"){
			errorHtml.html("请输入账户名");
			$("#userId0").focus();
		}else if(errorType == "2"){
     
			errorHtml_c.html("请输入登录密码");
      
			$("#userPass0").focus();
		}else if(errorType == "5"){
			$("#userPass0").focus();
			errorHtml.html("出现错误，请重试");
		}else if(errorType == "6"){
			$("#userId0").focus();
			errorHtml.html("账户名密码不匹配");
		}else if(errorType == "7"){
			$("#userId0").focus();
			errorHtml.html("账户名已经被停用");
		}
		$("#userId0").keyup(function(){
			errorHtml.html("");
		});
		$("#userPass0").focus(function(){
			errorHtml_c.html("");
		});
		$('#loginBtn0').val("立即登录");		
	}
  if (memberFlag==false) {
    autologin();
  }
  
  //登录判断
function cookie(name){ 
    var cookieArray=document.cookie.split("; "); //得到分割的cookie名值对    
    for (var i=0;i<cookieArray.length;i++){    
       var arr=cookieArray[i].split("=");       //将名和值分开   
       if(arr[0]==name)return unescape(arr[1]); //如果是指定的cookie，则返回它的值    
    } 
    return "";  
}
  
function autologin(){
    var myuser=cookie("MJUser");
    if(myuser!=null&&myuser!=''){
      $.post('/api/shop/member!autoLogin.do?ajax=yes',{cookieStr:myuser},function(data){
        var obj = eval('(' + data + ')');
        if(obj.result==1){
          var forward=''; 
                     location.href=obj.url;
        }
      });
    }else{
      //查询cookie,自动填充用户登录名
      var url = "/api/shop/member!getCookie.do";
      $.post(url,{},function(data){
          if(data!=""){
            var userNames = data.split(";");
            var length=userNames.length-1;
              $("#userId").val(userNames[length]);
            $("#userId").autocomplete(userNames,{
              minChars: 0,
              width: 340,
              max:5,
              multiple:false,
              autoFill:true,
              highlight: false,
              multiple: true,
              multipleSeparator: " ",
              autoFill:true,
              scroll: true,
              scrollHeight: 300,
            });
          }
        },'TEXT'); 
    }
  }


