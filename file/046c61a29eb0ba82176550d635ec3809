	function goToLjl(){
		var date = "";
		$.ajax({
			url: '/api/acctperiod/ljl!isApply.do',
			type: "post",
			dataType: "json",
			async:false,
			success: function (data) {
				date = data;
				if(date=="toLogin"){
					window.location="/acctperiod/ljl/index.html";
					return;
				}
				if(date=="register"){
					window.location="/acctperiod/ljl/index.html";
					return;
				}
				if(date=="enterpriseQuery"){
					window.location="/api/acctperiod/ljl!myLjl.do";
					return;
				}
				window.location="/acctperiod/ljl/index.html";
			}
		})
	}
	//去资产中心
	function goToPropertyCenter() {
