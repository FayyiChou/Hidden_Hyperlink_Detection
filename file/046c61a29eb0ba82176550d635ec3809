  <script>

	function loadSupplyEnter(){
		$.ajax({
			type : "get",
			url : "/api/shop/member!reloadMember.do",
			dataType : "json",
			success : function(data) {
				var url = "/login.html";
				//alert(data);
				var cloudOrdUrl = "";
				if(data){

					if(data.enterStep == 0 ){
						url = "/accountManager/enter.html"; //供应商入驻
					}else if(data.enterStep == 1){
						url = "/webtpl/pur_sup_information/company_qiyezizhi.html";
					}else if(data.enterStep == 3){
						url = "/supplier/suppliercount.html";
					}else{
						$("#supplyEnter").parent().hide();
					}

					$("#supplyEnter").attr("href",url);
					$("#supplyEnter").text("供应商入驻");

					if(data.isCloudAccountExists == 'true'){
						cloudOrdUrl = "/cloud/orderList.html?sp=0";
					}else{
						cloudOrdUrl = "/cloud/noLoginOrderList.html";
					}

					$("#myCloudOrder").attr("href",cloudOrdUrl);
					$("#myCloudOrder").text("我的云订单");

				}else{
					// window.location.href = url;
				}
			},
			error : function(e) {
			}
		});
	};


	loadSupplyEnter();


	function logOut() {
