  <script>
	//商品详情服务
	  function goods_xiangqing(){
	    $(".yicai_ser span").on("click", function(){
	      var span = $(this);
	      if (!span.hasClass("active")){
	        span.addClass("active");
	      } else {
	        span.removeClass("active");
	      }
	      
	      choosedService = new Array();
	      $(".yicai_ser span").each(function(){
	    	 if ($(this).hasClass('active')){
	    		 choosedService.push($(this).attr("value"));
	    	 } 
	      });
	      
	      caclGoodsDetailServiceFee();
	    })
	  }
	
	function setGoodsDetailService(){
		 $(".yicai_ser span").each(function(){
			 var span = $(this);
			 span.removeClass('active');
			 for (var i=0; choosedService&& i<choosedService.length; i++){
				 if (span.attr('value')==choosedService[i]){
					 span.addClass('active');
					 break;
				 }
			 }
	      });
		 caclGoodsDetailServiceFee();
	}
	  
	  function caclGoodsDetailServiceFee(){
		  var totalRate = 0
		  $(".yicai_ser span").each(function(){
		    	 if ($(this).hasClass('active')){
		    		 totalRate+= parseFloat($(this).attr('rate'));
		    	 } 
		  });
		  var price = totalRate*5328;
		  $("#serviceFeeSpan").text("￥"+parseInt(price+0.5));
	  }
	$(function(){
		goods_xiangqing();
	});
	</script> 
