  <script>
	//首页及底部导航条跳转，发布货源
	var oBody=document.getElementsByTagName("body")[0];
	function goToAddGoods(openNewWindow) {
		if (memberFlag == false) {
			$(".tcbg").show();
			$("#logbox").show();
			return;
		}
		var options = {
			url : "/api/shop/supplier!goToAddGoodsSource.do",
			cache : false,
			async : false,
			type : "POST",
			dataType : "json",
			error : function() {
				alert("错误，请稍后重试！");
			},
			complete : function() {
				destroyLoad(oBody);
			},
			success : function(data) {
				var dresult = data.result;
				if (data.result == null || data.result == undefined) {
					alert("错误，请稍后重试！");
					return;
				}

				if (data.result == 1){
					if (openNewWindow)
						window.open("/supplyManager/addGoodsSource.html");
					else
						window.location.href="/supplyManager/addGoodsSource.html";
					return;
				}
				var subTitle = '';
				if (dresult == 12) {
					var tips = "您修改的帐号信息正在审核，暂不能进行该操作。";
					subTitle = "如有疑问，请联系我们 400-0843-599.";
				}

				if (dresult == 13) {
					var tips = "您修改的账户信息没有审核通过，是否重新修改？";
					var ourl = "/supplier/suppliercount.html?action=edit&opt=4";
				}

				if (dresult == 20) {
					var tips = "账户信息异常，暂不能进行该操作。";
					subTitle = "如有疑问，请联系我们 400-0843-599.";
				}

				if (dresult == -11) {
					var tips = "您还不是供应商，是否马上入驻？";
					var ourl = "/accountManager/enter.html";
				}

				if (dresult == -20) {
					var tips = "您未成功入驻，是否入驻？";
					var ourl = "/accountManager/enter.html";
				}

				if (dresult == -19) {
					var tips = "您还没有入驻，是否前去入驻？";
					var ourl = "/accountManager/enter.html";
				}

				if (dresult == -18) {
					var tips = "您还没有确认入驻信息，是否前去确认并完成？";
					var ourl = "/supplier/supplier_rg_confirm.html";
				}

				if (dresult == -17) {
					var tips = "贵司的入驻信息正在审核中，暂不能进行该操作。";
					subTitle = "如有疑问，请联系我们 400-0843-599.";
				}

				if (dresult == -16) {
					var tips = "贵司的入驻信息未通过审核，是否前去修改？";
					var ourl = "/supplier/suppliercount.html?action=edit&opt=4";
				}
				if (dresult == 13  || dresult == -11
						|| dresult == -20 || dresult == -19 || dresult == -18 || dresult==-16) {
					showYescaiUiConfirm(tips, subTitle, 'alert', function() {
						if (ourl)
							window.open(ourl);
					});
				}

				if (dresult == 12 || dresult==20 || dresult==-20 || dresult==-17 ){
					showYescaiUiConfirm(tips, subTitle);
				}
			}
		};
		createLoad(oBody, "处理中，请稍后！");
		$.ajax(options);
	}

	//首页及底部导航条跳转,发布采购
	function goToFindGoods(){
		if(memberFlag==false){
			$(".tcbg").show();
			$("#logbox").show();
			$("#loginForward").val("/purchaseManager/quickfind.html");
			return;
		}
		$("#loginClick").html("<a id = 'loginA' target='_blank' href='/purchaseManager/quickfind.html' ></a>");
		document.getElementById("loginA").click();
	}

	function toCart(){
		if(memberFlag==false){
			$(".tcbg").show();
			$("#logbox").show();
			return;
		}
		window.location.href = "/purchase/maincart.html";
	}

</script>
