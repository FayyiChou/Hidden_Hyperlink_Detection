<script language="javascript">
function settab(con,hover,num)
{
	
	var setcon = hover.parentNode.parentNode.getElementsByTagName("li");
	for(var i=0;i<setcon.length;i++)
	{
		setcon[i].className="";
	}
	hover.parentNode.className='current';
	for(var j=0;j<setcon.length;j++)
	{
		document.getElementById(con+"_"+j.toString()).style.display="none";
	}
	document.getElementById(con+"_"+num.toString()).style.display="block";
	
	jQuery("#"+con+"_"+num.toString()+" img[src3]").each(function(){
		jQuery(this).attr("src", jQuery(this).attr("src3")).removeAttr("src3");
	});

}
</script>
