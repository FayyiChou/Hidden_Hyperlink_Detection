//信用用户登陆使用js代码
function ffff(){
	var val = document.getElementById("username").value;
	if(val.length==18 || val.length==15){
		document.getElementById("ffff").value="2";
		alert(document.getElementById("ffff").value);
	}else{
		document.getElementById("ffff").value="1";
		alert(document.getElementById("ffff").value);
	}

}


