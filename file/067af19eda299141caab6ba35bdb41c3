<h2>Photo Albums</h2>
<script>
$h = jQuery.noConflict();
$h(document).ready(function(){
	imageWidget("random",3,"boxG",54,40); //imageWidget(隨機顯示,顯示照片數,顯示照片的DIV的ID,寬度,高度);
});

var imageWidget = function(type,imgsNo,dom,width,height){
	$h.getJSON("/modules/modules/gallery/gallery.php",{fn:"getPicsForWidget",para:type,no:imgsNo},function(data){
		var dhtml = "";
		/*
		if(data.length!=imgsNo){
			imageWidget("random",imgsNo);
			return;
		}
		*/
		$h.each(data,function(i,image){
			var tp = image.picname.split(".");
		//	var alink = "/modules/modules/gallery/?pic="+image.id+"."+tp[0];
			var alink = "/modules/modules/gallery/";
			var tlink = "/modules/modules/gallery/thumbnails/"+image.picname;
			var desc="";
			if(image.description!=null)
				desc = image.description;
			dhtml+="<a href='"+alink+"' title='"+desc+"'><img height='"+height+"' width='"+width+"' src='"+tlink+"' /></a>";
		})
		$h("#"+dom).append(dhtml);
	})
}
</script>
