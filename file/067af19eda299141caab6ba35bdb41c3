<script type="text/javascript">
    var $i =jQuery.noConflict();
    (function($) {

        function init() {
            // add link on h1
            function backToHome() {
                $('#pageheader h1 span').on('click',function(){
                        window.location.href = $('#pageheader2 > a').attr('href');
                });
            }
            
            // add active class on menu1
            function menuActiveState() {
                var path = window.location.pathname.split('/')[2];
                // Add two forward slashes to make sure it gets the "/targetPath/"
                $('#menu1 ul li a[href*="' + '/' + path + '/' + '"]').addClass('active');
            }

            function submenuActiveState () {
                var url = window.location.href;
                    // Get the last segment of url
                    path = url.substr(url.lastIndexOf('/') + 1);
                    $('#submenu li a[href*="' + path + '"]').addClass('active');
            }

            // check if submenu exist
            function checkSubmenuExistence() {
                if($('#submenu li').length === 0){
                    $('body').addClass('no-submenu');
                }
            }

            // cycle settings
            function setCycle() {
                if($('#ADB ul li').length > 0) {
                    $('#pageheader2 > a img').hide();
                }
                if($('#ADB ul li').length > 1) {
                    $('#pageheader2 > a img').hide();
                    $('#ADB ul').before('<div class="pager">').cycle({
                        fx: 'fade',
                        pager: '.pager'
                    });
                }
            }

            // Set up default text for Google Search
            function searchDefaultText(selector, text){
                selector
                    .val(text)
                    .css('color', '#ccc')
                    .focus(function() {
                        $(this).css('color', "#000");
                        if($(this).val() == text){
                            $(this).val("");
                        }
                    })
                    .blur(function() {
                        $(this).css('color','#ccc');
                        if($(this).val() == ""){
                            $(this).val(text);
                        }
                    })
            }


            // decide what function are going to be using in homepage and other pages
            if($('body').attr('id') === 'page_main') {
                checkSubmenuExistence();

            } else{
                backToHome();
                submenuActiveState();
            }

        }
        
        $(document).ready(function($) {
            init();     
        });

    }(jQuery));
</script>   

