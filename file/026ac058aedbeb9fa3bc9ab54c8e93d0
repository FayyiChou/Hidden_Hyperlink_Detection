  <script language="javascript" type="text/javascript">
function chgIndexDiv(num,total,linkName,divName,classnow){
	if(document.getElementById(linkName+num).className==classnow+'on'){
		return;
	}else{
		for(i=1;i<total+1;i++){
			if(document.getElementById(linkName+i).className==classnow+'on'){
				document.getElementById(linkName+i).className=classnow+'off';
				document.getElementById(divName+i).style.display='none';
				document.getElementById(linkName+num).className=classnow+'on';
				document.getElementById(divName+num).style.display='';
				break;
			}
		}
	}
}
</script> 
