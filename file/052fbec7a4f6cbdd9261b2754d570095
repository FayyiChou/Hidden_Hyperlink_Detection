<script type="text/javascript">
    var CheckIndex = "0";
    var SwichID = "#index_newsCut";
    $(SwichID).find("#chk" + CheckIndex).addClass("cur_01");
    $(SwichID).find("#shk" + CheckIndex).show();

    function ShowCheck(src) {
        var ob = $(src);
        ob.parent('div.tab_01').parent('div.title_01').find('i a').attr('href', ob.attr('href'));
        $(src).parent().children().removeClass("cur_01");
        $(src).addClass("cur_01");
        var Index = $(src).attr("id");
        Index = Index.substring(3, Index.length);
        var ShowIndex = "#shk" + Index;
        $(src).parent().parent().parent().children(".block_cont_2").hide();
        $(src).parent().parent().parent().find(ShowIndex).show();
    }
</script>

