<SCRIPT>
function change(n,m,id1,id2){
		for(var x=1;x <= m;x++){
			if(x==n){
				var a=document.getElementById(id1+x);
				var b=document.getElementById(id2+x);
				a.className="t_hover";
				b.style.display="block";
				}else{				
					var a=document.getElementById(id1+x);
					var b=document.getElementById(id2+x);
					a.className="";
					b.style.display="none";					
				}
			}			
	}

</SCRIPT>
