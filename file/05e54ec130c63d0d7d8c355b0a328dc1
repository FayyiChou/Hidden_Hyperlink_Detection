    <script type="text/javascript">
      var InitializeDigitalEditionReader = function () {
        var serverError = true;
        var cookieCheckKey = 'cookieCheck';
        var bootstrapUrl = '/Reader/BootstrapPublicationDomain?editionGroupUrlFriendlyName=&versionUrlFriendlyName=&pageSet=&page=&contentID=&cacheBreaker=636153447584660227';
        var ieDownloadInterval;
        var totalSize;
        var percentComplete = 0;
        var maxBootstrapPercentage = 0.92;
        var ajaxRequestParams = {
          url: bootstrapUrl,
          dataType: 'script',
          xhr: function () {
            var thisXHR = new window.XMLHttpRequest();
            loadingIndicator.percentageLoader('setProgress', 0);
            if ($.browser.msie) {
              ieDownloadInterval = setInterval(function () {
                var randomPercentageDiff = Math.floor((Math.random() * 4.99)) * 0.01 + 0.03;
                percentComplete = percentComplete + randomPercentageDiff;
                if (percentComplete > maxBootstrapPercentage) {
                  percentComplete = maxBootstrapPercentage;
                }
                loadingIndicator.percentageLoader('setProgress', percentComplete);
              }, 1000);
            } else {
              thisXHR.addEventListener('progress', function (evt) {
                totalSize = thisXHR.getResponseHeader('Content-Length') * 9;
                percentComplete = evt.loaded / totalSize;
                if (percentComplete > maxBootstrapPercentage) {
                  percentComplete = maxBootstrapPercentage;
                }
                loadingIndicator.percentageLoader('setProgress', percentComplete);
              }, false);
            }
            return thisXHR;
          },
          complete: function () {
            percentComplete = maxBootstrapPercentage;
            loadingIndicator.percentageLoader('setProgress', percentComplete);
            window.imageDownloadInterval = setInterval(function () {
              percentComplete = percentComplete + 0.01;
              if (percentComplete > 0.99) {
                percentComplete = 0.99;
              }
              loadingIndicator.percentageLoader('setProgress', percentComplete);
            }, 1000);
            clearInterval(ieDownloadInterval);
          },
          error: function () {
            if (serverError) {
              alert("The Reader Bootstrap request generated an error or timed out. See new window for details.");
              window.open(bootstrapUrl);
            }
          }
        }
        if ($.cookie(cookieCheckKey)) {
          loadingIndicator = $('#InitialLoadingScreenProgress').percentageLoader('http://mozaicprod02.s3.amazonaws.com/0591/thumbnail/471950');
          $.ajax(ajaxRequestParams);
        } else {
          $('#InitialLoadingScreen').hide();
          window.location.href = "/Error/CookiesRequired";
        }
      }
        $(document).ready(InitializeDigitalEditionReader);
        /*
        window.onbeforeunload = function (e) {
            // Your logic to prepare for 'Stay on this Page' goes here 
            if (window.readerController) {
                window.readerController.beforeUnload.Fire();
            }
            delay(1500);
            return "Please click 'Stay on this Page";
        };


        function delay(ms) {
            var start = +new Date;
            while ((+new Date - start) < ms);
        }*/

    </script>

