		                sendSMSCodeTime = 119;
		                clearInterval(interval);
		              } else {
		                $("#sendSMSCodeButton").html(sendSMSCodeTime + " " + "重新获取").addClass('smsLogin_btn_disable');
		              }
		            }, 1000);
		            $("#sendSMSCodeButton").html(sendSMSCodeTime + " " + "重新获取").addClass('smsLogin_btn_disable');
