		var page = 1;
		//查询阅读量
		   ReadCount();
			function ReadCount(){
			var artId = "887144";
			$.ajax({
						url:"http://test.mp.3g.cnfol.com/article/modifynum",
						type:"get",
						data:{artId:artId},
						dataType:"json",
						success:function(json)
						{
							if(json!="false"){
								$("#viewCount").html(json['readCount']);
							}
							
						}
					});
		   }
		/*上滑加载*/
		$(function () {
			var LoadMore=new window.Event.Scroll({id:"listdatas-Wrap",callBack:function(t){
			var  url="http://test.mp.3g.cnfol.com/article/moreVideoList";
			var artId = "887144";
					$.ajax({
						url:url,
						type:"get",
						data:{page:page,artId:artId},
						dataType:"json",
						success:function(data)
						{
							if(JSON.stringify(data) == '[]'){
			                   t.Finish();
			                   t.SetUnable();
			                }else{
								var htmls = "";
								for(var a=0;a<data.length;a++){
										var html = '<li><a href="'+data[a].Url+'"><img src="'+data[a].Thumb2+'" alt="" /></a><a href="'+data[a].Url+'"><h3>'+data[a].Title+'</h3></a><span class="viwNum">'+data[a].readCount+'</span><span class="time">'+data[a].CreatedTime+'</span></li>';
										htmls += html; 
									}
									page++;
								   
								$('#List1').append(htmls);
								t.Finish();
							}
								
						}
					}); 				
				}})			
			});
			/*End 上滑加载*/
			getArticleMedia();
   //加载文章来源自媒体
   function getArticleMedia(){
	   var uid= "";
	   if(readCookie('userId')!=''){
		  uid=readCookie('userId');
	   }
	    var weMediaId = "15030";
	   var  url='http://test.mp.api.3g.cnfol.com/'+'api/articlemedia?weMediaId='+weMediaId+'&uid='+uid;
	   $.ajax({
                url:url,
                type:"get",
                dataType:"jsonp",
                jsonp: 'jsoncallback',
                success:function(json)
                {
	
						if(json=="") return false;
						var html = '';
						var isAttention = "关注";
						if(json.isCancel==null || json.isCancel==1){
							isAttention = "关注";
						}else if(json.isCancel==0){
							isAttention = "已关注";
						}

						html+="<a href='http://mp.3g.cnfol.com/article/pubnum/weMediaId/"+json.weMediaId+"'><img class=\"Ad-Logo\" src='"+json.logoUrl+"' /></a>";
						html+="<span class=\"Ad-Txt\"><a href='http://mp.3g.cnfol.com/article/pubnum/weMediaId/"+json.weMediaId+"'>"+json.mediaNickName+"</a></span>";
						html+="<a class=\"gzBtn\" onclick=\"fnNotice(this)\" >"+isAttention+"</a>";
						$("#mediainfo").append(html);
						
				}
            });
   }
   //关注效果
			function fnNotice(t){
				var uid= "";
			   if(readCookie('userId')!=''){
				  uid=readCookie('userId');
			   }else{
				   alert("请先登录！");
				  return false;
			   }
				var state=$(t).html();
				if(state=="关注"){
					t.innerText="已关注";
				}else{
					t.innerText="关注";
				}
				var weMediaId = "15030";
			   $.ajax({
						url:"http://test.mp.3g.cnfol.com/article/setAttention",
						type:"post",
						dataType:"json",
						data: {weMediaId:weMediaId},
						success:function(json)
						{
								
						}
					});
			}
			//加载广告内容
   getMediaAdvertise();
   function getMediaAdvertise(){
	   var weMediaId = "15030";
	   var  url='http://test.mp.api.3g.cnfol.com/'+'api/getMediaAdvertise?weMediaId='+weMediaId;	  
	   $.ajax({
		   url:url,
			type:"get",
			dataType:"jsonp",
			jsonp: 'jsoncallback',
			success:function(json){
				if(json=="") return false;
				$(".advertiseContent").html(json.advertiseContent);
				$(".adIdty").show();
			}
	   })
   }
