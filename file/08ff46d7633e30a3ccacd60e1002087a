	<div class="index_notice">
		<script type="text/javascript">
		function settab(name,num,n){
			for(i=1;i<=n;i++){
				var menu=document.getElementById(name+"_"+i);
				menu.className=i==num?"on":"out";
				var con=document.getElementById(name+"_"+"con"+"_"+i);
				con.style.display=i==num?"block":"none";
				if (con.style.display == "block") {
					resetHeight(num);
				}
			}
		}
		function resetHeight(num) {
			var d2 = document.getElementById("demo1_up_" + num);
			var d3 = document.getElementById("demo2_up_" + num);
			if (d2.offsetHeight < 220) {
				d2.style.height = "220px";
				d3.style.height = "220px";
			}
		}
		</script>
