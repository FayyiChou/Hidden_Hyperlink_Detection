<script language="javascript" type="text/javascript">
    $('#key').bind('keyup', function (event) {
        if (event.keyCode == "13") {
            //回车执行查询
            var key = encodeURIComponent($("#key").val());
            key = decodeURI(key);
            if (key == '请输入关键词...') {
                alert('请输入关键词');
                $("#key").focus(function () {
                    $("#key").attr("value", "");
                });
            } else {
                var Request = new UrlSearch(); //实例化
                if (typeof (Request.ID) == "undefined") {
                    window.location.href = "/web/SearchArticleList.aspx?key=" + key;
                } else {
                    window.location.href = "/web/SearchArticleList.aspx?ID=" + Request.ID + "&key=" + key;
                }
            }
        }
    });
</script>

