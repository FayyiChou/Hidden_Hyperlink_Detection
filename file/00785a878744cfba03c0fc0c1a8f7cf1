    //导航样式设置
    $(function () {
        var flag = true;
        $(".mainNav li a").each(function () {
            var va = $(this);
            if (window.location.pathname.indexOf(va.attr("href")) > -1) {
                va.attr("id", "selected");
                flag = false;
            }
            else {
                va.attr("id", "");
            }
        });
        if (flag) {
            $("#index").children().attr("id", "selected");
        }
    });
</script>
