            <script>
var time1=20//数值越大速度越慢
demo5.innerHTML=demo4.innerHTML
function movepic1(){
if(demo5.offsetWidth-demo3.scrollLeft<=0)
demo3.scrollLeft-=demo4.offsetWidth
else{
demo3.scrollLeft++
}
}
var timer1=setInterval(movepic1,time1)
demo3.onmouseover=function() {clearInterval(timer1)}
demo3.onmouseout=function() {timer1=setInterval(movepic1,time1)}
            </script>

                    
                    
                    
               
