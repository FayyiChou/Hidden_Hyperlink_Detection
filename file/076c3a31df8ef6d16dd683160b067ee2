              <script language="javascript" type="text/javascript">
    function yanzheng(){
    if(document.getElementById("txtmname").value==""){alert("联系人不能为空！");return false;}
    
    if(document.getElementById("txtmtel").value==""){alert("联系电话不能为空！");return false;}
    
     var patn=/^(1\d{8,20})|(\d{2,4}-\d{5,10})$/;
     
	if(!patn.test(document.getElementById("txtmtel").value))
    {
    
    alert("您的电话号码填写有误！请您填写***-*******或者您的手机号码");
    return false;
    
    }    
    
    if(document.getElementById("txtmemail").value!=""){
    
  	var patn = /^\w[-._\w]*\w@\w[-._\w]*\w\.\w{2,6}$/;
  	
  	if(!patn.test(document.getElementById("txtmemail").value)){
  	alert("您的邮箱填写有误！请您正确的填写！");
  	return false;
  	}
    
    }
}
    
    </script>
