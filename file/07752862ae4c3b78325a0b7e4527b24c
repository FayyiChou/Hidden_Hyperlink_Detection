		 }else
document.write("<li><a href=\"t20170331_9396635"+"_"+i+"."+"htm\" >"+(i+1)+"</a></li>&nbsp;");}


	if(countPage>1&&currentPage!=(countPage-1))
document.write("</ul><a href=\"t20170331_9396635"+"_"+nextPage+"."+"htm\" >下一页</a>&nbsp;");
	else
document.write("</ul><a class='tow'>下一页</a>&nbsp;");
        if (endPage>0)
	document.write("&nbsp;|&nbsp;<a  href='"+docurl+"_"+endPage+".html'>尾页</a>");
        else
	document.write("&nbsp;|&nbsp;<a  href='"+docurl+".html'>尾页</a>");
