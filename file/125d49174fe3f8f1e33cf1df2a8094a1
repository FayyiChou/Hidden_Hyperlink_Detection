<script type="text/javascript">
    //客服QQ显隐
    $(".customServiceIcon").click(function(){
        if($(this).hasClass("active")){
            $(this).removeClass("active").next().addClass("hide");
        }else{
            $(this).addClass("active").next().removeClass("hide");
        }
    });
</script>
	

