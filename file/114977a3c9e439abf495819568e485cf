<script type="text/javascript" language="javascript">
    var time = 1000;
    var h = 0;
    function addCount()
    {
        if(time>0)
        {
            time--;
            h = h+5;
        }
        else
        {
            return;
        }
        if(h>600)  //高度
        {
            return;
        }
        document.getElementById("ads").style.display = "";
        document.getElementById("ads").style.height = h+"px";
        var id1 = setTimeout("addCount()",30); 
    }
    
    window.onload = function showAds()
    {
        addCount();
        var id2 = setTimeout("noneAds()",15000000); //停留时间自己适当调整
    }

    var T = 400;
    var N = 600; //高度
    function noneAds()
    {
        if(T>0)
        {
            T--;
            N = N-5;
        }
        else
        {
            return;
        }
        if(N<0)
        {
            document.getElementById("ads").style.display = "none";
            return;
        }
        
        document.getElementById("ads").style.height = N+"px";
        var id3 = setTimeout("noneAds()",30); 
    }
    </script> 
