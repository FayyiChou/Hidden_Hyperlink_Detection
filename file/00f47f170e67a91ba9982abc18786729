<script type="text/javascript">
	var ct_counter = 0;
	var ct_timeout_id = 0;
	function ct_repeat_init() {
		if (typeof ct_core === 'function') {
			ct_core();
		} else if (ct_counter < 10) {
			window.clearTimeout(ct_timeout_id);
			ct_timeout_id = window.setTimeout(function() {
				ct_repeat_init();
			}, 300);
			ct_counter++;
		}
	}
	window.clearTimeout(ct_timeout_id);
	ct_timeout_id = window.setTimeout(function() {
		ct_repeat_init();
	}, 300);
</script>


