//查找用户是否收藏

var col_btn=true;

function col_check_ajax(uid){
	$.ajax({
			url:'http://my.xcar.com.cn/interface/fav_article/fav_article_cms_jsonp.php',
			dataType:"jsonp",
			jsonp:"jsonpcallback",
			data:{
				status:1,
				uid:uid,
				article_id:514377			},
			success:function(data){
				if(data.num){

                    $("#collection_bottom").html("已收藏");
                    $("#collection_bottom").addClass("collect_allready_txt");
                    $("#collection_bottom").unbind('click').css('cursor','default');//是否取消绑定
					bool='had';
				}else{

					$("#collection_bottom").html("收藏本文");
					bool='no';
				};

				if(!col_btn){
					col_ajax(bool);
				};

			},
			error:function(data){
				return true;
			}
		});
}
//改变鼠标移入小方块的提示语言
if(uid==undefined || uid=='' || uid==null){
    $("#collection_bottom").html('收藏本文');
}else{
	col_check_ajax(uid);
}

//进行用户收藏
function col_ajax(bol){

	if(bol=='had'){
		$("#collection_bottom").unbind('click').css('cursor','default');//是否取消绑定
		$("#attentionHover").css('display','none');
		$('#news_pop').css('display','inline');
		$('#weixin_bg').css('display','inline');
		$('.repeat').css('display','inline');
		$('.once').css('display','none');
       		hot_art = new btnRoll({
			root : 'hot_art_root',
			easing : "easeOutCirc",
			preCallBefore : function(c){
				change_art(c);
			},
			nextCallBefore : function(c){
				change_art(c);
			},
			goToCallBefore : function(c){
				change_art(c);
			}
		});
	}else if(bol=='no'){
        $("#collection_bottom").html("已收藏").addClass("collect_allready_txt");
        $("#collection_bottom").css('cursor','default');

		bool='had';
        $("#attentionHover").css('display','none');
		$('#news_pop').css('display','inline');
		$('#weixin_bg').css('display','inline');
		$('.repeat').css('display','none');
		$('.once').css('display','inline');
		hot_art = new btnRoll({
			root : 'hot_art_root',
			easing : "easeOutCirc",
			preCallBefore : function(c){
				change_art(c);
			},
			nextCallBefore : function(c){
				change_art(c);
			},
			goToCallBefore : function(c){
				change_art(c);
			}
		});

		var links=$("#newsbody p a");
		var img=null;
		for(var i=0;i<links.length;i++){
			if(links.eq(i).attr("href").indexOf("&aid=")>0){
				img=links.eq(i).find("img").attr("src");
				break;
			}
		}

	 	if(img){
			var res=img.split("/");
			res.push('s_'+res.pop().split("_").pop());
			var pic=res.join("/");
		}else{
			pic='http://icon.xcar.com.cn/news/images/xcarimg.jpg';
		}
	 	var jsonp_data={
			uid:uid,
			username:encodeURI(' '),
			channel_id:1,
			stype:31,
			pstype:3,
			coulumn_id:'120',
			article_id:514377,
			channel_name: encodeURI('drive'),
			coulumn_name: encodeURI('爱卡试驾'),							//栏目名
			title:encodeURI('被低估的价值 测长城哈弗H5两驱柴油版'),
			pic_url:pic,
			arcurl:'http://drive.xcar.com.cn/201205/news_514377_1.html',
			description:encodeURI('“两驱越野！疯了吧？！”能够说这种话的人一定是懂什么是越野，可是在现实社会中却有不少人没有越野的装备却又怀揣着“越野的勇气”，于是乎，歪打正着的出现了“爬坡门”这样的囧事，再于是，人们又对两驱SUV的实力大为怀疑，认为这样的车与两驱轿车无异，通过性非常有限。那么今天我们就来试试国内自主品牌中比较懂offroad的长城，看看它旗下的H5两驱究竟实力几何？'),
			author:encodeURI('莫让'),
			pub_time:'1338199140',
			collection_time:1491042125 ,
			source:encodeURI('文章资讯')
		}

	 	var jsonpurlstr = 'http://my.xcar.com.cn/interface/fav_article/fav_article_cms_jsonp.php?';
	 	for(var jsonp_data_i in jsonp_data){
	 		jsonpurlstr += "&" + jsonp_data_i + "=" + jsonp_data[jsonp_data_i];
	 	}
	 	jsonp({
		 	url:jsonpurlstr,
	 		callback:function(data){
	 			bool='had';
		 	},err:function(data){

		 	}});
	}

}

