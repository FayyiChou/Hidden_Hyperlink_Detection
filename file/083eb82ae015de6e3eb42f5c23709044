//2013.06.14
function doCallLoginBox(){
	$("#login_btn_click").unbind('click');
	$("#login_btn_click").xLoginBox({
	   func:function(){
		   callLoginScript();
		   updateCommentIfrsrc2.call(null);
	   },
	   args:[],
	   step:2,
	   show:true,
	   url:"http://drive.xcar.com.cn/201205/news_514377_1.html#commentpos"
	});
}



//====================换一换
var ch_one_ch_num=0;
$('.watching .ch_one_ch').click(function(){
	ch_one_ch_num++;
	if(ch_one_ch_num>=$('.watching ul').length){
		ch_one_ch_num=0;
	}
	$('.watching ul').hide();
	$('.watching ul').eq(ch_one_ch_num).show();
})
