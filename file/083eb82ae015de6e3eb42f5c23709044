//loationinfo
var timeclear = false;
var select_city = {
		//init初始化
		cheshi_city_id:0,
		cheshi_city_name:'',
		cheshi_province_id:0,
		cheshi_province_name:'',
		cheshi_bbs_id:'',
		cheshi_bbs_name:'',
		initselect:false,

		//获取地区
		getUserLocation : function(cityid){
			if(__ipLocationInfo.city_id){
				return __ipLocationInfo.city_id;
			}else{
				return 475;
			}
		},
		//获取经销商推荐信息
		getDealerRecInfo : function(city_id){
			if(timeclear==false)return;
			city_id = city_id ? city_id : 475;
			$.ajax({
				url:'http://dealer.xcar.com.cn/dealerdp_index.php?r=inner/priceCenter/recommand/parnter/10001/',
				dataType:"jsonp",
				jsonp:"callback",
				data:{
					city_id:city_id,
					pserid:783				},

				success:function(data){
					if(data['dealer'].length>0){
						dealhtml='';
						for(var i=0;i<data['dealer'].length;i++){
							if(data['dealer'][i]['premission']>'2'){
								var delem = data['dealer'][i]['coname']+'<em></em>';
							}else{
								var delem = data['dealer'][i]['coname'];
							}
							if(data['dealer'][i]['is400']=='1'){
								var phone = '<dd>免费电话：<span>'+data['dealer'][i]['tel']+'</span></dd>';
							}else{
								var phone = '<dd>电话：<span style="color:#999999;">'+data['dealer'][i]['tel']+'</span></dd>';
							}
							dealhtml+='<li class="clearfix"><dl><dt><a target="_blank" href="http://dealer.xcar.com.cn/'+data['dealer'][i]['did']+'/">'+delem+'</a></dt>'+phone+'</dl><b>'+data['dealer'][i]['price']+'万</b><i>↓ '+data['dealer'][i]['preferential']+'万</i><a target="_blank" class="more_btn" href="http://newcar.xcar.com.cn/auto/index.php?r=dealerPopw/order&pserid=783&did='+data['dealer'][i]['did']+'&city_id='+city_id+'&type=1&is_cms=662&cid=1"  onclick="clicklog(\'124156\',\'\',\'\')">问更低价</a></li>';
						}
						$('#price_title').html('<p><a href="http://newcar.xcar.com.cn/m'+data['dealer']['0']['mid']+'/" target="_blank" >'+data['base']['showname']+'</a></p><span>指导价：'+data['base']['manu_price']+'万</span>');
						$('#price_title').addClass('price_titls2');
				    	$('#price_ul').html(dealhtml);
					}else{
						 $('.price_notice').css('display','none');
					}

				}
			})
		},
		getCityInfo : function(cityid){
			var tempid;
			if(cityid){
				tempid = cityid;
			}else{
				tempid = select_city.getUserLocation();
			}
			if(typeof tempid=='undefined' || tempid==0){
				select_city.cheshi_city_id=475;
			}else{
				select_city.cheshi_city_id=tempid;
			}
			var cityname = $('#cjg_city_list_a').find('a[name="city'+select_city.cheshi_city_id+'"]')[0].text;
			getId('tab_nav').innerHTML = cityname;
			//异步切换
			select_city.getDealerRecInfo(select_city.cheshi_city_id);
		},


		//设置点击
		setCityNameclick:function(){
			var alist;
			var alist = getId('cjg_city_list_a').getElementsByTagName("A");
			for(var i=0,len=alist.length;i<len;i++){
				addHandler(alist.item(i),'click',function(event){
					var event = event||window.event;
					var target = event.target||event.srcElement;
					str = target.name;
					id = str.replace("city","");
					var cityname = $('#cjg_city_list_a').find('a[name="city'+id+'"]')[0].text;
					select_city.getCityInfo(id);
					getId('tab_nav').innerHTML = cityname;
					getId('cjg_city_change').style.display='none';
					return false;
				});
			}
		}
};

function resWriteCheshiname(){
  if(__ipLocationInfo.city_id != 475){
		var _str = __ipLocationInfo.city_name+"车市";
		document.getElementById("xHeaderIplocation").innerHTML ='<a href="'+__ipLocationInfo.url+'" target="_blank">'+_str+'</a>';
     	document.getElementById("xHeaderIplocation").parentNode.style.width=(_str.length * 18) + 60 + "px";
  }
}
var _ipLocationInfo = getCookie("_locationInfo_");
if(_ipLocationInfo){
	__ipLocationInfo=_ipLocationInfo = eval("("+_ipLocationInfo+")");
	__ipLocationInfo.city_name = decodeURIComponent(_ipLocationInfo.city_name);
	__ipLocationInfo.url=_ipLocationInfo.url;
	setTimeout(function(){
		select_city.setCityNameclick();
		select_city.getCityInfo();
		timeclear = true;
	},1000)

	delete _tmp,_ipLocationInfo;
}else{
	  loadScript("http://tools.xcar.com.cn/ip2city/ip2getcity.php?_t=json&s="+Date.parse(new Date()),function(){
		_ipLocationInfo = __ipLocationInfo;
		__ipLocationInfo.url=_ipLocationInfo.url[0];
		var _tmp='{url:"'+__ipLocationInfo.url+'",city_id:"'+__ipLocationInfo.city_id+'",province_id:"'+__ipLocationInfo.province_id+'", city_name:"'+encodeURIComponent(__ipLocationInfo.city_name)+'"}';
		setCookie("_locationInfo_",_tmp,1000*60*6,"/",".xcar.com.cn");
		resWriteCheshiname();
			setTimeout(function(){
				select_city.getCityInfo();
				timeclear = true;
			},100)
	  delete _tmp,_ipLocationInfo;
	 });
}
</script>
