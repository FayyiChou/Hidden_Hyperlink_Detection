<script type="text/javascript">
   jQuery.getScript("http://info.xcar.com.cn/ajax/get_cityprice.php?pserid=783&allpserid=783,783,783",function(){});

function fill_city_price(city_price)
{
    var city_price=eval(city_price);
    if (city_price.minPrice[783]!=null)
    {
        $("#nd_city_price").html(city_price.cityname+'销售价:<a href="http://price.xcar.com.cn/serise783/city'+city_price.cityid+'-1-1.htm" target="_blank"><em>'+city_price.minPrice[783]+'</em>&nbsp;万起</a>');
    }
    else if (city_price.guidePrice[783]!=null)
    {
        $("#nd_city_price").html('指导价:<a href="http://price.xcar.com.cn/serise783/city'+city_price.cityid+'-1-1.htm" target="_blank"><em>'+city_price.guidePrice[783]+'</em>&nbsp;万起</a>');
    }
    else
    {
        $("#nd_city_price").html(city_price.cityname+'销售价:暂无');
    }
    $("#bo_city_price").attr("href","http://price.xcar.com.cn/serise783/city"+city_price.cityid+"-1-1.htm");
	$("#price_t").attr("href","http://price.xcar.com.cn/serise783/city"+city_price.cityid+"-1-1.htm");

	//顶部报价替换
	var pserids = ["783","783","783"];	for(var j=0;j<pserids.length;j++){
		if (city_price.minPrice[pserids[j]]!=null)
	    {
	        $("#top_p_"+pserids[j]).html(city_price.cityname+'售价：<a href="http://price.xcar.com.cn/serise'+pserids[j]+'/city'+city_price.cityid+'-1-1.htm" target="_blank">'+city_price.minPrice[pserids[j]]+'<em>万</em></a>起');
	    }
	    else if (city_price.guidePrice[pserids[j]]!=null)
	    {
	    	$("#top_p_"+pserids[j]).html('指导价：<a href="http://price.xcar.com.cn/serise'+pserids[j]+'/city'+city_price.cityid+'-1-1.htm" target="_blank">'+city_price.guidePrice[pserids[j]]+'<em>万</em></a>起');
	    }
	    else
	    {
	    	$("#top_p_"+pserids[j]).html(city_price.cityname+'售价：暂无');
	    }
	}
}

function city_dealer(dealInfoCar){
	var dealInfoCar=eval(dealInfoCar);
	var dealInfo =dealInfoCar.dealInfoCar;
	var dealPriceinfo =dealInfoCar.dealPriceinfo;
	var newhtml = '<div class="car_activity_title h43"><a rel="nofollow" onclick="clicklog(\'125455\',\'\',\'\');" title="更多" target="_blank" class="more" href="http://dealer.xcar.com.cn/d1_'+dealInfoCar.cityid+'/30_783.htm">更多&gt;&gt;</a><span><a rel="nofollow" target="_blank" onclick="clicklog(\'125455\',\'\',\'\');"href="http://dealer.xcar.com.cn/d1_'+dealInfoCar.cityid+'/30_783.htm"><em>'+dealInfoCar.cityname+'</em>推荐4S店经销商</a></span></div>';
	if(dealInfo){
		for(var i=0;i<dealInfo.length;i++){

			if(dealInfo[i]['slt_title_s']){
				var cuxiao ='<a target="_blank" onclick="clicklog(\'125455\',\'\',\'\');" href="http://dealer.xcar.com.cn/'+dealInfo[i]['did']+'/market_'+dealInfo[i]['slt_newsid']+'.htm" title="'+dealInfo[i]['slt_title']+'" alt="'+dealInfo[i]['slt_title']+'">'+dealInfo[i]['slt_title_s']+'</a>';
			}else{
				var cuxiao ='<a onclick="clicklog(\'125455\',\'\',\'\');" href="http://dealer.xcar.com.cn/'+dealInfo[i]['did']+'/" target="_blank">进店逛逛</a>';
			}
			if(dealInfo[i]['is400']=='1'){
				var telinfo = '<dd>免费电话：<span class="car_activity_num">'+dealInfo[i]['tel']+'<em></em></span></dd>';
			}else{
				var telinfo = '<dd>电话：<span class="">'+dealInfo[i]['tel']+'<em></em></span></dd>';
			}

		newhtml+='<dl class="car_activity_list"><dt><a href="http://dealer.xcar.com.cn/'+dealInfo[i]['did']+'/" onclick="clicklog(\'125455\',\'\',\'\');" target="_blank">'+dealInfo[i]['coaname']+'</a></dt>'+telinfo+'<dd>促销信息：'+cuxiao+'</dd><dd><a class="down_price" onclick="clicklog(\'125455\',\'\',\'\');" href="http://newcar.xcar.com.cn/auto/index.php?r=dealerPopw/order&pserid=783&did='+dealInfo[i]['did']+'&city_id='+dealInfoCar.cityid+'&type=1&is_cms=755&cid=1" class="down_price" target="_blank">询底价</a><a class="test_drive" onclick="clicklog(\'125455\',\'\',\'\');" href="http://newcar.xcar.com.cn/auto/index.php?r=dealerPopw/order&pserid=783&did='+dealInfo[i]['did']+'&city_id='+dealInfoCar.cityid+'&type=2&is_cms=756&cid="  target="_blank">试驾</a></dd></dl>';
		}
	}else{
		newhtml='';
	}
	$('#dealer_info').html(newhtml);
	//经销商区块

	if(dealPriceinfo['dealer']!=null){
		dealhtml='';

		for(var i=0;i<dealPriceinfo['dealer'].length;i++){
			if(dealPriceinfo['dealer'][i]['premission']>'2'){
				var delem = dealPriceinfo['dealer'][i]['coname']+'<em></em>';
			}else{
				var delem = dealPriceinfo['dealer'][i]['coname'];
			}
			if(dealPriceinfo['dealer'][i]['is400']=='1'){
				var phone = '<dd>免费电话：<span>'+dealPriceinfo['dealer'][i]['tel']+'</span></dd>';
			}else{
				var phone = '<dd>电话：<span style="color:#999999;">'+dealPriceinfo['dealer'][i]['tel']+'</span></dd>';
			}
			dealhtml+='<li class="clearfix"><dl><dt><a target="_blank" onclick="clicklog(\'125454\',\'\',\'\');" href="http://dealer.xcar.com.cn/'+dealPriceinfo['dealer'][i]['did']+'/">'+delem+'</a></dt>'+phone+'</dl><b>'+dealPriceinfo['dealer'][i]['price']+'万</b><i>↓ '+dealPriceinfo['dealer'][i]['preferential']+'万</i><a target="_blank"  class="more_btn" href="http://newcar.xcar.com.cn/auto/index.php?r=dealerPopw/order&pserid=783&did='+dealPriceinfo['dealer'][i]['did']+'&city_id='+dealInfoCar.cityid+'&type=1&is_cms=662&cid=1" onclick="clicklog(\'124156\',\'\',\'\')">问更低价</a></li>';
		}
		$('#price_title').html('<p><a href="http://newcar.xcar.com.cn/m'+dealPriceinfo['dealer']['0']['mid']+'/"  onclick="clicklog(\'125454\',\'\',\'\');" target="_blank">'+dealPriceinfo['base']['showname']+'</a></p><span>指导价：'+dealPriceinfo['base']['manu_price']+'万</span>');
		$('#price_title').addClass('price_titls2');
    	$('#price_ul').html(dealhtml);
	}else{
		$('.price_notice').css('display','none');

	}
}

//==================赞和城市报价,经销商
function praise_callback(data)
{
	data=eval(data);

	if (data.code==1)
	{
		clicklog('50','点赞');
        $("#praise_bottom").addClass('point_praise_hover');

        $("#praise_bottom").css('cursor','default');
        $("#praise_bottom").unbind("click");

        $("#addone_float,#addone_bottom").fadeIn('fast').delay(1000).fadeOut('slow');
        $("i[id^='praise_num_']").html(data.nums);

		if(data.num>999)
		{
			setTimeout(function(){
				$("#praise_bottom").html('已赞 <i>'+data.nums+'+</i>');
			},510)
		}else{
			setTimeout(function(){
				$("#praise_bottom").html('已赞 <i>'+data.nums+'</i>');
			},510)
		}			
	}else{
		if(getCookie('praise_514377')){
    		if(data.nums>999)
    		{			
    			$("#praise_bottom").html('已赞 <i>'+data.nums+'+</i>');			           
    		}else{		
    			$("#praise_bottom").html('已赞 <i>'+data.nums+'</i>');						
    		}
    	}else{
    		if(data.nums>999)
    		{			
    			$("#praise_bottom").html('点赞 <i>'+data.nums+'+</i><span style="display:none;" id="addone_bottom" >+1</span>');			           
    		}else{		
    			$("#praise_bottom").html('点赞 <i>'+data.nums+'</i><span style="display:none;" id="addone_bottom" >+1</span>');						
    		}
		}
	}
}

