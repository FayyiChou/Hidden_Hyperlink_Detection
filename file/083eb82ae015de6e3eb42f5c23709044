<script>

//顶部车系随动
var fix_Box_Timer=null;
var isMac = (navigator.userAgent.indexOf("Mac") != -1); 
var isChrome = (navigator.userAgent.indexOf("Chrome") != -1); 
function keepshow(){
	var pser_Box = document.getElementById("car_pser");
	var fix_Box = document.getElementById("nav_line");
	var c_Height = getAbsTop(pser_Box)+ getOffsetHeight("car_pser");
	document.onmousewheel = fn1;
	if (document.addEventListener){
		document.addEventListener('DOMMouseScroll', fn1, false);
	};
	    
	function fn1(ev){
		var ev = ev || event;
		var iBtn = true;
		if (ev.wheelDelta) {
			iBtn = ev.wheelDelta > 0 ? true : false;
		} else {
			iBtn = ev.detail < 0 ? true : false;
		};
		
		if (iBtn) {
			clearTimeout(fix_Box_Timer);
			if($(window).scrollTop() > c_Height){
				$('#nav_line').show();
				fix_Box_Timer=setTimeout(function(){
					$('#nav_line').fadeOut();
				},2000)
			}else{
				fix_Box.style.display="none";
			}
		}else{
			if(isMac && isChrome){
				fix_Box.style.display="block";
			}else{
				fix_Box.style.display="none";
			}
		};
	}
	fix_Box.onmouseover=function(){
		clearTimeout(fix_Box_Timer);

	};
	fix_Box.onmouseout=function(){
		fix_Box_Timer=setTimeout(function(){
			$('#nav_line').fadeOut();

		},2000)

	};
	$(window).scroll(function(){
		if($(window).scrollTop()<c_Height){
			fix_Box.style.display="none";
		}	
	});
	
};
keepshow();
function xFocus(Box){
	var Con=$(Box+' .enter_width');//大图
	var Prev=$(Box+' .but_left');//上一页
	var Next=$(Box+' .but_right');//下一页
	var Counts=$(Con).size();//获取li总数
	var nowIndex=0;	var timer;
	$(Prev).add(Next).find('em').html(1).end().find('i').html(Counts);
	/* 打开相应的标签 */
	function gotoPage(i){
		$(Con).fadeOut(200).hide().eq(i).fadeIn(200).show();
		nowIndex=i;
		$(Prev).add(Next).find('em').html(i+1).end().find('i').html(Counts);
	};
	/* 下一页 */
	$(Next).click(function(){
		gotoR();
		clickP('237','4','');
	});
	function gotoR(){
		nowIndex++;
		if(nowIndex > Counts-1){
			nowIndex=0;
		}
		gotoPage(nowIndex);
	};
	/* 上一页 */
	$(Prev).click(function(){
		nowIndex--;
		if(nowIndex < 0){
			nowIndex=Counts-1
		}
		gotoPage(nowIndex);
		clickP('237','3','');
	});
	/* 鼠标经过按钮变深色 */
	$(Prev).hover(function(){
		$(this).addClass('but_left_change');
	},function(){
		$(this).removeClass('but_left_change');
	});
	$(Next).hover(function(){
		$(this).addClass('but_right_change');
	},function(){
		$(this).removeClass('but_right_change');
	});

};
if($(".enter_width").length>1){
	xFocus('#focus001');
}else{
	//$('.but_left').addClass('cursor');
	//$('.but_right').addClass('cursor');
}

