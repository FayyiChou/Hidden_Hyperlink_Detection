$(function(){
	function newCar_tab(id){
		$('#'+id+' li').mouseover(function(){
			$('#'+id+' li').removeClass('hot_top_active').find('div .topdiv').hide();
			$('#'+id+' li').find('div .botmdiv').css("display","block");
			$(this).addClass('hot_top_active').find('div .botmdiv').hide();
			$(this).find('div .topdiv').css("display","block");
		})
	};
	newCar_tab('hot_top');

});




