$(function(){
	// 新增分享按钮
	$('#share_weixin').hover(function(){
		var $t = $('#w_new');
      	if ($(".weixinImg").attr("src")=="http://icon.xcar.com.cn/2011newcar/images/auto/ps_logo.jpg")
      	{
          	$(".weixinImg").attr("src","http://hezuo.xcar.com.cn/index.php?partner=qrcode&url=http%3A%2F%2Fdrive.xcar.com.cn%2F201205%2Fnews_514377_1.html");
      	}
		$t.show();
	},function(){
		$('#w_new').hide();
	});
	$('#w_new').hover(function(){
		$(this).show();
	},function(){
		$(this).hide();
	});

	//新增收藏里的微信
