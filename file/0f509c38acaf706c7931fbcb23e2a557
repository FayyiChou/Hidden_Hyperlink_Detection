	//收藏网站
   function addFavorite() {
		var title = "新京报彩票";
		var url = "http://caipiao.bjnews.com.cn";
		try {
			window.external.addFavorite(url, title);
		} catch (e) {
			try {
				window.sidebar.addPanel(title, url, "");
			} catch (e) {
				alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请使用Ctrl+D进行添加");
			}
		}
   }

   (function (){
	   //频道导航切换
	   var loc = window.location.toString();
	   var urlUtilArr = new Array();
	   //urlUtilArr['cpcd0'] = {splitUrl:'http://caipiao.bjnews.com.cn'};
	   urlUtilArr['cpcd1'] = {splitUrl:'http://caipiao.bjnews.com.cn/hemai/'};
	   urlUtilArr['cpcd2'] = {splitUrl:'http://caipiao.bjnews.com.cn/kaijiang/'};
	   urlUtilArr['cpcd3'] = {splitUrl:'http://caipiao.bjnews.com.cn/ggtj/'};
	   urlUtilArr['cpcd4'] = {splitUrl:'http://live.caipiao.bjnews.com.cn/'};
	   urlUtilArr['cpcd5'] = {splitUrl:'http://zst.caipiao.bjnews.com.cn/'};
	   urlUtilArr['cpcd6'] = {splitUrl:'http://caipiao.bjnews.com.cn/pages/rank/'};
	   for(var tempkey in urlUtilArr){
