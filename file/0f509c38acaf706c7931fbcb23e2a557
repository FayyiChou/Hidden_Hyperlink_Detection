   var keyStr = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
   function encode64(input){
	    input = strUnicode2Ansi(input);
	    var output = "";
	    var chr1, chr2, chr3 = "";
	    var enc1, enc2, enc3, enc4 = "";
	    var i = 0;
	    do{
	        chr1 = input.charCodeAt(i++);
	        chr2 = input.charCodeAt(i++);
	        chr3 = input.charCodeAt(i++);

	        enc1 = chr1 >> 2;
	        enc2 = ((chr1 & 3) << 4) | (chr2 >> 4);
	        enc3 = ((chr2 & 15) << 2) | (chr3 >> 6);
	        enc4 = chr3 & 63;

	        if (isNaN(chr2)){
	            enc3 = enc4 = 64;
	        } else if(isNaN(chr3)) {
	            enc4 = 64;
	        }

	        output = output + 
	        keyStr.charAt(enc1) + 
	        keyStr.charAt(enc2) + 
	        keyStr.charAt(enc3) + 
	        keyStr.charAt(enc4);
	        chr1 = chr2 = chr3 = "";
	        enc1 = enc2 = enc3 = enc4 = "";
	    }while(i < input.length);
	    return output;
   }
   
   function strUnicode2Ansi(asContents) {
	    var len1 = asContents.length;
	    var temp = "";
	    for(var i=0;i<len1;i++) {
	        var varasc=asContents.charCodeAt(i);
	        if(varasc<0)
	             varasc+=65536;
	        if(varasc>127)
	             varasc=UnicodeToAnsi(varasc);
	        if(varasc>255) {
	            var varlow=varasc & 65280;
	            varlow=varlow>>8;
	            var varhigh=varasc & 255;
	            temp+=String.fromCharCode(varlow)+String.fromCharCode(varhigh);
	        } else {
	            temp+=String.fromCharCode(varasc);
	        }
	    }
	    return temp;
	}
	
   window.COMM_LOGIN = {
		 $Id : function(id){
			 return document.getElementById(id);
		 },  
		
		refreshInfo : function(){
			 var loginVal = this.getCookieVal("CPLOGIN");
			 var loginInfoVal =this.getCookieVal("CPLOGIN_INFO"); 
			 
			 var loginStatusNoLoginObj = this.$Id("loginStatusNoLogin"); 
			 var loginStatusWSObj = this.$Id("loginStatusWS"); 
			 var loginStatusNoWSObj = this.$Id("loginStatusNoWS"); 
			 
			 if (loginVal != null) {
				 if (loginInfoVal != null) {
					var jsonObj = eval("(" + loginInfoVal + ")");
					
					loginStatusNoLoginObj.style.display = "none";
					if(jsonObj.hascertNo == true){
						loginStatusWSObj.style.display = "";
						loginStatusNoWSObj.style.display = "none";
						this.$Id("loginStatusWSAccount").title = jsonObj.allname;
						this.$Id("loginStatusWSAccount").innerHTML = jsonObj.subname;
					} else {
						loginStatusWSObj.style.display = "none";
						loginStatusNoWSObj.style.display = "";
						this.$Id("loginStatusNoWSAccount").title = jsonObj.allname;
						this.$Id("loginStatusNoWSAccount").innerHTML = jsonObj.subname;
					}					
				} else {
					//登录超时
					loginStatusNoLoginObj.style.display = "";
					loginStatusWSObj.style.display = "none";
					loginStatusNoWSObj.style.display = "none";
				}
			 } else {
				 //未登录
				 loginStatusNoLoginObj.style.display = "";
				 loginStatusWSObj.style.display = "none";
				 loginStatusNoWSObj.style.display = "none";
			 }			 
		},
		
		setCookie : function(name, value, domain, expires) {
			domain = domain || document.domain;
			if (typeof (expires) == 'undefiend' || expires == null || expires == '') {
				document.cookie = name + "=" + encodeURIComponent(value)
						+ "; path=" + "/" + "; domain=" + domain ;
			} else {
				var expTimes = expires * 1000;
				var expDate = new Date();
				expDate.setTime(expDate.getTime() + expTimes);
				document.cookie = name + "=" + encodeURIComponent(value)
						+ ";expires=" + expDate.toGMTString() + "; path=" + "/"
						+ "; domain=" + domain ;
			}
		},
		
		getCookieVal : function(name){
			var cookieArr = document.cookie.replace(/\s/g, "").split(';');
			for(var i=0;i<cookieArr.length;i++){
				var tempObj = cookieArr[i].split('=');
				if(tempObj[0]==name)
					return decodeURIComponent(tempObj[1]);
			}
			return null;
		},
		
		showMoney : function(me){
			var _this = this;
			var moneySpan = _this.$Id("moneySpan");
			if(moneySpan.innerHTML.indexOf("显示余额")>=0){
				$.ajax({
					type : "get",
					async : false,
					url : "https://passport.aicai.com/xpassport/aicai/user/showMoney",
					dataType : "jsonp",
					success : function(jsonObj) {
						if(jsonObj.success){
							if(jsonObj.isLogin){
			    				var hiddenLink = '<a href="javascript:void(0);" onclick="window.COMM_LOGIN.showMoney();">隐藏</a>&nbsp;';
			    				var moneyFont = "<font color='red'>"+jsonObj.money+"&nbsp;元</font>";		    			
			    				moneySpan.innerHTML = hiddenLink + moneyFont;
			    			}else{
			    				_this.$Id("loginStatusNoLogin").style.display = "";
			    				_this.$Id("loginStatusWS").style.display = "none";
			    				_this.$Id("loginStatusNoWS").style.display = "none";
			    			}
			    		}else{
			    			alert(jsonObj.msg);
			    		}
			    		return false;
					}
				});
			}else{
				var moneySpanHtml = '<a onclick="window.COMM_LOGIN.showMoney();" href="javascript:void(0);">显示余额</a>';
				moneySpan.innerHTML = moneySpanHtml;
			}
			return false;
		},
		
		loginout : function(){
			var _this = this;
			$.ajax({
				type : "get",
				async : false,
				url : "https://passport.aicai.com/xpassport/aicai/login/loginOut",
				dataType : "jsonp",
				success : function(jsonObj) {
					if (jsonObj.success) {
						_this.$Id("loginStatusNoLogin").style.display = "";
						_this.$Id("loginStatusWS").style.display = "none";
						_this.$Id("loginStatusNoWS").style.display = "none";
						window.location = subsystem_webclient;
					} else {
						alert(jsonObj.msg);
					}
				}
			});
		},
		
		bjnewsLogin : function(){
			var loginLink = subsystem_passport + "/login/index?redirectUrl=" + encode64(window.parentUrl);
			window.location.href = loginLink;
		},
		
		getDomainHost : function(){
			var reg = /(([^.]+\.(com|cn|net|org|biz|info|cc|tv))|((\d+\.){3}(\d+)))((?=:)|)/;
			var domain = window.location.host;
			var domainHost = reg.exec(domain)[0];
			if (typeof (domainHost) == 'undefined' || domainHost == null
					|| domainHost == '') {
				domainHost = domain;
			}
			return domainHost;
		},
		
		isBlank : function(str){
			if(str == null || str == 'undefined' || str == ''){
				return true;
			}
			return false;
		}
   };
   var parentUrl = this.parent.window.location.href;
   //初始化页面，根据cookie刷新登录信息
   COMM_LOGIN.refreshInfo();
</script>	    

		





