		  $(".tabhover3  .lives").click(
			 function(){
				$(".tabcon3").hide();
				$(this).addClass("cur3");
				$(".tabhover3  .lives").not(this).removeClass("cur3");
				$(".tabcon3").eq($(".tabhover3  .lives").index($(this))).show();
			 }
		  )
		</script>
		<h2 class="ptitle5"><span><a href="http://www.yyzhibo.cc/luxiang/lanqiu/lqjd/" target="_blank">篮球经典录像</a></span></h2>
		<div class="bd">
		    <ul class="vediolist">
