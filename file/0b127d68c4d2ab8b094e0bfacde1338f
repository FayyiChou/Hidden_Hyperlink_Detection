		   <SCRIPT>
var speed3=25;//速度数值越大速度越慢
var www_qpsh_coma1 = document.getElementById("www_qpsh_coma1");    //非w3c标准要找到表单，必须是要用getElementById("XX");
var www_qpsh_coma2 = document.getElementById("www_qpsh_coma2");
var www_qpsh_coma = document.getElementById("www_qpsh_coma");
www_qpsh_coma2.innerHTML=www_qpsh_com1.innerHTML;

function Marquee3(){
  if(www_qpsh_coma2.offsetWidth-www_qpsh_coma.scrollLeft<=0)
    www_qpsh_coma.scrollLeft-=www_qpsh_coma1.offsetWidth;
  else{
    www_qpsh_coma.scrollLeft++;
  }
}
var MyMar3=setInterval(Marquee3,speed3)
www_qpsh_coma.onmouseover=function() {clearInterval(MyMar3)}
www_qpsh_coma.onmouseout=function() {MyMar3=setInterval(Marquee3,speed3)}
</SCRIPT></td>
