<SCRIPT type=text/javascript>
function selectTagg(name,s){
//领导讲话
for( var i=1; i<=4; i++){
	if(i==s)
	{	 
	 document.getElementById(name+'tag'+i).className =name+'tagdq';
	 document.getElementById(name+'con'+i).style.display = "block";
     document.getElementById(name+'gen'+i).style.display = "block";		 
	}	
	else {	
	   document.getElementById(name+'tag'+i).className =name+'tag';	
	   if (name!="bank"){  
	   document.getElementById(name+'con'+i).style.display = "none";
	   document.getElementById(name+'gen'+i).style.display = "none"; 
	   }
	// alert(s);
		 }
	  		} // return false;
}
</SCRIPT>
