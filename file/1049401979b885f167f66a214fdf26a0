			;(function ($) {
				$(window).load(function () {
					var o = $('.sf-menu');
					o.superfish();
					o.find('li a').each(function () {
						if ($(location).attr('href').indexOf($(this).attr('href')) >= 0){
							$(this).addClass('active');
							return;
						}
					})
					if (o.parents('aside').length){
						var width = $('.container').outerWidth() - $('aside').outerWidth();
						o.find('.sf-mega').each(function () {
							$(this).width(width);
						})
					}


				});
			})(jQuery);

			if ($(window).width() > 900) {
				$(document).ready(function(){
					$("#navigation").sticky({topSpacing:0});
					$('a.external').click(function(){
						var url = $(this).attr('href');
						$('.mask').fadeIn(250, function(){
							document.location.href = url;
						});
						$("#loader").fadeIn("slow");
						return false;
					});
					
				});
			}

			$(window).scroll(function(){
				var _scrollTop = $(this).scrollTop()+($(window).height()/2);
				if($('#box').size()) {
					$('#box').animate({'top' : _scrollTop}, 50);
				}
			});

			$(function() {
					
				var urlParId = 3;
				//alert(urlParId);
				var i = 0;
				var _active = 0;
				$(".slistpro h3").each(function() {					
					if($(this).parents('.container').attr('class') == 'container max-img') {
						return;
					}
					var _id = $(this).attr('data-id');
					//console.log($(this).parents('.container').attr('class'));
					console.log(urlParId, _id);
