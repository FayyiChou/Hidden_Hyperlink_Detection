					if(!$(this).find('li').length) {
						//console.log($(this).attr('id'));
						$('#'+$(this).attr('id')).hide();
					}	
				})
			})

			//加入我的最愛
			function AddWishlist(obj, id) {
				$(obj).attr('disabled', 'disabled');	//停用
				ajaxobj = new AJAXRequest;
				ajaxobj.method = "POST";
				ajaxobj.url = "action.php";
				ajaxobj.content = "action=AddWishlist&id=" + id;
				ajaxobj.callback = function (xmlobj) {
					var response = xmlobj.responseText;
					$('.wish').show();
					$('#wishlist_counter').html(response);
					$('.wishlist_tip_' + id).html('本產品已加入');
					$(obj).removeAttr('disabled', 'disabled');	//啟用
				};
				ajaxobj.send();
			}

			//加入購物車
			function AddShoppingCar(obj, id) {
				var num = 1;
				var dimension = "";
				var additionalBut = ".additionalBut";
				if ($('#num_' + id).length > 0) num = $('#num_' + id).val();
				if ($('#dimension_' + id).length > 0) dimension = $('#dimension_' + id).val();
				//$(obj).attr('disabled', 'disabled');	//停用
				//$(additionalBut).attr('disabled', 'disabled');  //加購停用
				//$(additionalBut).attr('onclick', '');  //加購啟用
				var flage = 1;
				if($('#sessionNum_'+id).attr('data-sessionnum')) {
					if(num >= $('#sessionNum_'+id).attr('data-sessionnum')) {
						flage = 0;
					} else {
						alert('請重新選購加購商品');
					}
				}
				ajaxobj = new AJAXRequest;
				ajaxobj.method = "POST";
				ajaxobj.url = "action.php";
				ajaxobj.content = "action=AddShoppingCar&id=" + id + "&num=" + num + "&dimension=" + dimension;
				ajaxobj.callback = function (xmlobj) {
					var response = xmlobj.responseText;
					var newvalue = response.split("#$@%^");
					
					$('#sessionNum_'+id).attr('data-sessionnum', num);
					
					$('#topright_shoppingcar_counter').html(newvalue[0]);	//右上角購物車產品數量
					$('.topright_shoppingcar_total').html(newvalue[1]);	//右上角小計
					$('.shopping_tip_' + id).html("<mark>" + newvalue[2] + "</mark>");			//已放入購物車
					$('.shopping_tip_' + id).delay(1500).hide(0);		//消失
					$('#topright_products_list').html(newvalue[3]);		//右上角清單
										$(obj).removeAttr('disabled', 'disabled');			//啟用
					
					$(additionalBut).each(function() {
						var additionalButId = $(this).data('id');
						var masterId = $(this).data('masterid');
						var selectId = $(this).data('selectid');
						//alert(selectId);
						//console.log(additionalButId);
						//if(selectId === null || selectId === undefined) {
							if(flage)
							$(this).attr('onclick', 'AddShoppingCarAdditional(this, '+additionalButId+', '+masterId+');').attr('data-selectid', '').text('我要加購');  //加購啟用
							//$(this).attr('onclick', 'AddShoppingCarAdditional(this, '+additionalButId+', '+masterId+');');  //加購啟用
						//}	
					})
				};
				ajaxobj.send();
			}

			function getUrlParameters(parameter, staticURL, decode){

		       var currLocation = (staticURL.length)? staticURL : window.location.search,
		           parArr = currLocation.split("?")[1].split("&"),
		           returnBool = true;

		       for(var i = 0; i < parArr.length; i++){
		            parr = parArr[i].split("=");
		            if(parr[0] == parameter){
		                return (decode) ? decodeURIComponent(parr[1]) : parr[1];
		                returnBool = true;
		            }else{
		                returnBool = false;            
		            }
		       }

		       if(!returnBool) return false;  
		    }

			//加入購物車-加購
			function AddShoppingCarAdditional(obj, id, masterId) {
				var num = 1;
				var dimension = "";
				if($('#sessionNum_' + masterId).attr('data-sessionNum')) {
					canSelectNum = $('#sessionNum_' + masterId).attr('data-sessionNum');
				} else {
					if ($('#num_' + masterId).length > 0) canSelectNum = $('#num_' + masterId).val();
				}
				//if ($('#num_' + masterId).length > 0) canSelectNum = $('#sessionNum_' + masterId).attr('data-sessionNum');
				if ($('#dimension_' + id).length > 0) dimension = $('#dimension_' + id).val();
				//$(obj).attr('disabled', 'disabled');	//停用
				//$(obj).attr('onclick', 'javascript:alert("此商品已加購")').text('已加購');
				//alert($(this).attr('data-selectId'));
				//return;
				/*
				$('.product_description').find('a').each(function() {
					var idParameter = getUrlParameters("id", $(this).attr('href'), true);
				});
				return;
				*/
				ajaxobj = new AJAXRequest;
				ajaxobj.method = "POST";
				ajaxobj.url = "action.php";
				ajaxobj.content = "action=ChkShoppingCarAdditional&id=" + masterId;
				ajaxobj.callback = function (xmlobj) {
					var response = xmlobj.responseText;
					var newvalue = response.split("#$@%^");
					//alert(newvalue[0]);
					if(newvalue[0] == 0) {
						alert('請選購商品才能加購');
						return;
					}

					selectRow = 0;
					$('.additionalBut').each(function(e) {
						if($(this).attr('data-selectId')) {
							a = $(this).attr('data-selectId').split(',');
							for (i = 0; i < a.length; i++) {
								console.log(a[i]);
								if(a[i] == masterId){	
									selectRow++;
								}
							}	
							
						}
					});
					//return;
					if(selectRow >= canSelectNum) {
						alert('只能加購' + canSelectNum + '項商品');
						return;
					}

					var del = "tr_"+ id + "_";
					$(obj).attr({'onclick': "_DeleteShoppingCar(this,"+id+", "+masterId+")", 'data-selectId': masterId}).text('取消');

					ajaxobj = new AJAXRequest;
					ajaxobj.method = "POST";
					ajaxobj.url = "action.php";
					ajaxobj.content = "action=AddShoppingCarAdditional&id=" + id + "&masterId=" + masterId + "&num=" + num + "&dimension=" + dimension;
					ajaxobj.callback = function (xmlobj) {
						var response = xmlobj.responseText;
						var newvalue = response.split("#$@%^");
						$('#topright_shoppingcar_counter').html(newvalue[0]);	//右上角購物車產品數量
						$('.topright_shoppingcar_total').html(newvalue[1]);	//右上角小計
						$('.shopping_tip_' + id).html("<mark>" + newvalue[2] + "</mark>");			//已放入購物車
						$('.shopping_tip_' + id).delay(1500).hide(0);		//消失
						$('#topright_products_list').html(newvalue[3]);		//右上角清單
												//$(obj).removeAttr('disabled', 'disabled');			//啟用
						//$(obj).text('已加購');	//停用
					};
					ajaxobj.send();

				};
				ajaxobj.send();	
			}

			//加入購物車-滿額加購
			function AddShoppingCarFullAdditional(obj, id, subtotal) {
				var num = 1;
				var dimension = "";
				var flag = 1;
				
				$('.fullAdditionalBut').each(function(k, v) {
					if($(this).text() === '已加購') {
						flag = 0;
					}
				});

				$('.tab1b').each(function(k, v) {
					if($(this).attr('data-type') == 'fullAdditional') {
						flag = 0;
					}
				})
				if(!flag) {
					alert('只能加購一項商品!');
					return false;
				} else {	
					//return;
					if ($('#num_' + id).length > 0) num = $('#num_' + id).val();
					if ($('#dimension_' + id).length > 0) dimension = $('#dimension_' + id).val();
					$(obj).attr('disabled', 'disabled');	//停用
					ajaxobj = new AJAXRequest;
					ajaxobj.method = "POST";
					ajaxobj.url = "action.php";
					ajaxobj.content = "action=AddShoppingCarFullAdditional&id=" + id + "&subtotal=" + subtotal + "&num=" + num + "&dimension=" + dimension;
					ajaxobj.callback = function (xmlobj) {
						var response = xmlobj.responseText;
						var newvalue = response.split("#$@%^");
						$('#topright_shoppingcar_counter').html(newvalue[0]);	//右上角購物車產品數量
						$('.topright_shoppingcar_total').html(newvalue[1]);	//右上角小計
						$('.shopping_tip_' + id).html("<mark>" + newvalue[2] + "</mark>");			//已放入購物車
						$('.shopping_tip_' + id).delay(1500).hide(0);		//消失
						$('#topright_products_list').html(newvalue[3]);		//右上角清單
						//$(obj).removeAttr('disabled', 'disabled');			//啟用
						$(obj).text('已加購');	//停用
											};
					ajaxobj.send();
				}	
			}

