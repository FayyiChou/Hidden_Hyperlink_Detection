			<script>
		$('.bwlight').find('a').on('click', function(e) {
			e.preventDefault();
			var stuff = [];
			//console.log($(this).children().index('.bwi'));
			$('.bwlight').find('a').each(function() {
				if($(this).parent().hasClass('col-xs-6')) {
					var captionId = $(this).attr('data-sub-html');
					//console.log($(this).parent().hasClass('col-xs-6'));
					stuff.push({
						'src': $(this).attr('href'), 
						'subHtml': $(captionId).html()
					});
				}
			})
			if(stuff.length) {
				$(this).lightGallery({
					dynamic: true,
					fullScreen: false,
					toogleThumb: true,
					download: false,
					thumbnail: true,
					animateThumb: false,
					showThumbByDefault: false,
					index: $(this).children().index('.bwi'),
					dynamicEl: stuff
				});
			}
			return false;
		});
