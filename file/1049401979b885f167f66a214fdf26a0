		$(function() {
			if(!$( ".mobile" ).is(":visible")) {
				$('.owl-carousel11').show();
				$('.owl-carousel12').hide();
			} else {
				$('.owl-carousel11').hide();
				$('.owl-carousel12').show();
			}
		})
		$( window ).resize(function() {
			if(!$( ".mobile" ).is(":visible")) {
				$('.owl-carousel11').show();
				$('.owl-carousel12').hide();
			} else {
				$('.owl-carousel11').hide();
				$('.owl-carousel12').show();
			}
		});
	
	</script>
