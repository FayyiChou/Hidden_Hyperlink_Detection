function QueryString(name) {
    var result = location.search.match(new RegExp("[\?\&]" + name + "=([^\&]+)", "i"));
    if (result == null || result.length < 1) {
        return "";
    }
    return result[1];
}

	if (QueryString("from") == "app") {
		jQuery(function () {			
				jQuery("#header").hide();
		  });
		 }else { 
		 jQuery(function () {			
				jQuery("#header").show();
		  });
		 }
