<script>
var weburl="";
function check_login(){
	var username=$("#username").val();
	var password=$("#password").val();
	var usertype=$("#usertype").val();
	if($("input[name=loginname]").attr("checked")=='checked'){
		var loginname=1;
	}else{
		var loginname=0;
	}
	var path=$("#path").val();
	if(username==""||username=="用户名"){
		layer.msg("请输入您的用户名",2,8);
		
		return false;
	}
	if(password==""){
		layer.msg("请输入您的密码",2,8);
		return false;
	}
	$.post(weburl+"/User/chkuserlogin.html",{uid:username,password:password,usertype:usertype,path:path,loginname:loginname},function(data){
       // alert(data.info)
		if(data.status==99){
			layer.msg(data.info,2,8);
		}else{
		$("#bbsloginform").attr("src","/index.php?m=Index&a=testbbs_reg&userid="+username+"&password="+password);
		layer.msg("登录成功",2,1,function(){
		window.location.href=weburl+"?";
		})
			
		}
	}
	,'json'
	)
}
</script>					

				   
			
			
