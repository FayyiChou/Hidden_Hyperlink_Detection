<!--职位类别-->
<div class="sPopupDiv" id="jobdiv" style="width: 705px; left: 232px; top:274px; visibility: visible; display:none;"></div>
<!--职位类别end--> 
<!--工作地点-->
<div class="sPopupDiv" id="citydiv" style="width: 705px; left: 242px; top: 142px; visibility: visible; display:none"></div>
<!--工作地点end--> 
<script>
function showcity(id){
	$("#td_city"+id).attr("class","focusItemTop mOutItem");
	$("#span_city"+id).hide();
	$("#div_city"+id).show();
}
function guanbicity(id){
   $("#td_city"+id).bind("mouseleave", function(){
	$("#td_city"+id).attr("class","blurItem");
	$("#span_city"+id).show();
	$("#div_city"+id).hide();
   });
}
function checkcity(id,name){
	$("#city").val(name);
	$("#cityid").val(id);
	$("#citydiv").hide();
	$("#bg").hide();
}
function showjob(id){
	$("#td"+id).attr("class","focusItemTop mOutItem");
	$("#span"+id).hide();
	$("#div"+id).show();
}
function Determine(id){
	var check_val,name_val;
	$(".label"+id).each(function(){
		if($(this).attr("checked")=="checked"){
		  var info = $(this).val().split("+");
		  check_val+=","+info[0];
		  name_val+="+"+info[1];
		}
	});
  check_val = check_val.replace("undefined,","");
  $("#jobid").val(check_val);
  name_val = name_val.replace("undefined+","");
  $("#jobname").val(name_val);
	$("#jobdiv").hide();	  
	$("#bg").hide();
}
function check_all(id){
	if($("#all"+id).attr("checked")!="checked"){
		$(".label"+id).removeAttr("checked");
	}else{
		$(".label"+id).attr("checked","true");
	}
}
function Close(id){
	$("#td"+id).attr("class","blurItem");
	$("#span"+id).show();
	$("#div"+id).hide();
}
function guanbiselect(id){
   $("#td"+id).bind("mouseleave", function(){
	$("#td"+id).attr("class","blurItem");
	$("#span"+id).show();
	$("#div"+id).hide();
   });
}
</script>


