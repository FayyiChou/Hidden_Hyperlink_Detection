    <script type="text/javascript">
        $(function () {
            var ERROR_BOX = 'error-box';
            var $loginBtn = $('.login-btn');
            var $userId = $('#userid');
            var $password = $('#password');
            var $inputs = [$userId, $password];
            var error = function ($input, msg) {
                var resultMsg = msg || $input.attr('data-errorMsg');
                $input.parents('.input-box').addClass(ERROR_BOX)
                    .next('.login-error-msg').html(resultMsg);
            };
            $.each($inputs, function () {
                var me = this;
                me.focus(function () {
                    me.parents('.input-box').removeClass(ERROR_BOX).next('.login-error-msg').html('');
                }).blur(function () {
                    if (!$.trim(me.val())) {
                        error(me);
                        return;
                    }
                });
                me.watermark(me.attr('placeholder'));
            });
            $('form').submit(function (e) {
                var bValide = true;
                $.each($inputs, function () {
                    if (!this.val()) {
                        error(this);
                        bValide = false;
                    }
                });
                return bValide;
            });

            $.ajax({
                url: 'http://check.wanfangdata.com.cn//api/getip',
                data: { requestUrl: window.location.href },
                type: "GET",
                dataType: "jsonp",
                success: function (data) {
                    if (data.IpString != '101.199.108.54') {
                        $(".ip-text").append('<br/><t>欢迎来自</t>')
                            .append(" " + data.IpString)
                            .append('<t>的用户</t>')
                            .addClass('ip-tip');
                    }
                }
            });
            //自动跳转到科研项目管理系统
            var kyxm = '';
            if (kyxm != null && kyxm != "") {
                window.location.href = 'http://et.wanfangdata.com.cn/ky/Login';
            }
        })
    </script>

