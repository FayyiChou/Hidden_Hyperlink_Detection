        $(function () {
            $('.centerbox a, .centerbox a img, .w_about a, .w_partner a img, .w_video a, .w_news a img, .w_idol a, .w_equip_index a img, .w_place a img, .w_contact a img, .footerin a, .footerin a img').css({

