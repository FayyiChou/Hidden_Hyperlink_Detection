<script language="JavaScript" type="text/javascript">
	$('[pa_ui_name]').each(function(){		
		var _id = $(this).attr("id");
		var _msgId = $(this).attr("msgId");
		if (_msgId){
			var _pa_ui_valid_show = msgInfo[_msgId]["show"] || "";
			var _pa_ui_valid_empty = msgInfo[_msgId]["empty"] || "";
			var _pa_ui_valid_focus = msgInfo[_msgId]["focus"] || "";
			var _pa_ui_valid_error = msgInfo[_msgId]["error"] || "";
			var _pa_ui_valid_correct = msgInfo[_msgId]["correct"] || "";
			
			$(this).attr('pa_ui_valid_show',_pa_ui_valid_show);
			$(this).attr('pa_ui_valid_empty',_pa_ui_valid_empty);
			$(this).attr('pa_ui_valid_focus',_pa_ui_valid_focus);
			$(this).attr('pa_ui_valid_error',_pa_ui_valid_error);
			$(this).attr('pa_ui_valid_correct',_pa_ui_valid_correct);
			

			switch (_id){
				case "first_name":
					$(this).attr("pa_ui_valid_rules","{required:{error:'请您输入真实姓名。'}}");//,exFunction:{name:'validate_username'}
					break;
				case "check_code":
					$(this).attr("pa_ui_valid_rules","{required:{error:'请您输入4位验证码。'},regex:{value:/^[0-9a-zA-Z]{4}$/}}");
					break;
				case "certcode":
					$(this).attr("pa_ui_valid_rules","{required:{error:'请您输入证件号码。'}}");//,exFunction:{name:'validate_id_all'}
					break;
			}

		}
	});	

</script>
<script language="javascript" type="text/javascript" src="https://resources.pingan.com.cn/app_js/ui30/js/pa_ui.js"></script>
<script type="text/javascript" language="javascript">
	var buttonClicked = false;  
	validate_id_all = function(str,element){
		var valu = $('#idTypeSel').val();
		var codevale = str;
	    var txt = $('#idTypeSel').find('option:selected').text() + '不符合规范，请您重新输入。';
	    if (codevale == "") return txt;
	    var reg = false;
	    if (valu == '0'){reg = true;}
	    if (valu == '1'){reg = validate_citizenid(codevale); }
		if (valu == '2'){reg = validate_passport(codevale);}
		if (valu == '3'){reg = validate_militaryCert(codevale);}
		if (valu == '6'){reg = validate_homeReturnCert(codevale);}
		if (valu == 'A'){reg = validate_otherCertcode(codevale);}
		if (valu == 'L'){reg = validate_otherCertcode(codevale);}
	
		if(!reg || codevale == ""){
			return txt;
		}
		else {
			return true;
		}
	}
	
	getUserIDByContact = function(){
		var ret = true;
		var formObj = document.getElementById("regForm");
		ret = $('#regForm').validator({triggerOnSubmit:true}).validator('check');
		if (ret){
				document.getElementById("sub_button").disabled = true;
				if(buttonClicked) {  
			    	return;      
				} 
				buttonClicked = true;
			   $("#first_name").val($.trim($("#first_name").val()));
				var _cnName = $("#first_name").val();
				var _idNo = $("#certcode").val();
				var _idType = $("#idTypeSel").val();
				var _validCode=$("#check_code").val();
				$.post("./validateCustomerTypeByBasicInfo.do",{"cnName":_cnName,"idNo":_idNo,"idType":_idType,"validCode":_validCode},function(json){
					eval("var obj=" + json);	
					if (obj.status == "fail"){
					   	document.getElementById("sub_button").disabled = false;
						buttonClicked = false;
					 	$("#errorInfo").html(obj.msg);
				    	$("#errorInfo").attr("className","ErrorMsg");
					   document.getElementById('validateImg').src='./paic/common/vcode.do?'+Math.random();
					   document.getElementById("check_code").value="";
					}else if (obj.status == "success"){
						   //如果是ewise客户，使用ewise提供的公钥加密，post到ewise
					    if(obj.data.postServer =="EWISE"){
						   	  var ewiseURL=obj.data.ewiseURL;
							  formObj.action=ewiseURL;
							  formObj.submit();
				       	}else{
			             //formObj.action = "./retrieveDispathViewByUserType.do";
		        	     //formObj.submit();
		        	     window.location = "./retrieveDispathViewByUserType.do?isPS=";
		            }
		       }				
				});	
		}
	}
</script>

</body></html>