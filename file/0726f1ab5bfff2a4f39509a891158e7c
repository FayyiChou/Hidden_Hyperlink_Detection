<script>
	function move(i)
	{
		if(i==1)
		{
			document.getElementById("top_ly1").style.display="block"; 
			document.getElementById("top_ly").style.display="none"; 
			document.getElementById("top_ly2").style.display="none"; 
			document.getElementById("top_ly3").style.display="none"; 
		}
		else if(i==2)
		{
			document.getElementById("top_ly1").style.display="none"; 
			document.getElementById("top_ly").style.display="block"; 
			document.getElementById("top_ly2").style.display="none"; 
			document.getElementById("top_ly3").style.display="none"; 
		}
		else if(i==4)
		{
			document.getElementById("top_ly1").style.display="none"; 
			document.getElementById("top_ly").style.display="none"; 
			document.getElementById("top_ly2").style.display="none"; 
			document.getElementById("top_ly3").style.display="block"; 
		}
		else
		{
			document.getElementById("top_ly1").style.display="none"; 
			document.getElementById("top_ly").style.display="none"; 
			document.getElementById("top_ly2").style.display="block"; 
			document.getElementById("top_ly3").style.display="none"; 
		}
	}
</script>
