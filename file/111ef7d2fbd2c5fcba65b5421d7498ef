<script>
$(document).ready(function(){
  //自動滑出
  //$("#btn_quickmenu").animate({"top" : 0-10,"height":20}, {duration: 1000, easing: "jswing"});
  //點選展開
  $("#btn_quickmenu").click(function(){
    fn_control_quickmenu();
  });
    //OPEN
  $("#btn_about").click(function(){
      var fix = 95;
      var h = $("#div_about").height();
      var mtop = parseInt($("#div_about").css("marginTop"));
      if (h == 30){
        $("#div_about").animate({"margin-top": (mtop-fix) + "px","height": h+fix},500);
        $("#btn_about").attr("src","images/indexbtn02.png");
      }else{
        $("#div_about").animate({"margin-top": (mtop+fix) + "px","height": h-fix},500);
        $("#btn_about").attr("src","images/indexbtn01.png");
      }
    });


});
//控制選單的部分
function fn_control_quickmenu(){
    var c_height = $("#quickmenu_content").outerHeight(true);
    if ($("#btn_quickmenu").hasClass("hide")){
      $("#quickmenu_container").stop().animate({"top" : -142,"height":132}, {duration: 1000, easing: "jswing"}); 
      $("#btn_quickmenu").removeClass("hide").addClass("show");
    }else{
      //隱藏
      $("#quickmenu_container").stop().animate({"top" : -10,"height":0}, {duration: 1000, easing: "jswing"}); 
      $("#btn_quickmenu").removeClass("show").addClass("hide");
    }
}
