      $(document).ready(function(){
                $("#searchkey").focus(function(){
          $(this).val("");
        }).blur(function(){
          var val = $(this).val();
          if (val==""){
            //$(this).val("Search");
          }
        });
        $("#searchkey").keydown(function(event){
          if (event.keyCode==13){
            var val = $("#searchkey").val();
            if (val !="" && val !="Search"){
              location.href="product_search.php?searchkey="+encodeURI(val);
            }
          }
        });
        $("#btn_search").click(function(){
            var val = $("#searchkey").val();
            if (val !="" && val !="Search"){
              location.href="product_search.php?searchkey="+encodeURI(val);
            }
        });

        $("#btn_search_cancel").click(function(){
          $("#searchkey").val("");
        });
      });
