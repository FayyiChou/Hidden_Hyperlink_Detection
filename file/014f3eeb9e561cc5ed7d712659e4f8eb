<script src="http://js.18183.duoku.com/pc/cwdb/flgg/js/swiper-3.3.1.jquery.min.js" type="text/javascript">
</script>
<script>
    $(function() {
        var allProductId = "";
        $(".sidebar-banner").find(".goodslist-item").each(function(i, j) {
            var productId = $(j).data("productid");
            allProductId += productId + ',';
        });
        $.ajax({
            type: "POST",
            url: "http://api.1.18183.com/web/product/detail?productIds=" + allProductId,
            data: "",
            dataType: 'JSONP',
            async: false,
            success: function(d) {
                $(".sidebar-banner").find(".goodslist-item").each(function(i, j) {
                    var productId = $(j).data("productid");
                    $(j).find("a").attr("href", d.data[productId].url);
                    $(j).find("img").attr("data-src", d.data[productId].coverImg + '@q_80');
                    $(j).find("h4 a").html(d.data[productId].name);
                    $(j).find("li.fl i").html(d.data[productId].price + "人次");
                    $(j).find("li.fr i").html(d.data[productId].lastPeriodSurplus + "人次");
                    var jd = (1 - d.data[productId].lastPeriodSurplus / d.data[productId].price).toFixed(2) * 100 + "%";
                    $(j).find(".progressBar-wrap span").css("width", jd);
                });

                var mySwiper = new Swiper('.swiper-container', {
                    //direction: 'vertical',
                    loop: true,
                    autoplay: 5000,
                    autoplayDisableOnInteraction: false,

                    // 如果需要前进后退按钮
                    nextButton: '.swiper-button-next',
                    prevButton: '.swiper-button-prev',
                    lazyLoading: true
                });
            }
        });
    });
</script>
