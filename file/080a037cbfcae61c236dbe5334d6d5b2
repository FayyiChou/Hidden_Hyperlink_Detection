		<script type="text/javascript">
    $(function () {
        var currenturl = window.location.href.toLowerCase();
        if (currenturl.indexOf("corp") > -1) {
            $("#gy").addClass("currentLi");
        }
        else if (currenturl.indexOf("contact") > -1) {
            $("#lx").addClass("currentLi");
        }
        else if (currenturl.indexOf("news") > -1) {
            $("#xw").addClass("currentLi");
        }
        else if (currenturl.indexOf("22955") > -1) {
            $("#3d").addClass("currentLi");
        }
        else if (currenturl.indexOf("22957") > -1) {
            $("#fz").addClass("currentLi");
        }
        else
        {
             $("#sy").addClass("currentLi");
        }
    })
</script>






	
