<script type="text/javascript">
        function SubmitSearch() {
            if ($("#textfield").val() == "") {
                alert("请输入您要搜索的产品关键字 ！");
                return;
            }
            else {
                window.open("/offerlist.html?q=" + encodeURI(document.getElementById("textfield").value));
            }
        }
</script>

        
