var num =1;
var tnum =5;
var my;
hideAll();
run();
document.getElementById("index_main").onmouseover=stopRun;
document.getElementById("index_main").onmouseout=run;
function hideAll() {
	for (var i=1; i<=tnum; i++) {
		document.getElementById("ads"+i).style.display="none";
	}
}
document.getElementById("ads1").style.display="block";
function autoShow() {
	hideAll();
	if (num < tnum) {num++;}else{num=1;}
	document.getElementById("ads"+num).style.display="block";
}
function stopRun() { clearInterval(my);}
function run(){my = setInterval(autoShow,2000);}
}
