<script type="text/JavaScript">
/*小数的js验证 */
function clearNoNum(obj){
	//先把非数字的都替换掉，除了数字和.
	obj.value = obj.value.replace(/[^\d.]/g,"");
	//必须保证第一个为数字而不是.
	obj.value = obj.value.replace(/^\./g,"");
	//保证只有出现一个.而没有多个.
	obj.value = obj.value.replace(/\.{2,}/g,".");
	//保证.只出现一次，而不能出现两次以上














	obj.value = obj.value.replace(".","$#$").replace(/\./g,"").replace("$#$",".");
}

function check(){
	var countryCode=document.getElementById("countryCode").value;
	var Grossweight=document.getElementById("Grossweight").value;
	var length=document.getElementById("length").value;
	var width=document.getElementById("width").value;
	var height=document.getElementById("height").value;
	if(countryCode==""){
		alert("请选择目的国家");
		return false;
	}
	if(checkNull(length)||checkNull(width)||checkNull(height)){
		   alert("请填写包裹尺寸");
		   return false;
	}
	if(Grossweight==""){
		alert("请填写包裹体重");
		return false;
	}
	return true;
}

function doTestArea(obj) {
		if (obj.value == "请输入运单号码...") {
			obj.value = "";
			obj.focus();
		}
	}	
	
	function doBlurTestArea(obj) {
		if (obj.value == "") {
			obj.value = "请输入运单号码...";
		}
	}	

	function indexCostBudget(){
		if(check()){
			document.getElementById("costBudgetForm").action="/util/indexCostBudget";
			document.getElementById("costBudgetForm").submit();
		}
	}
//刷新验证码
















function changeValidateCode(obj) {
	//获取当前的时间作为参数，无具体意义
















	var timenow = new Date().getTime();
	//每次请求需要一个不同的参数，否则可能会返回同样的验证码
	//可能和浏览器的缓存机制有关系
	obj.src="util/ValidateImage?d="+timenow;
}
//是否为空验证(true为空,false不为空)
function checkNull(check_var) {
	if (check_var == null || check_var.length == 0 ||check_var == "请输入运单号码...")
		return true;
	else
		return false;
}
//登录验证
function checkLoginForm(){
	var userName = document.getElementById("OperCode").value;
	var password = document.getElementById("Word").value;
	if(checkNull(userName)){
		alert("请输入帐号登录！");
		return false;
	}
	if(checkNull(password)){
		alert("请输入登录密码！");
		return false;
	}
	document.getElementById("loginForm").action="/page/login";
	document.getElementById("loginForm").submit();
}
//轨迹查询验证
function checkTrackForm() {
	var queryCode = document.getElementById("queryCode").value;
	if (checkNull(queryCode)) {
		alert("运单号不为能空，请输入运单号!");
		return false;
	}
	document.queryTrackForm.submit();
}
//选择那类新闻
function chooseNews(sum){
  if(sum==2){
    document.getElementById("news1").style.display="none";
    document.getElementById("news2").style.display="";
  }else{
    document.getElementById("news2").style.display="none";
    document.getElementById("news1").style.display="";
  }
}
