var speed=20
demoss2.innerHTML=demoss1.innerHTML;
function Marquee(){
if(demoss2.offsetTop-demoss.scrollTop<=0)
demoss.scrollTop-=demoss1.offsetHeight;
else{
demoss.scrollTop++;
}
}
var MyMar=setInterval(Marquee,speed);
demoss.onmouseover=function() {clearInterval(MyMar);}
demoss.onmouseout=function() {MyMar=setInterval(Marquee,speed);}
