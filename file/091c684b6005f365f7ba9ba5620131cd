<script language="javascript" type="text/javascript">
jQuery(document).ready(function($) {
    jQuery(window).scroll(function () {
        if (jQuery(this).scrollTop() > 40) {
            //jQuery('#sitelogo').addClass('small-logo');
			jQuery('#sitelogo').attr('src','https://www.fightdyslexia.com/wp-content/themes/mainsite/images/logo_small.jpg');
			jQuery('.main-navigation').css('margin-top','0rem');
        } else {
            jQuery('#sitelogo').attr('src','https://www.fightdyslexia.com/wp-content/themes/mainsite/images/logo.jpg');
			jQuery('.main-navigation').css('margin-top','1.71429rem');
        }
    });
});
</script>
