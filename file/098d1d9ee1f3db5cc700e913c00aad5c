        });

        $(".n_msgs").find("em,.msgs_box").mouseover(function () {
            if (!$(".n_msgs").hasClass("h")) {
                $(".n_msgs").addClass("h"); $(".n_msgs").find(".msgs_box").stop(true, true).slideDown(230);
