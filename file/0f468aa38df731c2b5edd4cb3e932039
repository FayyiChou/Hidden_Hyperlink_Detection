<script>
$(document).ready(function() {
	$(window).scroll(function(){
		var scroll_height=$(this).scrollTop()+100;
		
		$(".demo2").stop().animate({
			top:scroll_height			
			},1000);
	
		})
	
		var op=0;
	$(".part_lef").click(function(){
			if (op==0){
				$(".demo").animate({
				"right":"-129px"
	
				},function(){
					op=1;
					})
			}
			else{
				$(".demo").animate({
					"right":"0px"
					},function(){
							op=0;
				})				
			}
		})	
	$(".add_img1").click(function(){
		$(".demo2").hide();
		})
		
		
		
		
    
});
</script>


