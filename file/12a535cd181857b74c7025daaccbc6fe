    	    	function searchAllGroup()
    	{
        	key = $.trim($("#searchKeywords").attr("value"));
            if(key=='请输入圈子或圈主名称')
            {
            	$("#searchKeywords").focus();
            	return;
            }
        	if (key.length < 1)
        	{
            	$("#searchKeywords").focus();
            	return;
        	}
        	self.location.href= baseUrl + "group/search?name=" + encodeURIComponent(key);
    	}
