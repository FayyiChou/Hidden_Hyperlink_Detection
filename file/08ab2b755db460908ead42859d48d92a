function createCookie(name,value,time){
var expires=new Date();
expires.setTime(expires.getTime()+time*3600*1000);
var expiryDate=expires.toGMTString();
document.cookie=name+"="+value+";expires="+expiryDate;
}
var url = window.location.hash;
var loc = url.substring(url.lastIndexOf('#')+1, url.length);
if(loc != ""){
createCookie("FormUser",loc,0.35);
window.location.href='http://www.bolinwm.com/reg.asp';
}
function closeTop(){
	
	document.getElementById("desktop_id").style.display="none";
	
}
