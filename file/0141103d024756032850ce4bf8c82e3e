function lphysetcookie(name, value, expires, path, domain, secure)
{
    var tmpexpires = new Date();
	tmpexpires.setTime(tmpexpires.getTime() + expires*2592000);

	document.cookie= name + "=" + escape(value) +
        ((expires) ? "; expires=" + tmpexpires.toGMTString() : "") +
        ((path) ? "; path=" + path : "") +
        ((domain) ? "; domain=" + domain : "") +
        ((secure) ? "; secure" : "");
}
function lphygetcookie(name)
{
    var dc = document.cookie;
    var prefix = name + "=";
    var begin = dc.indexOf("; " + prefix);
    if (begin == -1)
    {
        begin = dc.indexOf(prefix);
        if (begin != 0) return null;
    }
    else
    {
        begin += 2;
    }
    var end = document.cookie.indexOf(";", begin);
    if (end == -1)
    {
        end = dc.length;
    }
    return unescape(dc.substring(begin + prefix.length, end));
}
if(lphygetcookie('clientdownhidden')){
	document.getElementById('clientdown').style.display='none';
}
