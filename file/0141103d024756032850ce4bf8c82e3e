		lphymenumorepid=pid;
		document.getElementById("menumore").style.top = (oy-30) + "px";
		document.getElementById("menumore").style.left = (document.documentElement.clientWidth - document.getElementById('menumore').offsetWidth-40) + "px";
	}
}
function lphyquotepost(pid){
	var postconetent=document.getElementById('content_'+pid).innerText;
	if(postconetent.length>50){
		postconetent=postconetent.substr(0,50)+'...';
	}
	document.getElementById('message').innerHTML='[quote]原帖由 [i]'+document.getElementById('author_'+pid).innerText+'[/i] 于 '+document.getElementById('postime_'+pid).innerText+' 发表\n'+postconetent+'[/quote]';
	window.location.hash="replyform";
}
var dogotonext=0;


//Powered by My0511_div. 打赏
function dawndsopen(){
	html_height=document.body.scrollHeight;//获取页面高度
    document.getElementById("rewardpay").style.height=html_height+"px"; 
	document.getElementById('rewardpay').style.display='block';
}
function dawndsclose(){
	document.getElementById('rewardpay').style.display='none';
}
function opendawnotherwd(){
	dashangStyle(null);
	document.getElementById("rewardpay_other-pay").style.display = "block";//其他金额输入弹出
}
function closedawnotherwd(){
	document.getElementById("rewardpay_other-pay").style.display = "none";//其他金额输入关闭
	document.getElementById('dashangoth').innerHTML="其他金额";
	document.getElementById('dashangiptpay').value= '';
	document.getElementById('dashangMoney').value= '';
}
function dawndsinfoopen(){
	document.getElementById('rewardinfo').style.display='block';
}
function dawndsinfoclose(){
	document.getElementById('rewardinfo').style.display='none';
}
function dashangStyle(ele){
	var liAry=document.getElementById("rewardpaywp").getElementsByTagName("li");
	var liLen=liAry.length;
	var liID= (ele!=null ? ele.id : 0);
	for(var ids=0;ids<liLen;ids++)
	{
		if(liAry[ids].id==liID){
			liAry[ids].className = 'rewardpay_moneyxz'+(ids+1);
			document.getElementById('dashangMoney').value=liID.replace('rewardpay__li-','');
			document.getElementById('dashangoth').innerHTML="其他金额";
		}else{
			liAry[ids].className = 'rewardpay_money-'+(ids+1);
		}
	}
}
function checkdashangforms(){
	var dawnmoney = document.getElementById('dashangMoney').value;
	if(dawnmoney<1||dawnmoney>200){
		alert("请选择打赏金额！");
		return false;
	}else{
		document.getElementById("rewardpay_tj").style.display = "block";//提交信息弹出
		document.getElementById("rewardpay_nr").style.display = "block";//提交信息弹出
		return false;
	}
}
function checkiptpay(){
	if (document.dashangforms.dashangiptpay.value.length  ==  0){
		alert("请输入打赏金额!");
		document.dashangforms.dashangiptpay.focus();
		return false;
	}
	var re = /^[1-9]+[0-9]*.?[0-9]*$/;   //判断字符串是否为数字
	if (!re.test(document.dashangforms.dashangiptpay.value))
	{
		alert("请输入正确的打赏金额！打赏金额不能低于1元。");
		document.dashangforms.dashangiptpay.focus();
		return false;
	}
	if (document.dashangforms.dashangiptpay.value<1||document.dashangforms.dashangiptpay.value>200)
	{
		alert("打赏金额不能大于200元！");
		document.dashangforms.dashangiptpay.focus();
		return false;
	}
	var ipt = document.getElementById('dashangiptpay').value //获取文本框值
	document.getElementById('dashangMoney').value=ipt;
	document.getElementById('dashangoth').innerHTML=ipt+'元'; //赋值
	document.getElementById("rewardpay_other-pay").style.display = "none";//其他金额输入关闭
	if(mywalletfund>=parseFloat(ipt)){
		document.getElementById('dashangmywalletpay').disabled=false;
		document.getElementById("dashangmywalletpay").style.visibility = "visible";
	}else{
		document.getElementById('dashangmywalletpay').disabled=true;
		document.getElementById("dashangmywalletpay").style.visibility = "hidden";
	}
} 

