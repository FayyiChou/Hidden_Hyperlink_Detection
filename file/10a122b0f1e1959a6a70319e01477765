<SCRIPT type=text/javascript>
function switch_item(ValueInputName,ValueText,ListInputName,HiddenInputName,HiddenInputValue)
{
switch_item(ValueInputName,ValueText,ListInputName,HiddenInputName,HiddenInputValue,'','');
}

function switch_item(ValueInputName,ValueText,ListInputName,HiddenInputName1,HiddenInputValue1,HiddenInputName2,HiddenInputValue2)
{
document.getElementById(ValueInputName).innerHTML = ValueText;
document.getElementById(ListInputName).style.display = "none";

if (document.getElementById(HiddenInputName1) != null) document.getElementById(HiddenInputName1).value = HiddenInputValue1;
if (document.getElementById(HiddenInputName2) != null) document.getElementById(HiddenInputName2).value = HiddenInputValue2; 
}

function show_item(InputName)
{
document.getElementById(InputName).style.display = "block";
}

function hide_item(InputName)
{
document.getElementById(InputName).style.display = "none";
}
</SCRIPT>
