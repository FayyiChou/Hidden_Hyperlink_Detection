	nclk(this,'cnx.more','','');
	document.location.href = "/ArticleSearchList.nhn?search.clubid=12048475&search.searchBy=0&search.sortBy=&search.query=" + encodeURIComponent('');
}






function findPurchaseUrl(){
	var message = '';

	
		message = '해당 카페의 회원만 구매가 가능 합니다.';
		alert(message);
		return "";
	
	
	
	
	
	
	
}

function purchaseItem(){
	var purchaseUrl = findPurchaseUrl();
	if (!purchaseUrl || purchaseUrl == "") {
		return;
	}
 
	window.open(purchaseUrl, 'Purchase', 700, 700, "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=1");
}

function purchaseNpay(){
	var purchaseUrl = findPurchaseUrl();
	if (!purchaseUrl || purchaseUrl == "") {
		return;
	}

	document.location.href = purchaseUrl + "&p__g=n__w";
}
