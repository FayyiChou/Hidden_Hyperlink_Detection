<script type="text/javascript">
    function uzSearchSubmit(oForm){
        var aSearchText = $(oForm).find("input[name='search_text']");
        if(aSearchText.length){
            var searchText = $(aSearchText[0]).val();
            if(typeof(searchText) == "undefined"){
                return false;
            }
            if(searchText.length < 3){
                alert("Для работы поиска необходимо ввести не менее 3 символов");
                return false;
            }
            var submitURL = "//www.wedstyle.su/katalog?&pf=1&action=search&search_subcats=1&search_text="+encodeURIComponent(searchText);
            document.location.href = submitURL;
        }
        return false;
    }
</script>


