    <script type="text/javascript">
        uzLastMenuItemTouchstart = 0;
        uzLastMenuItemClickCnt = 0;

        function uzOnMenuTouchstart(id){
            if(uzLastMenuItemTouchstart == id){
                uzLastMenuItemClickCnt++;
            } else {
            	uzLastMenuItemTouchstart = id;
                uzLastMenuItemClickCnt = 0;
            }
                    }
                                
        function uzOnMenuClick(id){
            var res = true;
            if(uzLastMenuItemTouchstart == id){
                if(uzLastMenuItemClickCnt == 0){
                	$("#j"+id).trigger("mouseover");
                    res = false;                 } else {
                    smclick();
                }
            } else {
                smclick();
            }
			            
            return res;
        }
        
    </script>
