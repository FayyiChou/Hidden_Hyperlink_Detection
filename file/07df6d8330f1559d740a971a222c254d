<script type="text/javascript">
    $("#YshHdSearch").on("submit", function(e){
        var cid = $("#YshHdSearchCate").val();
        if (!$("#YshHdAssistWord").val() && cid) {
            var domain = ($("select[data-adult-flg]").length) ? "adult.shopping.yahoo.co.jp":"shopping.yahoo.co.jp";
            var param  = $("#YshHdSearch").serialize();
            location.href = "http://"+ domain +"/category/"+ cid  +"/list?"+param;
            e.preventDefault();
        }
    });
</script>
