var speed=30
gpic5.innerHTML=gpic4.innerHTML
function Marquee(){
if(gpic5.offsetWidth-gpic3.scrollLeft<=0)
gpic3.scrollLeft-=gpic4.offsetWidth
else{
gpic3.scrollLeft++
}
}
