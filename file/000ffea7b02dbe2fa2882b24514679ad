var speed=25
zibet2.innerHTML=zibet1.innerHTML
//alert(zibet2.offsetWidth+"——"+zibet.scrollLeft);
function Marquee1(){

if(zibet2.offsetWidth-zibet.scrollLeft<=0)
zibet.scrollLeft-=zibet1.offsetWidth
else{
zibet.scrollLeft=zibet.scrollLeft+1
}
}
var MyMar1=setInterval(Marquee1,speed)
zibet.onmouseover=function() {clearInterval(MyMar1)}
