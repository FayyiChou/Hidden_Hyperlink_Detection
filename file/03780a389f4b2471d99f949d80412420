            $(function () {
                var len = $("#ad1Rotator_RotatorNumber > li").length;
                var index = 0;
                $("#ad1Rotator_RotatorNumber li").mouseover(function () {
                    index = $("#ad1Rotator_RotatorNumber li").index(this);
                    show_ad1Rotator_Images(index);
                });
                $("#ad1Rotator_RotatorContainer").hover(function () {
                    if (MyTime) {
                        clearInterval(MyTime);
                    }
                }, function () {
                    MyTime = setInterval(function () {
                        show_ad1Rotator_Images(index)
                        index++;
                        if (index == len) { index = 0; }
                    }, 5000);
                });
                var MyTime = setInterval(function () {
                    show_ad1Rotator_Images(index)
                    index++;
                    if (index == len) { index = 0; }
                }, 5000);
            })

            function show_ad1Rotator_Images(i) {
                $("#ad1Rotator_RotatorSlider").stop(true, false).animate({ top: -134 * i }, 800);
                $("#ad1Rotator_RotatorNumber li").eq(i).addClass("on").siblings().removeClass("on");
            }

            $(function () {
