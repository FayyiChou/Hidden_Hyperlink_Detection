$('.comic_map').click(function(e){
    var image_id = $(this).attr('imageid');
    get_read_comic_gift(image_id);
    e.stopPropagation();
});

/* 赠送礼物
 * @return {[type]} [description]
 */
function give_gift() {
    var gift_id = $('.btnList .cf').find('.cur').attr('giftid');
    var gift_count = $('#gift_body').find('.cur').attr("value");
    var gift_textarea = $('#gift_textarea').val();
    if (gift_textarea == '请输入送礼感言，不填写是无法送礼物哦！' || gift_textarea=='') {
        showMsg('请输入送礼感言，不填写是无法送礼物哦！');
        $("#gift_textarea").parent().addClass("err");
        return false;
    };
    if ($("#gift_body").find('.cur').length<1) {
        gift_count = $('#price_else_text').val();
    };
    var gift_price_count = $('#comic_gift_price').html();
    if (gift_count == 0 ) {
        showMsg('请选择赠送礼物数量');
        return false;
    };
    show_loading();
    var data = {
        gift_id : gift_id,
        gift_count : gift_count,
        gift_price_count : gift_price_count,
        comment : gift_textarea,
        comic_id : "147284"
    };
    $.ajax({
        url: "/comic/ajax.php?mod=comic_gift&act=comic_gift_save",
        data:  data,
        type: "POST",
        dataType : 'json',
        async:false,
        success: function(o){
            if (o.code>0) {
                hide_loading();
                $('.btn_tanchu_reset').click();
                $('#gift_num_'+gift_id).html(Number($('#gift_num_'+gift_id).html())+Number(gift_count));
                showDialogBox('恭喜你，赠送礼物成功！',o.message);
            } else {
                hide_loading();
                showMsg(o.message);
            };
        }
    });
}

//获取礼物弹窗
function  get_read_comic_gift (image_id) {
    var data = {
        image_id : image_id,
        comic_id : "147284"
    };
    $.ajax({
        url: "/comic/ajax.php?mod=chapter&act=get_read_comic_gift",
        data:  data,
        type: "GET",
        dataType : 'json',
        success: function(o){
            if (o.code>0) {
                var loadingdiv = '<div id ="loadingdiv" class="mark" style="width: 100%; height: 100%; background: #000; opacity: .6; z-index: 99; position: fixed; left: 0; top: 0;" style="filter:alpha(opacity:60);"></div>';
                $('body').append(loadingdiv);
                $('body').append(o.html);
                var gift_load_info = {
                    'oGiftImg' : $('.send_box .fl ul li'),
                    'oGiftBtn' : $('.send_box .fl ol li:not(.no)'),
                    'oGiftBtnR' : $('.send_box .fr .btnList a'),
                    'oPlus' : $('.amount-wrapper .plus'),
                    'oMinus' : $('.amount-wrapper .minus'),
                    'oCount' : $('.amount-wrapper .text-amount'),
                    'oNum_list' : $('.amount-wrapper .num_list a')
                }
                binding_gift_load(gift_load_info);


                var top = ($(window).height() - $('#comic_read_gift_body').height())/2;
                var scrollTop = $(document).scrollTop();
                top = Number(top)+Number(scrollTop);
                $('#comic_read_gift_body').css('top',top+'px').show();

                //文字滚动
                var _oUlTxt=$('.scroll_left_text ul');
                var _aLiTxt=_oUlTxt[0].getElementsByTagName('li');
                _oUlTxt[0].innerHTML+=_oUlTxt[0].innerHTML;

                _oUlTxt[0].style.width=(_aLiTxt[0].offsetWidth+10)*_aLiTxt.length+'px';
                var timer=null;
                var speed=-2;

                timer=setInterval(function(){
                    _oUlTxt[0].style.left=_oUlTxt[0].offsetLeft+speed+'px';
                    if(_oUlTxt[0].offsetLeft<-_oUlTxt[0].offsetWidth/2)
                    {
                        _oUlTxt[0].style.left='0';
                    }
                },50)
                $('.scroll_left_text').hover(function(){
                    clearInterval(timer);
                },function(){
                    timer=setInterval(function(){
                        _oUlTxt[0].style.left=_oUlTxt[0].offsetLeft+speed+'px';
                        if(_oUlTxt[0].offsetLeft<-_oUlTxt[0].offsetWidth/2)
                        {
                            _oUlTxt[0].style.left='0';
                        }
                    },50)
                })
            }
        }
    });
}

/**
* 切换到普通的阅读模式
*
*/
function changeReadmode(href)
{
    //修改cookie
    $.xcookie('readmode', '', { expires: 31536000, path: '/', domain: '.'+_cfg_domain } );

    //修改vip特权信息,执行完成后跳转
    updateReadmode(0, function() {
        href && (window.location.href = href);
    });
}

/**
* 切换阅读模式记录用户状态
*  @param int val
*  @return mixed
*/
function updateReadmode(val, callback) {
    callback && callback();
    /*
    login_ajax(function(){
        var user = get_user(true);

        if (user && user['group_user'] == 1) {
            $.get('/vip/ajax.php?mod=member&act=update_privilege', {pri_key : 'readmode', val : val}, function() {
                callback && callback();
            });
        }
    });
    */
}
