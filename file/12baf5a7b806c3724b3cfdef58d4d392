var binding_gift_load = function(obj) {
    var _oGiftImg=obj.oGiftImg;
    var _oGiftBtn=obj.oGiftBtn;
    var _oGiftBtnR=obj.oGiftBtnR;
    var _oPlus=obj.oPlus;
    var _oMinus=obj.oMinus;
    var _oCount=obj.oCount;
    var oNum_list=obj.oNum_list;
    var n=0;
    var i=1;
    _oGiftBtn.click(function(){
        n=$(this).index();
        $(this).siblings().removeClass('cur')
        $(this).addClass('cur')
        $('.send_title').html(_oGiftBtnR.eq(n).attr('gifttitle'));
        $('.send_btn_gift').find('span').html(_oGiftBtnR.eq(n).attr('giftname'));
        tab();
    });
    _oGiftBtnR.click(function(){
        n=$(this).index();
        $(this).siblings().removeClass('cur')
        $(this).addClass('cur')
        $('.send_title').html(_oGiftBtnR.eq(n).attr('gifttitle'));
        $('#gift_mini_'+_oGiftBtnR.eq(n).attr('giftid')).siblings().removeClass('cur');
        $('#gift_mini_'+_oGiftBtnR.eq(n).attr('giftid')).addClass('cur');
        $('.send_btn_gift').find('span').html(_oGiftBtnR.eq(n).attr('giftname'));
        tab();
    });

    _oPlus.click(function(){
        var gift_num = Number($('#gift_num').val())+1;
        if(gift_num>1)$('.J_Minus').find('em').addClass('cur');

        var gift_price = _oGiftBtnR.eq(n).attr('price');
        $('#gift_price_count').html(Number(gift_price)*Number(gift_num));
        _oCount.attr('value',gift_num);return false;
    });
    _oMinus.click(function(){
        var gift_num = Number($('#gift_num').val())-1;
        if(gift_num<=1){gift_num=1;$('.J_Minus').find('em').removeClass('cur');}
        var gift_price = _oGiftBtnR.eq(n).attr('price');
        $('#gift_price_count').html(Number(gift_price)*Number(gift_num));
        _oCount.attr('value',gift_num);return false;
    });
    oNum_list.click(function(){
        _oCount.attr('value',$(this).html());
        var gift_price = _oGiftBtnR.eq(n).attr('price');
        $('#gift_price_count').html(Number(gift_price)*Number($(this).html()));
    });

    $('body').delegate('#gift_num','keyup',function(){
        if(!/^\d+$/.test($(this).val())){
            $(this).val('');
        } else {
            var gift_price = _oGiftBtnR.eq(n).attr('price');
            $('#gift_price_count').html(Number(gift_price)*Number($(this).val()));
        }
    });

    function tab(){
        _oGiftImg.removeClass('cur');
        _oGiftImg.eq(n).addClass('cur');
        _oGiftBtnR.removeClass('cur');
        _oGiftBtnR.eq(n).addClass('cur');
        var gift_price = _oGiftBtnR.eq(n).attr('price');
        var gift_num = $('#gift_num').val();
        $('#gift_price_count').html(Number(gift_price)*Number(gift_num));
        $('.send_box .fr .title').html(_oGiftBtn.eq(n).attr('title'));
        $('.send_box .fr .title_con').html(_oGiftBtn.eq(n).attr('_stitle'));
    }
    $('.send_btn_gift').find('a').click(function(){
        var gift_id = $('.btnList').find('.cur').attr('giftid');
        var gift_count = $('#gift_num').attr("value");
        var gift_price_count = $('#gift_price_count').html();
        if (gift_count == 0 ) {
            showMsg('请选择赠送礼物数量');
            return false;
        };
        show_loading();
        var data = {
            gift_id : gift_id,
            gift_count : gift_count,
            gift_price_count : gift_price_count,
            comic_id : "147284",
            type : 1
        };
        $.ajax({
            url: "/comic/ajax.php?mod=comic_gift&act=comic_gift_save",
            data:  data,
            type: "POST",
            dataType : 'json',
            async:false,
            success: function(o){
                if (o.code>0) {
                    hide_loading();
                    $('body').append(o.html);
                    var top = ($(window).height() - $('#gift_thank').height())/2;
                    var scrollTop = $(document).scrollTop();
                    top = Number(top)+Number(scrollTop);
                    $('#gift_thank').css('top',top+'px').show();
                    $('#comic_read_gift_body').remove();

                } else {
                    hide_loading();
                    showMsg(o.message);
                };
            }
        });
    });
}

var oGift = [];
var oGift2 = [];
var gift_timer = [];


/**
 * 绑定热点区域事件
 * @param  {[type]} id [description]
 * @return {[type]}    [description]
 */
function binding_map(id,class_name,num) {
    oGift[num]=$('#'+id);
    oGift2[num]=$('.'+class_name);
    gift_timer[num]=null;
    var i = 0;
    gift_timer[num]=setInterval(function (){
        if(i%2==1)
        {
            oGift2[num].animate({opacity:0},300);
        }
        else
        {
            oGift2[num].animate({opacity:1},800);
        }
        i++;
    }, 1000);

    oGift[num].hover(function(){
        clearInterval(gift_timer[num]);
        oGift2[num].css({opacity:1});
    },function(){
        oGift2[num].css({opacity:0})
        gift_timer[num]=setInterval(function (){
            if(i%2==1)
            {
                oGift2[num].animate({opacity:0},300);
            }
            else
            {
                oGift2[num].animate({opacity:1},800);
            }
            i++;
        }, 1000);
    })
}
</script>

