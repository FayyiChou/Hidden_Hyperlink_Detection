  <script language="javascript">
        $(document).ready(function () {
            loadImage('http://www.iyhotel.com/public/images/iyloading.gif', function () {
            });//缓存加载图片
            imgpreload();//执行预加载
        });
        //window.onload = function(){
        //	imgpreload();//执行预加载
        //}
        // 预加载
        function imgpreload() {
            var domStr = arguments[0] ? arguments[0] : '';
            if (domStr == '') {
                $(".readyload").each(function () {
                    var info = $(this).attr('data-url');
                    $(this).preload_images({src: info});

                });
            } else {
                $(domStr).each(function () {
                    var info = $(this).attr('data-url');
                    $(this).preload_images({src: info});
                });
            }

        }
        function loadImage(url, callback) {

            var img = new Image(); //创建一个Image对象，实现图片的预下载
            img.src = url;

            if (img.complete) { // 如果图片已经存在于浏览器缓存，直接调用回调函数
                callback.call(img);
                return; // 直接返回，不用再处理onload事件
            }
            img.onload = function () { //图片下载完毕时异步调用callback函数。
                callback.call(img);//将回调函数的this替换为Image对象
            }
        }
        //!!!!!!!!!!!!!!!!!!!!!!!!!!!!
        jQuery.fn.preload_images = function (options) {

            options = $.extend({

                src: ""

            }, options);

            var _self = this;

            var img = new Image();

            $(img).load(function () {

                _self.attr("src", options.src);

            }).attr("src", options.src);

            return _self;

        }
    </script> 
