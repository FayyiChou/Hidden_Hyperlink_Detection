function fromto(type) {
    $('#bd_div6').removeClass("on");
    $('#bd_div6').click();
    if (type == 1) {
        var from_val = $("#input_from").val();
        var to_val = $("#input_to").val();
        if (from_val.indexOf('请输入起点') != -1)from_val = '';
        if (to_val.indexOf('请输入终点') != -1)to_val = '';
        if (from_val == '') {
            $("#input_from").val(to_val);
            $("#input_to").val(from_val);
        }
    } else {
        var from_val = $("#input_from").val();
        var to_val = $("#input_to").val();
        if (from_val.indexOf('请输入起点') != -1)from_val = '';
        if (to_val.indexOf('请输入终点') != -1)to_val = '';
        if (to_val == '') {
            $("#input_from").val(to_val);
            $("#input_to").val(from_val);
        }
    }
    return false;
}
function load_html(id) {
    $("#nav_box li").each(function () {
        $(this).removeClass("on");
    });
    $("#nav_box li").eq(id - 1).addClass("on");
    $("#nav_box_n li").each(function () {
        $(this).removeClass("on");
    });
    $("#nav_box_n li").eq(id - 1).addClass("on");
    if (id == 1) {
        $("#info_div").show();
        $("#picture_div").hide();
        $("#comment_div").show();
        $("#comment_div_hd").show();
        $("#map_div").hide();
    } else if (id == 2) {
        $("#info_div").hide();
        $("#comment_div").hide();
        $("#comment_div_hd").hide();
        $("#map_div").hide();
        $("#picture_div").show();
        var html = $("#picture_div").html();
        if (html == '') {
            var id = "120699";
            $.ajax({
                url: "http://www.iyhotel.com/hotelinfo2/get_photos",
                type: 'GET',
                data: {id: id},
                dataType: "html",
                beforeSend: function (data) {
                    showonediv('loading');
                },
                success: function (data) {
                    //加载内容
                    $("#picture_div").empty().html(data);
                    closeDiv('loading');
                },
                timeout: 50000,
                error: function () {
                }
            });
        }
    } else if (id == 3) {
        $("#info_div").hide();
        $("#picture_div").hide();
        $("#comment_div").show();
        $("#comment_div_hd").hide();
        $("#map_div").hide();
    } else if (id == 4) {
        $("#info_div").hide();
        $("#picture_div").hide();
        $("#comment_div").hide();
        $("#comment_div_hd").hide();
        $("#map_div").show();
        var html = $("#bigmap").html();
        if (html == '') {
            try {
                load_map_wrapper();
            } catch (e) {
            }
        }
    }
}
$(document).ready(function () {
    $("body").keydown(function (event) {
        if (event.keyCode == "27") { // 按 Esc
            closeDiv('loading');
        }
    });
    //加载日期控件
    $("#i_tm1,#i_tm2").each(function () {
        function c(e) {
            var t = e.target.id;
            if (!t) {
                t = $(e.target).closest("div").find("input:eq(0)").attr("id");
            }
            if (t == "i_tm1") {
                return  showCalendar('i_tm1', 'i_tm2', '' + ZNDate.format(ZNDate.today()) + '', '' + ZNDate.format(ZNDate.plus(ZNDate.today(), 60)) + '', '', '', 'text');
            } else {
                return showCalendar('i_tm2', '', $('#i_tm1').val(), '' + ZNDate.format(ZNDate.plus(ZNDate.today(), 60)) + '', '', '', 'text');
            }
        };
        $(this).click(function (e) {
            c(e);
            $(this).blur(function () {
                this.value = this.value.replace(/[^\d\-]/g, '');
            });
        });
    });
    $("#nav_box li").live('click', function () {
        var id = $(this).attr('id');
        load_html(id);
        var nextpos = $('#left_box').offset().top;
        $("html,body").animate({ scrollTop: nextpos }, 600);
    });
    $("#nav_box_n li").live('click', function () {
        var id = $(this).attr('id');
        load_html(id);
        var nextpos = $('#left_box').offset().top;
        $("html,body").animate({ scrollTop: nextpos }, 600);
    });
    $("#type_ul li").live('click', function () {
        $("#type_ul li").each(function () {
            $(this).removeClass("on");
        });
        $(this).addClass("on");
        var index = $(this).index();
        $("#type").val(index);
        getAjaxpages(1);
    });
    function resetLineSearch() {
        $('.viewport').removeClass('hg321');
        $('.map_reset_select').hide();
        $('.bus_way').hide();
        $('.search_taxi').hide();
        $('.dir_line').hide();
        $("#input_from").val("请输入起点").css("color", "#B8B8B8");
        $("#input_to").val("深圳蛇口泰格国际公寓");
        line_search.start = null;
        line_search.end = null;
        $('#line_scrollbar').tinyscrollbar();
        var scrollbarData = $('#line_scrollbar').data("plugin_tinyscrollbar");
        scrollbarData.update();
    }

    $(".map_nav").live('click', function () {
        var cname = $(this).attr("class")
        var data = $(this).attr("data");
        if (cname.indexOf('on') > 0) {
            $(this).removeClass("on");
            if (data == 6) {
                deleteoverlays();
                resetLineSearch();
                $('#line_scrollbar').hide();
            } else {
                deleteoverlays();
            }
        } else {
            $(this).addClass("on");
            if (data == 6) {
                deleteoverlays();
                resetLineSearch();
                $('#line_scrollbar').show();
            } else {
                $('#line_scrollbar').hide();
                local_search(data);
            }
        }
        return false;
    });
    $(".bus_taxi a").live('click', function () {
        var cname = $(this).attr("class")
        if (cname.indexOf('cur') == -1) {
            $(".bus_taxi a").each(function () {
                var _cname = $(this).attr("class").replace(/cur$/i, "");
                $(this).attr('class', _cname);
            });
            $(this).attr('class', cname + "cur");
            $('#line_type').val(cname);
            if (line_search.start && line_search.end) {
                var line_type = $('#line_type').val();
                line_search.line_type = line_type;
                if (line_type == 'bus') {
                    busline_search(line_search.start, line_search.end);
                } else if (line_type == 'taxi') {
                    taxiline_search(line_search.start, line_search.end);
                }
            }
        }
        return false;
    });
    $('#input_from').live('focus', function () {
        var o = $(this);
        if (o.val().indexOf('请输入起点') != -1) {
            o.val('').css("color", "#000000");
        }
    });
    $('#input_from').live('blur', function () {
        var o = $(this);
        if (o.val() == '') {
            o.val('请输入起点').css("color", "#B8B8B8");
        }
    });
    $('#input_to').live('focus', function () {
        var o = $(this);
        if (o.val().indexOf('请输入终点') != -1) {
            o.val('').css("color", "#000000");
        }
    });
    $('#input_to').live('blur', function () {
        var o = $(this);
        if (o.val() == '') {
            o.val('请输入终点').css("color", "#B8B8B8");
        }
    });
    $(".note a").live('click', function () {
        var from_val = $("#input_from").val();
        var to_val = $("#input_to").val();
        if (from_val.indexOf('请输入起点') != -1)from_val = '';
        if (to_val.indexOf('请输入终点') != -1)to_val = '';
        $("#input_from").val(to_val);
        $("#input_to").val(from_val);
        return false;
    });
    $("#search_line").live('click', function () {
        var from_val = $("#input_from").val();
        var to_val = $("#input_to").val();
        if (from_val.indexOf('请输入起点') != -1)from_val = '';
        if (to_val.indexOf('请输入终点') != -1)to_val = '';
        if (from_val == "") {
            $("#input_from").focus();
        } else if (to_val == "") {
            $("#input_to").focus();
        } else {
            if (from_val == to_val) {
                alert("起、终点的地址不能相同");
                return;
            }
            $(".bus_taxi a").each(function () {
                var cname = $(this).attr("class")
                if (cname.indexOf('cur') != -1) {
                    var _cname = $(this).attr("class").replace(/cur$/i, "");
                    $('#line_type').val(_cname);
                }
            });
            $('.viewport').addClass('hg321')
            var line_type = $('#line_type').val();
            if (from_val == hotelinfo.title) {
                line_search.s_type = 2;
                line_search.start = bigpoint;
            } else if (to_val == hotelinfo.title) {
                line_search.s_type = 1;
                line_search.end = bigpoint;
            }
            line_search.s_title = from_val;
            line_search.e_title = to_val;
            line_search.line_type = line_type;
            if (line_type == 'bus') {
                busline_search(from_val, to_val);
            } else if (line_type == 'taxi') {
                taxiline_search(from_val, to_val);
            }
        }
        return false;
    });
    $(".single input").live('click', function () {
        line_search.s_type = 0;
        line_search.policy = $(this).val();
        busline_search(line_search.start, line_search.end);
        //return false;
    });
    $(".drive_single input").live('click', function () {
        line_search.s_type = 0;
        line_search.policy = $(this).val();
        taxiline_search(line_search.start, line_search.end);
        //return false;
    });
    $("#note_search").live('click', function () {
        line_search.s_type = 0;
        var from_val = $("#input_from").val();
        var to_val = $("#input_to").val();
        line_search.e_title = from_val;
        line_search.s_title = to_val;
        if (line_search.line_type == 'bus') {
            busline_search(line_search.end, line_search.start);
        } else if (line_search.line_type == 'taxi') {
            taxiline_search(line_search.end, line_search.start);
        }
        //return false;
    });
    $("#research").live('click', function () {
        line_search.s_type = 0;
        reselectInput();
        //return false;
    });
    $("#pic_type_ul li").live('click', function () {
        $("#pic_type_ul li").each(function () {
            $(this).removeClass("on");
        });
        $(this).addClass("on");
        var id = $(this).attr('id');
        if (id == 0) {
            $("#hotelpic_list").show();
            $("#tujie_div").hide();
            $("#uppic_div").hide();
        } else if (id == 1) {
            $("#hotelpic_list").hide();
            $("#tujie_div").show();
            $("#uppic_div").hide();
        } else if (id == 2) {
            $("#hotelpic_list").hide();
            $("#tujie_div").hide();
            $("#uppic_div").show();
        }
        $("#pic_type").val(id);
    });
    $("#xinghao_ul li").live('click', function () {
        $("#xinghao_ul li").each(function () {
            $(this).removeClass("on");
        });
        $(this).addClass("on");
        var id = $(this).attr('id');
        $("#xinghao").val(id);
        pic_getAjaxpages(1);
    });
    $("#hotel_cookie li").live('mouseover', function () {
        $(this).children("div.closed").show();
    });
    $("#hotel_cookie li").live('mouseout', function () {
        $(this).children("div.closed").hide();
    });
    $("#pic_ul li").live('mouseover', function () {
        var pic_src = $(this).find('img').attr('src');
        pic_src = pic_src.replace("160x120", "500x375");
        $("#loadpic").children('img').attr('src', pic_src);
        var _postop = $(this).offset().top;
        var _posleft = $(this).offset().left;
        var postop = _postop + 118;
        var posleft = _posleft;
        $("#loadpic").css({"top": postop + "px", "left": posleft + "px"}).show('1000');
    });
    $("#pic_ul li").live('mouseout', function () {
        $("#loadpic").children('img').attr('src', 'http://www.iyhotel.com/public/images/imglod.png');
        $("#loadpic").hide();
    });
});


function load_map_wrapper() {
    var info = "<div style='width:350px;margin:0 10px;'><div style='float:right;width:154px;'><img style='margin:4px;padding: 4px;border: 1px solid #ccc;' src='http://tp1.znimg.com/Hotel_Images/120699/f6d5e3c8-4a88-4529-b845-61b574e34528.jpg ' width='139' height='104' title='深圳蛇口泰格国际公寓'/></div><div style='float:left;width:190px;'><h4 style='margin:0 0 5px 0; font-size:14px;'>深圳蛇口泰格国际公寓</h4><p>地址：<span style=\"font-size: 12px;\">南山区蛇口南海大道1033号(近蛇口客运码头)<br><font style=\"color:red\">订房电话:0755-61595105</font></span></p><p style='clear:both;margin-top:10px;text-align:left;font-size:12px;'><span class='map_go'><a href='javascript:void(0);' onclick='fromto(0);'>到这里去</a></span><span class='map_going'><a href='javascript:void(0);' onclick='fromto(1);'>从这里出发</a></span></p></div></div>";
    initialize("bigmap",22.490076517,113.91924631, 690, info);
    var from_search = new BMap.Autocomplete({
        "input": "input_from",
        "location": bigmap
    });
    var to_search = new BMap.Autocomplete({
        "input": "input_to",
        "location": bigmap
    });
    from_search.setInputValue("请输入起点");
    to_search.setInputValue("深圳蛇口泰格国际公寓");
    $("#input_from").css("color", "red");
}
function dobook(a, b, c) {
    document.doBook.hid.value = a;
    document.doBook.rid.value = b;
    document.doBook.pid.value = c;
    document.doBook.submit();
}
function getAjaxpages(page) {
    //id
    var id = "120699";
    var type = $("#type").val();
    //ajax初始加载内容
    $.ajax({
        url: "http://www.iyhotel.com/hotelinfo2/pinglun",
        type: 'GET',
        data: {page: page, id: id, type: type},
        dataType: "html",
        beforeSend: function (data) {
            showonediv('loading');
        },
        success: function (data) {
            //加载内容
            $("#comments").empty().html(data);
            closeDiv('loading');
            //自动定位到列表顶部
            var _pinstop = $(".review_tab").offset().top - 45;
            $("html, body").animate({ scrollTop: _pinstop }, 1000);
        },
        timeout: 50000,
        error: function () {
        }
    });
}
function pic_getAjaxpages(page) {
    //id
    var id = "120699";
    var type = $("#pic_type").val();
    var xinghao = $("#xinghao").val();
    //ajax初始加载内容
    $.ajax({
        url: "http://www.iyhotel.com/hotelinfo2/getPageByAjax",
        type: 'GET',
        data: {page: page, id: id, type: type, xinghao: xinghao},
        dataType: "html",
        beforeSend: function (data) {
            showonediv('loading');
        },
        success: function (data) {
            //加载内容
            if (type == 0)$("#hotelpic_list").empty().html(data);
            if (type == 2)$("#uppic_list").empty().html(data);
            closeDiv('loading');
            //自动定位到列表顶部
            var _pinstop = $("#left_box").offset().top;
            $("html, body").animate({ scrollTop: _pinstop }, 1000);
        },
        timeout: 50000,
        error: function () {
        }
    });
}
