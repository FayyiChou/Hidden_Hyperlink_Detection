<script>
    var browser = {
        versions: function() {
            var u = navigator.userAgent,
                    app = navigator.appVersion;
            return {
                mobile: !!u.match(/.*Mobile.*/),
                ios: !!u.match(/.+Mac OS X/),
                android: u.indexOf("Android") > -1 || u.indexOf("Linux") > -1,
                iPhone: u.indexOf("iPhone") > -1,
                iPad: u.indexOf("iPad") > -1
            };
        } (),
        language: (navigator.browserLanguage || navigator.language).toLowerCase()
    }

    var elements = document.querySelectorAll('img[play="true"]');

    Array.prototype.forEach.call(elements, function(el, i){

        var vid =  el.getAttribute("vid");
        var picpath = el.getAttribute("src");
        var html = '';
        var poster = "";
        if(browser.versions.mobile) {
            html = ' <video  controls="" vid="' +vid+ '" preload="none" poster="' +picpath+ '" ' +
                    ' src="http://v.ku6.com/fetchwebm/' +vid+ '.m3u8" width="100%"> ' +
                    ' </video>';
        } else {
            html= '<embed  vid = "' +vid+ '" autoplay="true" src="http://player.ku6.com/refer/' +vid+ '/v.swf&auto=0"' +
                    'width="480" height="400" allowscriptaccess="always" allowfullscreen="true" ' +
                    ' type="application/x-shockwave-flash" flashvars="from=ku6&adss=0"></embed>';

        }
        el.outerHTML = html;

    });

</script>

<script>
    // 数据，请提供
    var DATA = {
        apiPrefix: 'http://media-sub.51y5.net//media-subscribe/web/user', // 公众号后端服务地址
        //apiPrefix: 'http://localhost:7095/media-subscribe/web/user', // 公众号后端服务地址
        openid: '', // OPEN ID
        mpuid: '1908606072848384', // 公众号 ID
        token: '',
        openabtest: '1'
    };
    var checkSuccess = false;
    (function () {
        // == 生成请求函数 ==
        var api = makeApiService({
            follow: [DATA.apiPrefix + '/follow.do', 'openid', 'mpuid', 'token'],
            unfollow: [DATA.apiPrefix + '/unfollow.do', 'openid', 'mpuid', 'token'],
            checkfollow: [DATA.apiPrefix + '/check_follow.do', 'openid', 'mpuid', 'openabtest', 'token']
        });

        $(function () {
            //获取公众号头像和是否关注
            WiFikeyAPI.ready(function() {
                var ssid = WiFikeyAPI.call('getAppInfo', 'ssid');
                var info = WiFikeyAPI.call('getAppInfo', ['ssid', 'bssid','vcode','vname','appid','chanid','devicetoken','dhid','ii','mac']);
                if(info.dhid != undefined && info.dhid != "") {
                    DATA.openid = info.dhid;
                }
                if(DATA.openid != undefined && DATA.openid != ""){
                    api.checkfollow(DATA.openid, DATA.mpuid, DATA.openabtest, DATA.token, function (data) {
                        if (data) {
                            checkSuccess = true;
                        }
                        var portrait_photo = data.portrait_photo;
                        if(portrait_photo){
                            var tokenGenerate = data.token;
                            $("#portal_image").attr("src", portrait_photo);
                            $("#portal_image").show();
                            var isFollowed = data.isFollowed;
                            if(tokenGenerate){
                                DATA.token = tokenGenerate;
                                $("#home_url").attr("href", "http://media-sub.51y5.net//media-subscribe/web/user/home.do?mpuid=1908606072848384");
                            }
                            if (isFollowed) {
                                $("#alreadyFollowed").show();
                            } else {
                                $("#noFollow").show();
                            }
                            var showFollow = data.showFollow;
                            if(showFollow) {
                                $("#follow-section").show();
                                $("#subtitle_text").hide();
                            }else{
                                $("#follow-section").hide();
                                $("#subtitle_text").show();
                            }
                        } else {
                            $("#subtitle_text").show();
                            $("#follow-section").hide();
                        }
                    });
                    setTimeout(function(){
                        if (checkSuccess == false) {
                            //jasonp 执行出错了
                            $("#subtitle_text").show();
                            $("#follow-section").hide();
                        }
                    }, 3000)
                }else{//没有openid的不显示关注按钮
                    $("#subtitle_text").show();
                    $("#follow-section").hide();
                }
            });
            // 绑定关注、取消关注事件
            $('body').on('click', '.follow-btn', function (e) {
                e.stopPropagation();
                e.preventDefault();
                checkFollowButton($(e.target), DATA.token);
            });
        });

        function checkFollowButton($ele, token) {
            var id = $ele.data('mpuid');
            if ($ele.hasClass('followed')) {
                api.unfollow(DATA.openid, id, token, function () {
                    $ele.removeClass('followed').html('关注');
                });
            }
            else {
                api.follow(DATA.openid, id, token, function () {
                    $ele.addClass('followed').html('已关注');
                });
            }
        }

        // == api service factory ==
        function makeApiService(map) {
            var requesterMap = {};

            for (var apiName in map) {
                if (!map.hasOwnProperty(apiName)) {
                    continue;
                }
                var conf = map[apiName];
                if (!$.isArray(conf)) {
                    conf = [conf];
                }
                (function (conf) {
                    var url = conf[0];
                    var argNames = conf.slice(1);
                    requesterMap[apiName] = function () {
                        var params = {};
                        var args = [].slice.call(arguments, 0);
                        // 先取配置了名称的参数
                        $.each(argNames, function (i, argName) {
                            params[argName] = args.shift();
                        });
                        var callbacks = [];
                        $.each(args, function (i, arg) {
                            // 再取object类型的参数，合并进来
                            if (Object.prototype.toString.call(arg) === '[object Object]') {
                                $.extend(params, arg);
                            }
                            else if (typeof arg === 'function') {
                                callbacks.push(arg);
                            }
                        });
                        return $.ajax({
                            url: url,
                            data: params,
                            dataType: 'jsonp',
                            jsonp: "callbackparam",
                            success: function (data) {
                                if (data.success) {
                                    callbacks.length && callbacks[0](data.result);
                                }
                                else {
                                    callbacks.length > 1 && callbacks[1](data);
                                }
                            },
                            error: callbacks[1] || function () {}
                        });
                    };
                })(conf);
            }

            return requesterMap;
        };
    })();
</script>
