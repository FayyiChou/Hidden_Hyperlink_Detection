<script language="javascript" type="text/javascript">
//广告代码初始化
document.write("<div id='ad_right_lsw_gb' style='display:;'></div>")
ad_right_lsw_gb.innerHTML = "<div id='ad_right_lsw_gb_tcc' style='hidden;position:absolute;top: 0px;left:0px;z-index: 1;width: 0px;height:0px;'><table border='0' cellpadding='0' cellspacing='0'><tr><td valign='bottom' ID='ad_right_lsw_gb_tcc_str'></td></tr></table></div>";


//广告DIV位置定位
function ad_right_lsw_gbHtml_X_Y()
{
	
	
	var top = document.documentElement.scrollTop+document.body.scrollTop;
	var left = document.documentElement.scrollLeft+document.body.scrollLeft;
	var height = document.body.clientHeight;
	var width = document.body.clientWidth;
	
	
	ad_right_lsw_gb_tcc.style.top = ((50+top)) +"px";
	ad_right_lsw_gb_tcc.style.left = (((width+left)-ad_right_lsw_gb_tcc_str.scrollWidth)-0) +"px";
	//ad_right_lsw_gb_tcc.style.left = (((width+left)-170)-2) +"px";
}
//判断页页是否滚动
//function window.onscroll(){ad_right_lsw_gbHtml_X_Y();}
//function document.onscroll(){ad_right_lsw_gbHtml_X_Y();}

document.onscroll = function ()
{
	ad_right_lsw_gbHtml_X_Y();
}
window.onscroll = function ()
{
	ad_right_lsw_gbHtml_X_Y();
}

//判断窗口大小是否有改变
//function window.onresize(){ad_right_lsw_gbHtml_X_Y();}


//加入广告代码
function ad_right_lsw_gbHtml_Add(str)
{
	ad_right_lsw_gb_tcc_str.innerHTML = str;
	
	ad_right_lsw_gbHtml_X_Y();
}
</script>
<script language="javascript" type="text/javascript">
function indexadgd()
{
	ad_right_lsw_gbHtml_Add(ad1.innerHTML)//添加广告调用方法
}
//setTimeout("indexadgd()",1000)
indexadgd()
</script>

