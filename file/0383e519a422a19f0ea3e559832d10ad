    <script language="javascript" type="text/javascript"> 
            var applicationPath = "";
            var skinPath = "/templates/master/default";
            var subsiteuserId = "0";
            var HasWapRight = false;
            var IsOpenStores = true;
        </script><script language="javascript" type="text/javascript">window.onload=function(){getElementsByClassName("lijsbg3",document.body,"li")[0].style.display='none';}</script>
