function changeCity(val,cid){
	if(val=="999"){
		document.getElementById(cid).innerHTML = "<input type=\"text\" name=\"city2\" class=\"city2\" size=\"8\" > ";
	}
	else{
		document.getElementById(cid).innerHTML = "";
	}
}
