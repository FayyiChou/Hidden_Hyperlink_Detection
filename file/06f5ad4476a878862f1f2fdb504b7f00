<script>
    (function(){
        function reHeight(){
            var winHeight = window.innerHeight,
            header = document.querySelector('.public-header'),
            wrap = document.querySelector('.box'),
            bread = document.querySelector('.public-breadcrumb-bottom'),
            footer = document.querySelector('footer');

            var eleAry = [header, bread, footer];
            var r = 0;
            eleAry.forEach(function(val){
                if (val !== null) {
                    r = r + val.offsetHeight;
                }
            });

            if (wrap !== null) {
                if (r + wrap.offsetHeight < winHeight) {
                    wrap.style.minHeight = (winHeight - r) + 'px';
                }
            }
        }
        reHeight();
    })()
</script>
<script src="//src.focus.cn/static_files/wap/mobile/recreate/old/common/lib/zepto-1.1.3.min_aa2a985f609ec279a87916b520966ab9.js"></script>
<script>
    $(function(){
        if(window.localStorage){
            var pftSign = localStorage.getItem('pftSign');
            if(pftSign !== null){
                var r = JSON.parse(localStorage.getItem('pftSign'));
                if(!Array.isArray(r)) return;

                var items = $('.pft-item');
                for(var i = 0, len = r.length; i < len; i++){
                    var _item = items.filter('[data-line-id="' + r[i] + '"]').find('.box1_button_right');
                    _item.addClass('complete').html('已报名').attr('href', 'javascript:;');
                }
            }
        }
    })
</script>

