<script type="text/javascript">
function findValue(li) {
  if( li == null ) { return alert("No match!"); }
  if( !!li.extra ) {
	 var sValue = li.extra[0];
  } else {
	 var sValue = li.selectValue;
  }
}
function selectItem(li) {
	findValue(li);
}
function lookupAjax(){
	var oSuggest = $("#formSearchInput")[0].autocompleter;
	oSuggest.findValue();
	return false;
}
$(document).ready(function(){
  $("#formSearchInput").autocomplete("/ajax/search.inc.php", {
   	delay:500,
  	minChars:3,
	matchSubset:1,
	matchContains:1,
	cacheLength:10,
	onItemSelect:selectItem,
	onFindValue:findValue,
	autoFill:false,
	resultsClass:"AddArticle",
	lineSeparator:"\n",
	width:328
  });
});
</script>
	
</form>
