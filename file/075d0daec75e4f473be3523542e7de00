<script type="text/javascript">
	var editor;
	KindEditor.ready(function(K) {
		editor = K.create('textarea[name="publishContent"]', {
			resizeType : 1,
			allowPreviewEmoticons : false,
			allowImageUpload : false,

			minWidth : 500,
			
			items:[
			'formatblock', 'fontname', 'fontsize', '|', 'forecolor', 'hilitecolor', 'bold',
	        'italic', 'underline', 'strikethrough', 'lineheight', 'removeformat', '|',
			'justifyleft', 'justifycenter', 'justifyright',
			        'justifyfull', 'insertorderedlist', 'insertunorderedlist', 'indent', 'outdent', 'subscript',
			        'superscript', 
			        '|', 'pupload_video',  'table', 'hr',  
			        'link', 'unlink', 'source',,'|','emoticons', 'pupload_image', 'pupload_flash','pupload_word'
			],
			
			afterCreate : function() {
				this.sync();
			},
			afterBlur : function() {
				this.sync();
			},
			afterChange : function(){
				//统计字数
				var currentWordsSize=this.count('text');//不包括html样式
	            $("#wordcount").html("您还可以输入<strong>"+(3000-currentWordsSize) + '/' +3000+"</strong>个字 ");
				if(currentWordsSize>3000){//超过字数限制自动截取会出现问题  此处参照淘宝评价处理
	            	$("#wordcount").html("已经超出<span style='color: red' id='toLong'>"+(currentWordsSize-3000)+"</span>个字 ");
					//$("div.V-fb a").attr("disabled","disabled" );
				}//else{
				//	$("div.V-fb a").removeAttr("disabled");
				//}
			}
		});

	});
</script>
