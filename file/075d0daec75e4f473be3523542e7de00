<script type="text/javascript">

jQuery(document).ready(function(){
 	jQuery("form").validationEngine();
 });
var submitTimes=0;
$('.V-fb').children().click(function(){
	var textAreaSize=$("#toLong").text();
	var areaSize=$.trim($("#wordcount strong").text());
	if(textAreaSize!=""){
		$(this).validationEngine("showPrompt","您输入的文本内容已超出"+textAreaSize+"字!","error");
		setTimeout(function(){$(this).validationEngine('hideAll');},3000);
		return;
	}
	if((3000-areaSize.split("/")[0])<10){
		if(($("#publishContent").text().indexOf("<img")<0)&&($("#publishContent").text().indexOf("<embed")<0)){
			$(this).validationEngine("showPrompt","您的发帖内容太少！(不能少于10字)","error");
			setTimeout(function(){$(this).validationEngine('hideAll');},3000);
			return;
		}
	}
 	if($("form").validationEngine("validate")){
 		if((submitTimes++)<=0){
 			$("#form1").attr("action","/topic/save").submit();
			$(this).attr("disabled","disabled");
 		}
 	}
 	
 });
$(function(){
	 limitWords.countWords_title();
	 var isLogin=false;
	if(isLogin==true){
		$("#_finshLogin").show();
		$("#_waitLogin").hide();
	} 

});
function selectAll(obj){
	if($(obj).attr("checked") == "checked"){
		$("input[name='ids']").attr("checked",true);
		$("#selectName").text("不选");
	}else{
		$("input[name='ids']").attr("checked",false);
		$("#selectName").text("全选");
	}
     
	
}

function manage(type){
	var ids = getIds();
	if(ids == null || ids ==""){
		return;
	}
	var number = $("input[name='ids']:checked").length;
	openDialog(type,ids,number);
}
function single(type,id){
	openDialog(type,id,1);
}

function getIds(){
	var ids = "";
	$("input[name='ids']:checked").each(function() {
		ids = ids + this.value+",";
    });
	if(ids == ""){
		alert("您还没有选中任何贴子");
	}else{
		return ids;
	}
	
}

function openDialog(type,ids,number){	
	$.ajax({
		type:"post",
		url:"../openDialog",
		data:"operateType=0&ids="+ids+"&type="+type+"&number="+number+"&columnId="+852+"&pageNo="+1+"&date="+new Date(),
		success:function(data){
			In.ready("dialog",function(){
				var d=tp_artDialog({
					padding: 0,
				    content: data
				})
				d.showModal();
				
			})
		}
	});
}
var limitWords = function() {
	return {
		countWords_title : function() {
			jQuery.fn.extend({ 
				showWordCount_title: function() { 
					var _max = $(this).attr('max');
					var _length = $(this).val().length;
					if(_length > _max) {
						$(this).val($(this).val().substring(0, _max));
						var _length = $(this).val().length;
					}
					$("#title_tip").html("您还可以输入<strong>"+(_max-_length) + '/' + _max+"</strong>字 ");
				} 
			});
			$("#title").focus(function(){
				if($(this).val()=="您还可以输入<strong>30/30</strong>字"){
					$(this).val("");
				}else{
					$(this).showWordCount_title();
				}
			}).blur(function(){  
			$(this).showWordCount_title();
			}).keydown(function(){  
			$(this).showWordCount_title();
			}).click(function(){  
			$(this).showWordCount_title();
			}).bind("propertychange",function(){
				$(this).showWordCount_title();
			}).bind("paste",function(){
				$(this).showWordCount_title();
			})
			
		},
		empty : null
	}
}();	
function showDelDialog(){
	var number = $("input[name='ids']:checked").length;
	if(number <= 0){
		alert("您还没有选中任何贴子");
		return;
	}
	
	In.ready("dialog",function(){
		var d=tp_artDialog({
			padding: 0,
			content:'<div class="delect-box"><div class="delect-box-title"><span>选择了<strong>'+number+'</strong>篇帖子</span><i i="close"></i></div><div color="#333333"><div class="delect-box-con"><div class="delect-box-cons">'
			+'<div class="delect-sure">您确定<span>删除</span>这'+number+'篇主题吗？删除后不可恢复！</div>'
			+'<div class="delect-fb"><a href="javascript:delTopic()">确定</a></div></div></div></div>'
		    
		})
		d.showModal();
	})
	

	
}
function delTopic(){
	var ids = getIds();
	if(ids == null || ids ==""){
		return;
	}
	window.location.href = "../delTopic?ids="+ids+"&columnId="+852+"&pageNo="+1+"&date="+new Date();
}

//帖子移动
function showMoveDialog(){
	var ids = getIds();
	if(ids == null || ids ==""){
		return;
	}
	var number = $("input[name='ids']:checked").length;
	//获取主题帖的主题分类和分区栏目路径
	var idArray = $("input[name='ids']");
	var classifyIdArray = $("input[name='classifyId']");
	var levelPathArray = $("input[name='levelPaths']");
	var classifyId="";
	var levelPath ="";
	for(var i =0;i<idArray.length;i++){
		if(idArray[i].checked){
		  classifyId =classifyIdArray[i].value;
		  levelPath = levelPathArray[i].value;
		  break;
		}
	}
	
	$.ajax({
		type:"post",
		url:"../openMoveDialog",
		data:"type=0&ids="+ids+"&classifyId="+classifyId+"&levelPath="+levelPath+"&number="+number+"&dicId=7&columnId="+852+"&pageNo="+1+"&date="+new Date(),
		success:function(data){
			In.ready("dialog",function(){
				var d=tp_artDialog({
					padding: 0,
				    content: data
				})
				d.showModal();
				
			})
		}
	});
	

	
}
</script>

