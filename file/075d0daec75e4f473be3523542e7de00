<script>
$(document).ready(function(){
	$(document).on('click', '.delAttachment', function() {
		if (confirm('确定删除吗?')) {
			var thiss= $(this);
			var attId = thiss.attr("data-id");
			$.ajax({
			type:'post',//可选get
/* 			url:'/home/attachment/attDelete',//URL
 */			url:'/ajax/attachment/attDelete',//URL
			data:'attId='+attId,//参数用&连接
			//dataType:'text',//服务器返回的数据类型 
			success:function(msg){
				
				var dt=thiss.parent().prev();
				dt.remove()
				thiss.parent().remove();
				if(uploadCount>0)
					uploadCount--;
			}
		})

		} else {
			return false;
		}
	});
})
</script>
