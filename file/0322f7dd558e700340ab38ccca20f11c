$('#zhuanjiaScreening').on("change" , function(){
    var area_id = $(this).val() ;
    var ill_id  =$('#zhuanjiaScreening').attr("accesskey");
    var illid = 3365;
    if(!ill_id) {
        return false ;
    }
    $.ajax({
          type: "GET",
          data: {"area_id" : area_id , "ill_id" :  ill_id , "illid" : illid} ,
          url:  "http://3g.sgjb.xyxy.net/index/zhuanjia" ,
          success : function (data) {
             if(data !== "error" ) {
                 $("#zhuanjiaCont").empty().html(data) ;
             }
          }
     }) ;
 }) ;
</script>                                    <div class="" style="background: #f8f8f8;padding: 15px 0px;border-top: 1px #d6d6d6 solid;border-bottom: 1px #d6d6d6 solid;margin-top: 20px;margin-bottom: 20px;">
