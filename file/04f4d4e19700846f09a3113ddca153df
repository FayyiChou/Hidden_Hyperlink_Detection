							<script language="javascript" type="text/javascript">
								jQuery(document).ready(function() {

									$('#countdown_dashboard').countDown({
										targetDate : {
										},
										omitWeeks : true
									});
									var countdown_dashboard_1 = $("#countdown_dashboard_1");
									var countdown_dashboard_2 = $("#countdown_dashboard_2");
									set_by_date("2016-06-12".replace(/-/g, "/"));
									countdown_dashboard_1.addClass("time_t_bj");

									countdown_dashboard_1.bind("mouseover", function() {
										countdown_dashboard_2.removeClass("time_t_bj");
										$(this).addClass("time_t_bj");
										set_by_date("2016-06-12".replace(/-/g, "/"));

									});
									countdown_dashboard_2.bind("mouseover", function() {
										countdown_dashboard_1.removeClass("time_t_bj");
										$(this).addClass("time_t_bj");
										set_by_date("2016-06-07".replace(/-/g, "/"));
									});
								});
								function set_by_date(num) {
									d = new Date(Date.parse(num));
									//  获取当前日期
									//alert(d.getFullYear()+"年"+(d.getMonth()+1)+"月"+d.getDate());

									$('#countdown_dashboard').stopCountDown();
									$('#countdown_dashboard').setCountDown({
										targetDate : {
											'day' : d.getDate(),
											'month' : d.getMonth() + 1,
											'year' : d.getFullYear(),
											'hour' : 0,
											'min' : 0,
											'sec' : 0
										}
									});
