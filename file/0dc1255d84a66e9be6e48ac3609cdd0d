<script language =javascript >
    function uid2_onkeypress(e)
    {
        if(e.keyCode==13)
        {
            document.frmmain2.pwd2.focus(); 
            if(document.frmmain2.pwd2.value=="密碼：")
            {
                document.frmmain2.pwd2.value="";
            }
        }    
    
    }
    function pwd2_onkeypress(e)
    {
        if(e.keyCode==13)
        {
            tologin2();
        }    
    
    }

    function tologin2()
    {
        var f=document.frmmain2;
        f.eventvalue2.value="tologin";
        f.submit(); 
    
    
    }
    
</script> 
