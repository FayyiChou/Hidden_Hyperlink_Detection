    <script type="text/javascript">

 //更换验证码
 function reloadcode() {
        document.getElementById("yzm").src = "yanzhengma.aspx?id=" + Math.random();
    }  //模式窗口打开
    function Showwindow(url, width, heigh) {
        window.showModalDialog(url, '', 'dialogHeight:' + heigh + 'px;dialogWidth:' + width + 'px;status:no;scroll:no');
    }
    //验证输入
    function check() {
        if (document.getElementById("loginname").value == "") {
            document.getElementById("loginname").focus();
            document.getElementById("ShowMessage").innerHTML = "请输入用户名!";
            return false;
        }
        if (document.getElementById("loginname").value.length >= 30) {
            document.getElementById("loginname").value = "";
            document.getElementById("loginname").focus();
            document.getElementById("ShowMessage").innerHTML = "输入的用户名过长，请重新输入!";
            return false;
        }
        if (document.getElementById("loginpwd").value == "") {
            document.getElementById("loginpwd").focus();
            document.getElementById("ShowMessage").innerHTML = "请输入密码!";
            return false;
        }
        if (document.getElementById("txt_validcode").value == "") {
            document.getElementById("txt_validcode").focus();
            document.getElementById("ShowMessage").innerHTML = "请输入验证码!";
            return false;
        }
       
    }
    </script>

