var speed=40;

var FGDemo11=document.getElementById('doctor');

var FGDemo12=document.getElementById('doctor1');

var FGDemo23=document.getElementById('doctor2');

FGDemo23.innerHTML=FGDemo11.innerHTML

function Marquee1(){

if(FGDemo23.offsetHeight-FGDemo11.scrollTop<=0)

FGDemo11.scrollTop-=FGDemo12.offsetHeight

else{

FGDemo11.scrollTop++

}

}

var MyMar1=setInterval(Marquee1,speed)

FGDemo11.onmouseover=function() {clearInterval(MyMar1)}

FGDemo11.onmouseout=function() {MyMar1=setInterval(Marquee1,speed)}

