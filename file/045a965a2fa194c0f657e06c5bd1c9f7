<script type="text/javascript">
$(window).keydown(function(event){//键盘方向键控制
switch (event.keyCode) {
case 13: //enter
$(".next").click();
break;
case 37: //prev arrow
$(".prev").click();
break;
case 39: //next arrow
$(".next").click();
break;
}
});	
// 百度分享
var JartShare = $("#JartShare .do-share");
var timer = null;
 JartShare.bind("mouseover",function(){
 	clearTimeout(timer)
		$(".bdsharebuttonbox").show();
		JartShare.css("color","#ff4444");
		JartShare.addClass("do");
});
JartShare.bind("mouseout",function(){
	timer = setTimeout(function(){
		$(".bdsharebuttonbox").hide();
		JartShare.css("color","#333");
		JartShare.removeClass("do");
	},100);
})
</script>
