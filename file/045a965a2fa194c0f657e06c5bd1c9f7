$(document).keydown(function(event){
	if(event.keyCode == 37){
		$("#J-image .abtn-l").get(0).click();
	}else if (event.keyCode == 39){
		$("#J-image .abtn-r").get(0).click();
	}
});
/*计算图片宽高*/
new Image().src='http://i5.3conline.com/images/piclib/201208/21/batch/1/144991/1345563843542cn8e5qj50l_medium.jpg';
 /*计算图片宽高*/
(function setImgMargin(){
	var img = new Image,w,h,Timer;
    img.src = 'http://i5.3conline.com/images/piclib/201208/21/batch/1/144991/1345563843542cn8e5qj50l_medium.jpg';
    if(img.complete || img.width || img.height){
        // 缓存情况
        callBack(img);
    }else{
        w = img.width;
        h = img.height;
        Timer = setInterval(function(){
            var _w = img.width,
                _h = img.height;
            if(_w != w && _h != h){
               callBack(img);
               clearInterval(Timer);
            }
        },40)
   }
	function callBack(obj){
		maxAttr(obj);
		var $imgWidth=$("#imgBox .img").width();
		var $imgHeight=$("#imgBox .img").height();
		if($imgHeight<304){
			$('.main-area .abtn').css('height',304)
			$("#imgBox .img").css({"position":"absolute","margin-top":-$imgHeight/2,"margin-left":-$imgWidth/2,"top":"50%","left":"50%"}).show();
			$('.cor-des').css('height',204)
		}else{
			$('.main-area .abtn').css('height',$imgHeight);
			$('.cor-des').css('height',$imgHeight-132);
			$("#imgBox .img").attr("style",'')
		}
	}
	function maxAttr(obj){
		var imgW=obj.width;
		var imgH=obj.height;
		var sjW=$(document).width()-700
		if(imgW>=sjW){
			$("#imgBox img").attr("width",sjW);
		}else{
			$("#imgBox img").attr("width",imgW);
		}
	}
	window.setImgMargin=setImgMargin;
})();
var aa=new Slidenew("slide");

</script>

