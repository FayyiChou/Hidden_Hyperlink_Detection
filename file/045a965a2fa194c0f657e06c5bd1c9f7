					                var title = data[i].name;
					                var url = "http://pp.pcbaby.com.cn/"+data[i].url ;
					                var li = "<li><a href='" + url + "' hidefocus='true' target='_blank'> 	<img width='100%' src='" + cover + "' alt='" + title + "'> <span>"+title +"</span></a></li> ";
					                html = html + li;
					                num++;
	            			}
	            		 
	           
	            }
	            ul_data.innerHTML = html+ul_data.innerHTML;
	        }
		    </script>
		    <script src="http://pp.pcbaby.com.cn/latest_json/57.html"></script>
		   
        </div>
        <!-- /td -->
