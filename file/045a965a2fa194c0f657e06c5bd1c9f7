	<script>
	//最后图片出推荐广告
	function nexts() {
		var bol = true;
		var bol2 = true;
		var oldurl = $(".abtn-r")[0].href;
		var oldurl2 = $(".abtn-l")[0].href; 
		var oldimgsrc = $("#imgBox img")[0].src;
		$(".abtn-r")[0].href = "javascript:void(0)";

		$(".abtn-r").click(function(){
			if (bol) {
			$(".abtn-l")[0].href = "javascript:void(0)";
			$("#imgBox img")[0].src = "http://www1.pcbaby.com.cn/add/ad/2015/images/pp.jpg";
			$("#imgBox img")[0].alt = "扫一扫，把我装进口袋里哟！";
			$("#imgBox a")[0].href = "http://app.pcbaby.com.cn/phone/";
				bol = false;
				bol2 = true;
			}else{
				$(".abtn-r")[0].href  = oldurl;
			}
		})

		$(".abtn-l").click(function(){

			if (bol2) {
				$("#imgBox img")[0].src = oldimgsrc;
				bol = true;
				bol2 = false;
			}else
			{
				$(".abtn-l")[0].href = oldurl2;
			}

		})
	}
	//是否最后一张
	if ('0' == 1) {
		nexts();
	}
	</script>
