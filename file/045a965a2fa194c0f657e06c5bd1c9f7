<script>
	var id="20593";
	var iscollect=0;
	var headLines="5";
	var tag="手抄报,法制手抄报,小学生法制手抄报,法制手抄报资料,法制手抄报内容,法制手抄报图片,法制手抄报的内容";
	function checkCollect(){
		var common_session_id=getCookie("common_session_id");
		if(""==common_session_id||null==common_session_id){
			$('#'+id).html('<i class="favor"></i>收藏');
			return;
		}else{
			$.get("http://my.pcbaby.com.cn/collection/intf/collection/check_collected.jsp?type=8&id="+id+"&common_session_id="+common_session_id,function(data){eval('result='+data);if(result.code==1){$('#'+id).addClass('favor-after');iscollect=1;$('#'+id).html('<i class="favor"></i>已收藏');}});
		}
		
	}
	$(document).ready(function() {
		checkCollect();
		if(headLines=="0"){
			$(".expTxtC-list").hide();
		}
		if(tag==""||tag==null){
			$(".mark-tit").hide();
		}
	});

	function addToFavorite(obj) {
		var common_session_id=getCookie("common_session_id");
		if(""==common_session_id||null==common_session_id){
			openDialog1();
			return;
		}
		if(iscollect==0){
			$.post("http://my.pcbaby.com.cn/collection/intf/collection/collect.jsp?type=8&id="+id+"&common_session_id="+common_session_id,function(data){eval('result='+data);if(result.code>=0){$(obj).addClass('favor-after');iscollect=1;$(obj).html('<i class="favor"></i>已收藏');}else{alert("收藏失败！");}});
		}
		
	}
	function getCookie(name) {
		var arr = document.cookie.match(new RegExp("(^| )" + name
				+ "=([^;]*)(;|$)"));
		if (arr != null)
			return unescape(arr[2]);
		return null;
	}
	function checkLogin(){
		var username=$("#username").val();
		var pwd=$("#pwd").val();
		if(username==""||username==null||pwd==""||username==null){
			$(".dl-Form .dl-emTips").css("display","block");
			return false;
		}else{
			$("#lgForm").submit();
		}	
	}
