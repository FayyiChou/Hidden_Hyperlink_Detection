$(function(){
  $('.search_btn').on('click', function() {
          $(".search").slideToggle("slow");
        });
  $('.l_tbn').on('click', function() {
          $("#menu").slideToggle("slow");
          if($("#menu").is(':visible')) {
            $("#menu").data('showed', true);
          } else {
            $("#menu").data('showed', false);
          }
        });
});
