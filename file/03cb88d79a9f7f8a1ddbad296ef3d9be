<script type="text/javascript">
$(function(){
	var timeID;
	$("li.has-drop-down").mouseover(function(){	
		try
		{
			clearTimeout(timeID);
		}
		catch(e)
		{
			
		}
		var obj=$(this);
		timeID=setTimeout(function(){
			$("li.has-drop-down").removeClass('hover');
			obj.addClass('hover');
		},500);
		
	});
	
	$("li.has-drop-down").mouseout(function(){	
		try
		{
			clearTimeout(timeID);
		}
		catch(e)
		{
			
		}
		var obj=$(this);
		timeID=setTimeout(function(){
			obj.removeClass('hover');
		},1000);
	});
	
	$("li[imgSrc]").mouseover(function(){
		$('#visual-holder-img').attr('src',$(this).attr("imgSrc"));
		$('#visual-holder-description .title').html($(this).attr("dec"));
		$('#visual-holder-description .button').attr('href',$(this).find('a').attr("href"));
	});
});
</script>
