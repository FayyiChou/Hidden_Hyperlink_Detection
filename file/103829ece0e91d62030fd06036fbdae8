     <script>$(function() {
        var num;
        var ishover = false;
        //设置ul.erji的宽度
        $('.erji').each(function(index) {
          num = $(this).find('li').length;

          $(this).css("width", 100 * num);

          if (index == 0 || index == 1) {

            $(this).css("left", -80);

          }  else if (index == 2) {

            $(this).css("left", -200);

          } else if (index == 3) {

            $(this).css("left", -60);

          } else if (index == 4) {

            $(this).css("left", -50);

          } else if (index == 5) {

            $(this).css("left", -160);

          }

        });

        //鼠标经过效果

        $('.yiji').hover(function() {

          $('.erji').hide();

          if ($('.xiala').css("display") == "none") {

            $('.xiala').slideDown(500);

            $(this).find('.erji').show(500);

          } else {

            $(this).find('.erji').show();

          }

          $(this).addClass('hover');

          ishover = true;
        },

        function() {

          if (ishover === false) {

            $('.xiala').slideUp(500);

          }

          $('.erji').hide(500);

          $(this).removeClass('hover');

        });

        $('.daohang').hover(function() {

          ishover = true;

        },

        function() {

          ishover = false;

          $('.xiala').slideUp(500);

        });

      });</script>




