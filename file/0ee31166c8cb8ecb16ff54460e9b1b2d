        var  $oTal = $('table');
        var  $oLi  = $('.price-main li');
        var  $arr  = [];
        var  $showBtn = $('.show-btn');
        var  $offOn=true;
  
 
        for (var i = 0; i < $oTal.length; i++) {
               $arr.push($oTal[i]);
            }
     function getTal () { 
        for (var i = 0; i < $arr.length; i++) {
            if (i<2 && i%2==0) {
              $oLi.eq(0).append($arr[i]);
              $arr[i].style.display='table';
            }else if(i<2){
              $oLi.eq(1).append($arr[i]);
              $arr[i].style.display='table';
            }
          }               
        };

        
        getTal();

        $showBtn.click(function () {
              if ($offOn) {
                for (var i = 0; i < $arr.length; i++) {
                  if (i%2 == 0) {
                      $oLi.eq(0).append($arr[i]);
                      $arr[i].style.display='table';
                  }else{
                      $oLi.eq(1).append($arr[i]);
                      $arr[i].style.display='table';
                  }
                }
                this.style.background='url(/img/newhome/show-btn-bg02.jpg) no-repeat right';
                this.value='收'+' '+'起';
                $offOn = false;
              }else{           
                $oLi.eq(0).find('table').remove();
                $oLi.eq(1).find('table').remove();
                 getTal();
                this.style.background='url(/img/newhome/show-btn-bg.jpg) no-repeat right';
                this.value='查看全部';
                $offOn = true;
              }
         }) 
       

  
        $('button').click(function(event){

          var $twoCode=$('<div id="twocode-pic"><div class="twocode-bg"><img src="/img/newhome/wxgz.jpg" alt="t-pic"></div><div class="sys"></div></div><div class="twocode-wrap" ></div>');
          $('body').append($twoCode);
          
          var $tcodePic=$('#twocode-pic');
          $tcodePic.css('top',($('body').height()-$tcodePic.outerWidth())/2);
          $tcodePic.css('left',($('body').width()-$tcodePic.outerHeight())/2);

          event.stopPropagation();

          $tcodePic.click(function(event){
              event.stopPropagation();
             })
          $('body').click(function(){
             $twoCode.remove();
          })
 
        })

        var $bnrTop=$('.bnr-wrap');
        var $bnr_ul=$('.top-img');
        var $btnTop=$('.top-btn');
        var $btnOa=$btnTop.find('span');
        var $btnCir=$('.banner-circle');

        bnrMove(1,$bnrTop,$bnr_ul,$btnTop,$btnOa,$btnCir)

        var $banner=$('.bottom-pic');
        var $banner_ul=$('.slider');
        var $btn=$('.banner-btn');
        var $btn_a=$btn.find('span')[0];
        var $btn_b=$btn.find('span')[1];
        bnrMove(2,$banner,$banner_ul,$btn,$btn_a)
       


function bnrMove(ind,obj,oUl,oBtn,oA,dom){

        var v_width=obj.width();
        
        var page=1;
        
        var timer=null;
        var btnClass=null;
        
        var handleMove = false;
        var page_count=oUl.find('li').length;//把这个值赋给小圆点的个数
  
       
        var bnrCircle='<li class="selected" ><a></a></li>';
        for (var i = 1; i < page_count; i++) {
             bnrCircle+='<li><a ></a></li>';
        };
        
        if ( dom != null ) {
        dom.append(bnrCircle);
         };

        oUl.width(page_count*v_width);
        
        function move(obj,classname){
        if(ind==2){
      }
         //手动及自动播放
        if(true){
            if(ind==2){
          }
                if(classname=='prevBtn'){
                        if(page==1){
                            oUl.animate({left:-v_width*(page_count-1)});
                            page=page_count;
                            cirMove();
                        }
                        else{
                            oUl.animate({left:'+='+v_width},"slow");
                            page--; 
                            cirMove();                
                        }        
                }
                else{
                        var pre = page
                        if(page==page_count){
                            oUl.animate({left:0});
                            page=1;  
                            cirMove();    
                                }
                        else{
                            page++;
                            oUl.animate({left:'-'+(page-1)*v_width},"slow");
                            cirMove();           
                        }
                }
                        if(ind==2){
                      }
                }
        }
        if(dom){
          $('.banner-circle li').bind('click',function(){

                        var index=$('.banner-circle li').index(this);
                        if(index == 0){
                          page = page_count
                        } else {
                          page = index;
                        }
                        move($(this),this.className);

                })       
        }else {
        
        $($btn_a).mouseover(function(){
            clearInterval(timer);
        }).mouseout(function(){
            timer=setInterval(move,3000);
        }).click(function(event){
          event.stopPropagation();
            handleClick(true)
        })
        $($btn_b).mouseover(function(){
            clearInterval(timer);
        }).mouseout(function(){
            timer=setInterval(move,3000);
        }).click(function(event){
          event.stopPropagation();
          handleClick(false)
        })      
        var handleClick = function(left){
          if(handleMove == true){
            return;
          }
          handleMove = true
          if(left==true){
            if(page==1){
              handleMove = false;
              return
            }
            page -= 2;
            if(page<0){
              page += page_count;
            }
          }else {
            if(page==page_count){
              handleMove = false
              return;
            }
          }
          move($(this),this.className);
          handleMove= false;
        }
                }

        function cirMove(){
        //检测page的值，使当前的page与selected的小圆点一致
        if(dom){
        $('.banner-circle li').eq(page-1).addClass('selected')
        .siblings().removeClass('selected');
      }
        }
        obj.mouseover(function(){
                clearInterval(timer);
                 }).mouseout(function(){
                clearInterval(timer);
                timer=setInterval(move,3000);
                                }).trigger("mouseout");//激活自动播放

        oBtn.mouseover(function(){
                //实现透明渐变，阻止冒泡
                        $(this).animate({opacity:0.6},'fast');
                         return false;
                }).mouseleave(function(){
                        $(this).animate({opacity:1},'fast');
                         return false;
                }).click(function(){
                        //手动点击清除计时器
                        btnClass=this.className;
                        clearInterval(timer);
                        timer=setInterval(move,3000);
                        move($(this),this.className);
                });
           
    }        
});

</script>
