<script language="javascript">
( function($)
{
$.fn.hoverForIE6=function(option)
{
var s=$.extend({current:"hover"},option||{});

$.each(this,function(){
$(this).bind("mouseover",function()
{
$(this).addClass(s.current)
}).bind("mouseleave",function(){
$(this).removeClass(s.current);
})
})
}
})(jQuery);
    
    
    </script>
