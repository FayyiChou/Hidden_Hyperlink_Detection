<script type="text/javascript">
    
    jQuery(document).ready(function($) {
        
        var loc = window.location.href;
        if(loc.indexOf('niioz.ru/napravleniya/')!==-1)
            $('ul#menu-primary>li#menu-item-2671 a').css('color', '#ff5722');
        
        if(loc.indexOf('niioz.ru/publications/')!==-1)
            $('ul#menu-primary>li#menu-item-40 a').css('color', '#ff5722');
        
        if(loc.indexOf('niioz.ru/events/')!==-1)
            $('ul#menu-primary>li#menu-item-2885 a').css('color', '#ff5722');
        
        
        $('ul#menu-primary>li#menu-item-2671').click(function()
        {
            if($('#napr-megamenu').hasClass('open'))
            {
                $('#napr-megamenu').hide().removeClass('open');
                $(this).children('a').css('color', 'white');
            }
            else
            {
                $('#napr-megamenu').show().addClass('open');
                $(this).children('a').css('color', '#ff5722');
            }
        });
        
        $(document).click(function(event) 
        {
            if ($(event.target).closest("#napr-megamenu").length) return;
            if ($(event.target).closest("#menu-item-2671").length) return;
            
            $("#napr-megamenu").hide().removeClass('open');
           if(loc.indexOf('niioz.ru/napravleniya/')==-1)
               $('ul#menu-primary>li#menu-item-2671 a').css('color', 'white');
            event.stopPropagation();
        });
        
        $("#menu-item-2671").mouseenter(function(){
             $("#napr-megamenu").show();
             $('ul#menu-primary>li#menu-item-2671 a').css('color', '#ff5722');
        });
        
        $("#menu-item-2671").mouseleave(function(event){
            if($("#napr-megamenu").hasClass('open')) return;
            if(event.relatedTarget.closest("#napr-megamenu")) return;
            
            $("#napr-megamenu").hide();
            
            var loc = window.location.href;
            if(loc.indexOf('niioz.ru/napravleniya/')==-1)
               $('ul#menu-primary>li#menu-item-2671 a').css('color', 'white');
            event.stopPropagation();
            
        });   
        
        $("#napr-megamenu").mouseleave(function(event)
        {
            if($("#napr-megamenu").hasClass('open')) return;
            if(event.relatedTarget.closest("#menu-item-2671")) return;
            
            $("#napr-megamenu").hide();
            if(loc.indexOf('niioz.ru/napravleniya/')==-1)
               $('ul#menu-primary>li#menu-item-2671 a').css('color', 'white');
             event.stopPropagation();
        });
        
        $(window).scroll(function(){
            $("#napr-megamenu").hide().removeClass('open');
            if(loc.indexOf('niioz.ru/napravleniya/')==-1)
               $('ul#menu-primary>li#menu-item-2671 a').css('color', 'white');
        });   
        
 // обработка общего поиска
        
        $('.mainsearch-img').on('click', function(){
            $(this).hide();
            $('.mainsearch-block').height(80);
            $('#main-srch').show().focus();
            
        });
        
        $("#main-srch").keypress(function(e){
            if(e.keyCode==13){
                var url = "http://www.niioz.ru/search-result/?search="+$('#main-srch').val();
                document.location.href = url;
            }
        });
        
    });


function scrollToDiv(element,navheight){
    var offset = element.offset();
    var offsetTop = offset.top;
    var totalScroll = offsetTop-navheight;
 
    $('body,html').animate({
            scrollTop: totalScroll
    }, 500); 
}

    $(document).ready(function() {

        var oldWidth;
        var oldHeight;

        $("div.resizable-img-block").click(function() {alert('rrr');
	    if($(this).hasClass('bigimg'))
	    {
		$(this).animate({width: oldWidth, height: oldHeight}, 300).removeClass('bigimg');
		$(this).children('img').animate({width: oldWidth, height: oldHeight}, 300);
		$(this).children(".figure_icon").show();
		$(this).children(".close-icon").hide();

	    }else
	    {
	        var parent = $(this);
		oldWidth = parent.width();
		oldHeight = parent.height();
		var new_width = parent.parent().width();
		var new_height = parent.height() * new_width / parent.width();
		$(this).animate({width: new_width, height: new_height}, 300).addClass('bigimg');
		$(this).children('img').animate({width: new_width, height: new_height}, 300);
		$(this).children(".figure_icon").hide();
		$(this).children(".close-icon").show();
		scrollToDiv($(this), $('div.hMenu-Wraper').height()+5);
	    }

        });       
    });
</script>

