    <script>        
        $(function () {
            $('#overlay').splashScreen({
                textLayers: [
                            '//cdn.thefa.com/countyportal/images/blank.gif', //Lower images
                            '//cdn.thefa.com/countyportal/images/blank.gif',
                            '//cdn.thefa.com/countyportal/images/blank.gif'
                        ]
            });
        });

        $(window).load(function () {
            $('.cp-mainSplashImageLink').width($('.cp-mainSplashImageLink img').width() + 10);
            $('#splashScreen, .cp-enterSite-link').click(function () {
                $('#splashScreen').fadeOut('slow');

                //Run the client side commands interupted by the splash overlay
                $('.custom-checkbox-area input').customInput();
                $('.footer-form-fld .error').hide();
                $('#footer-searchBox-sbt').unbind("click").bind('click', checkEmail);

                if ($(this).attr('class') == 'cp-enterSite-link') {
                    return false;
                }
                return true;
            });

            //Stop the splash appearing when user clicks back and reloads from browser cache
            if (getCookie("hidesplash") == "true") { $('#splashScreen').hide(); }
            document.cookie = "hidesplash=true";

        });

        // A self-executing anonymous function,
        (function ($) {

            $.fn.splashScreen = function (settings) {

                // Providing default options:
                settings = $.extend({
                    textLayers: [],
                    textShowTime: 10000,  //Adjust to show splash for different times
                    textTopOffset: 80
                }, settings);

                var promoIMG = this;

                // Creating the splashScreen div.
                // The rest of the styling is in splashscreen.css

                var splashScreen = $('<div>', {
                    id: 'splashScreen',
                    css: {
                        backgroundImage: promoIMG.css('backgroundImage'),
                        backgroundPosition: 'center ' + promoIMG.offset().top + 'px',
                        height: $(document).height()
                    }
                });


                $(splashScreen).append($('#overlay'));                

                $('body').append(splashScreen);

                splashScreen.click(function () {
                    splashScreen.fadeOut('slow');
                });

                // Binding a custom event for changing the current visible text according
                // to the contents of the textLayers array (passed as a parameter)

                splashScreen.bind('changeText', function (e, newID) {

                    // If the image that we want to show is
                    // within the boundaries of the array:

                    if (settings.textLayers[newID]) {
                        showText(newID);
                    }
                    else {
                        splashScreen.click();
                    }
                });

                splashScreen.trigger('changeText', 0);

                // Extracting the functionality into a
                // separate function for convenience.

                function showText(id) {
                    var text = $('<img>', {
                        src: settings.textLayers[id],
                        css: {
                            marginTop: promoIMG.offset().top + settings.textTopOffset
                        }
                    }).hide();

                    text.load(function () {
                        text.fadeIn('slow').delay(settings.textShowTime).fadeOut('slow', function () {
                            text.remove();
                            splashScreen.trigger('changeText', [id + 1]);
                        });
                    });

                    splashScreen.append(text);
                }

                return this;
            };
        })(jQuery);

    function getCookie(c_name) {
        var i, x, y, ARRcookies = document.cookie.split(";");
        for (i = 0; i < ARRcookies.length; i++) {
            x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
            y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
            x = x.replace(/^\s+|\s+$/g, "");
            if (x == c_name) {
                return unescape(y);
            }
        }
    }
</script>
