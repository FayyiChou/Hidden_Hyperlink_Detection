        $(document).ready(function(){
            $(".erorMessage").click(function(){
                $(this).parents(".error").removeClass("error");
                $(this).siblings("input").focus();
            });

            $("#captcha_word").click(function(){
                $('#captcha_word').parent(".inputPopupField").removeClass("error");
                $('#captcha_word').attr('placeholder','');
            });

            $("form[name='SIMPLE_FORM_7']").submit(function(){
                var s = true;
                $(".req",$(this)).each(function(){

                    if($(this).val()){
                        $(this).parent(".inputPopupField").removeClass("error");
                    }else{
                        $(this).parent(".inputPopupField").addClass("error");
                        if ($(this).attr('id') == 'captcha_word')
                        {
                            $(this).attr('placeholder',"Поле заполнено неверно");
                        }
                        s = false;
                    }
                });
                return s;
            });

        });
    </script>

    
