    <script>
    $('#send').click(function (e) {
        e.preventDefault();

        $('#calcPopUp').fadeIn(function () {
            $('#ov1').click(function (e) {
                e.preventDefault();
                $('#calcPopUp').fadeOut();
            });
            $('#popUpClose').click(function (e) {
                e.preventDefault();
                $('#calcPopUp').fadeOut();
            });
        });
    });
	$('#download').click(function (e) {
        e.preventDefault();

        $('#downloadPopUp').fadeIn(function () {
            $('#ov2').click(function (e) {
                e.preventDefault();
                $('#downloadPopUp').fadeOut();
            });
            $('#downloadClose').click(function (e) {
                e.preventDefault();
                $('#downloadPopUp').fadeOut();
            });
        });
    });
