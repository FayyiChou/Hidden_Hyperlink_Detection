		<script type="text/javascript">
		$(document).ready(function() {
			var donorImg = new Image();
			var nonProfitImg = new Image();
			var foundationImg = new Image();
			
			donorImg.src = "https://www.americanphilanthropic.com/wp-content/themes/amp/i/blueDonorOval.png";
			nonProfitImg.src = "https://www.americanphilanthropic.com/wp-content/themes/amp/i/maroonNonProfitOval.png";
			foundationImg.src = "https://www.americanphilanthropic.com/wp-content/themes/amp/i/yellowFoundationOval.png";		
			
			$("#donorArea").mouseover(function() {
			  $("#donorOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/blueDonorOval.png');
			  $("#nonProfitQuestions").hide();
			  $("#nonProfitOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/greyNonProfitOval.png');
			  $("#foundationQuestions").hide();
			  $("#foundationOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/greyFoundationOval.png');
			  $("#donorQuestions").show();
			  $("#donorOval").css('z-index','1');
			  $("#nonProfitOval").css('zIndex','0');
			  $("#foundationOval").css('zIndex','0');		  
			});
			
			$("#nonProfitArea").mouseover(function() {
			  $("#nonProfitOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/maroonNonProfitOval.png');
			  $("#donorQuestions").hide();
			  $("#donorOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/greyDonorOval.png');
			  $("#foundationQuestions").hide();
			  $("#foundationOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/greyFoundationOval.png');
			  $("#nonProfitQuestions").show();
			  $("#nonProfitOval").css('z-index','1');
			  $("#foundationOval").css('zIndex','0');
			  $("#donorOval").css('zIndex','0');
			});
			
			$("#foundationArea").mouseover(function() {
			  $("#foundationOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/yellowFoundationOval.png');
			  $("#nonProfitQuestions").hide();
			  $("#nonProfitOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/greyNonProfitOval.png');
			  $("#donorQuestions").hide();
			  $("#donorOval").attr('src','https://www.americanphilanthropic.com/wp-content/themes/amp/i/greyDonorOval.png');
			  $("#foundationQuestions").show();
			  $("#foundationOval").css('z-index','1');
			  $("#donorOval").css('zIndex','0');
			  $("#nonProfitOval").css('zIndex','0');
			});
		});
		</script>
