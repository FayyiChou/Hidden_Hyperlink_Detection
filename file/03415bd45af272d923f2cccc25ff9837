  <script type="text/javascript">
    var varPx = 1;
	var interval1;
	var interval2;
	var speed = 40;
	function scrollDiv( dir, px, div ) {
        var scroller = document.getElementById(div);
        if (!scroller) return;
		var oriScrollLeft = scroller.scrollLeft;
        if (dir == 'l') {
            scroller.scrollLeft -= px;
        }
        else if (dir == 'r') {
            scroller.scrollLeft += px;
        }
		if ( oriScrollLeft == scroller.scrollLeft ) {
		    scroller.scrollLeft = 0;
		}
	}
	function setI1() {
	    interval1 = setInterval( "scrollDiv( 'r', varPx, 'scroller1')", speed );
	}
	function clearI1() {
	    clearInterval(interval1 );
	}
	function setI2() {
	    interval2 = setInterval( "scrollDiv( 'r', varPx, 'scroller2')", speed );
	}
	function clearI2() {
	    clearInterval(interval2 );
	}
	setI1();
	setI2();
	function bookmarkit() {
	    window.external.addFavorite('http://www.tzcoop.net','欢迎光临浙江省台州市供销社!');
	}
