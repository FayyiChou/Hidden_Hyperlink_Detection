<script type="text/javascript">

var speed=20;
var marquee_product2WGdPTI = document.getElementById("marquee_product2WGdPTI");
var marquee_product1WGdPTI = document.getElementById("marquee_product1WGdPTI");
var marquee_demoWGdPTI = document.getElementById("marquee_demoWGdPTI");

var showwidthWGdPTI;
var loopwidthWGdPTI;
var iWGdPTI;
var mWGdPTI,nWGdPTI;

showwidthWGdPTI=marquee_demoWGdPTI.offsetWidth*1;
loopwidthWGdPTI=marquee_product1WGdPTI.offsetWidth*1;

if(loopwidthWGdPTI<=1){loopwidthWGdPTI=1;}

mWGdPTI=Math.ceil(showwidthWGdPTI/loopwidthWGdPTI);

for(nWGdPTI=0;nWGdPTI<mWGdPTI;nWGdPTI++){
marquee_product2WGdPTI.innerHTML+=marquee_product1WGdPTI.innerHTML;
}

marquee_demoWGdPTI.scrollLeft=loopwidthWGdPTI;
iWGdPTI=0;

function MarqueeWGdPTI(){

if(iWGdPTI>=loopwidthWGdPTI){
 iWGdPTI=0;
 }else{
 iWGdPTI++;
}
marquee_demoWGdPTI.scrollLeft=loopwidthWGdPTI-iWGdPTI;

}
var MyMarWGdPTI=setInterval(MarqueeWGdPTI,speed);
marquee_demoWGdPTI.onmouseover=function(){clearInterval(MyMarWGdPTI);}
marquee_demoWGdPTI.onmouseout=function(){MyMarWGdPTI=setInterval(MarqueeWGdPTI,speed);}

</script>

