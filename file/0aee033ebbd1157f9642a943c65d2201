/*小纸条*/
var sendPPDialog;
function sendPapertape(userId,roleId){
getCss(static_dir+"js/plugs/artdialog/skins/idialog.css");
$.getScript(static_dir+"js/plugs/artdialog/jquery.artDialog.min.js",function(){
sendPPDialog = artDialog({
title:"小纸条",
initialize: function () {
$.ajax({
type:"get",
url:"/home///sms/create",
data:{"userId":userId,"roleId":roleId,"dialog":true},
success:function(data){
sendPPDialog.content(data)
}
});
}
});
sendPPDialog.lock();
})
}
</script>
