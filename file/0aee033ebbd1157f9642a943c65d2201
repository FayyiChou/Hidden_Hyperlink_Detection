<script type="text/javascript">
function confirmMark(){
getCss(static_dir+"js/plugs/artdialog/skins/idialog.css");
$.getScript(static_dir+"js/plugs/artdialog/jquery.artDialog.min.js",function(){
var markDialog = artDialog({
ok:function(){
this.close();
var val=$('input:radio[name="mark"]:checked').val();
var markname="满意";
if(val==2){
markname="不满意";
}
$.ajax({
type:"post",
url:"/front/resource/mark",
data:{"resourceId":9571,"mark":val},
success:function(data){
var markname="满意";
if(val==2){
markname="不满意";
}
$("#curmarkname").text(markname);
alert('评价成功');
}
});
return true ;
},
title:"评价确认",
okValue:"确定",
cancelValue:"取消",
//cancelVal: '关闭',
cancel: true ,
initialize: function () {
this.content($("#dg_mark").html());
var val=$('input:radio[name="mark"]:checked').val();
var markname="满意";
if(val==2){
markname="不满意";
}
$("#markname").text(markname)
}
});
markDialog.lock();
});
}
$(document).ready(function(){
$("input[name='mark']").live("click",function(){
confirmMark();
})
})
</script>
