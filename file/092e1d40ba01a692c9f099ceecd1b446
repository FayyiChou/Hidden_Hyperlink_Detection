function ShowZB(a){
	if (a==1){
		document.all.NoteZB.style.display ="";
			document.all.LocalZB.style.display ="none";
			document.all.OtherZB.style.display ="none";
		}
		if (a==2){
		document.all.NoteZB.style.display ="none";
			document.all.LocalZB.style.display ="";
			document.all.OtherZB.style.display ="none";
		}
		if (a==3){
		document.all.NoteZB.style.display ="none";
			document.all.LocalZB.style.display ="none";
			document.all.OtherZB.style.display ="";
		}
	}
	function ShowHB(a){
	if (a==1){
		document.all.HBList.style.display ="";
			document.all.LocalHB.style.display ="none";
			document.all.OtherHB.style.display ="none";
			document.all.ZHHB.style.display ="none";
		}
		if (a==2){
		document.all.HBList.style.display ="none";
			document.all.LocalHB.style.display ="";
			document.all.OtherHB.style.display ="none";
			document.all.ZHHB.style.display ="none";
		}
		if (a==3){
		document.all.HBList.style.display ="none";
			document.all.LocalHB.style.display ="none";
			document.all.OtherHB.style.display ="";
			document.all.ZHHB.style.display ="none";
		}
		if (a==4){
		document.all.HBList.style.display ="none";
			document.all.LocalHB.style.display ="none";
			document.all.OtherHB.style.display ="none";
			document.all.ZHHB.style.display ="";
		}
	}
