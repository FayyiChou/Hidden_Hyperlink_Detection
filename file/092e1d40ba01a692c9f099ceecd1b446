<script>
function ShowStock(a){
	if (a==1){
		document.all.StockList.style.display ="";
			document.all.LocalStock.style.display ="none";
			document.all.OtherStock.style.display ="none";
		}
		if (a==2){
		document.all.StockList.style.display ="none";
			document.all.LocalStock.style.display ="";
			document.all.OtherStock.style.display ="none";
		}
		if (a==3){
		document.all.StockList.style.display ="none";
			document.all.LocalStock.style.display ="none";
			document.all.OtherStock.style.display ="";
		}
	}
	function ShowProject(a){
	if (a==1){
		document.all.ProjectList.style.display ="";
			document.all.LocalProject.style.display ="none";
			document.all.OtherProject.style.display ="none";
		}
		if (a==2){
		document.all.ProjectList.style.display ="none";
			document.all.LocalProject.style.display ="";
			document.all.OtherProject.style.display ="none";
		}
		if (a==3){
		document.all.ProjectList.style.display ="none";
			document.all.LocalProject.style.display ="none";
			document.all.OtherProject.style.display ="";
		}
	}
</script>
