	<script>
		function checkIndexSearch(){
			if (document.IndexSearchForm.module.value==""){
				    alert("请选择一个栏目!");
					document.IndexSearchForm.module.focus();
					return false;
				}
				var tt =document.IndexSearchForm.module.value;
				if (parseInt(tt)==1){
				document.IndexSearchForm.target ='_blank';
				document.IndexSearchForm.action ='/news.do?method=list';
				}
				if (parseInt(tt)==2){
				document.IndexSearchForm.target ='_blank';
				document.IndexSearchForm.action ='/zb.do?method=list';
				}
				if (parseInt(tt)==3){
				document.IndexSearchForm.target ='_blank';
				document.IndexSearchForm.action ='/hb.do?method=list';
				}
				if (parseInt(tt)==4){
				document.IndexSearchForm.target ='_blank';
				document.IndexSearchForm.action ='/stock.do?method=list';
				}
				if (parseInt(tt)==5){
				document.IndexSearchForm.target ='_blank';
				document.IndexSearchForm.action ='/zj.do?method=list';
				}
				if (parseInt(tt)==8){
				document.IndexSearchForm.target ='_blank';
				document.IndexSearchForm.action ='/project.do?method=list';
				}
			}
		</script>
