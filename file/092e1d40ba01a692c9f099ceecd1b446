function openWin(id){
var iWidth=820;//模态窗口宽度
var iHeight=700;//模态窗口高度
var iTop=(window.screen.height-iHeight)/2;
var iLeft=(window.screen.width-iWidth)/2;
url ='/project/show.jsp?id='+id;
var str =window.showModalDialog(url,"newwin","dialogHeight:"+iHeight+"px;dialogWidth:"+iWidth+"px;dialogTop:"+iTop+"px;dialogLeft:"+iLeft+"px;edge:Raised;center:Yes;help:No;resizable:Yes;status:No;");
}
function getProject(id){
dwrBO.getAccessSessionID(id,backProject);
}
function backProject(data){
openWin(data);
}
