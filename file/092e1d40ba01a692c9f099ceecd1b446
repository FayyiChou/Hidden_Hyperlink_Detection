function ShowNews(a){
	if (a==1){
		document.all.NewsList.style.display ="";
			document.all.LocalNewsList.style.display ="none";
			document.all.YQTBNewsList.style.display ="none";
			document.all.JSGTNewsList.style.display ="none";
			document.all.JCXXNewsList.style.display ="none";
			document.all.GCKXNewsList.style.display ="none";
		}
		if (a==2){
		document.all.NewsList.style.display ="none";
			document.all.LocalNewsList.style.display ="";
			document.all.YQTBNewsList.style.display ="none";
			document.all.JSGTNewsList.style.display ="none";
			document.all.JCXXNewsList.style.display ="none";
			document.all.GCKXNewsList.style.display ="none";
		}
		if (a==3){
		document.all.NewsList.style.display ="none";
			document.all.LocalNewsList.style.display ="none";
			document.all.YQTBNewsList.style.display ="";
			document.all.JSGTNewsList.style.display ="none";
			document.all.JCXXNewsList.style.display ="none";
			document.all.GCKXNewsList.style.display ="none";
		}
		if (a==4){
		document.all.NewsList.style.display ="none";
			document.all.LocalNewsList.style.display ="none";
			document.all.YQTBNewsList.style.display ="none";
			document.all.JSGTNewsList.style.display ="";
			document.all.JCXXNewsList.style.display ="none";
			document.all.GCKXNewsList.style.display ="none";
		}
		if (a==5){
		document.all.NewsList.style.display ="none";
			document.all.LocalNewsList.style.display ="none";
			document.all.YQTBNewsList.style.display ="none";
			document.all.JSGTNewsList.style.display ="none";
			document.all.JCXXNewsList.style.display ="";
			document.all.GCKXNewsList.style.display ="none";
		}
		if (a==6){
		document.all.NewsList.style.display ="none";
			document.all.LocalNewsList.style.display ="none";
			document.all.YQTBNewsList.style.display ="none";
			document.all.JSGTNewsList.style.display ="none";
			document.all.JCXXNewsList.style.display ="none";
			document.all.GCKXNewsList.style.display ="";
		}
	}
