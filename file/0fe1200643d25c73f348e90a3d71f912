/*wrapslide*/
var host = document.location.hash;	
if(host == '#dl'){
	var ifram = $("#ifram").attr('src');
	$("#ifram").attr('src', ifram + '#dl');
}
if( "" != host &&  typeof(host) != 'undefined' ){
	var hostname=host.substr(1);
	if( $('.' + hostname).length > 0){
		$('.pub-info').hide();
		$('.' + hostname).show();
	}else{
		$('.gen').show();
	}
}else{
	$('.gen').show();
}
var hrefs = document.getElementsByTagName('a');
var _czc=[];
_czc.push(['_setAccount', '30091104']);
$(document).ready(function(){
	$('a').each(function(){
		$(this).bind('click', function(){
			var values = $(this).attr('data');
			values = values.split(',');
			if(values.length == 2){
				_czc.push(["_trackEvent", values[0], '点击连接', values[1]]);
			}
		});
	});
});
