                $(window).scroll(function(){
                　　var scrollTop = $(this).scrollTop();
                　　var scrollHeight = $(document).height();
                　　var windowHeight = $(this).height();
                　　if(scrollTop + windowHeight >= scrollHeight - 60){
                        if (!bottomFlag) {
                            // make some request
