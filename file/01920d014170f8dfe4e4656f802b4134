        <td>
<script language=javascript>
var isIE=!(navigator.userAgent.indexOf('MSIE')==-1);
var news2;
var curNew2=0;
var timer2;
function init2()
{
	var div2=document.getElementById("NewsPic2");
	if(!div2)return;
	var nav2=document.createElement("DIV");
	nav2.className="Nav";
	var nodes2;
	if(isIE)
	{
	    nodes2=div2.childNodes;
	}
	else
	{
	    nodes2=childrenNodes2(div2.childNodes);
	}
	news2=new Array(nodes2.length);
	for(var i=nodes2.length-1;i>=0;i--)
	{
	    var element2=nodes2[i];
	    
	    
		news2[i]={};
		news2[i].Element=element2;
		news2[i].Text=element2.getAttribute("title");
		news2[i].Url=element2.getAttribute("href");
		
		var n2=document.createElement("span");
		n2.innerHTML="<a herf=\"javascript:;\" onMouseMove=\"javascript:curNew2="+(i-1)+";change2();\">"+(i+1)+"</a>";
		if(i==curNew2)n2.className="Cur";
		nav2.appendChild(n2);
		
		news2[i].LinkElement=n2;
	}
	div2.appendChild(nav2);
	curNew2--;
	change2();

}
function childrenNodes2(node2)
{
    var c2=new Array();
    for(var i=0;i<node2.length;i++)
    {
        if(node2[i].nodeName.toLowerCase()=="a")
            c2.push(node2[i]);
    }
    return c2;
}
function change2()
{
    var div2=document.getElementById("NewsPic2");
    var text2=document.getElementById("NewsPicTxt2");
    if(!div2)return;
    curNew2=curNew2+1;
    if(curNew2>=news2.length)curNew2=0;
    for(var i=0;i<news2.length;i++)
    {
        if(i==curNew2)
        {
            news2[i].Element.style.display="block";
            news2[i].Element.style.visibility="visible";
            news2[i].LinkElement.className="Cur";
            text2.innerHTML="<a href=\""+news2[i].Url+"\" title=\""+news2[i].Text+"\" target=\"_blank\">"+news2[i].Text+"</a>";
        }
        else
        {
            news2[i].Element.style.visibility="hidden";
            news2[i].Element.style.display="none";
            news2[i].LinkElement.className="Normal";
        }
    }
    if(timer2)window.clearTimeout(timer2);
    timer2=window.setTimeout("change2()",3000);
    
}


</script>

