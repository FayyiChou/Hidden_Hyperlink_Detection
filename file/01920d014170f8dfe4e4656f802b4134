function selectOnChange(strUrl){
  if (strUrl == ""){
    return;
  }
  var strParam = "height=" + screen.availHeight + ",width=" + screen.availWidth + ",top=0,left=0,toolbar=yes,menubar=yes,scrollbars=yes, resizable=yes,location=yes, status=no";
  window.open(strUrl, "_blank", strParam);
}


