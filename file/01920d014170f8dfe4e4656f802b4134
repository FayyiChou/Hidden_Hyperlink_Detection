<script>
// url跟路径
var strUrl = unescape(window.location.href);
var strBaseUrl = strUrl.substring(0, strUrl.indexOf(":8099"));

function advanceSearch(){
	window.open("http://search.agri.gov.cn/agrisearch/advsearch.jsp","_blank","height=" + screen.availHeight + ",width=" + screen.availWidth + ",top=0,left=0,toolbar=yes,menubar=yes,scrollbars=yes, resizable=yes,location=yes, status=no");
}
// 普通全文检索
function commonSearch(){
  document.getElementById("channelid").value = document.getElementById("channelSelect").value;
  document.getElementById("keyword").value = document.getElementById("searchword").value;
  var iIndex = document.getElementById("channelSelect").selectedIndex;
  var strUrl;
  if (iIndex < 19){
    strUrl = "http://search.agri.gov.cn/agrisearch/search.jsp";
    document.getElementById("sortfield").value = "-日期";
  } else {
    strUrl = "http://search.agri.gov.cn/zxly/simple.jsp";
    document.getElementById("sortfield").value = "-DOCPUBTIME";
  }
  form1.action = strUrl;
  form1.target = "_blank";
  //form1.submit();
}
</script>

<script>
function tit_(value){
 var tit_t = value;
 while(tit_t.indexOf("<br />")!=-1)
 {tit_t=tit_t.replace("<br />"," ");
} 
 if(tit_t.indexOf("<br /")!=-1){
	tit_t=tit_t.replace("<br /"," ");}
 else if(tit_t.indexOf("<br ")!=-1){
	tit_t=tit_t.replace("<br "," ");}
else if(tit_t.indexOf("<br")!=-1){
	tit_t=tit_t.replace("<br"," ");}
else if(tit_t.indexOf("<b")!=-1){
	tit_t=tit_t.replace("<b"," ");}
/*else if(tit_t.indexOf("<")!=-1){
	tit_t=tit_t.replace("<"," ");}*/


 return tit_t;
 }
</script>
