function writePlayList(){
	var playlist = "";
	for(i=0; i < titles.length; i++){
		titles[i] = i + 1 + '. '+titles[i];
		if(i == 0) playlist += '<a href="javascript:void(0);" onclick="javascript:play('+i+');"><img src="'+pics[i]+'" alt="'+titles[i]+'"></a><p><a href="javascript:void(0);" onclick="javascript:play('+i+');">'+titles[i]+'</a></p>';
		if(i == 1) playlist += '<ul>';
		if(i) playlist += '<li><a href="javascript:void(0);" onclick="javascript:play('+i+');">'+titles[i]+'</a></li>';
		if(i && i == titles.length - 1) playlist += '</ul>';
	}
	document.writeln(playlist);
}
function play(index){
	_playindex = index;
	setcookie("single","",30);
	_playing = true;
	initData.playindex = _playindex;
	turntonextpage();
}
function playState(){
	//document.getElementById(initData.playcontentdiv).style.display=getcookie("single")?getcookie("single"):"";
	if(getcookie("single") && _playing){
		getFlashMovieObject(initData.playerid).jscpause();
		_playing = false;
	}
}
function downloadFlash(){
	loadDownDialog(aids[Math.max(initData.playindex - 1, 0)]);
}
