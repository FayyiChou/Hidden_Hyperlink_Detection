			function SetIframeHeight(){
				var iframeMessage=document.getElementById("iframeMessage");
				if (document.getElementById){
					if (iframeMessage && !window.opera){
						if (iframeMessage.contentDocument && iframeMessage.contentDocument.body.offsetHeight){
							iframeMessage.height = iframeMessage.contentDocument.body.offsetHeight;
						}else if(iframeMessage.Document && iframeMessage.Document.body.scrollHeight){
							iframeMessage.height = iframeMessage.Document.body.scrollHeight;
						}
					}
				}
			}
