$(document).ready(function(e) {

t = $('.bar_Right').offset().top;

mh = $('.bra_List').height();

fh = $('.bar_Right').height();

$(window).scroll(function(e){

s = $(document).scrollTop();

if(s > t - 10){

$('.bar_Right').css('position','fixed');

if(s + fh > mh){

$('.bar_Right').css('top',mh-s-fh+'px');

}

}else{

$('.bar_Right').css('position','');

}

})

}); 



