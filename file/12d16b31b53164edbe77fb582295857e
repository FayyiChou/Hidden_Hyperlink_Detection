<script>
jQuery(function($) {
	var p_searinp = $('.p_searinp')
	p_searinp.focus(function(e) {
		p_fastval = $(this).val();
		$(this).css('color','#333');
		if (p_fastval=='输入关键词') {
			$(this).val("");
		}
	});
	p_searinp.blur(function(e) {
		p_fastval = $(this).val();
		if (!p_fastval) {
			$(this).val('输入关键词');
			$(this).css('color','#888');
		}
	});
});
function searchSub()
{
    var v = $.trim($(".p_searinp").val());
    if(v == '' || v == '输入关键词')
        alert('谢谢您的支持，请填写搜索内容！');
    else
       $("#search").submit();
}
