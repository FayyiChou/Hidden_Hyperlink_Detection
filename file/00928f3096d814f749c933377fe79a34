function jqcookie(){
	var Then=new Date();
	Then.setTime(Then.getTime()+512000000);
	document.cookie="jq_Obj=1;expires="+Then.toGMTString()+";path=/;";
}

function jqscookie(){
	var Then=new Date();
	Then.setTime(Then.getTime()+36000000);
	document.cookie="jq_Objs=1;expires="+Then.toGMTString()+";path=/;";
}

function Set(){
	var Then=new Date();
	Then.setTime(Then.getTime()+Time*1000);
	document.cookie="netbei=1;expires="+Then.toGMTString()+";path=/;";
}

function Seter(){
	var Thener=new Date();
	Thener.setTime(Thener.getTime()+Time*1000);
