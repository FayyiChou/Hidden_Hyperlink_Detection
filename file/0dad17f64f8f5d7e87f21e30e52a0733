<style>
#ge-gt {
position: fixed;
right: 5px;
bottom: 215px;
z-index: 100;
filter: alpha(opacity=90);
opacity: .9;
}
</style>
<script>
  jq(function(){
    jq('#ge-gt').on('click', function(e){
      e.stopPropagation();
      e.preventDefault();
      jq('html,body').animate({'scrollTop':0},0);
    })
  });
  jq(window).scroll(function() {		
    if(jq(window).scrollTop() >= 100){
        jq('#ge-gt').fadeIn(300); 
    }else{    
        jq('#ge-gt').fadeOut(300);    
    }  
  })
</script>
