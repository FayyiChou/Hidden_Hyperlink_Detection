<script>
    $(function(){
        if($(window).scrollTop()==0){
            $("#returntop").hide();
        }
        $(window).scroll(function(){       
            var ViewHeight = $(window).height();
            if($(window).scrollTop() >= (ViewHeight-20)){
                $("#returntop").show();
            }else{
                $("#returntop").hide();
            }
        });
        $(".fix_erweima").hover(function(){
            $("#ewm").show();
        },function(){
            $("#ewm").hide();
        });
        $("#returntop").click(function(){
            $('body,html').animate({scrollTop:0},50);
            return false;
        });        
    });
</script>
