//选项卡
var cx_a = $(".tab dt a");
cx_a.mouseenter(function () {
var $th = $(this);
var dd = $th.parents(".tab").children("dd");
var index = $th.index();
$th.addClass("tabIn").siblings().removeClass("tabIn").end();
dd.eq(index).removeClass("hide").siblings("dd").addClass("hide").end();
});
</script>
