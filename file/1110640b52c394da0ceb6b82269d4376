		<script>
			function filter(a, b, j) {
				$("#caseform input").each(function () {
					var id = $(this).attr("class");
					if (id == a) {
						$(this).attr("value", b);
						$("#kjimgid").attr("value", j);
						var formurl = "/al/id/14/kjid/" + b + ".html";
						$("#caseform").attr("action", formurl);
						$("#caseform").submit();
					}
				});
			}
			function btn_search() {
			    var thistext = $("#icases_s_text").val();
			    $("#case_keywored").attr("value",thistext);
				var formurl = "/al/id/14.html";
				$("#caseform").attr("action", formurl);
				$("#caseform").submit();
			}
			function inputkeyword(thistext) {
				$("#par120").attr("value",thistext);
			}
		</script>
