#hh{ float:right; height:25px; padding:11px 0px 0px 0px;}
#hh span{cursor:pointer;}
#window{width:241px; height:242px;overflow:hidden;}
#window li { text-align:center; float:left; }
#window li a { line-height:16px; color:#333333; font-size:14px; display:block; }
