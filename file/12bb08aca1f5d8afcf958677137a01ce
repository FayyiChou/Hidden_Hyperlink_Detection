<script type="text/javascript"> 
$(function(){
$(".back").click(function(){
$("#window li:first").not(":animated").animate({marginLeft:"0"},1000,function(){
$("#window li:last").prependTo("#window ul").css("margin-left","-241px");
});
});
$(".next").click(function(){
$("#window li:eq(1)").not(":animated").animate({marginLeft:"-241px"},1000,function(){
$("#window li:first").appendTo("#window ul").css("margin-left","0");
});
});
t=setInterval("autoRun()",6000);
$("#hh span,#window ul").hover(function(){
clearInterval(t);
},function(){t=setInterval("autoRun()",6000);});
});
function autoRun(){
$(".next").trigger("click");
};
</script>
